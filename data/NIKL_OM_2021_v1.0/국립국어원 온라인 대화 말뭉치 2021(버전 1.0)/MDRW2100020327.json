        },
        {
          "id": "MDRW2100020327.1.6",
          "form": "환불각임",
          "original_form": "환불각임",
          "speaker_id": "2",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020327.1.7",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020327.1.8",
          "form": "다른 사람들 착샷이 너무 별로던데요",
          "original_form": "다른 사람들 착샷이 너무 별로던데요",
          "speaker_id": "2",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020327.1.9",
          "form": "나라고 다를까요",
          "original_form": "나라고 다를까요",
          "speaker_id": "2",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020327.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020327.1.11",
          "form": "자기객관화가 잘되시네요",
          "original_form": "자기객관화가 잘되시네요",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020327.1.12",
          "form": "지르기전에 착샷을",
          "original_form": "지르기전에 착샷을",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020327.1.13",
          "form": "먼저 확인하시지 그랬어요",
          "original_form": "먼저 확인하시지 그랬어요",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020327.1.14",
          "form": "ㅋㅋㅋㅋㅋ약간 새벽뽐뿌였어서",
          "original_form": "ㅋㅋㅋㅋㅋ약간 새벽뽐뿌였어서",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020327.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020327.1.16",
          "form": "걍살까요",
          "original_form": "걍살까요",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020327.1.17",
          "form": "조언 좀",
          "original_form": "조언 좀",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020327.1.18",
          "form": "돈없잖아요",
          "original_form": "돈없잖아요",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020327.1.19",
          "form": "환불하셈",
          "original_form": "환불하셈",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020327.1.20",
          "form": "그리고 저희한테 점심이나 거하게 한턱쏘심될듯",
          "original_form": "그리고 저희한테 점심이나 거하게 한턱쏘심될듯",
          "speaker_id": "1",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020327.1.21",
          "form": "ㅋㅋㅋㅋㄱ아니 뭔 맨날 고런 마무리죠",
          "original_form": "ㅋㅋㅋㅋㄱ아니 뭔 맨날 고런 마무리죠",
          "speaker_id": "2",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020327.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ양아치라그래요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ양아치라그래요",
          "speaker_id": "1",
          "time": "20210828 01:31"
        },
        {
          "id": "MDRW2100020327.1.23",
          "form": "그래서 춤추는 고영이 사실건가요",
          "original_form": "그래서 춤추는 고영이 사실건가요",
          "speaker_id": "1",
          "time": "20210828 01:31"
        },
        {
          "id": "MDRW2100020327.1.24",
          "form": "그 구매는 적극찬성이요",
          "original_form": "그 구매는 적극찬성이요",
          "speaker_id": "1",
          "time": "20210828 01:31"
        },
        {
          "id": "MDRW2100020327.1.25",
          "form": "그건 사야조 사무직 필수임",
          "original_form": "그건 사야조 사무직 필수임",
          "speaker_id": "2",
          "time": "20210828 01:32"
        },
        {
          "id": "MDRW2100020327.1.26",
          "form": "버킷햇은 잘 씁니까",
          "original_form": "버킷햇은 잘 씁니까",
          "speaker_id": "2",
          "time": "20210828 01:32"
        },
        {
          "id": "MDRW2100020327.1.27",
          "form": "사무직필수는 무슨 ㅋㅋ",
          "original_form": "사무직필수는 무슨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020327.1.28",
          "form": "버킷햇 출근할때 쓰고가긴 뭐하잖아요",
          "original_form": "버킷햇 출근할때 쓰고가긴 뭐하잖아요",
          "speaker_id": "1",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020327.1.29",
          "form": "내일 쓰고 나가려구요",
          "original_form": "내일 쓰고 나가려구요",
          "speaker_id": "1",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020327.1.30",
          "form": "하긴 출근할때 모자는 좀 그렇죠",
          "original_form": "하긴 출근할때 모자는 좀 그렇죠",
