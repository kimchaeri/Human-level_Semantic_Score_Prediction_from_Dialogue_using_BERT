      "utterance": [
        {
          "id": "MDRW2100002522.1.1",
          "form": "유튜브 같은건 많이 보시나요?",
          "original_form": "유튜브 같은건 많이 보시나요?",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002522.1.2",
          "form": "네 많이 보죠!",
          "original_form": "네 많이 보죠!",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002522.1.3",
          "form": "특히 요즘 시국에 유일한 재미거리기도 해서",
          "original_form": "특히 요즘 시국에 유일한 재미거리기도 해서",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002522.1.4",
          "form": "자주 보는거같아요!",
          "original_form": "자주 보는거같아요!",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002522.1.5",
          "form": "ㅋㅋㅋ좋아하는 유튜버 있으신가요",
          "original_form": "ㅋㅋㅋ좋아하는 유튜버 있으신가요",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002522.1.6",
          "form": "연예인 김종국씨!",
          "original_form": "연예인 김종국씨!",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002522.1.7",
          "form": "최근에 유튜브 개설하셨던데 재밌더라고요!",
          "original_form": "최근에 유튜브 개설하셨던데 재밌더라고요!",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002522.1.8",
          "form": "아 운동하는거 좋아하세요??",
          "original_form": "아 운동하는거 좋아하세요??",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002522.1.9",
          "form": "축구라던지 구기종목이나 그런 운동은 좋아하죠!",
          "original_form": "축구라던지 구기종목이나 그런 운동은 좋아하죠!",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002522.1.10",
          "form": "저는 haha ha라는고양이 유투버 좋아해요",
          "original_form": "저는 haha ha라는고양이 유투버 좋아해요",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002522.1.11",
          "form": "보신적 있으신가요??",
          "original_form": "보신적 있으신가요??",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002522.1.12",
          "form": "아아 알고리즘으로 봤어요!",
          "original_form": "아아 알고리즘으로 봤어요!",
          "speaker_id": "2",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002522.1.13",
          "form": "엄청 재밌죠!",
          "original_form": "엄청 재밌죠!",
          "speaker_id": "2",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002522.1.14",
          "form": "ㅋㅋㅋㅋ귀여워서 좋아요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ귀여워서 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002522.1.15",
          "form": "몽자라고 강아지 유투버도 있어요!",
          "original_form": "몽자라고 강아지 유투버도 있어요!",
          "speaker_id": "1",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002522.1.16",
          "form": "아아 몽자!",
          "original_form": "아아 몽자!",
          "speaker_id": "2",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002522.1.17",
          "form": "그것도 보고있죠! 엄청 귀엽더라구요 ㅠㅠ",
          "original_form": "그것도 보고있죠! 엄청 귀엽더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002522.1.18",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002522.1.19",
          "form": "저는 유튜브를 거의 동물만 보나봐요 호",
          "original_form": "저는 유튜브를 거의 동물만 보나봐요 호",
          "speaker_id": "1",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002522.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002522.1.21",
          "form": "동물 유튜브가",
          "original_form": "동물 유튜브가",
          "speaker_id": "2",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002522.1.22",
          "form": "힐링이 잘되죠 엄청",
          "original_form": "힐링이 잘되죠 엄청",
          "speaker_id": "2",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002522.1.23",
          "form": "그래서 저도 자주보긴해요!",
          "original_form": "그래서 저도 자주보긴해요!",
          "speaker_id": "2",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002522.1.24",
          "form": "ㅋㅋㅋㅋㅋ맞아요!",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요!",
          "speaker_id": "1",
