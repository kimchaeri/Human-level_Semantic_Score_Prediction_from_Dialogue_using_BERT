      "utterance": [
        {
          "id": "MDRW2100035315.1.1",
          "form": "자주는 사용 안하게 되네요",
          "original_form": "자주는 사용 안하게 되네요",
          "speaker_id": "2",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100035315.1.2",
          "form": "초반엔 신기해서 많이 사용했어요!",
          "original_form": "초반엔 신기해서 많이 사용했어요!",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100035315.1.3",
          "form": "그러시구나 저는 사세 버팔로윙 자주 먹어요",
          "original_form": "그러시구나 저는 사세 버팔로윙 자주 먹어요",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100035315.1.4",
          "form": "사세 드셔보신적 있나요?",
          "original_form": "사세 드셔보신적 있나요?",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100035315.1.5",
          "form": "아뇹 사세가 어떤건가요?",
          "original_form": "아뇹 사세가 어떤건가요?",
          "speaker_id": "2",
          "time": "20210829 14:53"
        },
        {
          "id": "MDRW2100035315.1.6",
          "form": "사세는 브랜드이름이구요",
          "original_form": "사세는 브랜드이름이구요",
          "speaker_id": "1",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100035315.1.7",
          "form": "호프집이나 피자집에 버팔로윙이 이제품인경우가 많아요",
          "original_form": "호프집이나 피자집에 버팔로윙이 이제품인경우가 많아요",
          "speaker_id": "1",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100035315.1.8",
          "form": "버팔로윙 좋아하시면 만족하실거예요",
          "original_form": "버팔로윙 좋아하시면 만족하실거예요",
          "speaker_id": "1",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100035315.1.9",
          "form": "아아!!!!! 그 제품이 사세군요",
          "original_form": "아아!!!!! 그 제품이 사세군요",
          "speaker_id": "2",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100035315.1.10",
          "form": "name1님은 에프 자주 사용하세요?",
          "original_form": "&name1&님은 에프 자주 사용하세요?",
          "speaker_id": "2",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100035315.1.11",
          "form": "종종 이용해요 피자나 , 버팔로윙 돌려먹을때 써요",
          "original_form": "종종 이용해요 피자나 , 버팔로윙 돌려먹을때 써요",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100035315.1.12",
          "form": "그런건 진짜 좋긴하더라구요",
          "original_form": "그런건 진짜 좋긴하더라구요",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100035315.1.13",
          "form": "전자.레인.지 보다 좋죠",
          "original_form": "전자.레인.지 보다 좋죠",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100035315.1.14",
          "form": "집에서 간단하게 피자빵? 만들어먹을때도",
          "original_form": "집에서 간단하게 피자빵? 만들어먹을때도",
