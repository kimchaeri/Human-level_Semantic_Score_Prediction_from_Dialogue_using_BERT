      "utterance": [
        {
          "id": "MDRW2100025981.1.1",
          "form": "식사는 뭐드세요?",
          "original_form": "식사는 뭐드세요?",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025981.1.2",
          "form": "비빔밥먹었어요ㅎ",
          "original_form": "비빔밥먹었어요ㅎ",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100025981.1.3",
          "form": "name1님은 드셨나요?",
          "original_form": "&name1&님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100025981.1.4",
          "form": "저는 떡만두국 해먹었답니다",
          "original_form": "저는 떡만두국 해먹었답니다",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100025981.1.5",
          "form": "선선해지니 따뜻한게 땡기네요",
          "original_form": "선선해지니 따뜻한게 땡기네요",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100025981.1.6",
          "form": "만두 맛있었겠다ㅎㅎㅎ",
          "original_form": "만두 맛있었겠다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100025981.1.7",
          "form": "만들어드셨나요?",
          "original_form": "만들어드셨나요?",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100025981.1.8",
          "form": "만들어 먹었죠",
          "original_form": "만들어 먹었죠",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025981.1.9",
          "form": "만두는 냉도만두죠 ㅎ",
          "original_form": "만두는 냉도만두죠 ㅎ",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025981.1.10",
          "form": "냉동만두라도 맛있죠!",
          "original_form": "냉동만두라도 맛있죠!",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100025981.1.11",
          "form": "워낙 판매되는  만두를 다들 맛있게 만들어서ㅎㅎ",
          "original_form": "워낙 판매되는  만두를 다들 맛있게 만들어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100025981.1.12",
          "form": "며칠전만해도 치킨과 맥주가 땡겼었는데요",
          "original_form": "며칠전만해도 치킨과 맥주가 땡겼었는데요",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025981.1.13",
          "form": "요즘은 밀키트도 잘 나와서 쉽게 해먹을수도있어서 좋은것같아요",
          "original_form": "요즘은 밀키트도 잘 나와서 쉽게 해먹을수도있어서 좋은것같아요",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100025981.1.14",
          "form": "재료 사는것 보다 밀키트가",
          "original_form": "재료 사는것 보다 밀키트가",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100025981.1.15",
          "form": "더싸더라구요",
          "original_form": "더싸더라구요",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100025981.1.16",
          "form": "오늘이 지나면 내일은 제주도에 있을건데 맛집 추천좀해주세요",
          "original_form": "오늘이 지나면 내일은 제주도에 있을건데 맛집 추천좀해주세요",
          "speaker_id": "1",
          "time": "20210902 07:03"
        },
        {
          "id": "MDRW2100025981.1.17",
          "form": "전 제주도 음식 다 실패했었어요ㅜㅜ",
          "original_form": "전 제주도 음식 다 실패했었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100025981.1.18",
          "form": "그냥 아침으로 먹은 순두부찌개가 제일 맛있더라구요ㅜㅜ",
          "original_form": "그냥 아침으로 먹은 순두부찌개가 제일 맛있더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100025981.1.19",
          "form": "맞아요 저도 실패 많이 했어요",
          "original_form": "맞아요 저도 실패 많이 했어요",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100025981.1.20",
          "form": "가격도 비싸고 맛도 없고요",
          "original_form": "가격도 비싸고 맛도 없고요",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100025981.1.21",
          "form": "제주도 음식은 학교나 관공서 근처로 가라고 하더라구요",
          "original_form": "제주도 음식은 학교나 관공서 근처로 가라고 하더라구요",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100025981.1.22",
          "form": "관공서 근처 괜찬을것같네요",
          "original_form": "관공서 근처 괜찬을것같네요",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100025981.1.23",
          "form": "공무원은 맛집리스트가 있을거같아요ㅋㅋ",
          "original_form": "공무원은 맛집리스트가 있을거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100025981.1.24",
          "form": "떠나기전에 찾아봐야겠어요 ㅋ",
          "original_form": "떠나기전에 찾아봐야겠어요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:02"
        }
      ]
    }
  ]
