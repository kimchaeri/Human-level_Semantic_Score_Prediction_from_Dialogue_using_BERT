      "utterance": [
        {
          "id": "MDRW2100000907.1.1",
          "form": "나 아르바이트 하는 곳",
          "original_form": "나 아르바이트 하는 곳",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000907.1.2",
          "form": "옷이 너무 불편해",
          "original_form": "옷이 너무 불편해",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000907.1.3",
          "form": "공연장?",
          "original_form": "공연장?",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000907.1.4",
          "form": "아 저번에 사진으로 봤다",
          "original_form": "아 저번에 사진으로 봤다",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000907.1.5",
          "form": "어셔란 그런 것...",
          "original_form": "어셔란 그런 것...",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000907.1.6",
          "form": "그래도 언제 그런 알바 해보겠노",
          "original_form": "그래도 언제 그런 알바 해보겠노",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000907.1.7",
          "form": "참신한 알바 많이 찾는다 진짜",
          "original_form": "참신한 알바 많이 찾는다 진짜",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000907.1.8",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000907.1.9",
          "form": "한번 해본거에 의미를 둔다",
          "original_form": "한번 해본거에 의미를 둔다",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000907.1.10",
          "form": "진짜 딱 한번 해보면 재밌을꺼같아",
          "original_form": "진짜 딱 한번 해보면 재밌을꺼같아",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000907.1.11",
          "form": "언제한번 놀러온나",
          "original_form": "언제한번 놀러온나",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000907.1.12",
          "form": "그럼 나 한 번 시켜주고",
          "original_form": "그럼 나 한 번 시켜주고",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000907.1.13",
          "form": "한달 월급 나 주라",
          "original_form": "한달 월급 나 주라",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000907.1.14",
          "form": "엄 우리는 일당이라서",
          "original_form": "엄 우리는 일당이라서",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000907.1.15",
          "form": "저번달 월급은 0원이었거든",
          "original_form": "저번달 월급은 0원이었거든",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000907.1.16",
          "form": "0원줄께!",
          "original_form": "0원줄께!",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000907.1.17",
          "form": "일당이몀 한 3일치 일당 나한테 주면 되겠네",
          "original_form": "일당이몀 한 3일치 일당 나한테 주면 되겠네",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000907.1.18",
          "form": "그러면 그걸로 엽떡 사먹어야겠다.",
          "original_form": "그러면 그걸로 엽떡 사먹어야겠다.",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000907.1.19",
          "form": "스트레스 확 풀릴 거 같아^^",
          "original_form": "스트레스 확 풀릴 거 같아^^",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000907.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아 알바는 결국",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아 알바는 결국",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000907.1.21",
          "form": "먹으려고 하는 것.",
          "original_form": "먹으려고 하는 것.",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000907.1.22",
          "form": "먹을꺼 사먹으려고 하는거지^^",
          "original_form": "먹을꺼 사먹으려고 하는거지^^",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000907.1.23",
          "form": "사고싶은 것도 많으니까...",
          "original_form": "사고싶은 것도 많으니까...",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000907.1.24",
          "form": "나랑 의견이 같구나!",
          "original_form": "나랑 의견이 같구나!",
          "speaker_id": "1",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000907.1.25",
          "form": "알바 내가 대신 할게",
          "original_form": "알바 내가 대신 할게",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000907.1.26",
          "form": "근데 어디서 근무하는지는 기억하나",
          "original_form": "근데 어디서 근무하는지는 기억하나",
          "speaker_id": "1",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000907.1.27",
          "form": "진상손님은 없었나?",
          "original_form": "진상손님은 없었나?",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000907.1.28",
          "form": "공염장이지.",
          "original_form": "공염장이지.",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000907.1.29",
          "form": "그거외엔 모르지.",
          "original_form": "그거외엔 모르지.",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000907.1.30",
          "form": "예전에 아이스크림 알바했었잖아",
          "original_form": "예전에 아이스크림 알바했었잖아",
          "speaker_id": "1",
          "time": "20210820 00:31"
        }
      ]
    }
