      "utterance": [
        {
          "id": "MDRW2100012743.1.1",
          "form": "독서 자주 하세요?",
          "original_form": "독서 자주 하세요?",
          "speaker_id": "2",
          "time": "20210825 01:57"
        },
        {
          "id": "MDRW2100012743.1.2",
          "form": "자주한다고 말하기는 부끄러운데ㅋㅋ 책 빌려서 읽고있어요",
          "original_form": "자주한다고 말하기는 부끄러운데ㅋㅋ 책 빌려서 읽고있어요",
          "speaker_id": "1",
          "time": "20210825 10:10"
        },
        {
          "id": "MDRW2100012743.1.3",
          "form": "오~ 저도 책 빌려서 보는데 무슨 책 독서하세요?",
          "original_form": "오~ 저도 책 빌려서 보는데 무슨 책 독서하세요?",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100012743.1.4",
          "form": "저 이번에 데미안 빌렸어요!",
          "original_form": "저 이번에 데미안 빌렸어요!",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100012743.1.5",
          "form": "오 데미안 어려운 책 아니에요?",
          "original_form": "오 데미안 어려운 책 아니에요?",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100012743.1.6",
          "form": "아니요! 오히려 책중에 얇은 편이라 골랐어요ㅋㅋ",
          "original_form": "아니요! 오히려 책중에 얇은 편이라 골랐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100012743.1.7",
          "form": "앗 얇은 책이였군요 ㅋㅋ",
          "original_form": "앗 얇은 책이였군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100012743.1.8",
          "form": "뭔가 제목이 어려운 책 같았어요",
          "original_form": "뭔가 제목이 어려운 책 같았어요",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100012743.1.9",
          "form": "저도 그럴줄 알았는데 별로 안어려워요ㅎㄹ",
          "original_form": "저도 그럴줄 알았는데 별로 안어려워요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100012743.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100012743.1.11",
          "form": "얼만큼 읽었어요?",
          "original_form": "얼만큼 읽었어요?",
          "speaker_id": "2",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100012743.1.12",
          "form": "다 읽었어요! 이제 반납만 하면 됩니다ㅎㅎ",
          "original_form": "다 읽었어요! 이제 반납만 하면 됩니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100012743.1.13",
          "form": "와우 무슨 내용이였어요?",
          "original_form": "와우 무슨 내용이였어요?",
          "speaker_id": "2",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100012743.1.14",
          "form": "싱클레어가 자라는 내용? 이정도네요 한줄요약하면",
          "original_form": "싱클레어가 자라는 내용? 이정도네요 한줄요약하면",
          "speaker_id": "1",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100012743.1.15",
          "form": "아 ㅋㅋㅋ 싱클레어가 주인공인가보네요",
          "original_form": "아 ㅋㅋㅋ 싱클레어가 주인공인가보네요",
          "speaker_id": "2",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100012743.1.16",
          "form": "네 데미안이 아니에요ㅋㅋ",
          "original_form": "네 데미안이 아니에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100012743.1.17",
          "form": "그럼 데미안은 무슨 뜻이에요?",
          "original_form": "그럼 데미안은 무슨 뜻이에요?",
          "speaker_id": "2",
          "time": "20210826 01:09"
        }
      ]
