      "utterance": [
        {
          "id": "MDRW2100015190.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015190.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015190.1.3",
          "form": "오늘 날씨 진짜 습하더라구요 ㅠ",
          "original_form": "오늘 날씨 진짜 습하더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015190.1.4",
          "form": "어제 비가 와서 그런가",
          "original_form": "어제 비가 와서 그런가",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015190.1.5",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015190.1.6",
          "form": "저지금 부산인데 비 장난아니에요",
          "original_form": "저지금 부산인데 비 장난아니에요",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015190.1.7",
          "form": "우와 부산 사세요??",
          "original_form": "우와 부산 사세요??",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015190.1.8",
          "form": "네 부산살아요 ㅎㅎ",
          "original_form": "네 부산살아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015190.1.9",
          "form": "부산 밤바다 너무 예쁘더라구요",
          "original_form": "부산 밤바다 너무 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015190.1.10",
          "form": "전 지금 서울인데 비 안 와요!",
          "original_form": "전 지금 서울인데 비 안 와요!",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015190.1.11",
          "form": "바다 좋긴한데 확실히 습해서 별로일때도 많아요 ㅋㅋ ㅠ",
          "original_form": "바다 좋긴한데 확실히 습해서 별로일때도 많아요 ㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015190.1.12",
          "form": "오 서울은 비안오는군요!",
          "original_form": "오 서울은 비안오는군요!",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015190.1.13",
          "form": "여긴 거진 침수 직전이에요",
          "original_form": "여긴 거진 침수 직전이에요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015190.1.14",
          "form": "헐 비 엄청 오는구나 ㅠ",
          "original_form": "헐 비 엄청 오는구나 ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015190.1.15",
          "form": "비 오는 거 좋아하시나요??",
          "original_form": "비 오는 거 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015190.1.16",
          "form": "네 ㅜ",
          "original_form": "네 ㅜ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015190.1.17",
          "form": "너무싫어요 ㅋㅋㅋ",
          "original_form": "너무싫어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015190.1.18",
          "form": "습하구 찝찝하구 그래서 안좋아해요 ㅋㅌ ㅠㅠ",
          "original_form": "습하구 찝찝하구 그래서 안좋아해요 ㅋㅌ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015190.1.19",
          "form": "저도..",
          "original_form": "저도..",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015190.1.20",
          "form": "산들산들한 가을날씨가",
          "original_form": "산들산들한 가을날씨가",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015190.1.21",
          "form": "최고..",
          "original_form": "최고..",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015190.1.22",
          "form": "마쟈요! 이제곧가을인데 가을옷 장만하려구요 ㅎㅎ",
          "original_form": "마쟈요! 이제곧가을인데 가을옷 장만하려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015190.1.23",
          "form": "가을 너무 빠르게 지나가서 아쉬워요 ㅠ",
          "original_form": "가을 너무 빠르게 지나가서 아쉬워요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100015190.1.24",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100015190.1.25",
          "form": "근데 또 오늘은 날씨가 쨍쨍하니 좋네요",
          "original_form": "근데 또 오늘은 날씨가 쨍쨍하니 좋네요",
          "speaker_id": "1",
          "time": "20210826 14:51"
        }
      ]
    }
