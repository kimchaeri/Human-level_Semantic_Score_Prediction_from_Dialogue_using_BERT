      "utterance": [
        {
          "id": "MDRW2100007918.1.1",
          "form": "게임은 잘 안하는데 어떠세요",
          "original_form": "게임은 잘 안하는데 어떠세요",
          "speaker_id": "1",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007918.1.2",
          "form": "음... 저도 게임 잘 안해요",
          "original_form": "음... 저도 게임 잘 안해요",
          "speaker_id": "2",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007918.1.3",
          "form": "하다가도 잘 그만둬요",
          "original_form": "하다가도 잘 그만둬요",
          "speaker_id": "2",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007918.1.4",
          "form": "최근에 한 게임은 쿠키킹덤이라는 게임을 하긴했었는데",
          "original_form": "최근에 한 게임은 쿠키킹덤이라는 게임을 하긴했었는데",
          "speaker_id": "2",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007918.1.5",
          "form": "추억의 게임같은거 있으세요?",
          "original_form": "추억의 게임같은거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007918.1.6",
          "form": "현생이 바쁘니 결국엔 접게되더라구요ㅠ",
          "original_form": "현생이 바쁘니 결국엔 접게되더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007918.1.7",
          "form": "어릴때했던거",
          "original_form": "어릴때했던거",
          "speaker_id": "1",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007918.1.8",
          "form": "카트라이더 정도?",
          "original_form": "카트라이더 정도?",
          "speaker_id": "2",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007918.1.9",
          "form": "그건 친구들 모여서 할 거 없으면 pc방가서 종종해요",
          "original_form": "그건 친구들 모여서 할 거 없으면 pc방가서 종종해요",
          "speaker_id": "2",
          "time": "20210822 11:52"
        },
        {
          "id": "MDRW2100007918.1.10",
          "form": "그래도 어릴때 가끔 메이플스토리나 라테일 정도는 했던것같아요ㅋㅋ",
          "original_form": "그래도 어릴때 가끔 메이플스토리나 라테일 정도는 했던것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:53"
        },
        {
          "id": "MDRW2100007918.1.11",
          "form": "스타크래프트도 그당시 핫한 게임이라 하긴했는데\r\n잘 못해서 남이 하는것만 봤던것같아요ㅋㅋ",
          "original_form": "스타크래프트도 그당시 핫한 게임이라 하긴했는데\r\n잘 못해서 남이 하는것만 봤던것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007918.1.12",
          "form": "name1님은 어떤 게임을 했었나요?",
          "original_form": "&name1&님은 어떤 게임을 했었나요?",
          "speaker_id": "2",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007918.1.13",
          "form": "저는 음 아 프린세스메이커 아세요?",
          "original_form": "저는 음 아 프린세스메이커 아세요?",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007918.1.14",
          "form": "아아 넵!",
          "original_form": "아아 넵!",
          "speaker_id": "2",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007918.1.15",
          "form": "그것도 어릴때 친구네 집에서 같이 했었어요ㅋㅋ",
          "original_form": "그것도 어릴때 친구네 집에서 같이 했었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007918.1.16",
          "form": "저는 진짜 열심히 했던거 같아요",
          "original_form": "저는 진짜 열심히 했던거 같아요",
          "speaker_id": "1",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007918.1.17",
          "form": "제가할때는 음 프린세스메이커2였어요",
          "original_form": "제가할때는 음 프린세스메이커2였어요",
          "speaker_id": "1",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007918.1.18",
          "form": "제가 아는 프린세스메이커는 하나밖에없는데 시리즈로 있나보네요",
          "original_form": "제가 아는 프린세스메이커는 하나밖에없는데 시리즈로 있나보네요",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007918.1.19",
          "form": "아아 방금 검색하고왔어요",
          "original_form": "아아 방금 검색하고왔어요",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007918.1.20",
          "form": "네넵,  저도 이거 말한거에요",
          "original_form": "네넵,  저도 이거 말한거에요",
          "speaker_id": "2",
          "time": "20210822 12:03"
        },
