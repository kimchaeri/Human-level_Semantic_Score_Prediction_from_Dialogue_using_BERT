      "utterance": [
        {
          "id": "MDRW2100020768.1.1",
          "form": "안녕하세요,  게임 좋아하시나요?",
          "original_form": "안녕하세요,  게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020768.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020768.1.3",
          "form": "게임 즐겨하지는 않습니다 음 하는건 사천성 정도 ,, ?",
          "original_form": "게임 즐겨하지는 않습니다 음 하는건 사천성 정도 ,, ?",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020768.1.4",
          "form": "게임 좋아하세요 ?",
          "original_form": "게임 좋아하세요 ?",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020768.1.5",
          "form": "저는 게임 좋아해서 많이 해요ㅎㅎ",
          "original_form": "저는 게임 좋아해서 많이 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100020768.1.6",
          "form": "어떤게임좋아하세요 ?",
          "original_form": "어떤게임좋아하세요 ?",
          "speaker_id": "2",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100020768.1.7",
          "form": "쿠키런 오븐브레이크 좋아해요",
          "original_form": "쿠키런 오븐브레이크 좋아해요",
          "speaker_id": "1",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100020768.1.8",
          "form": "아 친구들 쿠키런 많이하더라고요",
          "original_form": "아 친구들 쿠키런 많이하더라고요",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100020768.1.9",
          "form": "친구껄로 몇번해봤는데 어려웠어요",
          "original_form": "친구껄로 몇번해봤는데 어려웠어요",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100020768.1.10",
          "form": "고인물들의 게임이 되었지만 쿠키 스킨을 모으려고 하게돼요ㅠ",
          "original_form": "고인물들의 게임이 되었지만 쿠키 스킨을 모으려고 하게돼요ㅠ",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020768.1.11",
          "form": "그쵸 한번시작하면 다 모아야 그 쾌감",
          "original_form": "그쵸 한번시작하면 다 모아야 그 쾌감",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020768.1.12",
          "form": "오븐브레이크랑 킹덤은 다른건가요 ?",
          "original_form": "오븐브레이크랑 킹덤은 다른건가요 ?",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020768.1.13",
          "form": "킹덤은 약간 타이니팜이나 아이러브커피 같은 부분이 있고, 쿠키런은 달리는 게임이에요",
          "original_form": "킹덤은 약간 타이니팜이나 아이러브커피 같은 부분이 있고, 쿠키런은 달리는 게임이에요",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020768.1.14",
          "form": "아 저는 달리는게 더 적성인거같아요",
          "original_form": "아 저는 달리는게 더 적성인거같아요",
          "speaker_id": "2",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020768.1.15",
          "form": "나름 재밌어요ㅎㅎ",
          "original_form": "나름 재밌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020768.1.16",
          "form": "쿠키런말고는 어떤게임좋아하세요?",
          "original_form": "쿠키런말고는 어떤게임좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020768.1.17",
          "form": "저는 크아했었어요",
          "original_form": "저는 크아했었어요",
          "speaker_id": "2",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020768.1.18",
          "form": "그 전에는 포켓몬고 했어요",
          "original_form": "그 전에는 포켓몬고 했어요",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020768.1.19",
          "form": "아 그것도 재밌죠!",
          "original_form": "아 그것도 재밌죠!",
          "speaker_id": "2",
          "time": "20210828 13:19"
        }
      ]
