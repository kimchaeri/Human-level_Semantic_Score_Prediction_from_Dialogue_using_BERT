      "utterance": [
        {
          "id": "MDRW2100008753.1.1",
          "form": "날씨가 제법 선선해졌죠?",
          "original_form": "날씨가 제법 선선해졌죠?",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008753.1.2",
          "form": "네 바람도 불고 산책하기 딱 좋아요ㅎㅎ",
          "original_form": "네 바람도 불고 산책하기 딱 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008753.1.3",
          "form": "어젠 비오더니 오늘은 그쳤나봐요",
          "original_form": "어젠 비오더니 오늘은 그쳤나봐요",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008753.1.4",
          "form": "밖은 선선한가요?  집은 왜덥지ㅋ",
          "original_form": "밖은 선선한가요?  집은 왜덥지ㅋ",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008753.1.5",
          "form": "지금 딱 바람불어서 괜찮더라구요ㅎㅎ 안은 더워요ㅎㅎ",
          "original_form": "지금 딱 바람불어서 괜찮더라구요ㅎㅎ 안은 더워요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008753.1.6",
          "form": "그런거죠?ㅎㅎ 주말내내 방콕했드니",
          "original_form": "그런거죠?ㅎㅎ 주말내내 방콕했드니",
          "speaker_id": "2",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008753.1.7",
          "form": "이젠 태풍이 온다고 해서ㅠㅠ제습을 틀어야할거같아요ㅎㅎ",
          "original_form": "이젠 태풍이 온다고 해서ㅠㅠ제습을 틀어야할거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008753.1.8",
          "form": "이따 분리수거할겸 나가봐야겠네요",
          "original_form": "이따 분리수거할겸 나가봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008753.1.9",
          "form": "아까 뉴스보니 태풍올라온대요",
          "original_form": "아까 뉴스보니 태풍올라온대요",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008753.1.10",
          "form": "비바람분다던데 ㅠㅠ",
          "original_form": "비바람분다던데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008753.1.11",
          "form": "네 약간 그런느낌이...ㅠㅠ",
          "original_form": "네 약간 그런느낌이...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008753.1.12",
          "form": "내일은 비올거같아요",
          "original_form": "내일은 비올거같아요",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008753.1.13",
          "form": "비오고나면 가을오겠죠?",
          "original_form": "비오고나면 가을오겠죠?",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008753.1.14",
          "form": "가을이 오는건 좋은데 짧은데 아쉬워요",
          "original_form": "가을이 오는건 좋은데 짧은데 아쉬워요",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008753.1.15",
          "form": "저도요 가을을 제일 좋아하는데ㅠㅠ",
          "original_form": "저도요 가을을 제일 좋아하는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008753.1.16",
          "form": "점점 봄가을이 사라지는 느낌ㅠㅠ",
          "original_form": "점점 봄가을이 사라지는 느낌ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008753.1.17",
          "form": "트렌치코트 몇번입음  겨울이더라구요",
          "original_form": "트렌치코트 몇번입음  겨울이더라구요",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008753.1.18",
          "form": "올해는 꼭 입어야하는데ㅎㅎ",
          "original_form": "올해는 꼭 입어야하는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:11"
        }
