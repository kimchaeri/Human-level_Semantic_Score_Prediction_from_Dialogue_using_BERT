      "utterance": [
        {
          "id": "MDRW2100038776.1.1",
          "form": "혹시 동물 키워보신 적 있으세요?",
          "original_form": "혹시 동물 키워보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100038776.1.2",
          "form": "옛날에 멍멍이요",
          "original_form": "옛날에 멍멍이요",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038776.1.3",
          "form": "저도 말티즈 키웠었어요.. 예전에",
          "original_form": "저도 말티즈 키웠었어요.. 예전에",
          "speaker_id": "1",
          "time": "20210903 05:57"
        },
        {
          "id": "MDRW2100038776.1.4",
          "form": "name2님네 강아지는 어떤 종어있나요?",
          "original_form": "&name2&님네 강아지는 어떤 종어있나요?",
          "speaker_id": "1",
          "time": "20210903 05:57"
        },
        {
          "id": "MDRW2100038776.1.5",
          "form": "진돗개요ㅎㅎ 황구였어요",
          "original_form": "진돗개요ㅎㅎ 황구였어요",
          "speaker_id": "2",
          "time": "20210903 07:09"
        },
        {
          "id": "MDRW2100038776.1.6",
          "form": "진돗개라니 멋있네요",
          "original_form": "진돗개라니 멋있네요",
          "speaker_id": "1",
          "time": "20210903 07:19"
        },
        {
          "id": "MDRW2100038776.1.7",
          "form": "이름도 황구였나요?",
          "original_form": "이름도 황구였나요?",
          "speaker_id": "1",
          "time": "20210903 07:19"
        },
        {
          "id": "MDRW2100038776.1.8",
          "form": "멋있었어요 황둥이ㅋㅋ",
          "original_form": "멋있었어요 황둥이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:20"
        },
        {
          "id": "MDRW2100038776.1.9",
          "form": "황둥이.. 좋네요!",
          "original_form": "황둥이.. 좋네요!",
          "speaker_id": "1",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100038776.1.10",
          "form": "저희 강아지는 하니였어요",
          "original_form": "저희 강아지는 하니였어요",
          "speaker_id": "1",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100038776.1.11",
          "form": "달려라 하니",
          "original_form": "달려라 하니",
          "speaker_id": "1",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100038776.1.12",
          "form": "하니 진짜 귀여웠겠네요!",
          "original_form": "하니 진짜 귀여웠겠네요!",
          "speaker_id": "2",
          "time": "20210903 07:39"
        },
        {
          "id": "MDRW2100038776.1.13",
          "form": "순종이라서.. 털도 예쁘고 귀여웠죠",
          "original_form": "순종이라서.. 털도 예쁘고 귀여웠죠",
          "speaker_id": "1",
          "time": "20210903 07:41"
        },
        {
          "id": "MDRW2100038776.1.14",
          "form": "잡종도 키웠었는데.. 잡종도 귀여워요..ㅎㅎ",
          "original_form": "잡종도 키웠었는데.. 잡종도 귀여워요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 07:42"
        },
        {
          "id": "MDRW2100038776.1.15",
          "form": "아 여러마리 키우셨나봐요!",
          "original_form": "아 여러마리 키우셨나봐요!",
          "speaker_id": "2",
          "time": "20210903 08:11"
        },
        {
          "id": "MDRW2100038776.1.16",
          "form": "두마리요..ㅎㅎ",
          "original_form": "두마리요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 09:18"
        },
        {
          "id": "MDRW2100038776.1.17",
          "form": "귀엽기는 잡종이 귀엽고",
          "original_form": "귀엽기는 잡종이 귀엽고",
          "speaker_id": "1",
          "time": "20210903 09:18"
        },
        {
          "id": "MDRW2100038776.1.18",
          "form": "순종은 털이 너무 예뻐서 꾸며주기 좋았죠",
          "original_form": "순종은 털이 너무 예뻐서 꾸며주기 좋았죠",
          "speaker_id": "1",
          "time": "20210903 09:18"
        },
        {
          "id": "MDRW2100038776.1.19",
          "form": "똥개가 좋아요ㅎㅎ 귀여워요",
          "original_form": "똥개가 좋아요ㅎㅎ 귀여워요",
          "speaker_id": "2",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100038776.1.20",
          "form": "귀엽기는 똥개가 최고인 듯 합니다",
          "original_form": "귀엽기는 똥개가 최고인 듯 합니다",
          "speaker_id": "1",
