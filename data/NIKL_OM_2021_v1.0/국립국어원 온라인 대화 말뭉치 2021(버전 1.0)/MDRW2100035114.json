      "utterance": [
        {
          "id": "MDRW2100035114.1.1",
          "form": "ㅋㅋㅋㅋ참기발한거많아요",
          "original_form": "ㅋㅋㅋㅋ참기발한거많아요",
          "speaker_id": "2",
          "time": "20210829 03:02"
        },
        {
          "id": "MDRW2100035114.1.2",
          "form": "해외여행 다녀오신적 있으세용??",
          "original_form": "해외여행 다녀오신적 있으세용??",
          "speaker_id": "1",
          "time": "20210829 03:02"
        },
        {
          "id": "MDRW2100035114.1.3",
          "form": "옛날에요ㅠ",
          "original_form": "옛날에요ㅠ",
          "speaker_id": "2",
          "time": "20210829 03:02"
        },
        {
          "id": "MDRW2100035114.1.4",
          "form": "저는 일본 다녀왔어용 코로나 전에ㅠ",
          "original_form": "저는 일본 다녀왔어용 코로나 전에ㅠ",
          "speaker_id": "1",
          "time": "20210829 03:02"
        },
        {
          "id": "MDRW2100035114.1.5",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "2",
          "time": "20210829 03:02"
        },
        {
          "id": "MDRW2100035114.1.6",
          "form": "4년전에 일본 오키나와가 마지막이에요",
          "original_form": "4년전에 일본 오키나와가 마지막이에요",
          "speaker_id": "2",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.7",
          "form": "거기서 음 나는 일본음식이 안맞는구나~ 생각하고 돌아왔어용",
          "original_form": "거기서 음 나는 일본음식이 안맞는구나~ 생각하고 돌아왔어용",
          "speaker_id": "1",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.8",
          "form": "엇 저도 그맘때쯤인데ㅋㅋㅋㅋ",
          "original_form": "엇 저도 그맘때쯤인데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.9",
          "form": "저는 오사카랑 후쿠오카ㅋㅋ",
          "original_form": "저는 오사카랑 후쿠오카ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.10",
          "form": "정말요?!",
          "original_form": "정말요?!",
          "speaker_id": "2",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.11",
          "form": "대부분의 음식이 달거나 짜거나",
          "original_form": "대부분의 음식이 달거나 짜거나",
          "speaker_id": "1",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.12",
          "form": "달고짜거나",
          "original_form": "달고짜거나",
          "speaker_id": "1",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.14",
          "form": "오사카는 제 첫 해외여행지네요 ㅋㅋㅋ",
          "original_form": "오사카는 제 첫 해외여행지네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.15",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.16",
          "form": "9살인가 ㅋㅋㅋ10살인가",
          "original_form": "9살인가 ㅋㅋㅋ10살인가",
          "speaker_id": "2",
          "time": "20210829 03:03"
        },
        {
          "id": "MDRW2100035114.1.17",
          "form": "그때도 돈키호테 이런거 있었어용?",
          "original_form": "그때도 돈키호테 이런거 있었어용?",
          "speaker_id": "1",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.18",
          "form": "정말요? 전 어딜가든 음식은 맛잇엇는데",
          "original_form": "정말요? 전 어딜가든 음식은 맛잇엇는데",
          "speaker_id": "2",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.19",
          "form": "앗 부러워요ㅠㅠㅠ",
          "original_form": "앗 부러워요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.20",
          "form": "음식 안가리는것도 복이래요",
          "original_form": "음식 안가리는것도 복이래요",
          "speaker_id": "1",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.21",
          "form": "어렷을때 기억은 ㅋㅋㅋㅋ 하나밖에안나요 공항에서",
          "original_form": "어렷을때 기억은 ㅋㅋㅋㅋ 하나밖에안나요 공항에서",
          "speaker_id": "2",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.22",
          "form": "나올때사먹은 아이스크림 ㅋㅋ",
          "original_form": "나올때사먹은 아이스크림 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.23",
          "form": "우유아이스크림 진짜 맛잇엇어요ㅠㅠㅠ 소프트엿능데 ㅋㅋㅋㅋ",
          "original_form": "우유아이스크림 진짜 맛잇엇어요ㅠㅠㅠ 소프트엿능데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.24",
          "form": "ㅋㅋㅋㅋ 아이 눈높이엔 그게 제일 맛있었나봐용ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 아이 눈높이엔 그게 제일 맛있었나봐용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.25",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.26",
          "form": "제가지금 20년도 넘었는데 기억납니다 ㅋㅋㅋ",
          "original_form": "제가지금 20년도 넘었는데 기억납니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:04"
        },
        {
          "id": "MDRW2100035114.1.27",
          "form": "일본 유제품이 저도 그렇게 진하고 고소하니 맛있더라구요",
          "original_form": "일본 유제품이 저도 그렇게 진하고 고소하니 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 03:05"
        }
      ]
