      "utterance": [
        {
          "id": "MDRW2100034774.1.1",
          "form": "아 여의도에 모퉁이네라는 즉떡집이 사실 진짜 맛집이에요",
          "original_form": "아 여의도에 모퉁이네라는 즉떡집이 사실 진짜 맛집이에요",
          "speaker_id": "1",
          "time": "20210828 19:21"
        },
        {
          "id": "MDRW2100034774.1.2",
          "form": "밥종류 뭐있지..",
          "original_form": "밥종류 뭐있지..",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100034774.1.3",
          "form": "아 쌀국수좋아하세요?",
          "original_form": "아 쌀국수좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100034774.1.4",
          "form": "네! 쌀국수 좋아해요",
          "original_form": "네! 쌀국수 좋아해요",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100034774.1.5",
          "form": "서울대입구역에 안녕베트남 정말 맛있어요",
          "original_form": "서울대입구역에 안녕베트남 정말 맛있어요",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034774.1.6",
          "form": "부산 말씀해주실땐 구경거리 많이 얘기해주셨는데 전 먹을것만 말하네요...",
          "original_form": "부산 말씀해주실땐 구경거리 많이 얘기해주셨는데 전 먹을것만 말하네요...",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034774.1.7",
          "form": "서울은 아무래도 맛집이죠",
          "original_form": "서울은 아무래도 맛집이죠",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034774.1.8",
          "form": "저는 식도락이 최고라.. 너무 좋아요",
          "original_form": "저는 식도락이 최고라.. 너무 좋아요",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034774.1.9",
          "form": "ㅎㅎ다행이에요",
          "original_form": "ㅎㅎ다행이에요",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034774.1.10",
          "form": "덕분에 코로나 잠잠해지면",
          "original_form": "덕분에 코로나 잠잠해지면",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034774.1.11",
          "form": "가볼데가 많아졌어요",
          "original_form": "가볼데가 많아졌어요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034774.1.12",
          "form": "저도 코로나 잠잠해지면 부산가서 말씀해주신곳들 꼭 가볼게요!",
          "original_form": "저도 코로나 잠잠해지면 부산가서 말씀해주신곳들 꼭 가볼게요!",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034774.1.13",
          "form": "아!! 진짜 딱 하나만 추가하자면",
          "original_form": "아!! 진짜 딱 하나만 추가하자면",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034774.1.14",
          "form": "맛집은 아니고",
          "original_form": "맛집은 아니고",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034774.1.15",
          "form": "크라임씬 아세요?",
          "original_form": "크라임씬 아세요?",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034774.1.16",
          "form": "네 알아요",
          "original_form": "네 알아요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034774.1.17",
          "form": "앗 해보셨을까요?",
          "original_form": "앗 해보셨을까요?",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034774.1.18",
          "form": "해볼수있어요? 크라임씬을요?",
          "original_form": "해볼수있어요? 크라임씬을요?",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034774.1.19",
          "form": "프로그램으로 봤어요",
          "original_form": "프로그램으로 봤어요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034774.1.20",
          "form": "그 방송이랑 똑같이",
          "original_form": "그 방송이랑 똑같이",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034774.1.21",
          "form": "체험할수있는건데",
          "original_form": "체험할수있는건데",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034774.1.22",
          "form": "홍대 건대 강남에 있어요",
          "original_form": "홍대 건대 강남에 있어요",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034774.1.23",
          "form": "우와...신기해요",
          "original_form": "우와...신기해요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034774.1.24",
          "form": "각자 역할을 맡아서 연기하면서 범인을 추리하는거에요",
          "original_form": "각자 역할을 맡아서 연기하면서 범인을 추리하는거에요",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034774.1.25",
          "form": "정말 재밌어요!",
          "original_form": "정말 재밌어요!",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034774.1.26",
          "form": "재밌겠어요 감사해요",
          "original_form": "재밌겠어요 감사해요",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034774.1.27",
          "form": "이제 맛집들이 코로나를 잘이겨내길 빌면되겠네요",
          "original_form": "이제 맛집들이 코로나를 잘이겨내길 빌면되겠네요",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034774.1.28",
          "form": "ㅎㅎ 맞아요",
          "original_form": "ㅎㅎ 맞아요",
          "speaker_id": "1",
          "time": "20210828 19:48"
        }
      ]
    }
  ]
}
