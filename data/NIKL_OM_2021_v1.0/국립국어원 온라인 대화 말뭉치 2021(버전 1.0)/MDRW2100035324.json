      "utterance": [
        {
          "id": "MDRW2100035324.1.1",
          "form": "오늘 날씨 좋나요?",
          "original_form": "오늘 날씨 좋나요?",
          "speaker_id": "1",
          "time": "20210829 14:49"
        },
        {
          "id": "MDRW2100035324.1.2",
          "form": "비가 올랑말랑 흐릿흐릿해요!",
          "original_form": "비가 올랑말랑 흐릿흐릿해요!",
          "speaker_id": "2",
          "time": "20210829 14:52"
        },
        {
          "id": "MDRW2100035324.1.3",
          "form": "계신 곳은 날씨가 좋나요?",
          "original_form": "계신 곳은 날씨가 좋나요?",
          "speaker_id": "2",
          "time": "20210829 14:52"
        },
        {
          "id": "MDRW2100035324.1.4",
          "form": "네 여긴 오늘 비온다고했었는데 안왔어욬ㅋㅋ",
          "original_form": "네 여긴 오늘 비온다고했었는데 안왔어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100035324.1.5",
          "form": "날씨예보가 잘 안맞네요 ㅋㅋㅋ",
          "original_form": "날씨예보가 잘 안맞네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035324.1.6",
          "form": "여기는 하루종일 오락가락 왔다 안 왔다 마음대로였어요 ㅋㅋㅋ",
          "original_form": "여기는 하루종일 오락가락 왔다 안 왔다 마음대로였어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100035324.1.7",
          "form": "ㅋㄱㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100035324.1.8",
          "form": "그래도 내일은 안오면 좋을텐데ㅜㅠ",
          "original_form": "그래도 내일은 안오면 좋을텐데ㅜㅠ",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100035324.1.9",
          "form": "계속 우산들고다니려니까 불편해요",
          "original_form": "계속 우산들고다니려니까 불편해요",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100035324.1.10",
          "form": "맞아요 ㅋㅋㅋ 작은 것도 들고 다니면 무게가 꽤 느껴져서 불편해요ㅠㅠ",
          "original_form": "맞아요 ㅋㅋㅋ 작은 것도 들고 다니면 무게가 꽤 느껴져서 불편해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 19:09"
        },
        {
          "id": "MDRW2100035324.1.11",
          "form": "오늘은 또 하루종일 쨍쨍했어요!",
          "original_form": "오늘은 또 하루종일 쨍쨍했어요!",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100035324.1.12",
          "form": "날씨 괜찮았나요?",
          "original_form": "날씨 괜찮았나요?",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100035324.1.13",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100035324.1.14",
          "form": "여기도요!!!",
          "original_form": "여기도요!!!",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100035324.1.15",
          "form": "근데 내일부터 목요일까지 또 비소식있던데요..ㅎ",
          "original_form": "근데 내일부터 목요일까지 또 비소식있던데요..ㅎ",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100035324.1.16",
          "form": "이번주까지 우산은 가방에서 못나오겠어요..",
          "original_form": "이번주까지 우산은 가방에서 못나오겠어요..",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100035324.1.17",
          "form": "어깨 빠지게 생겼네요ㅠㅠ",
          "original_form": "어깨 빠지게 생겼네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100035324.1.18",
          "form": "비 오는 날은 어딜가도 그 꿉꿉한 발냄새가 나서 집 밖으로 나가기 싫어요ㅠㅠ",
          "original_form": "비 오는 날은 어딜가도 그 꿉꿉한 발냄새가 나서 집 밖으로 나가기 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100035324.1.19",
          "form": "악 맞아요",
          "original_form": "악 맞아요",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100035324.1.20",
          "form": "신발도 젖고ㅠㅜㅠ",
          "original_form": "신발도 젖고ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100035324.1.21",
          "form": "비오는날 너무 힘들죠",
          "original_form": "비오는날 너무 힘들죠",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100035324.1.22",
          "form": "특히 대중교통 이용해야되면 헬이에요,,",
          "original_form": "특히 대중교통 이용해야되면 헬이에요,,",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100035324.1.23",
          "form": "맞아요ㅠㅠ 대중교통 타서 다리에 우산 빗물 묻을 때도 너무 싫어요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ 대중교통 타서 다리에 우산 빗물 묻을 때도 너무 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100035324.1.24",
          "form": "오늘 계신 곳에 비 왔나요??",
          "original_form": "오늘 계신 곳에 비 왔나요??",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100035324.1.25",
          "form": "네네 여긴 호우주의보였어요ㅜㅜㅠ",
          "original_form": "네네 여긴 호우주의보였어요ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100035324.1.26",
          "form": "아이고ㅠ 오늘 힘드셨겠네요ㅠㅠ",
          "original_form": "아이고ㅠ 오늘 힘드셨겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100035324.1.27",
          "form": "여기는 오늘 비 온다더니 오지도않고 습하기만했어요 ㅋㅋㅋㅋ",
          "original_form": "여기는 오늘 비 온다더니 오지도않고 습하기만했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100035324.1.28",
          "form": "다행히 나갈일은 많지않았네요ㅎㅎ",
          "original_form": "다행히 나갈일은 많지않았네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100035324.1.29",
          "form": "비안맞으신것도 다행이구요ㅎㅎㅎ",
          "original_form": "비안맞으신것도 다행이구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:09"
        }
      ]
    }
