      "utterance": [
        {
          "id": "MDRW2100045875.1.1",
          "form": "블라디보스톡 혹시 가보셨나요?",
          "original_form": "블라디보스톡 혹시 가보셨나요?",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045875.1.2",
          "form": "전 거기 가려고 하다가 코로나 터졌어요 ㅋㅋ",
          "original_form": "전 거기 가려고 하다가 코로나 터졌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045875.1.3",
          "form": "안가봤어요!",
          "original_form": "안가봤어요!",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045875.1.4",
          "form": "가서 랍스터먹고싶어요 ㅋㅋㅋ",
          "original_form": "가서 랍스터먹고싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045875.1.5",
          "form": "거기가면 진짜 내내 랍스터만 먹고와야한다면서요",
          "original_form": "거기가면 진짜 내내 랍스터만 먹고와야한다면서요",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045875.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045875.1.7",
          "form": "레알 좋을거같아욬ㅋㅋ",
          "original_form": "레알 좋을거같아욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045875.1.8",
          "form": "거긴 진짜 작아서",
          "original_form": "거긴 진짜 작아서",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045875.1.9",
          "form": "2박3일정도만 있어도 된다고 들은 것 같아요",
          "original_form": "2박3일정도만 있어도 된다고 들은 것 같아요",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045875.1.10",
          "form": "근데 제가 추위를 못 견뎌서 언제 갈수 있을런지 모르겠어요",
          "original_form": "근데 제가 추위를 못 견뎌서 언제 갈수 있을런지 모르겠어요",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045875.1.11",
          "form": "블라디보스톡은 먹는거 말곤 뭐가 유명해요~~?",
          "original_form": "블라디보스톡은 먹는거 말곤 뭐가 유명해요~~?",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045875.1.12",
          "form": "여름에 가면 괜찮대요!!",
          "original_form": "여름에 가면 괜찮대요!!",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045875.1.13",
          "form": "아하 ㅎㅂ",
          "original_form": "아하 ㅎㅂ",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045875.1.14",
          "form": "내년 여름엔",
          "original_form": "내년 여름엔",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045875.1.15",
          "form": "비행기탈수있겠죠...?",
          "original_form": "비행기탈수있겠죠...?",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045875.1.16",
          "form": "하 제발",
          "original_form": "하 제발",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045875.1.17",
          "form": "음 그냥 가까이 있는 유럽이라 많이들 가는 것 같아요",
          "original_form": "음 그냥 가까이 있는 유럽이라 많이들 가는 것 같아요",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045875.1.18",
          "form": "저는 가서 발레를 보고오고싶어요!",
          "original_form": "저는 가서 발레를 보고오고싶어요!",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045875.1.19",
          "form": "ㅎㅎ멋진 계획이에요",
          "original_form": "ㅎㅎ멋진 계획이에요",
          "speaker_id": "2",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045875.1.20",
          "form": "공연은 눈으로 보면 감동이 남다르더라구요",
          "original_form": "공연은 눈으로 보면 감동이 남다르더라구요",
          "speaker_id": "2",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045875.1.21",
          "form": "제가 평소에 뮤지컬 보는걸 너무 좋아하는데",
          "original_form": "제가 평소에 뮤지컬 보는걸 너무 좋아하는데",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045875.1.22",
          "form": "와 그러시구나 네",
          "original_form": "와 그러시구나 네",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045875.1.23",
          "form": "니진스키 관련 뮤지컬을 봤었거든요",
          "original_form": "니진스키 관련 뮤지컬을 봤었거든요",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045875.1.24",
          "form": "마린스키가 블라디보스톡에 있다고 하기에",
          "original_form": "마린스키가 블라디보스톡에 있다고 하기에",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045875.1.25",
          "form": "마린스키에 올라오는 발레를 한번 봐보고 싶어요 ㅎㅎ",
          "original_form": "마린스키에 올라오는 발레를 한번 봐보고 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045875.1.26",
          "form": "발레 문외한인데",
          "original_form": "발레 문외한인데",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045875.1.27",
          "form": "함 찾아봐야겠어요 ㅎㅎ",
          "original_form": "함 찾아봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045875.1.28",
          "form": "ㅋㅋㅋㅌ 너무 궁금한데",
          "original_form": "ㅋㅋㅋㅌ 너무 궁금한데",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045875.1.29",
