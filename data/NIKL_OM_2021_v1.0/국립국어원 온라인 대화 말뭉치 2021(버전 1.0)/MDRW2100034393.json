      "utterance": [
        {
          "id": "MDRW2100034393.1.1",
          "form": "뭐 구매 하고 싶으신 물건 있으세요?",
          "original_form": "뭐 구매 하고 싶으신 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210828 03:09"
        },
        {
          "id": "MDRW2100034393.1.2",
          "form": "전 링피트가 요즘 탐나요",
          "original_form": "전 링피트가 요즘 탐나요",
          "speaker_id": "1",
          "time": "20210828 03:09"
        },
        {
          "id": "MDRW2100034393.1.3",
          "form": "와오 꿈의물건ㅜㅜ",
          "original_form": "와오 꿈의물건ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 03:10"
        },
        {
          "id": "MDRW2100034393.1.4",
          "form": "링피트사도 할시간없어요",
          "original_form": "링피트사도 할시간없어요",
          "speaker_id": "2",
          "time": "20210828 03:10"
        },
        {
          "id": "MDRW2100034393.1.5",
          "form": "저는 버티컬마우스사서 잘쓰고있어요",
          "original_form": "저는 버티컬마우스사서 잘쓰고있어요",
          "speaker_id": "2",
          "time": "20210828 03:10"
        },
        {
          "id": "MDRW2100034393.1.6",
          "form": "ㅠㅠㅠ만이 바쁘신가 봐요??",
          "original_form": "ㅠㅠㅠ만이 바쁘신가 봐요??",
          "speaker_id": "1",
          "time": "20210828 03:10"
        },
        {
          "id": "MDRW2100034393.1.7",
          "form": "버티컬 마우스는 어떤 마우스인가요",
          "original_form": "버티컬 마우스는 어떤 마우스인가요",
          "speaker_id": "1",
          "time": "20210828 03:10"
        },
        {
          "id": "MDRW2100034393.1.8",
          "form": "그 혹시 인체공학 마우스?",
          "original_form": "그 혹시 인체공학 마우스?",
          "speaker_id": "1",
          "time": "20210828 03:11"
        },
        {
          "id": "MDRW2100034393.1.9",
          "form": "그거인가요?",
          "original_form": "그거인가요?",
          "speaker_id": "1",
          "time": "20210828 03:11"
        },
        {
          "id": "MDRW2100034393.1.10",
          "form": "네 그런거에요",
          "original_form": "네 그런거에요",
          "speaker_id": "2",
          "time": "20210828 03:12"
        },
        {
          "id": "MDRW2100034393.1.11",
          "form": "장시간 컴쓰는사람은 필수더라구요",
          "original_form": "장시간 컴쓰는사람은 필수더라구요",
          "speaker_id": "2",
          "time": "20210828 03:12"
        },
        {
          "id": "MDRW2100034393.1.12",
          "form": "오오오 안그래도 제친구들도",
          "original_form": "오오오 안그래도 제친구들도",
          "speaker_id": "1",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100034393.1.13",
          "form": "그걸로 바꾸곤 되게 편하더라 그러더라구요 ㅎㅎㅎ",
          "original_form": "그걸로 바꾸곤 되게 편하더라 그러더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100034393.1.14",
          "form": "전 친구꺼한번 써봤는데 뭔가 적응이 안돼서 안샀어요",
          "original_form": "전 친구꺼한번 써봤는데 뭔가 적응이 안돼서 안샀어요",
          "speaker_id": "1",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100034393.1.15",
          "form": "쓸만하세요?",
          "original_form": "쓸만하세요?",
          "speaker_id": "1",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100034393.1.16",
          "form": "오오 그러시군요",
          "original_form": "오오 그러시군요",
          "speaker_id": "2",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100034393.1.17",
          "form": "익숙해져야죠 아프면 늦어요",
          "original_form": "익숙해져야죠 아프면 늦어요",
          "speaker_id": "2",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100034393.1.18",
          "form": "헉... 무서운 말이네요",
          "original_form": "헉... 무서운 말이네요",
          "speaker_id": "1",
          "time": "20210828 03:14"
        },
        {
          "id": "MDRW2100034393.1.19",
          "form": "아프면 늦다는 말....",
          "original_form": "아프면 늦다는 말....",
          "speaker_id": "1",
          "time": "20210828 03:14"
        },
        {
          "id": "MDRW2100034393.1.20",
          "form": "저도 사실 링피트를 살까했던게",
          "original_form": "저도 사실 링피트를 살까했던게",
          "speaker_id": "1",
          "time": "20210828 03:14"
        },
        {
          "id": "MDRW2100034393.1.21",
          "form": "몸이 슬슬 늙어가는게 느껴져서 그랫어요 흑",
          "original_form": "몸이 슬슬 늙어가는게 느껴져서 그랫어요 흑",
          "speaker_id": "1",
          "time": "20210828 03:14"
        },
        {
          "id": "MDRW2100034393.1.22",
          "form": "건강땜에다들사시죠ㅜㅜ",
          "original_form": "건강땜에다들사시죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 03:14"
        },
        {
          "id": "MDRW2100034393.1.23",
          "form": "닌텐도는있어요?",
          "original_form": "닌텐도는있어요?",
          "speaker_id": "2",
          "time": "20210828 03:14"
        },
        {
          "id": "MDRW2100034393.1.24",
          "form": "맞아요ㅠ... 건강때문에",
          "original_form": "맞아요ㅠ... 건강때문에",
          "speaker_id": "1",
          "time": "20210828 03:15"
        },
        {
          "id": "MDRW2100034393.1.25",
          "form": "네네 동숲하고싶어서 닌텐도 삿엇어요!!",
          "original_form": "네네 동숲하고싶어서 닌텐도 삿엇어요!!",
          "speaker_id": "1",
          "time": "20210828 03:15"
        },
        {
          "id": "MDRW2100034393.1.26",
          "form": "근데 몇번 하고는 잘 안하게돼요 ㅋㅋㅋ",
          "original_form": "근데 몇번 하고는 잘 안하게돼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 03:15"
        },
        {
          "id": "MDRW2100034393.1.27",
          "form": "ㅋㅋ아 동숲재밌지않나요 전 일부러안샀어요ㅜ",
          "original_form": "ㅋㅋ아 동숲재밌지않나요 전 일부러안샀어요ㅜ",
          "speaker_id": "2",
          "time": "20210828 03:15"
        },
        {
          "id": "MDRW2100034393.1.28",
          "form": "전..중독이 심하거든욧",
          "original_form": "전..중독이 심하거든욧",
          "speaker_id": "2",
          "time": "20210828 03:16"
        },
        {
          "id": "MDRW2100034393.1.29",
          "form": "앗ㅋㅋㅋㅋㅋ 중독이 심하시군요 원래 게임같은거 하시면 푹 빠지시는 편인가요?",
          "original_form": "앗ㅋㅋㅋㅋㅋ 중독이 심하시군요 원래 게임같은거 하시면 푹 빠지시는 편인가요?",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100034393.1.30",
          "form": "동숲은 재밌는데 한 한달? 그정도 하면 할만큼 했다는 생각이 드는 것 같아요!ㅋㅋㅋ",
          "original_form": "동숲은 재밌는데 한 한달? 그정도 하면 할만큼 했다는 생각이 드는 것 같아요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100034393.1.31",
          "form": "오오 적응이 빠르시네요",
          "original_form": "오오 적응이 빠르시네요",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100034393.1.32",
          "form": "제친구는 푹빠져살던데 취향이아니신가봐요",
          "original_form": "제친구는 푹빠져살던데 취향이아니신가봐요",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100034393.1.33",
          "form": "네 좀 취향에는 덜 맞았나봐요!ㅋㅋㅋ",
          "original_form": "네 좀 취향에는 덜 맞았나봐요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100034393.1.34",
          "form": "아기자기하고 꾸미고",
          "original_form": "아기자기하고 꾸미고",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100034393.1.35",
          "form": "이러는거랑은 거리가 멀어서 ㅎㅎㅎ",
          "original_form": "이러는거랑은 거리가 멀어서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100034393.1.36",
          "form": "아 저는 그런거 좋아하는데 개미처럼하는거 ㅋㅋㅋ",
          "original_form": "아 저는 그런거 좋아하는데 개미처럼하는거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
