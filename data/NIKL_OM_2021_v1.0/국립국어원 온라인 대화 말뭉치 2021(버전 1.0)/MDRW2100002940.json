      "utterance": [
        {
          "id": "MDRW2100002940.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210821 13:52"
        },
        {
          "id": "MDRW2100002940.1.2",
          "form": "네 여행 좋아하죠!",
          "original_form": "네 여행 좋아하죠!",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100002940.1.3",
          "form": "국내여행 위주로 몇번 다녀본적도 있죠 ㅎㅎ",
          "original_form": "국내여행 위주로 몇번 다녀본적도 있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100002940.1.4",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100002940.1.5",
          "form": "어디 다녀보신적 있으신가요?",
          "original_form": "어디 다녀보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100002940.1.6",
          "form": "얼마전 남해 갔어요",
          "original_form": "얼마전 남해 갔어요",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100002940.1.7",
          "form": "오오 부럽네요!",
          "original_form": "오오 부럽네요!",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100002940.1.8",
          "form": "남해도 한번 가보고 싶긴해요!",
          "original_form": "남해도 한번 가보고 싶긴해요!",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100002940.1.9",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100002940.1.10",
          "form": "저는 동해를 좀 오래전에 가보긴했는데",
          "original_form": "저는 동해를 좀 오래전에 가보긴했는데",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100002940.1.11",
          "form": "바다보는게 참 좋더라고요!",
          "original_form": "바다보는게 참 좋더라고요!",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100002940.1.12",
          "form": "맘이 편하죠",
          "original_form": "맘이 편하죠",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100002940.1.13",
          "form": "그쵸 바다 소리들으면",
          "original_form": "그쵸 바다 소리들으면",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100002940.1.14",
          "form": "마음이 편안해지는게 좋더라고요",
          "original_form": "마음이 편안해지는게 좋더라고요",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100002940.1.15",
          "form": "그래서 계곡도 여름되면 자주 놀러가곤 했던거같아요",
          "original_form": "그래서 계곡도 여름되면 자주 놀러가곤 했던거같아요",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100002940.1.16",
          "form": "물이 좋죠",
          "original_form": "물이 좋죠",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100002940.1.17",
          "form": "그렇죠 그 물 흐르는 소리가",
          "original_form": "그렇죠 그 물 흐르는 소리가",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100002940.1.18",
          "form": "마음을 편안하게 해주죠 ㅎㅎ",
          "original_form": "마음을 편안하게 해주죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100002940.1.19",
          "form": "그리고 계곡이나 바다만의 각각 매력도 있어서 좋아요!",
          "original_form": "그리고 계곡이나 바다만의 각각 매력도 있어서 좋아요!",
          "speaker_id": "2",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100002940.1.20",
          "form": "색다르죠",
          "original_form": "색다르죠",
          "speaker_id": "1",
          "time": "20210821 16:51"
        }
      ]
