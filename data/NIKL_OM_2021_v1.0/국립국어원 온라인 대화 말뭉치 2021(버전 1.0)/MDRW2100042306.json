      "utterance": [
        {
          "id": "MDRW2100042306.1.1",
          "form": "나는 오늘 미트볼 먹었는데 역시 학교 미트볼은 끔찍해",
          "original_form": "나는 오늘 미트볼 먹었는데 역시 학교 미트볼은 끔찍해",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042306.1.2",
          "form": "오늘 연구실에서 자허블 사줘서",
          "original_form": "오늘 연구실에서 자허블 사줘서",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042306.1.3",
          "form": "언닌 자허블 마셨는뎅 히히",
          "original_form": "언닌 자허블 마셨는뎅 히히",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042306.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042306.1.5",
          "form": "나 어제 짜장면 먹으러 가서 탕슉도 먹었는데",
          "original_form": "나 어제 짜장면 먹으러 가서 탕슉도 먹었는데",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042306.1.6",
          "form": "아빠가 무슨 회사 급식보다 맛있다고 하는고야",
          "original_form": "아빠가 무슨 회사 급식보다 맛있다고 하는고야",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042306.1.7",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042306.1.8",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042306.1.9",
          "form": "짜장먄 먹고 싶다",
          "original_form": "짜장먄 먹고 싶다",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042306.1.10",
          "form": "내가 보낸거 보내졌어?",
          "original_form": "내가 보낸거 보내졌어?",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042306.1.11",
          "form": "스벅에서 음료 다같이 주문하는데",
          "original_form": "스벅에서 음료 다같이 주문하는데",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042306.1.12",
          "form": "아무튼 당연히 급식보다 맛있지!",
          "original_form": "아무튼 당연히 급식보다 맛있지!",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042306.1.13",
          "form": "오 스벅 자허블이엇구나",
          "original_form": "오 스벅 자허블이엇구나",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042306.1.14",
          "form": "암튼 내 옆에 잇던 분은 차이티라떼를 시켯어",
          "original_form": "암튼 내 옆에 잇던 분은 차이티라떼를 시켯어",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042306.1.15",
          "form": "따뜻한 걸로!",
          "original_form": "따뜻한 걸로!",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042306.1.16",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042306.1.17",
          "form": "나만 그런 음료를 마시는 줄 알았는데",
          "original_form": "나만 그런 음료를 마시는 줄 알았는데",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042306.1.18",
          "form": "나랑 취향이 비슷한가봐",
          "original_form": "나랑 취향이 비슷한가봐",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042306.1.19",
          "form": "그런가봥",
          "original_form": "그런가봥",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042306.1.20",
          "form": "언니도 차이티라떼 좋아하고 티 라떼는 뮤조건 따뜻하게 마셔",
          "original_form": "언니도 차이티라떼 좋아하고 티 라떼는 뮤조건 따뜻하게 마셔",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042306.1.21",
          "form": "근데 웃긴 게 스벅에 원래 얼그레이티라떼 팔았는데",
          "original_form": "근데 웃긴 게 스벅에 원래 얼그레이티라떼 팔았는데",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.22",
          "form": "이게 약간 잇엇다가 사라졋다가 하는 메뉴라서",
          "original_form": "이게 약간 잇엇다가 사라졋다가 하는 메뉴라서",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.23",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.24",
          "form": "무슨 카페가 밀크티를 안팔아!",
          "original_form": "무슨 카페가 밀크티를 안팔아!",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.25",
          "form": "어이없지",
          "original_form": "어이없지",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.26",
          "form": "그러겡",
          "original_form": "그러겡",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.27",
          "form": "언닌 얼그레이 티라떼 마실 때",
          "original_form": "언닌 얼그레이 티라떼 마실 때",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.28",
          "form": "비인기 메뉴인가?",
          "original_form": "비인기 메뉴인가?",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.29",
          "form": "물적게 티많이 해서",
          "original_form": "물적게 티많이 해서",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.30",
          "form": "마셔",
          "original_form": "마셔",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.31",
          "form": "에이설마",
          "original_form": "에이설마",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042306.1.32",
          "form": "대박~",
          "original_form": "대박~",
          "speaker_id": "1",
          "time": "20210906 21:34"
        }
      ]
    }
