      "utterance": [
        {
          "id": "MDRW2100004580.1.1",
          "form": "운동 하시는거 있나요?",
          "original_form": "운동 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004580.1.2",
          "form": "저는 필라테스 하고있어요",
          "original_form": "저는 필라테스 하고있어요",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004580.1.3",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004580.1.4",
          "form": "했었는데 요즘은 중단중이요ㅎ",
          "original_form": "했었는데 요즘은 중단중이요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004580.1.5",
          "form": "필라테스 자세교정되고 좋다던데요",
          "original_form": "필라테스 자세교정되고 좋다던데요",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004580.1.6",
          "form": "코로나땜에 운동가기도 힘들죠",
          "original_form": "코로나땜에 운동가기도 힘들죠",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004580.1.7",
          "form": "저는 허리아파서 시작했는데 좋아요",
          "original_form": "저는 허리아파서 시작했는데 좋아요",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004580.1.8",
          "form": "코어근육도 생기고 좋대요",
          "original_form": "코어근육도 생기고 좋대요",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004580.1.9",
          "form": "맞아요  코어강화에 필라가 짱인거같아요",
          "original_form": "맞아요  코어강화에 필라가 짱인거같아요",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004580.1.10",
          "form": "해보면 코어에 힘이 있는사람 없는사람 표시가 나요",
          "original_form": "해보면 코어에 힘이 있는사람 없는사람 표시가 나요",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004580.1.11",
          "form": "저도 잠깐 했었어요",
          "original_form": "저도 잠깐 했었어요",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004580.1.12",
          "form": "살은 안빠지는데 근력운동이라근가",
          "original_form": "살은 안빠지는데 근력운동이라근가",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004580.1.13",
          "form": "네맞아요",
          "original_form": "네맞아요",
          "speaker_id": "2",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004580.1.14",
          "form": "살은 확실히 식단조절같아요",
          "original_form": "살은 확실히 식단조절같아요",
          "speaker_id": "2",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004580.1.15",
          "form": "맞아요 운동과 식단병행",
          "original_form": "맞아요 운동과 식단병행",
          "speaker_id": "1",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004580.1.16",
          "form": "식단안하고 운동만하니 별 효과가 없더라구요",
          "original_form": "식단안하고 운동만하니 별 효과가 없더라구요",
          "speaker_id": "2",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004580.1.17",
          "form": "확실히 먹는게 중요해요",
          "original_form": "확실히 먹는게 중요해요",
          "speaker_id": "1",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004580.1.18",
          "form": "홍지민인가? 살빼서나오던데 와 많이 뺏더라구요",
          "original_form": "홍지민인가? 살빼서나오던데 와 많이 뺏더라구요",
          "speaker_id": "2",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004580.1.19",
          "form": "엄청뺐던데요",
          "original_form": "엄청뺐던데요",
          "speaker_id": "1",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004580.1.20",
          "form": "식단많이 조절했더라구요",
          "original_form": "식단많이 조절했더라구요",
          "speaker_id": "1",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004580.1.21",
          "form": "근데 저는 십키로 이상 뺀줄알앟는데그건 아니더라구요",
          "original_form": "근데 저는 십키로 이상 뺀줄알앟는데그건 아니더라구요",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004580.1.22",
          "form": "그래요? 엄청뺀줄알았는데",
          "original_form": "그래요? 엄청뺀줄알았는데",
