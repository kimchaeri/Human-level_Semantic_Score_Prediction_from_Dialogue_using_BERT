      "utterance": [
        {
          "id": "MDRW2100003668.1.1",
          "form": "건강은 괜찮으신가요?",
          "original_form": "건강은 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003668.1.2",
          "form": "전 요새 비만이되서",
          "original_form": "전 요새 비만이되서",
          "speaker_id": "2",
          "time": "20210821 10:21"
        },
        {
          "id": "MDRW2100003668.1.3",
          "form": "살빼야해요",
          "original_form": "살빼야해요",
          "speaker_id": "2",
          "time": "20210821 10:21"
        },
        {
          "id": "MDRW2100003668.1.4",
          "form": "치아도 별로예요 ㅜㅜ",
          "original_form": "치아도 별로예요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 10:21"
        },
        {
          "id": "MDRW2100003668.1.5",
          "form": "아 저도 다이어트하고 있구\r\n치아보험넣고 2년뒤에 싹 갈아 엎었어요ㅋㅋㅋ",
          "original_form": "아 저도 다이어트하고 있구\r\n치아보험넣고 2년뒤에 싹 갈아 엎었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 10:22"
        },
        {
          "id": "MDRW2100003668.1.6",
          "form": "전 보험을 안해서",
          "original_form": "전 보험을 안해서",
          "speaker_id": "2",
          "time": "20210821 10:22"
        },
        {
          "id": "MDRW2100003668.1.7",
          "form": "임플란트 하다 수백 깨졌어요 ㅜㅜ",
          "original_form": "임플란트 하다 수백 깨졌어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 10:22"
        },
        {
          "id": "MDRW2100003668.1.8",
          "form": "지금도 병원가고있어요 치과",
          "original_form": "지금도 병원가고있어요 치과",
          "speaker_id": "2",
          "time": "20210821 10:22"
        },
        {
          "id": "MDRW2100003668.1.9",
          "form": "치아건강이 오복중 하나라는데 ㅜㅜ",
          "original_form": "치아건강이 오복중 하나라는데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003668.1.10",
          "form": "일부러 이 안좋다는거 알고 넣고 2년뒤에 다시 갔어요ㅋㅋㅋ\r\n조금이라도 돈 적게드려구요ㅜㅜ",
          "original_form": "일부러 이 안좋다는거 알고 넣고 2년뒤에 다시 갔어요ㅋㅋㅋ\r\n조금이라도 돈 적게드려구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003668.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003668.1.12",
          "form": "저도 그럴걸 넘 과신했어요",
          "original_form": "저도 그럴걸 넘 과신했어요",
          "speaker_id": "2",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003668.1.13",
          "form": "치아가 아픈게 정말 엄청난 고통이더라구요",
          "original_form": "치아가 아픈게 정말 엄청난 고통이더라구요",
          "speaker_id": "1",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003668.1.14",
          "form": "임플란트 진짜 힘들어요",
          "original_form": "임플란트 진짜 힘들어요",
          "speaker_id": "2",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003668.1.15",
          "form": "네 진짜 매번 무서워요",
          "original_form": "네 진짜 매번 무서워요",
          "speaker_id": "2",
          "time": "20210821 10:24"
        },
        {
          "id": "MDRW2100003668.1.16",
          "form": "아직 임플란트는 안해봤는데 아프던가요?!",
          "original_form": "아직 임플란트는 안해봤는데 아프던가요?!",
          "speaker_id": "1",
          "time": "20210821 10:24"
        },
        {
          "id": "MDRW2100003668.1.17",
          "form": "네 아프고 일단 시간이 오래 걸려요",
          "original_form": "네 아프고 일단 시간이 오래 걸려요",
          "speaker_id": "2",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100003668.1.18",
          "form": "근데 요샌 좀 안아프게 절해요",
          "original_form": "근데 요샌 좀 안아프게 절해요",
          "speaker_id": "2",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100003668.1.19",
          "form": "잘해요",
          "original_form": "잘해요",
          "speaker_id": "2",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100003668.1.20",
          "form": "아직도 어색해요",
          "original_form": "아직도 어색해요",
          "speaker_id": "2",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100003668.1.21",
          "form": "6달지났어요",
          "original_form": "6달지났어요",
          "speaker_id": "2",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100003668.1.22",
          "form": "아하! 저도 조심할려고 노력하는데 치아도 타고 나는거 같애요ㅠ",
          "original_form": "아하! 저도 조심할려고 노력하는데 치아도 타고 나는거 같애요ㅠ",
          "speaker_id": "1",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100003668.1.23",
          "form": "맞아요 유전이 반이예요",
          "original_form": "맞아요 유전이 반이예요",
          "speaker_id": "2",
          "time": "20210821 10:26"
        },
        {
          "id": "MDRW2100003668.1.24",
          "form": "아버지도 임플란트하시다가 결국 틀니까지 하셨는데,.\r\n걱정이 많이 됩니다ㅠㅠ",
          "original_form": "아버지도 임플란트하시다가 결국 틀니까지 하셨는데,.\r\n걱정이 많이 됩니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 10:28"
        },
        {
          "id": "MDRW2100003668.1.25",
          "form": "저희딸은 치아가 태어날때 문제가 있어서",
          "original_form": "저희딸은 치아가 태어날때 문제가 있어서",
          "speaker_id": "2",
          "time": "20210821 10:28"
        },
        {
          "id": "MDRW2100003668.1.26",
          "form": "녹아 내려요",
          "original_form": "녹아 내려요",
          "speaker_id": "2",
          "time": "20210821 10:28"
        }
      ]
    }
  ]
