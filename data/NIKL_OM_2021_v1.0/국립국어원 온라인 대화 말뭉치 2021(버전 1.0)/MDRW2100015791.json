      "utterance": [
        {
          "id": "MDRW2100015791.1.1",
          "form": "요즘 날이 선선해서 좋아요",
          "original_form": "요즘 날이 선선해서 좋아요",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015791.1.2",
          "form": "습하고 더워질 줄 알았는데 습하고 시원해졌어요ㅋㅋ",
          "original_form": "습하고 더워질 줄 알았는데 습하고 시원해졌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100015791.1.3",
          "form": "아 맞아요 ㅋㅋㅋ 습한거 제일 싫어하는데",
          "original_form": "아 맞아요 ㅋㅋㅋ 습한거 제일 싫어하는데",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015791.1.4",
          "form": "약간 더 덥게 느껴지지않나요 습하면 ??",
          "original_form": "약간 더 덥게 느껴지지않나요 습하면 ??",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015791.1.5",
          "form": "맞아요! 근데 요즘은 습함을 뚫고 찬기가 느껴져서 신기해요ㅋㅌ",
          "original_form": "맞아요! 근데 요즘은 습함을 뚫고 찬기가 느껴져서 신기해요ㅋㅌ",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100015791.1.6",
          "form": "맞아요 ㅎㅎ 저는 원래 여름보단 겨울을 좋아해서",
          "original_form": "맞아요 ㅎㅎ 저는 원래 여름보단 겨울을 좋아해서",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100015791.1.7",
          "form": "빨리 왔으면 좋겠어요!",
          "original_form": "빨리 왔으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100015791.1.8",
          "form": "저두 가을, 겨울을 더 좋아해요ㅎㅎ",
          "original_form": "저두 가을, 겨울을 더 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100015791.1.9",
          "form": "저두요 ㅎㅎ 땀나는걸 싫어해서 ㅠㅠㅠ",
          "original_form": "저두요 ㅎㅎ 땀나는걸 싫어해서 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100015791.1.10",
          "form": "이유가 따로 있으신가요 !",
          "original_form": "이유가 따로 있으신가요 !",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100015791.1.11",
          "form": "습하고 더운 게 싫어서요! 선선하고 시원한 날씨가 좋네요ㅎㅎ",
          "original_form": "습하고 더운 게 싫어서요! 선선하고 시원한 날씨가 좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100015791.1.12",
          "form": "저두요 ㅎㅎㅎ 차라리 이불을 덮는게 나아요 !",
          "original_form": "저두요 ㅎㅎㅎ 차라리 이불을 덮는게 나아요 !",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100015791.1.13",
          "form": "근데 또 막상 겨울되면 너무 춥다고 하겠죠 ㅋㅋㅋ",
          "original_form": "근데 또 막상 겨울되면 너무 춥다고 하겠죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100015791.1.14",
          "form": "겨울되면 봄을 바랄 것 같아요ㅋㅋㅋ",
          "original_form": "겨울되면 봄을 바랄 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100015791.1.15",
          "form": "봄도 좋긴해요 ㅎㅎ 뭔가 놀러가기 딱 좋지 않나요?",
          "original_form": "봄도 좋긴해요 ㅎㅎ 뭔가 놀러가기 딱 좋지 않나요?",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100015791.1.16",
          "form": "비도 잘 안오니까 ㅎㅎ",
          "original_form": "비도 잘 안오니까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100015791.1.17",
          "form": "미세먼지만 좀 걱정돼요ㅋㅋ",
          "original_form": "미세먼지만 좀 걱정돼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100015791.1.18",
          "form": "맞아요 ㅋㅋ 아 또 중국에서 미세먼지가 오겠네요 ㅠㅠ",
          "original_form": "맞아요 ㅋㅋ 아 또 중국에서 미세먼지가 오겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100015791.1.19",
          "form": "황사나 미세먼지만 제외하면 정말 좋은 계절인데요ㅠ",
          "original_form": "황사나 미세먼지만 제외하면 정말 좋은 계절인데요ㅠ",
          "speaker_id": "1",
          "time": "20210826 14:35"
        }
      ]
