      "utterance": [
        {
          "id": "MDRW2100046223.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046223.1.2",
          "form": "ㅎㅇㅇ",
          "original_form": "ㅎㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046223.1.3",
          "form": "점심 뭐 드실 예정이신가요??",
          "original_form": "점심 뭐 드실 예정이신가요??",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046223.1.4",
          "form": "아직 생각이없는데",
          "original_form": "아직 생각이없는데",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046223.1.5",
          "form": "엄마랑 고민해보려고요ㅎㅎ",
          "original_form": "엄마랑 고민해보려고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046223.1.6",
          "form": "메뉴 정한거 있으세요",
          "original_form": "메뉴 정한거 있으세요",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046223.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046223.1.8",
          "form": "전 도시락 싸왔어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 도시락 싸왔어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046223.1.9",
          "form": "소세지 구운거랑 해서 먹을 예정이예요",
          "original_form": "소세지 구운거랑 해서 먹을 예정이예요",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046223.1.10",
          "form": "와 직접만드신거에요??",
          "original_form": "와 직접만드신거에요??",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046223.1.11",
          "form": "우와앙...되게 부지런하시네요!",
          "original_form": "우와앙...되게 부지런하시네요!",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046223.1.12",
          "form": "저는 음...비오니까",
          "original_form": "저는 음...비오니까",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046223.1.13",
          "form": "뭔가 튀김류가 땡기기도하고",
          "original_form": "뭔가 튀김류가 땡기기도하고",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046223.1.14",
          "form": "튀김 맛있겠네요 ㅠㅠㅋㅋㅋ",
          "original_form": "튀김 맛있겠네요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046223.1.15",
          "form": "돈까스 이런거 맛있을거 같아요 ㅎㅎ",
          "original_form": "돈까스 이런거 맛있을거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046223.1.16",
          "form": "앗 완전 괜찮은데요ㅋㅋ",
          "original_form": "앗 완전 괜찮은데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046223.1.17",
          "form": "빨래 끝나고 이따 말씀드려봐야겠어요ㅌㅋ",
          "original_form": "빨래 끝나고 이따 말씀드려봐야겠어요ㅌㅋ",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046223.1.18",
          "form": "최근에 돈까스 먹은적이 없어서ㅠㅠ",
          "original_form": "최근에 돈까스 먹은적이 없어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046223.1.19",
          "form": "ㅋㅋㅋㅋㅋ저도 갑자기 돈까스가 땡기네요 ㅠㅠㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저도 갑자기 돈까스가 땡기네요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.20",
          "form": "ㅋㅋㅋㅋ앜ㅋㅋ죄송해요",
          "original_form": "ㅋㅋㅋㅋ앜ㅋㅋ죄송해요",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.21",
          "form": "돈까스는 어떤거 선호하세요??",
          "original_form": "돈까스는 어떤거 선호하세요??",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.22",
          "form": "ㅋㅋㅋㅋㅋ아녜욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아녜욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.23",
          "form": "아근데 잘만든 돈까스는",
          "original_form": "아근데 잘만든 돈까스는",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.24",
          "form": "뭐든 맛있는거같아요ㅠㅠ",
          "original_form": "뭐든 맛있는거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.25",
          "form": "돈까스 카레 우동",
          "original_form": "돈까스 카레 우동",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.26",
          "form": "일식돈까스~ 경양식~",
          "original_form": "일식돈까스~ 경양식~",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.27",
          "form": "맞아요 ㅠㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅠㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046223.1.28",
          "form": "치즈 잔뜩 들어있는 그 동그랗고 통통하게 생긴",
          "original_form": "치즈 잔뜩 들어있는 그 동그랗고 통통하게 생긴",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046223.1.29",
          "form": "그 돈까스도 먹고 싶네요...",
          "original_form": "그 돈까스도 먹고 싶네요...",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046223.1.30",
          "form": "앟 완전좋아",
          "original_form": "앟 완전좋아",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046223.1.31",
          "form": "그거 반 갈라서 나오면",
          "original_form": "그거 반 갈라서 나오면",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046223.1.32",
          "form": "락교도먹고 초절임도 먹고",
          "original_form": "락교도먹고 초절임도 먹고",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046223.1.33",
          "form": "치즈도 한입하고",
          "original_form": "치즈도 한입하고",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046223.1.34",
          "form": "후루룩하면 뚝딱인데ㅠㅠ",
          "original_form": "후루룩하면 뚝딱인데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046223.1.35",
          "form": "그쵸그쵸 ㅠㅠㅠㅠㅠ",
          "original_form": "그쵸그쵸 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046223.1.36",
          "form": "그리고 딸려오는 뜨끈한 국도",
          "original_form": "그리고 딸려오는 뜨끈한 국도",
          "speaker_id": "1",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046223.1.37",
          "form": "홀짝 하고...",
          "original_form": "홀짝 하고...",
          "speaker_id": "1",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046223.1.38",
          "form": "맞아여...",
          "original_form": "맞아여...",
          "speaker_id": "2",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046223.1.39",
          "form": "아 그 제주도에 연돈 먹으러가보고싶어여ㅠㅠ",
          "original_form": "아 그 제주도에 연돈 먹으러가보고싶어여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046223.1.40",
          "form": "아 거기 유명하던데 ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 거기 유명하던데 ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046223.1.41",
          "form": "네네ㅠㅠ 진짜",
          "original_form": "네네ㅠㅠ 진짜",
          "speaker_id": "2",
          "time": "20210908 11:30"
        },
        {
          "id": "MDRW2100046223.1.42",
          "form": "백신 다 맞고 놀러가서 꼭 먹고싶어여 ㅋㅋ",
          "original_form": "백신 다 맞고 놀러가서 꼭 먹고싶어여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:30"
        }
      ]
    }
