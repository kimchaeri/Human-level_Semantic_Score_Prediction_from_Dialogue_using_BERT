      "utterance": [
        {
          "id": "MDRW2100045333.1.1",
          "form": "낭만 고양이나 메탈 남이요",
          "original_form": "낭만 고양이나 메탈 남이요",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045333.1.2",
          "form": "낭만 고양이는 어떤가요",
          "original_form": "낭만 고양이는 어떤가요",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045333.1.3",
          "form": "길고양이 돌보시는 분이에요",
          "original_form": "길고양이 돌보시는 분이에요",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045333.1.4",
          "form": "유튜브 인가요",
          "original_form": "유튜브 인가요",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045333.1.5",
          "form": "보통 아프리카로 생방송이 요",
          "original_form": "보통 아프리카로 생방송이 요",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045333.1.6",
          "form": "아프리카도 가봐야 되겠군요",
          "original_form": "아프리카도 가봐야 되겠군요",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045333.1.7",
          "form": "저번 여름 침수가 한번 돼서",
          "original_form": "저번 여름 침수가 한번 돼서",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045333.1.8",
          "form": "고양이들은 괜찮았나요",
          "original_form": "고양이들은 괜찮았나요",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045333.1.9",
          "form": "다행이",
          "original_form": "다행이",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045333.1.10",
          "form": "고양이 관련 많아서 좋아요",
          "original_form": "고양이 관련 많아서 좋아요",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045333.1.11",
          "form": "골라보는 재미가 있어요",
          "original_form": "골라보는 재미가 있어요",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045333.1.12",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045333.1.13",
          "form": "후원도 하고 그랬다는",
          "original_form": "후원도 하고 그랬다는",
