      "utterance": [
        {
          "id": "MDRW2100045783.1.1",
          "form": "먹으면 바로 ㅜㅜ",
          "original_form": "먹으면 바로 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045783.1.2",
          "form": "요리하는거 보니 저도 아침 먹을려고요",
          "original_form": "요리하는거 보니 저도 아침 먹을려고요",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045783.1.3",
          "form": "오 마싯게 드세요",
          "original_form": "오 마싯게 드세요",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045783.1.4",
          "form": "아삭아삭 소리 참을슈 없네요",
          "original_form": "아삭아삭 소리 참을슈 없네요",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045783.1.5",
          "form": "그래도 물은 잘 나오네요",
          "original_form": "그래도 물은 잘 나오네요",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045783.1.6",
          "form": "당분간 생식만 가능한 세계 ㅜㅜ",
          "original_form": "당분간 생식만 가능한 세계 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045783.1.7",
          "form": "채집만 하러가시넹 ㅋㅋ",
          "original_form": "채집만 하러가시넹 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045783.1.8",
          "form": "저 저런 수집 하는거 좋아하는뎅",
          "original_form": "저 저런 수집 하는거 좋아하는뎅",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045783.1.9",
          "form": "갑자기 게임 땡기네요 ㅋㅋ",
          "original_form": "갑자기 게임 땡기네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045783.1.10",
          "form": "마을도 채집이된다니",
          "original_form": "마을도 채집이된다니",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045783.1.11",
          "form": "마늘",
          "original_form": "마늘",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045783.1.12",
          "form": "아무곳이나 채집되나바요",
          "original_form": "아무곳이나 채집되나바요",
          "speaker_id": "1",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045783.1.13",
          "form": "포도도 줏으시네",
          "original_form": "포도도 줏으시네",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045783.1.14",
          "form": "꽃두요 ㅋㅋㅋ",
          "original_form": "꽃두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045783.1.15",
          "form": "먹어질까용",
          "original_form": "먹어질까용",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045783.1.16",
          "form": "이 브금만 들으면 긴장이",
