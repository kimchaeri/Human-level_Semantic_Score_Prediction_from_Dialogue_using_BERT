      "utterance": [
        {
          "id": "MDRW2100030977.1.1",
          "form": "요즘 날씨 되게 좋아졌어요",
          "original_form": "요즘 날씨 되게 좋아졌어요",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100030977.1.2",
          "form": "맞아요 얼마전까지만 해도 엄청 더웠는데",
          "original_form": "맞아요 얼마전까지만 해도 엄청 더웠는데",
          "speaker_id": "2",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100030977.1.3",
          "form": "많이 시원해졌더라구요",
          "original_form": "많이 시원해졌더라구요",
          "speaker_id": "1",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030977.1.4",
          "form": "그쵸 가을이 진짜 왔나봐요 ㅎㅎ",
          "original_form": "그쵸 가을이 진짜 왔나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030977.1.5",
          "form": "가을 좋아해서 좋아요",
          "original_form": "가을 좋아해서 좋아요",
          "speaker_id": "1",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030977.1.6",
          "form": "가을 날씨가 제일 좋은 거 같아요",
          "original_form": "가을 날씨가 제일 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030977.1.7",
          "form": "맞아요 선선하니 딱 좋죠",
          "original_form": "맞아요 선선하니 딱 좋죠",
          "speaker_id": "1",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030977.1.8",
          "form": "풍요의 계절이죠 ㅎㅎ",
          "original_form": "풍요의 계절이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030977.1.9",
          "form": "사과 등... 맛난 과일도 많이 나오니 좋아요",
          "original_form": "사과 등... 맛난 과일도 많이 나오니 좋아요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030977.1.10",
          "form": "제가 좋아하는 과일은 여름에 많이 나와요",
          "original_form": "제가 좋아하는 과일은 여름에 많이 나와요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030977.1.11",
          "form": "수박같은거 좋아하ㄴ시나요?",
          "original_form": "수박같은거 좋아하ㄴ시나요?",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030977.1.12",
          "form": "네 수박 복숭아 포도 등등",
          "original_form": "네 수박 복숭아 포도 등등",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030977.1.13",
          "form": "오 여름에 먹으면 맛있죠",
          "original_form": "오 여름에 먹으면 맛있죠",
          "speaker_id": "1",
          "time": "20210904 15:05"
        }
      ]
