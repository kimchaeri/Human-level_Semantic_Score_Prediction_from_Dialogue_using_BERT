      "utterance": [
        {
          "id": "MDRW2100014944.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014944.1.2",
          "form": "집안일 좋아하는 편이에요?",
          "original_form": "집안일 좋아하는 편이에요?",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014944.1.3",
          "form": "안녕하세요 저는 안좋아해요 ㅋㅋㅋ",
          "original_form": "안녕하세요 저는 안좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014944.1.4",
          "form": "집에서 막내라 언니오빠들이 대부분 하네요 ㅋㅋ",
          "original_form": "집에서 막내라 언니오빠들이 대부분 하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014944.1.5",
          "form": "저도요 ㅜㅜ..",
          "original_form": "저도요 ㅜㅜ..",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014944.1.6",
          "form": "그래도 방청소는 제가하려구해요",
          "original_form": "그래도 방청소는 제가하려구해요",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014944.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014944.1.8",
          "form": "그래도 언니오빠들이 해주니 좋네요. 전 혼자 살아서 혼자 다해야해요.",
          "original_form": "그래도 언니오빠들이 해주니 좋네요. 전 혼자 살아서 혼자 다해야해요.",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014944.1.9",
          "form": "헐 자취하세요?",
          "original_form": "헐 자취하세요?",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.10",
          "form": "네 ㅜㅜ 자취해서 설거지, 세탁, 청소 등 다 해요..",
          "original_form": "네 ㅜㅜ 자취해서 설거지, 세탁, 청소 등 다 해요..",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.11",
          "form": "와 대단하네요",
          "original_form": "와 대단하네요",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.12",
          "form": "설거지가 제일 귀찮고 청소도 제일 귀찮아요",
          "original_form": "설거지가 제일 귀찮고 청소도 제일 귀찮아요",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.13",
          "form": "요리도 하세요??",
          "original_form": "요리도 하세요??",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.14",
          "form": "세탁기가 대신 해주는 세탁은 그나마 합니다..",
          "original_form": "세탁기가 대신 해주는 세탁은 그나마 합니다..",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.15",
          "form": "설거지 진짜 제일 싫어요",
          "original_form": "설거지 진짜 제일 싫어요",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.16",
          "form": "요리 좋아합니다.",
          "original_form": "요리 좋아합니다.",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.17",
          "form": "근데 설거지는...",
          "original_form": "근데 설거지는...",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.18",
          "form": "설거지도 식기세척기가 해줬으면",
          "original_form": "설거지도 식기세척기가 해줬으면",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014944.1.20",
          "form": "그쵸 ㅋㅋ 식기세척기 LG꺼 사고싶어요",
          "original_form": "그쵸 ㅋㅋ 식기세척기 LG꺼 사고싶어요",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014944.1.21",
          "form": "오 lg꺼 좋아요?? 저도 사드리고싶어요",
          "original_form": "오 lg꺼 좋아요?? 저도 사드리고싶어요",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014944.1.22",
          "form": "나중에 돈벌면 ㅋㅋㅋ",
          "original_form": "나중에 돈벌면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014944.1.23",
          "form": "네~ 손으로 전문가가 빡빡 씻는 것보다 더 깨끗하대요~",
          "original_form": "네~ 손으로 전문가가 빡빡 씻는 것보다 더 깨끗하대요~",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014944.1.24",
          "form": "오오오 뽐뿌오네요",
          "original_form": "오오오 뽐뿌오네요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014944.1.25",
          "form": "그리고 로봇청소기만 사면되겠네요",
          "original_form": "그리고 로봇청소기만 사면되겠네요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014944.1.26",
          "form": "트윈타워도 갖고 싶어요..",
          "original_form": "트윈타워도 갖고 싶어요..",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014944.1.27",
          "form": "이젠 건조 시키는 것 마저 귀찮아요..",
          "original_form": "이젠 건조 시키는 것 마저 귀찮아요..",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014944.1.28",
          "form": "오 그게 뭔가요?",
          "original_form": "오 그게 뭔가요?",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014944.1.29",
          "form": "세탁기란 건조기랑 일체형으로 된거요~",
          "original_form": "세탁기란 건조기랑 일체형으로 된거요~",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014944.1.30",
          "form": "와 건조기가 트윈타워인가요?",
          "original_form": "와 건조기가 트윈타워인가요?",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014944.1.31",
          "form": "와 요즘은 그런 편한것도 있네요",
          "original_form": "와 요즘은 그런 편한것도 있네요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        }
      ]
