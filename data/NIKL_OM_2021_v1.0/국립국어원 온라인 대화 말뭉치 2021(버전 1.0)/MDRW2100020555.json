      "utterance": [
        {
          "id": "MDRW2100020555.1.1",
          "form": "김밥이 먹고싶네요 급",
          "original_form": "김밥이 먹고싶네요 급",
          "speaker_id": "2",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100020555.1.2",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100020555.1.3",
          "form": "김밥은 집에서 만들기 넘 힘들어요",
          "original_form": "김밥은 집에서 만들기 넘 힘들어요",
          "speaker_id": "2",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100020555.1.4",
          "form": "버거킹은 좀 멀어졌어요",
          "original_form": "버거킹은 좀 멀어졌어요",
          "speaker_id": "2",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100020555.1.5",
          "form": "저는 그 밥 간맛추는거 너무 어려워",
          "original_form": "저는 그 밥 간맛추는거 너무 어려워",
          "speaker_id": "1",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100020555.1.6",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "2",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100020555.1.7",
          "form": "아앗.. 안녕히가세요 버거킹~ 나중에 만나요",
          "original_form": "아앗.. 안녕히가세요 버거킹~ 나중에 만나요",
          "speaker_id": "1",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100020555.1.8",
          "form": "김밥 은근 난이도 높은 작업..",
          "original_form": "김밥 은근 난이도 높은 작업..",
          "speaker_id": "2",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100020555.1.9",
          "form": "메뉴를 여러개 거치며",
          "original_form": "메뉴를 여러개 거치며",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.10",
          "form": "그리고 손도 많이가고",
          "original_form": "그리고 손도 많이가고",
          "speaker_id": "1",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.11",
          "form": "바뀌었나요 마음이",
          "original_form": "바뀌었나요 마음이",
          "speaker_id": "1",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.12",
          "form": "아침메뉴가 계속 바뀌는 중이에요",
          "original_form": "아침메뉴가 계속 바뀌는 중이에요",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.13",
          "form": "사람마음은 원래 갈대같습니다",
          "original_form": "사람마음은 원래 갈대같습니다",
          "speaker_id": "1",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.14",
          "form": "수십번 바뀐듯..",
          "original_form": "수십번 바뀐듯..",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.15",
          "form": "당연한겁니다",
          "original_form": "당연한겁니다",
          "speaker_id": "1",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.16",
          "form": "name1님은 어느 정도",
          "original_form": "&name1&님은 어느 정도",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.17",
          "form": "메뉴를 정하셨나요",
          "original_form": "메뉴를 정하셨나요",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100020555.1.18",
          "form": "전 점심에 어제 사놓은 빵을 먹을 예정이라",
          "original_form": "전 점심에 어제 사놓은 빵을 먹을 예정이라",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100020555.1.19",
          "form": "빵 좋아하시나요",
          "original_form": "빵 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100020555.1.20",
          "form": "앗 참 그랬죠",
          "original_form": "앗 참 그랬죠",
          "speaker_id": "2",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100020555.1.21",
