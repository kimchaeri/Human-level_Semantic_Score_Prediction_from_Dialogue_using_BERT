      "utterance": [
        {
          "id": "MDRW2100044118.1.1",
          "form": "카페 자주가시나요",
          "original_form": "카페 자주가시나요",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044118.1.2",
          "form": "요즘은 못갔네요 \r\n테이크아웃 많이 해요",
          "original_form": "요즘은 못갔네요 \r\n테이크아웃 많이 해요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044118.1.3",
          "form": "디저트는 잘 안드세요?",
          "original_form": "디저트는 잘 안드세요?",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044118.1.4",
          "form": "좋아해요 케이크랑 마카롱도요ㅋ",
          "original_form": "좋아해요 케이크랑 마카롱도요ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044118.1.5",
          "form": "오 케익 머조아하세여",
          "original_form": "오 케익 머조아하세여",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044118.1.6",
          "form": "티라미수요ㅋㅋ",
          "original_form": "티라미수요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044118.1.7",
          "form": "오 좋죵 어디서사드세요",
          "original_form": "오 좋죵 어디서사드세요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044118.1.8",
          "form": "전투썸",
          "original_form": "전투썸",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044118.1.9",
          "form": "투썸맛잇죠 저도 좋아해요",
          "original_form": "투썸맛잇죠 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044118.1.10",
          "form": "투썸 혹시 초크생 아세요",
          "original_form": "투썸 혹시 초크생 아세요",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044118.1.11",
          "form": "그건 모죠?",
          "original_form": "그건 모죠?",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044118.1.12",
          "form": "전 먹는거만 먹어서",
          "original_form": "전 먹는거만 먹어서",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044118.1.13",
          "form": "초코생크림 ? 머 이런건데",
          "original_form": "초코생크림 ? 머 이런건데",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044118.1.14",
          "form": "짱마싯어요",
          "original_form": "짱마싯어요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044118.1.15",
          "form": "담에 먹어봐야겟네요",
          "original_form": "담에 먹어봐야겟네요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044118.1.16",
          "form": "투썸이 케이크 맛잇는거같아여",
          "original_form": "투썸이 케이크 맛잇는거같아여",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044118.1.17",
          "form": "마자용마자용",
          "original_form": "마자용마자용",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044118.1.18",
          "form": "먹으러가고싶네요",
          "original_form": "먹으러가고싶네요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        }
      ]
