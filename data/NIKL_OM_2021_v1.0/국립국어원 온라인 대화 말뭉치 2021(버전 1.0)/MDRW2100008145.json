      "utterance": [
        {
          "id": "MDRW2100008145.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008145.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008145.1.3",
          "form": "막상가면 힘들지만요",
          "original_form": "막상가면 힘들지만요",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008145.1.4",
          "form": "해외 자주나가셨나요?",
          "original_form": "해외 자주나가셨나요?",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008145.1.5",
          "form": "내 자주",
          "original_form": "내 자주",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008145.1.6",
          "form": "어디가셨나요?",
          "original_form": "어디가셨나요?",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008145.1.7",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008145.1.8",
          "form": "거긴인종차별어떠나요?",
          "original_form": "거긴인종차별어떠나요?",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008145.1.9",
          "form": "친구가 엄청당했다고 하더라고요",
          "original_form": "친구가 엄청당했다고 하더라고요",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008145.1.10",
          "form": "여행으로 가니 좋던데",
          "original_form": "여행으로 가니 좋던데",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008145.1.11",
          "form": "케바켄가보네요",
          "original_form": "케바켄가보네요",
          "speaker_id": "2",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008145.1.12",
          "form": "카페에서 아예 주문을 안받았대요",
          "original_form": "카페에서 아예 주문을 안받았대요",
          "speaker_id": "2",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008145.1.13",
          "form": "학 그런일이..",
          "original_form": "학 그런일이..",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008145.1.14",
          "form": "뒷사람만 받고",
          "original_form": "뒷사람만 받고",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008145.1.15",
          "form": "충격이었어요 저도",
          "original_form": "충격이었어요 저도",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008145.1.16",
          "form": "거기도 이상하네요",
          "original_form": "거기도 이상하네요",
          "speaker_id": "1",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008145.1.17",
          "form": "호주가 심하다고 들어서 편견이 생겼어요",
          "original_form": "호주가 심하다고 들어서 편견이 생겼어요",
          "speaker_id": "2",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008145.1.18",
          "form": "저는 중국자주갔는데 좋더라고요",
          "original_form": "저는 중국자주갔는데 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008145.1.19",
          "form": "시끄럽지 않나오?",
          "original_form": "시끄럽지 않나오?",
          "speaker_id": "1",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008145.1.20",
          "form": "부자동네라 그런지 여유롭고 좋더라고요",
          "original_form": "부자동네라 그런지 여유롭고 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 13:48"
        },
        {
          "id": "MDRW2100008145.1.21",
          "form": "수다가 시끄럽긴해요 모여있으면",
          "original_form": "수다가 시끄럽긴해요 모여있으면",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008145.1.22",
          "form": "네 각자 다 말하덜라구요",
          "original_form": "네 각자 다 말하덜라구요",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008145.1.23",
          "form": "듣는 사람ㅇ없이 말하는 사람들먼 ㅋㅋ",
          "original_form": "듣는 사람ㅇ없이 말하는 사람들먼 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:49"
        }
      ]
