      "utterance": [
        {
          "id": "MDRW2100033997.1.1",
          "form": "공동 3위 윌스미스",
          "original_form": "공동 3위 윌스미스",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100033997.1.2",
          "form": "영화한편에 400 억 덜덜이다",
          "original_form": "영화한편에 400 억 덜덜이다",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033997.1.3",
          "form": "그러게 외국은 후덜덜하다",
          "original_form": "그러게 외국은 후덜덜하다",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033997.1.4",
          "form": "워낙 자본이 많으니... 배우 할만하네",
          "original_form": "워낙 자본이 많으니... 배우 할만하네",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033997.1.5",
          "form": "2위 우리가 어제 얘기했던 드웨인 존슨",
          "original_form": "2위 우리가 어제 얘기했던 드웨인 존슨",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033997.1.6",
          "form": "1 위가 아니네? 도대체 1 위는 누구?",
          "original_form": "1 위가 아니네? 도대체 1 위는 누구?",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100033997.1.7",
          "form": "5000만 달러 ㅋㅋ 거의 650억 2편 찍으면 1000억이다 완전 단위가 틀리네",
          "original_form": "5000만 달러 ㅋㅋ 거의 650억 2편 찍으면 1000억이다 완전 단위가 틀리네",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100033997.1.8",
          "form": "엄청나게 받네.",
          "original_form": "엄청나게 받네.",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100033997.1.9",
          "form": "대망의 1위 다니엘 크레이그",
          "original_form": "대망의 1위 다니엘 크레이그",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100033997.1.10",
          "form": "검색 좀 하고 오께ㅋㅋ",
          "original_form": "검색 좀 하고 오께ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033997.1.11",
          "form": "이분은 예상밖인데 1위네",
          "original_form": "이분은 예상밖인데 1위네",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033997.1.12",
          "form": "007 그분",
          "original_form": "007 그분",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100033997.1.13",
          "form": "아 그분?  매력적이긴 하지",
          "original_form": "아 그분?  매력적이긴 하지",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033997.1.14",
          "form": "넷플릭스에서 속편을 찍었나보다",
          "original_form": "넷플릭스에서 속편을 찍었나보다",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033997.1.15",
          "form": "근데 정말 의외다",
          "original_form": "근데 정말 의외다",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033997.1.16",
          "form": "1억달러다 1000억을 넘긴다",
          "original_form": "1억달러다 1000억을 넘긴다",
