        {
          "id": "MDRW2100041018.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100041018.1.2",
          "form": "네 비빔면요ㅋㅋ",
          "original_form": "네 비빔면요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100041018.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 15:16"
        },
        {
          "id": "MDRW2100041018.1.4",
          "form": "저는 주꾸미 볶음",
          "original_form": "저는 주꾸미 볶음",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100041018.1.5",
          "form": "맛잇는거드셨네요",
          "original_form": "맛잇는거드셨네요",
          "speaker_id": "1",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100041018.1.6",
          "form": "깻잎이랑 무쌈 천사채 옥수수",
          "original_form": "깻잎이랑 무쌈 천사채 옥수수",
          "speaker_id": "2",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100041018.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100041018.1.8",
          "form": "여행은 먹는게 반이죠ㅋㅋ",
          "original_form": "여행은 먹는게 반이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100041018.1.9",
          "form": "쌈 으로 먹었어요",
          "original_form": "쌈 으로 먹었어요",
          "speaker_id": "2",
          "time": "20210905 15:18"
        },
        {
          "id": "MDRW2100041018.1.10",
          "form": "ㅎㅎ집에서 먹기는 했지만",
          "original_form": "ㅎㅎ집에서 먹기는 했지만",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100041018.1.11",
          "form": "맛있고 배불리 먹었네요~",
          "original_form": "맛있고 배불리 먹었네요~",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100041018.1.12",
          "form": "배불리먹어야 또 여행잘다니죠ㅋㅋ",
          "original_form": "배불리먹어야 또 여행잘다니죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100041018.1.13",
          "form": "아아 그렇죠",
          "original_form": "아아 그렇죠",
          "speaker_id": "2",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100041018.1.14",
          "form": "여행에서도 사실 먹는 즐거움이 반 이상이예요",
          "original_form": "여행에서도 사실 먹는 즐거움이 반 이상이예요",
          "speaker_id": "2",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100041018.1.15",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100041018.1.16",
          "form": "그곳에서만 먹을수있는 그런것",
          "original_form": "그곳에서만 먹을수있는 그런것",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100041018.1.17",
          "form": "네 맛집 찾아가게 되고",
          "original_form": "네 맛집 찾아가게 되고",
          "speaker_id": "2",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100041018.1.18",
          "form": "그 지역에서만 먹을 수 있는 건 꼭 찾아먹고",
          "original_form": "그 지역에서만 먹을 수 있는 건 꼭 찾아먹고",
          "speaker_id": "2",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100041018.1.19",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041018.1.20",
          "form": "그런게 여행의재미죠",
