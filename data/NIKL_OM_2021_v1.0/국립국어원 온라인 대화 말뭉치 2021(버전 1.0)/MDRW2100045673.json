      "utterance": [
        {
          "id": "MDRW2100045673.1.1",
          "form": "스벅에서는 무슨 메뉴 좋아하세요?",
          "original_form": "스벅에서는 무슨 메뉴 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045673.1.2",
          "form": "전 슈크림라떼요!",
          "original_form": "전 슈크림라떼요!",
          "speaker_id": "2",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045673.1.3",
          "form": "한정메뉴였죠ㅠㅠㅠ",
          "original_form": "한정메뉴였죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045673.1.4",
          "form": "아아ㅠㅠㅠ한정메뉴는 너무 아쉬워요ㅠㅠ",
          "original_form": "아아ㅠㅠㅠ한정메뉴는 너무 아쉬워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045673.1.5",
          "form": "아직 보낼 준비가 안 되었는데...",
          "original_form": "아직 보낼 준비가 안 되었는데...",
          "speaker_id": "1",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045673.1.6",
          "form": "그러니까요ㅠㅠㅠ 한정 너무 맛있는게 많아요",
          "original_form": "그러니까요ㅠㅠㅠ 한정 너무 맛있는게 많아요",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045673.1.7",
          "form": "옛날에 푸딩 넣은 음료수? 이름이 기억안나는데",
          "original_form": "옛날에 푸딩 넣은 음료수? 이름이 기억안나는데",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045673.1.8",
          "form": "그것도 너무 맛있게 먹었거든요",
          "original_form": "그것도 너무 맛있게 먹었거든요",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045673.1.9",
          "form": "아 그 흔들면 변하는 거요?!",
          "original_form": "아 그 흔들면 변하는 거요?!",
          "speaker_id": "1",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045673.1.10",
          "form": "저 그거에 완전 꽂혀서 맨날 먹었어요ㅠㅠ",
          "original_form": "저 그거에 완전 꽂혀서 맨날 먹었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045673.1.11",
          "form": "마트에서 파는거라면 그것도 맛있죠!!!",
          "original_form": "마트에서 파는거라면 그것도 맛있죠!!!",
          "speaker_id": "2",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045673.1.12",
          "form": "스벅에도 푸딩 통째로 들어간 메뉴가 따로 있었지만",
          "original_form": "스벅에도 푸딩 통째로 들어간 메뉴가 따로 있었지만",
          "speaker_id": "2",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045673.1.13",
          "form": "아아 헐 그걸 몰랐다니",
          "original_form": "아아 헐 그걸 몰랐다니",
          "speaker_id": "1",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045673.1.14",
          "form": "흔들면 변하는건 환타 쉐이크 같은거? 말씀하시는거 맞죠 ㅋㅋㅋㅋ?",
          "original_form": "흔들면 변하는건 환타 쉐이크 같은거? 말씀하시는거 맞죠 ㅋㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045673.1.15",
          "form": "너무 아쉽네요ㅠㅠㅠ",
          "original_form": "너무 아쉽네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045673.1.16",
          "form": "네ㅠㅠㅠㅠㅠㅠ",
          "original_form": "네ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045673.1.17",
          "form": "ㅠㅠㅠㅠ진짜 맛잇고",
          "original_form": "ㅠㅠㅠㅠ진짜 맛잇고",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045673.1.18",
          "form": "흑흑ㅠㅠㅠㅠㅠ",
          "original_form": "흑흑ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045673.1.19",
          "form": "손에 딱 들어가는 사이즈라 귀여웠는데",
          "original_form": "손에 딱 들어가는 사이즈라 귀여웠는데",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045673.1.20",
          "form": "다시 출시해주면 좋겠어요",
          "original_form": "다시 출시해주면 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045673.1.21",
          "form": "맛있고 좋은건 왜이렇게 빨리 사라질까요",
          "original_form": "맛있고 좋은건 왜이렇게 빨리 사라질까요",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045673.1.22",
          "form": "푸딩하니까 그 단종된 푸딩도 생각나네요",
          "original_form": "푸딩하니까 그 단종된 푸딩도 생각나네요",
          "speaker_id": "1",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045673.1.23",
          "form": "가끔 부활시켜서 추억마케팅이나 하다가",
          "original_form": "가끔 부활시켜서 추억마케팅이나 하다가",
          "speaker_id": "2",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045673.1.24",
          "form": "아 쁘띠첼 맞죠",
          "original_form": "아 쁘띠첼 맞죠",
          "speaker_id": "2",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045673.1.25",
          "form": "커스타드 푸딩 시판하던거 너무 맛있었는데ㅠㅜㅠ",
          "original_form": "커스타드 푸딩 시판하던거 너무 맛있었는데ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045673.1.26",
          "form": "아니 그걸 왜 단종을",
          "original_form": "아니 그걸 왜 단종을",
          "speaker_id": "2",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045673.1.27",
          "form": "아아ㅠㅠㅠㅠ 맞아요ㅜㅜㅜㅠ",
          "original_form": "아아ㅠㅠㅠㅠ 맞아요ㅜㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045673.1.28",
          "form": "아니 진짜 대체 왜",
          "original_form": "아니 진짜 대체 왜",
          "speaker_id": "1",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045673.1.29",
          "form": "진짜 인기도 많았을텐데ㅠㅠㅠㅠ",
          "original_form": "진짜 인기도 많았을텐데ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045673.1.30",
          "form": "한국에서는 음식들이 빨리 유행했다가 빨리 사라지긴 하는 것 같아요ㅠㅠㅠ",
          "original_form": "한국에서는 음식들이 빨리 유행했다가 빨리 사라지긴 하는 것 같아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045673.1.31",
          "form": "대만카스테라도 그렇고",
          "original_form": "대만카스테라도 그렇고",
          "speaker_id": "2",
