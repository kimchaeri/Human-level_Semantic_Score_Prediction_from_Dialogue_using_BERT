      "utterance": [
        {
          "id": "MDRW2100041984.1.1",
          "form": "비오는데",
          "original_form": "비오는데",
          "speaker_id": "2",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041984.1.2",
          "form": "name3 안아주세요",
          "original_form": "&name3& 안아주세요",
          "speaker_id": "2",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100041984.1.3",
          "form": "네 오늘도 벌벌 떨더라고요 ㅠㅠ",
          "original_form": "네 오늘도 벌벌 떨더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041984.1.4",
          "form": "근데 가게 나올땐 신나서 나왔네여",
          "original_form": "근데 가게 나올땐 신나서 나왔네여",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041984.1.5",
          "form": "힝구구 애기 나가는건 좋은가봐요 ㅋㅋㅋㅋ",
          "original_form": "힝구구 애기 나가는건 좋은가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100041984.1.6",
          "form": "근데 천둥번개없이 걍 빗소리만 나두 떨어요? ㅠ",
          "original_form": "근데 천둥번개없이 걍 빗소리만 나두 떨어요? ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100041984.1.7",
          "form": "근데 지금은 막상 안떨고있네요",
          "original_form": "근데 지금은 막상 안떨고있네요",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100041984.1.8",
          "form": "이사한집이 낯선지 계속 붙어다니거나 화장실에 들어가있어요",
          "original_form": "이사한집이 낯선지 계속 붙어다니거나 화장실에 들어가있어요",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100041984.1.9",
          "form": "에궁 아직 낯가리나봐요 ㅋㅋ 하긴 새로운 집 냄새에 적응하려먄 좀 걸리겟어요",
          "original_form": "에궁 아직 낯가리나봐요 ㅋㅋ 하긴 새로운 집 냄새에 적응하려먄 좀 걸리겟어요",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100041984.1.10",
          "form": "애깅.. 내일 저 회사가는데 걱정입니당",
          "original_form": "애깅.. 내일 저 회사가는데 걱정입니당",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100041984.1.11",
          "form": "지금은 아주 쿨쿨 자고있네요",
          "original_form": "지금은 아주 쿨쿨 자고있네요",
          "speaker_id": "1",
          "time": "20210907 01:04"
        },
        {
          "id": "MDRW2100041984.1.12",
          "form": "ㅋㅋㅋ다행이에요 ㅋㅋ귀여운짜아식ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ다행이에요 ㅋㅋ귀여운짜아식ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100041984.1.13",
          "form": "저 출근하는데도 일어나지도 않고 잠 잘자더라구요",
          "original_form": "저 출근하는데도 일어나지도 않고 잠 잘자더라구요",
          "speaker_id": "1",
          "time": "20210907 08:19"
        },
        {
          "id": "MDRW2100041984.1.14",
          "form": "짜식ㅋㅋㅋㅋㅋ푹잠들었네요 ㅋㅋㅋㅋ 귀여운것ㅋㅋㅋㅋ",
          "original_form": "짜식ㅋㅋㅋㅋㅋ푹잠들었네요 ㅋㅋㅋㅋ 귀여운것ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:21"
        },
        {
          "id": "MDRW2100041984.1.15",
          "form": "사실...항상 그래요...엄청 예전엔 일어나서 배웅해주고 그랬는데요..",
          "original_form": "사실...항상 그래요...엄청 예전엔 일어나서 배웅해주고 그랬는데요..",
          "speaker_id": "1",
          "time": "20210907 08:23"
        },
        {
          "id": "MDRW2100041984.1.16",
          "form": "물론 사실 배웅이 아니라 자기도 데리고 나가라고 따라 나온거지만여",
          "original_form": "물론 사실 배웅이 아니라 자기도 데리고 나가라고 따라 나온거지만여",
          "speaker_id": "1",
          "time": "20210907 08:23"
        },
        {
          "id": "MDRW2100041984.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100041984.1.18",
          "form": "강아지랑 같이 출근하면 얼마나좋을까요",
          "original_form": "강아지랑 같이 출근하면 얼마나좋을까요",
          "speaker_id": "2",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100041984.1.19",
          "form": "그대신 얌전한애들이어야함",
          "original_form": "그대신 얌전한애들이어야함",
          "speaker_id": "2",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100041984.1.20",
          "form": "그르게요 그런 회사 다니고싶네요",
          "original_form": "그르게요 그런 회사 다니고싶네요",
          "speaker_id": "1",
          "time": "20210907 08:25"
        },
        {
          "id": "MDRW2100041984.1.21",
          "form": "ㅋㅋㅋ 강아지안고 일하기ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ 강아지안고 일하기ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:41"
        }
      ]
    }
