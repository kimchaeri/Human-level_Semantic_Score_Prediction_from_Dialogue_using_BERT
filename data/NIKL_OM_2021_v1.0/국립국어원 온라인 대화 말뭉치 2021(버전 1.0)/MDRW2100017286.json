      "utterance": [
        {
          "id": "MDRW2100017286.1.1",
          "form": "최근에 구매하신거 있으세여",
          "original_form": "최근에 구매하신거 있으세여",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017286.1.2",
          "form": "안그래도 채널 신고 당하고 그래서",
          "original_form": "안그래도 채널 신고 당하고 그래서",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017286.1.3",
          "form": "오오 쇼핑",
          "original_form": "오오 쇼핑",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017286.1.4",
          "form": "최근에 구매하신거 있으세요",
          "original_form": "최근에 구매하신거 있으세요",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017286.1.5",
          "form": "오늘 아침에 촬영소품",
          "original_form": "오늘 아침에 촬영소품",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017286.1.6",
          "form": "구매했어요",
          "original_form": "구매했어요",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017286.1.7",
          "form": "유튜브 하시나요",
          "original_form": "유튜브 하시나요",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017286.1.8",
          "form": "네 그렇습니다!",
          "original_form": "네 그렇습니다!",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017286.1.9",
          "form": "소품 어떤게 있지",
          "original_form": "소품 어떤게 있지",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017286.1.10",
          "form": "카메라 구매하신건가",
          "original_form": "카메라 구매하신건가",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017286.1.11",
          "form": "아니에요 ㅋㅋ 분장용품이요",
          "original_form": "아니에요 ㅋㅋ 분장용품이요",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017286.1.12",
          "form": "마녀 분장 하려고",
          "original_form": "마녀 분장 하려고",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017286.1.13",
          "form": "마녀 코? 주문했어요",
          "original_form": "마녀 코? 주문했어요",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017286.1.14",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017286.1.15",
          "form": "모자 같은거 주문하신줄ㅋㄲ",
          "original_form": "모자 같은거 주문하신줄ㅋㄲ",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017286.1.16",
          "form": "아 ㅋㅋ 모자도 샀어요",
          "original_form": "아 ㅋㅋ 모자도 샀어요",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017286.1.17",
          "form": "요술모자 ㅋㅋㅋ",
          "original_form": "요술모자 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017286.1.18",
          "form": "귀엽겠다",
          "original_form": "귀엽겠다",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017286.1.19",
          "form": "요술모자라니",
          "original_form": "요술모자라니",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017286.1.20",
          "form": "맞아요! 뭔가 촬영 시작하면서",
          "original_form": "맞아요! 뭔가 촬영 시작하면서",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017286.1.21",
          "form": "신기한거 사보니까 재밌어요",
          "original_form": "신기한거 사보니까 재밌어요",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017286.1.22",
          "form": "나중에 name1님도 도전해보세요",
          "original_form": "나중에 &name1&님도 도전해보세요",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017286.1.23",
          "form": "재밌을거같아요ㅋㅋ",
          "original_form": "재밌을거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017286.1.24",
          "form": "저는 최근에 영양제 구매했어요",
          "original_form": "저는 최근에 영양제 구매했어요",
          "speaker_id": "1",
          "time": "20210826 18:35"
        }
