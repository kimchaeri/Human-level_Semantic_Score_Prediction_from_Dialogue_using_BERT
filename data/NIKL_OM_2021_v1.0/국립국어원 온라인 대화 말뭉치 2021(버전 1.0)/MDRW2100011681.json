        {
          "id": "MDRW2100011681.1.1",
          "form": "가격이 다 말해주는듯 \n식탁\n얼마생각해?",
          "original_form": "가격이 다 말해주는듯 \n식탁\n얼마생각해?",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011681.1.2",
          "form": "2~300??",
          "original_form": "2~300??",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011681.1.3",
          "form": "500이상이면 진짜 고급스러운거 골라줄 자신있다",
          "original_form": "500이상이면 진짜 고급스러운거 골라줄 자신있다",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011681.1.4",
          "form": "나도 500이상이면 자신있다~ㅋㅋ",
          "original_form": "나도 500이상이면 자신있다~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011681.1.5",
          "form": "이쁜거살려면...",
          "original_form": "이쁜거살려면...",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011681.1.6",
          "form": "너거 형부 뒤로 넘어간다",
          "original_form": "너거 형부 뒤로 넘어간다",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011681.1.7",
          "form": "350이상",
          "original_form": "350이상",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011681.1.8",
          "form": "쇼파는 어디거 사려고?",
          "original_form": "쇼파는 어디거 사려고?",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011681.1.9",
          "form": "200대와 300 은 느낌이 다르네",
          "original_form": "200대와 300 은 느낌이 다르네",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011681.1.10",
          "form": "아직 모름",
          "original_form": "아직 모름",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011681.1.11",
          "form": "근데 아주 애매한 금액이다",
          "original_form": "근데 아주 애매한 금액이다",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011681.1.12",
          "form": "그니깐",
          "original_form": "그니깐",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011681.1.13",
          "form": "뭐든 비싼게 좋드라",
          "original_form": "뭐든 비싼게 좋드라",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011681.1.14",
          "form": "우선 돌아보아야지",
          "original_form": "우선 돌아보아야지",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011681.1.15",
          "form": "싸고 좋은건 없다",
          "original_form": "싸고 좋은건 없다",
          "speaker_id": "1",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011681.1.16",
          "form": "가격이 다 말해주는듯하다",
          "original_form": "가격이 다 말해주는듯하다",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011681.1.17",
          "form": "엉 비슷하지만...완벽하게 다르지",
          "original_form": "엉 비슷하지만...완벽하게 다르지",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100011681.1.18",
          "form": "너는 뭐 사고싶은데?",
          "original_form": "너는 뭐 사고싶은데?",
