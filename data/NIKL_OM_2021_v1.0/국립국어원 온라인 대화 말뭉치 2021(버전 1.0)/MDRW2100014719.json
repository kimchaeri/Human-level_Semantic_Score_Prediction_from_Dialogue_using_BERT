        {
          "id": "MDRW2100014719.1.1",
          "form": "그럼 가족분들도 다 2차까지 백신 맞으신 거예요?",
          "original_form": "그럼 가족분들도 다 2차까지 백신 맞으신 거예요?",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014719.1.2",
          "form": "아니요ㅎ 저만ㅎㅎ",
          "original_form": "아니요ㅎ 저만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014719.1.3",
          "form": "그렇시구나",
