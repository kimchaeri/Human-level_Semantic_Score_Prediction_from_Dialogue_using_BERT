      "utterance": [
        {
          "id": "MDRW2100021497.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021497.1.2",
          "form": "좋아하는 스포츠 있으신가요",
          "original_form": "좋아하는 스포츠 있으신가요",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021497.1.3",
          "form": "저는 최근에 배구에 관심 생겼어요!",
          "original_form": "저는 최근에 배구에 관심 생겼어요!",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021497.1.4",
          "form": "우와 올림픽 보고 관심 생겼는데 직관가고 싶어요 ㅋㅋ",
          "original_form": "우와 올림픽 보고 관심 생겼는데 직관가고 싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100021497.1.5",
          "form": "프로팀도 잘 아시나요",
          "original_form": "프로팀도 잘 아시나요",
          "speaker_id": "1",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100021497.1.6",
          "form": "저는 이제 알아가고 있어요ㅋㅋ 이번에 열린 프로배구대회부터 보고 있네요",
          "original_form": "저는 이제 알아가고 있어요ㅋㅋ 이번에 열린 프로배구대회부터 보고 있네요",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021497.1.7",
          "form": "오오 어떤 팀 응원해요",
          "original_form": "오오 어떤 팀 응원해요",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021497.1.8",
          "form": "저 ibk 품고 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저 ibk 품고 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021497.1.9",
          "form": "오오 제일 강한 팀이에요?",
          "original_form": "오오 제일 강한 팀이에요?",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021497.1.10",
          "form": "그건 아니지만... 응원하는 선수들이 많아서요ㅋㅋㅋ",
          "original_form": "그건 아니지만... 응원하는 선수들이 많아서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100021497.1.11",
          "form": "김희진 선수가 그쪽인가요?",
          "original_form": "김희진 선수가 그쪽인가요?",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100021497.1.12",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021497.1.13",
          "form": "오 김희진 선수 너무 귀여워요",
          "original_form": "오 김희진 선수 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021497.1.14",
          "form": "겉보기와 다르게 생각보다 쾌활하시더라고요ㅋㅋㅋ",
          "original_form": "겉보기와 다르게 생각보다 쾌활하시더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021497.1.15",
          "form": "네 뭔가 곰돌이 같기도하고",
          "original_form": "네 뭔가 곰돌이 같기도하고",
