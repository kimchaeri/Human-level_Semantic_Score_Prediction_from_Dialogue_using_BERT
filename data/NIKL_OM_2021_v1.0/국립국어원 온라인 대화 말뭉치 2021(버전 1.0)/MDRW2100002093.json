      "utterance": [
        {
          "id": "MDRW2100002093.1.1",
          "form": "뭐 사고싶은거 있어요?",
          "original_form": "뭐 사고싶은거 있어요?",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002093.1.2",
          "form": "저 안마의자...",
          "original_form": "저 안마의자...",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002093.1.3",
          "form": "ㅋㅋㅋㅋㅋ공감...누가 안마의자좀 사주세요",
          "original_form": "ㅋㅋㅋㅋㅋ공감...누가 안마의자좀 사주세요",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002093.1.4",
          "form": "그니까요 ㅠㅠ 너무 필요해요",
          "original_form": "그니까요 ㅠㅠ 너무 필요해요",
          "speaker_id": "1",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002093.1.5",
          "form": "폼롤러 최근에 사서 온몸에 굴리고 있는데 시원하고 좋아요",
          "original_form": "폼롤러 최근에 사서 온몸에 굴리고 있는데 시원하고 좋아요",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002093.1.6",
          "form": "정말요? 하나 구해야겠네요",
          "original_form": "정말요? 하나 구해야겠네요",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002093.1.7",
          "form": "그리고 바이크 사고싶어요 교통이 좀 불편한데 살아서",
          "original_form": "그리고 바이크 사고싶어요 교통이 좀 불편한데 살아서",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002093.1.8",
          "form": "오 바이크~요즘 전기자전거도 많이 구매하지 않아요?",
          "original_form": "오 바이크~요즘 전기자전거도 많이 구매하지 않아요?",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002093.1.9",
          "form": "네 아마 보험 안해도 돼서 구매하는것같아요",
          "original_form": "네 아마 보험 안해도 돼서 구매하는것같아요",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002093.1.10",
          "form": "아하~그런 장점이 있군요",
          "original_form": "아하~그런 장점이 있군요",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002093.1.11",
          "form": "네 이륜차는 보험료가 세니...",
          "original_form": "네 이륜차는 보험료가 세니...",
          "speaker_id": "1",
          "time": "20210819 23:50"
        },
        {
          "id": "MDRW2100002093.1.12",
          "form": "저도 자전거는 사고싶어요ㅋㅋ",
          "original_form": "저도 자전거는 사고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002093.1.13",
          "form": "어떤 자전거 사고싶으세요?",
          "original_form": "어떤 자전거 사고싶으세요?",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002093.1.14",
          "form": "전기자전거요!!근데 넘 비싸요",
