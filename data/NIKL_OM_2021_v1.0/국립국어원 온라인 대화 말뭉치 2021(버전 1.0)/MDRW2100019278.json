      "utterance": [
        {
          "id": "MDRW2100019278.1.1",
          "form": "요새 날씨 진짜 선선하더라구요",
          "original_form": "요새 날씨 진짜 선선하더라구요",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019278.1.2",
          "form": "맞죠 ! 곧 가을 아닐까요 ㅎㅎ",
          "original_form": "맞죠 ! 곧 가을 아닐까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019278.1.3",
          "form": "뭔가 9월에 비가 많이 올거같기도 해요",
          "original_form": "뭔가 9월에 비가 많이 올거같기도 해요",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019278.1.4",
          "form": "맞아요 가을장마라더라구요",
          "original_form": "맞아요 가을장마라더라구요",
          "speaker_id": "1",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019278.1.5",
          "form": "가을장마도 있는줄 첨알았어요 ㅋㅋㅋ",
          "original_form": "가을장마도 있는줄 첨알았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100019278.1.6",
          "form": "맞죠 ㅋㅋㅋ 원래 여름에 장마 아닌가요!",
          "original_form": "맞죠 ㅋㅋㅋ 원래 여름에 장마 아닌가요!",
          "speaker_id": "2",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100019278.1.7",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100019278.1.8",
          "form": "어쩐지 요새 비가 많이 오더라구여",
          "original_form": "어쩐지 요새 비가 많이 오더라구여",
          "speaker_id": "1",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100019278.1.9",
          "form": "맞죠 막 안온다해놓고 비 엄청 오고 ㅋㅋ",
          "original_form": "맞죠 막 안온다해놓고 비 엄청 오고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019278.1.10",
          "form": "온다하면 안오고 ㅋㅋ",
          "original_form": "온다하면 안오고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019278.1.11",
          "form": "맞아요 변덕스러워요 ㅠ",
          "original_form": "맞아요 변덕스러워요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019278.1.12",
          "form": "우산들고다니기 번거로워요",
          "original_form": "우산들고다니기 번거로워요",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019278.1.13",
          "form": "또 비오면 많이와서",
          "original_form": "또 비오면 많이와서",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019278.1.14",
          "form": "장우산 들고다녀야되니까 더 무거워요 ㅋㅋ",
          "original_form": "장우산 들고다녀야되니까 더 무거워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100019278.1.15",
          "form": "맞아요 ㅋㅋㅋㅋ 3단은 너무 약해서요ㅠㅠ",
          "original_form": "맞아요 ㅋㅋㅋㅋ 3단은 너무 약해서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019278.1.16",
          "form": "장우산은 걸어다닐때도 애매해요 뒷사람 칠거같아서 ㅋㅋㅋㅋ",
          "original_form": "장우산은 걸어다닐때도 애매해요 뒷사람 칠거같아서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019278.1.17",
          "form": "아 맞아요 ㅋㅋ 저 항상 제 발 치고 다니는데 ㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋ 저 항상 제 발 치고 다니는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019278.1.18",
          "form": "맞아여 ㅋㅋㅋ 3단이 들고다니긴 편한데 단점들이..ㅠㅠ",
          "original_form": "맞아여 ㅋㅋㅋ 3단이 들고다니긴 편한데 단점들이..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:05"
        },
        {
          "id": "MDRW2100019278.1.19",
          "form": "ㅋㅋ 맞아요 전 여름에 양산도 쓰고 다니는데",
          "original_form": "ㅋㅋ 맞아요 전 여름에 양산도 쓰고 다니는데",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019278.1.20",
          "form": "가벼워서 좋드라구요",
          "original_form": "가벼워서 좋드라구요",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019278.1.21",
          "form": "우산도 3단이 뒤집히지만 않으면 쓸텐데 ..",
          "original_form": "우산도 3단이 뒤집히지만 않으면 쓸텐데 ..",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019278.1.22",
          "form": "맞아요 날씨 궂을땐 3단쓰면 난리나요",
          "original_form": "맞아요 날씨 궂을땐 3단쓰면 난리나요",
