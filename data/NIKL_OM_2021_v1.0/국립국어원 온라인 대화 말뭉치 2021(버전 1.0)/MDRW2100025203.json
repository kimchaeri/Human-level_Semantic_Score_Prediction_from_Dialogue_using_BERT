      "utterance": [
        {
          "id": "MDRW2100025203.1.1",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100025203.1.2",
          "form": "아뇨 전 무교입니다",
          "original_form": "아뇨 전 무교입니다",
          "speaker_id": "2",
          "time": "20210901 00:47"
        },
        {
          "id": "MDRW2100025203.1.3",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100025203.1.4",
          "form": "그래서 종교에 대해서 크게 생각이 없긴 해요",
          "original_form": "그래서 종교에 대해서 크게 생각이 없긴 해요",
          "speaker_id": "1",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100025203.1.5",
          "form": "그럼 신자들에 대해서 어떻게 생각하시나요",
          "original_form": "그럼 신자들에 대해서 어떻게 생각하시나요",
          "speaker_id": "1",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100025203.1.6",
          "form": "뭐 스스로 믿는 건 상관 없는데 전도만 안 했음 좋겠어요 ㅎㅎ",
          "original_form": "뭐 스스로 믿는 건 상관 없는데 전도만 안 했음 좋겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100025203.1.7",
          "form": "아 도를 아십니까 당하면 화나죠 ㅎㅎ",
          "original_form": "아 도를 아십니까 당하면 화나죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100025203.1.8",
          "form": "이젠 눈도 안 마주쳐요 ㅎㅎ",
          "original_form": "이젠 눈도 안 마주쳐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:51"
        },
        {
          "id": "MDRW2100025203.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025203.1.10",
          "form": "노하우가 느셨네요",
          "original_form": "노하우가 느셨네요",
          "speaker_id": "1",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025203.1.11",
          "form": "사실 당하는 사람들이 왜 노하우가 늘어야 하는지 이해가 안되긴 합니다!",
          "original_form": "사실 당하는 사람들이 왜 노하우가 늘어야 하는지 이해가 안되긴 합니다!",
          "speaker_id": "1",
          "time": "20210901 00:52"
        },
        {
          "id": "MDRW2100025203.1.12",
          "form": "아 길 물어보는 척 하다가 전도하는 경우도 있더라구요",
          "original_form": "아 길 물어보는 척 하다가 전도하는 경우도 있더라구요",
          "speaker_id": "2",
          "time": "20210901 00:53"
        },
        {
          "id": "MDRW2100025203.1.13",
          "form": "그리고 전도하면서 당당하게 음료 한잔 베풀어달라고 ㅋㅋㅋ",
          "original_form": "그리고 전도하면서 당당하게 음료 한잔 베풀어달라고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:53"
        },
        {
          "id": "MDRW2100025203.1.14",
          "form": "참 왜 그러나 모르겠어요~",
          "original_form": "참 왜 그러나 모르겠어요~",
          "speaker_id": "1",
          "time": "20210901 00:54"
        },
        {
          "id": "MDRW2100025203.1.15",
          "form": "사 주는 것도 아니고ㅋㅋㅋ",
          "original_form": "사 주는 것도 아니고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100025203.1.16",
          "form": "그러니까요!ㅋㅋㅋ",
          "original_form": "그러니까요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100025203.1.17",
          "form": "버거킹 자주 갑니다",
          "original_form": "버거킹 자주 갑니다",
          "speaker_id": "1",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100025203.1.18",
          "form": "진짜로 믿음이 필요한 사람은 전도하지 않더라도 알아서 찾아가더라구요",
          "original_form": "진짜로 믿음이 필요한 사람은 전도하지 않더라도 알아서 찾아가더라구요",
          "speaker_id": "2",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100025203.1.19",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100025203.1.20",
          "form": "대부분 종교가",
          "original_form": "대부분 종교가",
          "speaker_id": "1",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100025203.1.21",
          "form": "벼랑끝에 내몰렸을 때 믿음을 얻거나 그러니",
          "original_form": "벼랑끝에 내몰렸을 때 믿음을 얻거나 그러니",
          "speaker_id": "1",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100025203.1.22",
          "form": "절박한 사람은 따로 있는 것 가타요",
          "original_form": "절박한 사람은 따로 있는 것 가타요",
          "speaker_id": "1",
          "time": "20210901 01:17"
        }
      ]
    }
