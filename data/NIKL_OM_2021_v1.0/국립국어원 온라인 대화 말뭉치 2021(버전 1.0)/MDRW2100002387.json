      "utterance": [
        {
          "id": "MDRW2100002387.1.1",
          "form": "봉사 하시나요",
          "original_form": "봉사 하시나요",
          "speaker_id": "1",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002387.1.2",
          "form": "과거에 했는데 지금은 쉬고있어요",
          "original_form": "과거에 했는데 지금은 쉬고있어요",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002387.1.3",
          "form": "name1님은 봉사하시나요?",
          "original_form": "&name1&님은 봉사하시나요?",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002387.1.4",
          "form": "저도 지금은 안하고 있어요",
          "original_form": "저도 지금은 안하고 있어요",
          "speaker_id": "1",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002387.1.5",
          "form": "기회가된다면 하실생각이신가요",
          "original_form": "기회가된다면 하실생각이신가요",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002387.1.6",
          "form": "네 하고 싶어요",
          "original_form": "네 하고 싶어요",
          "speaker_id": "1",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002387.1.7",
          "form": "어떤 곳에서 봉사해보고싶나요?",
          "original_form": "어떤 곳에서 봉사해보고싶나요?",
          "speaker_id": "2",
          "time": "20210820 11:10"
        },
        {
          "id": "MDRW2100002387.1.8",
          "form": "아이들 교육 봉사요",
          "original_form": "아이들 교육 봉사요",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002387.1.9",
          "form": "아동센터 같은 곳이요?",
          "original_form": "아동센터 같은 곳이요?",
          "speaker_id": "2",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002387.1.10",
          "form": "네 그런 곳이요",
          "original_form": "네 그런 곳이요",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002387.1.11",
          "form": "교육봉사면 어떤걸 교육하시나요?",
          "original_form": "교육봉사면 어떤걸 교육하시나요?",
          "speaker_id": "2",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002387.1.12",
          "form": "영어 가르치는거요",
          "original_form": "영어 가르치는거요",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002387.1.13",
          "form": "아 영어를 잘 하시는구나",
          "original_form": "아 영어를 잘 하시는구나",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002387.1.14",
          "form": "엄청 도움이 되는 과목이네요",
          "original_form": "엄청 도움이 되는 과목이네요",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002387.1.15",
          "form": "네 아이들 도와주면 좋죠",
          "original_form": "네 아이들 도와주면 좋죠",
