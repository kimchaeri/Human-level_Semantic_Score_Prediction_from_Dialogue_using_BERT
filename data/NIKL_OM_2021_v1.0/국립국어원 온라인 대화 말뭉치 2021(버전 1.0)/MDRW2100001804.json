      "utterance": [
        {
          "id": "MDRW2100001804.1.1",
          "form": "가족들과 사시나요?",
          "original_form": "가족들과 사시나요?",
          "speaker_id": "1",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001804.1.2",
          "form": "네 가족들하고 살아요",
          "original_form": "네 가족들하고 살아요",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001804.1.3",
          "form": "독립하셨어요?",
          "original_form": "독립하셨어요?",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001804.1.4",
          "form": "아뇨 저도 가족들이랑 살아요 ~",
          "original_form": "아뇨 저도 가족들이랑 살아요 ~",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001804.1.5",
          "form": "저랑 같군요 ㅎ",
          "original_form": "저랑 같군요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001804.1.6",
          "form": "전 요새 독립도 하고싶어요",
          "original_form": "전 요새 독립도 하고싶어요",
          "speaker_id": "2",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001804.1.7",
          "form": "다 장단점이 있는거같아요 ㅠㅠ",
          "original_form": "다 장단점이 있는거같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001804.1.8",
          "form": "저는 전에 룸메랑 한두달 살았는데",
          "original_form": "저는 전에 룸메랑 한두달 살았는데",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001804.1.9",
          "form": "그것도 쉽지않더라구요 ㅋㅋ",
          "original_form": "그것도 쉽지않더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001804.1.10",
          "form": "아하 룸메랑 살면 안맞는 부분도 있으니까요 ㅠ",
          "original_form": "아하 룸메랑 살면 안맞는 부분도 있으니까요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001804.1.11",
          "form": "저도 한달간 친구랑 자취한 적 있는데 확실히 배려가 필요하더라구여",
          "original_form": "저도 한달간 친구랑 자취한 적 있는데 확실히 배려가 필요하더라구여",
          "speaker_id": "2",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001804.1.12",
          "form": "외적으로 잘꾸미고 하는애라",
          "original_form": "외적으로 잘꾸미고 하는애라",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001804.1.13",
          "form": "관리잘할줄알았는데",
          "original_form": "관리잘할줄알았는데",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001804.1.14",
          "form": "왠걸 ..",
          "original_form": "왠걸 ..",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001804.1.15",
          "form": "너무 지저분한거있죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 지저분한거있죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001804.1.16",
          "form": "경악했어요 정말",
          "original_form": "경악했어요 정말",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001804.1.17",
          "form": "아 진짜요? ㅋㅋㅋ ㅋㅋㅋㅋ 원래 내가 알던 친구의 모습이 아니니 그럴 거 같아요",
          "original_form": "아 진짜요? ㅋㅋㅋ ㅋㅋㅋㅋ 원래 내가 알던 친구의 모습이 아니니 그럴 거 같아요",
          "speaker_id": "2",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001804.1.18",
          "form": "맞아여 생활패턴이 다른사람이랑",
          "original_form": "맞아여 생활패턴이 다른사람이랑",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001804.1.19",
          "form": "사는거 참 어려워요 ",
          "original_form": "사는거 참 어려워요 ",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001804.1.20",
          "form": "가족이 최고 ..ㅋㅋ",
          "original_form": "가족이 최고 ..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100001804.1.21",
          "form": "ㅋㅋ ㅋㅋㅋㅋ 맞아요 가족이 편하긴하죠 ㅎㅎ",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋ 맞아요 가족이 편하긴하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001804.1.22",
          "form": "그래도 살면서 한번은 혼자서 자취도 해보고싶어요",
          "original_form": "그래도 살면서 한번은 혼자서 자취도 해보고싶어요",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001804.1.23",
          "form": "그쵸 ㅎㅎ 자취해서 내식대로 꾸미고 살아보고파요",
          "original_form": "그쵸 ㅎㅎ 자취해서 내식대로 꾸미고 살아보고파요",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001804.1.24",
          "form": "저두요 !! 저의 로망입니다",
          "original_form": "저두요 !! 저의 로망입니다",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001804.1.25",
          "form": "더 늦지않게 하고프네요",
          "original_form": "더 늦지않게 하고프네요",
