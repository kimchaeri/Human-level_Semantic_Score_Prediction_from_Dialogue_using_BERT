      "utterance": [
        {
          "id": "MDRW2100037997.1.1",
          "form": "오모나 밥은 먹고 다닐까요? ㅠㅠ",
          "original_form": "오모나 밥은 먹고 다닐까요? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037997.1.2",
          "form": "그러게요ㅜ",
          "original_form": "그러게요ㅜ",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037997.1.3",
          "form": "매일 생사확인할때마다 안도해요ㅜ",
          "original_form": "매일 생사확인할때마다 안도해요ㅜ",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037997.1.4",
          "form": "다행히 해코지하는 사람은 없는것같거든요 동네에",
          "original_form": "다행히 해코지하는 사람은 없는것같거든요 동네에",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037997.1.5",
          "form": "여기 부산인데 비가 많이와요 ㅠㅠ",
          "original_form": "여기 부산인데 비가 많이와요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037997.1.6",
          "form": "비오면 애들이 제일 걱정되네요 어디 잘 숨어있겠죠",
          "original_form": "비오면 애들이 제일 걱정되네요 어디 잘 숨어있겠죠",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037997.1.7",
          "form": "그죠 사실 길고양이들은 참 매일매일이 서바이벌이랄까",
          "original_form": "그죠 사실 길고양이들은 참 매일매일이 서바이벌이랄까",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100037997.1.8",
          "form": "저희쪽은 그 공원은 먹이주지말라고 돼있더라고요",
          "original_form": "저희쪽은 그 공원은 먹이주지말라고 돼있더라고요",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100037997.1.9",
          "form": "너무해요 ㅠㅠ",
          "original_form": "너무해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100037997.1.10",
          "form": "오늘 턱시도냥이 한마리",
          "original_form": "오늘 턱시도냥이 한마리",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100037997.1.11",
          "form": "비맞고 있더라구요 ㅜㅠ",
          "original_form": "비맞고 있더라구요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100037997.1.12",
          "form": "그 공원말고좀만 올라오면 밥그릇물그릇 있는곳 있는데ㅜ",
          "original_form": "그 공원말고좀만 올라오면 밥그릇물그릇 있는곳 있는데ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100037997.1.13",
          "form": "싶었어요ㅜㅜ",
          "original_form": "싶었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100037997.1.14",
          "form": "길애들은 물을 잘못먹는다더라구요",
          "original_form": "길애들은 물을 잘못먹는다더라구요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100037997.1.15",
          "form": "그죠 그게 다 부은건데 살찐거라고하고",
          "original_form": "그죠 그게 다 부은건데 살찐거라고하고",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100037997.1.16",
          "form": "저 고양이도 없는데 고다 가입되있어요 ㅋㅋ",
          "original_form": "저 고양이도 없는데 고다 가입되있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100037997.1.17",
          "form": "헉 ㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100037997.1.18",
          "form": "가입하려면 고양이 사진 인증하래서",
          "original_form": "가입하려면 고양이 사진 인증하래서",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100037997.1.19",
          "form": "친구 다둥이네 사진 빌려서 ㅋㅋ",
          "original_form": "친구 다둥이네 사진 빌려서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100037997.1.20",
          "form": "그런 인증도 필요한가봐요",
          "original_form": "그런 인증도 필요한가봐요",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100037997.1.21",
          "form": "요즘은",
          "original_form": "요즘은",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100037997.1.22",
          "form": "이상한 사람들 많이 들어와서 ㅜㅜ",
          "original_form": "이상한 사람들 많이 들어와서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100037997.1.23",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100037997.1.24",
          "form": "전고양이도없는데 거기 눈팅해요 ㅋㅋㅋ",
          "original_form": "전고양이도없는데 거기 눈팅해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100037997.1.25",
          "form": "저는 인스타도 많이봐요 ㅎㅎ",
          "original_form": "저는 인스타도 많이봐요 ㅎㅎ",
