      "utterance": [
        {
          "id": "MDRW2100002910.1.1",
          "form": "아 ㅋㅋㅋㅋ 배고프니까 냉장고 여는거구나",
          "original_form": "아 ㅋㅋㅋㅋ 배고프니까 냉장고 여는거구나",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002910.1.2",
          "form": "전 뭐해먹으려고 그런단줄알고",
          "original_form": "전 뭐해먹으려고 그런단줄알고",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002910.1.3",
          "form": "혼자 살면 밥해야하잖아요",
          "original_form": "혼자 살면 밥해야하잖아요",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002910.1.4",
          "form": "아 그쵸!ㅠㅠ",
          "original_form": "아 그쵸!ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002910.1.5",
          "form": "냉장고에 음식 보관이 많이 되어있으니까 그걸로 이용해서 종종 조리는 해먹어요!ㅎ.ㅎ",
          "original_form": "냉장고에 음식 보관이 많이 되어있으니까 그걸로 이용해서 종종 조리는 해먹어요!ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002910.1.6",
          "form": "저는 혼자 살면 더 배달 시킬켜먹을꺼같아요ㅠㅠ..",
          "original_form": "저는 혼자 살면 더 배달 시킬켜먹을꺼같아요ㅠㅠ..",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002910.1.7",
          "form": "워낙 입맛이 배달음식을 조아해서...",
          "original_form": "워낙 입맛이 배달음식을 조아해서...",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002910.1.8",
          "form": "아무래도 그래지죠 ㅠ",
          "original_form": "아무래도 그래지죠 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002910.1.9",
          "form": "식구들이랑 살아야 집밥 먹어지고요",
          "original_form": "식구들이랑 살아야 집밥 먹어지고요",
          "speaker_id": "2",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002910.1.10",
          "form": "맞아요 가족들이랑 살고있어서 그나마 집밥 챙겨먹게되더라구요!",
          "original_form": "맞아요 가족들이랑 살고있어서 그나마 집밥 챙겨먹게되더라구요!",
          "speaker_id": "1",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002910.1.11",
          "form": "그리고 확실히 혼자있음 쫌 심심해요",
          "original_form": "그리고 확실히 혼자있음 쫌 심심해요",
          "speaker_id": "1",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002910.1.12",
          "form": "자유롭고 좋은데 시간오래되면 심심하더라구요ㅠㅠ",
          "original_form": "자유롭고 좋은데 시간오래되면 심심하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002910.1.13",
          "form": "맞아요\r\n가족이 몇명이에요?",
          "original_form": "맞아요\r\n가족이 몇명이에요?",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002910.1.14",
          "form": "저는 4명에 반려동물1마리예요!",
          "original_form": "저는 4명에 반려동물1마리예요!",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002910.1.15",
          "form": "식구가 몇명이세요?",
          "original_form": "식구가 몇명이세요?",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002910.1.16",
          "form": "우리집도 네명이에요",
          "original_form": "우리집도 네명이에요",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002910.1.17",
          "form": "아이들이 아직 어려서 개인생활 없는 ㅋ",
          "original_form": "아이들이 아직 어려서 개인생활 없는 ㅋ",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002910.1.18",
          "form": "애기들이 잠들어야지만 개인생활이 있겠어요ㅠㅠ!",
          "original_form": "애기들이 잠들어야지만 개인생활이 있겠어요ㅠㅠ!",
          "speaker_id": "1",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002910.1.19",
          "form": "그래도 애들은 넘 귀여울꺼같아요!",
          "original_form": "그래도 애들은 넘 귀여울꺼같아요!",
          "speaker_id": "1",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002910.1.20",
          "form": "귀여울 시기는 지났어요 ㅠㅠ 진작",
          "original_form": "귀여울 시기는 지났어요 ㅠㅠ 진작",
          "speaker_id": "2",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002910.1.21",
          "form": "가족과 시간 많이 보내시나요",
          "original_form": "가족과 시간 많이 보내시나요",
          "speaker_id": "2",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002910.1.22",
          "form": "아니요ㅠㅠ일하고 그러다보니까 다들 시간대가 달라서 많이 못보내요ㅠㅠ",
          "original_form": "아니요ㅠㅠ일하고 그러다보니까 다들 시간대가 달라서 많이 못보내요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002910.1.23",
          "form": "아이들이랑 많이 보내시죠? 지금 코로나라서 더 힘드시겠어요",
          "original_form": "아이들이랑 많이 보내시죠? 지금 코로나라서 더 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002910.1.24",
          "form": "다음주 개학이에요",
          "original_form": "다음주 개학이에요",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002910.1.25",
          "form": "애들은 체력이 어마어마한데 해소를 못하니까ㅠㅠ",
          "original_form": "애들은 체력이 어마어마한데 해소를 못하니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002910.1.26",
          "form": "히히 저혼자 멍때리죠",
          "original_form": "히히 저혼자 멍때리죠",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002910.1.27",
          "form": "개학이라니.. 아침에 더 바빠지시겠어요!",
          "original_form": "개학이라니.. 아침에 더 바빠지시겠어요!",
          "speaker_id": "1",
          "time": "20210820 16:49"
        }
      ]
    }
