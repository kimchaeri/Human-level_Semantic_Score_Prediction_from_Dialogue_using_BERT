      "utterance": [
        {
          "id": "MDRW2100006319.1.1",
          "form": "아 3단계 4주 연장이네",
          "original_form": "아 3단계 4주 연장이네",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006319.1.2",
          "form": "그러게 추석이 있어서 그런가봐",
          "original_form": "그러게 추석이 있어서 그런가봐",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006319.1.3",
          "form": "이번 추석연휴가 길지",
          "original_form": "이번 추석연휴가 길지",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100006319.1.4",
          "form": "응 주말까지 포함하면 길어",
          "original_form": "응 주말까지 포함하면 길어",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100006319.1.5",
          "form": "모임 가능 수랑 영업 시간 변동 있는 것 같아",
          "original_form": "모임 가능 수랑 영업 시간 변동 있는 것 같아",
          "speaker_id": "1",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100006319.1.6",
          "form": "많은 변동은 없는 것 같네",
          "original_form": "많은 변동은 없는 것 같네",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100006319.1.7",
          "form": "우리랑 해당은 많이 없는듯 ㅋㅋ",
          "original_form": "우리랑 해당은 많이 없는듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100006319.1.8",
          "form": "그래도 웬만하면 집콕해야지",
          "original_form": "그래도 웬만하면 집콕해야지",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100006319.1.9",
          "form": "그렇지 안 나가고 안 만나면 코로나 안 걸려",
          "original_form": "그렇지 안 나가고 안 만나면 코로나 안 걸려",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006319.1.10",
          "form": "안 나갈 수 없잖아 사람들이 잘 지켜야지",
          "original_form": "안 나갈 수 없잖아 사람들이 잘 지켜야지",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006319.1.11",
          "form": "맞아 참 이걸 추석형 거리두기라고 하더라",
          "original_form": "맞아 참 이걸 추석형 거리두기라고 하더라",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006319.1.12",
          "form": "이름을 붙였네 ㅋㅋ 우린 주로 둘이 먹으니까 별로 해당 없네",
          "original_form": "이름을 붙였네 ㅋㅋ 우린 주로 둘이 먹으니까 별로 해당 없네",
          "speaker_id": "1",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100006319.1.13",
          "form": "아하 그러네 고향 갈 일도 없고",
          "original_form": "아하 그러네 고향 갈 일도 없고",
          "speaker_id": "2",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100006319.1.14",
          "form": "코로나 진단검사 해야한다던데",
          "original_form": "코로나 진단검사 해야한다던데",
