      "utterance": [
        {
          "id": "MDRW2100027041.1.1",
          "form": "아침 드셨나요?ㅎㅎ",
          "original_form": "아침 드셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027041.1.2",
          "form": "샐러디에서",
          "original_form": "샐러디에서",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027041.1.3",
          "form": "사왔어요!!!",
          "original_form": "사왔어요!!!",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027041.1.4",
          "form": "오픈하자마자 달려감",
          "original_form": "오픈하자마자 달려감",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027041.1.5",
          "form": "오 샐러디",
          "original_form": "오 샐러디",
          "speaker_id": "2",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100027041.1.6",
          "form": "맛은 괜찮은데 은근 비싸서",
          "original_form": "맛은 괜찮은데 은근 비싸서",
          "speaker_id": "2",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100027041.1.7",
          "form": "그니까요....",
          "original_form": "그니까요....",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027041.1.8",
          "form": "소스도 일반 샐러드 가게에서 못보는",
          "original_form": "소스도 일반 샐러드 가게에서 못보는",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027041.1.9",
          "form": "그래서 결국 또 먹는거죠ㅠㅠ",
          "original_form": "그래서 결국 또 먹는거죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027041.1.10",
          "form": "ㅋㅋ저는 회사 선결제",
          "original_form": "ㅋㅋ저는 회사 선결제",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027041.1.11",
          "form": "샐러디에 긁어놓고 먹습니다",
          "original_form": "샐러디에 긁어놓고 먹습니다",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027041.1.12",
          "form": "그건뭐예여??",
          "original_form": "그건뭐예여??",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027041.1.13",
          "form": "회사에서 점심값은 월급으로 안주고",
          "original_form": "회사에서 점심값은 월급으로 안주고",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027041.1.14",
          "form": "어.... 어렵다",
          "original_form": "어.... 어렵다",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027041.1.15",
          "form": "식비가 음 뭐",
          "original_form": "식비가 음 뭐",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027041.1.16",
          "form": "월급 적당하니 ㅋㅋ",
          "original_form": "월급 적당하니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027041.1.17",
          "form": "괜찮아요",
          "original_form": "괜찮아요",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027041.1.18",
          "form": "ㅋㅋㅋㅋㅋ월급은 받으면",
          "original_form": "ㅋㅋㅋㅋㅋ월급은 받으면",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027041.1.19",
          "form": "식당에 가서 선결제로 얼마 이렇게 달아두는 거예요??",
          "original_form": "식당에 가서 선결제로 얼마 이렇게 달아두는 거예요??",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027041.1.20",
          "form": "그럼 먹을때마다 금액만큼 빼고?!",
          "original_form": "그럼 먹을때마다 금액만큼 빼고?!",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027041.1.21",
          "form": "아 한달에 4만5천씩 식당에 선결제할수 있어요",
          "original_form": "아 한달에 4만5천씩 식당에 선결제할수 있어요",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027041.1.22",
          "form": "저희 실근무가 16일정도 되서",
          "original_form": "저희 실근무가 16일정도 되서",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027041.1.23",
          "form": "야간 근무 있긴한데 일단 출근은 1번이니까요",
          "original_form": "야간 근무 있긴한데 일단 출근은 1번이니까요",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027041.1.24",
          "form": "오~~ 좋네요",
          "original_form": "오~~ 좋네요",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027041.1.25",
          "form": "가끔 샐러드 먹어줘야해여ㅋㅋㅋ",
          "original_form": "가끔 샐러드 먹어줘야해여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027041.1.26",
          "form": "네 건강해지는 느낌ㅋㅋ",
          "original_form": "네 건강해지는 느낌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027041.1.27",
          "form": "어떤 샐러드 좋아하세요?",
          "original_form": "어떤 샐러드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 07:15"
        },
        {
          "id": "MDRW2100027041.1.28",
          "form": "자주 드시는 샐러드 종류?!",
