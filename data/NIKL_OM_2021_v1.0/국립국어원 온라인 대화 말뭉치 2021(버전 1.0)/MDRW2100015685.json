      "utterance": [
        {
          "id": "MDRW2100015685.1.1",
          "form": "안녕하세요 오늘 저녁 드셨나요?",
          "original_form": "안녕하세요 오늘 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015685.1.2",
          "form": "안녕하세요 네ㅎㅎ 전 소고기무국 먹었어요!!",
          "original_form": "안녕하세요 네ㅎㅎ 전 소고기무국 먹었어요!!",
          "speaker_id": "2",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015685.1.3",
          "form": "헐 소고기무국ㅠㅠㅠㅠㅠ",
          "original_form": "헐 소고기무국ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015685.1.4",
          "form": "진짜 맛있는데ㅠㅠㅠ",
          "original_form": "진짜 맛있는데ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015685.1.5",
          "form": "엄청 먹고싶네요",
          "original_form": "엄청 먹고싶네요",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015685.1.6",
          "form": "저는 치킨 먹었답니다ㅎㅎ",
          "original_form": "저는 치킨 먹었답니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015685.1.7",
          "form": "혹시 치킨 좋아하시나요?",
          "original_form": "혹시 치킨 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015685.1.8",
          "form": "헉 치킨 완전 좋죠",
          "original_form": "헉 치킨 완전 좋죠",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015685.1.9",
          "form": "무슨 치킨 드셨어요??",
          "original_form": "무슨 치킨 드셨어요??",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015685.1.10",
          "form": "bhc 뿌링클 먹었습니다!",
          "original_form": "bhc 뿌링클 먹었습니다!",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015685.1.11",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015685.1.12",
          "form": "네 먹어봤어요!",
          "original_form": "네 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015685.1.13",
          "form": "근데 먹다보면 약간 물려서? 많이는 못먹겠더라구요ㅋㅋㅠ",
          "original_form": "근데 먹다보면 약간 물려서? 많이는 못먹겠더라구요ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015685.1.14",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015685.1.15",
          "form": "그래서 누구랑 같이 먹을때만 먹고있어요!",
          "original_form": "그래서 누구랑 같이 먹을때만 먹고있어요!",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015685.1.16",
          "form": "어느 브랜드 치킨 좋아하시나요?",
          "original_form": "어느 브랜드 치킨 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015685.1.17",
          "form": "저는 교촌치킨 좋아해요!",
          "original_form": "저는 교촌치킨 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015685.1.18",
          "form": "특히 허니콤보요ㅎㅎ",
          "original_form": "특히 허니콤보요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015685.1.19",
          "form": "헉 저도 허니콤보 좋아해요ㅠㅠ",
          "original_form": "헉 저도 허니콤보 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100015685.1.20",
          "form": "어제 치킨 먹었는데도 먹고싶네요ㅠㅠ",
          "original_form": "어제 치킨 먹었는데도 먹고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100015685.1.21",
          "form": "매운거는 잘드시나요?",
          "original_form": "매운거는 잘드시나요?",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100015685.1.22",
          "form": "아뇨 저 맵찔이에요ㅋㅋㅠㅠㅠ",
          "original_form": "아뇨 저 맵찔이에요ㅋㅋㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100015685.1.23",
          "form": "근데 요즘 로제떡볶이에 엄청 꽂혔어요ㅋㅋㅋ",
          "original_form": "근데 요즘 로제떡볶이에 엄청 꽂혔어요ㅋㅋㅋ",
