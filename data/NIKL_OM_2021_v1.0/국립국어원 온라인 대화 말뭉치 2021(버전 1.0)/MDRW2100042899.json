      "utterance": [
        {
          "id": "MDRW2100042899.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100042899.1.2",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100042899.1.3",
          "form": "이상형이라.. 자기 인생 열심히 사는 사람??",
          "original_form": "이상형이라.. 자기 인생 열심히 사는 사람??",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042899.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042899.1.5",
          "form": "저는 말 잘 통하는 사람이요 ㅎㅎ",
          "original_form": "저는 말 잘 통하는 사람이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042899.1.6",
          "form": "결혼에 대한 로망있으세요?",
          "original_form": "결혼에 대한 로망있으세요?",
          "speaker_id": "1",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042899.1.7",
          "form": "하고는싶어요 ㅋㅋ 그런데 딩크로",
          "original_form": "하고는싶어요 ㅋㅋ 그런데 딩크로",
          "speaker_id": "2",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042899.1.8",
          "form": "오,, 애기는 부담스러우세요..?",
          "original_form": "오,, 애기는 부담스러우세요..?",
          "speaker_id": "1",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100042899.1.9",
          "form": "네네 키울자신이없어요ㅋㅋㅋ일도 계속 하고싶고",
          "original_form": "네네 키울자신이없어요ㅋㅋㅋ일도 계속 하고싶고",
          "speaker_id": "2",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100042899.1.10",
          "form": "ㅋㅋ아 저두요 자신이 없어요,, 한 생명을 키우는게",
          "original_form": "ㅋㅋ아 저두요 자신이 없어요,, 한 생명을 키우는게",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100042899.1.11",
          "form": "부모님이 대단해 보이세요",
          "original_form": "부모님이 대단해 보이세요",
          "speaker_id": "1",
          "time": "20210907 13:11"
        },
        {
          "id": "MDRW2100042899.1.12",
          "form": "그러니까욬ㅋㅋ 요즘은 애가 30이되어도책임져야하는데",
          "original_form": "그러니까욬ㅋㅋ 요즘은 애가 30이되어도책임져야하는데",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100042899.1.13",
          "form": "ㅋㅋㅋ앜ㅋㅋ,, 독립하셨나요?",
          "original_form": "ㅋㅋㅋ앜ㅋㅋ,, 독립하셨나요?",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042899.1.14",
          "form": "네...31살에 이제 독립했아욬ㅋㅋㅋㅋ",
          "original_form": "네...31살에 이제 독립했아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042899.1.15",
          "form": "ㅋㅋㅋㅋ부럽네요 저도 31살에 독립하고 싶네요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ부럽네요 저도 31살에 독립하고 싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100042899.1.16",
          "form": "부담스럽지 않으세요?",
          "original_form": "부담스럽지 않으세요?",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100042899.1.17",
          "form": "저는 월세살이라 무지 부담스럽습니다 ㅋㅋㅋ 다 돈이에요",
          "original_form": "저는 월세살이라 무지 부담스럽습니다 ㅋㅋㅋ 다 돈이에요",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100042899.1.18",
          "form": "그니까요ㅠㅠ 저도 계산해봤는데 엄두가 안나더라구요 ,,ㅋ큐ㅠㅠ",
          "original_form": "그니까요ㅠㅠ 저도 계산해봤는데 엄두가 안나더라구요 ,,ㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100042899.1.19",
          "form": "얼마나 되셨어요?",
          "original_form": "얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100042899.1.20",
          "form": "4월부터차 5개월됐어욬ㅋㅋㅋㅋ",
          "original_form": "4월부터차 5개월됐어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100042899.1.21",
          "form": "우왘ㅋㅋㅋㅋ",
          "original_form": "우왘ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100042899.1.22",
          "form": "벌레 같은거 잘 잡으세요?",
          "original_form": "벌레 같은거 잘 잡으세요?",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100042899.1.23",
          "form": "어우 아뇨ㅋㅋ다행이도벌레는없어요ㅋㅋㅋ",
          "original_form": "어우 아뇨ㅋㅋ다행이도벌레는없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042899.1.24",
          "form": "일부러 오피스텔로와서",
          "original_form": "일부러 오피스텔로와서",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042899.1.25",
          "form": "오 오피스텔은 벌레가 없군요! 다행이예요",
          "original_form": "오 오피스텔은 벌레가 없군요! 다행이예요",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100042899.1.26",
          "form": "저는 자취하면 벌레가 가장 고민일 것 같아요 ,,ㅋㅋㅎ",
          "original_form": "저는 자취하면 벌레가 가장 고민일 것 같아요 ,,ㅋㅋㅎ",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100042899.1.27",
          "form": "그러면 오피스텔이나 아파트가 최곱니다..",
          "original_form": "그러면 오피스텔이나 아파트가 최곱니다..",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100042899.1.28",
          "form": "아니면 애인을 벌레잘잡는분으류..",
          "original_form": "아니면 애인을 벌레잘잡는분으류..",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042899.1.29",
          "form": "으앜ㅋㅋㅋㅋ무조건 오피스텔로 가야겠네요,,",
          "original_form": "으앜ㅋㅋㅋㅋ무조건 오피스텔로 가야겠네요,,",
          "speaker_id": "1",
          "time": "20210907 13:29"
        }
      ]
