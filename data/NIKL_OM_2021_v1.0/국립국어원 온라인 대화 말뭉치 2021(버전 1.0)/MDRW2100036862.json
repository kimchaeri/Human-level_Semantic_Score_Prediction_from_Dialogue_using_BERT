        },
        {
          "id": "MDRW2100036862.1.2",
          "form": "식빵 하나 먹었어요 식사하셨나요!",
          "original_form": "식빵 하나 먹었어요 식사하셨나요!",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100036862.1.3",
          "form": "식빵으로 식사가 되세요?",
          "original_form": "식빵으로 식사가 되세요?",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100036862.1.4",
          "form": "저는 닭가ㅅ살이요 ㅎ",
          "original_form": "저는 닭가ㅅ살이요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100036862.1.5",
          "form": "귀찮아서 ㅎㅎ",
          "original_form": "귀찮아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100036862.1.6",
          "form": "닭가슴살도 배 안차지않나요?ㅠㅠ",
          "original_form": "닭가슴살도 배 안차지않나요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100036862.1.7",
          "form": "치즈퐁듀 닭이라 괜찮았어요",
          "original_form": "치즈퐁듀 닭이라 괜찮았어요",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100036862.1.8",
          "form": "좀느끼하긴하더라구요",
          "original_form": "좀느끼하긴하더라구요",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100036862.1.9",
          "form": "헉 치즈퐁듀닭... 맛있을 것 같아요",
          "original_form": "헉 치즈퐁듀닭... 맛있을 것 같아요",
          "speaker_id": "2",
          "time": "20210901 06:44"
        },
        {
          "id": "MDRW2100036862.1.10",
          "form": "맛은 괜찮았어요",
          "original_form": "맛은 괜찮았어요",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100036862.1.11",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100036862.1.12",
          "form": "네 구내식당에서 해결했네요",
          "original_form": "네 구내식당에서 해결했네요",
          "speaker_id": "1",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100036862.1.13",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100036862.1.14",
          "form": "네 저는 서브웨이 먹었어요",
          "original_form": "네 저는 서브웨이 먹었어요",
          "speaker_id": "2",
          "time": "20210903 12:56"
        },
        {
          "id": "MDRW2100036862.1.15",
          "form": "오늘은 메뉴 좋았나용?",
          "original_form": "오늘은 메뉴 좋았나용?",
          "speaker_id": "2",
          "time": "20210903 12:56"
        },
        {
          "id": "MDRW2100036862.1.16",
          "form": "오늘은 탕수육이 나와서",
          "original_form": "오늘은 탕수육이 나와서",
          "speaker_id": "1",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100036862.1.17",
          "form": "사람들이 많았네요 ㅎ",
          "original_form": "사람들이 많았네요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100036862.1.18",
          "form": "오 탕수육!! 좋져",
          "original_form": "오 탕수육!! 좋져",
          "speaker_id": "2",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100036862.1.19",
