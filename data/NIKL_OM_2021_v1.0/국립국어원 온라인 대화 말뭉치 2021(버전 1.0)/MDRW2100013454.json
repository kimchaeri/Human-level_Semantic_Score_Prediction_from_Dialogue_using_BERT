      "utterance": [
        {
          "id": "MDRW2100013454.1.1",
          "form": "녹차카페는 자주 가시나요?",
          "original_form": "녹차카페는 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013454.1.2",
          "form": "네 자주 가요~~",
          "original_form": "네 자주 가요~~",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013454.1.3",
          "form": "언제부터 활동하셨어요?",
          "original_form": "언제부터 활동하셨어요?",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013454.1.4",
          "form": "전  1년전인가",
          "original_form": "전  1년전인가",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013454.1.5",
          "form": "등급도 낮아요ㅠ",
          "original_form": "등급도 낮아요ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013454.1.6",
          "form": "오신지 얼마 안되셨군요~",
          "original_form": "오신지 얼마 안되셨군요~",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013454.1.7",
          "form": "네 등업 해야하는데ㅠ",
          "original_form": "네 등업 해야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013454.1.8",
          "form": "저도 처음 왔을때 누르면 읽을 수 없는 글때문에 등업이 엄청 하고 싶었어요 ㅋㅋ",
          "original_form": "저도 처음 왔을때 누르면 읽을 수 없는 글때문에 등업이 엄청 하고 싶었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013454.1.9",
          "form": "계속 미루게되요ㅠ",
          "original_form": "계속 미루게되요ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100013454.1.10",
          "form": "등업 욕망보다 귀찮음이 더 크시군요 ㅋㅋ",
          "original_form": "등업 욕망보다 귀찮음이 더 크시군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100013454.1.11",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100013454.1.12",
          "form": "전 읽고 싶은 맘이 더 커서 등업을 했어요 무려 감동티 ㅋ",
          "original_form": "전 읽고 싶은 맘이 더 커서 등업을 했어요 무려 감동티 ㅋ",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100013454.1.13",
          "form": "감동티 와우",
          "original_form": "감동티 와우",
          "speaker_id": "2",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100013454.1.14",
          "form": "근데 막상 읽게 되니 별거 아니더라고요",
          "original_form": "근데 막상 읽게 되니 별거 아니더라고요",
