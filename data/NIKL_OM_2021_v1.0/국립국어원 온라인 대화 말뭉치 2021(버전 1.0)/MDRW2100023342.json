      "utterance": [
        {
          "id": "MDRW2100023342.1.1",
          "form": "일터에 진짜 이상한 사람있음 스트레스죠",
          "original_form": "일터에 진짜 이상한 사람있음 스트레스죠",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023342.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ 네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네",
          "speaker_id": "2",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023342.1.3",
          "form": "지금도 있어요 ㅋㅋㅋㅋ 어딜가나 있음",
          "original_form": "지금도 있어요 ㅋㅋㅋㅋ 어딜가나 있음",
          "speaker_id": "2",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023342.1.4",
          "form": "name1은 재택이신거죠?",
          "original_form": "&name1&은 재택이신거죠?",
          "speaker_id": "2",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023342.1.5",
          "form": "전 지금 재택휴식이요..",
          "original_form": "전 지금 재택휴식이요..",
          "speaker_id": "1",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023342.1.6",
          "form": "백수요..",
          "original_form": "백수요..",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023342.1.7",
          "form": "낼 출근하세요?",
          "original_form": "낼 출근하세요?",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023342.1.8",
          "form": "아 부러웡 재택휴식 엇 자가격리이신건가?",
          "original_form": "아 부러웡 재택휴식 엇 자가격리이신건가?",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023342.1.9",
          "form": "네 일개미는 일을하러",
          "original_form": "네 일개미는 일을하러",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023342.1.10",
          "form": "몇시 출근이에여?",
          "original_form": "몇시 출근이에여?",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023342.1.11",
          "form": "전 9시까지요 ㅋㅋ",
          "original_form": "전 9시까지요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023342.1.12",
          "form": "출근할려면 너무 행복하세겠어요.. ㅋㅋㅋㅋ",
          "original_form": "출근할려면 너무 행복하세겠어요.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023342.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ^^ 네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ^^ 네",
          "speaker_id": "2",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023342.1.14",
          "form": "내일도 일 일심히 하시고..",
          "original_form": "내일도 일 일심히 하시고..",
          "speaker_id": "1",
          "time": "20210830 00:45"
        },
        {
          "id": "MDRW2100023342.1.15",
          "form": "돈 많이 벌어오세여ㅡ..ㅋㅋ",
          "original_form": "돈 많이 벌어오세여ㅡ..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:45"
        },
        {
          "id": "MDRW2100023342.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 싫네요 ... 하루만 저대신 일해주세요..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 싫네요 ... 하루만 저대신 일해주세요..",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023342.1.17",
          "form": "월요일 너무 행복하겠어요.. \r\n저도 할 수 잇다면 대신 일하고 싶어요.. ㅋㅋㅋ \r\n(라고 말하고 계속 누워있는다. )",
          "original_form": "월요일 너무 행복하겠어요.. \r\n저도 할 수 잇다면 대신 일하고 싶어요.. ㅋㅋㅋ \r\n(라고 말하고 계속 누워있는다. )",
          "speaker_id": "1",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100023342.1.18",
          "form": "아 부럽다 11시...",
          "original_form": "아 부럽다 11시...",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023342.1.19",
          "form": "너무 지치는 얼굴이 흙색이네요 전",
          "original_form": "너무 지치는 얼굴이 흙색이네요 전",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023342.1.20",
          "form": "월말 월욜은 진짜 일하기 싫어지죠",
          "original_form": "월말 월욜은 진짜 일하기 싫어지죠",
