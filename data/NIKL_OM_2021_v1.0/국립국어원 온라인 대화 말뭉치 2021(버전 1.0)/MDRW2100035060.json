      "utterance": [
        {
          "id": "MDRW2100035060.1.1",
          "form": "나 어제 소개팅 했어",
          "original_form": "나 어제 소개팅 했어",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100035060.1.2",
          "form": "웬일이야",
          "original_form": "웬일이야",
          "speaker_id": "1",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100035060.1.3",
          "form": "처음 해본 소개팅이었어",
          "original_form": "처음 해본 소개팅이었어",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100035060.1.4",
          "form": "연애를 오랜만에 하네",
          "original_form": "연애를 오랜만에 하네",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100035060.1.5",
          "form": "아직 연애 단계는 아니야",
          "original_form": "아직 연애 단계는 아니야",
          "speaker_id": "2",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100035060.1.6",
          "form": "소개팅 상대는 마음에 들었니",
          "original_form": "소개팅 상대는 마음에 들었니",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100035060.1.7",
          "form": "일단 외모는  취향이었어",
          "original_form": "일단 외모는  취향이었어",
          "speaker_id": "2",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100035060.1.8",
          "form": "외모가 취향이면 성공적이네",
          "original_form": "외모가 취향이면 성공적이네",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100035060.1.9",
          "form": "문제는 성격이야",
          "original_form": "문제는 성격이야",
          "speaker_id": "2",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100035060.1.10",
          "form": "어떤 길래 그래",
          "original_form": "어떤 길래 그래",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100035060.1.11",
          "form": "남에 대한 배려가 부족해",
          "original_form": "남에 대한 배려가 부족해",
          "speaker_id": "2",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100035060.1.12",
          "form": "예시를 틀어줘",
          "original_form": "예시를 틀어줘",
          "speaker_id": "1",
          "time": "20210829 01:27"
        },
        {
          "id": "MDRW2100035060.1.13",
          "form": "내 얘기를 잘 안듣던데",
          "original_form": "내 얘기를 잘 안듣던데",
          "speaker_id": "2",
          "time": "20210829 01:27"
        },
        {
          "id": "MDRW2100035060.1.14",
          "form": "매너가 별로네",
          "original_form": "매너가 별로네",
          "speaker_id": "1",
          "time": "20210829 01:27"
