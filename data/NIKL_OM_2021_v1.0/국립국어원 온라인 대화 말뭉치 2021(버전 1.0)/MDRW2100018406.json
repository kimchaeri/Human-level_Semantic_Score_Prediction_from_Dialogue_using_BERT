      "utterance": [
        {
          "id": "MDRW2100018406.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100018406.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100018406.1.3",
          "form": "혹시 연애중이세요?",
          "original_form": "혹시 연애중이세요?",
          "speaker_id": "2",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100018406.1.4",
          "form": "연애하고계시나요?",
          "original_form": "연애하고계시나요?",
          "speaker_id": "1",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100018406.1.5",
          "form": "아 저는 솔로입니다 ㅠ",
          "original_form": "아 저는 솔로입니다 ㅠ",
          "speaker_id": "1",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100018406.1.6",
          "form": "앗 저도 솔로에요 ㅠㅠ",
          "original_form": "앗 저도 솔로에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018406.1.7",
          "form": "마지막 연애는 언제셨어요?",
          "original_form": "마지막 연애는 언제셨어요?",
          "speaker_id": "2",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018406.1.8",
          "form": "굉장히 오래됫어요 ㅠ",
          "original_form": "굉장히 오래됫어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018406.1.9",
          "form": "언제에요?",
          "original_form": "언제에요?",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100018406.1.10",
          "form": "저도 한 사오년 된거 같아요",
          "original_form": "저도 한 사오년 된거 같아요",
          "speaker_id": "2",
          "time": "20210827 01:17"
        },
        {
          "id": "MDRW2100018406.1.11",
          "form": "주변에서 소개팅하라고 뭐라 안하나요ㅋㅋㅋㅠ",
          "original_form": "주변에서 소개팅하라고 뭐라 안하나요ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210827 01:17"
        },
        {
          "id": "MDRW2100018406.1.12",
          "form": "그것도 한창 듣긴했는데 좀지나니까 딱히 그런말없어요 ㅋㅋㅋㅋ ㅠ",
          "original_form": "그것도 한창 듣긴했는데 좀지나니까 딱히 그런말없어요 ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100018406.1.13",
          "form": "저도 막 그렇게 관심이안생겨서 ㅜ",
          "original_form": "저도 막 그렇게 관심이안생겨서 ㅜ",
          "speaker_id": "1",
          "time": "20210827 01:19"
        },
        {
          "id": "MDRW2100018406.1.14",
          "form": "아ㅋㅋㅋ그쵸",
          "original_form": "아ㅋㅋㅋ그쵸",
          "speaker_id": "2",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100018406.1.15",
          "form": "저도 하기 귀찮은데 자꾸 주위에서 참견이 심하네요 ㅠㅠ",
          "original_form": "저도 하기 귀찮은데 자꾸 주위에서 참견이 심하네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100018406.1.16",
          "form": "만약 연애하시면 결혼 생각은 있으세요?",
          "original_form": "만약 연애하시면 결혼 생각은 있으세요?",
          "speaker_id": "2",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100018406.1.17",
          "form": "음 진짜 정말 너무 좋은 사람만나면 생각이 들수도있겠다? 이정도에요",
          "original_form": "음 진짜 정말 너무 좋은 사람만나면 생각이 들수도있겠다? 이정도에요",
          "speaker_id": "1",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100018406.1.18",
          "form": "딱히 결혼이나 아기가지고 이런마음은 없어여",
          "original_form": "딱히 결혼이나 아기가지고 이런마음은 없어여",
          "speaker_id": "1",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100018406.1.19",
          "form": "엇 저두요 전 확고한 비혼주의자랍니다",
          "original_form": "엇 저두요 전 확고한 비혼주의자랍니다",
          "speaker_id": "2",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100018406.1.20",
          "form": "결혼하기엔 귀찮은 일이 넘 많을거 같더라구요..ㅎㅎㅎ",
          "original_form": "결혼하기엔 귀찮은 일이 넘 많을거 같더라구요..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018406.1.21",
          "form": "아아 그렇군요! 외로움은 잘 안타는편이세요?",
          "original_form": "아아 그렇군요! 외로움은 잘 안타는편이세요?",
          "speaker_id": "1",
          "time": "20210827 01:24"
        },
        {
          "id": "MDRW2100018406.1.22",
          "form": "네네 맞아요 혼자 사는게 제일 편한 성격이에요",
          "original_form": "네네 맞아요 혼자 사는게 제일 편한 성격이에요",
          "speaker_id": "2",
          "time": "20210827 01:26"
        },
        {
          "id": "MDRW2100018406.1.23",
          "form": "name1은 어떠세요?",
          "original_form": "&name1&은 어떠세요?",
          "speaker_id": "2",
          "time": "20210827 01:27"
        },
        {
          "id": "MDRW2100018406.1.24",
          "form": "네네 저도 혼자 잘 지내요 ㅋㅋㅋㅋ혼자잘 돌아니구요",
          "original_form": "네네 저도 혼자 잘 지내요 ㅋㅋㅋㅋ혼자잘 돌아니구요",
          "speaker_id": "1",
          "time": "20210827 01:27"
        },
        {
          "id": "MDRW2100018406.1.25",
          "form": "근데 또 어떨땐 말할사람이 필요하긴 하도라구요",
          "original_form": "근데 또 어떨땐 말할사람이 필요하긴 하도라구요",
          "speaker_id": "1",
          "time": "20210827 01:27"
        },
        {
          "id": "MDRW2100018406.1.26",
          "form": "친구들도 점점 바쁘다보니 예전보다 대화 덜하는거같아요 ㅜ",
          "original_form": "친구들도 점점 바쁘다보니 예전보다 대화 덜하는거같아요 ㅜ",
          "speaker_id": "1",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100018406.1.27",
          "form": "아 맞아요 ㅠㅠ 나이들수록 친구들이랑 카톡텀이 뜸해지는 거 같아요",
          "original_form": "아 맞아요 ㅠㅠ 나이들수록 친구들이랑 카톡텀이 뜸해지는 거 같아요",
          "speaker_id": "2",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100018406.1.28",
          "form": "그래서 친구들 귀찮게 안하려고 열심히 취미를 늘리고 있어요..",
          "original_form": "그래서 친구들 귀찮게 안하려고 열심히 취미를 늘리고 있어요..",
          "speaker_id": "2",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100018406.1.29",
          "form": "맞아요 ㅠㅠ 연락도 맘편히 하기는 쉽지 않더라구요",
          "original_form": "맞아요 ㅠㅠ 연락도 맘편히 하기는 쉽지 않더라구요",
          "speaker_id": "1",
