      "utterance": [
        {
          "id": "MDRW2100044694.1.1",
          "form": "아침마다 일찍일어날려니 너무 피곤해여ㅠㅠ",
          "original_form": "아침마다 일찍일어날려니 너무 피곤해여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044694.1.2",
          "form": "힘들죠ㅠ\r\n아이들은 몇 시에 일어나나요?",
          "original_form": "힘들죠ㅠ\r\n아이들은 몇 시에 일어나나요?",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044694.1.3",
          "form": "첫째학교가니 7시30분 조금 넘어서 일어나야되는데",
          "original_form": "첫째학교가니 7시30분 조금 넘어서 일어나야되는데",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044694.1.4",
          "form": "안일어나도 되는 둘째도 그때 일어나서는...ㅠㅠ",
          "original_form": "안일어나도 되는 둘째도 그때 일어나서는...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044694.1.5",
          "form": "아ㅠㅠ 저희도 8시면 기상요",
          "original_form": "아ㅠㅠ 저희도 8시면 기상요",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044694.1.6",
          "form": "왜 그렇게 일찍 일어나는지...ㅠㅠ",
          "original_form": "왜 그렇게 일찍 일어나는지...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044694.1.7",
          "form": "잠도 없어여ㅠ",
          "original_form": "잠도 없어여ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044694.1.8",
          "form": "둘째들은 좀 더 자면 좋겠어여ㅠㅋㅋ",
          "original_form": "둘째들은 좀 더 자면 좋겠어여ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044694.1.9",
          "form": "하나 케어하기도 정신없는데 ㅋㅋ",
          "original_form": "하나 케어하기도 정신없는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044694.1.10",
          "form": "그니까여 ㅠㅠ 제에발...쫌...",
          "original_form": "그니까여 ㅠㅠ 제에발...쫌...",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044694.1.11",
          "form": "말귀는 다 알아들으니",
          "original_form": "말귀는 다 알아들으니",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044694.1.12",
          "form": "문앞에도 젤 먼저 가 있어요 ㅋㅋ",
          "original_form": "문앞에도 젤 먼저 가 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044694.1.13",
          "form": "귀엽네요 ㅎㅎㅎ 저희는 첫째가 혼자 가니깐 인사를 잘해줘서 안녕~~~~~~",
          "original_form": "귀엽네요 ㅎㅎㅎ 저희는 첫째가 혼자 가니깐 인사를 잘해줘서 안녕~~~~~~",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044694.1.14",
          "form": "아 첫째 혼자 가요?",
          "original_form": "아 첫째 혼자 가요?",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044694.1.15",
          "form": "네 3학년 이니까요 ㅎㅎ",
          "original_form": "네 3학년 이니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044694.1.16",
          "form": "여기는 단지내 학교라 1학년 좀만 가면 그담부터는 다 혼자가요 ㅎㅎ",
          "original_form": "여기는 단지내 학교라 1학년 좀만 가면 그담부터는 다 혼자가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044694.1.17",
          "form": "아 초품아인가보군용ㅋㅋ",
          "original_form": "아 초품아인가보군용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044694.1.18",
          "form": "네 ㅎㅎ 단지에서 바로 후문으로 들어가는건 정말 조은것 같아여",
          "original_form": "네 ㅎㅎ 단지에서 바로 후문으로 들어가는건 정말 조은것 같아여",
