      "utterance": [
        {
          "id": "MDRW2100027307.1.1",
          "form": "안녕하세요 최근에 뭐 사셨어요?",
          "original_form": "안녕하세요 최근에 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027307.1.2",
          "form": "음... 밀키트 구매했어요!",
          "original_form": "음... 밀키트 구매했어요!",
          "speaker_id": "2",
          "time": "20210902 11:24"
        },
        {
          "id": "MDRW2100027307.1.3",
          "form": "최근에 뭐 구매하셨나요?",
          "original_form": "최근에 뭐 구매하셨나요?",
          "speaker_id": "2",
          "time": "20210902 11:24"
        },
        {
          "id": "MDRW2100027307.1.4",
          "form": "오 맛있겠네요",
          "original_form": "오 맛있겠네요",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027307.1.5",
          "form": "전 엄청 많이 샀는데 일단 체중계요!",
          "original_form": "전 엄청 많이 샀는데 일단 체중계요!",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027307.1.6",
          "form": "오 체중계는 어디꺼 사셨어요? 저는 사두고 쓰질않아요 ㅋㅋㅋㅋㅋ",
          "original_form": "오 체중계는 어디꺼 사셨어요? 저는 사두고 쓰질않아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027307.1.7",
          "form": "밀키트는 어떤거 사셨어요?",
          "original_form": "밀키트는 어떤거 사셨어요?",
          "speaker_id": "1",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027307.1.8",
          "form": "ㅋㅋㅋ오아 인바디 체중계요! 이제 살 좀 빼려구요 ㅠㅠㅋㅋ",
          "original_form": "ㅋㅋㅋ오아 인바디 체중계요! 이제 살 좀 빼려구요 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027307.1.9",
          "form": "저는 프레시지? 할인하길래 스테이크랑 그 뭐냐 그 새우요리... 음... 그거샀어요 ㅋㅋㅋㅋ",
          "original_form": "저는 프레시지? 할인하길래 스테이크랑 그 뭐냐 그 새우요리... 음... 그거샀어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027307.1.10",
          "form": "아 분명... 분명 알았는데",
          "original_form": "아 분명... 분명 알았는데",
          "speaker_id": "1",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027307.1.11",
          "form": "저도 갑자기 기억이 안나네요.....",
          "original_form": "저도 갑자기 기억이 안나네요.....",
          "speaker_id": "1",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027307.1.12",
          "form": "뭔지아시져 그 마늘이랑 오일이랑",
          "original_form": "뭔지아시져 그 마늘이랑 오일이랑",
          "speaker_id": "2",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027307.1.13",
          "form": "빵찍어먹구 ㅋㅋㅋㅋ",
          "original_form": "빵찍어먹구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027307.1.14",
          "form": "네ㅋㅋㅋ이 답답함ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ이 답답함ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027307.1.15",
          "form": "프레시지가 밀키트 젤 맛있는거 같아요",
          "original_form": "프레시지가 밀키트 젤 맛있는거 같아요",
          "speaker_id": "1",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027307.1.16",
          "form": "세글자였는데...ㅋㅋㅋㅋㅋㅋㅋ가끔이래요 정말ㅋㅋㅋㅋ",
          "original_form": "세글자였는데...ㅋㅋㅋㅋㅋㅋㅋ가끔이래요 정말ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027307.1.17",
          "form": "오오 드셔보셨군요!!!",
          "original_form": "오오 드셔보셨군요!!!",
          "speaker_id": "2",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027307.1.18",
          "form": "어떤거 드셔보셨어요?",
          "original_form": "어떤거 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027307.1.19",
          "form": "저는 함박스테이크였나",
          "original_form": "저는 함박스테이크였나",
          "speaker_id": "1",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027307.1.20",
          "form": "큐브스테이크였나",
          "original_form": "큐브스테이크였나",
          "speaker_id": "1",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027307.1.21",
          "form": "야채랑 같이 볶아먹는거랑 밀푀유나베 먹었어요",
          "original_form": "야채랑 같이 볶아먹는거랑 밀푀유나베 먹었어요",
          "speaker_id": "1",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027307.1.22",
          "form": "제가 산거 감바스였어요 ㅋㅋㅋㅋ배송중인 목록 가서 확인하고 왔어요 ㅋㅋㅋㅋ 속시원 ㅋㅋㅋ하 감바스",
          "original_form": "제가 산거 감바스였어요 ㅋㅋㅋㅋ배송중인 목록 가서 확인하고 왔어요 ㅋㅋㅋㅋ 속시원 ㅋㅋㅋ하 감바스",
          "speaker_id": "2",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027307.1.23",
          "form": "악ㅋㅋㅋㅋㅋㅋㅋㅋ똠냥꿍 이런거 생각중이었는데",
          "original_form": "악ㅋㅋㅋㅋㅋㅋㅋㅋ똠냥꿍 이런거 생각중이었는데",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027307.1.24",
          "form": "오 ㅋㅋ다음엔 저도 그거 사봐야겠어요!!",
          "original_form": "오 ㅋㅋ다음엔 저도 그거 사봐야겠어요!!",
          "speaker_id": "2",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027307.1.25",
          "form": "감바스 들으니까 속시원하네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "감바스 들으니까 속시원하네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027307.1.26",
          "form": "저는 피쿡거 인바디 체중계 샀었는데",
