      "utterance": [
        {
          "id": "MDRW2100037658.1.1",
          "form": "아이고 ㅠㅠ",
          "original_form": "아이고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037658.1.2",
          "form": "저도 그래서 아예 파우치에",
          "original_form": "저도 그래서 아예 파우치에",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100037658.1.3",
          "form": "안약을 두 개씩 갖고 다녀요...",
          "original_form": "안약을 두 개씩 갖고 다녀요...",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100037658.1.4",
          "form": "저도 안약하고 알레르기약, 인공눈물 들고다녀요ㅠㅠ",
          "original_form": "저도 안약하고 알레르기약, 인공눈물 들고다녀요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 15:10"
        },
        {
          "id": "MDRW2100037658.1.5",
          "form": "아이고 저랑 비슷하시네요...",
          "original_form": "아이고 저랑 비슷하시네요...",
          "speaker_id": "2",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100037658.1.6",
          "form": "병원 가면 알러지 약만 처방해주지 않나요?ㅠ",
          "original_form": "병원 가면 알러지 약만 처방해주지 않나요?ㅠ",
          "speaker_id": "2",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100037658.1.7",
          "form": "저는 이번에간곳에서 저 3가지 다 처방해주셨어요",
          "original_form": "저는 이번에간곳에서 저 3가지 다 처방해주셨어요",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100037658.1.8",
          "form": "그러셨구나ㅠ",
          "original_form": "그러셨구나ㅠ",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037658.1.9",
          "form": "저는 알러지 약만 먹으면 이상하게 잠이 쏟아져요",
          "original_form": "저는 알러지 약만 먹으면 이상하게 잠이 쏟아져요",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037658.1.10",
          "form": "저도 이번에 처방받은약이 먹으면 졸리더라구요",
          "original_form": "저도 이번에 처방받은약이 먹으면 졸리더라구요",
          "speaker_id": "1",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100037658.1.11",
          "form": "어제 백신을 맞아서 또 나른하네요",
          "original_form": "어제 백신을 맞아서 또 나른하네요",
          "speaker_id": "1",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100037658.1.12",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100037658.1.13",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100037658.1.14",
          "form": "아직 1차만 맞았어요!",
          "original_form": "아직 1차만 맞았어요!",
          "speaker_id": "2",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100037658.1.15",
          "form": "저는 지르텍 먹으면 그렇게 잠이 쏟아지던데ㅠ",
          "original_form": "저는 지르텍 먹으면 그렇게 잠이 쏟아지던데ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100037658.1.16",
          "form": "그러시구나 1차맞고 어떠셨나요?",
          "original_form": "그러시구나 1차맞고 어떠셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100037658.1.17",
          "form": "저는 정말",
          "original_form": "저는 정말",
          "speaker_id": "2",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100037658.1.18",
          "form": "맞은 부위가",
          "original_form": "맞은 부위가",
          "speaker_id": "2",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100037658.1.19",
          "form": "하루이틀 아픈 거 빼고는",
          "original_form": "하루이틀 아픈 거 빼고는",
          "speaker_id": "2",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100037658.1.20",
          "form": "괜찮았어요!",
          "original_form": "괜찮았어요!",
          "speaker_id": "2",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100037658.1.21",
          "form": "저도 아직 팔만좀 욱신하고 괜찮네요",
          "original_form": "저도 아직 팔만좀 욱신하고 괜찮네요",
          "speaker_id": "1",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100037658.1.22",
          "form": "보통은 이런가봐요",
          "original_form": "보통은 이런가봐요",
          "speaker_id": "1",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100037658.1.23",
          "form": "얼른 일주일 지나서 맘편히 운동하고싶어요",
          "original_form": "얼른 일주일 지나서 맘편히 운동하고싶어요",
          "speaker_id": "1",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100037658.1.24",
          "form": "다행이네요 언제 맞으셨댔죠!?",
          "original_form": "다행이네요 언제 맞으셨댔죠!?",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100037658.1.25",
          "form": "어제요!",
          "original_form": "어제요!",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100037658.1.26",
          "form": "아 맞다맞다!",
          "original_form": "아 맞다맞다!",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100037658.1.27",
          "form": "사실 저는",
          "original_form": "사실 저는",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100037658.1.28",
          "form": "일주일 안지났는데도 필라테스 갔었어요ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "일주일 안지났는데도 필라테스 갔었어요ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:23"
        }
      ]
    }
