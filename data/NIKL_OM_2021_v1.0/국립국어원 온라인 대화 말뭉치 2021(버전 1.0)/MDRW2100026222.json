      "utterance": [
        {
          "id": "MDRW2100026222.1.1",
          "form": "동호회 활동하세요?",
          "original_form": "동호회 활동하세요?",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026222.1.2",
          "form": "예전에 독서모임를 오래했어요",
          "original_form": "예전에 독서모임를 오래했어요",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026222.1.3",
          "form": "지금은 안해요^^",
          "original_form": "지금은 안해요^^",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026222.1.4",
          "form": "아 멋지세요!!!",
          "original_form": "아 멋지세요!!!",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026222.1.5",
          "form": "코로나 끝나면 동호회 활동하고싶어요",
          "original_form": "코로나 끝나면 동호회 활동하고싶어요",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026222.1.6",
          "form": "어떤 동호회요?",
          "original_form": "어떤 동호회요?",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026222.1.7",
          "form": "지금 생각중이에요 독서 괜챃나요??",
          "original_form": "지금 생각중이에요 독서 괜챃나요??",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026222.1.8",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026222.1.9",
          "form": "일부러라도 책을 읽고",
          "original_form": "일부러라도 책을 읽고",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026222.1.10",
          "form": "다양한 생각을 들을 수 있어서",
          "original_form": "다양한 생각을 들을 수 있어서",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026222.1.11",
          "form": "추천해여",
          "original_form": "추천해여",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026222.1.12",
          "form": "아 진짜 다양한 생각을 받아들이는거 너무 중요하고 제게 필요해요!",
          "original_form": "아 진짜 다양한 생각을 받아들이는거 너무 중요하고 제게 필요해요!",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026222.1.13",
          "form": "근데 동호회나 모임하면 빌런이 꼭 있어요",
          "original_form": "근데 동호회나 모임하면 빌런이 꼭 있어요",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026222.1.14",
          "form": "그게 문제예요 ㅎㅎ",
          "original_form": "그게 문제예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026222.1.15",
          "form": "아..맞아요... 또라이질량보존의 법칙... ㅋㄱ",
          "original_form": "아..맞아요... 또라이질량보존의 법칙... ㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026222.1.16",
          "form": "어디든 핀트나간 사람들도 있고요",
          "original_form": "어디든 핀트나간 사람들도 있고요",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026222.1.17",
          "form": "좋은 모임인데",
          "original_form": "좋은 모임인데",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026222.1.18",
          "form": "한 사람땜에 나가기 싫어진 적도 있구요",
          "original_form": "한 사람땜에 나가기 싫어진 적도 있구요",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026222.1.19",
          "form": "그럼 진짜 아쉬울거같아요ㅠ 정든 모임인데요ㅜ",
          "original_form": "그럼 진짜 아쉬울거같아요ㅠ 정든 모임인데요ㅜ",
          "speaker_id": "1",
          "time": "20210902 07:26"
        },
        {
          "id": "MDRW2100026222.1.20",
          "form": "그래서 나중엔 지인들 추천한 사람만 들어오는 모님으로 운영했어요 그러니까 굿굿",
          "original_form": "그래서 나중엔 지인들 추천한 사람만 들어오는 모님으로 운영했어요 그러니까 굿굿",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100026222.1.21",
          "form": "그럼 필터링이 되니까 좋네요!",
          "original_form": "그럼 필터링이 되니까 좋네요!",
