      "utterance": [
        {
          "id": "MDRW2100000321.1.1",
          "form": "집에서 집안일 해?",
          "original_form": "집에서 집안일 해?",
          "speaker_id": "1",
          "time": "20210818 15:03"
        },
        {
          "id": "MDRW2100000321.1.2",
          "form": "나는 오늘 아침 설거지하고 출근했어",
          "original_form": "나는 오늘 아침 설거지하고 출근했어",
          "speaker_id": "1",
          "time": "20210818 15:03"
        },
        {
          "id": "MDRW2100000321.1.3",
          "form": "나는 오늘 저녁때 집에가면 저녁차리고 방청소를 할거야",
          "original_form": "나는 오늘 저녁때 집에가면 저녁차리고 방청소를 할거야",
          "speaker_id": "2",
          "time": "20210818 15:07"
        },
        {
          "id": "MDRW2100000321.1.4",
          "form": "안하잖아 ㅋㅋ",
          "original_form": "안하잖아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210818 15:08"
        },
        {
          "id": "MDRW2100000321.1.5",
          "form": "거짓말하지마",
          "original_form": "거짓말하지마",
          "speaker_id": "1",
          "time": "20210818 15:08"
        },
        {
          "id": "MDRW2100000321.1.6",
          "form": "해야돼 오늘은 초과안하는 날이라 일찍퇴근하는데",
          "original_form": "해야돼 오늘은 초과안하는 날이라 일찍퇴근하는데",
          "speaker_id": "2",
          "time": "20210818 15:10"
        },
        {
          "id": "MDRW2100000321.1.7",
          "form": "엄빠 저녁약속이라 차려먹어야대 진짜ㅋㅋ",
          "original_form": "엄빠 저녁약속이라 차려먹어야대 진짜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210818 15:11"
        },
        {
          "id": "MDRW2100000321.1.8",
          "form": "그렇고만",
          "original_form": "그렇고만",
          "speaker_id": "1",
          "time": "20210818 15:11"
        },
        {
          "id": "MDRW2100000321.1.9",
          "form": "나는 name3이 입덧해서",
          "original_form": "나는 &name3&이 입덧해서",
          "speaker_id": "1",
          "time": "20210818 15:11"
        },
        {
          "id": "MDRW2100000321.1.10",
          "form": "내가 집안일 해",
          "original_form": "내가 집안일 해",
          "speaker_id": "1",
          "time": "20210818 15:11"
        },
        {
          "id": "MDRW2100000321.1.11",
          "form": "벌써 입덧시작? ㄷㄷ",
          "original_form": "벌써 입덧시작? ㄷㄷ",
          "speaker_id": "2",
          "time": "20210818 15:11"
        },
        {
          "id": "MDRW2100000321.1.12",
          "form": "어제는 집에 감자가 많길래",
          "original_form": "어제는 집에 감자가 많길래",
          "speaker_id": "1",
          "time": "20210818 15:11"
        },
        {
          "id": "MDRW2100000321.1.13",
          "form": "감자 수프했어",
          "original_form": "감자 수프했어",
          "speaker_id": "1",
          "time": "20210818 15:12"
        },
        {
          "id": "MDRW2100000321.1.14",
          "form": "올~~~~~",
          "original_form": "올~~~~~",
          "speaker_id": "2",
          "time": "20210818 15:12"
        },
        {
          "id": "MDRW2100000321.1.15",
          "form": "감자삶고",
          "original_form": "감자삶고",
          "speaker_id": "1",
          "time": "20210818 15:12"
        },
        {
          "id": "MDRW2100000321.1.16",
          "form": "감자샐러드먹고싶네 갑자기 감자얘기하니까",
          "original_form": "감자샐러드먹고싶네 갑자기 감자얘기하니까",
          "speaker_id": "2",
          "time": "20210818 15:12"
        },
        {
          "id": "MDRW2100000321.1.17",
          "form": "양파를 빠다에 볶고",
          "original_form": "양파를 빠다에 볶고",
          "speaker_id": "1",
          "time": "20210818 15:12"
        },
        {
          "id": "MDRW2100000321.1.18",
          "form": "갈아서",
          "original_form": "갈아서",
          "speaker_id": "1",
          "time": "20210818 15:12"
        },
        {
          "id": "MDRW2100000321.1.19",
          "form": "삶으믄돼",
          "original_form": "삶으믄돼",
          "speaker_id": "1",
          "time": "20210818 15:12"
        },
        {
          "id": "MDRW2100000321.1.20",
          "form": "치즈넣고",
          "original_form": "치즈넣고",
          "speaker_id": "1",
          "time": "20210818 15:12"
        },
        {
          "id": "MDRW2100000321.1.21",
          "form": "모짜렐라?",
          "original_form": "모짜렐라?",
