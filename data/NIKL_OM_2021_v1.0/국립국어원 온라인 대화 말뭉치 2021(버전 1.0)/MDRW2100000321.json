          "time": "20210818 15:14"
        },
        {
          "id": "MDRW2100000321.1.27",
          "form": "비때문에 습기차서",
          "original_form": "비때문에 습기차서",
          "speaker_id": "1",
          "time": "20210818 15:14"
        },
        {
          "id": "MDRW2100000321.1.28",
          "form": "쌀이 다 맛이 갔어",
          "original_form": "쌀이 다 맛이 갔어",
          "speaker_id": "1",
          "time": "20210818 15:14"
        },
        {
          "id": "MDRW2100000321.1.29",
          "form": "냄새 안촣고",
          "original_form": "냄새 안촣고",
          "speaker_id": "1",
          "time": "20210818 15:14"
        },
        {
          "id": "MDRW2100000321.1.30",
          "form": "쌀은 밀폐용기에 넣어서",
