      "utterance": [
        {
          "id": "MDRW2100042811.1.1",
          "form": "오 그럼 지금",
          "original_form": "오 그럼 지금",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042811.1.2",
          "form": "갈아타셔야하나욤",
          "original_form": "갈아타셔야하나욤",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042811.1.3",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042811.1.4",
          "form": "비싸서못 사여ㅠㅠㅠ",
          "original_form": "비싸서못 사여ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042811.1.5",
          "form": "엄청 비싸드라구욤 흑",
          "original_form": "엄청 비싸드라구욤 흑",
          "speaker_id": "2",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042811.1.6",
          "form": "ㅜㅠ 요즘은 싼폰이없는것같아여ㅜ",
          "original_form": "ㅜㅠ 요즘은 싼폰이없는것같아여ㅜ",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042811.1.7",
          "form": "요금제도 넘 비싸서",
          "original_form": "요금제도 넘 비싸서",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042811.1.8",
          "form": "알뜰요금제가 대세더라구요",
          "original_form": "알뜰요금제가 대세더라구요",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042811.1.9",
          "form": "맞아여ㅠㅠ 8만원짜리ㅠㅠ",
          "original_form": "맞아여ㅠㅠ 8만원짜리ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042811.1.10",
          "form": "쇼핑하신것들중에",
          "original_form": "쇼핑하신것들중에",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042811.1.11",
          "form": "잘산템 잇으신가요?",
          "original_form": "잘산템 잇으신가요?",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042811.1.12",
          "form": "저는 손목보호대 사고",
          "original_form": "저는 손목보호대 사고",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042811.1.13",
          "form": "식세기요ㅎ",
          "original_form": "식세기요ㅎ",
          "speaker_id": "1",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042811.1.14",
          "form": "주부라",
          "original_form": "주부라",
          "speaker_id": "1",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042811.1.15",
          "form": "삶의질이달라졋어용ㅎㅎ",
          "original_form": "삶의질이달라졋어용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042811.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042811.1.17",
          "form": "손목보호대  애낳을때 써봤어여ㅋㅋㅋ",
          "original_form": "손목보호대  애낳을때 써봤어여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042811.1.18",
          "form": "낳고나서 ㅎㅎ",
          "original_form": "낳고나서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042811.1.19",
          "form": "저는 일할때ㅋㅋ지금도 차고잇어요",
          "original_form": "저는 일할때ㅋㅋ지금도 차고잇어요",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042811.1.20",
          "form": "ㅜㅜ진작살껄ㅜㅜ",
          "original_form": "ㅜㅜ진작살껄ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042811.1.21",
          "form": "식세기 건조기 필수템이라고하던데",
          "original_form": "식세기 건조기 필수템이라고하던데",
          "speaker_id": "2",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100042811.1.22",
          "form": "진짜좋나바여",
          "original_form": "진짜좋나바여",
          "speaker_id": "2",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100042811.1.23",
          "form": "네 넘편해요~",
          "original_form": "네 넘편해요~",
          "speaker_id": "1",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100042811.1.24",
          "form": "시간도 확줄고ㅡ",
          "original_form": "시간도 확줄고ㅡ",
          "speaker_id": "1",
          "time": "20210907 10:22"
        },
        {
          "id": "MDRW2100042811.1.25",
          "form": "저도 내년에 혼수템에 넣어놧어요 ㅋ",
          "original_form": "저도 내년에 혼수템에 넣어놧어요 ㅋ",
