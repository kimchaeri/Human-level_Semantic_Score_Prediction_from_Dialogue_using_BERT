      "utterance": [
        {
          "id": "MDRW2100007053.1.1",
          "form": "결혼하셨어요?ㅋ",
          "original_form": "결혼하셨어요?ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:25"
        },
        {
          "id": "MDRW2100007053.1.2",
          "form": "아뇨 ㅋㅋㅋㅋ!!",
          "original_form": "아뇨 ㅋㅋㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210822 09:25"
        },
        {
          "id": "MDRW2100007053.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007053.1.4",
          "form": "30대넘어가면 많이 하셨더라구요",
          "original_form": "30대넘어가면 많이 하셨더라구요",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007053.1.5",
          "form": "저는 결혼했어요ㅋ",
          "original_form": "저는 결혼했어요ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007053.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007053.1.7",
          "form": "저는 생각없어요ㅜ",
          "original_form": "저는 생각없어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007053.1.8",
          "form": "사이좋으신가용 ㅋㅋ",
          "original_form": "사이좋으신가용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007053.1.9",
          "form": "아 연애는 하세요?",
          "original_form": "아 연애는 하세요?",
          "speaker_id": "2",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007053.1.10",
          "form": "아뇨 안해요 ㅋㅋ 전혀",
          "original_form": "아뇨 안해요 ㅋㅋ 전혀",
          "speaker_id": "1",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007053.1.11",
          "form": "아에 생각이없으신가요?",
          "original_form": "아에 생각이없으신가요?",
          "speaker_id": "2",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007053.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007053.1.13",
          "form": "연애감정이랄게있는지도모르겠오요 전ㅋㅋ",
          "original_form": "연애감정이랄게있는지도모르겠오요 전ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:27"
        },
        {
          "id": "MDRW2100007053.1.14",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210822 09:28"
        },
        {
          "id": "MDRW2100007053.1.15",
          "form": "연애감정을 잘 못느끼셨나봐요",
          "original_form": "연애감정을 잘 못느끼셨나봐요",
          "speaker_id": "2",
          "time": "20210822 09:28"
        },
        {
          "id": "MDRW2100007053.1.16",
          "form": "네 그전에 제가 원체 사람안만나고 혼자있는거좋아해서 ㅋㅋ",
          "original_form": "네 그전에 제가 원체 사람안만나고 혼자있는거좋아해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007053.1.17",
          "form": "혼자서만 살거같아요 ㅜㅜㅋㅋㅋ",
          "original_form": "혼자서만 살거같아요 ㅜㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007053.1.18",
          "form": "아 ㅋㅋㅋ저도 사실 결혼을 늦게하려고했는데",
          "original_form": "아 ㅋㅋㅋ저도 사실 결혼을 늦게하려고했는데",
          "speaker_id": "2",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007053.1.19",
          "form": "좀 일찍한편이고ㅋㅋ",
          "original_form": "좀 일찍한편이고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007053.1.20",
          "form": "주변에는",
          "original_form": "주변에는",
          "speaker_id": "2",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007053.1.21",
          "form": "결혼은 선택이라고 하네요",
          "original_form": "결혼은 선택이라고 하네요",
          "speaker_id": "2",
          "time": "20210822 09:30"
        },
        {
          "id": "MDRW2100007053.1.22",
          "form": "ㅋㅋㅋ필수가 아니고요",
          "original_form": "ㅋㅋㅋ필수가 아니고요",
          "speaker_id": "2",
          "time": "20210822 09:30"
        },
        {
          "id": "MDRW2100007053.1.23",
          "form": "네 전 뭔가\r\n인지능력이 생겼을때부터\r\n결혼안해야지 했었는데",
          "original_form": "네 전 뭔가\r\n인지능력이 생겼을때부터\r\n결혼안해야지 했었는데",
          "speaker_id": "1",
          "time": "20210822 09:30"
        },
        {
          "id": "MDRW2100007053.1.24",
          "form": "아무도안믿더라구요 그런애가 제일 빨리간다고",
          "original_form": "아무도안믿더라구요 그런애가 제일 빨리간다고",
          "speaker_id": "1",
          "time": "20210822 09:30"
        },
        {
          "id": "MDRW2100007053.1.25",
          "form": "자녀계획은 있으신가용",
          "original_form": "자녀계획은 있으신가용",
          "speaker_id": "1",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007053.1.26",
          "form": "저는 딸하나 아들하나있어요",
          "original_form": "저는 딸하나 아들하나있어요",
          "speaker_id": "2",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007053.1.27",
          "form": "자녀계획은 끝났고ㅋ 열심히 키우고있는중이랍니다ㅋ",
          "original_form": "자녀계획은 끝났고ㅋ 열심히 키우고있는중이랍니다ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007053.1.28",
          "form": "아하 ㅋㅋㅋㅋㅋㅋ 몇살이에요!",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋ 몇살이에요!",
          "speaker_id": "1",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007053.1.29",
          "form": "35살이에요ㅋㅋ",
          "original_form": "35살이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007053.1.30",
          "form": "아니 자녀분이 ㅋㅋㅋㅋ",
          "original_form": "아니 자녀분이 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007053.1.31",
          "form": "28살에 첫째가 태어났어요.ㅋ",
          "original_form": "28살에 첫째가 태어났어요.ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007053.1.32",
          "form": "오왕",
          "original_form": "오왕",
          "speaker_id": "1",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007053.1.33",
          "form": "낳는거 죽다살아나는거라던데",
          "original_form": "낳는거 죽다살아나는거라던데",
          "speaker_id": "1",
