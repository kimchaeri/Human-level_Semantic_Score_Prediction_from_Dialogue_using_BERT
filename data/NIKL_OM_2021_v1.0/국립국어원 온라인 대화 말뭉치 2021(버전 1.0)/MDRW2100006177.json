      "utterance": [
        {
          "id": "MDRW2100006177.1.1",
          "form": "자기 2차맞고 좀 어때?",
          "original_form": "자기 2차맞고 좀 어때?",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006177.1.2",
          "form": "주말에 그렇게 헤롱거리더니",
          "original_form": "주말에 그렇게 헤롱거리더니",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006177.1.3",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006177.1.4",
          "form": "이제 좀 괜찮아진거 같애",
          "original_form": "이제 좀 괜찮아진거 같애",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006177.1.5",
          "form": "너무 힘들었다구",
          "original_form": "너무 힘들었다구",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006177.1.6",
          "form": "고생했엉",
          "original_form": "고생했엉",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006177.1.7",
          "form": "2차가 이렇게 아플줄이야",
          "original_form": "2차가 이렇게 아플줄이야",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100006177.1.8",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100006177.1.9",
          "form": "그냥 안아프고 지나가는사람도 있던데",
          "original_form": "그냥 안아프고 지나가는사람도 있던데",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006177.1.10",
          "form": "울 언니는 그냥 지나다가시피 했는데",
          "original_form": "울 언니는 그냥 지나다가시피 했는데",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006177.1.11",
          "form": "난 아닌가봐",
          "original_form": "난 아닌가봐",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006177.1.12",
          "form": "사람마다 다 다르대",
          "original_form": "사람마다 다 다르대",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006177.1.13",
          "form": "난 어떨라나",
          "original_form": "난 어떨라나",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006177.1.14",
          "form": "주변에 그냥 가볍게 지나가는 사람도 꽤 있더라고",
          "original_form": "주변에 그냥 가볍게 지나가는 사람도 꽤 있더라고",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006177.1.15",
          "form": "좀 쉬어주고 그렇게 넘어갔으면 좋겠다",
          "original_form": "좀 쉬어주고 그렇게 넘어갔으면 좋겠다",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006177.1.16",
          "form": "맞고나서 14일지나야 항체 생긴대",
          "original_form": "맞고나서 14일지나야 항체 생긴대",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006177.1.17",
          "form": "근데이거 맞으며ㆍ",
          "original_form": "근데이거 맞으며ㆍ",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006177.1.18",
          "form": "안걸리는건 맞나?",
          "original_form": "안걸리는건 맞나?",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006177.1.19",
          "form": "마스크 잘쓰고 하면",
          "original_form": "마스크 잘쓰고 하면",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006177.1.20",
          "form": "안걸리지 않을까??",
          "original_form": "안걸리지 않을까??",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006177.1.21",
          "form": "그럴라나",
          "original_form": "그럴라나",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006177.1.22",
          "form": "하도 돌파감염 어쩌구 그래서",
          "original_form": "하도 돌파감염 어쩌구 그래서",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006177.1.23",
          "form": "불안한건 어쩔수 없지만",
          "original_form": "불안한건 어쩔수 없지만",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100006177.1.24",
          "form": "백신 안맞는거 보단 나으니 ㅎㅎ",
          "original_form": "백신 안맞는거 보단 나으니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:35"
        }
      ]
