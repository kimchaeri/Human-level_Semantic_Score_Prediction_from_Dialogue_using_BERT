      "utterance": [
        {
          "id": "MDRW2100002072.1.1",
          "form": "뭐하구있옹",
          "original_form": "뭐하구있옹",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002072.1.2",
          "form": "침대랑 혼연일체",
          "original_form": "침대랑 혼연일체",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002072.1.3",
          "form": "나도 누워서 핸드폰보는중",
          "original_form": "나도 누워서 핸드폰보는중",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002072.1.4",
          "form": "반려동물님은 뭐하고있으심",
          "original_form": "반려동물님은 뭐하고있으심",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002072.1.5",
          "form": "나는 씻고 머리말리는중이여",
          "original_form": "나는 씻고 머리말리는중이여",
          "speaker_id": "1",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002072.1.6",
          "form": "아니ㅋㅋ 반려동물 없어야?",
          "original_form": "아니ㅋㅋ 반려동물 없어야?",
          "speaker_id": "2",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002072.1.7",
          "form": "아~ㅋㅋㅋㅋ웅 근데 고양이 좋아해",
          "original_form": "아~ㅋㅋㅋㅋ웅 근데 고양이 좋아해",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002072.1.8",
          "form": "울집은 주인님 한분 계셔",
          "original_form": "울집은 주인님 한분 계셔",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002072.1.9",
          "form": "매일 캔따개 하는중이여",
          "original_form": "매일 캔따개 하는중이여",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002072.1.10",
          "form": "오 캔따개구나",
          "original_form": "오 캔따개구나",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002072.1.11",
          "form": "고양이 너무 귀여워",
          "original_form": "고양이 너무 귀여워",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002072.1.12",
          "form": "그거 알어?",
          "original_form": "그거 알어?",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002072.1.13",
          "form": "고양이는 사실 액체가 아니여",
          "original_form": "고양이는 사실 액체가 아니여",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002072.1.14",
          "form": "액체가 아니면 뭐여?",
          "original_form": "액체가 아니면 뭐여?",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002072.1.15",
          "form": "몰러. 정확히는 모르겠어",
          "original_form": "몰러. 정확히는 모르겠어",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002072.1.16",
          "form": "일단 확실한건 마셔봤는디 안마셔졌어",
          "original_form": "일단 확실한건 마셔봤는디 안마셔졌어",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002072.1.17",
          "form": "ㅋㄱㅋㄱㅋ아 귀욥겠다 나도 고양이ㅠ",
          "original_form": "ㅋㄱㅋㄱㅋ아 귀욥겠다 나도 고양이ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002072.1.18",
          "form": "캣타워 올라가서 날 내려다봐줬으면 좋겠어",
          "original_form": "캣타워 올라가서 날 내려다봐줬으면 좋겠어",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002072.1.19",
          "form": "지배당하는걸 좋아하는구나",
          "original_form": "지배당하는걸 좋아하는구나",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002072.1.20",
          "form": "그렇다면 넌 집사가 적성이야",
          "original_form": "그렇다면 넌 집사가 적성이야",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002072.1.21",
          "form": "ㅋㅋㅋㅋㅋ언젠간 꼭 데려온다 고양이..",
          "original_form": "ㅋㅋㅋㅋㅋ언젠간 꼭 데려온다 고양이..",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002072.1.22",
          "form": "꼭 두분 모셔라",
          "original_form": "꼭 두분 모셔라",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002072.1.23",
          "form": "한분만 모시니까 굉장히 무료해하심",
          "original_form": "한분만 모시니까 굉장히 무료해하심",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002072.1.24",
          "form": "집사는 그게 참 안타깝다",
          "original_form": "집사는 그게 참 안타깝다",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002072.1.25",
          "form": "돈도 한분이나 두분이나 비슷하게 나가는데",
          "original_form": "돈도 한분이나 두분이나 비슷하게 나가는데",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002072.1.26",
          "form": "아 구래??사료나 간식 두배로 나갈줄알았더니",
          "original_form": "아 구래??사료나 간식 두배로 나갈줄알았더니",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002072.1.27",
          "form": "그르케 많이를 안잡솨",
          "original_form": "그르케 많이를 안잡솨",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002072.1.28",
          "form": "혼자있으면 많이 심심해하시더라구",
          "original_form": "혼자있으면 많이 심심해하시더라구",
          "speaker_id": "2",
          "time": "20210819 23:48"
        }
      ]
    }
  ]
