      "utterance": [
        {
          "id": "MDRW2100038651.1.1",
          "form": "비오나용???",
          "original_form": "비오나용???",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100038651.1.2",
          "form": "아까 잠깐 오더니 그쳤어요!",
          "original_form": "아까 잠깐 오더니 그쳤어요!",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100038651.1.3",
          "form": "비 오나요??",
          "original_form": "비 오나요??",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100038651.1.4",
          "form": "저희도 지금 그쳤어용 !",
          "original_form": "저희도 지금 그쳤어용 !",
          "speaker_id": "1",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100038651.1.5",
          "form": "아깐 심하게 왔는데ㅜㅜ",
          "original_form": "아깐 심하게 왔는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100038651.1.6",
          "form": "장마가 왜 이렇게 오락가락하게 오는걸까요 ㅋㅋㅋㅋ 올거면 오고! 말거면 가고!",
          "original_form": "장마가 왜 이렇게 오락가락하게 오는걸까요 ㅋㅋㅋㅋ 올거면 오고! 말거면 가고!",
          "speaker_id": "2",
          "time": "20210902 19:44"
        },
        {
          "id": "MDRW2100038651.1.7",
          "form": "맞아요 !!!!",
          "original_form": "맞아요 !!!!",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100038651.1.8",
          "form": "차라리 확 와라 !!!",
          "original_form": "차라리 확 와라 !!!",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100038651.1.9",
          "form": "아 근데 어제 확 왔는데 하",
          "original_form": "아 근데 어제 확 왔는데 하",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100038651.1.10",
          "form": "극혐이였어여 집가는길...",
          "original_form": "극혐이였어여 집가는길...",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100038651.1.11",
          "form": "갑자기 공격 받으셨네요ㅠㅠ",
          "original_form": "갑자기 공격 받으셨네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100038651.1.12",
          "form": "새삼 가을장마가 이렇게 길었나싶어요",
          "original_form": "새삼 가을장마가 이렇게 길었나싶어요",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100038651.1.13",
          "form": "여름장마가 짧았어서",
          "original_form": "여름장마가 짧았어서",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100038651.1.14",
          "form": "더 긴가봐요 ㅜㅜㅜ",
          "original_form": "더 긴가봐요 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100038651.1.15",
          "form": "장마가 길어지면 모기 개체수가 폭발한다는데 우울하네요 ㅎㅎ",
          "original_form": "장마가 길어지면 모기 개체수가 폭발한다는데 우울하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100038651.1.16",
          "form": "헐 진짜요 ㅜㅠ?",
          "original_form": "헐 진짜요 ㅜㅠ?",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038651.1.17",
          "form": "이번 여름 모기가 잘 안보이더니",
          "original_form": "이번 여름 모기가 잘 안보이더니",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038651.1.18",
          "form": "가을에 다 나오겠군요...?",
          "original_form": "가을에 다 나오겠군요...?",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038651.1.19",
          "form": "저는 어제도 두 마리를 잡았답니다...",
          "original_form": "저는 어제도 두 마리를 잡았답니다...",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100038651.1.20",
          "form": "야무지게 피도 잘 먹었더라구요...",
          "original_form": "야무지게 피도 잘 먹었더라구요...",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100038651.1.21",
          "form": "헐 진짜여..?",
          "original_form": "헐 진짜여..?",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100038651.1.22",
          "form": "전 제 방에 안들어오도록",
          "original_form": "전 제 방에 안들어오도록",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100038651.1.23",
          "form": "노력하지만 곧 모기가 들어오겠군요...",
          "original_form": "노력하지만 곧 모기가 들어오겠군요...",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100038651.1.24",
          "form": "또 모기가 제 피를 좋아한답니다 하하",
          "original_form": "또 모기가 제 피를 좋아한답니다 하하",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100038651.1.25",
          "form": "o형 이신가요 ㅎㅎ",
          "original_form": "o형 이신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100038651.1.26",
          "form": "엇 AB형인데",
          "original_form": "엇 AB형인데",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038651.1.27",
          "form": "몸 온도가 높아여ㅜㅜ",
          "original_form": "몸 온도가 높아여ㅜㅜ",
