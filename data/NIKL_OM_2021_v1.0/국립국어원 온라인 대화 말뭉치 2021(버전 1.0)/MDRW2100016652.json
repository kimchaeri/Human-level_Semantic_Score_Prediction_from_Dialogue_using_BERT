      "utterance": [
        {
          "id": "MDRW2100016652.1.1",
          "form": "요새하는게임이있나요",
          "original_form": "요새하는게임이있나요",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016652.1.2",
          "form": "모바일로 가끔 루미큐브 합니다!",
          "original_form": "모바일로 가끔 루미큐브 합니다!",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016652.1.3",
          "form": "언니는?",
          "original_form": "언니는?",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016652.1.4",
          "form": "모바일도있다니",
          "original_form": "모바일도있다니",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016652.1.5",
          "form": "어제 만난사람이 류미큐브달인이라고 나알려주고싶다햇는데",
          "original_form": "어제 만난사람이 류미큐브달인이라고 나알려주고싶다햇는데",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016652.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016652.1.7",
          "form": "어제 만난 사람이 누군데?ㅋㅋㅋ달인이라니!!",
          "original_form": "어제 만난 사람이 누군데?ㅋㅋㅋ달인이라니!!",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016652.1.8",
          "form": "ㅋㅋㅋㅋ잇어",
          "original_form": "ㅋㅋㅋㅋ잇어",
          "speaker_id": "1",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016652.1.9",
          "form": "알려하지마 다쳐",
          "original_form": "알려하지마 다쳐",
          "speaker_id": "1",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016652.1.10",
          "form": "ㅋㅋㅋ저번에 사촌언니와서 내기했는데 나 꼴찌해서 아이스크림 쐈어",
          "original_form": "ㅋㅋㅋ저번에 사촌언니와서 내기했는데 나 꼴찌해서 아이스크림 쐈어",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016652.1.11",
          "form": "ㅋㅋㅋㅋ으그",
          "original_form": "ㅋㅋㅋㅋ으그",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016652.1.12",
          "form": "비법전수받아서 알려줘 ㅋㅋㅋ",
          "original_form": "비법전수받아서 알려줘 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016652.1.13",
          "form": "분발하세요",
          "original_form": "분발하세요",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016652.1.14",
          "form": "또오면 복수하겠다 ㅋㅋㅋ",
          "original_form": "또오면 복수하겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016652.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그럼안되",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그럼안되",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016652.1.16",
          "form": "승부욕도 중요하지만",
          "original_form": "승부욕도 중요하지만",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016652.1.17",
          "form": "되려당할수있다고",
          "original_form": "되려당할수있다고",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016652.1.18",
          "form": "포커페이스를 유지해!",
          "original_form": "포커페이스를 유지해!",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016652.1.19",
          "form": "ㅋㅋㅋㅋ더 큰 상품을 걸고 내기하겠다",
          "original_form": "ㅋㅋㅋㅋ더 큰 상품을 걸고 내기하겠다",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016652.1.20",
          "form": "그럼 게임이안되요",
          "original_form": "그럼 게임이안되요",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016652.1.21",
          "form": "ㅋㅋㅋ이번엔 과자파티로!!!ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ이번엔 과자파티로!!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016652.1.22",
          "form": "예전에 친구 집들이에서 보드게임 할리갈리하다가 내 친구 피봄",
          "original_form": "예전에 친구 집들이에서 보드게임 할리갈리하다가 내 친구 피봄",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016652.1.23",
          "form": "ㅋㅋㅋㅋㅋ다 큰 어른들이 주책바가지야",
          "original_form": "ㅋㅋㅋㅋㅋ다 큰 어른들이 주책바가지야",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016652.1.24",
          "form": "ㅋㅋㅋ난 블루마블밖에모루눈디",
          "original_form": "ㅋㅋㅋ난 블루마블밖에모루눈디",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016652.1.25",
          "form": "ㅋㅋㅋ맞아 그것도 재밌지ㅋㅋ 모바일로도 했었는데 모두의 마블!",
          "original_form": "ㅋㅋㅋ맞아 그것도 재밌지ㅋㅋ 모바일로도 했었는데 모두의 마블!",
          "speaker_id": "2",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016652.1.26",
          "form": "요새는진짜하는게없네 ㅋㅋ겜말고 할께많아",
          "original_form": "요새는진짜하는게없네 ㅋㅋ겜말고 할께많아",
          "speaker_id": "1",
