      "utterance": [
        {
          "id": "MDRW2100023027.1.1",
          "form": "눈 많이 불편해?",
          "original_form": "눈 많이 불편해?",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023027.1.2",
          "form": "ㅇㅇ 핸드폰이나 화면 보면 금세 뻑뻑한 느낌이 들어서",
          "original_form": "ㅇㅇ 핸드폰이나 화면 보면 금세 뻑뻑한 느낌이 들어서",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023027.1.3",
          "form": "일회용 인공눈물은 많이 넣어도 될거야 방부제 없어서",
          "original_form": "일회용 인공눈물은 많이 넣어도 될거야 방부제 없어서",
          "speaker_id": "2",
          "time": "20210829 20:45"
        },
        {
          "id": "MDRW2100023027.1.4",
          "form": "일회용 말고 다회용은?",
          "original_form": "일회용 말고 다회용은?",
          "speaker_id": "1",
          "time": "20210829 20:45"
        },
        {
          "id": "MDRW2100023027.1.5",
          "form": "그건 방부제 있어서 너무 많이 넣으면 안된대",
          "original_form": "그건 방부제 있어서 너무 많이 넣으면 안된대",
          "speaker_id": "2",
          "time": "20210829 20:45"
        },
        {
          "id": "MDRW2100023027.1.6",
          "form": "그렇구나 ㅎㅎ 어찌 알았대?",
          "original_form": "그렇구나 ㅎㅎ 어찌 알았대?",
          "speaker_id": "1",
          "time": "20210829 20:45"
        },
        {
          "id": "MDRW2100023027.1.7",
          "form": "예전에 나 안구건조 때문에 안과 많이 다녔거든",
          "original_form": "예전에 나 안구건조 때문에 안과 많이 다녔거든",
          "speaker_id": "2",
          "time": "20210829 20:45"
        },
        {
          "id": "MDRW2100023027.1.8",
          "form": "아.. 나도 요즘 안구건조가 심하네",
          "original_form": "아.. 나도 요즘 안구건조가 심하네",
          "speaker_id": "1",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100023027.1.9",
          "form": "약국에서 사지 말고 병원에서 처방받는 것도 괜찮아",
          "original_form": "약국에서 사지 말고 병원에서 처방받는 것도 괜찮아",
          "speaker_id": "2",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100023027.1.10",
          "form": "인공눈물도 처방해주나?",
          "original_form": "인공눈물도 처방해주나?",
          "speaker_id": "1",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100023027.1.11",
          "form": "그럼~ 진료 받으면 다 처방해줌",
          "original_form": "그럼~ 진료 받으면 다 처방해줌",
          "speaker_id": "2",
          "time": "20210829 20:47"
        },
        {
          "id": "MDRW2100023027.1.12",
          "form": "약국 인공눈물은 비싸던데 병원에서 처방받으면 좀 저렴하게 살 수 있으려나?",
          "original_form": "약국 인공눈물은 비싸던데 병원에서 처방받으면 좀 저렴하게 살 수 있으려나?",
          "speaker_id": "1",
          "time": "20210829 20:47"
        },
        {
          "id": "MDRW2100023027.1.13",
          "form": "아무래도 의료보험 적용되지 싸지 않을까?",
          "original_form": "아무래도 의료보험 적용되지 싸지 않을까?",
          "speaker_id": "2",
          "time": "20210829 20:47"
        },
        {
          "id": "MDRW2100023027.1.14",
          "form": "일회용 인공눈물 만원씩 받으니 좀 부담되더라고",
          "original_form": "일회용 인공눈물 만원씩 받으니 좀 부담되더라고",
          "speaker_id": "1",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023027.1.15",
          "form": "근데 요즘 매일 피곤해서 어떻게 하냐..",
          "original_form": "근데 요즘 매일 피곤해서 어떻게 하냐..",
          "speaker_id": "2",
          "time": "20210829 20:53"
        }
      ]
