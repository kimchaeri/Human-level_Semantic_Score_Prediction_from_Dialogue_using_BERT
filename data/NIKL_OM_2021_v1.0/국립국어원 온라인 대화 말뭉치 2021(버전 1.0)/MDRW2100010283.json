      "utterance": [
        {
          "id": "MDRW2100010283.1.1",
          "form": "치즈돈가스 맛집 가보신적 있나요?",
          "original_form": "치즈돈가스 맛집 가보신적 있나요?",
          "speaker_id": "1",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100010283.1.2",
          "form": "네 간성에 치즈돈가스 진짜 맛집있어요",
          "original_form": "네 간성에 치즈돈가스 진짜 맛집있어요",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010283.1.3",
          "form": "강원도 쪽인가요?",
          "original_form": "강원도 쪽인가요?",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010283.1.4",
          "form": "저도 치돈좋아하는데 궁금해요!",
          "original_form": "저도 치돈좋아하는데 궁금해요!",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010283.1.5",
          "form": "네네. 간성터미널 건너편에 있어요",
          "original_form": "네네. 간성터미널 건너편에 있어요",
          "speaker_id": "2",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010283.1.6",
          "form": "상호명이 어떻게되나용?",
          "original_form": "상호명이 어떻게되나용?",
          "speaker_id": "1",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010283.1.7",
          "form": "돈가스천국이었던것같아요",
          "original_form": "돈가스천국이었던것같아요",
          "speaker_id": "2",
          "time": "20210823 17:30"
        },
        {
          "id": "MDRW2100010283.1.8",
          "form": "리스트에 추가해야겠어요ㅋㅋ 다른메뉴도 있나요?",
          "original_form": "리스트에 추가해야겠어요ㅋㅋ 다른메뉴도 있나요?",
          "speaker_id": "1",
          "time": "20210823 17:32"
        },
        {
          "id": "MDRW2100010283.1.9",
          "form": "고구마치즈돈가스가 진짜 맛있어요",
          "original_form": "고구마치즈돈가스가 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210823 17:38"
        },
        {
          "id": "MDRW2100010283.1.10",
          "form": "세상에 고치돈은 진리죠",
          "original_form": "세상에 고치돈은 진리죠",
          "speaker_id": "1",
          "time": "20210823 17:40"
        },
        {
          "id": "MDRW2100010283.1.11",
          "form": "고치돈 맛집 아시는데 있어요?",
          "original_form": "고치돈 맛집 아시는데 있어요?",
          "speaker_id": "2",
          "time": "20210823 17:40"
        },
        {
          "id": "MDRW2100010283.1.12",
          "form": "국수나무가 가성빈데 지금도 하는지 모르겠어요ㅠ",
          "original_form": "국수나무가 가성빈데 지금도 하는지 모르겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010283.1.13",
          "form": "기억해둬야겠네요",
          "original_form": "기억해둬야겠네요",
          "speaker_id": "2",
          "time": "20210823 17:46"
        },
        {
          "id": "MDRW2100010283.1.14",
          "form": "체인점이라 한번 찾아보세용!",
          "original_form": "체인점이라 한번 찾아보세용!",
