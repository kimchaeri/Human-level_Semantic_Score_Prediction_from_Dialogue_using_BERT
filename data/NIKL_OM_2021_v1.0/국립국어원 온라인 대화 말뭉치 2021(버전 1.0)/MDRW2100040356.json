      "utterance": [
        {
          "id": "MDRW2100040356.1.1",
          "form": "내일그대와 안봤어요!",
          "original_form": "내일그대와 안봤어요!",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040356.1.2",
          "form": "방영당시엔 뭔내용이야 지루해 이러고 잠깐봤는데",
          "original_form": "방영당시엔 뭔내용이야 지루해 이러고 잠깐봤는데",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040356.1.3",
          "form": "갑자기 이제훈 날티나는 모습이 자꾸 아른거리는거예요",
          "original_form": "갑자기 이제훈 날티나는 모습이 자꾸 아른거리는거예요",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040356.1.4",
          "form": "밤새봤어요ㅋㅋㅋㅋㅋ",
          "original_form": "밤새봤어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040356.1.5",
          "form": "날티나는건 아닌데 말투가 시그널같지 않고 모범택시스러워서",
          "original_form": "날티나는건 아닌데 말투가 시그널같지 않고 모범택시스러워서",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040356.1.6",
          "form": "ㅋㅋㅋㅋㅋ 개인적으로 신민아가 연기잘하는지모르겠어서 ㅜㅜ 안봤던거같아요...ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 개인적으로 신민아가 연기잘하는지모르겠어서 ㅜㅜ 안봤던거같아요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040356.1.7",
          "form": "내용은 걍.....ㅋㅋㅋㅋ뒤죽박죽인데 신민아랑 너무 잘어울려서",
          "original_form": "내용은 걍.....ㅋㅋㅋㅋ뒤죽박죽인데 신민아랑 너무 잘어울려서",
          "speaker_id": "1",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040356.1.8",
          "form": "근데 또 뭐 이러다 다시 볼수도있고 ㅋㅋㅋㅋ",
          "original_form": "근데 또 뭐 이러다 다시 볼수도있고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040356.1.9",
          "form": "부러워서봤어요ㅋㅋㅋ",
          "original_form": "부러워서봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.10",
          "form": "님 그거보셨어요? 이리와 안아줘",
          "original_form": "님 그거보셨어요? 이리와 안아줘",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.11",
          "form": "장기용 나온거요!",
          "original_form": "장기용 나온거요!",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.12",
          "form": "연기구멍많지만 ㅋㅋㅋㅋ 허준호님 대박 무서워요",
          "original_form": "연기구멍많지만 ㅋㅋㅋㅋ 허준호님 대박 무서워요",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.13",
          "form": "알긴다아는뎈ㅋㅋㅋㅋㅋㅋㅋ안봤어요",
          "original_form": "알긴다아는뎈ㅋㅋㅋㅋㅋㅋㅋ안봤어요",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.14",
          "form": "거기도 나오셔요...?",
          "original_form": "거기도 나오셔요...?",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.15",
          "form": "장기용도 진짜 잘생겼죠",
          "original_form": "장기용도 진짜 잘생겼죠",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.16",
          "form": "허준호님......쉬던거 아니였어요?ㅋㅋㅋㅋㅋ",
          "original_form": "허준호님......쉬던거 아니였어요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.17",
          "form": "네 거기서 진짜 완전 무서워요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 거기서 진짜 완전 무서워요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040356.1.18",
          "form": "그거재밌어요? 장기용나오는거만 알아요ㅋㅋㅋ",
          "original_form": "그거재밌어요? 장기용나오는거만 알아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:59"
        }
      ]
