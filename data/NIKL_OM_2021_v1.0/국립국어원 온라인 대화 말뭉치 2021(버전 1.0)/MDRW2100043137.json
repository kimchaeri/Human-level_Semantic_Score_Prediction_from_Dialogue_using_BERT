      "utterance": [
        {
          "id": "MDRW2100043137.1.1",
          "form": "깜짝놀랬네여 ㅠㅜ",
          "original_form": "깜짝놀랬네여 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043137.1.2",
          "form": "역시먹는게 최고죠 ㅋㅋ",
          "original_form": "역시먹는게 최고죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043137.1.3",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043137.1.4",
          "form": "아닭발먹고싳네요",
          "original_form": "아닭발먹고싳네요",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043137.1.5",
          "form": "아 닭발 못참져...",
          "original_form": "아 닭발 못참져...",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043137.1.6",
          "form": "매운거잘드세요?",
          "original_form": "매운거잘드세요?",
          "speaker_id": "2",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043137.1.7",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043137.1.8",
          "form": "맵찔인데",
          "original_form": "맵찔인데",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043137.1.9",
          "form": "참고먹으면어느정도 먹는거같아요",
          "original_form": "참고먹으면어느정도 먹는거같아요",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043137.1.10",
          "form": "헉그래요???저는 매운걸좋아해서 엽떡 젤 매운맛으로먹어야해요..",
          "original_form": "헉그래요???저는 매운걸좋아해서 엽떡 젤 매운맛으로먹어야해요..",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043137.1.11",
          "form": "헐대박이네요 ㅠㅠ",
          "original_form": "헐대박이네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100043137.1.12",
          "form": "그래서 매운ㅇ므식할때는 무조건 맵게해달라해야해요",
          "original_form": "그래서 매운ㅇ므식할때는 무조건 맵게해달라해야해요",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043137.1.13",
          "form": "헐랭 부러워요 ㅠㅠ",
          "original_form": "헐랭 부러워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043137.1.14",
          "form": "앜ㅋㅋ 부러울건없어요..사실건강에안좋은...",
          "original_form": "앜ㅋㅋ 부러울건없어요..사실건강에안좋은...",
          "speaker_id": "2",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043137.1.15",
          "form": "그래도 잘먹는게 ㅠㅠ",
          "original_form": "그래도 잘먹는게 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 16:13"
        },
        {
          "id": "MDRW2100043137.1.16",
          "form": "맵부심부릴수있다가 유일한 장점..",
          "original_form": "맵부심부릴수있다가 유일한 장점..",
          "speaker_id": "2",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100043137.1.17",
          "form": "그것만우로도",
          "original_form": "그것만우로도",
          "speaker_id": "1",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100043137.1.18",
          "form": "좋쿠먼요",
          "original_form": "좋쿠먼요",
