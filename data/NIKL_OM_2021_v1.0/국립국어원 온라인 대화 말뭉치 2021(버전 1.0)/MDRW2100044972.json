      "utterance": [
        {
          "id": "MDRW2100044972.1.1",
          "form": "최근에 얘기한 인형 얼굴에 어울리는 눈 찾았어?",
          "original_form": "최근에 얘기한 인형 얼굴에 어울리는 눈 찾았어?",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044972.1.2",
          "form": "아 그 화장한?",
          "original_form": "아 그 화장한?",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100044972.1.3",
          "form": "일단 사두긴 했는데 아직 내 손에 없어수 모르겠다",
          "original_form": "일단 사두긴 했는데 아직 내 손에 없어수 모르겠다",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100044972.1.4",
          "form": "ㅇㅇ 이번에 새로 화장 맡긴거!",
          "original_form": "ㅇㅇ 이번에 새로 화장 맡긴거!",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100044972.1.5",
          "form": "얼굴이 아직 안오긴 했는데",
          "original_form": "얼굴이 아직 안오긴 했는데",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100044972.1.6",
          "form": "녹색 안구 하나 샀엉!",
          "original_form": "녹색 안구 하나 샀엉!",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100044972.1.7",
          "form": "안어울리면... 뭐 먼길 떠나야지...ㅠ",
          "original_form": "안어울리면... 뭐 먼길 떠나야지...ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100044972.1.8",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100044972.1.9",
          "form": "탐라에 보라색 눈?이랑 매칭해보길래",
          "original_form": "탐라에 보라색 눈?이랑 매칭해보길래",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100044972.1.10",
          "form": "이번에 새로 장만한 얼굴인줄 알았어",
          "original_form": "이번에 새로 장만한 얼굴인줄 알았어",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100044972.1.11",
          "form": "ㅇㅎ 그건 원래있던 친구",
          "original_form": "ㅇㅎ 그건 원래있던 친구",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044972.1.12",
          "form": "아직 안왔엉",
          "original_form": "아직 안왔엉",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044972.1.13",
          "form": "그친구는 초록초록으로 해주려고 그랬는데",
          "original_form": "그친구는 초록초록으로 해주려고 그랬는데",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044972.1.14",
          "form": "안먹으면 어카지",
          "original_form": "안먹으면 어카지",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044972.1.15",
          "form": "아냐 잘 어울릴거야",
          "original_form": "아냐 잘 어울릴거야",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044972.1.16",
          "form": "고민해서 샀으니까...",
          "original_form": "고민해서 샀으니까...",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044972.1.17",
          "form": "ㅠㅠ 고마워...",
          "original_form": "ㅠㅠ 고마워...",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100044972.1.18",
          "form": "새로 오면 함 사진 찍어서 보낼게",
