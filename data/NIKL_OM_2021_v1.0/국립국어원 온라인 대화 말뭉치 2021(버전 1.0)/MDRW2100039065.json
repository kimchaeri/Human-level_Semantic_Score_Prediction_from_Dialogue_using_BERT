      "utterance": [
        {
          "id": "MDRW2100039065.1.1",
          "form": "그렇게 모으셔서 뭐 사셨나요?ㅎㅎ",
          "original_form": "그렇게 모으셔서 뭐 사셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 08:50"
        },
        {
          "id": "MDRW2100039065.1.2",
          "form": "애 옷도 사고ㅋㅋ공모주도 하고ㅋㅋ 어제는 소고기샀죠ㅋ",
          "original_form": "애 옷도 사고ㅋㅋ공모주도 하고ㅋㅋ 어제는 소고기샀죠ㅋ",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039065.1.3",
          "form": "와 엄청 오래 하셨나봐요",
          "original_form": "와 엄청 오래 하셨나봐요",
          "speaker_id": "1",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039065.1.4",
          "form": "길게는못했어요ㅋㅋ 그래도 꽤쏠쏠해서요ㅋㅋ",
          "original_form": "길게는못했어요ㅋㅋ 그래도 꽤쏠쏠해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:52"
        },
        {
          "id": "MDRW2100039065.1.5",
          "form": "가을옷도 사야되는데ㅋㅋ",
          "original_form": "가을옷도 사야되는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:52"
        },
        {
          "id": "MDRW2100039065.1.6",
          "form": "오늘 가을옷사려구요.",
          "original_form": "오늘 가을옷사려구요.",
          "speaker_id": "2",
          "time": "20210903 08:52"
        },
        {
          "id": "MDRW2100039065.1.7",
          "form": "어떤 옷 보고 계세요?",
          "original_form": "어떤 옷 보고 계세요?",
          "speaker_id": "1",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100039065.1.8",
          "form": "뭐사신거있으세요?",
          "original_form": "뭐사신거있으세요?",
          "speaker_id": "2",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100039065.1.9",
          "form": "아이 바지요ㅋㅋ",
          "original_form": "아이 바지요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100039065.1.10",
          "form": "긴바지가 필요해서요ㅋㅋ",
          "original_form": "긴바지가 필요해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100039065.1.11",
          "form": "다 작아졌어요ㅋ",
          "original_form": "다 작아졌어요ㅋ",
          "speaker_id": "2",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100039065.1.12",
          "form": "애들이 쑥쑥 크는군요ㅎㅎ",
          "original_form": "애들이 쑥쑥 크는군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100039065.1.13",
          "form": "저는 가디건 하나 샀어요",
          "original_form": "저는 가디건 하나 샀어요",
          "speaker_id": "1",
          "time": "20210903 08:54"
        },
        {
          "id": "MDRW2100039065.1.14",
          "form": "내 정말 쑥쑥 커요",
          "original_form": "내 정말 쑥쑥 커요",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100039065.1.15",
          "form": "매년 옷을 사는 것 같아요",
          "original_form": "매년 옷을 사는 것 같아요",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100039065.1.16",
          "form": "가디건 무슨 색깔 사셨나요",
          "original_form": "가디건 무슨 색깔 사셨나요",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100039065.1.17",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100039065.1.18",
          "form": "베이지로 샀어요",
          "original_form": "베이지로 샀어요",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100039065.1.19",
          "form": "좀 밝게 입어보려구요",
          "original_form": "좀 밝게 입어보려구요",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100039065.1.20",
          "form": "오 베이지는 무난하이 다 잘어울리잖ㅇㅏ요ㅋㅋ",
          "original_form": "오 베이지는 무난하이 다 잘어울리잖ㅇㅏ요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100039065.1.21",
          "form": "잘사셨네요ㅋ",
          "original_form": "잘사셨네요ㅋ",
          "speaker_id": "2",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100039065.1.22",
          "form": "네ㅎㅎ 지금 배송 기다리고 있어요",
          "original_form": "네ㅎㅎ 지금 배송 기다리고 있어요",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100039065.1.23",
          "form": "좋네요 ㅋㅋㅋ 저는 스판끼 있는 바지 하나 사려구요",
          "original_form": "좋네요 ㅋㅋㅋ 저는 스판끼 있는 바지 하나 사려구요",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100039065.1.24",
          "form": "달라붙는 거로요?",
          "original_form": "달라붙는 거로요?",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039065.1.25",
          "form": "아니요ㅋㅋ",
          "original_form": "아니요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:05"
        }
      ]
