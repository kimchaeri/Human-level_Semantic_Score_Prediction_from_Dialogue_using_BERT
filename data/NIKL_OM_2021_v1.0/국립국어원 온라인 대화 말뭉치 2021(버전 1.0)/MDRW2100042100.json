      "utterance": [
        {
          "id": "MDRW2100042100.1.1",
          "form": "지금 계신곳 비오나요?",
          "original_form": "지금 계신곳 비오나요?",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042100.1.2",
          "form": "네 비와요 ㅠ 거긴용?",
          "original_form": "네 비와요 ㅠ 거긴용?",
          "speaker_id": "2",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100042100.1.3",
          "form": "네 여기도ㅠㅠ",
          "original_form": "네 여기도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:04"
        },
        {
          "id": "MDRW2100042100.1.4",
          "form": "모레까지 올건가봐요",
          "original_form": "모레까지 올건가봐요",
          "speaker_id": "1",
          "time": "20210906 20:04"
        },
        {
          "id": "MDRW2100042100.1.5",
          "form": "맞아요 ㅠㅠ 낼도 비많이온다더라구용",
          "original_form": "맞아요 ㅠㅠ 낼도 비많이온다더라구용",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042100.1.6",
          "form": "요즘 아침저녁으로 추운데",
          "original_form": "요즘 아침저녁으로 추운데",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042100.1.7",
          "form": "비까지오면 뭘입을지 모르겠네요",
          "original_form": "비까지오면 뭘입을지 모르겠네요",
          "speaker_id": "1",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042100.1.8",
          "form": "그러게요 ㅠ 이제 셔츠입을날씨네요",
          "original_form": "그러게요 ㅠ 이제 셔츠입을날씨네요",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042100.1.9",
          "form": "이번여름에 비피해는 없으셨죠?",
          "original_form": "이번여름에 비피해는 없으셨죠?",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042100.1.10",
          "form": "남쪽은 많이와서 걱정이더라구요ㅜㅜ",
          "original_form": "남쪽은 많이와서 걱정이더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042100.1.11",
          "form": "네네 여긴 경기도라 없었어요",
          "original_form": "네네 여긴 경기도라 없었어요",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042100.1.12",
          "form": "다행이네요ㅜㅜ",
          "original_form": "다행이네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042100.1.13",
          "form": "얼른 비 그치고 맑았으면 좋겠네요",
          "original_form": "얼른 비 그치고 맑았으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042100.1.14",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042100.1.15",
          "form": "비오면 여기저기아파서ㅋㅋ",
          "original_form": "비오면 여기저기아파서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042100.1.16",
          "form": "ㅋㅋㅋㅋㅋ벌써 쑤시나요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ벌써 쑤시나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042100.1.17",
          "form": "아침에도 억수 많이오네요ㅜㅜ",
          "original_form": "아침에도 억수 많이오네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042100.1.18",
          "form": "출근잘하셨나요!",
          "original_form": "출근잘하셨나요!",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042100.1.19",
          "form": "여기도 비 엄청와요 ..",
          "original_form": "여기도 비 엄청와요 ..",
