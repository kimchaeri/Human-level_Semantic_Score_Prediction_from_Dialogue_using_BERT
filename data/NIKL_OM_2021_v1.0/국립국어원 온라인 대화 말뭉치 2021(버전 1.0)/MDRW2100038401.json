      "utterance": [
        {
          "id": "MDRW2100038401.1.1",
          "form": "앗 저희 학교 주변 마라탕집은 다 사골 베이스걸랑요",
          "original_form": "앗 저희 학교 주변 마라탕집은 다 사골 베이스걸랑요",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038401.1.2",
          "form": "아아아 마자여!!!",
          "original_form": "아아아 마자여!!!",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038401.1.3",
          "form": "오 그것도 매력있겠는걸요!!",
          "original_form": "오 그것도 매력있겠는걸요!!",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038401.1.4",
          "form": "고기는 뭐 넣어드세요?",
          "original_form": "고기는 뭐 넣어드세요?",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038401.1.5",
          "form": "저는 양도 좋아하지만!! 소고기를 제일 좋아해요!!",
          "original_form": "저는 양도 좋아하지만!! 소고기를 제일 좋아해요!!",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038401.1.6",
          "form": "양은 국에 들어가면 사알짝 비린(",
          "original_form": "양은 국에 들어가면 사알짝 비린(",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038401.1.7",
          "form": "맛이 나더라거여!",
          "original_form": "맛이 나더라거여!",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038401.1.8",
          "form": "아 맞아요 가끔 냄새두 나요 !!",
          "original_form": "아 맞아요 가끔 냄새두 나요 !!",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038401.1.9",
          "form": "소고기 짱이죠 ㅎㅎ 전 항상 고기추가해여 ㅋㅋㅋ",
          "original_form": "소고기 짱이죠 ㅎㅎ 전 항상 고기추가해여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038401.1.10",
          "form": "저도요 저 가끔 배고플땐 2번 추가해요!",
          "original_form": "저도요 저 가끔 배고플땐 2번 추가해요!",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038401.1.11",
          "form": "제가 가는 곡은 최소 주문금액에",
          "original_form": "제가 가는 곡은 최소 주문금액에",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038401.1.12",
          "form": "고기포함 5천원이걸랑요",
          "original_form": "고기포함 5천원이걸랑요",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038401.1.13",
          "form": "오 헐",
          "original_form": "오 헐",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038401.1.14",
          "form": "왤케 싸요!",
          "original_form": "왤케 싸요!",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038401.1.15",
          "form": "ㅎㅎ 그래서 진짜 좋아하는 재료만 널어요!!",
          "original_form": "ㅎㅎ 그래서 진짜 좋아하는 재료만 널어요!!",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038401.1.16",
          "form": "대박이다 체인이였으면 좋겠어여...",
          "original_form": "대박이다 체인이였으면 좋겠어여...",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038401.1.17",
          "form": "애신 마라탕인데",
          "original_form": "애신 마라탕인데",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100038401.1.18",
          "form": "체인인지는 모르겠어요 ㅎ.ㅎ",
          "original_form": "체인인지는 모르겠어요 ㅎ.ㅎ",
