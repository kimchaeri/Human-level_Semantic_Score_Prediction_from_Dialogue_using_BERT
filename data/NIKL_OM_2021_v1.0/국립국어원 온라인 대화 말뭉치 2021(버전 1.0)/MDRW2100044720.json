      "utterance": [
        {
          "id": "MDRW2100044720.1.1",
          "form": "흑임자는 근데 맛있는 집 가야해",
          "original_form": "흑임자는 근데 맛있는 집 가야해",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044720.1.2",
          "form": "맞아 탄산음료 먹어?",
          "original_form": "맞아 탄산음료 먹어?",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044720.1.3",
          "form": "거의 안먹는 듯",
          "original_form": "거의 안먹는 듯",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044720.1.4",
          "form": "자주 마심?",
          "original_form": "자주 마심?",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044720.1.5",
          "form": "나는 탄산수 마셔",
          "original_form": "나는 탄산수 마셔",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044720.1.6",
          "form": "콜라사이다 너무 달아",
          "original_form": "콜라사이다 너무 달아",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044720.1.7",
          "form": "맞아 넘 달아서 힘들",
          "original_form": "맞아 넘 달아서 힘들",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044720.1.8",
          "form": "어디 탄산수 마셔",
          "original_form": "어디 탄산수 마셔",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044720.1.9",
          "form": "빅토리아 탄산슈 자몽 굿굿",
          "original_form": "빅토리아 탄산슈 자몽 굿굿",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044720.1.10",
          "form": "오 자몽은 안먹어봤는데",
          "original_form": "오 자몽은 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044720.1.11",
          "form": "난 청포도 추천",
          "original_form": "난 청포도 추천",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044720.1.12",
          "form": "오 아니야 내취향아이엿서.. 넘단향",
          "original_form": "오 아니야 내취향아이엿서.. 넘단향",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044720.1.13",
          "form": "우유는 마셔?",
          "original_form": "우유는 마셔?",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044720.1.14",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044720.1.15",
          "form": "우유 자주 마시지",
          "original_form": "우유 자주 마시지",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044720.1.16",
          "form": "우유 안먹는 편?",
          "original_form": "우유 안먹는 편?",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044720.1.17",
          "form": "홀 난 우유 아에 앙먹어",
          "original_form": "홀 난 우유 아에 앙먹어",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044720.1.18",
          "form": "생우유 먹음? 흰우유",
          "original_form": "생우유 먹음? 흰우유",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044720.1.19",
          "form": "헐 락토프리도?",
          "original_form": "헐 락토프리도?",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044720.1.20",
          "form": "ㅇㅇ흰우유 먹지",
          "original_form": "ㅇㅇ흰우유 먹지",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044720.1.21",
          "form": "락토프리고뭐고 그냥 흰우유 못먹겟어 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "락토프리고뭐고 그냥 흰우유 못먹겟어 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044720.1.22",
          "form": "배탈나서 못먹는게아니여",
          "original_form": "배탈나서 못먹는게아니여",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044720.1.23",
          "form": "ㅋㅋㅋㅋㅋ마시면 바로 신호오나",
          "original_form": "ㅋㅋㅋㅋㅋ마시면 바로 신호오나",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044720.1.24",
          "form": "아 뭐고 ㅋㅋㅋ",
          "original_form": "아 뭐고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044720.1.25",
          "form": "안먹어...흰우유 묘하게 비링거같기도하고",
          "original_form": "안먹어...흰우유 묘하게 비링거같기도하고",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044720.1.26",
          "form": "ㅋㅋㅋㅋㅋ라떼는먹음",
          "original_form": "ㅋㅋㅋㅋㅋ라떼는먹음",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044720.1.27",
          "form": "구랴? 맛있는건 고소한데 ㅋㅋㅋㅋ",
          "original_form": "구랴? 맛있는건 고소한데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044720.1.28",
          "form": "ㅋㅋㅋㅋㅋ아 그렇넼ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아 그렇넼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:39"
        }
      ]
    }
