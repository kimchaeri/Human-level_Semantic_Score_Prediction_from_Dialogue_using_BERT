      "utterance": [
        {
          "id": "MDRW2100025810.1.1",
          "form": "하 진짜 미쳐",
          "original_form": "하 진짜 미쳐",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025810.1.2",
          "form": "너무좋아",
          "original_form": "너무좋아",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025810.1.3",
          "form": "그 서울시티버전 티져 그엇 그대로 앨범 내주려나?",
          "original_form": "그 서울시티버전 티져 그엇 그대로 앨범 내주려나?",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025810.1.4",
          "form": "옷",
          "original_form": "옷",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025810.1.5",
          "form": "엔시티 옷들 중에선 그나마 괜찮아보여서 맘에 들어",
          "original_form": "엔시티 옷들 중에선 그나마 괜찮아보여서 맘에 들어",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025810.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025810.1.7",
          "form": "더...괜찮은 옷은 없을까",
          "original_form": "더...괜찮은 옷은 없을까",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025810.1.8",
          "form": "근데 생각보다",
          "original_form": "근데 생각보다",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025810.1.9",
          "form": "모르지",
          "original_form": "모르지",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025810.1.10",
          "form": "개인티저 옷은 예뻐서",
          "original_form": "개인티저 옷은 예뻐서",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025810.1.11",
          "form": "엔시티 코디는 믿을 수 없어",
          "original_form": "엔시티 코디는 믿을 수 없어",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.12",
          "form": "좀 놀랐어",
          "original_form": "좀 놀랐어",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.13",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.14",
          "form": "땡진이는",
          "original_form": "땡진이는",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.15",
          "form": "믿을수없지만",
          "original_form": "믿을수없지만",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.16",
          "form": "물론 쟈니 티저는 옷이 눈에 안들어오긴 해",
          "original_form": "물론 쟈니 티저는 옷이 눈에 안들어오긴 해",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.17",
          "form": "이어파티 걔 이름 뭐더라 걔는 그래도 ㄱㅊ았어",
          "original_form": "이어파티 걔 이름 뭐더라 걔는 그래도 ㄱㅊ았어",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.18",
          "form": "그치만 나머지 멤버들은 괜찮아보여",
          "original_form": "그치만 나머지 멤버들은 괜찮아보여",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.19",
          "form": "물론 슈주꺼 돌려입은 거지만",
          "original_form": "물론 슈주꺼 돌려입은 거지만",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.20",
          "form": "악세사리가 코디 친구 브랜드거라 구리지만",
          "original_form": "악세사리가 코디 친구 브랜드거라 구리지만",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.21",
          "form": "아 ㄹㅇ 대체",
          "original_form": "아 ㄹㅇ 대체",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.22",
          "form": "인맥좀,,,작작했으면",
          "original_form": "인맥좀,,,작작했으면",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025810.1.23",
          "form": "학연지연혈연",
          "original_form": "학연지연혈연",
          "speaker_id": "1",
          "time": "20210901 17:15"
        }
