      "utterance": [
        {
          "id": "MDRW2100012079.1.1",
          "form": "기억에 남는 기념품 있나요?",
          "original_form": "기억에 남는 기념품 있나요?",
          "speaker_id": "2",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100012079.1.2",
          "form": "기념품말고 여행지 기억나네요",
          "original_form": "기념품말고 여행지 기억나네요",
          "speaker_id": "1",
          "time": "20210824 19:57"
        },
        {
          "id": "MDRW2100012079.1.3",
          "form": "어디가 기억나시나요",
          "original_form": "어디가 기억나시나요",
          "speaker_id": "2",
          "time": "20210824 20:00"
        },
        {
          "id": "MDRW2100012079.1.4",
          "form": "대만이요!",
          "original_form": "대만이요!",
          "speaker_id": "1",
          "time": "20210824 20:01"
        },
        {
          "id": "MDRW2100012079.1.5",
          "form": "아 대만은 길거리 먹거리 다양하지않나요?",
          "original_form": "아 대만은 길거리 먹거리 다양하지않나요?",
          "speaker_id": "2",
          "time": "20210824 20:02"
        },
        {
          "id": "MDRW2100012079.1.6",
          "form": "길거리 음식 천국이죠ㅎㅎ",
          "original_form": "길거리 음식 천국이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012079.1.7",
          "form": "와 좋네요 뭐가 맛잇던가요",
          "original_form": "와 좋네요 뭐가 맛잇던가요",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012079.1.8",
          "form": "우육면이 최고였어요",
          "original_form": "우육면이 최고였어요",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012079.1.9",
          "form": "와 우육면.. 깊은 맛이겠어요",
          "original_form": "와 우육면.. 깊은 맛이겠어요",
          "speaker_id": "2",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100012079.1.10",
          "form": "우리나라에서도 몇번 먹었는데 그맛이 안나더라구요",
          "original_form": "우리나라에서도 몇번 먹었는데 그맛이 안나더라구요",
          "speaker_id": "1",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012079.1.11",
          "form": "그렇군오 지파이?도 들어봤어요",
          "original_form": "그렇군오 지파이?도 들어봤어요",
          "speaker_id": "2",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100012079.1.12",
          "form": "지파이는 향신료땜에 못먹었어요ㅠ",
          "original_form": "지파이는 향신료땜에 못먹었어요ㅠ",
