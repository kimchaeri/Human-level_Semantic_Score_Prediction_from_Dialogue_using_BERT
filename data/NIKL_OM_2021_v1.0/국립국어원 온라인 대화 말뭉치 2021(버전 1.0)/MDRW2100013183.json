      "utterance": [
        {
          "id": "MDRW2100013183.1.1",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013183.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013183.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013183.1.4",
          "form": "저두 없어요 ㅜㅜ 왜 안믿으세요??",
          "original_form": "저두 없어요 ㅜㅜ 왜 안믿으세요??",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013183.1.5",
          "form": "신앙심이",
          "original_form": "신앙심이",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013183.1.6",
          "form": "딱히안생겨서요ㅋㅋㅋ",
          "original_form": "딱히안생겨서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013183.1.7",
          "form": "아 교회같은거 가보신적은 잇으세요?",
          "original_form": "아 교회같은거 가보신적은 잇으세요?",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013183.1.8",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013183.1.9",
          "form": "어렸을때요",
          "original_form": "어렸을때요",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013183.1.10",
          "form": "친구따라서요",
          "original_form": "친구따라서요",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013183.1.11",
          "form": "아하 그러시구나",
          "original_form": "아하 그러시구나",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100013183.1.12",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013183.1.13",
          "form": "전 아주 오래전이라 ㅋㅋ",
          "original_form": "전 아주 오래전이라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100013183.1.14",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100013183.1.15",
          "form": "왜 무교신가요?",
          "original_form": "왜 무교신가요?",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100013183.1.16",
          "form": "매일 어디가기가 귀찮아서요 ㅋㅋ",
          "original_form": "매일 어디가기가 귀찮아서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100013183.1.17",
          "form": "ㅋㅋㅋㅋ특이한이유네요",
          "original_form": "ㅋㅋㅋㅋ특이한이유네요",
          "speaker_id": "2",
          "time": "20210827 02:13"
        },
        {
          "id": "MDRW2100013183.1.18",
          "form": "님은 왜 무교세요?",
          "original_form": "님은 왜 무교세요?",
          "speaker_id": "1",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100013183.1.19",
          "form": "신앙심이 안생겨서요!",
          "original_form": "신앙심이 안생겨서요!",
          "speaker_id": "2",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100013183.1.20",
          "form": "앜ㅋㅋ 제가 물아봤었네료 ㅋㅋ",
          "original_form": "앜ㅋㅋ 제가 물아봤었네료 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100013183.1.21",
          "form": "불교는 어덯게 생각하세요?",
          "original_form": "불교는 어덯게 생각하세요?",
          "speaker_id": "1",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100013183.1.22",
          "form": "불교도 괜찮죠ㅋㅋㅋ",
          "original_form": "불교도 괜찮죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100013183.1.23",
          "form": "저 템플스테이가보고싶어요",
          "original_form": "저 템플스테이가보고싶어요",
          "speaker_id": "2",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100013183.1.24",
          "form": "체험으로요",
          "original_form": "체험으로요",
          "speaker_id": "2",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100013183.1.25",
          "form": "헐 저두요 근데 그것도 은근비싸더라구요 ㅋㅋ",
          "original_form": "헐 저두요 근데 그것도 은근비싸더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100013183.1.26",
          "form": "얼마던가요?????",
          "original_form": "얼마던가요?????",
          "speaker_id": "2",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100013183.1.27",
          "form": "1박에 5만원하는것같아요",
          "original_form": "1박에 5만원하는것같아요",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100013183.1.28",
          "form": "오 그정도군요",
          "original_form": "오 그정도군요",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100013183.1.29",
          "form": "재밌을것같긴해요ㅠ",
          "original_form": "재밌을것같긴해요ㅠ",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100013183.1.30",
          "form": "맞아요 고즈넉한 분위기가 좋을거ㅛ샅은데..",
          "original_form": "맞아요 고즈넉한 분위기가 좋을거ㅛ샅은데..",
          "speaker_id": "1",
