      "utterance": [
        {
          "id": "MDRW2100007067.1.1",
          "form": "점심 드셨나요,",
          "original_form": "점심 드셨나요,",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100007067.1.2",
          "form": "늦잠자서 아점 먹었어요 ㅎㅎ",
          "original_form": "늦잠자서 아점 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100007067.1.3",
          "form": "원래는 식단조절한다고 샐러드 먹는대",
          "original_form": "원래는 식단조절한다고 샐러드 먹는대",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100007067.1.4",
          "form": "오늘은 칼칼하게 뼈해장국 먹었네요 ㅎ",
          "original_form": "오늘은 칼칼하게 뼈해장국 먹었네요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100007067.1.5",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100007067.1.6",
          "form": "오늘 날씨에 뼈해장국 너무 맛있으셨겠어요ㅠㅠ",
          "original_form": "오늘 날씨에 뼈해장국 너무 맛있으셨겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100007067.1.7",
          "form": "저는 저녁에 떡볶이 먹기로해서 점심은 다이어트볶음밥 먹었네요!",
          "original_form": "저는 저녁에 떡볶이 먹기로해서 점심은 다이어트볶음밥 먹었네요!",
          "speaker_id": "1",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100007067.1.8",
          "form": "진짜 떡볶이 귀신이시네요 ㅋㅋ!!",
          "original_form": "진짜 떡볶이 귀신이시네요 ㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100007067.1.9",
          "form": "엄청 맛있죠 정말",
          "original_form": "엄청 맛있죠 정말",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100007067.1.10",
          "form": "얘기하다보니 저도 너무 먹고싶네요",
          "original_form": "얘기하다보니 저도 너무 먹고싶네요",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100007067.1.11",
          "form": "다이어트 볶음밥은 뭐드셨나요?",
          "original_form": "다이어트 볶음밥은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100007067.1.12",
          "form": "제가 당면을 좋아해서 아임닭 잡채볶음밥 사봤는데",
          "original_form": "제가 당면을 좋아해서 아임닭 잡채볶음밥 사봤는데",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100007067.1.13",
          "form": "맛이 괜찮더라구요!",
          "original_form": "맛이 괜찮더라구요!",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100007067.1.14",
          "form": "요즘 볶음밥도 냉동으로 잘나와서 조리하기 참 편하네요",
          "original_form": "요즘 볶음밥도 냉동으로 잘나와서 조리하기 참 편하네요",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100007067.1.15",
          "form": "오 저도 아임닭많이시켜먹는데",
          "original_form": "오 저도 아임닭많이시켜먹는데",
          "speaker_id": "2",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100007067.1.16",
          "form": "항상 김치볶음밥만 먹었거든요",
          "original_form": "항상 김치볶음밥만 먹었거든요",
          "speaker_id": "2",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100007067.1.17",
          "form": "다음에 한번 시켜먹어봐야겠어요",
          "original_form": "다음에 한번 시켜먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100007067.1.18",
          "form": "잡채 좋아하시면 맛있게 드실 것 같아요~",
          "original_form": "잡채 좋아하시면 맛있게 드실 것 같아요~",
          "speaker_id": "1",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100007067.1.19",
          "form": "김치볶음밥도 맛있나요? 싱거울 것 같아서 안시켰었는데..",
          "original_form": "김치볶음밥도 맛있나요? 싱거울 것 같아서 안시켰었는데..",
          "speaker_id": "1",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100007067.1.20",
          "form": "저는 너무 맛있더라고요",
          "original_form": "저는 너무 맛있더라고요",
          "speaker_id": "2",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100007067.1.21",
          "form": "간도 딱좋았어요!!",
          "original_form": "간도 딱좋았어요!!",
          "speaker_id": "2",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100007067.1.22",
          "form": "계란 프라이 하나랑 해서 먹으면 최고예요 ㅎㅎ",
          "original_form": "계란 프라이 하나랑 해서 먹으면 최고예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100007067.1.23",
          "form": "다음에 한번 주문해봐야겠어요!",
          "original_form": "다음에 한번 주문해봐야겠어요!",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100007067.1.24",
          "form": "아임닭 제품이 참 괜찮더라구요 소세지도 그렇고..",
          "original_form": "아임닭 제품이 참 괜찮더라구요 소세지도 그렇고..",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100007067.1.25",
          "form": "맞아요! 그 공약바라고",
          "original_form": "맞아요! 그 공약바라고",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100007067.1.26",
          "form": "있는데 그것도 엄청 맛있어요",
          "original_form": "있는데 그것도 엄청 맛있어요",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100007067.1.27",
          "form": "곤약바 ㅎㅎ",
          "original_form": "곤약바 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100007067.1.28",
          "form": "그것도 먹어볼게요 곤약좋아해요",
          "original_form": "그것도 먹어볼게요 곤약좋아해요",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100007067.1.29",
          "form": "곤약은 살도 안찌고 맛있어요ㅎㅎ",
          "original_form": "곤약은 살도 안찌고 맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100007067.1.30",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100007067.1.31",
          "form": "칠리맛만 먹어봤는대",
          "original_form": "칠리맛만 먹어봤는대",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100007067.1.32",
          "form": "콘소매도 맛나보이더라고요",
          "original_form": "콘소매도 맛나보이더라고요",
          "speaker_id": "2",
          "time": "20210822 14:37"
        }
      ]
    }
