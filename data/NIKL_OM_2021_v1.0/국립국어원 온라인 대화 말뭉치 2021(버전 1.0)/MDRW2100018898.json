      "utterance": [
        {
          "id": "MDRW2100018898.1.1",
          "form": "요즘 즐겨보는 드라마 있으세요?",
          "original_form": "요즘 즐겨보는 드라마 있으세요?",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018898.1.2",
          "form": "요즘 보는 드라마있으신가요??",
          "original_form": "요즘 보는 드라마있으신가요??",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018898.1.3",
          "form": "헉 같은 질문 ㅋㅋ",
          "original_form": "헉 같은 질문 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018898.1.4",
          "form": "전 요즘 슬의생봐요 ㅎ",
          "original_form": "전 요즘 슬의생봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018898.1.5",
          "form": "찌찌뽕!!! 재밌나요??",
          "original_form": "찌찌뽕!!! 재밌나요??",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018898.1.6",
          "form": "넵 점점 뻔하긴 한데 그래도 재밌네요 ㅋㅋㅋ",
          "original_form": "넵 점점 뻔하긴 한데 그래도 재밌네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018898.1.7",
          "form": "넷플릭스로 보세요?",
          "original_form": "넷플릭스로 보세요?",
          "speaker_id": "1",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018898.1.8",
          "form": "최대한 본방으로 보는데",
          "original_form": "최대한 본방으로 보는데",
          "speaker_id": "2",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018898.1.9",
          "form": "일잇어서 놓치면 넷플릭스로 봐요!!",
          "original_form": "일잇어서 놓치면 넷플릭스로 봐요!!",
          "speaker_id": "2",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018898.1.10",
          "form": "넷플릭스 뭐가 재밌나요??",
          "original_form": "넷플릭스 뭐가 재밌나요??",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018898.1.11",
          "form": "음..전 좀 유치한거 좋아해서",
          "original_form": "음..전 좀 유치한거 좋아해서",
          "speaker_id": "2",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018898.1.12",
          "form": "좋아하면 울리는 이라고",
          "original_form": "좋아하면 울리는 이라고",
          "speaker_id": "2",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018898.1.13",
          "form": "오 송강나오는!",
          "original_form": "오 송강나오는!",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018898.1.14",
          "form": "웹툰이 원작인 드라마를 최근에봤어요 호",
          "original_form": "웹툰이 원작인 드라마를 최근에봤어요 호",
          "speaker_id": "2",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018898.1.15",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018898.1.16",
          "form": "알고있지만은 안보시나요?",
          "original_form": "알고있지만은 안보시나요?",
          "speaker_id": "1",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018898.1.17",
          "form": "그건 아직 안봤는데",
          "original_form": "그건 아직 안봤는데",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018898.1.18",
          "form": "재미있나요??",
          "original_form": "재미있나요??",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018898.1.19",
          "form": "그것도 웹툰원작인데",
          "original_form": "그것도 웹툰원작인데",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018898.1.20",
          "form": "송강은 웹툰원작인거 많이하는것닽아요",
          "original_form": "송강은 웹툰원작인거 많이하는것닽아요",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018898.1.21",
          "form": "맞아요! 그냥 볼만해요...강추는 아닌ㅎㅎ...",
          "original_form": "맞아요! 그냥 볼만해요...강추는 아닌ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018898.1.22",
          "form": "송강 좋아하세요?",
          "original_form": "송강 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018898.1.23",
          "form": "그런건 아니지만",
          "original_form": "그런건 아니지만",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018898.1.24",
          "form": "본의아니게 많이 보게되네요 ㅋㅋㅋ",
          "original_form": "본의아니게 많이 보게되네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018898.1.25",
          "form": "송강 좋알람에선 연기 잘 하나요?",
          "original_form": "송강 좋알람에선 연기 잘 하나요?",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018898.1.26",
          "form": "네 무난하게 잘했어요 ㅋㅋ",
          "original_form": "네 무난하게 잘했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018898.1.27",
          "form": "안봤지만 알고있지만이랑 비슷할것 같아요",
          "original_form": "안봤지만 알고있지만이랑 비슷할것 같아요",
          "speaker_id": "2",
          "time": "20210827 13:28"
        }
      ]
    }
