      "utterance": [
        {
          "id": "MDRW2100011457.1.1",
          "form": "결혼에 대해 어떻게 생각하세요?",
          "original_form": "결혼에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011457.1.2",
          "form": "저는 생각이 없어서요",
          "original_form": "저는 생각이 없어서요",
          "speaker_id": "2",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100011457.1.3",
          "form": "주변에서 행복하단 사람을 못봤어요",
          "original_form": "주변에서 행복하단 사람을 못봤어요",
          "speaker_id": "2",
          "time": "20210824 12:51"
        },
        {
          "id": "MDRW2100011457.1.4",
          "form": "결혼하고나서요?ㅠㅠ",
          "original_form": "결혼하고나서요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100011457.1.5",
          "form": "네 다들 후회하더라고요",
          "original_form": "네 다들 후회하더라고요",
          "speaker_id": "2",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011457.1.6",
          "form": "전부다요?ㅠㅠ",
          "original_form": "전부다요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011457.1.7",
          "form": "스트레스받느니 혼자사는것도 나쁘지않을것 같아요",
          "original_form": "스트레스받느니 혼자사는것도 나쁘지않을것 같아요",
          "speaker_id": "2",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100011457.1.8",
          "form": "맞아요ㅠㅠ 그래도",
          "original_form": "맞아요ㅠㅠ 그래도",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100011457.1.9",
          "form": "좋다는사람이 한명도없다는건 좀 놀랍네요\r\n전부후회라니",
          "original_form": "좋다는사람이 한명도없다는건 좀 놀랍네요\r\n전부후회라니",
          "speaker_id": "1",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100011457.1.10",
          "form": "결혼한 친구들 자체가 많지 않아서 그럴수도 있고요ㅋㅋ",
          "original_form": "결혼한 친구들 자체가 많지 않아서 그럴수도 있고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100011457.1.11",
          "form": "사실 혼자도",
          "original_form": "사실 혼자도",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011457.1.12",
          "form": "괜찮을거같긴해요진짜ㅋㅋㅋ",
          "original_form": "괜찮을거같긴해요진짜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011457.1.13",
          "form": "저희엄마는  늙어서 외로우니까 해야한다고ㅋ",
          "original_form": "저희엄마는  늙어서 외로우니까 해야한다고ㅋ",
          "speaker_id": "2",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011457.1.14",
          "form": "ㅋㅋㅋ 아무래도",
          "original_form": "ㅋㅋㅋ 아무래도",
          "speaker_id": "1",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011457.1.15",
          "form": "아직은 한국 인식이 그런편이죠ㅠ",
          "original_form": "아직은 한국 인식이 그런편이죠ㅠ",
          "speaker_id": "1",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011457.1.16",
          "form": "그치만 늙을때를 위해 괴롭게 사는건",
          "original_form": "그치만 늙을때를 위해 괴롭게 사는건",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011457.1.17",
          "form": "너무하자나요",
          "original_form": "너무하자나요",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011457.1.18",
          "form": "ㅋㅋㅋㅠ",
          "original_form": "ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011457.1.19",
          "form": "결혼이 참",
          "original_form": "결혼이 참",
          "speaker_id": "1",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011457.1.20",
          "form": "딜레마네요",
          "original_form": "딜레마네요",
          "speaker_id": "1",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011457.1.21",
          "form": "맞아요 저도 그런생각 들어요ㅋㅋ",
