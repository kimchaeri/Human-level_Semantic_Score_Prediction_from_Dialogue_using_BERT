      "utterance": [
        {
          "id": "MDRW2100024764.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024764.1.2",
          "form": "여긴 비가 와요 거긴 어떠세요?",
          "original_form": "여긴 비가 와요 거긴 어떠세요?",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024764.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100024764.1.4",
          "form": "여긴 한여름이었어요 ㅎㅎ",
          "original_form": "여긴 한여름이었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100024764.1.5",
          "form": "해가 너무 쨍쨍하고 습했습니다ㅠ",
          "original_form": "해가 너무 쨍쨍하고 습했습니다ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100024764.1.6",
          "form": "허걱 여긴 진짜 비 너무많이와서",
          "original_form": "허걱 여긴 진짜 비 너무많이와서",
          "speaker_id": "1",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100024764.1.7",
          "form": "외출하고 들어오면 거의 빨래수준이었어요",
          "original_form": "외출하고 들어오면 거의 빨래수준이었어요",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100024764.1.8",
          "form": "아니 다른 곳은 가을이 와서 선선하다는데",
          "original_form": "아니 다른 곳은 가을이 와서 선선하다는데",
          "speaker_id": "2",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100024764.1.9",
          "form": "그 곳은 같은 나라가 아닌걸까요 ㅋㅋㅋㅋ",
          "original_form": "그 곳은 같은 나라가 아닌걸까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024764.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024764.1.11",
          "form": "아 여긴왜이러지.....",
          "original_form": "아 여긴왜이러지.....",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024764.1.12",
          "form": "바람도...불고요",
          "original_form": "바람도...불고요",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024764.1.13",
          "form": "그 바람 저희 지역에도 보내주세요 ㅎㅎ",
          "original_form": "그 바람 저희 지역에도 보내주세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024764.1.14",
          "form": "정말 너무... 습하고 덥습니다 ㅋㅋㅋㅋ",
          "original_form": "정말 너무... 습하고 덥습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024764.1.15",
          "form": "어제 에어컨 청소한다고 필터 빼놨는데 오늘 이러네요 ㅎㅎ",
          "original_form": "어제 에어컨 청소한다고 필터 빼놨는데 오늘 이러네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024764.1.16",
          "form": "헉 저는 습하고 추워요ㅋㅋㅋ",
          "original_form": "헉 저는 습하고 추워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024764.1.17",
          "form": "감기 걸릴뻔해서 오랜만에 에어컨 안틉니다",
          "original_form": "감기 걸릴뻔해서 오랜만에 에어컨 안틉니다",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024764.1.18",
          "form": "밖에 귀뚜라미는 우는데 가을은 안오고, 습하고 춥고 덥고 난리네요 ㅎㅎ",
          "original_form": "밖에 귀뚜라미는 우는데 가을은 안오고, 습하고 춥고 덥고 난리네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100024764.1.19",
          "form": "얼른 가을 왔으면 좋겠어요!",
          "original_form": "얼른 가을 왔으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024764.1.20",
          "form": "진짜요 비그치고 선선한 날씨에",
          "original_form": "진짜요 비그치고 선선한 날씨에",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024764.1.21",
          "form": "긴팔입고 돌아다니고싶어요ㅋㅋ",
          "original_form": "긴팔입고 돌아다니고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024764.1.22",
          "form": "가을맞이 옷정리 다 하셨나요 ㅎㅎ",
          "original_form": "가을맞이 옷정리 다 하셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024764.1.23",
          "form": "아니요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024764.1.24",
          "form": "저도 아직 안했어요 ㅋㅋㅋㅋ 가을 옷 주문은 잔뜩 해놨는데 이것도 그냥 쌓아만 놨어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 아직 안했어요 ㅋㅋㅋㅋ 가을 옷 주문은 잔뜩 해놨는데 이것도 그냥 쌓아만 놨어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024764.1.25",
          "form": "우와 사셨군요",
          "original_form": "우와 사셨군요",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100024764.1.26",
          "form": "전 아직 1도안사섴ㅋㅋ",
          "original_form": "전 아직 1도안사섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100024764.1.27",
          "form": "하나쯤은 사도 되겠죠?",
          "original_form": "하나쯤은 사도 되겠죠?",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100024764.1.28",
          "form": "그렇다고해주세요ㅋㅋ",
          "original_form": "그렇다고해주세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100024764.1.29",
          "form": "이미 옷은 많지만 가을옷 하나쯤은 사도 되겠죠?",
          "original_form": "이미 옷은 많지만 가을옷 하나쯤은 사도 되겠죠?",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100024764.1.30",
          "form": "입을옷은 없으니까요",
          "original_form": "입을옷은 없으니까요",
          "speaker_id": "1",
          "time": "20210831 23:08"
        }
      ]
