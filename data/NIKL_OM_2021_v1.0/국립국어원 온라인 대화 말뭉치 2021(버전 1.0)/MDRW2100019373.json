      "utterance": [
        {
          "id": "MDRW2100019373.1.1",
          "form": "재택근무 해보셨나여?",
          "original_form": "재택근무 해보셨나여?",
          "speaker_id": "1",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019373.1.2",
          "form": "아뇨 전 아직 학생이라서요ㅎㅎ",
          "original_form": "아뇨 전 아직 학생이라서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019373.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019373.1.4",
          "form": "저도 아직이요ㅠ 부모님은 좀 하셨어여!",
          "original_form": "저도 아직이요ㅠ 부모님은 좀 하셨어여!",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019373.1.5",
          "form": "아하 어떤 직종하셨었나요?",
          "original_form": "아하 어떤 직종하셨었나요?",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019373.1.6",
          "form": "회사원이신데 작년에 1주씩 돌아가시면서 재택하시더러구여",
          "original_form": "회사원이신데 작년에 1주씩 돌아가시면서 재택하시더러구여",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019373.1.7",
          "form": "아 코로나 때문인가요?",
          "original_form": "아 코로나 때문인가요?",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100019373.1.8",
          "form": "네네! 대신 회사 보안 프로그램을 개인 컴퓨터에 설치할 수 없어서 거의 휴가 같은 재택이었어요",
          "original_form": "네네! 대신 회사 보안 프로그램을 개인 컴퓨터에 설치할 수 없어서 거의 휴가 같은 재택이었어요",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100019373.1.9",
          "form": "헉 완전 좋으셨겠네요ㅎㅎ",
          "original_form": "헉 완전 좋으셨겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100019373.1.10",
          "form": "그럼 지금은 다시 출근하시는건가요?",
          "original_form": "그럼 지금은 다시 출근하시는건가요?",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100019373.1.11",
          "form": "네네ㅋㅋㅋ 잠깐 제외하고 다시 출근이에여ㅠ",
          "original_form": "네네ㅋㅋㅋ 잠깐 제외하고 다시 출근이에여ㅠ",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100019373.1.12",
          "form": "다시 적응하시느라 힘드셨겠네요ㅠ",
          "original_form": "다시 적응하시느라 힘드셨겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100019373.1.13",
          "form": "출퇴근이 정말 힘드셨대요ㅠ",
          "original_form": "출퇴근이 정말 힘드셨대요ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100019373.1.14",
          "form": "정말 그럴 것같아요 종강 개강 느낌...이겠죠?",
          "original_form": "정말 그럴 것같아요 종강 개강 느낌...이겠죠?",
          "speaker_id": "2",
          "time": "20210829 01:12"
        },
        {
          "id": "MDRW2100019373.1.15",
          "form": "네네ㅋㅋㅋㅋㅋ 짧던 방학이 휘리릭....ㅠㅠ",
          "original_form": "네네ㅋㅋㅋㅋㅋ 짧던 방학이 휘리릭....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:15"
        },
        {
          "id": "MDRW2100019373.1.16",
          "form": "ㅠㅠ개강하셨나요?",
          "original_form": "ㅠㅠ개강하셨나요?",
          "speaker_id": "2",
          "time": "20210829 19:39"
        }
      ]
