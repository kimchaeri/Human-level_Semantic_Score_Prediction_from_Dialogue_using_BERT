      "utterance": [
        {
          "id": "MDRW2100047662.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100047662.1.2",
          "form": "좋아하는 음식이 뭐인가요",
          "original_form": "좋아하는 음식이 뭐인가요",
          "speaker_id": "1",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100047662.1.3",
          "form": "가리는거 없이 다 좋아해요 ^^ 치킨,피자요런것들",
          "original_form": "가리는거 없이 다 좋아해요 ^^ 치킨,피자요런것들",
          "speaker_id": "2",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100047662.1.4",
          "form": "어디 치킨을 가장 좋아하세요?",
          "original_form": "어디 치킨을 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100047662.1.5",
          "form": "교촌 레드콤보요",
          "original_form": "교촌 레드콤보요",
          "speaker_id": "2",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100047662.1.6",
          "form": "앗 저와 비슷하시네요ㅎ",
          "original_form": "앗 저와 비슷하시네요ㅎ",
          "speaker_id": "1",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100047662.1.7",
          "form": "전 레드오리지널 좋아해요",
          "original_form": "전 레드오리지널 좋아해요",
          "speaker_id": "1",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100047662.1.8",
          "form": "매콤한 맛 좋아하시나봐요",
          "original_form": "매콤한 맛 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100047662.1.9",
          "form": "네 ㅎㅎ 엽떡도 좋아하고 근데 그렇게 많이 매운건 못먹구요 적당하게 ㅋㅋ",
          "original_form": "네 ㅎㅎ 엽떡도 좋아하고 근데 그렇게 많이 매운건 못먹구요 적당하게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100047662.1.10",
          "form": "저도 매운맛 아주 잘 먹지는 못해도 좋아해요ㅎ",
          "original_form": "저도 매운맛 아주 잘 먹지는 못해도 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100047662.1.11",
          "form": "치킨은 어느 부위 좋아하세요?",
          "original_form": "치킨은 어느 부위 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100047662.1.12",
          "form": "저는 뻑살말고 ㅋㅋ",
          "original_form": "저는 뻑살말고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100047662.1.13",
          "form": "저도 뻑뻑한 살은 좀ㅎㅎ",
          "original_form": "저도 뻑뻑한 살은 좀ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100047662.1.14",
          "form": "날개 좋아합니다",
          "original_form": "날개 좋아합니다",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100047662.1.15",
          "form": "다리와 날개 중에서는 뭐 좋아하세요?",
          "original_form": "다리와 날개 중에서는 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100047662.1.16",
          "form": "저도 날개 다리..ㅋㅋ 최애죠",
          "original_form": "저도 날개 다리..ㅋㅋ 최애죠",
          "speaker_id": "2",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100047662.1.17",
          "form": "다리입니다ㅋㅋ",
          "original_form": "다리입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100047662.1.18",
          "form": "저는 날개인데ㅎㅎ",
          "original_form": "저는 날개인데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100047662.1.19",
          "form": "같이 먹으면 딱 분담되겠네용",
          "original_form": "같이 먹으면 딱 분담되겠네용",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100047662.1.20",
          "form": "앗 근데 뻑살이 남네요ㅎ",
          "original_form": "앗 근데 뻑살이 남네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100047662.1.21",
          "form": "그니까요ㅋㅋㄱ뻑살은 ㅠㅠ 콤보를먹으면",
          "original_form": "그니까요ㅋㅋㄱ뻑살은 ㅠㅠ 콤보를먹으면",
          "speaker_id": "2",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100047662.1.22",
          "form": "됩니다",
          "original_form": "됩니다",
          "speaker_id": "2",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100047662.1.23",
          "form": "그치만ㅜ 그럼 목살이 없는 걸요",
          "original_form": "그치만ㅜ 그럼 목살이 없는 걸요",
