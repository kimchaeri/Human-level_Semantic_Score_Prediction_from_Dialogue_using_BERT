        {
          "id": "MDRW2100011950.1.2",
          "form": "나도 가을이 제일 좋아",
          "original_form": "나도 가을이 제일 좋아",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011950.1.3",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011950.1.4",
          "form": "덥지도 않고 춥지도 않고",
          "original_form": "덥지도 않고 춥지도 않고",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011950.1.5",
          "form": "난 내 생일 계절이라서",
          "original_form": "난 내 생일 계절이라서",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
