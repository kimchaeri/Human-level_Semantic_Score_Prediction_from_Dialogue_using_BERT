      "utterance": [
        {
          "id": "MDRW2100004769.1.1",
          "form": "야 싸이월드 부활한대",
          "original_form": "야 싸이월드 부활한대",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004769.1.2",
          "form": "아 왜케 답장 느림?",
          "original_form": "아 왜케 답장 느림?",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004769.1.3",
          "form": "니 싸이월드 계정 있음?",
          "original_form": "니 싸이월드 계정 있음?",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004769.1.4",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004769.1.5",
          "form": "있는데",
          "original_form": "있는데",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004769.1.6",
          "form": "까머금",
          "original_form": "까머금",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004769.1.7",
          "form": "헐 그렇군이라니",
          "original_form": "헐 그렇군이라니",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004769.1.8",
          "form": "완전 성의 없네",
          "original_form": "완전 성의 없네",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004769.1.9",
          "form": "계정 있다고했자나ㄱ-",
          "original_form": "계정 있다고했자나ㄱ-",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004769.1.10",
          "form": "그럼 아이디 찾기 했음?",
          "original_form": "그럼 아이디 찾기 했음?",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004769.1.11",
          "form": "못찾는다고나오던데",
          "original_form": "못찾는다고나오던데",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004769.1.12",
          "form": "근데 싸이월드 언제부터 다시 들어갈 수 있는거임?",
          "original_form": "근데 싸이월드 언제부터 다시 들어갈 수 있는거임?",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004769.1.13",
          "form": "ㅁㄹ나도 아마 연말은되야하지않나.?",
          "original_form": "ㅁㄹ나도 아마 연말은되야하지않나.?",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004769.1.14",
          "form": "지금 막 bgm 다시 듣기? 뭐 이런 음원은 나오는 것 같던데",
          "original_form": "지금 막 bgm 다시 듣기? 뭐 이런 음원은 나오는 것 같던데",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004769.1.15",
          "form": "그거나와서머한다고 그게먼저나오냨ㅋㄱㅋ",
          "original_form": "그거나와서머한다고 그게먼저나오냨ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004769.1.16",
          "form": "그르게말이여",
          "original_form": "그르게말이여",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004769.1.17",
          "form": "근데 난 자꾸 뭔가 옛날 노래 리메이크 하는 거 별로인 듯",
          "original_form": "근데 난 자꾸 뭔가 옛날 노래 리메이크 하는 거 별로인 듯",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004769.1.18",
          "form": "그거부활해도 한2년있다가섭종할듯",
          "original_form": "그거부활해도 한2년있다가섭종할듯",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004769.1.19",
          "form": "인정 아무도 안쓸텐데",
          "original_form": "인정 아무도 안쓸텐데",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004769.1.20",
          "form": "돈이썩어나네~",
          "original_form": "돈이썩어나네~",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004769.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 사람들 싸이는 그리워하면서 미투데이는 안그리워하는 거 이상함ㅋㅋㅋㅋㅋㅋㅋ",
