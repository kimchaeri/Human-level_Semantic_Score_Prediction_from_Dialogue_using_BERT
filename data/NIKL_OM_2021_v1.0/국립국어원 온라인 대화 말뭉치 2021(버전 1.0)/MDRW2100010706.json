      "utterance": [
        {
          "id": "MDRW2100010706.1.1",
          "form": "거기도 비와?",
          "original_form": "거기도 비와?",
          "speaker_id": "1",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010706.1.2",
          "form": "오지 주륵주륵",
          "original_form": "오지 주륵주륵",
          "speaker_id": "2",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010706.1.3",
          "form": "올해는 여름 장마는 비 별로 안 오고..",
          "original_form": "올해는 여름 장마는 비 별로 안 오고..",
          "speaker_id": "2",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010706.1.4",
          "form": "(여기만 그랬나?)",
          "original_form": "(여기만 그랬나?)",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010706.1.5",
          "form": "봄 가을에 비 많이 옴",
          "original_form": "봄 가을에 비 많이 옴",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010706.1.6",
          "form": "뭐 비와서 더위는 가셨지만..",
          "original_form": "뭐 비와서 더위는 가셨지만..",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100010706.1.7",
          "form": "태풍온대",
          "original_form": "태풍온대",
          "speaker_id": "1",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010706.1.8",
          "form": "응 들었음",
          "original_form": "응 들었음",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010706.1.9",
          "form": "오마이스",
          "original_form": "오마이스",
          "speaker_id": "1",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010706.1.10",
          "form": "게다가 산사태 문자도 계속 옴",
          "original_form": "게다가 산사태 문자도 계속 옴",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010706.1.11",
          "form": "왕언니가 라디오 들었는데 태풍안내하는 아나운서가 오마이뉴스라고 했대",
          "original_form": "왕언니가 라디오 들었는데 태풍안내하는 아나운서가 오마이뉴스라고 했대",
          "speaker_id": "1",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010706.1.12",
          "form": "태풍 이름 신기...",
          "original_form": "태풍 이름 신기...",
          "speaker_id": "1",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010706.1.13",
          "form": "우와~ 어쩐다냐",
          "original_form": "우와~ 어쩐다냐",
          "speaker_id": "2",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010706.1.14",
          "form": "오마이스 어느 나라에서 지은 이름이얌!",
          "original_form": "오마이스 어느 나라에서 지은 이름이얌!",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010706.1.15",
          "form": "이불킥 하고",
          "original_form": "이불킥 하고",
          "speaker_id": "1",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010706.1.16",
          "form": "이불킥이 문제야? 방송 끝나고 겁나 깨졌울걸",
          "original_form": "이불킥이 문제야? 방송 끝나고 겁나 깨졌울걸",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010706.1.17",
          "form": "ㅋㅋㅋ미국이 낸 이름이래",
          "original_form": "ㅋㅋㅋ미국이 낸 이름이래",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010706.1.18",
          "form": "오! 나이스 야?",
          "original_form": "오! 나이스 야?",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010706.1.19",
          "form": "아..마이스구나",
          "original_form": "아..마이스구나",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010706.1.20",
          "form": "팔라우어 래",
          "original_form": "팔라우어 래",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010706.1.21",
          "form": "주위를 어슬렁거리는",
          "original_form": "주위를 어슬렁거리는",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010706.1.22",
          "form": "강한 건 아닌가봐",
          "original_form": "강한 건 아닌가봐",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010706.1.23",
          "form": "날씨는 싸늘해졌눈데..",
          "original_form": "날씨는 싸늘해졌눈데..",
          "speaker_id": "2",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010706.1.24",
          "form": "가끔 훅 열이 올라서 덥다",
          "original_form": "가끔 훅 열이 올라서 덥다",
          "speaker_id": "2",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010706.1.25",
          "form": "약한 듯",
          "original_form": "약한 듯",
          "speaker_id": "1",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010706.1.26",
          "form": "오늘 처서였어",
          "original_form": "오늘 처서였어",
          "speaker_id": "1",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010706.1.27",
          "form": "열올라 더워서 선풍기 켜면 더워 ㅠㅠ",
          "original_form": "열올라 더워서 선풍기 켜면 더워 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010706.1.28",
          "form": "아니 추워",
          "original_form": "아니 추워",
          "speaker_id": "2",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010706.1.29",
          "form": "오늘 사무실도 더웠어",
          "original_form": "오늘 사무실도 더웠어",
          "speaker_id": "1",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010706.1.30",
          "form": "아니 추웠어",
          "original_form": "아니 추웠어",
          "speaker_id": "1",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010706.1.31",
          "form": "임산부출근한 날인데",
          "original_form": "임산부출근한 날인데",
          "speaker_id": "1",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010706.1.32",
          "form": "에어컨 젤 약하게 해도 춥더라고",
          "original_form": "에어컨 젤 약하게 해도 춥더라고",
          "speaker_id": "1",
          "time": "20210823 21:01"
        },
        {
          "id": "MDRW2100010706.1.33",
          "form": "웅?",
          "original_form": "웅?",
          "speaker_id": "2",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100010706.1.34",
          "form": "재택/사무실 병행 근무중인데, 임산부는 거의 재택하거든",
          "original_form": "재택/사무실 병행 근무중인데, 임산부는 거의 재택하거든",
          "speaker_id": "1",
          "time": "20210823 21:03"
        },
        {
          "id": "MDRW2100010706.1.35",
          "form": "가끔 사무실 출근",
          "original_form": "가끔 사무실 출근",
          "speaker_id": "1",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010706.1.36",
          "form": "아하 요새 에어컨 켜면 춥지",
          "original_form": "아하 요새 에어컨 켜면 춥지",
          "speaker_id": "2",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010706.1.37",
          "form": "긑데 날씨가 이러니...",
          "original_form": "긑데 날씨가 이러니...",
          "speaker_id": "1",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010706.1.38",
          "form": "이모는 긴 팔 입구 왔더라",
          "original_form": "이모는 긴 팔 입구 왔더라",
          "speaker_id": "2",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010706.1.39",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010706.1.40",
          "form": "긴 팔 입어야 되나",
          "original_form": "긴 팔 입어야 되나",
          "speaker_id": "1",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010706.1.41",
          "form": "긴팔은 또 덥지",
          "original_form": "긴팔은 또 덥지",
          "speaker_id": "2",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100010706.1.42",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 21:07"
        },
        {
          "id": "MDRW2100010706.1.43",
          "form": "결국 반팔이구나",
          "original_form": "결국 반팔이구나",
          "speaker_id": "1",
          "time": "20210823 21:08"
        },
        {
          "id": "MDRW2100010706.1.44",
          "form": "반팔에 얇은 가디건?",
          "original_form": "반팔에 얇은 가디건?",
          "speaker_id": "2",
          "time": "20210823 21:09"
        },
        {
          "id": "MDRW2100010706.1.45",
          "form": "옷은 맨날 없어",
          "original_form": "옷은 맨날 없어",
          "speaker_id": "1",
          "time": "20210823 21:10"
        }
      ]
    }
  ]
}
