      "utterance": [
        {
          "id": "MDRW2100017641.1.1",
          "form": "허니콤보와 신전 로제 먹었어요! name1님은요?",
          "original_form": "허니콤보와 신전 로제 먹었어요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017641.1.2",
          "form": "저 된장국 먹었어요ㅎ",
          "original_form": "저 된장국 먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017641.1.3",
          "form": "배달시켜먹으려했는데..반대에 부딪혀ㅎ",
          "original_form": "배달시켜먹으려했는데..반대에 부딪혀ㅎ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017641.1.4",
          "form": "허니콤보 어땠나요",
          "original_form": "허니콤보 어땠나요",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017641.1.5",
          "form": "제가 간장베이스를 좋아해서 맛나게 먹었어요ㅋㅋㅋ 다리만 골라 먹었네요ㅎㅎ",
          "original_form": "제가 간장베이스를 좋아해서 맛나게 먹었어요ㅋㅋㅋ 다리만 골라 먹었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017641.1.6",
          "form": "다리좋아하시는구나",
          "original_form": "다리좋아하시는구나",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017641.1.7",
          "form": "전 날개좋아합니다 ㅎ",
          "original_form": "전 날개좋아합니다 ㅎ",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017641.1.8",
          "form": "로제는 어떠셨어요",
          "original_form": "로제는 어떠셨어요",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017641.1.9",
          "form": "신전 로제가 떡에 소스가 잘 배어 맛있더라구요ㅎㅎ 적당히 꾸덕하고 진해거 맛나게 먹었어요!",
          "original_form": "신전 로제가 떡에 소스가 잘 배어 맛있더라구요ㅎㅎ 적당히 꾸덕하고 진해거 맛나게 먹었어요!",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017641.1.10",
          "form": "사리 추가 뭐 더 하신거있으신가요",
          "original_form": "사리 추가 뭐 더 하신거있으신가요",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017641.1.11",
          "form": "음 소세지나 메추리알이나 취향 아니시려나",
          "original_form": "음 소세지나 메추리알이나 취향 아니시려나",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017641.1.12",
          "form": "치킨을 시켜서 사리 추가는 안 했어요! 원래는 중당 좋아해요ㅎㅎ",
          "original_form": "치킨을 시켜서 사리 추가는 안 했어요! 원래는 중당 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017641.1.13",
          "form": "중당 넣으면 되게 맛있는데  ㅎ",
          "original_form": "중당 넣으면 되게 맛있는데  ㅎ",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017641.1.14",
          "form": "음료는 어떤거 드셨나요",
          "original_form": "음료는 어떤거 드셨나요",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017641.1.15",
          "form": "콜라? 사이다?",
          "original_form": "콜라? 사이다?",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017641.1.16",
          "form": "사이다 좋아하는데 이번엔 콜라로 시켰어요ㅠ",
          "original_form": "사이다 좋아하는데 이번엔 콜라로 시켰어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017641.1.17",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017641.1.18",
          "form": "이유가 있나요",
          "original_form": "이유가 있나요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017641.1.19",
          "form": "동생이 시켜서요...ㅠㅠㅠㅠ",
          "original_form": "동생이 시켜서요...ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017641.1.20",
          "form": "아..하...?",
          "original_form": "아..하...?",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017641.1.21",
          "form": "결정권이 동생에게 있었군요ㅠㅠ",
          "original_form": "결정권이 동생에게 있었군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017641.1.22",
          "form": "아쉬우셨겠어요",
          "original_form": "아쉬우셨겠어요",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017641.1.23",
          "form": "항상 사이다였으니 이 번만 봐주려구요..ㅎㅎ",
          "original_form": "항상 사이다였으니 이 번만 봐주려구요..ㅎㅎ",
