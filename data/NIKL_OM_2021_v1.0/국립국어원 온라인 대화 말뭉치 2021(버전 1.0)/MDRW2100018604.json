      "utterance": [
        {
          "id": "MDRW2100018604.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018604.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018604.1.3",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018604.1.4",
          "form": "올림픽 이후로 여자배구 좋아하게 됐습니다 ㅎㅎ\n좋아하는 스포츠 있으세요?",
          "original_form": "올림픽 이후로 여자배구 좋아하게 됐습니다 ㅎㅎ\n좋아하는 스포츠 있으세요?",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018604.1.5",
          "form": "전 원래 야구 좋아하다가",
          "original_form": "전 원래 야구 좋아하다가",
          "speaker_id": "2",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018604.1.6",
          "form": "요즘은 마음에서 멀어지고 있어요ㅎㅎ",
          "original_form": "요즘은 마음에서 멀어지고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018604.1.7",
          "form": "배구 엄청 재미있더라구요!!",
          "original_form": "배구 엄청 재미있더라구요!!",
          "speaker_id": "2",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018604.1.8",
          "form": "영업해도 되나요 ㅎㅎ",
          "original_form": "영업해도 되나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018604.1.9",
          "form": "배구로 오세요! 선수들 진짜 너무 열심히 해서 마음이 간답니다ㅠ",
          "original_form": "배구로 오세요! 선수들 진짜 너무 열심히 해서 마음이 간답니다ㅠ",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018604.1.10",
          "form": "주변에 배구장이 없어서",
          "original_form": "주변에 배구장이 없어서",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018604.1.11",
          "form": "직관도 못가고ㅜㅜ",
          "original_form": "직관도 못가고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018604.1.12",
          "form": "중계로 보면 되기는 하느ㄴ데",
          "original_form": "중계로 보면 되기는 하느ㄴ데",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018604.1.13",
          "form": "제가 직관을 되게 좋아해서...ㅎㅎ",
          "original_form": "제가 직관을 되게 좋아해서...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018604.1.14",
          "form": "저도 경기장이랑 먼 곳에 살아서 주로 중계만 보고 있어요ㅠ",
          "original_form": "저도 경기장이랑 먼 곳에 살아서 주로 중계만 보고 있어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018604.1.15",
          "form": "경기장이 좀 전국 곳곳에 있었으면 좋았을텐데ㅠㅠ",
          "original_form": "경기장이 좀 전국 곳곳에 있었으면 좋았을텐데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 09:30"
        },
        {
          "id": "MDRW2100018604.1.16",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018604.1.17",
          "form": "직관이 짜릿하긴 하더라구요ㅋㅋㅋ",
          "original_form": "직관이 짜릿하긴 하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018604.1.18",
          "form": "맞아요! 농구랑 야구 몇 번 직관 다녀봤는데 확실히 더 재밌었어요 ㅋㅋㅋ",
          "original_form": "맞아요! 농구랑 야구 몇 번 직관 다녀봤는데 확실히 더 재밌었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018604.1.19",
          "form": "홈런칠때 그 짜릿함ㅋㅋㅋ",
          "original_form": "홈런칠때 그 짜릿함ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018604.1.20",
          "form": "약간 역사적인 순간에 함께 있다는것도 엄청 크더라구요!",
          "original_form": "약간 역사적인 순간에 함께 있다는것도 엄청 크더라구요!",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018604.1.21",
          "form": "홈런 ㅋㅋㅋㅋㅋ\n저 부산 살아서 아주라! 당해본 적 있어요 ㅋㅋㅋㅋ 11살이었나 아주라! 해서 그때 홈런볼을 받았는데, 옆에 6살쯤 되는 남자애가 저를 찾아와가지고 뺏기다시피 줬죠.. ㅎㅎ",
          "original_form": "홈런 ㅋㅋㅋㅋㅋ\n저 부산 살아서 아주라! 당해본 적 있어요 ㅋㅋㅋㅋ 11살이었나 아주라! 해서 그때 홈런볼을 받았는데, 옆에 6살쯤 되는 남자애가 저를 찾아와가지고 뺏기다시피 줬죠.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018604.1.22",
          "form": "요즘도 그거 많은데",
          "original_form": "요즘도 그거 많은데",
          "speaker_id": "2",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018604.1.23",
          "form": "그렇게 인식이 좋은지는 모르겠어요",
          "original_form": "그렇게 인식이 좋은지는 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018604.1.24",
          "form": "저도 당해봤서 그럴까요....",
          "original_form": "저도 당해봤서 그럴까요....",
          "speaker_id": "2",
          "time": "20210827 10:45"
        },
        {
          "id": "MDRW2100018604.1.25",
          "form": "동지를 만났네요ㅠ\n저거 참 줘야하는 입장일 때 너무 기분 별로였어요 ㅋㅋㅋㅋ\n11살이면 나도 애였는데ㅠ ㅋㅋㅋㅋ",
          "original_form": "동지를 만났네요ㅠ\n저거 참 줘야하는 입장일 때 너무 기분 별로였어요 ㅋㅋㅋㅋ\n11살이면 나도 애였는데ㅠ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:50"
        }
      ]
