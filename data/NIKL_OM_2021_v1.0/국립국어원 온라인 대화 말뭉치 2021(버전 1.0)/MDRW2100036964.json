      "utterance": [
        {
          "id": "MDRW2100036964.1.1",
          "form": "저도 자세히 모르는데  정신 장애 연극 프로젝트 그런 걸로 해서 돈 주는게 있더라고요",
          "original_form": "저도 자세히 모르는데  정신 장애 연극 프로젝트 그런 걸로 해서 돈 주는게 있더라고요",
          "speaker_id": "1",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036964.1.2",
          "form": "오 우와!!!",
          "original_form": "오 우와!!!",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036964.1.3",
          "form": "장애 있는 분들 재활에 좋은",
          "original_form": "장애 있는 분들 재활에 좋은",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036964.1.4",
          "form": "프로그램을 진행하도록 하는거군요!",
          "original_form": "프로그램을 진행하도록 하는거군요!",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036964.1.5",
          "form": "그런데 그런 지원 사업이 장 단점이 있는 거 같긴 해요",
          "original_form": "그런데 그런 지원 사업이 장 단점이 있는 거 같긴 해요",
          "speaker_id": "1",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036964.1.6",
          "form": "근데 정말 몇 명만 기회가 주어지겠네요",
          "original_form": "근데 정말 몇 명만 기회가 주어지겠네요",
          "speaker_id": "2",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100036964.1.7",
          "form": "네 일단 나라에서 하는 거니까 또 어느정도 거기 인맥 있는 사람들이 하는 거 같더라고요",
          "original_form": "네 일단 나라에서 하는 거니까 또 어느정도 거기 인맥 있는 사람들이 하는 거 같더라고요",
          "speaker_id": "1",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100036964.1.8",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "2",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100036964.1.9",
          "form": "화려한 무대 뒤에 이런 고충이 있네요",
          "original_form": "화려한 무대 뒤에 이런 고충이 있네요",
          "speaker_id": "2",
          "time": "20210831 15:51"
        },
        {
          "id": "MDRW2100036964.1.10",
          "form": "그리고 이건 연극은 아닌데 지원 사업을 빙자해서 강의를 진짜 많이 하거든요",
          "original_form": "그리고 이건 연극은 아닌데 지원 사업을 빙자해서 강의를 진짜 많이 하거든요",
          "speaker_id": "1",
          "time": "20210831 15:51"
        },
        {
          "id": "MDRW2100036964.1.11",
          "form": "그런 강의를 보면 그냥 하나마나한 강의들이 많아서요",
          "original_form": "그런 강의를 보면 그냥 하나마나한 강의들이 많아서요",
          "speaker_id": "1",
          "time": "20210831 15:51"
        },
        {
          "id": "MDRW2100036964.1.12",
          "form": "강사만 배불리는..",
          "original_form": "강사만 배불리는..",
          "speaker_id": "1",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036964.1.13",
          "form": "헐 그렇군요",
          "original_form": "헐 그렇군요",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036964.1.14",
          "form": "그러면 이것도 빈익빈 부익부",
          "original_form": "그러면 이것도 빈익빈 부익부",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036964.1.15",
          "form": "하던 사람만 계속 맡아서 하겠네요?",
          "original_form": "하던 사람만 계속 맡아서 하겠네요?",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036964.1.16",
          "form": "예술 공연도 결국 인맥이라니...",
          "original_form": "예술 공연도 결국 인맥이라니...",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036964.1.17",
          "form": "뭐 어디든 그렇지만요ㅠㅠ",
          "original_form": "뭐 어디든 그렇지만요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036964.1.18",
          "form": "아 그게 실적이 되니까 경력이 되잖아요",
          "original_form": "아 그게 실적이 되니까 경력이 되잖아요",
          "speaker_id": "1",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100036964.1.19",
          "form": "시장 못 한 사람은 계속 경력이 없으니까 못 하는 거 같아요",
          "original_form": "시장 못 한 사람은 계속 경력이 없으니까 못 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100036964.1.20",
          "form": "시작",
          "original_form": "시작",
          "speaker_id": "1",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100036964.1.21",
          "form": "그러게요!!!!",
          "original_form": "그러게요!!!!",
          "speaker_id": "2",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100036964.1.22",
          "form": "맞아요 이것도 뭐라 그래야되지 일종의",
          "original_form": "맞아요 이것도 뭐라 그래야되지 일종의",
          "speaker_id": "2",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100036964.1.23",
          "form": "경력 있는 신입사원",
          "original_form": "경력 있는 신입사원",
          "speaker_id": "2",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100036964.1.24",
          "form": "그런 느낌의 모순이랄까요 ㅎㅎㅎ",
          "original_form": "그런 느낌의 모순이랄까요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100036964.1.25",
          "form": "네 그냥 명분은 좋은데 알고 보면 다 좀 모순이 있는 거 같아요",
          "original_form": "네 그냥 명분은 좋은데 알고 보면 다 좀 모순이 있는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100036964.1.26",
          "form": "에휴 그러게요 명분은 좋네요...",
          "original_form": "에휴 그러게요 명분은 좋네요...",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036964.1.27",
          "form": "앞으로 공연 보러 갈 때",
          "original_form": "앞으로 공연 보러 갈 때",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036964.1.28",
          "form": "생각이 많아질 것 같네요!!!",
          "original_form": "생각이 많아질 것 같네요!!!",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036964.1.29",
          "form": "얼른 코로나가 끝나고 공연 많이 보면 좋겠어요",
          "original_form": "얼른 코로나가 끝나고 공연 많이 보면 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100036964.1.30",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100036964.1.31",
          "form": "문화생활을 못하니까 일상이 너무 단조로워요...",
          "original_form": "문화생활을 못하니까 일상이 너무 단조로워요...",
          "speaker_id": "2",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100036964.1.32",
          "form": "이젠 영화관 갈 때도 큰 맘 먹고 가야하니까요ㅠ",
          "original_form": "이젠 영화관 갈 때도 큰 맘 먹고 가야하니까요ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:12"
        }
      ]
