          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 욱겨 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 욱겨 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.15",
          "form": "name5은 계속 생머리 하고 싶다고 하다가",
          "original_form": "&name5&은 계속 생머리 하고 싶다고 하다가",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.16",
          "form": "누가봐도 어제 파마했음 ㅋㅋㅋ",
          "original_form": "누가봐도 어제 파마했음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.17",
          "form": "name6도 내일 똑같이 파마하고 온다고 하니",
          "original_form": "&name6&도 내일 똑같이 파마하고 온다고 하니",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.18",
          "form": "원래가 생머리잖아여??",
          "original_form": "원래가 생머리잖아여??",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.19",
          "form": "그제서야 웃고 학교로 갔음",
          "original_form": "그제서야 웃고 학교로 갔음",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.20",
          "form": "어 생머리지",
          "original_form": "어 생머리지",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.21",
          "form": "아 원래 대로~",
          "original_form": "아 원래 대로~",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.22",
          "form": "완전 뻗치는 머리",
          "original_form": "완전 뻗치는 머리",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016577.1.23",
          "form": "진짜 누가 봐도 방금 파마 한 아이 임",
          "original_form": "진짜 누가 봐도 방금 파마 한 아이 임",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016577.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ우리 첫찌는 아직도 티비에",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ우리 첫찌는 아직도 티비에",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016577.1.25",
          "form": "제발 name6가 더 뽀글 해서 오기를",
          "original_form": "제발 &name6&가 더 뽀글 해서 오기를",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016577.1.26",
          "form": "베컴머리같은거 나오면",
          "original_form": "베컴머리같은거 나오면",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016577.1.27",
          "form": "name5 머리라고 ㅋㅋ",
          "original_form": "&name5& 머리라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016577.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016577.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ둘이 배틀하는거예여??",
