      "utterance": [
        {
          "id": "MDRW2100007401.1.1",
          "form": "타리 없을 때 여기서 이런 거 풀어보자",
          "original_form": "타리 없을 때 여기서 이런 거 풀어보자",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007401.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007401.1.3",
          "form": "최근에",
          "original_form": "최근에",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007401.1.4",
          "form": "집에 블라인드를 설치햇고요",
          "original_form": "집에 블라인드를 설치햇고요",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007401.1.5",
          "form": "당근에서 로봇청소기를 네개 구매햇고요",
          "original_form": "당근에서 로봇청소기를 네개 구매햇고요",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007401.1.6",
          "form": "의자를 두개 삿습니다",
          "original_form": "의자를 두개 삿습니다",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007401.1.7",
          "form": "왜 로봇을 4개나 샀어요??",
          "original_form": "왜 로봇을 4개나 샀어요??",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007401.1.8",
          "form": "한놈은",
          "original_form": "한놈은",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007401.1.9",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007401.1.10",
          "form": "쓸모없어서",
          "original_form": "쓸모없어서",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007401.1.11",
          "form": "폐기처분 하는 바람에",
          "original_form": "폐기처분 하는 바람에",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007401.1.12",
          "form": "어느 양심없는 분이",
          "original_form": "어느 양심없는 분이",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007401.1.13",
          "form": "풀충하고도 10분밖에 안되는걸 팔아서요",
          "original_form": "풀충하고도 10분밖에 안되는걸 팔아서요",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007401.1.14",
          "form": "두개는 집에서 한개는 학교에서",
          "original_form": "두개는 집에서 한개는 학교에서",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007401.1.15",
          "form": "그렇게 네갭니다",
          "original_form": "그렇게 네갭니다",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007401.1.16",
          "form": "물로 청소하는 로봇은 따로삿거든요",
          "original_form": "물로 청소하는 로봇은 따로삿거든요",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007401.1.17",
          "form": "아아 물청소도 로봇이 있군요",
          "original_form": "아아 물청소도 로봇이 있군요",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.18",
          "form": "저는 아침에 먹을 스콘을",
          "original_form": "저는 아침에 먹을 스콘을",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.20",
          "form": "배송비 줄이려고 30개 샀어요",
          "original_form": "배송비 줄이려고 30개 샀어요",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.21",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.22",
          "form": "냉동보관",
          "original_form": "냉동보관",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.23",
          "form": "하면되나요",
          "original_form": "하면되나요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.24",
          "form": "좋은 소비네요",
          "original_form": "좋은 소비네요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.25",
          "form": "소비요정으로서 박수드립니다",
          "original_form": "소비요정으로서 박수드립니다",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007401.1.26",
          "form": "위시리스트 있으신가요?",
          "original_form": "위시리스트 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007401.1.27",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007401.1.28",
          "form": "건조기",
          "original_form": "건조기",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007401.1.29",
          "form": "사고싶은데",
          "original_form": "사고싶은데",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007401.1.30",
          "form": "집에 공간이 없어요",
          "original_form": "집에 공간이 없어요",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007401.1.31",
          "form": "저는 대체로 저의 가사노동을 대신해줄 물건을 쇼핑하는걸 좋아해요",
          "original_form": "저는 대체로 저의 가사노동을 대신해줄 물건을 쇼핑하는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007401.1.32",
          "form": "뭐가 있을까요 식세기 정도 떠오르네요",
          "original_form": "뭐가 있을까요 식세기 정도 떠오르네요",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007401.1.33",
          "form": "식기세척기는",
          "original_form": "식기세척기는",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007401.1.34",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007401.1.35",
          "form": "잇으니까",
          "original_form": "잇으니까",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007401.1.36",
          "form": "으음",
          "original_form": "으음",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007401.1.37",
          "form": "노예가 필요한거같아요",
          "original_form": "노예가 필요한거같아요",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007401.1.38",
          "form": "노예는 쇼핑으로 구매하기가 어려우니...",
          "original_form": "노예는 쇼핑으로 구매하기가 어려우니...",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007401.1.39",
          "form": "건조기?",
          "original_form": "건조기?",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.40",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.41",
          "form": "역시 그것뿐이겟죠",
          "original_form": "역시 그것뿐이겟죠",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.42",
          "form": "건 조 기?",
          "original_form": "건 조 기?",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.43",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.44",
          "form": "건조기",
          "original_form": "건조기",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.45",
          "form": "얼마안하던데",
          "original_form": "얼마안하던데",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.46",
          "form": "당근에서",
          "original_form": "당근에서",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.47",
          "form": "문제는 수도를 연결하기가...",
          "original_form": "문제는 수도를 연결하기가...",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007401.1.48",
          "form": "수도 연결을 또 해야 하는군요",
          "original_form": "수도 연결을 또 해야 하는군요",
          "speaker_id": "1",
          "time": "20210822 01:59"
        }
      ]
