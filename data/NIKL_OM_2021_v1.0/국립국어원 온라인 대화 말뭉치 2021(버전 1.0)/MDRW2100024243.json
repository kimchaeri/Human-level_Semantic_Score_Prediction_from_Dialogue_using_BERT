          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024243.1.35",
          "form": "이게또 운도 따르는게요",
          "original_form": "이게또 운도 따르는게요",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
