      "utterance": [
        {
          "id": "MDRW2100024243.1.1",
          "form": "카톡에 저옷 삿어요 ㅎㅎ",
          "original_form": "카톡에 저옷 삿어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100024243.1.2",
          "form": "근데 남아가 입어도 예쁘더라구요",
          "original_form": "근데 남아가 입어도 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024243.1.3",
          "form": "카톡?ㅎㅎ",
          "original_form": "카톡?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100024243.1.4",
          "form": "어떤거요?ㅎㅎ",
          "original_form": "어떤거요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024243.1.5",
          "form": "네네 ㅋㅋ",
          "original_form": "네네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024243.1.6",
          "form": "단톡에 올렷으요 ㅎㅎ",
          "original_form": "단톡에 올렷으요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024243.1.7",
          "form": "완전 쨍하게 예브더라구요",
          "original_form": "완전 쨍하게 예브더라구요",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100024243.1.8",
          "form": "이쁘겟네요 ㅎㅎㅎ",
          "original_form": "이쁘겟네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024243.1.9",
          "form": "야얍 ㅋ",
          "original_form": "야얍 ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024243.1.10",
          "form": "아이 옷을 사주고싶은데ㅎ 사이즈를 잘몰라요ㅎ",
          "original_form": "아이 옷을 사주고싶은데ㅎ 사이즈를 잘몰라요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024243.1.11",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024243.1.12",
          "form": "괜히 샀다가 사이즈 안맞으면 반품하기도그렇고ㄹㄹ",
          "original_form": "괜히 샀다가 사이즈 안맞으면 반품하기도그렇고ㄹㄹ",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024243.1.13",
          "form": "이번에도 그랬구요 ㄹㅎ",
          "original_form": "이번에도 그랬구요 ㄹㅎ",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024243.1.14",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024243.1.15",
          "form": "타미는 기본사이즈가 나와있잖아요ㅎ",
          "original_form": "타미는 기본사이즈가 나와있잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024243.1.16",
          "form": "저희애가  5살인네 6~7t 입는데",
          "original_form": "저희애가  5살인네 6~7t 입는데",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024243.1.17",
          "form": "근데 사면 좀 작다는 생각이등어요 ㅣㅎ",
          "original_form": "근데 사면 좀 작다는 생각이등어요 ㅣㅎ",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024243.1.18",
          "form": "음 6이 좀 작았어요ㅎ",
          "original_form": "음 6이 좀 작았어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024243.1.19",
          "form": "제가볼땜 8t 는 입어야 할거 같아요",
          "original_form": "제가볼땜 8t 는 입어야 할거 같아요",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024243.1.20",
          "form": "7은 딱맞을것같은?",
          "original_form": "7은 딱맞을것같은?",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024243.1.21",
          "form": "이번에 8오면 알것같아요ㅎㅎ",
          "original_form": "이번에 8오면 알것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024243.1.22",
          "form": "근데 이게 어떤건 또 크고",
          "original_form": "근데 이게 어떤건 또 크고",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024243.1.23",
          "form": "그리거 스타일에 따라서 좀 다르기도 하구요",
          "original_form": "그리거 스타일에 따라서 좀 다르기도 하구요",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024243.1.24",
          "form": "아까 보여드린옷은 8t 인데  오바사이즈로 예쁘더라구요 ㅎㅎ",
          "original_form": "아까 보여드린옷은 8t 인데  오바사이즈로 예쁘더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024243.1.25",
          "form": "ㅎㅎ 사야하는데 절약을 실천해보려고ㅎㅎㅎ",
          "original_form": "ㅎㅎ 사야하는데 절약을 실천해보려고ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024243.1.26",
          "form": "근데 이쁠것같긴해요ㅎ",
          "original_form": "근데 이쁠것같긴해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024243.1.27",
          "form": "우리아이도 찐한색이 잘어울려요ㄹ",
          "original_form": "우리아이도 찐한색이 잘어울려요ㄹ",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024243.1.28",
          "form": "주 저거 0원에 삿어여 ㅎ",
          "original_form": "주 저거 0원에 삿어여 ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
