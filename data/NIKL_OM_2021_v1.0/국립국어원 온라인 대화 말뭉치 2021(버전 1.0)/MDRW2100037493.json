      "utterance": [
        {
          "id": "MDRW2100037493.1.1",
          "form": "히히 건강 이야기가 하고 싶어졌어요 ",
          "original_form": "히히 건강 이야기가 하고 싶어졌어요 ",
          "speaker_id": "1",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100037493.1.2",
          "form": "건강은 괜찮으신가요??",
          "original_form": "건강은 괜찮으신가요??",
          "speaker_id": "2",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100037493.1.3",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100037493.1.4",
          "form": "근데 이제 갑자기 선선해지니까",
          "original_form": "근데 이제 갑자기 선선해지니까",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100037493.1.5",
          "form": "비염이 심해지더라구요,, name2님은 비염 없으신가요?",
          "original_form": "비염이 심해지더라구요,, &name2&님은 비염 없으신가요?",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100037493.1.6",
          "form": "축농증이랑 비염을 달고 살았는데",
          "original_form": "축농증이랑 비염을 달고 살았는데",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037493.1.7",
          "form": "어느 날 갑자기 없어졌어요!",
          "original_form": "어느 날 갑자기 없어졌어요!",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037493.1.8",
          "form": "없어진지 한.. 6년된 것 같아요",
          "original_form": "없어진지 한.. 6년된 것 같아요",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037493.1.9",
          "form": "비염 너무 괴롭죠ㅠㅠ",
          "original_form": "비염 너무 괴롭죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037493.1.10",
          "form": "헐 우와",
          "original_form": "헐 우와",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037493.1.11",
          "form": "갑자기요?!",
          "original_form": "갑자기요?!",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037493.1.12",
          "form": "생활습관 바뀐 게 있었나요?!",
          "original_form": "생활습관 바뀐 게 있었나요?!",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037493.1.13",
          "form": "그걸 모르겠어요... 오트리빈은 항상 비상용으로 들고 다니는데",
          "original_form": "그걸 모르겠어요... 오트리빈은 항상 비상용으로 들고 다니는데",
          "speaker_id": "2",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100037493.1.14",
          "form": "어느날부터 안 써서 왜지.. 싶거든요 ㅎㅎ",
          "original_form": "어느날부터 안 써서 왜지.. 싶거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100037493.1.15",
          "form": "침대머리 방향이 바뀐 것 같고...",
          "original_form": "침대머리 방향이 바뀐 것 같고...",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100037493.1.16",
          "form": "근데 이게 상관이있나싶네욤... 진짜 왜지...",
          "original_form": "근데 이게 상관이있나싶네욤... 진짜 왜지...",
          "speaker_id": "2",
          "time": "20210901 16:02"
        },
        {
          "id": "MDRW2100037493.1.17",
          "form": "헐 우와 신기해요",
          "original_form": "헐 우와 신기해요",
          "speaker_id": "1",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100037493.1.18",
          "form": "따로 이사를 가신 것도 아닌데...!!!",
          "original_form": "따로 이사를 가신 것도 아닌데...!!!",
          "speaker_id": "1",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100037493.1.19",
          "form": "방정리를 하신 것도 아닌데?!",
          "original_form": "방정리를 하신 것도 아닌데?!",
          "speaker_id": "1",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100037493.1.20",
          "form": "네 진짜 이게 해결책이었다싶은게 있으면 알려드리고싶은데ㅠ",
          "original_form": "네 진짜 이게 해결책이었다싶은게 있으면 알려드리고싶은데ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100037493.1.21",
          "form": "진짜 뭐가 달라진게 없었거든요ㅠㅠ",
          "original_form": "진짜 뭐가 달라진게 없었거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100037493.1.22",
          "form": "그래도 희망이 보여요...!!!!",
          "original_form": "그래도 희망이 보여요...!!!!",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037493.1.23",
          "form": "저는 아토피가 그렇게 사라졌었거든요!",
          "original_form": "저는 아토피가 그렇게 사라졌었거든요!",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037493.1.24",
          "form": "원래 아토피 -> 비염 -> 천식 이렇게 진행되는거긴 하지만...",
          "original_form": "원래 아토피 -> 비염 -> 천식 이렇게 진행되는거긴 하지만...",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037493.1.25",
          "form": "헐 그렇게 연관되나요??",
          "original_form": "헐 그렇게 연관되나요??",
          "speaker_id": "2",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037493.1.26",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037493.1.27",
          "form": "이게 다 자가면역질환이고",
          "original_form": "이게 다 자가면역질환이고",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037493.1.28",
          "form": "알러지 행진 이라고 하거든요",
          "original_form": "알러지 행진 이라고 하거든요",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037493.1.29",
          "form": "그래서 어렸을 때 아토피 있던 애들이 자라면 높은 확률로 비염도 생겨요ㅠ",
          "original_form": "그래서 어렸을 때 아토피 있던 애들이 자라면 높은 확률로 비염도 생겨요ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:26"
        },
        {
          "id": "MDRW2100037493.1.30",
          "form": "오! 의료쪽 공부하시나요?? 알러지행진이라는 말 처음 들어봤어요 ㅎㅎ",
          "original_form": "오! 의료쪽 공부하시나요?? 알러지행진이라는 말 처음 들어봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:27"
        },
        {
          "id": "MDRW2100037493.1.31",
          "form": "그럼 천식을 조심해야겠네요 저는 ㅎㅎ",
          "original_form": "그럼 천식을 조심해야겠네요 저는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:27"
        },
        {
          "id": "MDRW2100037493.1.32",
          "form": "앜ㅋㅋ 의료쪽은 아니지만",
          "original_form": "앜ㅋㅋ 의료쪽은 아니지만",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100037493.1.33",
          "form": "한 때 약대 편입 준비를 하긴 했었어요!!",
          "original_form": "한 때 약대 편입 준비를 하긴 했었어요!!",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100037493.1.34",
          "form": "네네 자가면역질환은 항상 조심해야하셔야해요ㅠ",
          "original_form": "네네 자가면역질환은 항상 조심해야하셔야해요ㅠ",
