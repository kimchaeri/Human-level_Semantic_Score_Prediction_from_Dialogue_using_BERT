        {
          "id": "MDRW2100033646.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 19:57"
        },
        {
          "id": "MDRW2100033646.1.2",
          "form": "아뇨 전 안키워요",
          "original_form": "아뇨 전 안키워요",
          "speaker_id": "2",
          "time": "20210824 19:57"
        },
        {
          "id": "MDRW2100033646.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "2",
          "time": "20210824 19:57"
        },
        {
          "id": "MDRW2100033646.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210824 19:57"
        },
        {
          "id": "MDRW2100033646.1.5",
          "form": "키우신적 있으신가요?",
          "original_form": "키우신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 19:58"
        },
        {
          "id": "MDRW2100033646.1.6",
          "form": "강아지 어릴 때요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "강아지 어릴 때요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:58"
        },
        {
          "id": "MDRW2100033646.1.7",
          "form": "저는 닭 키워봤어요ㅋㅋ",
          "original_form": "저는 닭 키워봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:58"
        },
        {
          "id": "MDRW2100033646.1.8",
          "form": "헉 닭이요ㅋㅋㅋㅋㅋ?",
          "original_form": "헉 닭이요ㅋㅋㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210824 20:01"
        },
        {
          "id": "MDRW2100033646.1.9",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210824 20:01"
        },
        {
          "id": "MDRW2100033646.1.10",
          "form": "귀여워요ㅋㅋ",
          "original_form": "귀여워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:01"
        },
        {
          "id": "MDRW2100033646.1.11",
          "form": "병아리 때부터 키웠어요?",
          "original_form": "병아리 때부터 키웠어요?",
          "speaker_id": "2",
          "time": "20210824 20:02"
        },
        {
          "id": "MDRW2100033646.1.12",
          "form": "네ㅋㅋ 초딩때 오백원 주고 데려왔어요",
          "original_form": "네ㅋㅋ 초딩때 오백원 주고 데려왔어요",
          "speaker_id": "1",
          "time": "20210824 20:02"
        },
        {
          "id": "MDRW2100033646.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ우오ㅓ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ우오ㅓ",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100033646.1.14",
          "form": "거기서 닭이 된거에요?",
          "original_form": "거기서 닭이 된거에요?",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100033646.1.15",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100033646.1.16",
          "form": "ㅋㅋㅋㅋ 애지중지 키웠어요",
          "original_form": "ㅋㅋㅋㅋ 애지중지 키웠어요",
