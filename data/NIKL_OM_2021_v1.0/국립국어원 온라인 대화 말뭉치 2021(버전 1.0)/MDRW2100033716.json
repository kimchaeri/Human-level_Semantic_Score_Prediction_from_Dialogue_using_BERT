      "utterance": [
        {
          "id": "MDRW2100033716.1.1",
          "form": "name2님은 혼자 지내시면 집안일을 많이 하시겠어요",
          "original_form": "&name2&님은 혼자 지내시면 집안일을 많이 하시겠어요",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100033716.1.2",
          "form": "네 집안일을 독박이죠ㅋ",
          "original_form": "네 집안일을 독박이죠ㅋ",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100033716.1.3",
          "form": "모든걸 제가해야되니",
          "original_form": "모든걸 제가해야되니",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100033716.1.4",
          "form": "name1님은 부모님이 해주시나요?",
          "original_form": "&name1&님은 부모님이 해주시나요?",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100033716.1.5",
          "form": "평일에는 주로 부모님이 하시는 편이고",
          "original_form": "평일에는 주로 부모님이 하시는 편이고",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033716.1.6",
          "form": "주말엔 집안일 돕고 있어요",
          "original_form": "주말엔 집안일 돕고 있어요",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033716.1.7",
          "form": "그래봤자 설거지나 청소 정도지만",
          "original_form": "그래봤자 설거지나 청소 정도지만",
          "speaker_id": "1",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033716.1.8",
          "form": "그게어디예요ㅎ",
          "original_form": "그게어디예요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033716.1.9",
          "form": "전 화장실청소랑 음식물버리는게 젤귀찮더라구요",
          "original_form": "전 화장실청소랑 음식물버리는게 젤귀찮더라구요",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100033716.1.10",
          "form": "아아 귀찮은 집안일 1,2순위인 듯요",
          "original_form": "아아 귀찮은 집안일 1,2순위인 듯요",
          "speaker_id": "1",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100033716.1.11",
          "form": "근데 안하면 냄새나고 곰팡이나 벌레 생길 수도 있고...",
          "original_form": "근데 안하면 냄새나고 곰팡이나 벌레 생길 수도 있고...",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033716.1.12",
          "form": "맞아요 물때 특히요ㅠㅠ",
          "original_form": "맞아요 물때 특히요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033716.1.13",
          "form": "누가 와서 해줌 좋으련만ㅎ",
          "original_form": "누가 와서 해줌 좋으련만ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033716.1.14",
          "form": "저도 전에 자취할 때 화장실 청소랑 음식물 쓰레기 버리는 것 정말 귀찮았어요",
          "original_form": "저도 전에 자취할 때 화장실 청소랑 음식물 쓰레기 버리는 것 정말 귀찮았어요",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033716.1.15",
          "form": "심지어 고기 굽는 것도 설거지 생각하면 망설이게 되더라고요ㅜ",
          "original_form": "심지어 고기 굽는 것도 설거지 생각하면 망설이게 되더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100033716.1.16",
          "form": "그래서 화장실은 자주안해요ㅎㅎ",
          "original_form": "그래서 화장실은 자주안해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100033716.1.17",
          "form": "돈많음 업체부름될텐데^^",
          "original_form": "돈많음 업체부름될텐데^^",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100033716.1.18",
          "form": "ㅎㅎ집안일도 돈 있으면 편해지죠~",
          "original_form": "ㅎㅎ집안일도 돈 있으면 편해지죠~",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033716.1.19",
          "form": "저는 재작년에 건조기랑 식기세척기 들이고 정말 한결 편해졌어요",
          "original_form": "저는 재작년에 건조기랑 식기세척기 들이고 정말 한결 편해졌어요",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033716.1.20",
          "form": "식기세척기좋나요?",
          "original_form": "식기세척기좋나요?",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033716.1.21",
          "form": "애벌? 세척안해도 넣기만함 닦여요?",
          "original_form": "애벌? 세척안해도 넣기만함 닦여요?",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033716.1.22",
          "form": "네 설거지하고 싱크대에 물 떨어지는 일 없으니 좋더라고요",
          "original_form": "네 설거지하고 싱크대에 물 떨어지는 일 없으니 좋더라고요",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033716.1.23",
          "form": "저는 음식물은 한번 씻어서 넣어요",
          "original_form": "저는 음식물은 한번 씻어서 넣어요",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033716.1.24",
          "form": "그게 더 깨끗하긴 하죠",
          "original_form": "그게 더 깨끗하긴 하죠",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033716.1.25",
          "form": "건조기도 있음 편하다던데",
          "original_form": "건조기도 있음 편하다던데",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033716.1.26",
          "form": "네 음식물 찌꺼기 걸러준다는데",
          "original_form": "네 음식물 찌꺼기 걸러준다는데",
          "speaker_id": "1",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033716.1.27",
          "form": "그것 치우는 것도 일일 듯해서",
          "original_form": "그것 치우는 것도 일일 듯해서",
          "speaker_id": "1",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033716.1.28",
          "form": "세상 좋아졌어요ㅋ",
          "original_form": "세상 좋아졌어요ㅋ",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033716.1.29",
          "form": "육아도 아이템빨이라는데",
          "original_form": "육아도 아이템빨이라는데",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033716.1.30",
          "form": "가사도 그런가봐요",
          "original_form": "가사도 그런가봐요",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033716.1.31",
          "form": "네 가사도 아이템빨이 맞는 것 같아요ㅎㅎ",
          "original_form": "네 가사도 아이템빨이 맞는 것 같아요ㅎㅎ",
