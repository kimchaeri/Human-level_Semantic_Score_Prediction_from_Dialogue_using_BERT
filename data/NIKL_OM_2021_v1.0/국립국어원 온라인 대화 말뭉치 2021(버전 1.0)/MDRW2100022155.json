      "utterance": [
        {
          "id": "MDRW2100022155.1.1",
          "form": "재택근무",
          "original_form": "재택근무",
          "speaker_id": "1",
          "time": "20210829 09:45"
        },
        {
          "id": "MDRW2100022155.1.2",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "1",
          "time": "20210829 09:45"
        },
        {
          "id": "MDRW2100022155.1.3",
          "form": "아니요 저는...",
          "original_form": "아니요 저는...",
          "speaker_id": "2",
          "time": "20210829 09:45"
        },
        {
          "id": "MDRW2100022155.1.4",
          "form": "제가 다니는 곳은",
          "original_form": "제가 다니는 곳은",
          "speaker_id": "2",
          "time": "20210829 09:45"
        },
        {
          "id": "MDRW2100022155.1.5",
          "form": "재택을 안시켜주네요...",
          "original_form": "재택을 안시켜주네요...",
          "speaker_id": "2",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100022155.1.6",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100022155.1.7",
          "form": "융통성이 없군요!!!",
          "original_form": "융통성이 없군요!!!",
          "speaker_id": "1",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100022155.1.8",
          "form": "제 사위는 가끔씩 재택을 하는데",
          "original_form": "제 사위는 가끔씩 재택을 하는데",
          "speaker_id": "1",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100022155.1.9",
          "form": "그럴 때면 집에 와서 일을 도와줘요",
          "original_form": "그럴 때면 집에 와서 일을 도와줘요",
          "speaker_id": "1",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100022155.1.10",
          "form": "우와 재택근무의 순기능이네요",
          "original_form": "우와 재택근무의 순기능이네요",
          "speaker_id": "2",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100022155.1.11",
          "form": "제 친구들도 보면",
          "original_form": "제 친구들도 보면",
          "speaker_id": "2",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.12",
          "form": "재택근무 하는거 되게 좋아하더라구요",
          "original_form": "재택근무 하는거 되게 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.13",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.14",
          "form": "먹는 것도 마음대로 먹을 수 있고",
          "original_form": "먹는 것도 마음대로 먹을 수 있고",
          "speaker_id": "1",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.15",
          "form": "교통비도 안들고",
          "original_form": "교통비도 안들고",
          "speaker_id": "1",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.16",
          "form": "그리고...ㅎㅎ",
          "original_form": "그리고...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.17",
          "form": "보기 싫은 사람도 안볼 수 있어요!!!",
          "original_form": "보기 싫은 사람도 안볼 수 있어요!!!",
          "speaker_id": "2",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.18",
          "form": "그게 가장 큰 장점이네요!!!",
          "original_form": "그게 가장 큰 장점이네요!!!",
          "speaker_id": "1",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.19",
          "form": "이제 점점 재택 비중이 높아지면서",
          "original_form": "이제 점점 재택 비중이 높아지면서",
          "speaker_id": "2",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.20",
          "form": "미국의 경우에는",
          "original_form": "미국의 경우에는",
          "speaker_id": "2",
          "time": "20210829 09:47"
        },
        {
          "id": "MDRW2100022155.1.21",
          "form": "연봉을 삭감하는 곳도 있다고 하더라구요",
          "original_form": "연봉을 삭감하는 곳도 있다고 하더라구요",
          "speaker_id": "2",
          "time": "20210829 09:48"
        },
        {
          "id": "MDRW2100022155.1.22",
          "form": "네??? 그럼 너무 억울할거같은데요",
          "original_form": "네??? 그럼 너무 억울할거같은데요",
          "speaker_id": "1",
          "time": "20210829 09:48"
        },
        {
          "id": "MDRW2100022155.1.23",
          "form": "그쵸!!!",
          "original_form": "그쵸!!!",
          "speaker_id": "2",
          "time": "20210829 09:48"
        },
        {
          "id": "MDRW2100022155.1.24",
          "form": "심지어",
          "original_form": "심지어",
          "speaker_id": "2",
          "time": "20210829 09:48"
        },
        {
          "id": "MDRW2100022155.1.25",
          "form": "동네 물가에 따라서",
          "original_form": "동네 물가에 따라서",
          "speaker_id": "2",
          "time": "20210829 09:48"
        },
        {
          "id": "MDRW2100022155.1.26",
          "form": "물가가 싼 동네에 사는 사람한테는",
          "original_form": "물가가 싼 동네에 사는 사람한테는",
          "speaker_id": "2",
          "time": "20210829 09:48"
        },
        {
          "id": "MDRW2100022155.1.27",
          "form": "그만큼 월급을 조금만 준대요",
          "original_form": "그만큼 월급을 조금만 준대요",
          "speaker_id": "2",
          "time": "20210829 09:48"
        },
        {
          "id": "MDRW2100022155.1.28",
          "form": "그게 말이 되나요???",
          "original_form": "그게 말이 되나요???",
          "speaker_id": "1",
          "time": "20210829 09:48"
        },
        {
          "id": "MDRW2100022155.1.29",
          "form": "재택 때문에",
          "original_form": "재택 때문에",
          "speaker_id": "1",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.30",
          "form": "오히려 피해를 입는군요",
          "original_form": "오히려 피해를 입는군요",
          "speaker_id": "1",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.32",
          "form": "그리고 물가가 싼 동네에 사는 사람일수록",
          "original_form": "그리고 물가가 싼 동네에 사는 사람일수록",
          "speaker_id": "2",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.33",
          "form": "그만큼 벌이가 없다는 걸텐데",
          "original_form": "그만큼 벌이가 없다는 걸텐데",
          "speaker_id": "2",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.34",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.35",
          "form": "그리고 그 동네에서만 지내는 것도 아니고",
          "original_form": "그리고 그 동네에서만 지내는 것도 아니고",
          "speaker_id": "1",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.36",
          "form": "비행기 타고 어디든 가는 시대에...",
          "original_form": "비행기 타고 어디든 가는 시대에...",
          "speaker_id": "1",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.37",
          "form": "정말 이상한 방침이네요",
          "original_form": "정말 이상한 방침이네요",
          "speaker_id": "1",
          "time": "20210829 09:49"
        },
        {
          "id": "MDRW2100022155.1.38",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 09:50"
        },
        {
          "id": "MDRW2100022155.1.39",
          "form": "미국이면 뭐든 선진적일 줄 알았는데...",
          "original_form": "미국이면 뭐든 선진적일 줄 알았는데...",
          "speaker_id": "2",
          "time": "20210829 09:50"
        },
        {
          "id": "MDRW2100022155.1.40",
          "form": "이상해요!!!",
          "original_form": "이상해요!!!",
          "speaker_id": "2",
          "time": "20210829 09:50"
        },
        {
          "id": "MDRW2100022155.1.41",
          "form": "재택근무가 일상이 되면서",
          "original_form": "재택근무가 일상이 되면서",
          "speaker_id": "1",
          "time": "20210829 09:50"
        },
        {
          "id": "MDRW2100022155.1.42",
          "form": "생각지도 못한 일들이 생기네요",
          "original_form": "생각지도 못한 일들이 생기네요",
          "speaker_id": "1",
          "time": "20210829 09:50"
        },
        {
          "id": "MDRW2100022155.1.43",
          "form": "그러게요 ㅠ",
          "original_form": "그러게요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 09:50"
        }
      ]
    }
  ]
