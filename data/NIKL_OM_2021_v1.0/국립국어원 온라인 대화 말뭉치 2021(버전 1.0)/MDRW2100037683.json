      "utterance": [
        {
          "id": "MDRW2100037683.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100037683.1.2",
          "form": "네네 좋아해요 ㅎㅎ name1님은요!",
          "original_form": "네네 좋아해요 ㅎㅎ &name1&님은요!",
          "speaker_id": "2",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100037683.1.3",
          "form": "저도요 휴양위주로요",
          "original_form": "저도요 휴양위주로요",
          "speaker_id": "1",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100037683.1.4",
          "form": "오 기억에 남는 곳 있나요",
          "original_form": "오 기억에 남는 곳 있나요",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100037683.1.5",
          "form": "푸켓이여~~^^",
          "original_form": "푸켓이여~~^^",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100037683.1.6",
          "form": "오 푸켓 저도 가보고싶은데..",
          "original_form": "오 푸켓 저도 가보고싶은데..",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037683.1.7",
          "form": "리조트에 묵으셨나요",
          "original_form": "리조트에 묵으셨나요",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037683.1.8",
          "form": "네네  숙소도 경치도 멋졌어요",
          "original_form": "네네  숙소도 경치도 멋졌어요",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037683.1.9",
          "form": "저는 방콕은 갔었는데 푸켓도 궁금하네요 갔다온 사람들이 다 좋다고",
          "original_form": "저는 방콕은 갔었는데 푸켓도 궁금하네요 갔다온 사람들이 다 좋다고",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037683.1.10",
          "form": "네 좋더하더라구요",
          "original_form": "네 좋더하더라구요",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037683.1.11",
          "form": "강추",
          "original_form": "강추",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037683.1.12",
          "form": "첨엔 저도 푹 쉬는 여행 취향은 아니었는데 한번 가보니까 사람들이 왜 가는지 알겠더라고요",
          "original_form": "첨엔 저도 푹 쉬는 여행 취향은 아니었는데 한번 가보니까 사람들이 왜 가는지 알겠더라고요",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037683.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037683.1.14",
          "form": "넘 편하고 좋아여",
          "original_form": "넘 편하고 좋아여",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037683.1.15",
          "form": "그쵸 사실 한국에서도 그렇게 쉴 기회는 잘 없잖아요",
          "original_form": "그쵸 사실 한국에서도 그렇게 쉴 기회는 잘 없잖아요",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037683.1.16",
          "form": "어딜가든 바쁘게 사니까..",
          "original_form": "어딜가든 바쁘게 사니까..",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037683.1.17",
          "form": "배낭여행은 힘들구여",
          "original_form": "배낭여행은 힘들구여",
          "speaker_id": "1",
