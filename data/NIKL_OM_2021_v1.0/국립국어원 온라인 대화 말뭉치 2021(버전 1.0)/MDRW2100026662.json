          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026662.1.17",
          "form": "뭐가재밌었어?",
          "original_form": "뭐가재밌었어?",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026662.1.18",
          "form": "맞아ㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026662.1.19",
          "form": "소크라테스 익스프레스!!!",
          "original_form": "소크라테스 익스프레스!!!",
