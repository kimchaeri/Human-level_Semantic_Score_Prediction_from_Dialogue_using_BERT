        },
        {
          "id": "MDRW2100027032.1.3",
          "form": "중고거래 해보셨나요",
          "original_form": "중고거래 해보셨나요",
          "speaker_id": "2",
          "time": "20210902 07:48"
        },
        {
          "id": "MDRW2100027032.1.4",
          "form": "아니요... name2은 해보셨나요?",
          "original_form": "아니요... &name2&은 해보셨나요?",
          "speaker_id": "1",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100027032.1.5",
          "form": "네 당근에서 구매해 봣어요",
          "original_form": "네 당근에서 구매해 봣어요",
          "speaker_id": "2",
          "time": "20210902 07:50"
        },
        {
          "id": "MDRW2100027032.1.6",
          "form": "어떤 물건이었나요",
          "original_form": "어떤 물건이었나요",
          "speaker_id": "1",
          "time": "20210902 07:52"
        },
        {
          "id": "MDRW2100027032.1.7",
          "form": "커블체어의자를 구매했어요\r\n반값에 ......",
          "original_form": "커블체어의자를 구매했어요\r\n반값에 ......",
          "speaker_id": "2",
          "time": "20210902 07:56"
        },
        {
          "id": "MDRW2100027032.1.8",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "1",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100027032.1.9",
          "form": "저는 정가 주고 사버려서ㅋㅋ",
          "original_form": "저는 정가 주고 사버려서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100027032.1.10",
          "form": "계속 사용할지 잘몰라서 저렴한 당근에서 삿어요",
          "original_form": "계속 사용할지 잘몰라서 저렴한 당근에서 삿어요",
          "speaker_id": "2",
          "time": "20210902 07:58"
        },
        {
          "id": "MDRW2100027032.1.11",
          "form": "그거 편하고 좋아요",
          "original_form": "그거 편하고 좋아요",
          "speaker_id": "1",
          "time": "20210902 07:59"
        },
        {
          "id": "MDRW2100027032.1.12",
          "form": "효과 많이 봤어요",
          "original_form": "효과 많이 봤어요",
          "speaker_id": "1",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100027032.1.13",
          "form": "전 큰 효과보단 넘 아퍼서 포기 했어요",
          "original_form": "전 큰 효과보단 넘 아퍼서 포기 했어요",
          "speaker_id": "2",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100027032.1.14",
          "form": "그거에 아프시면 병원 가보시는것두...",
          "original_form": "그거에 아프시면 병원 가보시는것두...",
          "speaker_id": "1",
          "time": "20210902 08:02"
        },
        {
          "id": "MDRW2100027032.1.15",
          "form": "아 그건 생각 못해봣네요",
          "original_form": "아 그건 생각 못해봣네요",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027032.1.16",
          "form": "그거 때문에 병원 가보신 분이 결과가 안좋았다고 하더라구요",
