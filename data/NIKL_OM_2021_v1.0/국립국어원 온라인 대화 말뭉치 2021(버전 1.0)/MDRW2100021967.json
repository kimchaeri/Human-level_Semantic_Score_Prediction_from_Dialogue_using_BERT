      "utterance": [
        {
          "id": "MDRW2100021967.1.1",
          "form": "노래 좋아하시나요?",
          "original_form": "노래 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021967.1.2",
          "form": "저는 뮤지컬 노래 좋아하는 편이에요",
          "original_form": "저는 뮤지컬 노래 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021967.1.3",
          "form": "아니면 2000년대 노래들!",
          "original_form": "아니면 2000년대 노래들!",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021967.1.4",
          "form": "제일 좋아하시는 노래는 어떤거예요??",
          "original_form": "제일 좋아하시는 노래는 어떤거예요??",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021967.1.5",
          "form": "음 가요중에는 디셈버 노래 좋아했어요!",
          "original_form": "음 가요중에는 디셈버 노래 좋아했어요!",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021967.1.6",
          "form": "최근엔 가인 카니발!",
          "original_form": "최근엔 가인 카니발!",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021967.1.7",
          "form": "아 카니발! 그거 신나죠",
          "original_form": "아 카니발! 그거 신나죠",
          "speaker_id": "1",
          "time": "20210829 08:02"
        },
        {
          "id": "MDRW2100021967.1.8",
          "form": "자주 들었던 기억이 나네요",
          "original_form": "자주 들었던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210829 08:02"
        },
        {
          "id": "MDRW2100021967.1.9",
          "form": "맞아요 죽음을 다룬 노래를 그렇게 만들 수 있다는 것도 대단한 것 같아요",
          "original_form": "맞아요 죽음을 다룬 노래를 그렇게 만들 수 있다는 것도 대단한 것 같아요",
          "speaker_id": "2",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100021967.1.10",
          "form": "맞아요 처음에 들었을때는 그냥 축제인줄 알았어요",
          "original_form": "맞아요 처음에 들었을때는 그냥 축제인줄 알았어요",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100021967.1.11",
          "form": "그 코코 영화도 생각나서 좋았어요",
          "original_form": "그 코코 영화도 생각나서 좋았어요",
          "speaker_id": "2",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100021967.1.12",
          "form": "오.. 그렇네요",
          "original_form": "오.. 그렇네요",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100021967.1.13",
          "form": "들으니 느낌이 나네요",
          "original_form": "들으니 느낌이 나네요",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100021967.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100021967.1.15",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100021967.1.16",
          "form": "저도 리스트에 넣어야겠어요ㅋㅋ",
          "original_form": "저도 리스트에 넣어야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100021967.1.17",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100021967.1.18",
          "form": "또 좋은 노래 뭐가 있을까요",
          "original_form": "또 좋은 노래 뭐가 있을까요",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100021967.1.19",
          "form": "여름엔 싹쓰리 노래 많이 들었어요!",
          "original_form": "여름엔 싹쓰리 노래 많이 들었어요!",
          "speaker_id": "1",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100021967.1.20",
          "form": "아 너무좋죠ㅠㅠ",
          "original_form": "아 너무좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100021967.1.21",
          "form": "지금 여기바닷ㄴ가ㅠㅠ",
          "original_form": "지금 여기바닷ㄴ가ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100021967.1.22",
          "form": "맞아요~  여름=싹쓰리 이 공식이 되버렸어요",
          "original_form": "맞아요~  여름=싹쓰리 이 공식이 되버렸어요",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100021967.1.23",
          "form": "맞아요 어떻게 그렇게 기획을 했는지",
          "original_form": "맞아요 어떻게 그렇게 기획을 했는지",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100021967.1.24",
          "form": "너무돟아요",
          "original_form": "너무돟아요",
          "speaker_id": "2",
          "time": "20210829 17:53"
        }
      ]
