      "utterance": [
        {
          "id": "MDRW2100021063.1.1",
          "form": "골프좋아해?",
          "original_form": "골프좋아해?",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100021063.1.2",
          "form": "요즘 대세가 골프잔아",
          "original_form": "요즘 대세가 골프잔아",
          "speaker_id": "2",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100021063.1.3",
          "form": "요즘 tv에서 자주나오니 더 관심생겨",
          "original_form": "요즘 tv에서 자주나오니 더 관심생겨",
          "speaker_id": "1",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100021063.1.4",
          "form": "프로그램중 어떤거봐?",
          "original_form": "프로그램중 어떤거봐?",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021063.1.5",
          "form": "조금씩은 다본것같아",
          "original_form": "조금씩은 다본것같아",
          "speaker_id": "2",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021063.1.6",
          "form": "박세리 나오는 프로랑 이경규 나오는 프로 재밌더라",
          "original_form": "박세리 나오는 프로랑 이경규 나오는 프로 재밌더라",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021063.1.7",
          "form": "세레머니를 많이 봤어",
          "original_form": "세레머니를 많이 봤어",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021063.1.8",
          "form": "다시 시작해보고 싶긴도 해",
          "original_form": "다시 시작해보고 싶긴도 해",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021063.1.9",
          "form": "나도 할생각은 있어",
          "original_form": "나도 할생각은 있어",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021063.1.10",
          "form": "허리아플까 겁나기도 하고",
          "original_form": "허리아플까 겁나기도 하고",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021063.1.11",
          "form": "손도 아프고 허리도 아파 걱정이지",
          "original_form": "손도 아프고 허리도 아파 걱정이지",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021063.1.12",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021063.1.13",
          "form": "배구 요즘 재밌더라",
          "original_form": "배구 요즘 재밌더라",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021063.1.14",
          "form": "올림픽 후 배구선수 관심생겨",
          "original_form": "올림픽 후 배구선수 관심생겨",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021063.1.15",
          "form": "여자배구 재밌게봤엇는데",
          "original_form": "여자배구 재밌게봤엇는데",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021063.1.16",
          "form": "김연경선수는 식빵언니로 파리바게트 모델 된다더라",
          "original_form": "김연경선수는 식빵언니로 파리바게트 모델 된다더라",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021063.1.17",
          "form": "김연경이 없어서 다시 흥미가 안가더라",
