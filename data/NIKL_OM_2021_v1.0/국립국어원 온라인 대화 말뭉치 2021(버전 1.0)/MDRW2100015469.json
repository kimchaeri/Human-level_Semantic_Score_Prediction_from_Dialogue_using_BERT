      "utterance": [
        {
          "id": "MDRW2100015469.1.1",
          "form": "퇴근하면",
          "original_form": "퇴근하면",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015469.1.2",
          "form": "거의 잘때까지 혼자 다 돌보는거 같던데요",
          "original_form": "거의 잘때까지 혼자 다 돌보는거 같던데요",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015469.1.3",
          "form": "지금은 코로나땜에",
          "original_form": "지금은 코로나땜에",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015469.1.4",
          "form": "에고 너무 하네",
          "original_form": "에고 너무 하네",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015469.1.5",
          "form": "저도 그래요ㅠ",
          "original_form": "저도 그래요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015469.1.6",
          "form": "재택근무해서 거의 독박육아수준이던데요",
          "original_form": "재택근무해서 거의 독박육아수준이던데요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015469.1.7",
          "form": "너무하네요",
          "original_form": "너무하네요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015469.1.8",
          "form": "그런데도 그렇게 하면ㅊ안되죠",
          "original_form": "그런데도 그렇게 하면ㅊ안되죠",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015469.1.9",
          "form": "일도하지 육아도 신경쓰지",
          "original_form": "일도하지 육아도 신경쓰지",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015469.1.10",
          "form": "그러면 서로 사이가 좋긴해요?",
          "original_form": "그러면 서로 사이가 좋긴해요?",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015469.1.11",
          "form": "그래서물어봤는데",
          "original_form": "그래서물어봤는데",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015469.1.12",
          "form": "안좋은거 같아요",
          "original_form": "안좋은거 같아요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015469.1.13",
          "form": "그렇죠ㅠ",
          "original_form": "그렇죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015469.1.14",
          "form": "저도 그랬어요ㅠ 결정하기전까지도 힘들었죠ㅠ",
          "original_form": "저도 그랬어요ㅠ 결정하기전까지도 힘들었죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015469.1.15",
          "form": "근데 막상결정하면 기분이 좋아져요ㅎㅎ",
          "original_form": "근데 막상결정하면 기분이 좋아져요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015469.1.16",
          "form": "오히려 아내가 더 힘들어하죠",
          "original_form": "오히려 아내가 더 힘들어하죠",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015469.1.17",
          "form": "헌터님이 오래 참으신거죠",
          "original_form": "헌터님이 오래 참으신거죠",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015469.1.18",
          "form": "오래도 아니에요ㅎㅎ 3년 조금 넘는데요뭐ㄹㅎ",
          "original_form": "오래도 아니에요ㅎㅎ 3년 조금 넘는데요뭐ㄹㅎ",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015469.1.19",
          "form": "집안일도 많이도와줬두요",
          "original_form": "집안일도 많이도와줬두요",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015469.1.20",
          "form": "정말 가정생활 충실했는데 결과가 이렇네요ㅎㅎ",
          "original_form": "정말 가정생활 충실했는데 결과가 이렇네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015469.1.21",
          "form": "제친구 애가 5살",
          "original_form": "제친구 애가 5살",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015469.1.22",
          "form": "지금까지 참고있긴 합니다",
          "original_form": "지금까지 참고있긴 합니다",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015469.1.23",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015469.1.24",
          "form": "계속참으실 예정이신가보네요ㅍ",
          "original_form": "계속참으실 예정이신가보네요ㅍ",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015469.1.25",
          "form": "너무 힘들겟네요ㅠ",
          "original_form": "너무 힘들겟네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015469.1.26",
          "form": "계속이 될까요?",
          "original_form": "계속이 될까요?",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015469.1.27",
          "form": "참 .... 친구가 불쌍해보여서",
          "original_form": "참 .... 친구가 불쌍해보여서",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015469.1.28",
          "form": "맘이 좀 그래요",
          "original_form": "맘이 좀 그래요",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015469.1.29",
          "form": "근데 조언해주시기 좀 그렇잖아요ㅎ",
          "original_form": "근데 조언해주시기 좀 그렇잖아요ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015469.1.30",
          "form": "시간이 해결될꺼애요ㅎ",
          "original_form": "시간이 해결될꺼애요ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015469.1.31",
          "form": "어쩔수없이 선택을 해야하는 시기가ㅎ",
          "original_form": "어쩔수없이 선택을 해야하는 시기가ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015469.1.32",
          "form": "그냥 흘러가는대로 냅두시는게 좋아요ㅎ",
          "original_form": "그냥 흘러가는대로 냅두시는게 좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015469.1.33",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015469.1.34",
          "form": "친구 부부사이에 끼어들기도 그렇고 ㅋㅋ",
          "original_form": "친구 부부사이에 끼어들기도 그렇고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015469.1.35",
          "form": "ㅎㅎ 참아야죠ㅎㅎ",
          "original_form": "ㅎㅎ 참아야죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015469.1.36",
          "form": "저도 맨날 참는데ㅎㅎ 서로 각자 존중해주기위해서ㅎㅎ",
          "original_form": "저도 맨날 참는데ㅎㅎ 서로 각자 존중해주기위해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015469.1.37",
          "form": "화병나겠어요",
          "original_form": "화병나겠어요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015469.1.38",
          "form": "참기만 해야하니",
          "original_form": "참기만 해야하니",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015469.1.39",
          "form": "그래서 이젠 마음이편해요ㅎㅎ",
          "original_form": "그래서 이젠 마음이편해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:35"
        }
      ]
    }
