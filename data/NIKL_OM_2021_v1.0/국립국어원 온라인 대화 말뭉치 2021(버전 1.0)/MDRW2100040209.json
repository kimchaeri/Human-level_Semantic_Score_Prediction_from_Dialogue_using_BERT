      "utterance": [
        {
          "id": "MDRW2100040209.1.1",
          "form": "어떤 게임 하시나요?",
          "original_form": "어떤 게임 하시나요?",
          "speaker_id": "1",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100040209.1.2",
          "form": "저는 요즘 게임하는 것이 없어요",
          "original_form": "저는 요즘 게임하는 것이 없어요",
          "speaker_id": "2",
          "time": "20210904 19:04"
        },
        {
          "id": "MDRW2100040209.1.3",
          "form": "전에 포켓몬고 열심히 했어요ㅎㅎ",
          "original_form": "전에 포켓몬고 열심히 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 19:04"
        },
        {
          "id": "MDRW2100040209.1.4",
          "form": "저도 포켓몬고 했었는데 한두달 하다 말았던 거 같아요 ㅎㅎ",
          "original_form": "저도 포켓몬고 했었는데 한두달 하다 말았던 거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100040209.1.5",
          "form": "저는 주변사람들이 열심히 해서 한 2년은 했어요",
          "original_form": "저는 주변사람들이 열심히 해서 한 2년은 했어요",
          "speaker_id": "2",
          "time": "20210904 19:06"
        },
        {
          "id": "MDRW2100040209.1.6",
          "form": "꽤 오래 하셨네요",
          "original_form": "꽤 오래 하셨네요",
          "speaker_id": "1",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100040209.1.7",
          "form": "네ㅎㅎ 그거 하느라 정말 많이 걸어다녔어요",
          "original_form": "네ㅎㅎ 그거 하느라 정말 많이 걸어다녔어요",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100040209.1.8",
          "form": "전 며칠전에 프렌즈샷 설치해서 좀 하고 있는데 재밌더라구요 ㅎㅎ",
          "original_form": "전 며칠전에 프렌즈샷 설치해서 좀 하고 있는데 재밌더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100040209.1.9",
          "form": "게임하면서 건강해졌죠ㅎㅎ",
          "original_form": "게임하면서 건강해졌죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100040209.1.10",
          "form": "프렌즈샷이요?",
          "original_form": "프렌즈샷이요?",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100040209.1.11",
          "form": "어떤 게임인가요?",
          "original_form": "어떤 게임인가요?",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100040209.1.12",
          "form": "ㅋㅋ실제로 엄청 걸으셨군요\r\n골프게임이에요",
          "original_form": "ㅋㅋ실제로 엄청 걸으셨군요\r\n골프게임이에요",
          "speaker_id": "1",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100040209.1.13",
          "form": "오 골프게임",
          "original_form": "오 골프게임",
          "speaker_id": "2",
          "time": "20210904 21:34"
        },
        {
          "id": "MDRW2100040209.1.14",
          "form": "재미있을 것 같아요!!",
          "original_form": "재미있을 것 같아요!!",
          "speaker_id": "2",
          "time": "20210904 21:34"
        },
        {
          "id": "MDRW2100040209.1.15",
          "form": "은근 어렵긴한데 또 재밌더라구요 ㅋㅋ",
          "original_form": "은근 어렵긴한데 또 재밌더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100040209.1.16",
          "form": "조금 어려운 걸 깨나갈 때의 희열이 있죠",
          "original_form": "조금 어려운 걸 깨나갈 때의 희열이 있죠",
          "speaker_id": "2",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100040209.1.17",
          "form": "ㅋㅋ 코스도 리얼하고 해볼만 한 것 같아요",
          "original_form": "ㅋㅋ 코스도 리얼하고 해볼만 한 것 같아요",
          "speaker_id": "1",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100040209.1.18",
          "form": "한번 찾아봐야겠네요",
