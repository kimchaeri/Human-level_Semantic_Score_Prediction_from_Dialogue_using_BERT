        },
        {
          "id": "MDRW2100014553.1.27",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014553.1.28",
          "form": "거저네요",
          "original_form": "거저네요",
