          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014553.1.9",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014553.1.10",
          "form": "어찌 그리싸게",
          "original_form": "어찌 그리싸게",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014553.1.11",
          "form": "요즘 폰성지인가 거기서 싸게 팔더라구요",
          "original_form": "요즘 폰성지인가 거기서 싸게 팔더라구요",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014553.1.12",
          "form": "헉 저만 바보네요",
          "original_form": "헉 저만 바보네요",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014553.1.13",
          "form": "성지를",
          "original_form": "성지를",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014553.1.14",
          "form": "첨엔 최신꺼 중고사려다가 12만원이라길래",
          "original_form": "첨엔 최신꺼 중고사려다가 12만원이라길래",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014553.1.15",
          "form": "얘기는?들었어요 ㅜㅜ",
          "original_form": "얘기는?들었어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014553.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014553.1.17",
          "form": "그냥 이거주세요 했어요 ㅎ",
          "original_form": "그냥 이거주세요 했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014553.1.18",
          "form": "새거사야죠 ㅋㅋ",
          "original_form": "새거사야죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014553.1.19",
          "form": "잘했어요 중고후회해요",
          "original_form": "잘했어요 중고후회해요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014553.1.20",
          "form": "85천원짜리 요금제 6갤쓰는조건이요",
          "original_form": "85천원짜리 요금제 6갤쓰는조건이요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014553.1.21",
          "form": "원래 69000원인가 쓰고있어서 ㅎ",
          "original_form": "원래 69000원인가 쓰고있어서 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014553.1.22",
          "form": "오케이했죠 ㅎ",
          "original_form": "오케이했죠 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014553.1.23",
          "form": "신랑도 의심하고 머다른거 더 있는거아니냐고",
          "original_form": "신랑도 의심하고 머다른거 더 있는거아니냐고",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014553.1.24",
          "form": "와조건 파격인데요",
          "original_form": "와조건 파격인데요",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014553.1.25",
          "form": "그래도 싸요",
