      "utterance": [
        {
          "id": "MDRW2100022185.1.1",
          "form": "요즘 주로 식사는 어떻게 하십니까",
          "original_form": "요즘 주로 식사는 어떻게 하십니까",
          "speaker_id": "1",
          "time": "20210829 10:39"
        },
        {
          "id": "MDRW2100022185.1.2",
          "form": "슥배송으로 식자재를 주문하고",
          "original_form": "슥배송으로 식자재를 주문하고",
          "speaker_id": "2",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100022185.1.3",
          "form": "간단하게 해먹네요",
          "original_form": "간단하게 해먹네요",
          "speaker_id": "2",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100022185.1.4",
          "form": "코로나로 인하여 마트나 시장은 지양합니다",
          "original_form": "코로나로 인하여 마트나 시장은 지양합니다",
          "speaker_id": "2",
          "time": "20210829 10:40"
        },
        {
          "id": "MDRW2100022185.1.5",
          "form": "오~ 배운 사람",
          "original_form": "오~ 배운 사람",
          "speaker_id": "1",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100022185.1.6",
          "form": "아침은 뭘 드셨나요",
          "original_form": "아침은 뭘 드셨나요",
          "speaker_id": "1",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100022185.1.7",
          "form": "아파트에 커뮤니티가 있어서 해산물이나 육고기를 주문하면 바로 집앞까지 가져다주니까 나갈일이 없습니다",
          "original_form": "아파트에 커뮤니티가 있어서 해산물이나 육고기를 주문하면 바로 집앞까지 가져다주니까 나갈일이 없습니다",
          "speaker_id": "2",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100022185.1.8",
          "form": "아침에는 크로플 구워 먹엇습니다",
          "original_form": "아침에는 크로플 구워 먹엇습니다",
          "speaker_id": "2",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100022185.1.9",
          "form": "저흰 떡만두국을 먹고 왔어요",
          "original_form": "저흰 떡만두국을 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100022185.1.10",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100022185.1.11",
          "form": "저 떡만둣국 너무 좋아합니다",
          "original_form": "저 떡만둣국 너무 좋아합니다",
          "speaker_id": "2",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100022185.1.12",
          "form": "미국인이신가요",
          "original_form": "미국인이신가요",
          "speaker_id": "1",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100022185.1.13",
          "form": "미국인 아닙니다",
          "original_form": "미국인 아닙니다",
          "speaker_id": "2",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100022185.1.14",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "2",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100022185.1.15",
          "form": "왜 미국인이라 생각하셧나요?",
          "original_form": "왜 미국인이라 생각하셧나요?",
          "speaker_id": "2",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100022185.1.16",
          "form": "크로플 맛있겠군요",
          "original_form": "크로플 맛있겠군요",
          "speaker_id": "1",
          "time": "20210829 10:42"
        },
        {
          "id": "MDRW2100022185.1.17",
          "form": "크로플은 메이플시럽이 잘 어울리더라구요",
          "original_form": "크로플은 메이플시럽이 잘 어울리더라구요",
          "speaker_id": "2",
          "time": "20210829 10:43"
        },
        {
          "id": "MDRW2100022185.1.18",
          "form": "여긴 아무 말 대잔치 입니다",
          "original_form": "여긴 아무 말 대잔치 입니다",
          "speaker_id": "1",
          "time": "20210829 10:43"
        },
        {
          "id": "MDRW2100022185.1.19",
          "form": "커피는 드시나요?",
          "original_form": "커피는 드시나요?",
          "speaker_id": "2",
          "time": "20210829 10:43"
        },
        {
          "id": "MDRW2100022185.1.20",
          "form": "오늘 처음이라 미숙합니다 이해 부탁드려요",
          "original_form": "오늘 처음이라 미숙합니다 이해 부탁드려요",
          "speaker_id": "2",
          "time": "20210829 10:43"
        },
        {
          "id": "MDRW2100022185.1.21",
          "form": "으른커피 한사발 하고 있습니다",
          "original_form": "으른커피 한사발 하고 있습니다",
          "speaker_id": "1",
          "time": "20210829 10:43"
        },
        {
          "id": "MDRW2100022185.1.22",
          "form": "으른커피가 맥심인가요?",
          "original_form": "으른커피가 맥심인가요?",
          "speaker_id": "2",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100022185.1.23",
          "form": "봄봄 어른 커피입니다ㅋㅋ",
          "original_form": "봄봄 어른 커피입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100022185.1.24",
          "form": "아내분께",
          "original_form": "아내분께",
          "speaker_id": "1",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100022185.1.25",
          "form": "아 그런게 있군요 ㅎㅎ",
          "original_form": "아 그런게 있군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100022185.1.26",
          "form": "크로플 두어장 좀 구워달라고 주문좀 넣어주세요~",
          "original_form": "크로플 두어장 좀 구워달라고 주문좀 넣어주세요~",
          "speaker_id": "1",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100022185.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100022185.1.28",
          "form": "네 ㅎㅎㅎ제가 해드릴게요",
          "original_form": "네 ㅎㅎㅎ제가 해드릴게요",
          "speaker_id": "2",
