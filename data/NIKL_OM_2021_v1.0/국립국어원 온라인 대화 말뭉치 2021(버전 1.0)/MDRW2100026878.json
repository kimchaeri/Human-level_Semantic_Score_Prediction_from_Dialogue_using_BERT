        {
          "id": "MDRW2100026878.1.1",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026878.1.2",
          "form": "결혼임다",
          "original_form": "결혼임다",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026878.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026878.1.4",
          "form": "비혼입니다",
          "original_form": "비혼입니다",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026878.1.5",
          "form": "결혼하셨나요",
          "original_form": "결혼하셨나요",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026878.1.6",
          "form": "왜 비혼이에요? 이유가 있나요?",
          "original_form": "왜 비혼이에요? 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026878.1.7",
          "form": "네 저는 옆방에 동거인이 있어요",
          "original_form": "네 저는 옆방에 동거인이 있어요",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026878.1.8",
          "form": "독박 육아 하기 싫어서요",
          "original_form": "독박 육아 하기 싫어서요",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026878.1.9",
          "form": "한국에선 결혼하면 여자는손해같아요",
          "original_form": "한국에선 결혼하면 여자는손해같아요",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026878.1.10",
          "form": "저는 결혼했지만 아이는 없습니다",
          "original_form": "저는 결혼했지만 아이는 없습니다",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026878.1.11",
          "form": "딩크이신가요",
          "original_form": "딩크이신가요",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026878.1.12",
          "form": "네 한국은 가족과 가족의 만남이지요",
          "original_form": "네 한국은 가족과 가족의 만남이지요",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026878.1.13",
          "form": "네 자녀계획없습니다",
          "original_form": "네 자녀계획없습니다",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026878.1.14",
          "form": "그래도 연애는 좋게보고있어요",
          "original_form": "그래도 연애는 좋게보고있어요",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026878.1.15",
          "form": "서로 의견맞으면 딩크도 많지요",
          "original_form": "서로 의견맞으면 딩크도 많지요",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026878.1.16",
          "form": "연매는하고있습니다",
          "original_form": "연매는하고있습니다",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026878.1.17",
          "form": "네 활력을 주죠",
          "original_form": "네 활력을 주죠",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026878.1.18",
          "form": "딩크도 좋지요",
          "original_form": "딩크도 좋지요",
