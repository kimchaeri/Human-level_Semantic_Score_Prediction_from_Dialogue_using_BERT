      "utterance": [
        {
          "id": "MDRW2100017972.1.1",
          "form": "게임 좋아하셔서 게이밍 노트북이랑",
          "original_form": "게임 좋아하셔서 게이밍 노트북이랑",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.2",
          "form": "게이밍 용품 자주 사시는 군요",
          "original_form": "게이밍 용품 자주 사시는 군요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.3",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.4",
          "form": "나중에 본업가지면",
          "original_form": "나중에 본업가지면",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.5",
          "form": "자취해서",
          "original_form": "자취해서",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.6",
          "form": "컴퓨터사고 마우스랑 키보드도",
          "original_form": "컴퓨터사고 마우스랑 키보드도",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.7",
          "form": "앜ㅋㅋㅋㅋ 멋진 꿈이네요",
          "original_form": "앜ㅋㅋㅋㅋ 멋진 꿈이네요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.8",
          "form": "다 게이밍으로 사는게 버킷리스트라서",
          "original_form": "다 게이밍으로 사는게 버킷리스트라서",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.9",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017972.1.10",
          "form": "나만의 취미방 느낌 ㅎㅎ",
          "original_form": "나만의 취미방 느낌 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017972.1.11",
          "form": "마우스는 진짜 몇십 주더라도 FPS 전용은 하나 살까해요 나중에",
          "original_form": "마우스는 진짜 몇십 주더라도 FPS 전용은 하나 살까해요 나중에",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017972.1.12",
          "form": "오 FPS가 뭐에요?",
          "original_form": "오 FPS가 뭐에요?",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017972.1.13",
          "form": "총게임이죠!",
          "original_form": "총게임이죠!",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017972.1.14",
          "form": "마우스 비싼 건 진짜 어마어마하더라구요",
          "original_form": "마우스 비싼 건 진짜 어마어마하더라구요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017972.1.15",
          "form": "서든어택 같은 게임이 FPS에요!",
          "original_form": "서든어택 같은 게임이 FPS에요!",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017972.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017972.1.17",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017972.1.18",
          "form": "그럼 스팀 하시나요?",
          "original_form": "그럼 스팀 하시나요?",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017972.1.19",
          "form": "네 하죵",
          "original_form": "네 하죵",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017972.1.20",
          "form": "글쿤",
          "original_form": "글쿤",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017972.1.21",
          "form": "스팀 세일 자주 구매하세요?",
          "original_form": "스팀 세일 자주 구매하세요?",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017972.1.22",
          "form": "필요한것만 사요 거의 ㅋㅋㅋ",
          "original_form": "필요한것만 사요 거의 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017972.1.23",
          "form": "제가 지금 컴터도 없는 실정이라",
          "original_form": "제가 지금 컴터도 없는 실정이라",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017972.1.24",
          "form": "할려면 피방이나 여동생꺼 빌려서 해야되는지라",
          "original_form": "할려면 피방이나 여동생꺼 빌려서 해야되는지라",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017972.1.25",
          "form": "그래서 노트북 살려고 하는거에용!",
          "original_form": "그래서 노트북 살려고 하는거에용!",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017972.1.26",
          "form": "앜ㅋㅋ 여동생꺼",
          "original_form": "앜ㅋㅋ 여동생꺼",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017972.1.27",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017972.1.28",
          "form": "하나 필요하겠어요",
          "original_form": "하나 필요하겠어요",
          "speaker_id": "1",
          "time": "20210826 21:59"
        }
      ]
    }
