        {
          "id": "MDRW2100011978.1.1",
          "form": "겜 좋아하시나요",
          "original_form": "겜 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 13:01"
        },
        {
          "id": "MDRW2100011978.1.2",
          "form": "간단한 게임좋아해요",
          "original_form": "간단한 게임좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100011978.1.3",
          "form": "즐겨하는 게임 있으시나요,",
          "original_form": "즐겨하는 게임 있으시나요,",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100011978.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100011978.1.5",
          "form": "피파온라인이요",
          "original_form": "피파온라인이요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100011978.1.6",
          "form": "아 그러시군요ㅋㅋㅋ",
          "original_form": "아 그러시군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100011978.1.7",
          "form": "겜 하시는거있나여",
          "original_form": "겜 하시는거있나여",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100011978.1.8",
          "form": "저는 카카오게임 간단한것만 해요ㅋ",
          "original_form": "저는 카카오게임 간단한것만 해요ㅋ",
          "speaker_id": "2",
          "time": "20210826 07:27"
        },
        {
          "id": "MDRW2100011978.1.9",
          "form": "모바일겜인가요?",
          "original_form": "모바일겜인가요?",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100011978.1.10",
          "form": "네넨 모바일게임이요.ㅋㅋ",
          "original_form": "네넨 모바일게임이요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100011978.1.11",
          "form": "오 한번해봐야겠어요",
          "original_form": "오 한번해봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100011978.1.12",
          "form": "아아 ㅋㅋㅋ 간단해서 쉬운거예요. 아이랑 같이 하려구 ㅋㅋ 그냥 다운받은거라서요.",
          "original_form": "아아 ㅋㅋㅋ 간단해서 쉬운거예요. 아이랑 같이 하려구 ㅋㅋ 그냥 다운받은거라서요.",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100011978.1.13",
          "form": "아이랑 같이하면 더 재밌겠어요",
          "original_form": "아이랑 같이하면 더 재밌겠어요",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100011978.1.14",
          "form": "같이 해야되는데 ㅋㅋㅋ 자꾸 먼저 더 하려고해요 ㅋㅋ",
          "original_form": "같이 해야되는데 ㅋㅋㅋ 자꾸 먼저 더 하려고해요 ㅋㅋ",
          "speaker_id": "2",
