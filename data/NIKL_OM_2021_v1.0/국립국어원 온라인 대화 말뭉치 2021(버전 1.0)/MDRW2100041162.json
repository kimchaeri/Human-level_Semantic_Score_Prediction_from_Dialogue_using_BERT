      "utterance": [
        {
          "id": "MDRW2100041162.1.1",
          "form": "베라 여름한정메뉴 먹어봤어요?",
          "original_form": "베라 여름한정메뉴 먹어봤어요?",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041162.1.2",
          "form": "어뇨",
          "original_form": "어뇨",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.3",
          "form": "아이스폴라베어",
          "original_form": "아이스폴라베어",
          "speaker_id": "2",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.4",
          "form": "저 여름엔 그거만먹어요",
          "original_form": "저 여름엔 그거만먹어요",
          "speaker_id": "2",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.5",
          "form": "아 먹은듯",
          "original_form": "아 먹은듯",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.6",
          "form": "에에에",
          "original_form": "에에에",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.7",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.8",
          "form": "ㅋㅋㅋ민초맛에 슈팅스타그 팡팡쏘는초콜렛 그거들어있어요",
          "original_form": "ㅋㅋㅋ민초맛에 슈팅스타그 팡팡쏘는초콜렛 그거들어있어요",
          "speaker_id": "2",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.9",
          "form": "바닐라초콜릿칩같은거",
          "original_form": "바닐라초콜릿칩같은거",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.10",
          "form": "아님 ?",
          "original_form": "아님 ?",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.11",
          "form": "하얀칩",
          "original_form": "하얀칩",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041162.1.12",
          "form": "칩은 아닌뎅!!!!",
          "original_form": "칩은 아닌뎅!!!!",
          "speaker_id": "2",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041162.1.13",
          "form": "음.. 다른건가",
          "original_form": "음.. 다른건가",
          "speaker_id": "1",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041162.1.14",
          "form": "생각보다 달아서 후회한거잇는데",
          "original_form": "생각보다 달아서 후회한거잇는데",
          "speaker_id": "1",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041162.1.15",
          "form": "하얀칩은 민초쿠키앤크림인가 그거",
          "original_form": "하얀칩은 민초쿠키앤크림인가 그거",
          "speaker_id": "2",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041162.1.16",
          "form": "화이트초콜릿가튼거",
          "original_form": "화이트초콜릿가튼거",
          "speaker_id": "1",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041162.1.17",
          "form": "마자요 그거들은건 다른맛 \n저도 그건한번먹고 안먹엇어요",
          "original_form": "마자요 그거들은건 다른맛 \n저도 그건한번먹고 안먹엇어요",
          "speaker_id": "2",
          "time": "20210905 18:58"
        },
        {
          "id": "MDRW2100041162.1.18",
          "form": "엥 아닌가",
          "original_form": "엥 아닌가",
          "speaker_id": "2",
          "time": "20210905 18:58"
        },
        {
          "id": "MDRW2100041162.1.19",
          "form": "그거들은거먹엇는데 실망",
          "original_form": "그거들은거먹엇는데 실망",
          "speaker_id": "1",
          "time": "20210905 18:58"
        },
        {
          "id": "MDRW2100041162.1.20",
          "form": "하늘색ㅁ베이스",
          "original_form": "하늘색ㅁ베이스",
          "speaker_id": "1",
          "time": "20210905 18:59"
        },
        {
          "id": "MDRW2100041162.1.21",
          "form": "흠 그게그건지 모르겠네요 전 슈팅스타 파핑이 더 기억에남아서",
          "original_form": "흠 그게그건지 모르겠네요 전 슈팅스타 파핑이 더 기억에남아서",
          "speaker_id": "2",
          "time": "20210905 19:00"
        },
        {
          "id": "MDRW2100041162.1.22",
          "form": "후잉.ㅜㅜ 이상하게 그게안느껴지고 ㅠㅠ초콜릿만 ㅜㅜㅠ",
          "original_form": "후잉.ㅜㅜ 이상하게 그게안느껴지고 ㅠㅠ초콜릿만 ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100041162.1.23",
          "form": "휴낼 월욜이네요 회사밥드심?",
          "original_form": "휴낼 월욜이네요 회사밥드심?",
          "speaker_id": "1",
          "time": "20210905 22:38"
        }
      ]
