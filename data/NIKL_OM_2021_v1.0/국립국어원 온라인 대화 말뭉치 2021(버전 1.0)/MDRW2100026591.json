      "utterance": [
        {
          "id": "MDRW2100026591.1.1",
          "form": "안녕하세요. 여행 좋아하세요?",
          "original_form": "안녕하세요. 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026591.1.2",
          "form": "안녕하세요~ 여행 좋죠ㅠㅠ 코로나 때문에 한참 못갔네요",
          "original_form": "안녕하세요~ 여행 좋죠ㅠㅠ 코로나 때문에 한참 못갔네요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026591.1.3",
          "form": "국내 여행도 못가고 계신가요",
          "original_form": "국내 여행도 못가고 계신가요",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026591.1.4",
          "form": "해외여행을 더 좋아하기도 하고",
          "original_form": "해외여행을 더 좋아하기도 하고",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026591.1.5",
          "form": "어디 가려고 하면 더 심해지는 바람에 안 갔네요 ㅎㅎ",
          "original_form": "어디 가려고 하면 더 심해지는 바람에 안 갔네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026591.1.6",
          "form": "그렇군요. 해외는 어디 가보셨나요",
          "original_form": "그렇군요. 해외는 어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026591.1.7",
          "form": "주로 일본을 자주 갔네요 ㅎㅎ 해외랑 국내 중 어디를 더 좋아하시나요?",
          "original_form": "주로 일본을 자주 갔네요 ㅎㅎ 해외랑 국내 중 어디를 더 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026591.1.8",
          "form": "아무래도 해외가 더 재밌긴한데",
          "original_form": "아무래도 해외가 더 재밌긴한데",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026591.1.9",
          "form": "국내여행도 좋아해요",
          "original_form": "국내여행도 좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026591.1.10",
          "form": "이번 주말엔 제주도 다녀왔구요 ㅎㅎ",
          "original_form": "이번 주말엔 제주도 다녀왔구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026591.1.11",
          "form": "오 제주도 좋으셨겠어요",
          "original_form": "오 제주도 좋으셨겠어요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026591.1.12",
          "form": "전 몇년전에 가족여행으로 제주도 가봤었는데 좋았어요",
          "original_form": "전 몇년전에 가족여행으로 제주도 가봤었는데 좋았어요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026591.1.13",
          "form": "네네 좋더라구요. 제주도 어느 쪽 가샸나요",
          "original_form": "네네 좋더라구요. 제주도 어느 쪽 가샸나요",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026591.1.14",
          "form": "어느 쪽이더라... 몇년 전이라 자세힌 기억이 안나는데 말타본 건 기억나네요 ㅋㅋ",
          "original_form": "어느 쪽이더라... 몇년 전이라 자세힌 기억이 안나는데 말타본 건 기억나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026591.1.15",
          "form": "와ㅋㅋㅋ 말이라니",
          "original_form": "와ㅋㅋㅋ 말이라니",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026591.1.16",
          "form": "위험하진 않앗나요",
          "original_form": "위험하진 않앗나요",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026591.1.17",
          "form": "도와주시는 분이 있어서 괜찮았던 것 같아요",
          "original_form": "도와주시는 분이 있어서 괜찮았던 것 같아요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026591.1.18",
          "form": "그런데 오히려 지금 다시 타면 무서울 것 같기도 해요 ㅎㅎ",
          "original_form": "그런데 오히려 지금 다시 타면 무서울 것 같기도 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026591.1.19",
          "form": "여행가면 기념품 챙기는 편인가요",
          "original_form": "여행가면 기념품 챙기는 편인가요",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026591.1.20",
          "form": "챙겨요~ 사실 제것보다 주변 선물이 ㅎㅎ 여행 마지막 날에 선물로 돌릴 기념품 사느라 내내 돌아다닌 적도 있네요",
          "original_form": "챙겨요~ 사실 제것보다 주변 선물이 ㅎㅎ 여행 마지막 날에 선물로 돌릴 기념품 사느라 내내 돌아다닌 적도 있네요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        }
      ]
    }
