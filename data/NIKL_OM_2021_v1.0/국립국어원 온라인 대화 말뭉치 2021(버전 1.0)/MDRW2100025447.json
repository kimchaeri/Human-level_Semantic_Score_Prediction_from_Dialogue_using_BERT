      "utterance": [
        {
          "id": "MDRW2100025447.1.1",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "1",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025447.1.2",
          "form": "전 볼펜을 샀어요",
          "original_form": "전 볼펜을 샀어요",
          "speaker_id": "2",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025447.1.3",
          "form": "최근에 쇼핑하셨어요",
          "original_form": "최근에 쇼핑하셨어요",
          "speaker_id": "1",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025447.1.4",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025447.1.5",
          "form": "오오 어떤걸 사셨나요",
          "original_form": "오오 어떤걸 사셨나요",
          "speaker_id": "1",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025447.1.6",
          "form": "디자인이 예쁜거인가요",
          "original_form": "디자인이 예쁜거인가요",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.7",
          "form": "5색인줄알았는데...",
          "original_form": "5색인줄알았는데...",
          "speaker_id": "2",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.8",
          "form": "스누피에요",
          "original_form": "스누피에요",
          "speaker_id": "2",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.9",
          "form": "다양한 색상이군요",
          "original_form": "다양한 색상이군요",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.10",
          "form": "스누피 귀엽죠 ㅠ",
          "original_form": "스누피 귀엽죠 ㅠ",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.11",
          "form": "볼펜케이스색만다르고..",
          "original_form": "볼펜케이스색만다르고..",
          "speaker_id": "2",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.12",
          "form": "검은색이더라구요..",
          "original_form": "검은색이더라구요..",
          "speaker_id": "2",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.13",
          "form": "혹시 다",
          "original_form": "혹시 다",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.14",
          "form": "검은색?",
          "original_form": "검은색?",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.15",
          "form": "네.",
          "original_form": "네.",
          "speaker_id": "2",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.16",
          "form": "아이고 ㅠㅠ",
          "original_form": "아이고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.17",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.18",
          "form": "저주세요",
          "original_form": "저주세요",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.20",
          "form": "그럴게요!",
          "original_form": "그럴게요!",
          "speaker_id": "2",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100025447.1.21",
          "form": "전 최근에 가방을 샀어요",
          "original_form": "전 최근에 가방을 샀어요",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.22",
          "form": "스누핀가요?",
          "original_form": "스누핀가요?",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.24",
          "form": "현대몰에서 출석 이벤트해서",
          "original_form": "현대몰에서 출석 이벤트해서",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.25",
          "form": "아뇨 ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.26",
          "form": "이번달도 이벤트하나요?!",
          "original_form": "이번달도 이벤트하나요?!",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.27",
          "form": "포인트 모으고 쿠폰 뿌린거 써서",
          "original_form": "포인트 모으고 쿠폰 뿌린거 써서",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.28",
          "form": "6만원 넘는거 3500원에 득템했어요",
          "original_form": "6만원 넘는거 3500원에 득템했어요",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.29",
          "form": "이벤트는 좋더라구요ㅋㅋㅋ",
          "original_form": "이벤트는 좋더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.30",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.31",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.32",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210901 11:33"
        },
        {
          "id": "MDRW2100025447.1.33",
          "form": "그리고 샴푸도 샀어요",
          "original_form": "그리고 샴푸도 샀어요",
          "speaker_id": "1",
          "time": "20210901 11:33"
        }
      ]
