      "utterance": [
        {
          "id": "MDRW2100019137.1.1",
          "form": "온라인쇼핑 많이 하세요?",
          "original_form": "온라인쇼핑 많이 하세요?",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019137.1.2",
          "form": "네 저는 거의 오프보단 온라인으로만 사요 ㅋㅋㅋ",
          "original_form": "네 저는 거의 오프보단 온라인으로만 사요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019137.1.3",
          "form": "저도요 훨 편하죠?",
          "original_form": "저도요 훨 편하죠?",
          "speaker_id": "1",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019137.1.4",
          "form": "네 택배뜯는 재미도 있어요 설렘 ㅋㅋㅋ",
          "original_form": "네 택배뜯는 재미도 있어요 설렘 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019137.1.5",
          "form": "그쵸 세상에서 젤 반가운 띵동소리ㅋㅋ",
          "original_form": "그쵸 세상에서 젤 반가운 띵동소리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019137.1.6",
          "form": "맞아요 배달음식만큼 반갑답니다",
          "original_form": "맞아요 배달음식만큼 반갑답니다",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019137.1.7",
          "form": "옷 주로 어디서사세요?",
          "original_form": "옷 주로 어디서사세요?",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019137.1.8",
          "form": "백화점몰이나 아울렛몰이요",
          "original_form": "백화점몰이나 아울렛몰이요",
          "speaker_id": "1",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019137.1.9",
          "form": "추천하시는 백화점몰 있나요??",
          "original_form": "추천하시는 백화점몰 있나요??",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100019137.1.10",
          "form": "딱히 생각나는데가 없어요ㅜㅜ",
          "original_form": "딱히 생각나는데가 없어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019137.1.11",
          "form": "아!! 바바닷컴처럼 브랜드세일하는데도 좋아요",
          "original_form": "아!! 바바닷컴처럼 브랜드세일하는데도 좋아요",
          "speaker_id": "1",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019137.1.12",
          "form": "아그래요? 처음 들어봐요",
          "original_form": "아그래요? 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019137.1.13",
          "form": "찾아봐야겠네요",
          "original_form": "찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019137.1.14",
          "form": "네 한번 둘러보세요~~",
          "original_form": "네 한번 둘러보세요~~",
          "speaker_id": "1",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019137.1.15",
          "form": "넵 요새 시즌오프기간이라",
          "original_form": "넵 요새 시즌오프기간이라",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019137.1.16",
          "form": "많이 할인하더라구요",
          "original_form": "많이 할인하더라구요",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019137.1.17",
          "form": "오 꿀정보",
          "original_form": "오 꿀정보",
