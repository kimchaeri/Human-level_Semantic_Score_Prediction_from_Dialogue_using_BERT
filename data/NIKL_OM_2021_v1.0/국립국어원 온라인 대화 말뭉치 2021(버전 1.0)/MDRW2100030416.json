        },
        {
          "id": "MDRW2100030416.1.2",
          "form": "우리 나중에 대전 놀러가자",
          "original_form": "우리 나중에 대전 놀러가자",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030416.1.3",
          "form": "기차타고 ㅋㅋㅋ",
          "original_form": "기차타고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030416.1.4",
          "form": "내일 또 봐여~~",
          "original_form": "내일 또 봐여~~",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030416.1.5",
          "form": "조아염 캠핑도 가고 싶어오",
          "original_form": "조아염 캠핑도 가고 싶어오",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030416.1.6",
          "form": "내일 봐요~~~",
          "original_form": "내일 봐요~~~",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030416.1.7",
          "form": "안녕하세오",
          "original_form": "안녕하세오",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100030416.1.8",
          "form": "꿩 잘 먹었나오??",
          "original_form": "꿩 잘 먹었나오??",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100030416.1.9",
          "form": "넹 꿩냉면 먹었습니다",
          "original_form": "넹 꿩냉면 먹었습니다",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100030416.1.10",
          "form": "맛 어떤지 물어봐주세오",
          "original_form": "맛 어떤지 물어봐주세오",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100030416.1.11",
          "form": "오오 맛 어땠나요",
          "original_form": "오오 맛 어땠나요",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100030416.1.12",
          "form": "냉면육수는 평양냉면이라 살짝 슴슴하고 꿩은",
          "original_form": "냉면육수는 평양냉면이라 살짝 슴슴하고 꿩은",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100030416.1.13",
          "form": "어땠을까요?",
          "original_form": "어땠을까요?",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100030416.1.14",
          "form": "닭이랑 비슷 한가오?",
          "original_form": "닭이랑 비슷 한가오?",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100030416.1.15",
          "form": "아주 정확해오",
          "original_form": "아주 정확해오",
          "speaker_id": "2",
          "time": "20210904 23:01"
