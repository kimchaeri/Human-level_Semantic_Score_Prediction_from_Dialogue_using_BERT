      "utterance": [
        {
          "id": "MDRW2100043051.1.1",
          "form": "불닭 맛있게 드셔요",
          "original_form": "불닭 맛있게 드셔요",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043051.1.2",
          "form": "ㅋㅋㅋ 와우 맵네여ㅠㅠ",
          "original_form": "ㅋㅋㅋ 와우 맵네여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043051.1.3",
          "form": "오랭만에 먹으니",
          "original_form": "오랭만에 먹으니",
          "speaker_id": "1",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043051.1.4",
          "form": "그쳐 그거 오랜만에 먹으면 진짜 매워요",
          "original_form": "그쳐 그거 오랜만에 먹으면 진짜 매워요",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043051.1.5",
          "form": "그래도 땡기는 매운맛이니까!!!",
          "original_form": "그래도 땡기는 매운맛이니까!!!",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043051.1.6",
          "form": "그니까여 ㅋㅋ 자꾸 드가도생각나는 매운맛",
          "original_form": "그니까여 ㅋㅋ 자꾸 드가도생각나는 매운맛",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043051.1.7",
          "form": "저는 불닭에 소스 다는 못넣어 먹어요 맵찔이라",
          "original_form": "저는 불닭에 소스 다는 못넣어 먹어요 맵찔이라",
          "speaker_id": "2",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100043051.1.8",
          "form": "근데도 먹습니다 맛있어서 ㅋㅋㅋㅋ",
          "original_form": "근데도 먹습니다 맛있어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043051.1.9",
          "form": "중독 소스에여 ㅋㅋㅋ 배가 왜 안부르져ㅠ",
          "original_form": "중독 소스에여 ㅋㅋㅋ 배가 왜 안부르져ㅠ",
          "speaker_id": "1",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100043051.1.10",
          "form": "불닭먹으면 좀 허한 느낌 들어요",
          "original_form": "불닭먹으면 좀 허한 느낌 들어요",
          "speaker_id": "2",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100043051.1.11",
          "form": "그래서 군만두 같이 먹나봐요",
          "original_form": "그래서 군만두 같이 먹나봐요",
          "speaker_id": "2",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100043051.1.12",
          "form": "양이 작아요ㅠㅎ",
          "original_form": "양이 작아요ㅠㅎ",
          "speaker_id": "1",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100043051.1.13",
          "form": "생각해보니 치즈볼 같이 먹어도 맛있을거 같은 느낌이네요",
          "original_form": "생각해보니 치즈볼 같이 먹어도 맛있을거 같은 느낌이네요",
          "speaker_id": "2",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043051.1.14",
          "form": "치즈볼이업쪄여ㅠㅠㅠ",
          "original_form": "치즈볼이업쪄여ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043051.1.15",
          "form": "치즈볼은 그냥 먹어도.. 맛...잇져ㅠㅠ",
          "original_form": "치즈볼은 그냥 먹어도.. 맛...잇져ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043051.1.16",
          "form": "까까라도!!",
          "original_form": "까까라도!!",
          "speaker_id": "2",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100043051.1.17",
          "form": "ㅋㅋ 저는 지금 뽀또 먹고 있습니다",
          "original_form": "ㅋㅋ 저는 지금 뽀또 먹고 있습니다",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043051.1.18",
          "form": "지금 망고쥬스에..",
          "original_form": "지금 망고쥬스에..",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043051.1.19",
          "form": "파바 빵또 뜯.....",
          "original_form": "파바 빵또 뜯.....",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043051.1.20",
          "form": "맛있는거 드시는데요",
          "original_form": "맛있는거 드시는데요",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043051.1.21",
          "form": "ㅋㅋㅋ 파바 빵!!!",
          "original_form": "ㅋㅋㅋ 파바 빵!!!",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043051.1.22",
