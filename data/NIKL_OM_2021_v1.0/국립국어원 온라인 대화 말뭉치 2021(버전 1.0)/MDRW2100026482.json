      "utterance": [
        {
          "id": "MDRW2100026482.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026482.1.2",
          "form": "앗ㅋㅋ 최근에 뭐 사셧어오?",
          "original_form": "앗ㅋㅋ 최근에 뭐 사셧어오?",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026482.1.3",
          "form": "네 전 좋아해요~ 이것저것 사는 재미로 살아요 ㅋㅋㅋ",
          "original_form": "네 전 좋아해요~ 이것저것 사는 재미로 살아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026482.1.4",
          "form": "저는 최근에 미세모 칫솔샀어요 ㅋㅋ 잇몸이 약해서요",
          "original_form": "저는 최근에 미세모 칫솔샀어요 ㅋㅋ 잇몸이 약해서요",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026482.1.5",
          "form": "요새 산 것 중에 가장 잘샀다 싶으신 거 있으세요??",
          "original_form": "요새 산 것 중에 가장 잘샀다 싶으신 거 있으세요??",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026482.1.6",
          "form": "우와 미세모 칫솔 어때여?",
          "original_form": "우와 미세모 칫솔 어때여?",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026482.1.7",
          "form": "저는 의자요!",
          "original_form": "저는 의자요!",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026482.1.8",
          "form": "등이 딱 안정감있게 받쳐지니 좋아요!!",
          "original_form": "등이 딱 안정감있게 받쳐지니 좋아요!!",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026482.1.9",
          "form": "와 큰 거 지르셨네요!!!",
          "original_form": "와 큰 거 지르셨네요!!!",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026482.1.10",
          "form": "듀오백 같은 거였을까요??",
          "original_form": "듀오백 같은 거였을까요??",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026482.1.11",
          "form": "컴프프로인가 그랬어요!",
          "original_form": "컴프프로인가 그랬어요!",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026482.1.12",
          "form": "미세모칫솔은....그냥 부드러워요",
          "original_form": "미세모칫솔은....그냥 부드러워요",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026482.1.13",
          "form": "오... 그렇군요!ㅋㅋㅋ 그럼 그냥 제 칫솔 쓰는 걸로!",
          "original_form": "오... 그렇군요!ㅋㅋㅋ 그럼 그냥 제 칫솔 쓰는 걸로!",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026482.1.14",
          "form": "앗ㅋㅋㅋㅋㅋㅋ 바이럴좀 잘 좀 해볼 걸 그랬네요ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋ 바이럴좀 잘 좀 해볼 걸 그랬네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026482.1.15",
          "form": "컴프프로 사실 처음 들어봤는데 나중에 의자 바꾸게 되면 후보군에 넣어야겠엉ㅅ",
          "original_form": "컴프프로 사실 처음 들어봤는데 나중에 의자 바꾸게 되면 후보군에 넣어야겠엉ㅅ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026482.1.16",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026482.1.17",
          "form": "ㅋㅋㅋㅋㅋ 너무 웃겨요 저는 그럼 의자 영업을 잘 해볼게요",
          "original_form": "ㅋㅋㅋㅋㅋ 너무 웃겨요 저는 그럼 의자 영업을 잘 해볼게요",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026482.1.18",
          "form": "아 근데 의자가 진짜 좋긴 하더라고요",
          "original_form": "아 근데 의자가 진짜 좋긴 하더라고요",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026482.1.19",
          "form": "사실 거기가 의자가 아니라 책상으로 유명해요",
          "original_form": "사실 거기가 의자가 아니라 책상으로 유명해요",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026482.1.20",
          "form": "오래 앉아있어도 편안한가요??",
          "original_form": "오래 앉아있어도 편안한가요??",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026482.1.21",
          "form": "사실 아까 검색해봤다가 책상가격 보고 눈 튀어나올 뻔 했어요",
          "original_form": "사실 아까 검색해봤다가 책상가격 보고 눈 튀어나올 뻔 했어요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026482.1.22",
          "form": "네네! 저는 그랬어요!",
          "original_form": "네네! 저는 그랬어요!",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026482.1.23",
          "form": "책상도 샀어요 ㅋㅋㅋ",
          "original_form": "책상도 샀어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026482.1.24",
          "form": "와!",
          "original_form": "와!",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026482.1.25",
          "form": "정말 큰 지름을 하셨군요!!",
          "original_form": "정말 큰 지름을 하셨군요!!",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026482.1.26",
          "form": "책상이 그 뭐지 대만에서만 팔던 건데 특허를 샀대나?",
          "original_form": "책상이 그 뭐지 대만에서만 팔던 건데 특허를 샀대나?",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026482.1.27",
          "form": "그래서 이제 컴프프로에서만 판대요",
          "original_form": "그래서 이제 컴프프로에서만 판대요",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026482.1.28",
          "form": "넹!! 그런 셈이죠 크크",
          "original_form": "넹!! 그런 셈이죠 크크",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026482.1.29",
          "form": "우와 그건 못참죠 ㅋㅋㅋㅋㅋ",
          "original_form": "우와 그건 못참죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026482.1.30",
          "form": "구매할 가치가 있네요",
          "original_form": "구매할 가치가 있네요",
          "speaker_id": "2",
