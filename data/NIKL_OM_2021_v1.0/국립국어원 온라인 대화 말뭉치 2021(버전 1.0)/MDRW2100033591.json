      "utterance": [
        {
          "id": "MDRW2100033591.1.1",
          "form": "집안일 하는 거 좋아하세요?",
          "original_form": "집안일 하는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100033591.1.2",
          "form": "전 싫어해요",
          "original_form": "전 싫어해요",
          "speaker_id": "1",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100033591.1.3",
          "form": "저도 그닥 좋아하는 편은 아니에요",
          "original_form": "저도 그닥 좋아하는 편은 아니에요",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100033591.1.4",
          "form": "가정하기 싫은 일은 뭔가요",
          "original_form": "가정하기 싫은 일은 뭔가요",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100033591.1.5",
          "form": "가장 하기 싫은 뭔가요",
          "original_form": "가장 하기 싫은 뭔가요",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100033591.1.6",
          "form": "전 설거지요",
          "original_form": "전 설거지요",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033591.1.7",
          "form": "설거지가 너무 싫어요ㅋㅋ",
          "original_form": "설거지가 너무 싫어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033591.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033591.1.9",
          "form": "저는 분리수거가 너무 싫어요",
          "original_form": "저는 분리수거가 너무 싫어요",
          "speaker_id": "2",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100033591.1.10",
          "form": "외워야 할 것도 많고 헷갈리는 것도 많고 그래서 싫어요",
          "original_form": "외워야 할 것도 많고 헷갈리는 것도 많고 그래서 싫어요",
          "speaker_id": "2",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100033591.1.11",
          "form": "아 분리수거 그것도 나름 귀찮죠",
          "original_form": "아 분리수거 그것도 나름 귀찮죠",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100033591.1.12",
          "form": "음쓰 버리는 것도 싫어해요",
          "original_form": "음쓰 버리는 것도 싫어해요",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100033591.1.13",
          "form": "특히 여름엔 ",
          "original_form": "특히 여름엔 ",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100033591.1.14",
          "form": "진짜 너무  싫으네요ㅠ",
          "original_form": "진짜 너무  싫으네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100033591.1.15",
          "form": "음식물 쓰레기 같은 경우는 제때 안 걸리면 벌레도 꼬이잖아요",
          "original_form": "음식물 쓰레기 같은 경우는 제때 안 걸리면 벌레도 꼬이잖아요",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100033591.1.16",
          "form": "맞아요 초파리가 정말",
          "original_form": "맞아요 초파리가 정말",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100033591.1.17",
          "form": "최악이죠",
          "original_form": "최악이죠",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100033591.1.18",
          "form": "초파리 진짜 끔찍해요",
          "original_form": "초파리 진짜 끔찍해요",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100033591.1.19",
          "form": "쪼끄만 게 빠르기도 빠르고 잡히지도 않아서 아 진짜",
          "original_form": "쪼끄만 게 빠르기도 빠르고 잡히지도 않아서 아 진짜",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100033591.1.20",
          "form": "맞아요 순삭이에요 정말",
          "original_form": "맞아요 순삭이에요 정말",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100033591.1.21",
          "form": "초파리 트랩 이런 거 만들어도 소용이 없더라고요",
          "original_form": "초파리 트랩 이런 거 만들어도 소용이 없더라고요",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033591.1.22",
          "form": "화릉 부르는.. 음쓰...",
          "original_form": "화릉 부르는.. 음쓰...",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033591.1.23",
          "form": "진짜요 번식속도가 아주..",
          "original_form": "진짜요 번식속도가 아주..",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033591.1.24",
          "form": "미쳤어요 진짜",
          "original_form": "미쳤어요 진짜",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033591.1.25",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033591.1.26",
          "form": "진짜 쓰레기는 제때 제때 버려야 하는 것 같아요",
          "original_form": "진짜 쓰레기는 제때 제때 버려야 하는 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033591.1.27",
          "form": "맞아요 미루면 나중에 후회해요 ㅠㅠㅠ",
          "original_form": "맞아요 미루면 나중에 후회해요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 16:31"
        }
      ]
    }
