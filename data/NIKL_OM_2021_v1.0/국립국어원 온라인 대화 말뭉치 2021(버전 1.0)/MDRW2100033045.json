      "utterance": [
        {
          "id": "MDRW2100033045.1.1",
          "form": "오오 이건 제가 고른거예요!",
          "original_form": "오오 이건 제가 고른거예요!",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.2",
          "form": "은행 공인인증서",
          "original_form": "은행 공인인증서",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.3",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.4",
          "form": "앗 또 바꼈네요",
          "original_form": "앗 또 바꼈네요",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.5",
          "form": "앗 주제 이거 재밌어보여서",
          "original_form": "앗 주제 이거 재밌어보여서",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.6",
          "form": "와 월급 백만원",
          "original_form": "와 월급 백만원",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.7",
          "form": "제가 고른거예요 ㅋㅋㅋㅋㅋ",
          "original_form": "제가 고른거예요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.8",
          "form": "아니 진짜 군인들",
          "original_form": "아니 진짜 군인들",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.9",
          "form": "요즘 군대는 거의 뭐",
          "original_form": "요즘 군대는 거의 뭐",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033045.1.10",
          "form": "워킹홀리데이",
          "original_form": "워킹홀리데이",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033045.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033045.1.12",
          "form": "약간 백만원이면",
          "original_form": "약간 백만원이면",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033045.1.13",
          "form": "저렇게까지 해줘야하나 싶어요 어차피 군대 다녀오면 스펙도 되는데",
          "original_form": "저렇게까지 해줘야하나 싶어요 어차피 군대 다녀오면 스펙도 되는데",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033045.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033045.1.15",
          "form": "여자도 일부러 들어갈 듯요",
          "original_form": "여자도 일부러 들어갈 듯요",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033045.1.16",
          "form": "나을지도 모르겠네요",
          "original_form": "나을지도 모르겠네요",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033045.1.17",
          "form": "약간 이정부가",
          "original_form": "약간 이정부가",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033045.1.18",
          "form": "군대 내 성 범죄 문제부터",
          "original_form": "군대 내 성 범죄 문제부터",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.19",
          "form": "근데 그런거 암것도안써도",
          "original_form": "근데 그런거 암것도안써도",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.20",
          "form": "제대로 해결하고 여자를 군대에 보내겠다 해야지ㅠ",
          "original_form": "제대로 해결하고 여자를 군대에 보내겠다 해야지ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.21",
          "form": "한번씩뜨더라구요",
          "original_form": "한번씩뜨더라구요",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.22",
          "form": "이거 지금",
          "original_form": "이거 지금",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.23",
          "form": "2030 남자들 눈치 오지게 봐서 그런거",
          "original_form": "2030 남자들 눈치 오지게 봐서 그런거",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.24",
          "form": "저는 예전에 남자가 성전환해서",
          "original_form": "저는 예전에 남자가 성전환해서",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.25",
          "form": "같은데요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "같은데요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.26",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.27",
          "form": "여군시켜달라하는거보고 소름",
          "original_form": "여군시켜달라하는거보고 소름",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033045.1.28",
          "form": "여군으로 인정해달라고 하는거요!?",
          "original_form": "여군으로 인정해달라고 하는거요!?",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
