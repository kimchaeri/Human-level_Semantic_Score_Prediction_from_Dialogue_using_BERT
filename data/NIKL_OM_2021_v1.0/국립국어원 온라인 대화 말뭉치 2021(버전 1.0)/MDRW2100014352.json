      "utterance": [
        {
          "id": "MDRW2100014352.1.1",
          "form": "우리 또 여행이야?",
          "original_form": "우리 또 여행이야?",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014352.1.2",
          "form": "ㅋㅋㅋㅋㅋ너 여행중이니까 어디갈껀지 말해바",
          "original_form": "ㅋㅋㅋㅋㅋ너 여행중이니까 어디갈껀지 말해바",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014352.1.3",
          "form": "나?ㅋㅋㅋㄱ전주한옥마을",
          "original_form": "나?ㅋㅋㅋㄱ전주한옥마을",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014352.1.4",
          "form": "카페",
          "original_form": "카페",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014352.1.5",
          "form": "객리단길",
          "original_form": "객리단길",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014352.1.6",
          "form": "숙소가 한옥마을?ㅋㅋ",
          "original_form": "숙소가 한옥마을?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100014352.1.7",
          "form": "아니 하루는 일반호텔 하루는 한옥",
          "original_form": "아니 하루는 일반호텔 하루는 한옥",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014352.1.8",
          "form": "ㅋㅋ옮기기 힘든데 하루씩 옮기는거?ㅎㅎ",
          "original_form": "ㅋㅋ옮기기 힘든데 하루씩 옮기는거?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014352.1.9",
          "form": "오빠가 한옥을 가야한대",
          "original_form": "오빠가 한옥을 가야한대",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014352.1.10",
          "form": "귀찮",
          "original_form": "귀찮",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014352.1.11",
          "form": "ㅋㅋㅋㅋ나도 한옥 추천",
          "original_form": "ㅋㅋㅋㅋ나도 한옥 추천",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014352.1.12",
          "form": "근데 한옥이 다 게하더라고ㅜ",
          "original_form": "근데 한옥이 다 게하더라고ㅜ",
          "speaker_id": "2",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014352.1.13",
          "form": "늦게예약해서",
          "original_form": "늦게예약해서",
          "speaker_id": "2",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014352.1.14",
          "form": "아ㅜ 게하는 힘들어 이제ㅜ",
          "original_form": "아ㅜ 게하는 힘들어 이제ㅜ",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014352.1.15",
          "form": "나이가 먹었나봐ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나이가 먹었나봐ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014352.1.16",
          "form": "으응..그래서 안갈라했는데 오빠가 숙소괜찮다고ㄷㄷ",
          "original_form": "으응..그래서 안갈라했는데 오빠가 숙소괜찮다고ㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014352.1.17",
          "form": "엄마는 어떠시대?",
          "original_form": "엄마는 어떠시대?",
          "speaker_id": "1",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100014352.1.18",
          "form": "엄마 오늘 알았어ㅋㅋㅋㅋ게하라고?ㅋㅋ놀람",
          "original_form": "엄마 오늘 알았어ㅋㅋㅋㅋ게하라고?ㅋㅋ놀람",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
