      "utterance": [
        {
          "id": "MDRW2100006695.1.1",
          "form": "아침메뉴는 정하셨나요?",
          "original_form": "아침메뉴는 정하셨나요?",
          "speaker_id": "1",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100006695.1.2",
          "form": "네 방금 먹었습니다  ㅎ",
          "original_form": "네 방금 먹었습니다  ㅎ",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100006695.1.3",
          "form": "오 부지런하세요",
          "original_form": "오 부지런하세요",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100006695.1.4",
          "form": "머 드셨어요?",
          "original_form": "머 드셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100006695.1.5",
          "form": "미역국에 밑반찬이요 ㅎㅎ",
          "original_form": "미역국에 밑반찬이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006695.1.6",
          "form": "아 미역국 맛나겠어요",
          "original_form": "아 미역국 맛나겠어요",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006695.1.7",
          "form": "저는 도넛 먹으려구요",
          "original_form": "저는 도넛 먹으려구요",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006695.1.8",
          "form": "어제 미니도넛이라고 던킨에서 사왔거든요",
          "original_form": "어제 미니도넛이라고 던킨에서 사왔거든요",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100006695.1.9",
          "form": "미니도넛 드셔보셨어요?",
          "original_form": "미니도넛 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100006695.1.10",
          "form": "아뇨?크기가 작은건가요??",
          "original_form": "아뇨?크기가 작은건가요??",
          "speaker_id": "2",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006695.1.11",
          "form": "손바닥만해요",
          "original_form": "손바닥만해요",
          "speaker_id": "1",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006695.1.12",
          "form": "남자라면 한입에 다 넣지 싶어요",
          "original_form": "남자라면 한입에 다 넣지 싶어요",
          "speaker_id": "1",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006695.1.13",
          "form": "첨엔 해피앱에서 미니도넛팩 천원 쿠폰주길래",
          "original_form": "첨엔 해피앱에서 미니도넛팩 천원 쿠폰주길래",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100006695.1.14",
          "form": "그래도 비싸겠지 했는데",
          "original_form": "그래도 비싸겠지 했는데",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100006695.1.15",
          "form": "3300원이더라구요",
          "original_form": "3300원이더라구요",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100006695.1.16",
          "form": "거기에 천원 쿠폰쓰고 2300원에 가져왔어요 ㅎ",
          "original_form": "거기에 천원 쿠폰쓰고 2300원에 가져왔어요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100006695.1.17",
          "form": "도넛 좋아하시나요ㅡㅋ 먼치킨하고는 또 다른거죠?",
          "original_form": "도넛 좋아하시나요ㅡㅋ 먼치킨하고는 또 다른거죠?",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100006695.1.18",
          "form": "네 먼치킨하고는 달라요",
          "original_form": "네 먼치킨하고는 달라요",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006695.1.19",
          "form": "진짜 도넛머양 이예요",
          "original_form": "진짜 도넛머양 이예요",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006695.1.20",
          "form": "저는 도넛 잘 안사먹었거든요 ㅎㅎ",
          "original_form": "저는 도넛 잘 안사먹었거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006695.1.21",
          "form": "달고 비싸다는 생각에 ㅎ",
          "original_form": "달고 비싸다는 생각에 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006695.1.22",
          "form": "그냥 빵사먹는게 낫다고 ㅎㅎㅎ",
          "original_form": "그냥 빵사먹는게 낫다고 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006695.1.23",
          "form": "근데 이번엔 천원 쿠폰줘서 한번 사봤어요 ㅎ",
          "original_form": "근데 이번엔 천원 쿠폰줘서 한번 사봤어요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006695.1.24",
          "form": "아 노티드 도넛 아세요?",
          "original_form": "아 노티드 도넛 아세요?",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006695.1.25",
          "form": "아뇨 첨들어요",
          "original_form": "아뇨 첨들어요",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100006695.1.26",
          "form": "유명한가요?",
          "original_form": "유명한가요?",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100006695.1.27",
          "form": "네 유명하다더라구요..지방살아서..제주도 여행간김에 먹어봤어요",
          "original_form": "네 유명하다더라구요..지방살아서..제주도 여행간김에 먹어봤어요",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100006695.1.28",
          "form": "저도 지방인데 ㅎㅎ",
          "original_form": "저도 지방인데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100006695.1.29",
          "form": "첨들어봐요 맛나나봐요",
          "original_form": "첨들어봐요 맛나나봐요",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100006695.1.30",
