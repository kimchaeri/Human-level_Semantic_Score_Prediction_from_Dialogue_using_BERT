      "utterance": [
        {
          "id": "MDRW2100044984.1.1",
          "form": "? 무슨 떡 먹었어요??",
          "original_form": "? 무슨 떡 먹었어요??",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044984.1.2",
          "form": "인절미요",
          "original_form": "인절미요",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044984.1.3",
          "form": "헐 맛있겠네요. 제껀 없었나요?",
          "original_form": "헐 맛있겠네요. 제껀 없었나요?",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044984.1.4",
          "form": "네 점심밥이었음",
          "original_form": "네 점심밥이었음",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044984.1.5",
          "form": "오셔서 드실?",
          "original_form": "오셔서 드실?",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044984.1.6",
          "form": "아뇨 포장으로 부탁드려요",
          "original_form": "아뇨 포장으로 부탁드려요",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044984.1.7",
          "form": "떡좋아함?",
          "original_form": "떡좋아함?",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044984.1.8",
          "form": "네 떡 진짜 좋아해요.",
          "original_form": "네 떡 진짜 좋아해요.",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044984.1.9",
          "form": "무슨떡이요?",
          "original_form": "무슨떡이요?",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044984.1.10",
          "form": "저는 꿀떡 좋아해요!",
          "original_form": "저는 꿀떡 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044984.1.11",
          "form": "그거 맛있죠",
          "original_form": "그거 맛있죠",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044984.1.12",
          "form": "쉰떡들도 좋아해요",
          "original_form": "쉰떡들도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044984.1.13",
          "form": "전 찹살떡 좊아요",
          "original_form": "전 찹살떡 좊아요",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044984.1.14",
          "form": "인절미는 여기저기 같이나오는것도 많죠",
          "original_form": "인절미는 여기저기 같이나오는것도 많죠",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044984.1.15",
          "form": "설빙처럼",
          "original_form": "설빙처럼",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044984.1.16",
          "form": "아 설빙 인절미 빙수 진짜 맛있어요 !",
          "original_form": "아 설빙 인절미 빙수 진짜 맛있어요 !",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044984.1.17",
          "form": "설빙 요즘 신메뉴 이상해요",
          "original_form": "설빙 요즘 신메뉴 이상해요",
          "speaker_id": "1",
