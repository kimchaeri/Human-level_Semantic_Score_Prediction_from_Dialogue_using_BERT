      "utterance": [
        {
          "id": "MDRW2100029273.1.1",
          "form": "너 행시 결과는 어떻게 됐어",
          "original_form": "너 행시 결과는 어떻게 됐어",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029273.1.2",
          "form": "엄청 어려웠어",
          "original_form": "엄청 어려웠어",
          "speaker_id": "1",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029273.1.3",
          "form": "결과는 언제 나오는데",
          "original_form": "결과는 언제 나오는데",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029273.1.4",
          "form": "다다음주인가",
          "original_form": "다다음주인가",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029273.1.5",
          "form": "떨어지면 뭐할거야",
          "original_form": "떨어지면 뭐할거야",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029273.1.6",
          "form": "학원 등록해놨어",
          "original_form": "학원 등록해놨어",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029273.1.7",
          "form": "학원은 언제부터가는데",
          "original_form": "학원은 언제부터가는데",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029273.1.8",
          "form": "학원 담주월욜부터",
          "original_form": "학원 담주월욜부터",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029273.1.9",
          "form": "다음 시험까지 다니는거야???",
          "original_form": "다음 시험까지 다니는거야???",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029273.1.10",
          "form": "아니야 일단 10월 15일까지",
          "original_form": "아니야 일단 10월 15일까지",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029273.1.11",
          "form": "왜 10월까지만 다녀",
          "original_form": "왜 10월까지만 다녀",
          "speaker_id": "2",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029273.1.12",
          "form": "그냥 쭉 다녀",
          "original_form": "그냥 쭉 다녀",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029273.1.13",
          "form": "일단 학원 스케줄보고",
          "original_form": "일단 학원 스케줄보고",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029273.1.14",
          "form": "한과목만 등록해놨어",
          "original_form": "한과목만 등록해놨어",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029273.1.15",
          "form": "다른것도 등록해",
          "original_form": "다른것도 등록해",
