      "utterance": [
        {
          "id": "MDRW2100043356.1.1",
          "form": "안 그래도 처음",
          "original_form": "안 그래도 처음",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.2",
          "form": "거기 살았을 때",
          "original_form": "거기 살았을 때",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.3",
          "form": "우리 엄마는 짐만 거기였지 가게에서 지냈잖아",
          "original_form": "우리 엄마는 짐만 거기였지 가게에서 지냈잖아",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.4",
          "form": "근데 가스비가 너무 많이 나오는 거야",
          "original_form": "근데 가스비가 너무 많이 나오는 거야",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.5",
          "form": "그래서 확인해보니까",
          "original_form": "그래서 확인해보니까",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.6",
          "form": "계량기가 고장 난 채로 계속 돈이 나가고 있었던 거지",
          "original_form": "계량기가 고장 난 채로 계속 돈이 나가고 있었던 거지",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.7",
          "form": "살림하는사람이면 그냥 그돈 냈을거아니가",
          "original_form": "살림하는사람이면 그냥 그돈 냈을거아니가",
          "speaker_id": "2",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.8",
          "form": "헐 너무하네요",
          "original_form": "헐 너무하네요",
          "speaker_id": "1",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210907 18:10"
        },
        {
          "id": "MDRW2100043356.1.10",
          "form": "왜 그런가 보다 하고",
          "original_form": "왜 그런가 보다 하고",
          "speaker_id": "1",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.11",
          "form": "그래서 그거 고쳐달라해가지고",
          "original_form": "그래서 그거 고쳐달라해가지고",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.12",
          "form": "다시 사용하고 있지",
          "original_form": "다시 사용하고 있지",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.13",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.14",
          "form": "모르면 그냥 당하는 거,,",
          "original_form": "모르면 그냥 당하는 거,,",
          "speaker_id": "1",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.15",
          "form": "글치",
          "original_form": "글치",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.16",
          "form": "근데 관리비가 진짜 많이 나오긴 하더라",
          "original_form": "근데 관리비가 진짜 많이 나오긴 하더라",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.17",
          "form": "그거 돈 살펴보면 쓸데없는 돈 많이 나감",
          "original_form": "그거 돈 살펴보면 쓸데없는 돈 많이 나감",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.18",
          "form": "직원들 월급 이런것도 많고",
          "original_form": "직원들 월급 이런것도 많고",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100043356.1.19",
          "form": "나는 용지를 한 번도 못 봤어요",
          "original_form": "나는 용지를 한 번도 못 봤어요",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.20",
          "form": "솔직히 그 많은 세대수에 비해",
          "original_form": "솔직히 그 많은 세대수에 비해",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.21",
          "form": "직원이 몇명있노",
          "original_form": "직원이 몇명있노",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.22",
          "form": "관리도 안되면서",
          "original_form": "관리도 안되면서",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.23",
          "form": "그렇죠 직원 나 몇 명 못 봤는데",
          "original_form": "그렇죠 직원 나 몇 명 못 봤는데",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.24",
          "form": "난 그때 엄마가 관리비 많이 나온다고 해서",
          "original_form": "난 그때 엄마가 관리비 많이 나온다고 해서",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.25",
          "form": "살펴봤는데",
          "original_form": "살펴봤는데",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.26",
          "form": "여기가 유독 더 그런 든요",
          "original_form": "여기가 유독 더 그런 든요",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.27",
          "form": "직원들이나 뭐 그런거에 돈이 많이 나가더라고",
          "original_form": "직원들이나 뭐 그런거에 돈이 많이 나가더라고",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.28",
          "form": "그런갑다",
          "original_form": "그런갑다",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.29",
          "form": "엘에치 사고 계속 터지는거보니",
          "original_form": "엘에치 사고 계속 터지는거보니",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.30",
          "form": "여기 사람들 너무 착한가 봄",
          "original_form": "여기 사람들 너무 착한가 봄",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.31",
          "form": "비리가 많은듯",
          "original_form": "비리가 많은듯",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100043356.1.32",
          "form": "맞아 여 맞아",
          "original_form": "맞아 여 맞아",
          "speaker_id": "1",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043356.1.33",
          "form": "고발하고싶네 ㅋㄱㄱㅋㄱ",
          "original_form": "고발하고싶네 ㅋㄱㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043356.1.34",
          "form": "밥했나??",
          "original_form": "밥했나??",
          "speaker_id": "2",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100043356.1.35",
          "form": "난 투 언어센터와서 기다리고 있다",
          "original_form": "난 투 언어센터와서 기다리고 있다",
          "speaker_id": "2",
          "time": "20210907 18:13"
        }
      ]
    }
