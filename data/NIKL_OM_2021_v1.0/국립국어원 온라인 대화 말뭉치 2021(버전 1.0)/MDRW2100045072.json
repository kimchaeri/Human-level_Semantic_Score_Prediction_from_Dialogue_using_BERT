      "utterance": [
        {
          "id": "MDRW2100045072.1.1",
          "form": "팬케이크 맞죠!",
          "original_form": "팬케이크 맞죠!",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045072.1.2",
          "form": "네 맞아요! ㅋㅋㅋㅋㅋ",
          "original_form": "네 맞아요! ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045072.1.3",
          "form": "대박 대박 대박",
          "original_form": "대박 대박 대박",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045072.1.4",
          "form": "마지막 힌트가 진짜 결정적이었어요",
          "original_form": "마지막 힌트가 진짜 결정적이었어요",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045072.1.5",
          "form": "잼이나 시럽이랑 먹거",
          "original_form": "잼이나 시럽이랑 먹거",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045072.1.6",
          "form": "집에서도 만드는 거요ㅋㅋㅋㅋ",
          "original_form": "집에서도 만드는 거요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045072.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그쵸 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045072.1.8",
          "form": "ㅋㅋㅋ그 전까지는 이 세상에 있는 음식이긴 한가",
          "original_form": "ㅋㅋㅋ그 전까지는 이 세상에 있는 음식이긴 한가",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045072.1.9",
          "form": "싶었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "싶었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045072.1.10",
          "form": "감자를 맞추실 때 이런 느낌이셨겠군요",
          "original_form": "감자를 맞추실 때 이런 느낌이셨겠군요",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045072.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 네 그쵸 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 네 그쵸 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045072.1.12",
          "form": "맞추고 나니까 허무하더라구여 ㅋㅋㅋㅋ",
          "original_form": "맞추고 나니까 허무하더라구여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045072.1.13",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045072.1.14",
          "form": "서로 너무했어요 ㅋㅋㅋㅋ짱 웃기다",
          "original_form": "서로 너무했어요 ㅋㅋㅋㅋ짱 웃기다",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045072.1.15",
          "form": "이번엔 과일 맞추기 해보실래요?",
          "original_form": "이번엔 과일 맞추기 해보실래요?",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045072.1.16",
          "form": "식재료 하나씩 주고받은걸로 해요 ㅋㅋㅋㅋ",
          "original_form": "식재료 하나씩 주고받은걸로 해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045072.1.17",
          "form": "넵 ㅋㅋㅋㅋㅋ",
          "original_form": "넵 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045072.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ딜교환",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ딜교환",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045072.1.19",
          "form": "제가 생각하는 과일 맞춰보세요!",
          "original_form": "제가 생각하는 과일 맞춰보세요!",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045072.1.20",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045072.1.21",
          "form": "나무에서 열리나요?",
          "original_form": "나무에서 열리나요?",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045072.1.22",
          "form": "네! 나무에서 열리는 과일입니다",
          "original_form": "네! 나무에서 열리는 과일입니다",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045072.1.23",
          "form": "송이로 열리나요?",
          "original_form": "송이로 열리나요?",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045072.1.24",
          "form": "어... 검색해보고 올게요 송이인가",
          "original_form": "어... 검색해보고 올게요 송이인가",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045072.1.25",
          "form": "헐 몰랐는데 뭉쳐서 나네요?",
          "original_form": "헐 몰랐는데 뭉쳐서 나네요?",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045072.1.26",
          "form": "열릴 때는 송이입니다 ㅋㅋㅋㅋ",
          "original_form": "열릴 때는 송이입니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045072.1.27",
          "form": "붉은 색 계열인가요?",
          "original_form": "붉은 색 계열인가요?",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045072.1.28",
          "form": "아니요! 빨간 과일 아니예요",
          "original_form": "아니요! 빨간 과일 아니예요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        }
