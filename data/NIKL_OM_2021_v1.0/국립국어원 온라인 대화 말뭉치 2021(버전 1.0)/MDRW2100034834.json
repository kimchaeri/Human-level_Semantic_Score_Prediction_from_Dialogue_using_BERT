      "utterance": [
        {
          "id": "MDRW2100034834.1.1",
          "form": "그거 송중기가 나쁜남자 역할인가용?",
          "original_form": "그거 송중기가 나쁜남자 역할인가용?",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100034834.1.2",
          "form": "우와 진짜 많이 보셨네요ㅋㅋ",
          "original_form": "우와 진짜 많이 보셨네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100034834.1.3",
          "form": "네네 맞아요 나쁜남자인데 뒤늦게 후회하는",
          "original_form": "네네 맞아요 나쁜남자인데 뒤늦게 후회하는",
          "speaker_id": "1",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100034834.1.4",
          "form": "내용이에요 ㅎㅎ",
          "original_form": "내용이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100034834.1.5",
          "form": "헐 후회 남주 이런거 꿀잼인데ㅋㅋㅋㅋ",
          "original_form": "헐 후회 남주 이런거 꿀잼인데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:31"
        },
        {
          "id": "MDRW2100034834.1.6",
          "form": "저는 제 인생드라마",
          "original_form": "저는 제 인생드라마",
          "speaker_id": "2",
          "time": "20210828 20:31"
        },
        {
          "id": "MDRW2100034834.1.7",
          "form": "라이프온마스에용",
          "original_form": "라이프온마스에용",
          "speaker_id": "2",
          "time": "20210828 20:31"
        },
        {
          "id": "MDRW2100034834.1.8",
          "form": "어 그거 정경호 나오는거죠?!",
          "original_form": "어 그거 정경호 나오는거죠?!",
          "speaker_id": "1",
          "time": "20210828 20:31"
        },
        {
          "id": "MDRW2100034834.1.9",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210828 20:31"
        },
        {
          "id": "MDRW2100034834.1.10",
          "form": "예전에 홍보하는거 봤어요 ㅋㅋ 박성웅씨랑",
          "original_form": "예전에 홍보하는거 봤어요 ㅋㅋ 박성웅씨랑",
          "speaker_id": "1",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034834.1.11",
          "form": "이거 진짜진짜 재밌어요",
          "original_form": "이거 진짜진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034834.1.12",
          "form": "연출도 넘 좋고",
          "original_form": "연출도 넘 좋고",
          "speaker_id": "2",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034834.1.13",
          "form": "아 카이로스 라는 드라마도 아세요?",
          "original_form": "아 카이로스 라는 드라마도 아세요?",
          "speaker_id": "2",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034834.1.14",
          "form": "아 카이로스는 봤어요 ! 주변에서 저만 본 기억이 ...",
          "original_form": "아 카이로스는 봤어요 ! 주변에서 저만 본 기억이 ...",
          "speaker_id": "1",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034834.1.15",
          "form": "앜ㅋㅋㅋㅋ 이거 진짜 너무 재밌죠",
          "original_form": "앜ㅋㅋㅋㅋ 이거 진짜 너무 재밌죠",
          "speaker_id": "2",
          "time": "20210828 20:32"
        },
        {
          "id": "MDRW2100034834.1.16",
          "form": "신성록 배우 너무 잘하더라구요",
          "original_form": "신성록 배우 너무 잘하더라구요",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100034834.1.17",
          "form": "재밌는데 사람들이 몰라주더라구요 ㅋㅋ ㅠㅠ",
          "original_form": "재밌는데 사람들이 몰라주더라구요 ㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100034834.1.18",
          "form": "연기를 이렇게 잘하는줄 처음알았어요",
          "original_form": "연기를 이렇게 잘하는줄 처음알았어요",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100034834.1.19",
          "form": "그러게요ㅠㅠ 시청률도 안나왔고ㅠ",
          "original_form": "그러게요ㅠㅠ 시청률도 안나왔고ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100034834.1.20",
          "form": "맞아요 ! 전 신성록 배우 대상까지 기대했었는데 ㅋㅋ",
          "original_form": "맞아요 ! 전 신성록 배우 대상까지 기대했었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100034834.1.21",
          "form": "그 아이 잃은 아빠 연기를 너무 잘해가지고ㅋㅋㅋㅋ",
          "original_form": "그 아이 잃은 아빠 연기를 너무 잘해가지고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100034834.1.22",
          "form": "남규리도 다른의미로 소름돋게 연기 잘했어요ㅋㅋㅋㅋㅋ",
          "original_form": "남규리도 다른의미로 소름돋게 연기 잘했어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100034834.1.23",
          "form": "맞아요ㅋㅋ ㅜㅜ",
          "original_form": "맞아요ㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:35"
        }
      ]
    }
  ]
