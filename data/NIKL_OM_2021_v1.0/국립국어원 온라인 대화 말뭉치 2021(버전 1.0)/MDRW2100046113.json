      "utterance": [
        {
          "id": "MDRW2100046113.1.1",
          "form": "결혼 하셨어요?",
          "original_form": "결혼 하셨어요?",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046113.1.2",
          "form": "네 했습니다",
          "original_form": "네 했습니다",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046113.1.3",
          "form": "아이도 있으세요?",
          "original_form": "아이도 있으세요?",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046113.1.4",
          "form": "네 대학생 둘 있어요",
          "original_form": "네 대학생 둘 있어요",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046113.1.5",
          "form": "오...다 키우셨네요.^^",
          "original_form": "오...다 키우셨네요.^^",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046113.1.6",
          "form": "전 고딩 아들 2 있어요",
          "original_form": "전 고딩 아들 2 있어요",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046113.1.7",
          "form": "요즘 힘드시겠어요",
          "original_form": "요즘 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046113.1.8",
          "form": "이번 주부터 둘다 등교했어요",
          "original_form": "이번 주부터 둘다 등교했어요",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046113.1.9",
          "form": "남편분이 이상형 이셨나요",
          "original_form": "남편분이 이상형 이셨나요",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046113.1.10",
          "form": "이상형은 아니었어요.ㅋㅋ",
          "original_form": "이상형은 아니었어요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046113.1.11",
          "form": "이상형이랑 결혼하셨어요?",
          "original_form": "이상형이랑 결혼하셨어요?",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046113.1.12",
          "form": "현실과 이상형은 좀다르죠",
          "original_form": "현실과 이상형은 좀다르죠",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046113.1.13",
          "form": "그쵸. 아상형과 결혼해도 싸우고",
          "original_form": "그쵸. 아상형과 결혼해도 싸우고",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046113.1.14",
          "form": "결혼하면 다 같아지는것 같죠",
          "original_form": "결혼하면 다 같아지는것 같죠",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046113.1.15",
          "form": "네. 다들 비슷한거 같아요",
          "original_form": "네. 다들 비슷한거 같아요",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046113.1.16",
          "form": "결혼 몇년차세요?",
          "original_form": "결혼 몇년차세요?",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046113.1.17",
          "form": "23년차 이네요",
          "original_form": "23년차 이네요",
