      "utterance": [
        {
          "id": "MDRW2100007406.1.1",
          "form": "요즘 재밌는책있나요?",
          "original_form": "요즘 재밌는책있나요?",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007406.1.2",
          "form": "저는 요즘 재테크책파고 있어요 ㅎ",
          "original_form": "저는 요즘 재테크책파고 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007406.1.3",
          "form": "좋아하시는 작가 있으세요?",
          "original_form": "좋아하시는 작가 있으세요?",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007406.1.4",
          "form": "전 책과친하지가 않아서 ㅎ 예전엔 가끔읽었는데",
          "original_form": "전 책과친하지가 않아서 ㅎ 예전엔 가끔읽었는데",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007406.1.5",
          "form": "재테크 어떤거 보시는거예요?",
          "original_form": "재테크 어떤거 보시는거예요?",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007406.1.6",
          "form": "다양하게 봐요",
          "original_form": "다양하게 봐요",
          "speaker_id": "2",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007406.1.7",
          "form": "요즘 주식책 이런거 많은거같아요",
          "original_form": "요즘 주식책 이런거 많은거같아요",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007406.1.8",
          "form": "주식책 미국주식책\r\n부자되기위한방법? ㅋㅋ그말이 그말인거같은 그런책이요",
          "original_form": "주식책 미국주식책\r\n부자되기위한방법? ㅋㅋ그말이 그말인거같은 그런책이요",
          "speaker_id": "2",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007406.1.9",
          "form": "공부를 하긴해야되는데 책읽기는 넘 힘들어요ㅎ",
          "original_form": "공부를 하긴해야되는데 책읽기는 넘 힘들어요ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007406.1.10",
          "form": "공감하면 훌훌 넘어가요",
          "original_form": "공감하면 훌훌 넘어가요",
          "speaker_id": "2",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007406.1.11",
          "form": "짠돌이 책도 있는데 읽어보셨어요?",
          "original_form": "짠돌이 책도 있는데 읽어보셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007406.1.12",
          "form": "커넥츠 할땐 그래도 베스트셀러가 대충 뭐구나 했는데",
          "original_form": "커넥츠 할땐 그래도 베스트셀러가 대충 뭐구나 했는데",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007406.1.13",
          "form": "1일1짠 테크인가...그랬던거같아요",
          "original_form": "1일1짠 테크인가...그랬던거같아요",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007406.1.14",
          "form": "짠카페에서 출판한건가요?",
          "original_form": "짠카페에서 출판한건가요?",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007406.1.15",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007406.1.16",
          "form": "진짜 돈버는 방법이 나오나요?",
          "original_form": "진짜 돈버는 방법이 나오나요?",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007406.1.17",
          "form": "짠카페에서 수기공모한거같더라구요",
          "original_form": "짠카페에서 수기공모한거같더라구요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007406.1.18",
          "form": "수기집이예요",
          "original_form": "수기집이예요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007406.1.19",
          "form": "아 한분이 쓴게 아니군요",
          "original_form": "아 한분이 쓴게 아니군요",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007406.1.20",
          "form": "짠테크 어떻거했는지 어떻게 아꼈는지..",
          "original_form": "짠테크 어떻거했는지 어떻게 아꼈는지..",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007406.1.21",
          "form": "네 여러명이 쓰셨어요",
          "original_form": "네 여러명이 쓰셨어요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007406.1.22",
          "form": "함 읽어봐야겠네요",
          "original_form": "함 읽어봐야겠네요",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007406.1.23",
          "form": "이건 공감되셔서 훌훌 넘기실꺼예요",
          "original_form": "이건 공감되셔서 훌훌 넘기실꺼예요",
          "speaker_id": "2",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007406.1.24",
          "form": "한번 읽어보세요",
          "original_form": "한번 읽어보세요",
          "speaker_id": "2",
          "time": "20210822 02:02"
        },
        {
