      "utterance": [
        {
          "id": "MDRW2100020756.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 12:09"
        },
        {
          "id": "MDRW2100020756.1.2",
          "form": "네 저 집밥 먹었어요",
          "original_form": "네 저 집밥 먹었어요",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020756.1.3",
          "form": "된장찌개랑 깻잎",
          "original_form": "된장찌개랑 깻잎",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020756.1.4",
          "form": "와 된장찌개에 뭐 넣었나요",
          "original_form": "와 된장찌개에 뭐 넣었나요",
          "speaker_id": "2",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020756.1.5",
          "form": "두부랑 감자 들어있어요",
          "original_form": "두부랑 감자 들어있어요",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020756.1.6",
          "form": "제가 끓인건 아니구!",
          "original_form": "제가 끓인건 아니구!",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020756.1.7",
          "form": "오호 ㅋㅋㅋ 맛있겠다",
          "original_form": "오호 ㅋㅋㅋ 맛있겠다",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020756.1.8",
          "form": "차돌박이 된장찌개 먹어 봤어요?",
          "original_form": "차돌박이 된장찌개 먹어 봤어요?",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020756.1.9",
          "form": "네 좋아해요ㅎㅎ",
          "original_form": "네 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020756.1.10",
          "form": "자주 가는 식당에 비빔밥이랑 세트로 팔아요",
          "original_form": "자주 가는 식당에 비빔밥이랑 세트로 팔아요",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020756.1.11",
          "form": "오~ 진짜 맛있어요 ㅜㅜ",
          "original_form": "오~ 진짜 맛있어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020756.1.12",
          "form": "누가 된장찌개에 차돌박이 넣을 생각 했을까요?",
          "original_form": "누가 된장찌개에 차돌박이 넣을 생각 했을까요?",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020756.1.13",
          "form": "맞아요 누가 그랬지",
          "original_form": "맞아요 누가 그랬지",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100020756.1.14",
          "form": "그리고 유행해서 좋아용ㅎㅎ",
          "original_form": "그리고 유행해서 좋아용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100020756.1.15",
          "form": "ㅎㅎㅎ 거기에다가ㅜ밥 말아먹는 것도 최고",
          "original_form": "ㅎㅎㅎ 거기에다가ㅜ밥 말아먹는 것도 최고",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100020756.1.16",
          "form": "김치찌개도 좋아해요?",
          "original_form": "김치찌개도 좋아해요?",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100020756.1.17",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100020756.1.18",
          "form": "저 가끔 급식에 나오던 참치김치찌개 아세요? 묽은.. 그게 땡겨요ㅋㅋㅋ",
          "original_form": "저 가끔 급식에 나오던 참치김치찌개 아세요? 묽은.. 그게 땡겨요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100020756.1.19",
          "form": "아 그거 뭔지 알아요 ㅋㅋㅋ",
          "original_form": "아 그거 뭔지 알아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100020756.1.20",
          "form": "저는 참치보다는 고기 들어간게 더 좋더라구요",
          "original_form": "저는 참치보다는 고기 들어간게 더 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100020756.1.21",
          "form": "특히 삼겹살이요!",
          "original_form": "특히 삼겹살이요!",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100020756.1.22",
          "form": "하 고기 들어간것도 좋죠ㅠㅠ",
          "original_form": "하 고기 들어간것도 좋죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100020756.1.23",
          "form": "김치찌개 얚하니까 먹고싶네요",
          "original_form": "김치찌개 얚하니까 먹고싶네요",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100020756.1.24",
          "form": "내일 점심은 김치찌개 백반 각이에요",
          "original_form": "내일 점심은 김치찌개 백반 각이에요",
          "speaker_id": "2",
          "time": "20210829 23:55"
        }
      ]
    }
