      "utterance": [
        {
          "id": "MDRW2100016854.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016854.1.2",
          "form": "네 피자 먹었어요.",
          "original_form": "네 피자 먹었어요.",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016854.1.3",
          "form": "오늘 저녁 메뉴는 뭔가요",
          "original_form": "오늘 저녁 메뉴는 뭔가요",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016854.1.4",
          "form": "저는 오늘 중국음식 먹으려구요~ 깐풍기가 땡겨서요 ㅋㅋ",
          "original_form": "저는 오늘 중국음식 먹으려구요~ 깐풍기가 땡겨서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016854.1.5",
          "form": "깐풍기 맛있죠ㅎ",
          "original_form": "깐풍기 맛있죠ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016854.1.6",
          "form": "말 들으니 전 양장피 당기네요ㅎ",
          "original_form": "말 들으니 전 양장피 당기네요ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016854.1.7",
          "form": "오 양장피좋아하시는구나",
          "original_form": "오 양장피좋아하시는구나",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016854.1.8",
          "form": "톡 쏘는 맛이 좋아요",
          "original_form": "톡 쏘는 맛이 좋아요",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016854.1.9",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016854.1.10",
          "form": "그럼 짬뽕 짜장 하나둘셋?",
          "original_form": "그럼 짬뽕 짜장 하나둘셋?",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016854.1.11",
          "form": "짬뽕밥이요",
          "original_form": "짬뽕밥이요",
          "speaker_id": "1",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016854.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016854.1.13",
          "form": "오 구체적이시다!",
          "original_form": "오 구체적이시다!",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016854.1.14",
          "form": "전 짜장이요 ㅎㅎ",
          "original_form": "전 짜장이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016854.1.15",
          "form": "제가 면보다 밥파라서요ㅎ",
          "original_form": "제가 면보다 밥파라서요ㅎ",
          "speaker_id": "1",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016854.1.16",
          "form": "보통? 유니? 간짜장?",
          "original_form": "보통? 유니? 간짜장?",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016854.1.17",
          "form": "아무거나 다 좋아해요~ 짜장소스를 좋아하는거라!",
          "original_form": "아무거나 다 좋아해요~ 짜장소스를 좋아하는거라!",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016854.1.18",
          "form": "아하 짜장소스가 고소하지요",
          "original_form": "아하 짜장소스가 고소하지요",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016854.1.19",
          "form": "중국  자작면?은 이 맛이 안 나더라고요",
          "original_form": "중국  자작면?은 이 맛이 안 나더라고요",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016854.1.20",
          "form": "맞아요! 그냥 짭잘한 콩맛? 그런맛이더라구욮ㅋㅋ",
          "original_form": "맞아요! 그냥 짭잘한 콩맛? 그런맛이더라구욮ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016854.1.21",
          "form": "소스도 적어서 비벼지나 의심했어요ㅎ",
          "original_form": "소스도 적어서 비벼지나 의심했어요ㅎ",
          "speaker_id": "1",
          "time": "20210826 16:12"
        }
      ]
