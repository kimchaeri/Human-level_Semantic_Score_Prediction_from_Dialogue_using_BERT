      "utterance": [
        {
          "id": "MDRW2100005114.1.1",
          "form": "쇼핑 요즘 자제하고 있지",
          "original_form": "쇼핑 요즘 자제하고 있지",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005114.1.2",
          "form": "어 근데 칫솔 하나 살까하고 보는중",
          "original_form": "어 근데 칫솔 하나 살까하고 보는중",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005114.1.3",
          "form": "음파 진동 칫솔",
          "original_form": "음파 진동 칫솔",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005114.1.4",
          "form": "아 워터픽으로는 만족하지 못하시나요?",
          "original_form": "아 워터픽으로는 만족하지 못하시나요?",
          "speaker_id": "2",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005114.1.5",
          "form": "워터픽은 잇새를 닦아주는 거잖아",
          "original_form": "워터픽은 잇새를 닦아주는 거잖아",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005114.1.6",
          "form": "칫솔질 자체를 도와주는 음파 진동 칫솔이 있더라고",
          "original_form": "칫솔질 자체를 도와주는 음파 진동 칫솔이 있더라고",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005114.1.7",
          "form": "이를 닦아주는 용도",
          "original_form": "이를 닦아주는 용도",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005114.1.8",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005114.1.9",
          "form": "샤오미꺼 괜찮대서 보니까 한 6만원정도 하더라고",
          "original_form": "샤오미꺼 괜찮대서 보니까 한 6만원정도 하더라고",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005114.1.10",
          "form": "그 정도면 괜찮겠군",
          "original_form": "그 정도면 괜찮겠군",
          "speaker_id": "2",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005114.1.11",
          "form": "어 글고 되게 신기한게 칫솔전용 앱이 있어",
          "original_form": "어 글고 되게 신기한게 칫솔전용 앱이 있어",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005114.1.12",
          "form": "이닦을 때마다 점수줌",
          "original_form": "이닦을 때마다 점수줌",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005114.1.13",
          "form": "엥?ㅋㅋ",
          "original_form": "엥?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005114.1.14",
          "form": "신기한 상품이군요",
          "original_form": "신기한 상품이군요",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005114.1.15",
          "form": "입안을 스캔하는지",
          "original_form": "입안을 스캔하는지",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005114.1.16",
          "form": "어느 부분이 덜 닦였나",
          "original_form": "어느 부분이 덜 닦였나",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005114.1.17",
          "form": "표시해줘 ㅋㅋㅋ",
          "original_form": "표시해줘 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005114.1.18",
          "form": "요망하군",
          "original_form": "요망하군",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005114.1.19",
          "form": "21세기야 신기하지",
          "original_form": "21세기야 신기하지",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005114.1.20",
          "form": "넌 관심없어?",
          "original_form": "넌 관심없어?",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005114.1.21",
          "form": "글쎄",
          "original_form": "글쎄",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005114.1.22",
          "form": "워터픽부터 성실하게ㅋㅋ",
          "original_form": "워터픽부터 성실하게ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005114.1.23",
          "form": "ㅋㅋㅋ그래",
          "original_form": "ㅋㅋㅋ그래",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005114.1.24",
          "form": "내가 먼저 써보고 좋으면 추천할게",
          "original_form": "내가 먼저 써보고 좋으면 추천할게",
