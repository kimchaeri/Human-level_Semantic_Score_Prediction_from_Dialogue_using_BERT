        },
        {
          "id": "MDRW2100040155.1.2",
          "form": "도쿄",
          "original_form": "도쿄",
          "speaker_id": "2",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100040155.1.3",
          "form": "아키하바라는 왜 간건지 모르겠음",
          "original_form": "아키하바라는 왜 간건지 모르겠음",
          "speaker_id": "2",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100040155.1.4",
          "form": "ㅋㅋㅋ아키하바라가",
          "original_form": "ㅋㅋㅋ아키하바라가",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.5",
          "form": "지역이름임?",
          "original_form": "지역이름임?",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.6",
          "form": "약간 용산 같은 데래",
          "original_form": "약간 용산 같은 데래",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.7",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.9",
          "form": "거기 되게 신기함",
          "original_form": "거기 되게 신기함",
          "speaker_id": "2",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.10",
          "form": "노잼이었음?",
          "original_form": "노잼이었음?",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.11",
          "form": "어ㅐ",
          "original_form": "어ㅐ",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.12",
          "form": "뭔데",
          "original_form": "뭔데",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100040155.1.13",
          "form": "그 뮤ㅓ야",
          "original_form": "그 뮤ㅓ야",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040155.1.14",
          "form": "도박장이 막 그냥 있름",
          "original_form": "도박장이 막 그냥 있름",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040155.1.15",
          "form": "그냥 지나가다 한다며",
          "original_form": "그냥 지나가다 한다며",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040155.1.16",
          "form": "그냥 손쉽게 그거 보고 놀랐음",
          "original_form": "그냥 손쉽게 그거 보고 놀랐음",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040155.1.17",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040155.1.18",
          "form": "도박장?",
          "original_form": "도박장?",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040155.1.19",
          "form": "진짜 이렇게 있구나 하고",
          "original_form": "진짜 이렇게 있구나 하고",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040155.1.20",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100040155.1.21",
          "form": "막777이런거ㅡ",
          "original_form": "막777이런거ㅡ",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040155.1.22",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040155.1.23",
          "form": "동전 넣고 하는 기계",
          "original_form": "동전 넣고 하는 기계",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040155.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040155.1.25",
          "form": "뭔가신기하네",
          "original_form": "뭔가신기하네",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040155.1.26",
          "form": "너해봄?",
          "original_form": "너해봄?",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040155.1.27",
          "form": "아니 굳이",
          "original_form": "아니 굳이",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040155.1.28",
          "form": "파칭코래",
          "original_form": "파칭코래",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.30",
          "form": "돈을 그런데에 써",
          "original_form": "돈을 그런데에 써",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.31",
          "form": "들어본거같음",
          "original_form": "들어본거같음",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.32",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.33",
          "form": "운좋으면벌수도있잖아",
          "original_form": "운좋으면벌수도있잖아",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.34",
          "form": "운 안좋으면 그냥 내다 버리는 거잖아",
          "original_form": "운 안좋으면 그냥 내다 버리는 거잖아",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.35",
          "form": "그 돈으로 편의점 가서 유부 우동이나 먹어라",
          "original_form": "그 돈으로 편의점 가서 유부 우동이나 먹어라",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.36",
          "form": "그렇긴함",
          "original_form": "그렇긴함",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.37",
          "form": "ㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.38",
          "form": "겨울에갔으면",
          "original_form": "겨울에갔으면",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100040155.1.39",
          "form": "편의점에서",
          "original_form": "편의점에서",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100040155.1.40",
          "form": "어묵같은거",
          "original_form": "어묵같은거",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100040155.1.41",
          "form": "있었겠네",
          "original_form": "있었겠네",
          "speaker_id": "1",
          "time": "20210904 12:23"
        }
      ]
