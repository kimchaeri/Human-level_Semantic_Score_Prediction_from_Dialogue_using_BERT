      "utterance": [
        {
          "id": "MDRW2100045216.1.1",
          "form": "요즘 취업하기 참 어려운것 같아요",
          "original_form": "요즘 취업하기 참 어려운것 같아요",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045216.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045216.1.3",
          "form": "자격증을 따려고 하고 있긴 한데",
          "original_form": "자격증을 따려고 하고 있긴 한데",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045216.1.4",
          "form": "이게 맞는지도 모르겠어요",
          "original_form": "이게 맞는지도 모르겠어요",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045216.1.5",
          "form": "무슨 자격증 따려고 하세요 ?",
          "original_form": "무슨 자격증 따려고 하세요 ?",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045216.1.6",
          "form": "식품기사요",
          "original_form": "식품기사요",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045216.1.7",
          "form": "그런데 문제를 풀어도 풀어도 모르겠네여",
          "original_form": "그런데 문제를 풀어도 풀어도 모르겠네여",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045216.1.8",
          "form": "헐 식품기사 완전 어렵지 않나요 ?",
          "original_form": "헐 식품기사 완전 어렵지 않나요 ?",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045216.1.9",
          "form": "기사 합격률 중에 혼자 1퍼센트도 안되던데",
          "original_form": "기사 합격률 중에 혼자 1퍼센트도 안되던데",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045216.1.10",
          "form": "너무.. 어려워요...",
          "original_form": "너무.. 어려워요...",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045216.1.11",
          "form": "출제자들 다 머리에 구멍 뚫어주고 싶어요",
          "original_form": "출제자들 다 머리에 구멍 뚫어주고 싶어요",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045216.1.12",
          "form": "아 물론 새로운 바람이 불어야 한다는 뜻이예요",
          "original_form": "아 물론 새로운 바람이 불어야 한다는 뜻이예요",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045216.1.13",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045216.1.14",
          "form": "너무 멋진 생각이세요",
          "original_form": "너무 멋진 생각이세요",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045216.1.15",
          "form": "새로운 바람이 불때가 됐죠 !",
          "original_form": "새로운 바람이 불때가 됐죠 !",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045216.1.16",
          "form": "그렇죠 그렇죠",
          "original_form": "그렇죠 그렇죠",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045216.1.17",
          "form": "바람이 많이 불수록 합격률이 올라가겠죠?",
          "original_form": "바람이 많이 불수록 합격률이 올라가겠죠?",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045216.1.18",
          "form": "그랬으면 좋겠네요 바람구멍 땜질해서 지금보다 더 고이지 말았으면..",
          "original_form": "그랬으면 좋겠네요 바람구멍 땜질해서 지금보다 더 고이지 말았으면..",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045216.1.19",
          "form": "어휴 이번에는 붙었으면 좋겠네요",
          "original_form": "어휴 이번에는 붙었으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045216.1.20",
          "form": "name1님은 취업 준비 하시나요??",
          "original_form": "&name1&님은 취업 준비 하시나요??",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045216.1.21",
          "form": "네 ! 저도 취업 준비하고 있어요ㅠㅜㅜㅜ",
          "original_form": "네 ! 저도 취업 준비하고 있어요ㅠㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045216.1.22",
          "form": "헉 어떤 준비하고 계시나요?",
          "original_form": "헉 어떤 준비하고 계시나요?",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
