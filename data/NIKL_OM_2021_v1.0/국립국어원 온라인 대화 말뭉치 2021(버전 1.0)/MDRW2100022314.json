      "utterance": [
        {
          "id": "MDRW2100022314.1.1",
          "form": "AI학과 대세",
          "original_form": "AI학과 대세",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022314.1.2",
          "form": "이야기해볼까요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이야기해볼까요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022314.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 저 아는게 별로 없는 것 같은데...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저 아는게 별로 없는 것 같은데...",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022314.1.4",
          "form": "오픽 준비하는 느낌으로다가...",
          "original_form": "오픽 준비하는 느낌으로다가...",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022314.1.5",
          "form": "인공지능이",
          "original_form": "인공지능이",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022314.1.6",
          "form": "인간을 대체할 수도 있다는 이야기를 하면 되지 않을까요?",
          "original_form": "인간을 대체할 수도 있다는 이야기를 하면 되지 않을까요?",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022314.1.7",
          "form": "아 맞아요!",
          "original_form": "아 맞아요!",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022314.1.8",
          "form": "예전엔 정말 터무니 없다고 생각했는데",
          "original_form": "예전엔 정말 터무니 없다고 생각했는데",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022314.1.9",
          "form": "요즘 나오는 기계들 보면 많이 발전해서 놀라워요...",
          "original_form": "요즘 나오는 기계들 보면 많이 발전해서 놀라워요...",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022314.1.10",
          "form": "지니랑도 말이 꽤 잘통하더라고요...!",
          "original_form": "지니랑도 말이 꽤 잘통하더라고요...!",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022314.1.11",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022314.1.12",
          "form": "name2은 블루투스 스피커를 사용하시나요?!",
          "original_form": "&name2&은 블루투스 스피커를 사용하시나요?!",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022314.1.13",
          "form": "저는 한 번도 사용해본 적 없지만",
          "original_form": "저는 한 번도 사용해본 적 없지만",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022314.1.14",
          "form": "점점 발전해가는 게 느껴지긴 해요...!!!",
          "original_form": "점점 발전해가는 게 느껴지긴 해요...!!!",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022314.1.15",
          "form": "저는 말고 엄마 집에 하나가 있던데 저도 처음 써봤어요!",
          "original_form": "저는 말고 엄마 집에 하나가 있던데 저도 처음 써봤어요!",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022314.1.16",
          "form": "잘은 기억 안나는데 이름 부르면 재깍재깍 대답 하더라구요 ㅋㅋㅋ",
          "original_form": "잘은 기억 안나는데 이름 부르면 재깍재깍 대답 하더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022314.1.17",
          "form": "예전에는 핸드폰 인공지능도 음성 인식 조차도 잘 안됐는데 지금은 정말 잘되는 것 같아요...",
          "original_form": "예전에는 핸드폰 인공지능도 음성 인식 조차도 잘 안됐는데 지금은 정말 잘되는 것 같아요...",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022314.1.18",
          "form": "앗 맞아요",
          "original_form": "앗 맞아요",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022314.1.19",
          "form": "제 폰은 갤럭시라서",
          "original_form": "제 폰은 갤럭시라서",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022314.1.20",
          "form": "빅스비를 부르면",
          "original_form": "빅스비를 부르면",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022314.1.21",
          "form": "잘 대답하더라구요 ㅎㅎㅎ",
          "original_form": "잘 대답하더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022314.1.22",
          "form": "이름이 시리보다 특이해서",
          "original_form": "이름이 시리보다 특이해서",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100022314.1.23",
          "form": "엉뚱한 상황에 나오는 일도 적은거같아요",
          "original_form": "엉뚱한 상황에 나오는 일도 적은거같아요",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100022314.1.24",
          "form": "맞아요! 저도 갤럭시 사용중인데 빅스비보다는 구글이 자꾸 나와서ㅜㅜ",
          "original_form": "맞아요! 저도 갤럭시 사용중인데 빅스비보다는 구글이 자꾸 나와서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022314.1.25",
          "form": "지하철에서 계속 노래도 끊기고... 난감할때가 많더라구요",
          "original_form": "지하철에서 계속 노래도 끊기고... 난감할때가 많더라구요",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022314.1.26",
          "form": "그래서 꺼뒀어요...!",
          "original_form": "그래서 꺼뒀어요...!",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022314.1.27",
          "form": "아직까지는 익숙하지 않아서 별로 사용할 일이 없긴 한 거 같아요",
          "original_form": "아직까지는 익숙하지 않아서 별로 사용할 일이 없긴 한 거 같아요",
          "speaker_id": "2",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022314.1.28",
          "form": "앗 저도요!!!",
          "original_form": "앗 저도요!!!",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022314.1.29",
          "form": "눈치없게 구글이 반응할 때가 많아요 ㅋㅋㅋㅋ",
          "original_form": "눈치없게 구글이 반응할 때가 많아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022314.1.30",
          "form": "그리고 뭔가 민망하기도 해요",
          "original_form": "그리고 뭔가 민망하기도 해요",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022314.1.31",
          "form": "뭔가 말로 한다는게?!",
          "original_form": "뭔가 말로 한다는게?!",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022314.1.32",
          "form": "맞아요! 말로 하는게 편할때는 방에 누워서 불꺼줘! 이런건데",
          "original_form": "맞아요! 말로 하는게 편할때는 방에 누워서 불꺼줘! 이런건데",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022314.1.33",
          "form": "정작 그런건 안되고...",
          "original_form": "정작 그런건 안되고...",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022314.1.34",
          "form": "저는 전화거는 것도 힘들어요 ㅋㅋㅋㅋ 대상을 잘 못찾더라고요 빅스비가...",
          "original_form": "저는 전화거는 것도 힘들어요 ㅋㅋㅋㅋ 대상을 잘 못찾더라고요 빅스비가...",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022314.1.35",
          "form": "아잌ㅋㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022314.1.36",
          "form": "빅스비가 아직 그건 서툴군요",
          "original_form": "빅스비가 아직 그건 서툴군요",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022314.1.37",
          "form": "인공지능 때문에 일자리가 줄어들 수도 있다는데",
          "original_form": "인공지능 때문에 일자리가 줄어들 수도 있다는데",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022314.1.38",
          "form": "아직까진 크게 걱정 안해도 되겠어요 ^~^",
          "original_form": "아직까진 크게 걱정 안해도 되겠어요 ^~^",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100022314.1.39",
          "form": "맞아요 그래도 사람만 할 수 있는 일이 있죠!",
          "original_form": "맞아요 그래도 사람만 할 수 있는 일이 있죠!",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022314.1.40",
          "form": "인공 지능이 어디까지 흉내낼 수 있는지 궁금하긴 하지만 한계가 있을거라고 생각해요...",
          "original_form": "인공 지능이 어디까지 흉내낼 수 있는지 궁금하긴 하지만 한계가 있을거라고 생각해요...",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022314.1.41",
          "form": "자비스처럼 탁탁탁 다 해주면 아마 암기 공부는 안해도 되는 시대가 오지 않을까요...",
          "original_form": "자비스처럼 탁탁탁 다 해주면 아마 암기 공부는 안해도 되는 시대가 오지 않을까요...",
          "speaker_id": "2",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022314.1.42",
          "form": "우와 자비스...!!!!",
          "original_form": "우와 자비스...!!!!",
          "speaker_id": "1",
