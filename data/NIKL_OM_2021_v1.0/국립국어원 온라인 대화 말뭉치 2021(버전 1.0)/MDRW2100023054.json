        {
          "id": "MDRW2100023054.1.1",
          "form": "김밥에 들어가는 재료 소박한데 맛있지 ㅎㅎ",
          "original_form": "김밥에 들어가는 재료 소박한데 맛있지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100023054.1.2",
          "form": "ㅇㅇ 진짜 들어가는 것도 없어. 단무지 당근 햄 시금치 이정도?",
          "original_form": "ㅇㅇ 진짜 들어가는 것도 없어. 단무지 당근 햄 시금치 이정도?",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023054.1.3",
          "form": "참기름을 좋은 거 쓰시나 엄청 고소하더라",
          "original_form": "참기름을 좋은 거 쓰시나 엄청 고소하더라",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023054.1.4",
          "form": "밥도 고슬고슬 맛있고",
          "original_form": "밥도 고슬고슬 맛있고",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023054.1.5",
          "form": "쫄면 위에 얹어 먹으면 맛있겠네",
          "original_form": "쫄면 위에 얹어 먹으면 맛있겠네",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023054.1.6",
          "form": "김밥 위에 쫄면을 얹어야지 ㅋㅋ",
          "original_form": "김밥 위에 쫄면을 얹어야지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023054.1.7",
          "form": "뭐가 달라? 그게 그거지 ㅎㅎ",
          "original_form": "뭐가 달라? 그게 그거지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023054.1.8",
          "form": "어떻게 먹어도 다 맛있음",
          "original_form": "어떻게 먹어도 다 맛있음",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023054.1.9",
          "form": "아 계속 말하니 배고파",
          "original_form": "아 계속 말하니 배고파",
          "speaker_id": "2",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023054.1.10",
          "form": "아까 라면 먹었잖아",
          "original_form": "아까 라면 먹었잖아",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023054.1.11",
          "form": "애들 다 나눠주고 난 얼마 안먹어서 ㅠ",
          "original_form": "애들 다 나눠주고 난 얼마 안먹어서 ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023054.1.12",
          "form": "ㅎㅎ 충분히 좀 먹지~~",
          "original_form": "ㅎㅎ 충분히 좀 먹지~~",
