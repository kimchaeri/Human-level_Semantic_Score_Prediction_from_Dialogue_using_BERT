      "utterance": [
        {
          "id": "MDRW2100035039.1.1",
          "form": "name2님 오늘 저녁뭐드셨었나요",
          "original_form": "&name2&님 오늘 저녁뭐드셨었나요",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100035039.1.2",
          "form": "이미 너무 옛날이라 기억아지 않습니다 ㅋㅋ 전 지금 샌드위치를 먹고 있습니다",
          "original_form": "이미 너무 옛날이라 기억아지 않습니다 ㅋㅋ 전 지금 샌드위치를 먹고 있습니다",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100035039.1.3",
          "form": "name1님은 식사하셨나요?",
          "original_form": "&name1&님은 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100035039.1.4",
          "form": "전 아점으로 간단하게먹었어요 ㅋㅋㅋㅋ계란이랑 장조림으로",
          "original_form": "전 아점으로 간단하게먹었어요 ㅋㅋㅋㅋ계란이랑 장조림으로",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035039.1.5",
          "form": "어떤샌드위치 먹었어요?",
          "original_form": "어떤샌드위치 먹었어요?",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035039.1.6",
          "form": "아보카도를 추가하면 배가 불러서 쿠키도 콜라도 생략해요",
          "original_form": "아보카도를 추가하면 배가 불러서 쿠키도 콜라도 생략해요",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035039.1.7",
          "form": "생수랑 샌드위치를 먹었습니다",
          "original_form": "생수랑 샌드위치를 먹었습니다",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035039.1.8",
          "form": "어 말 하나가 안갔네요",
          "original_form": "어 말 하나가 안갔네요",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035039.1.9",
          "form": "서브웨이 비엘티에",
          "original_form": "서브웨이 비엘티에",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035039.1.10",
          "form": "아보카도 추가요",
          "original_form": "아보카도 추가요",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100035039.1.11",
          "form": "서브웨이는 식사되져ㅎㅎ알차서",
          "original_form": "서브웨이는 식사되져ㅎㅎ알차서",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035039.1.12",
          "form": "서브웨이에서 비엘티 젤 좋아하시나요",
          "original_form": "서브웨이에서 비엘티 젤 좋아하시나요",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035039.1.13",
          "form": "그 때 그 때 달라요 ㅋㅋ터키도 좋고 에그도 좋고",
          "original_form": "그 때 그 때 달라요 ㅋㅋ터키도 좋고 에그도 좋고",
          "speaker_id": "2",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100035039.1.14",
          "form": "황소가 일을 잘한다고 하면",
          "original_form": "황소가 일을 잘한다고 하면",
          "speaker_id": "2",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100035039.1.15",
          "form": "까만소가 서운해하듯",
          "original_form": "까만소가 서운해하듯",
          "speaker_id": "2",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100035039.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그쳐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그쳐",
          "speaker_id": "1",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035039.1.17",
          "form": "비엘티를 좋다고 하면",
          "original_form": "비엘티를 좋다고 하면",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035039.1.18",
          "form": "미트볼과 햄이 슬퍼해요",
          "original_form": "미트볼과 햄이 슬퍼해요",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035039.1.19",
          "form": "얘기를 해야되니깐",
          "original_form": "얘기를 해야되니깐",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035039.1.20",
          "form": "별 헛소리를 지금 제가 다하는데ㅋㅋ",
          "original_form": "별 헛소리를 지금 제가 다하는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035039.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ애정이보입니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ애정이보입니다",
          "speaker_id": "1",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035039.1.22",
          "form": "ㅋㅋㅋㅋ좋게 해석해주시네욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ좋게 해석해주시네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035039.1.23",
          "form": "그리고 서브웨이 메뉴는 할인메뉴가 매달 바뀌어서",
          "original_form": "그리고 서브웨이 메뉴는 할인메뉴가 매달 바뀌어서",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035039.1.24",
          "form": "할인메뉴를 고를때가 많아요",
          "original_form": "할인메뉴를 고를때가 많아요",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035039.1.25",
          "form": "name2님 음식철학있으신것같아여 음식열정 본받고싶어요 ㅋㅋㅋㅋ",
          "original_form": "&name2&님 음식철학있으신것같아여 음식열정 본받고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035039.1.26",
          "form": "맞아요 저도 그때그때 할인하는거위주로 사먹어요",
          "original_form": "맞아요 저도 그때그때 할인하는거위주로 사먹어요",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035039.1.27",
          "form": "ㅋㅋ얘기해야 되니깐 음식얘길 많이 하는 거긴 한데 ㅋㅋ 말하가보니 철학이 생기네요",
          "original_form": "ㅋㅋ얘기해야 되니깐 음식얘길 많이 하는 거긴 한데 ㅋㅋ 말하가보니 철학이 생기네요",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100035039.1.28",
          "form": "아 좀 덜 선호해는건 닭  과숨살이요 이게 매장마다 다른데",
          "original_form": "아 좀 덜 선호해는건 닭  과숨살이요 이게 매장마다 다른데",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035039.1.29",
          "form": "뜨뜻하게 안데워주거나 포장하면 닭괏슘이 다시 식어서 퍽퍽해요",
          "original_form": "뜨뜻하게 안데워주거나 포장하면 닭괏슘이 다시 식어서 퍽퍽해요",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035039.1.30",
          "form": "아 맞아요ㅜ 닭가슴살은 또 식으면 맛이없어서ㅜㅜ",
          "original_form": "아 맞아요ㅜ 닭가슴살은 또 식으면 맛이없어서ㅜㅜ",
