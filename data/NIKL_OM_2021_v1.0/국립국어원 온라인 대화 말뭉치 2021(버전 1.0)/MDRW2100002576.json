      "utterance": [
        {
          "id": "MDRW2100002576.1.1",
          "form": "요즘도 일 때문에 스트레스 많이 받아?ㅜ",
          "original_form": "요즘도 일 때문에 스트레스 많이 받아?ㅜ",
          "speaker_id": "2",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002576.1.2",
          "form": "응 항상 퇴사하고싶지",
          "original_form": "응 항상 퇴사하고싶지",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002576.1.3",
          "form": "너는어때",
          "original_form": "너는어때",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002576.1.4",
          "form": "나 스트레스 많이 받아서 퇴사했어ㅠㅜ",
          "original_form": "나 스트레스 많이 받아서 퇴사했어ㅠㅜ",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002576.1.5",
          "form": "갠차너??",
          "original_form": "갠차너??",
          "speaker_id": "1",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002576.1.6",
          "form": "헉 전에 탈모오고그런다더니",
          "original_form": "헉 전에 탈모오고그런다더니",
          "speaker_id": "1",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002576.1.7",
          "form": "어,,,진짜 못 버티겠어서 사직서 냈어ㅠㅜ",
          "original_form": "어,,,진짜 못 버티겠어서 사직서 냈어ㅠㅜ",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002576.1.8",
          "form": "실업급여는???",
          "original_form": "실업급여는???",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002576.1.9",
          "form": "진짜 일할 때 상사랑 잘 맞는지가 엄청 중요한 것 같음ㅜ",
          "original_form": "진짜 일할 때 상사랑 잘 맞는지가 엄청 중요한 것 같음ㅜ",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002576.1.10",
          "form": "실업급여 신청하려고 서류 준비 중이야ㅜ",
          "original_form": "실업급여 신청하려고 서류 준비 중이야ㅜ",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002576.1.11",
          "form": "일은 힘들어도돼",
          "original_form": "일은 힘들어도돼",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002576.1.12",
          "form": "사람이힘들면안돼",
          "original_form": "사람이힘들면안돼",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002576.1.13",
          "form": "맞아ㅠ일은 진짜 적응이 되는데 사람은 그게 안되더라ㅋㅋㅋ",
          "original_form": "맞아ㅠ일은 진짜 적응이 되는데 사람은 그게 안되더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002576.1.14",
          "form": "너 요새 일은 좀 어때?",
          "original_form": "너 요새 일은 좀 어때?",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002576.1.15",
          "form": "곧 추석인데 또 엄청 바빠지지?ㅜ",
          "original_form": "곧 추석인데 또 엄청 바빠지지?ㅜ",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002576.1.16",
          "form": "엄청 바쁘지...지금은 지금",
          "original_form": "엄청 바쁘지...지금은 지금",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002576.1.17",
          "form": "즐기기전이야",
          "original_form": "즐기기전이야",
          "speaker_id": "1",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002576.1.18",
          "form": "ㅋㅋㅋ이번에도 집 가서 일하고 그래야해?ㅜ",
          "original_form": "ㅋㅋㅋ이번에도 집 가서 일하고 그래야해?ㅜ",
          "speaker_id": "2",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002576.1.19",
          "form": "아니 아직은",
          "original_form": "아니 아직은",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002576.1.20",
          "form": "명절땨는 집에서일할듯",
          "original_form": "명절땨는 집에서일할듯",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002576.1.21",
          "form": "아휴 진짜 고생이다ㅜ",
          "original_form": "아휴 진짜 고생이다ㅜ",
          "speaker_id": "2",
          "time": "20210820 14:33"
        }
      ]
