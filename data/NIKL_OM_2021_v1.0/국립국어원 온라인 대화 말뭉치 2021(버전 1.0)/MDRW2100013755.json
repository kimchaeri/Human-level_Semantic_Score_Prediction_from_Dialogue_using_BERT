      "utterance": [
        {
          "id": "MDRW2100013755.1.1",
          "form": "맛있겠네 나도",
          "original_form": "맛있겠네 나도",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013755.1.2",
          "form": "사줭~",
          "original_form": "사줭~",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013755.1.3",
          "form": "만들어 먹으면 되지",
          "original_form": "만들어 먹으면 되지",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013755.1.4",
          "form": "오호?",
          "original_form": "오호?",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013755.1.5",
          "form": "만들어 줄까",
          "original_form": "만들어 줄까",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013755.1.6",
          "form": "옹옹~ 오징어나 해산물 넣어서 ㅋ",
          "original_form": "옹옹~ 오징어나 해산물 넣어서 ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013755.1.7",
          "form": "해산물이 없다",
          "original_form": "해산물이 없다",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013755.1.8",
          "form": "그러면 그냥 ㅋㅋ",
          "original_form": "그러면 그냥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013755.1.9",
          "form": "그냥 파 넣고 맛있게 해 줄게",
          "original_form": "그냥 파 넣고 맛있게 해 줄게",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013755.1.10",
          "form": "옹옹 밀가루 넘 많지 않게",
          "original_form": "옹옹 밀가루 넘 많지 않게",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013755.1.11",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013755.1.12",
          "form": "바삭바삭하게!",
          "original_form": "바삭바삭하게!",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013755.1.13",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013755.1.14",
          "form": "빨리 먹고싶다 냠냠",
          "original_form": "빨리 먹고싶다 냠냠",
