        },
        {
          "id": "MDRW2100045761.1.5",
          "form": "데스각 ㅎㅎ",
          "original_form": "데스각 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045761.1.6",
          "form": "설마 먹진 않겠죠",
          "original_form": "설마 먹진 않겠죠",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045761.1.7",
          "form": "마지막 음식인가 봐요",
          "original_form": "마지막 음식인가 봐요",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045761.1.8",
          "form": "음식 어쩌실려고 그러는지",
          "original_form": "음식 어쩌실려고 그러는지",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045761.1.9",
          "form": "냉동실에도 있긴 했네요",
          "original_form": "냉동실에도 있긴 했네요",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045761.1.10",
          "form": "요리는 안되나",
          "original_form": "요리는 안되나",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045761.1.11",
          "form": "되던데 전기가 없나요",
          "original_form": "되던데 전기가 없나요",
          "speaker_id": "2",
          "time": "20210908 06:49"
        },
        {
          "id": "MDRW2100045761.1.12",
          "form": "발전기도 필요한가 봐요",
