      "utterance": [
        {
          "id": "MDRW2100017219.1.1",
          "form": "싱글대디인데",
          "original_form": "싱글대디인데",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017219.1.2",
          "form": "거기서 겪는 어려움",
          "original_form": "거기서 겪는 어려움",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017219.1.3",
          "form": "이런 거 볼 수 있어 좋았어요",
          "original_form": "이런 거 볼 수 있어 좋았어요",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017219.1.4",
          "form": "아이랑 같이 성장하는 영화군요",
          "original_form": "아이랑 같이 성장하는 영화군요",
          "speaker_id": "1",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017219.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017219.1.6",
          "form": "재밌을거같아요",
          "original_form": "재밌을거같아요",
          "speaker_id": "1",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017219.1.7",
          "form": "요즘 육아에 관심이 많아서",
          "original_form": "요즘 육아에 관심이 많아서",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017219.1.8",
          "form": "육아 영화 본 것 같아요",
          "original_form": "육아 영화 본 것 같아요",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017219.1.9",
          "form": "예전같으면 안봤을텐데 ㅋㅋㅋㅋ",
          "original_form": "예전같으면 안봤을텐데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017219.1.10",
          "form": "아 아이가 있으세쇼?",
          "original_form": "아 아이가 있으세쇼?",
          "speaker_id": "1",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017219.1.11",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017219.1.12",
          "form": "제가 곧 서른이라",
          "original_form": "제가 곧 서른이라",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017219.1.13",
          "form": "결혼 육아 주제에 관심이 많아져서요",
          "original_form": "결혼 육아 주제에 관심이 많아져서요",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017219.1.14",
          "form": "아.. 그러시구나",
          "original_form": "아.. 그러시구나",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017219.1.15",
          "form": "그래서 육아 영상들 좀 봐요",
          "original_form": "그래서 육아 영상들 좀 봐요",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017219.1.16",
          "form": "나는 저것을 원하는가 마는가",
          "original_form": "나는 저것을 원하는가 마는가",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017219.1.17",
          "form": "생각해보려고 ㅎㅎㅎ",
          "original_form": "생각해보려고 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017219.1.18",
          "form": "name1님은 이십대 중반이시죠?",
          "original_form": "&name1&님은 이십대 중반이시죠?",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017219.1.19",
          "form": "슈 돌 같은 것도 보시겠어요",
          "original_form": "슈 돌 같은 것도 보시겠어요",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017219.1.20",
          "form": "저 초반이에요",
          "original_form": "저 초반이에요",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017219.1.21",
          "form": "그래요 초반일 것 같았어요",
          "original_form": "그래요 초반일 것 같았어요",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017219.1.22",
          "form": "아하하  티가나던가요",
          "original_form": "아하하  티가나던가요",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017219.1.23",
          "form": "직장인이라그래서 중반으로 올렸을 뿐...",
          "original_form": "직장인이라그래서 중반으로 올렸을 뿐...",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017219.1.24",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:09"
        }
