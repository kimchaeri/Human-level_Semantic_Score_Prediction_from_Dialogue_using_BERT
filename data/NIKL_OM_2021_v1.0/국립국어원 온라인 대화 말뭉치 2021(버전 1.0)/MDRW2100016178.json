      "utterance": [
        {
          "id": "MDRW2100016178.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100016178.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016178.1.3",
          "form": "운동 하시나요",
          "original_form": "운동 하시나요",
          "speaker_id": "1",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016178.1.4",
          "form": "코로나전에 수영했어요!",
          "original_form": "코로나전에 수영했어요!",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016178.1.5",
          "form": "수영 하셨구나 너무 멋지시네요",
          "original_form": "수영 하셨구나 너무 멋지시네요",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016178.1.6",
          "form": "수영은 어떤 점이 가장 좋았나요?",
          "original_form": "수영은 어떤 점이 가장 좋았나요?",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016178.1.7",
          "form": "저는 수영을 못해서ㅎ",
          "original_form": "저는 수영을 못해서ㅎ",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016178.1.8",
          "form": "수영은 일단 땀이 안나서 좋아요ㅋㅋ",
          "original_form": "수영은 일단 땀이 안나서 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016178.1.9",
          "form": "아니 땀은 나긴하는데 그다지 신경 안쓰인달까요",
          "original_form": "아니 땀은 나긴하는데 그다지 신경 안쓰인달까요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016178.1.10",
          "form": "하긴 물속에서 하는 스포츠니까",
          "original_form": "하긴 물속에서 하는 스포츠니까",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016178.1.11",
          "form": "아 수영도 땀이나요?",
          "original_form": "아 수영도 땀이나요?",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016178.1.12",
          "form": "몰랐던 사실 ㄴㅇㄱ",
          "original_form": "몰랐던 사실 ㄴㅇㄱ",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016178.1.13",
          "form": "ㅋㅋㅋ그냥 물에 씻겨내려가는거 아닐까요",
          "original_form": "ㅋㅋㅋ그냥 물에 씻겨내려가는거 아닐까요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016178.1.14",
          "form": "저도 잘ㅋㅋ",
          "original_form": "저도 잘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016178.1.15",
          "form": "하여튼 하고나면 밥도 많이 먹고",
          "original_form": "하여튼 하고나면 밥도 많이 먹고",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016178.1.16",
          "form": "기초대사가 활발해지는 느낌?입니다ㅋㅋ",
          "original_form": "기초대사가 활발해지는 느낌?입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016178.1.17",
          "form": "수영은 언제부터 배우신건가요",
          "original_form": "수영은 언제부터 배우신건가요",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016178.1.18",
          "form": "이건 스토리가 있습니당",
          "original_form": "이건 스토리가 있습니당",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016178.1.19",
          "form": "네 들려주시죠",
          "original_form": "네 들려주시죠",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016178.1.20",
          "form": "어떤 스토리입니까",
          "original_form": "어떤 스토리입니까",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016178.1.21",
          "form": "태국 여행갔을때 크루저?같은거 타고 바다로 나갔거든요",
          "original_form": "태국 여행갔을때 크루저?같은거 타고 바다로 나갔거든요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016178.1.22",
          "form": "바다 한가운데 정박하고 방송으로 놀라고하시는데",
          "original_form": "바다 한가운데 정박하고 방송으로 놀라고하시는데",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016178.1.23",
          "form": "다들 뛰어들어서 노시더라고요",
          "original_form": "다들 뛰어들어서 노시더라고요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016178.1.24",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016178.1.25",
          "form": "(흥미진진)",
          "original_form": "(흥미진진)",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016178.1.26",
          "form": "근데 딱 보니 서양인들은 수영할줄아는데",
          "original_form": "근데 딱 보니 서양인들은 수영할줄아는데",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016178.1.27",
          "form": "동양인들만 멀뚱멀뚱...",
          "original_form": "동양인들만 멀뚱멀뚱...",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016178.1.28",
          "form": "음~ 못하셨구나",
          "original_form": "음~ 못하셨구나",
          "speaker_id": "1",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016178.1.29",
          "form": "그래서 제가 용기내서",
          "original_form": "그래서 제가 용기내서",
          "speaker_id": "2",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016178.1.30",
          "form": "점프했는데 수영 못 하니까",
          "original_form": "점프했는데 수영 못 하니까",
          "speaker_id": "2",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016178.1.31",
          "form": "당연히 허우적하다 가라앉죠ㅋㅋ",
          "original_form": "당연히 허우적하다 가라앉죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016178.1.32",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016178.1.33",
          "form": "위험했네요",
          "original_form": "위험했네요",
          "speaker_id": "1",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016178.1.34",
          "form": "그 계기로 배우시게되신건가오",
          "original_form": "그 계기로 배우시게되신건가오",
          "speaker_id": "1",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016178.1.35",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016178.1.36",
          "form": "서양아재가",
          "original_form": "서양아재가",
          "speaker_id": "2",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016178.1.37",
          "form": "구해주면서",
          "original_form": "구해주면서",
          "speaker_id": "2",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016178.1.38",
          "form": "수영배우라고하셨어요ㅋㅋ",
          "original_form": "수영배우라고하셨어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:22"
        }
      ]
