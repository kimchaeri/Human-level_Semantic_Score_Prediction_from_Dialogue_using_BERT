        },
        {
          "id": "MDRW2100042857.1.2",
          "form": "애같이 살지 ㅎㅎㅎ",
          "original_form": "애같이 살지 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042857.1.3",
          "form": "그러니깐 ㅋㅋㅋ",
          "original_form": "그러니깐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042857.1.4",
          "form": "우리집은 티비때문에 싸울때",
          "original_form": "우리집은 티비때문에 싸울때",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042857.1.5",
          "form": "신랑한테 그거 팝콘뇌얘기하면서 나두 ㅋㅋㅋ",
          "original_form": "신랑한테 그거 팝콘뇌얘기하면서 나두 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042857.1.6",
          "form": "이제 우리도 폰을 애들앞에서는 덜보고, 티비는 아에 꺼놓고 있자",
          "original_form": "이제 우리도 폰을 애들앞에서는 덜보고, 티비는 아에 꺼놓고 있자",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042857.1.7",
          "form": "하니깐 알겠때",
          "original_form": "하니깐 알겠때",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042857.1.8",
          "form": "어디 있노?? 애들 재우는데 혼자 폰 보고있음",
          "original_form": "어디 있노?? 애들 재우는데 혼자 폰 보고있음",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042857.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042857.1.10",
          "form": "그래 진짜 잘때",
          "original_form": "그래 진짜 잘때",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042857.1.11",
          "form": "이불 뒤집어쓰고",
          "original_form": "이불 뒤집어쓰고",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
