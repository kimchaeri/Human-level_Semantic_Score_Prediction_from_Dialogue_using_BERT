          "id": "MDRW2100042857.1.12",
          "form": "그러니깐은",
          "original_form": "그러니깐은",
          "speaker_id": "1",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042857.1.13",
          "form": "그것도 게임.",
          "original_form": "그것도 게임.",
          "speaker_id": "1",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042857.1.14",
