      "utterance": [
        {
          "id": "MDRW2100030983.1.1",
          "form": "날씨와 계절이라니까 사회과 부도같잖아",
          "original_form": "날씨와 계절이라니까 사회과 부도같잖아",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030983.1.2",
          "form": "집안 온도 어때",
          "original_form": "집안 온도 어때",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100030983.1.3",
          "form": "요즘 그래도 시원해지지않았냐",
          "original_form": "요즘 그래도 시원해지지않았냐",
          "speaker_id": "2",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100030983.1.4",
          "form": "완전히 가을날씨 다 됐어",
          "original_form": "완전히 가을날씨 다 됐어",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100030983.1.5",
          "form": "입추지나고 나서 좀 시원해지더니 이제 살만하더라",
          "original_form": "입추지나고 나서 좀 시원해지더니 이제 살만하더라",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100030983.1.6",
          "form": "어제 그 뭐지",
          "original_form": "어제 그 뭐지",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100030983.1.7",
          "form": "이런 엄청 신기하게 절기 딱 맞게 날씨가 변하는거 같애",
          "original_form": "이런 엄청 신기하게 절기 딱 맞게 날씨가 변하는거 같애",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100030983.1.8",
          "form": "타코벨 가는데 원래 좀 멀거든 근데 적당히 더우니 갔다올만 했어",
          "original_form": "타코벨 가는데 원래 좀 멀거든 근데 적당히 더우니 갔다올만 했어",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100030983.1.9",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100030983.1.10",
          "form": "요즘 외출하기 좋은 날씨인거 같자너",
          "original_form": "요즘 외출하기 좋은 날씨인거 같자너",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100030983.1.11",
          "form": "이번 가을은 길겠지?",
          "original_form": "이번 가을은 길겠지?",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100030983.1.12",
          "form": "지난주까지 엄청 습했는데",
          "original_form": "지난주까지 엄청 습했는데",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100030983.1.13",
          "form": "제발 그랬음 좋겠는데 금방 겨울될거 같기도 함ㅋㅋ",
          "original_form": "제발 그랬음 좋겠는데 금방 겨울될거 같기도 함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030983.1.14",
          "form": "아 진짜.. 비가 갑자기 내리더니...이대로 늦은 장마돌입인가했는데 더 안와서 다행인지 불행인지",
          "original_form": "아 진짜.. 비가 갑자기 내리더니...이대로 늦은 장마돌입인가했는데 더 안와서 다행인지 불행인지",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030983.1.15",
          "form": "글게",
          "original_form": "글게",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030983.1.16",
          "form": "가을을 즐기고싶다",
          "original_form": "가을을 즐기고싶다",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030983.1.17",
          "form": "마저",
          "original_form": "마저",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030983.1.18",
          "form": "근데 나 가을옷이 없다",
          "original_form": "근데 나 가을옷이 없다",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030983.1.19",
          "form": "작년에 봄가을 엄청 짧앗엇자나",
          "original_form": "작년에 봄가을 엄청 짧앗엇자나",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030983.1.20",
          "form": "그래서 옷을 안샀더니....ㅋㅋㅋ옷이 없어",
          "original_form": "그래서 옷을 안샀더니....ㅋㅋㅋ옷이 없어",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030983.1.21",
          "form": "응... 하지만 그 짧은 사이에 고통을 겪고 나니 간절기옷에 대한 간절함이 장난아님",
          "original_form": "응... 하지만 그 짧은 사이에 고통을 겪고 나니 간절기옷에 대한 간절함이 장난아님",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030983.1.22",
          "form": "아 진짜 근데 나도",
          "original_form": "아 진짜 근데 나도",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030983.1.23",
          "form": "외투하나만 제대로 된 간절기고",
          "original_form": "외투하나만 제대로 된 간절기고",
          "speaker_id": "2",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100030983.1.24",
          "form": "맞어 우리나라 작년까지 간절기 엄청 짧았자나",
          "original_form": "맞어 우리나라 작년까지 간절기 엄청 짧았자나",
          "speaker_id": "1",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100030983.1.25",
          "form": "너무 짧아서 제대로 된거 사면 머쓱함",
          "original_form": "너무 짧아서 제대로 된거 사면 머쓱함",
          "speaker_id": "2",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100030983.1.26",
          "form": "막상 봄가을옷 사도 며칠 입지도 못한다고",
          "original_form": "막상 봄가을옷 사도 며칠 입지도 못한다고",
          "speaker_id": "1",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100030983.1.27",
          "form": "ㅇㅇ...이제 우린... 4계절이 뚜렷하지 않아...",
          "original_form": "ㅇㅇ...이제 우린... 4계절이 뚜렷하지 않아...",
          "speaker_id": "2",
          "time": "20210904 15:13"
        }
      ]
    }
