      "utterance": [
        {
          "id": "MDRW2100002287.1.1",
          "form": "안녕하세요 ~ 뮤지컬 좋아하시나요?",
          "original_form": "안녕하세요 ~ 뮤지컬 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 23:01"
        },
        {
          "id": "MDRW2100002287.1.2",
          "form": "좋아하죠 근데 볼 기회가 거의 없네요",
          "original_form": "좋아하죠 근데 볼 기회가 거의 없네요",
          "speaker_id": "2",
          "time": "20210821 00:14"
        },
        {
          "id": "MDRW2100002287.1.3",
          "form": "맞아요 요즘 코로나때문에 더 그렇죠 저는 조승우님의 헤드윅을 정말 보고싶은데 티켓팅이 장난 아니라 하더라고요",
          "original_form": "맞아요 요즘 코로나때문에 더 그렇죠 저는 조승우님의 헤드윅을 정말 보고싶은데 티켓팅이 장난 아니라 하더라고요",
          "speaker_id": "1",
          "time": "20210821 00:15"
        },
        {
          "id": "MDRW2100002287.1.4",
          "form": "조승우 배우 좋아요",
          "original_form": "조승우 배우 좋아요",
          "speaker_id": "2",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100002287.1.5",
          "form": "연기도 잘하고 멋져요 저두 보고 싶네요",
          "original_form": "연기도 잘하고 멋져요 저두 보고 싶네요",
          "speaker_id": "2",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100002287.1.6",
          "form": "저는 중학생때 신성록씨 뮤지컬 보고 반해서",
          "original_form": "저는 중학생때 신성록씨 뮤지컬 보고 반해서",
          "speaker_id": "1",
          "time": "20210821 00:17"
        },
        {
          "id": "MDRW2100002287.1.7",
          "form": "몇번이나 보러갔었어요 ㅋㅋㅋㅋ그때는 뜨기 전이었는데",
          "original_form": "몇번이나 보러갔었어요 ㅋㅋㅋㅋ그때는 뜨기 전이었는데",
          "speaker_id": "1",
          "time": "20210821 00:17"
        },
        {
          "id": "MDRW2100002287.1.8",
          "form": "일년에 몇번정도 보세요?",
          "original_form": "일년에 몇번정도 보세요?",
          "speaker_id": "2",
          "time": "20210821 00:18"
        },
        {
          "id": "MDRW2100002287.1.9",
          "form": "예전에는 자주 봤는데 요즘에는 거의 안봐요 일하면서 흥미가 떨어졌어요 ㅠㅠ",
          "original_form": "예전에는 자주 봤는데 요즘에는 거의 안봐요 일하면서 흥미가 떨어졌어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:19"
        },
        {
          "id": "MDRW2100002287.1.10",
          "form": "시국이 이래서 볼기회도 줄어든것 같긴해요",
          "original_form": "시국이 이래서 볼기회도 줄어든것 같긴해요",
          "speaker_id": "2",
          "time": "20210821 00:19"
        },
        {
          "id": "MDRW2100002287.1.11",
          "form": "맞아요 그래서 더 안찾게 돼요",
          "original_form": "맞아요 그래서 더 안찾게 돼요",
          "speaker_id": "1",
          "time": "20210821 00:20"
        },
        {
          "id": "MDRW2100002287.1.12",
          "form": "영화도 예전보다 훨씬 조금봐요",
          "original_form": "영화도 예전보다 훨씬 조금봐요",
          "speaker_id": "1",
          "time": "20210821 00:20"
        },
        {
          "id": "MDRW2100002287.1.13",
          "form": "저두요 원래 한달에 한번은봤는데",
          "original_form": "저두요 원래 한달에 한번은봤는데",
          "speaker_id": "2",
          "time": "20210821 00:20"
        },
        {
          "id": "MDRW2100002287.1.14",
          "form": "코로나때문에 가까운 극장도 문을 닫았더라고요 ㅠㅠ",
          "original_form": "코로나때문에 가까운 극장도 문을 닫았더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:21"
        },
        {
          "id": "MDRW2100002287.1.15",
          "form": "그럼 더 기회가 없겠네요",
          "original_form": "그럼 더 기회가 없겠네요",
          "speaker_id": "2",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100002287.1.16",
          "form": "요즘 집에서 많이봐요",
          "original_form": "요즘 집에서 많이봐요",
          "speaker_id": "2",
          "time": "20210821 00:23"
        },
        {
          "id": "MDRW2100002287.1.17",
          "form": "더 편한것 같기도하고",
          "original_form": "더 편한것 같기도하고",
          "speaker_id": "2",
          "time": "20210821 00:23"
        },
        {
          "id": "MDRW2100002287.1.18",
          "form": "저도요 ! 구매해서 집에서 보게 돼요",
          "original_form": "저도요 ! 구매해서 집에서 보게 돼요",
          "speaker_id": "1",
          "time": "20210821 00:23"
        },
        {
          "id": "MDRW2100002287.1.19",
          "form": "영화관에서 음식물 섭취도 안돼서 더 그래요",
          "original_form": "영화관에서 음식물 섭취도 안돼서 더 그래요",
          "speaker_id": "1",
          "time": "20210821 00:23"
        },
        {
          "id": "MDRW2100002287.1.20",
          "form": "모가디슈 보셨어요?",
          "original_form": "모가디슈 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100002287.1.21",
          "form": "재밌던데",
          "original_form": "재밌던데",
          "speaker_id": "2",
          "time": "20210821 00:24"
        }
      ]
    }
