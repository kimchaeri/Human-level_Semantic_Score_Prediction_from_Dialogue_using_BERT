      "utterance": [
        {
          "id": "MDRW2100004729.1.1",
          "form": "이전대화 까먹었어ㅋㅋㅋ",
          "original_form": "이전대화 까먹었어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004729.1.2",
          "form": "무슨 얘길하고있었더라",
          "original_form": "무슨 얘길하고있었더라",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004729.1.3",
          "form": "여름밤 여행?",
          "original_form": "여름밤 여행?",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004729.1.4",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004729.1.5",
          "form": "ㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004729.1.6",
          "form": "디즈니랜드에 가고싶어",
          "original_form": "디즈니랜드에 가고싶어",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004729.1.7",
          "form": "환상의 디즈니랜드...",
          "original_form": "환상의 디즈니랜드...",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004729.1.8",
          "form": "한국에도 디즈니랜드 들어와주라",
          "original_form": "한국에도 디즈니랜드 들어와주라",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004729.1.9",
          "form": "부산에 유치 기원합니다",
          "original_form": "부산에 유치 기원합니다",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004729.1.10",
          "form": "부산에 새로 생긴다던 유원지는 아직 소식이 없는가?",
          "original_form": "부산에 새로 생긴다던 유원지는 아직 소식이 없는가?",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004729.1.11",
          "form": "아 제발 부산에도 놀이공원 제발!",
          "original_form": "아 제발 부산에도 놀이공원 제발!",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004729.1.12",
          "form": "그니깐!",
          "original_form": "그니깐!",
          "speaker_id": "1",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004729.1.13",
          "form": "나 지금 진지하게 검색중이야",
          "original_form": "나 지금 진지하게 검색중이야",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.15",
          "form": "오시리아 역이던가",
          "original_form": "오시리아 역이던가",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.16",
          "form": "들어온다고 한지가 몇 년전인데",
          "original_form": "들어온다고 한지가 몇 년전인데",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.17",
          "form": "아직도 안들어옴",
          "original_form": "아직도 안들어옴",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.18",
          "form": "그치!",
          "original_form": "그치!",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.19",
          "form": "어? 꽤 진행됐네?",
          "original_form": "어? 꽤 진행됐네?",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.21",
          "form": "이거 개장되려나?",
          "original_form": "이거 개장되려나?",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004729.1.22",
          "form": "부산도 돌아볼게 많아",
          "original_form": "부산도 돌아볼게 많아",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004729.1.23",
          "form": "이거 개장되면, 부산 당일치기 여행이나 짤까",
          "original_form": "이거 개장되면, 부산 당일치기 여행이나 짤까",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004729.1.24",
          "form": "개장되면 좋겠다",
          "original_form": "개장되면 좋겠다",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004729.1.25",
          "form": "돈 적게들고 재밌겠다",
          "original_form": "돈 적게들고 재밌겠다",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004729.1.26",
          "form": "근데 너무 조용함ㅋㅋ",
          "original_form": "근데 너무 조용함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004729.1.27",
          "form": "제발 개장해주세요!",
          "original_form": "제발 개장해주세요!",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004729.1.28",
          "form": "제발!",
          "original_form": "제발!",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004729.1.29",
          "form": "부산도 여행계획짜면 좋을듯",
          "original_form": "부산도 여행계획짜면 좋을듯",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004729.1.30",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004729.1.31",
          "form": "잠은 각자 집에서 자고 낮에 돌아다니고",
          "original_form": "잠은 각자 집에서 자고 낮에 돌아다니고",
          "speaker_id": "1",
          "time": "20210821 18:19"
        }
      ]
    }
  ]
