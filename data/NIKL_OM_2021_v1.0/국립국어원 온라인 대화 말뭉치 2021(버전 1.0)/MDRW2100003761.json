      "utterance": [
        {
          "id": "MDRW2100003761.1.1",
          "form": "근데 웃긴게 있어요",
          "original_form": "근데 웃긴게 있어요",
          "speaker_id": "2",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003761.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003761.1.3",
          "form": "남편이 남들한테는 엄~~~~~~청 잘해요",
          "original_form": "남편이 남들한테는 엄~~~~~~청 잘해요",
          "speaker_id": "2",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003761.1.4",
          "form": "가족 제외하고요?",
          "original_form": "가족 제외하고요?",
          "speaker_id": "1",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003761.1.5",
          "form": "결혼 전에는 제가 남이었던 거죠!!!",
          "original_form": "결혼 전에는 제가 남이었던 거죠!!!",
          "speaker_id": "2",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003761.1.6",
          "form": "ㅋㅋㅋㅋㅋ와 대박이다",
          "original_form": "ㅋㅋㅋㅋㅋ와 대박이다",
          "speaker_id": "1",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003761.1.7",
          "form": "그정도면 사기결혼 아니예요?",
          "original_form": "그정도면 사기결혼 아니예요?",
          "speaker_id": "1",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003761.1.8",
          "form": "이제 완전 반대가 된거에요ㅠㅠ",
          "original_form": "이제 완전 반대가 된거에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:50"
        },
        {
          "id": "MDRW2100003761.1.9",
          "form": "그 말 생각난다",
          "original_form": "그 말 생각난다",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.10",
          "form": "님자에 점하나 붙으면 남이고",
          "original_form": "님자에 점하나 붙으면 남이고",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.11",
          "form": "그래서 결혼하고 계속 몇년간 싸웠어요",
          "original_form": "그래서 결혼하고 계속 몇년간 싸웠어요",
          "speaker_id": "2",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.12",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.13",
          "form": "지금은요?",
          "original_form": "지금은요?",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.14",
          "form": "적응하신거예요?",
          "original_form": "적응하신거예요?",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.15",
          "form": "남편분 안바뀌죠?",
          "original_form": "남편분 안바뀌죠?",
          "speaker_id": "1",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.16",
          "form": "그래서 이혼 생각도 많이 하고",
          "original_form": "그래서 이혼 생각도 많이 하고",
          "speaker_id": "2",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.17",
          "form": "사실 저는 극단적인 생각까지도 했었어요ㅠㅠ",
          "original_form": "사실 저는 극단적인 생각까지도 했었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:51"
        },
        {
          "id": "MDRW2100003761.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003761.1.19",
          "form": "제 주변에 돌싱 많은데",
          "original_form": "제 주변에 돌싱 많은데",
          "speaker_id": "1",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003761.1.20",
          "form": "너무 안맞고 힘들어서요",
          "original_form": "너무 안맞고 힘들어서요",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003761.1.21",
          "form": "다들 애만 없으면 이혼하는게 최선이라고 하더라구요",
          "original_form": "다들 애만 없으면 이혼하는게 최선이라고 하더라구요",
          "speaker_id": "1",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003761.1.22",
          "form": "사람이 제정신으로 못산다고...",
          "original_form": "사람이 제정신으로 못산다고...",
          "speaker_id": "1",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003761.1.23",
          "form": "그런얘기 들으니 더 결혼과 멀어졌어요ㅋㅋㅋ",
          "original_form": "그런얘기 들으니 더 결혼과 멀어졌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003761.1.24",
          "form": "네~다행이 애는 없어요",
          "original_form": "네~다행이 애는 없어요",
          "speaker_id": "2",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003761.1.25",
          "form": "정말 결혼은 신중히 해야 할것 같아요",
          "original_form": "정말 결혼은 신중히 해야 할것 같아요",
          "speaker_id": "2",
          "time": "20210821 11:54"
        },
        {
          "id": "MDRW2100003761.1.26",
          "form": "애 없으시면 이혼도 괜찮지 않나요?",
          "original_form": "애 없으시면 이혼도 괜찮지 않나요?",
          "speaker_id": "1",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003761.1.27",
          "form": "물론 저는 결혼도 아예 안해봐서 조심스러운 발언이긴한데",
          "original_form": "물론 저는 결혼도 아예 안해봐서 조심스러운 발언이긴한데",
          "speaker_id": "1",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003761.1.28",
          "form": "저도 이혼하려고 이것저것 다 알아봤었어요",
          "original_form": "저도 이혼하려고 이것저것 다 알아봤었어요",
          "speaker_id": "2",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003761.1.29",
          "form": "근데 그때마다 너무 부모님이 눈에 밟혀서 자꾸 눈물만 나는거에요",
          "original_form": "근데 그때마다 너무 부모님이 눈에 밟혀서 자꾸 눈물만 나는거에요",
          "speaker_id": "2",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003761.1.30",
          "form": "아이구",
          "original_form": "아이구",
          "speaker_id": "1",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003761.1.31",
          "form": "name2님 힘들어하시는 것보다 이혼하고 잘 사시는걸 더 바라시지 않을까요",
          "original_form": "&name2&님 힘들어하시는 것보다 이혼하고 잘 사시는걸 더 바라시지 않을까요",
          "speaker_id": "1",
          "time": "20210821 11:57"
        },
        {
          "id": "MDRW2100003761.1.32",
          "form": "부모님은 제가 힘들게 사는지 모르시거든요",
          "original_form": "부모님은 제가 힘들게 사는지 모르시거든요",
          "speaker_id": "2",
