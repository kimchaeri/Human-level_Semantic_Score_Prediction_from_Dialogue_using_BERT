      "utterance": [
        {
          "id": "MDRW2100036899.1.1",
          "form": "편한걸로 돌아왔네ㅋㅋ",
          "original_form": "편한걸로 돌아왔네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100036899.1.2",
          "form": "ㅋㅋㅋㅋ편하게 왔습니더ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ편하게 왔습니더ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036899.1.3",
          "form": "그래ㅋ 이제  또 자유시간ㅋㅋ",
          "original_form": "그래ㅋ 이제  또 자유시간ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036899.1.4",
          "form": "좋네요 전 둘째있어서ㅋ",
          "original_form": "좋네요 전 둘째있어서ㅋ",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100036899.1.5",
          "form": "자유시간 끝입니다ㅋㅋ",
          "original_form": "자유시간 끝입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100036899.1.6",
          "form": "아;;다들 둘째육아 들어갔네",
          "original_form": "아;;다들 둘째육아 들어갔네",
          "speaker_id": "2",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100036899.1.7",
          "form": "네 둘째육아들어갑니다.",
          "original_form": "네 둘째육아들어갑니다.",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036899.1.8",
          "form": "아후..",
          "original_form": "아후..",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036899.1.9",
          "form": "고생이 많다ㅜ",
          "original_form": "고생이 많다ㅜ",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100036899.1.10",
          "form": "지금부터가 제일바쁘겠네",
          "original_form": "지금부터가 제일바쁘겠네",
          "speaker_id": "2",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100036899.1.11",
          "form": "네 언니 지금은 좀 괜찮은데",
          "original_form": "네 언니 지금은 좀 괜찮은데",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036899.1.12",
          "form": "조금이따가",
          "original_form": "조금이따가",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036899.1.13",
          "form": "케이스마트가야되요",
          "original_form": "케이스마트가야되요",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036899.1.14",
          "form": "애들 거기에서 놀고싶다고해서요..",
          "original_form": "애들 거기에서 놀고싶다고해서요..",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036899.1.15",
          "form": "케이스 놀이터ㅋ 신나지ㅋㅋ",
          "original_form": "케이스 놀이터ㅋ 신나지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036899.1.16",
          "form": "우리도 주말마다 가는듯ㅋ",
          "original_form": "우리도 주말마다 가는듯ㅋ",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036899.1.17",
          "form": "아 진짜요?ㅋㅋㅋㅋ웃겨ㅋ",
          "original_form": "아 진짜요?ㅋㅋㅋㅋ웃겨ㅋ",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036899.1.18",
          "form": "그렇게노는구만요ㅋㅋ",
          "original_form": "그렇게노는구만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036899.1.19",
          "form": "그래 공짜 키카ㅋㅋ",
          "original_form": "그래 공짜 키카ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100036899.1.20",
          "form": "잘 애용해야지",
          "original_form": "잘 애용해야지",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100036899.1.21",
          "form": "장도보고",
          "original_form": "장도보고",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100036899.1.22",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100036899.1.23",
          "form": "그래서 소고기사러가는김에ㅋㅋ",
          "original_form": "그래서 소고기사러가는김에ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100036899.1.24",
          "form": "장도보고ㅋㅋ",
          "original_form": "장도보고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100036899.1.25",
          "form": "오늘 저녁은 뭐예요?",
          "original_form": "오늘 저녁은 뭐예요?",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100036899.1.26",
          "form": "오늘도 된찌",
          "original_form": "오늘도 된찌",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036899.1.27",
          "form": "ㅋㅋㅋㅋㅇㅋ",
          "original_form": "ㅋㅋㅋㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036899.1.28",
          "form": "반챤이 참 그릏다ㅜ",
          "original_form": "반챤이 참 그릏다ㅜ",
          "speaker_id": "2",
