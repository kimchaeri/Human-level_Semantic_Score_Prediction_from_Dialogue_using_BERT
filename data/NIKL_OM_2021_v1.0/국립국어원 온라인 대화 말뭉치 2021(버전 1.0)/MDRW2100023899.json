      "utterance": [
        {
          "id": "MDRW2100023899.1.1",
          "form": "저녁 식사 하셨나요?",
          "original_form": "저녁 식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023899.1.2",
          "form": "네 저는 오늘 저녁에 치킨먹었어요~",
          "original_form": "네 저는 오늘 저녁에 치킨먹었어요~",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023899.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023899.1.4",
          "form": "전 된장찌개랑 밥 먹었어요 ㅎㅎ",
          "original_form": "전 된장찌개랑 밥 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023899.1.5",
          "form": "어떤 치킨 드셨어요?",
          "original_form": "어떤 치킨 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023899.1.6",
          "form": "된장찌개 맛있죠 ~",
          "original_form": "된장찌개 맛있죠 ~",
          "speaker_id": "2",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023899.1.7",
          "form": "저는 페리카나요!",
          "original_form": "저는 페리카나요!",
          "speaker_id": "2",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100023899.1.8",
          "form": "저도 어제 치킨 먹었는데, 자담 맵슐랭 맛있더라구요",
          "original_form": "저도 어제 치킨 먹었는데, 자담 맵슐랭 맛있더라구요",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023899.1.9",
          "form": "자담은 안먹어봤는데 먹어보고싶네요",
          "original_form": "자담은 안먹어봤는데 먹어보고싶네요",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023899.1.10",
          "form": "맵슐랭 적어놔야겠어요ㅋㅋ",
          "original_form": "맵슐랭 적어놔야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023899.1.11",
          "form": "맛 단계를 조절할 수 있더라구요",
          "original_form": "맛 단계를 조절할 수 있더라구요",
          "speaker_id": "1",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023899.1.12",
          "form": "전 덜 매운 걸로 시켰는데 저한테는 부담없이 딱 좋은 정도였어요 ㅎㅎ",
          "original_form": "전 덜 매운 걸로 시켰는데 저한테는 부담없이 딱 좋은 정도였어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023899.1.13",
          "form": "맛 조절 가능해서 좋네요",
          "original_form": "맛 조절 가능해서 좋네요",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023899.1.14",
          "form": "그럼 제일 좋아하는 치킨이 맵슐랭인가요?",
          "original_form": "그럼 제일 좋아하는 치킨이 맵슐랭인가요?",
          "speaker_id": "2",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023899.1.15",
          "form": "맵슐랭은 어제 처음 시켜봤어요ㅎㅎ",
          "original_form": "맵슐랭은 어제 처음 시켜봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023899.1.16",
          "form": "제일 좋아하는 건 bbq 황금올리브요",
          "original_form": "제일 좋아하는 건 bbq 황금올리브요",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023899.1.17",
          "form": "bbq맛있죠 ㅎㅎ",
          "original_form": "bbq맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023899.1.18",
          "form": "저는 교촌 허니 좋아해요~",
          "original_form": "저는 교촌 허니 좋아해요~",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023899.1.19",
          "form": "그것도 자주 먹은 편이에요 ㅎㅎ",
          "original_form": "그것도 자주 먹은 편이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023899.1.20",
          "form": "오 치킨 잘알이시네요ㅋㅋㅋ",
          "original_form": "오 치킨 잘알이시네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023899.1.21",
          "form": "ㅋㅋㅋ 다들 베스트셀러 메뉴들이니까요",
