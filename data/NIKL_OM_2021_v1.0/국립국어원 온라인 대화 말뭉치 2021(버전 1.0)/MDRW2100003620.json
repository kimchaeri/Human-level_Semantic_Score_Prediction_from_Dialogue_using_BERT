      "utterance": [
        {
          "id": "MDRW2100003620.1.1",
          "form": "군대는 어때요?",
          "original_form": "군대는 어때요?",
          "speaker_id": "1",
          "time": "20210821 08:54"
        },
        {
          "id": "MDRW2100003620.1.2",
          "form": "쇼핑에 최적화",
          "original_form": "쇼핑에 최적화",
          "speaker_id": "2",
          "time": "20210821 08:54"
        },
        {
          "id": "MDRW2100003620.1.3",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003620.1.4",
          "form": "다시다시",
          "original_form": "다시다시",
          "speaker_id": "2",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003620.1.5",
          "form": "군대 다시",
          "original_form": "군대 다시",
          "speaker_id": "2",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003620.1.6",
          "form": "군대는 어때요?",
          "original_form": "군대는 어때요?",
          "speaker_id": "1",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003620.1.7",
          "form": "저는 군대안가잖아요",
          "original_form": "저는 군대안가잖아요",
          "speaker_id": "1",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003620.1.8",
          "form": "탱크는 운전병말고",
          "original_form": "탱크는 운전병말고",
          "speaker_id": "2",
          "time": "20210821 08:59"
        },
        {
          "id": "MDRW2100003620.1.9",
          "form": "따로 있을거에요 ㅋㅋㅋ",
          "original_form": "따로 있을거에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:59"
        },
        {
          "id": "MDRW2100003620.1.10",
          "form": "따로있어요?",
          "original_form": "따로있어요?",
          "speaker_id": "1",
          "time": "20210821 08:59"
        },
        {
          "id": "MDRW2100003620.1.11",
          "form": "몰랐던 사실이다",
          "original_form": "몰랐던 사실이다",
          "speaker_id": "1",
          "time": "20210821 08:59"
        },
        {
          "id": "MDRW2100003620.1.12",
          "form": "그리고 탱크 운전하면",
          "original_form": "그리고 탱크 운전하면",
          "speaker_id": "2",
          "time": "20210821 08:59"
        },
        {
          "id": "MDRW2100003620.1.13",
          "form": "그만큼 빡센부대라는거",
          "original_form": "그만큼 빡센부대라는거",
          "speaker_id": "2",
          "time": "20210821 08:59"
        },
        {
          "id": "MDRW2100003620.1.14",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210821 08:59"
        },
        {
          "id": "MDRW2100003620.1.15",
          "form": "그런거였어요?",
          "original_form": "그런거였어요?",
          "speaker_id": "1",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.16",
          "form": "저 그거 궁금해요 화생방 어때요?",
          "original_form": "저 그거 궁금해요 화생방 어때요?",
          "speaker_id": "1",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.18",
          "form": "듣기만 해도 어이없는 약간 그런건가",
          "original_form": "듣기만 해도 어이없는 약간 그런건가",
          "speaker_id": "1",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.19",
          "form": "눈이 겁나 맵고",
          "original_form": "눈이 겁나 맵고",
          "speaker_id": "2",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.20",
          "form": "콧물이 흐르고",
          "original_form": "콧물이 흐르고",
          "speaker_id": "2",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.21",
          "form": "침이 질질 나오는",
          "original_form": "침이 질질 나오는",
          "speaker_id": "2",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.22",
          "form": "그런수준?",
          "original_form": "그런수준?",
          "speaker_id": "2",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.23",
          "form": "으앗",
          "original_form": "으앗",
          "speaker_id": "1",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003620.1.24",
          "form": "더럽네요",
          "original_form": "더럽네요",
          "speaker_id": "2",
          "time": "20210821 09:01"
        },
        {
          "id": "MDRW2100003620.1.25",
          "form": "죄송...",
          "original_form": "죄송...",
          "speaker_id": "2",
          "time": "20210821 09:01"
        },
        {
          "id": "MDRW2100003620.1.26",
          "form": "그 순간 무슨 생각들어요?",
          "original_form": "그 순간 무슨 생각들어요?",
          "speaker_id": "1",
          "time": "20210821 09:01"
        },
        {
          "id": "MDRW2100003620.1.27",
          "form": "아니요 괜찮아요",
          "original_form": "아니요 괜찮아요",
          "speaker_id": "1",
          "time": "20210821 09:01"
        },
        {
          "id": "MDRW2100003620.1.28",
          "form": "미치겠다",
          "original_form": "미치겠다",
          "speaker_id": "2",
          "time": "20210821 09:01"
        },
        {
          "id": "MDRW2100003620.1.29",
          "form": "나가고싶다",
          "original_form": "나가고싶다",
          "speaker_id": "2",
          "time": "20210821 09:01"
        },
        {
          "id": "MDRW2100003620.1.30",
          "form": "바로못나가잖아요ㅠ",
          "original_form": "바로못나가잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210821 09:01"
        },
        {
          "id": "MDRW2100003620.1.31",
          "form": "말도 잘 못해요",
          "original_form": "말도 잘 못해요",
          "speaker_id": "2",
          "time": "20210821 09:01"
        },
        {
          "id": "MDRW2100003620.1.32",
          "form": "막 몸이 먼저 움직여요",
          "original_form": "막 몸이 먼저 움직여요",
          "speaker_id": "2",
          "time": "20210821 09:01"
        }
      ]
    }
