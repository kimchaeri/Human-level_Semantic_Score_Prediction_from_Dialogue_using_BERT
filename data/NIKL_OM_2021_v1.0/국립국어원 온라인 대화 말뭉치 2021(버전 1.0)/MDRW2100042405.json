      "utterance": [
        {
          "id": "MDRW2100042405.1.1",
          "form": "음.... 사대 안 되는 것 빼고는 괜찮아요",
          "original_form": "음.... 사대 안 되는 것 빼고는 괜찮아요",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042405.1.2",
          "form": "음? 4대보험이 안되요?",
          "original_form": "음? 4대보험이 안되요?",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042405.1.3",
          "form": "인턴만 안되는건가요?",
          "original_form": "인턴만 안되는건가요?",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042405.1.4",
          "form": "아... 정확히는 제가 일경험 수련생 신분이라서요...",
          "original_form": "아... 정확히는 제가 일경험 수련생 신분이라서요...",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042405.1.5",
          "form": "인턴이 뭐 다 수련생인거잖아요?",
          "original_form": "인턴이 뭐 다 수련생인거잖아요?",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042405.1.6",
          "form": "머 그렇긴 한데... 그게 구별을 시키더라구요 그래서 사대 없어요",
          "original_form": "머 그렇긴 한데... 그게 구별을 시키더라구요 그래서 사대 없어요",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042405.1.7",
          "form": "글쿤요.",
          "original_form": "글쿤요.",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042405.1.8",
          "form": "인턴 끝나면 직원으로 전환되나요?",
          "original_form": "인턴 끝나면 직원으로 전환되나요?",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042405.1.9",
          "form": "아니면 그냥 끝?",
          "original_form": "아니면 그냥 끝?",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042405.1.10",
          "form": "그냥 끝이에요",
          "original_form": "그냥 끝이에요",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042405.1.11",
          "form": "아..,.",
          "original_form": "아..,.",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042405.1.12",
          "form": "아직 학생이신가요?",
          "original_form": "아직 학생이신가요?",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042405.1.13",
          "form": "취준생이에요ㅠㅠ",
          "original_form": "취준생이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042405.1.14",
          "form": "하기사 저도 그래요.",
          "original_form": "하기사 저도 그래요.",
          "speaker_id": "2",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100042405.1.15",
          "form": "제대로 된 곳 취직해야하는데...",
          "original_form": "제대로 된 곳 취직해야하는데...",
          "speaker_id": "2",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100042405.1.16",
          "form": "그러게요ㅠㅠ 근데 또 요즘은 사람들 적게 뽑아서ㅠㅠ",
          "original_form": "그러게요ㅠㅠ 근데 또 요즘은 사람들 적게 뽑아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100042405.1.17",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042405.1.18",
          "form": "내 한몸 건사하기가 왜 이리 힘든지 ㅠㅠㅠ",
          "original_form": "내 한몸 건사하기가 왜 이리 힘든지 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042405.1.19",
          "form": "그래도 박사 따셨으면 취업 쉽지 않나요?",
          "original_form": "그래도 박사 따셨으면 취업 쉽지 않나요?",
          "speaker_id": "1",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100042405.1.20",
          "form": "비교를 할 수 없으니까 잘 모르기도 하거니와",
          "original_form": "비교를 할 수 없으니까 잘 모르기도 하거니와",
          "speaker_id": "2",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100042405.1.21",
          "form": "그 이전에 박사까지 하면 전공이 좁아진다고 해야하나...",
          "original_form": "그 이전에 박사까지 하면 전공이 좁아진다고 해야하나...",
          "speaker_id": "2",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100042405.1.22",
          "form": "내가 공부한것과 들어맞는 직장이 그렇게 많진 않달까...",
          "original_form": "내가 공부한것과 들어맞는 직장이 그렇게 많진 않달까...",
          "speaker_id": "2",
          "time": "20210907 11:55"
        },
        {
          "id": "MDRW2100042405.1.23",
          "form": "음 그런 면이 있군요",
          "original_form": "음 그런 면이 있군요",
          "speaker_id": "1",
          "time": "20210907 11:58"
        },
        {
          "id": "MDRW2100042405.1.24",
          "form": "저는 단순히 박사인 사람이 적으니까... 하고 생각했어요",
          "original_form": "저는 단순히 박사인 사람이 적으니까... 하고 생각했어요",
          "speaker_id": "1",
          "time": "20210907 11:59"
        }
      ]
