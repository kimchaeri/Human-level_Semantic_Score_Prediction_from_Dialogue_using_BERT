      "utterance": [
        {
          "id": "MDRW2100024201.1.1",
          "form": "같은거네요~?",
          "original_form": "같은거네요~?",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100024201.1.2",
          "form": "ㅎㅎ넵!!바꾸고 싶으시면 바꿔도 돼요!!",
          "original_form": "ㅎㅎ넵!!바꾸고 싶으시면 바꿔도 돼요!!",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100024201.1.3",
          "form": "오 같은것도 가눙한지 몰랏어요",
          "original_form": "오 같은것도 가눙한지 몰랏어요",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024201.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100024201.1.5",
          "form": "아니애요 좋아요 가장 핫한주제아닌가싶네여 ㅋㅋㅋㅋㅋ",
          "original_form": "아니애요 좋아요 가장 핫한주제아닌가싶네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100024201.1.6",
          "form": "ㅎㅎ 그쵸",
          "original_form": "ㅎㅎ 그쵸",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100024201.1.7",
          "form": "그래도 회사 생활 자체는 괜찮으신가봐요!!",
          "original_form": "그래도 회사 생활 자체는 괜찮으신가봐요!!",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024201.1.8",
          "form": "사람관계나 그런거요!",
          "original_form": "사람관계나 그런거요!",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024201.1.9",
          "form": "전 인간관계도 어렵더라구요ㅎㅎㅠ",
          "original_form": "전 인간관계도 어렵더라구요ㅎㅎㅠ",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024201.1.10",
          "form": "아 첨엔힘글엇는데",
          "original_form": "아 첨엔힘글엇는데",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024201.1.11",
          "form": "다행히 입사동기들이랑 친해지고 마음맞아서",
          "original_form": "다행히 입사동기들이랑 친해지고 마음맞아서",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024201.1.12",
          "form": "아주... 이 힘으로 다니지 안그러면 괴로웠을거에요 \r\n 사람이 중요한 사람인지라...",
          "original_form": "아주... 이 힘으로 다니지 안그러면 괴로웠을거에요 \r\n 사람이 중요한 사람인지라...",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024201.1.13",
          "form": "인간관ㄱ메는 평생 새롭지않을까싶어요 ~ 사람이 다 다르다고 하내요 ..",
          "original_form": "인간관ㄱ메는 평생 새롭지않을까싶어요 ~ 사람이 다 다르다고 하내요 ..",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024201.1.14",
          "form": "맘맞는 사람들하고 같이 있는거 정말 행운인 것 같아요ㅠ!!",
          "original_form": "맘맞는 사람들하고 같이 있는거 정말 행운인 것 같아요ㅠ!!",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024201.1.15",
          "form": "뭔가 학생때는 다 수평적이니 그런게 없었는데 회사는 눈치도 봐야하고",
          "original_form": "뭔가 학생때는 다 수평적이니 그런게 없었는데 회사는 눈치도 봐야하고",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024201.1.16",
          "form": "음...근데 이것도 나중엔 별거아니게 느껴질 것 같아요ㅎㅎ",
          "original_form": "음...근데 이것도 나중엔 별거아니게 느껴질 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024201.1.17",
          "form": "그런거같긴해여.. 사람 .. 최고즁요...",
          "original_form": "그런거같긴해여.. 사람 .. 최고즁요...",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024201.1.18",
          "form": "하 그게 그렇죠? 가만히만있어도 나도모르게 신경이쓰여서",
          "original_form": "하 그게 그렇죠? 가만히만있어도 나도모르게 신경이쓰여서",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024201.1.19",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024201.1.20",
          "form": "기운이 빠져요 ㅋㅋ 일이거의없던날도",
          "original_form": "기운이 빠져요 ㅋㅋ 일이거의없던날도",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024201.1.21",
          "form": "그러개요 많이 지나야 괜찮아질거갗네요 ㅋㅋㅋㅋ",
          "original_form": "그러개요 많이 지나야 괜찮아질거갗네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024201.1.22",
          "form": "네ㅜㅜㅠ 진짜 별거아닌 반응에도 괜히 신경쓰게 되고요ㅎㅎ",
          "original_form": "네ㅜㅜㅠ 진짜 별거아닌 반응에도 괜히 신경쓰게 되고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024201.1.23",
          "form": "그래도 마음맞고 의지되는 동료있으묜 정말 다닐수있어요",
          "original_form": "그래도 마음맞고 의지되는 동료있으묜 정말 다닐수있어요",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024201.1.24",
          "form": "그니까요~~",
          "original_form": "그니까요~~",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024201.1.25",
          "form": "스스로 철판도 되면 더 좋규요 ㅋㅌㅋㅋ",
          "original_form": "스스로 철판도 되면 더 좋규요 ㅋㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024201.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다음 사회생활할땐 좀 더 편한 마음으로 다닐 수 있을 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다음 사회생활할땐 좀 더 편한 마음으로 다닐 수 있을 것 같아요",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024201.1.27",
          "form": "처음이라 쓸데없이 더 긴장하고....",
          "original_form": "처음이라 쓸데없이 더 긴장하고....",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024201.1.28",
          "form": "그랬던 것 같네요ㅋㅋㅋㅋ하핳",
          "original_form": "그랬던 것 같네요ㅋㅋㅋㅋ하핳",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024201.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024201.1.30",
          "form": "그럴수밖애요 ㅠㅠ",
          "original_form": "그럴수밖애요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024201.1.31",
          "form": "name1님 잘할수있을거에요",
          "original_form": "&name1&님 잘할수있을거에요",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024201.1.32",
          "form": "기죽지마시고 무조건 근자감이 답인듯요! ㅎㅎ",
          "original_form": "기죽지마시고 무조건 근자감이 답인듯요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024201.1.33",
          "form": "감사해요ㅠㅠ!!!",
          "original_form": "감사해요ㅠㅠ!!!",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024201.1.34",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024201.1.35",
          "form": "친한 언니가 알려주셨는데.. 직장이 마니힘들거나 아니다 싳으면 바로바로 관두면 되는거래요",
          "original_form": "친한 언니가 알려주셨는데.. 직장이 마니힘들거나 아니다 싳으면 바로바로 관두면 되는거래요",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024201.1.36",
          "form": "직장 얼마든지 많대요. 자기는 처음에 넘 힘들엇던곳 참고 꾸역꾸역다녓던거 후회하신다먼서.. 그럴거 없었다고 하시더라규요",
          "original_form": "직장 얼마든지 많대요. 자기는 처음에 넘 힘들엇던곳 참고 꾸역꾸역다녓던거 후회하신다먼서.. 그럴거 없었다고 하시더라규요",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024201.1.37",
          "form": "ㅜㅠㅠㅠㅠ좋은 얘기 넘 감사해요!!!",
          "original_form": "ㅜㅠㅠㅠㅠ좋은 얘기 넘 감사해요!!!",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024201.1.38",
          "form": "잘은 몰라도 name2님은 말씀하시는것도",
          "original_form": "잘은 몰라도 &name2&님은 말씀하시는것도",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024201.1.39",
          "form": "넘 똑부러지시고(?!)",
          "original_form": "넘 똑부러지시고(?!)",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024201.1.40",
          "form": "어느 상황에서도 잘 해결해나가실것같아요",
          "original_form": "어느 상황에서도 잘 해결해나가실것같아요",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024201.1.41",
          "form": "에고.. 오늘 친한언니한테 저 멍청하다고 푸념했는걸요",
          "original_form": "에고.. 오늘 친한언니한테 저 멍청하다고 푸념했는걸요",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024201.1.42",
          "form": "ㅋㅋ언니한테 혼났어요 ㅋㅋ",
          "original_form": "ㅋㅋ언니한테 혼났어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024201.1.43",
          "form": "셀프디스 하면 혼납미다..",
          "original_form": "셀프디스 하면 혼납미다..",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024201.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024201.1.45",
