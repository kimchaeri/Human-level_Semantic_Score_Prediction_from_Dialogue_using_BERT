      "utterance": [
        {
          "id": "MDRW2100010444.1.1",
          "form": "카카페인가용?",
          "original_form": "카카페인가용?",
          "speaker_id": "2",
          "time": "20210823 18:18"
        },
        {
          "id": "MDRW2100010444.1.2",
          "form": "아뇨아뇨 네이버요!",
          "original_form": "아뇨아뇨 네이버요!",
          "speaker_id": "1",
          "time": "20210823 18:18"
        },
        {
          "id": "MDRW2100010444.1.3",
          "form": "무협이긴 한데",
          "original_form": "무협이긴 한데",
          "speaker_id": "1",
          "time": "20210823 18:18"
        },
        {
          "id": "MDRW2100010444.1.4",
          "form": "최근에 웹소가 1억뷰를 넘어서",
          "original_form": "최근에 웹소가 1억뷰를 넘어서",
          "speaker_id": "1",
          "time": "20210823 18:19"
        },
        {
          "id": "MDRW2100010444.1.5",
          "form": "무협도 가끔봐요!",
          "original_form": "무협도 가끔봐요!",
          "speaker_id": "2",
          "time": "20210823 18:19"
        },
        {
          "id": "MDRW2100010444.1.6",
          "form": "웹툰화도 최근에 된 편이거든요!!",
          "original_form": "웹툰화도 최근에 된 편이거든요!!",
          "speaker_id": "1",
          "time": "20210823 18:19"
        },
        {
          "id": "MDRW2100010444.1.7",
          "form": "베스트셀러네용",
          "original_form": "베스트셀러네용",
          "speaker_id": "2",
          "time": "20210823 18:19"
        },
        {
          "id": "MDRW2100010444.1.8",
          "form": "무협 뭐 좋아하세요?!",
          "original_form": "무협 뭐 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210823 18:19"
        },
        {
          "id": "MDRW2100010444.1.9",
          "form": "마자용마자용",
          "original_form": "마자용마자용",
          "speaker_id": "1",
          "time": "20210823 18:19"
        },
        {
          "id": "MDRW2100010444.1.10",
          "form": "주로 네이버많이보는데",
          "original_form": "주로 네이버많이보는데",
          "speaker_id": "2",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010444.1.11",
          "form": "갓오하도 조금봤어요",
          "original_form": "갓오하도 조금봤어요",
          "speaker_id": "2",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010444.1.12",
          "form": "갓오하 오오",
          "original_form": "갓오하 오오",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010444.1.13",
          "form": "이영싫을 많이 보시던데",
          "original_form": "이영싫을 많이 보시던데",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010444.1.14",
          "form": "이영싫도 보셨나요?!",
          "original_form": "이영싫도 보셨나요?!",
          "speaker_id": "1",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010444.1.15",
          "form": "추천받아봤었는데!",
          "original_form": "추천받아봤었는데!",
          "speaker_id": "2",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010444.1.16",
          "form": "아직 못봤어용ㅋㅋㅋ",
          "original_form": "아직 못봤어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010444.1.17",
          "form": "마자요 이미 완결나섴ㅋㅋㅋㅋㅋ",
          "original_form": "마자요 이미 완결나섴ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010444.1.18",
          "form": "까기에 용기가 필요해요",
          "original_form": "까기에 용기가 필요해요",
          "speaker_id": "1",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010444.1.19",
          "form": "그러면 혹시 정글쥬스 보셨나요",
          "original_form": "그러면 혹시 정글쥬스 보셨나요",
          "speaker_id": "1",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010444.1.20",
          "form": "저 최근에 정글쥬스 봤는데 진짜 흥미롭더라고요",
          "original_form": "저 최근에 정글쥬스 봤는데 진짜 흥미롭더라고요",
          "speaker_id": "1",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010444.1.21",
          "form": "검색해보니까 판타지 액션장른가봐요!",
          "original_form": "검색해보니까 판타지 액션장른가봐요!",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100010444.1.22",
          "form": "그림도 고퀄이네용ㅋㅋㅋ재밌어보여요",
          "original_form": "그림도 고퀄이네용ㅋㅋㅋ재밌어보여요",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100010444.1.23",
          "form": "맞아요 잼써요!!!!!!",
          "original_form": "맞아요 잼써요!!!!!!",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100010444.1.24",
          "form": "추천합니다",
          "original_form": "추천합니다",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100010444.1.25",
          "form": "무협은 뭐보세요?",
          "original_form": "무협은 뭐보세요?",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100010444.1.26",
          "form": "요즘은 로판위주로 봐서..!",
          "original_form": "요즘은 로판위주로 봐서..!",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100010444.1.27",
          "form": "작품명이 잘생각안나용ㅋㅋ쿠ㅜㅜㅜ",
          "original_form": "작품명이 잘생각안나용ㅋㅋ쿠ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100010444.1.28",
          "form": "요즘은 로판위주로만 보는거같아용ㅋㅋ쿠ㅜㅜ",
          "original_form": "요즘은 로판위주로만 보는거같아용ㅋㅋ쿠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:39"
        }
      ]
