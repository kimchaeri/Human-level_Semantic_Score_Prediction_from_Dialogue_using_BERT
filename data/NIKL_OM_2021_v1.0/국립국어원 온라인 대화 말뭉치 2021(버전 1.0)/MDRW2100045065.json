      "utterance": [
        {
          "id": "MDRW2100045065.1.1",
          "form": "라면 하루에 한개씩 먹는 아들이 있어요.",
          "original_form": "라면 하루에 한개씩 먹는 아들이 있어요.",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045065.1.2",
          "form": "아 귀여웡 직접끓여먹나여? 엄마가해주나여?ㅋ",
          "original_form": "아 귀여웡 직접끓여먹나여? 엄마가해주나여?ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045065.1.3",
          "form": "직접 끓여 먹어요",
          "original_form": "직접 끓여 먹어요",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045065.1.4",
          "form": "아들만의 조리법이있을듯ㅎㅎ",
          "original_form": "아들만의 조리법이있을듯ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045065.1.5",
          "form": "제일 잘 끓임.ㅋㅋㅋ",
          "original_form": "제일 잘 끓임.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045065.1.6",
          "form": "선호하는 라면있어요?",
          "original_form": "선호하는 라면있어요?",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045065.1.7",
          "form": "대파 엄청 많이 써요",
          "original_form": "대파 엄청 많이 써요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045065.1.8",
          "form": "울 집서 대파 소비량이 제일 많아요.ㅋㅋㅋ",
          "original_form": "울 집서 대파 소비량이 제일 많아요.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045065.1.9",
          "form": "진라면 매운맛이요.",
          "original_form": "진라면 매운맛이요.",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045065.1.10",
          "form": "우왕 대파까지넣고",
          "original_form": "우왕 대파까지넣고",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045065.1.11",
          "form": "와....저도 좀 먹어보곱네요ㅋㅋ",
          "original_form": "와....저도 좀 먹어보곱네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045065.1.12",
          "form": "어떤 라면 좋아하세요?",
          "original_form": "어떤 라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045065.1.13",
          "form": "신라면이요ㅋㅋ",
          "original_form": "신라면이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045065.1.14",
          "form": "저는 계란꼭 넣어요!ㅋㅋ",
          "original_form": "저는 계란꼭 넣어요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045065.1.15",
          "form": "울 아들도 계란 꼭 넣더라구요",
          "original_form": "울 아들도 계란 꼭 넣더라구요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045065.1.16",
          "form": "저는 푸는데 안 풀고.ㅋㅋㅋ",
          "original_form": "저는 푸는데 안 풀고.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045065.1.17",
          "form": "와 진짜먹을줄안당ㅎㅎㅎ칭찬마니해주세요!ㅋㅋㅋ",
          "original_form": "와 진짜먹을줄안당ㅎㅎㅎ칭찬마니해주세요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045065.1.18",
          "form": "저도 라면좀찾아바야겠어요ㅋㅋ",
          "original_form": "저도 라면좀찾아바야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045065.1.19",
          "form": "밥도같이말아먹나여?",
          "original_form": "밥도같이말아먹나여?",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045065.1.20",
          "form": "네. 없을 때 빼고는 꼭 먹던데요",
          "original_form": "네. 없을 때 빼고는 꼭 먹던데요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045065.1.21",
          "form": "너무제스타일ㅋㅋㅋ",
          "original_form": "너무제스타일ㅋㅋㅋ",
          "speaker_id": "2",
