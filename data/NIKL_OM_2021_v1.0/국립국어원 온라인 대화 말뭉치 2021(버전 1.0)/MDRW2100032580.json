      "utterance": [
        {
          "id": "MDRW2100032580.1.1",
          "form": "라면 머 잴 조아해",
          "original_form": "라면 머 잴 조아해",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032580.1.2",
          "form": "아마도 육개장인거 같음",
          "original_form": "아마도 육개장인거 같음",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032580.1.3",
          "form": "근데 진매도 좋고 스낵면듀 좋아",
          "original_form": "근데 진매도 좋고 스낵면듀 좋아",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032580.1.4",
          "form": "님은 머 좋아햐",
          "original_form": "님은 머 좋아햐",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032580.1.5",
          "form": "음 까르보불닭보끔면..?",
          "original_form": "음 까르보불닭보끔면..?",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032580.1.6",
          "form": "음 까르보 맛잇지 ㅋㅋㅋㅋㅋ",
          "original_form": "음 까르보 맛잇지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032580.1.7",
          "form": "까르보불닭도 뭔가 1개 모자란 느낌 아나...",
          "original_form": "까르보불닭도 뭔가 1개 모자란 느낌 아나...",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032580.1.8",
          "form": "ㅋㅋㅋㅋㅋ삼김은 뭐 조아해",
          "original_form": "ㅋㅋㅋㅋㅋ삼김은 뭐 조아해",
          "speaker_id": "1",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032580.1.9",
          "form": "맞아 애매해 한개 ㅠ",
          "original_form": "맞아 애매해 한개 ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032580.1.10",
          "form": "ㅋㅋㅋㅋ참마가 진리아니겟니",
          "original_form": "ㅋㅋㅋㅋ참마가 진리아니겟니",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032580.1.11",
          "form": "당연쓰 참마 맛잇어 ㅠㅜ",
          "original_form": "당연쓰 참마 맛잇어 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032580.1.12",
          "form": "근데 전주비빔파도 많도라",
          "original_form": "근데 전주비빔파도 많도라",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032580.1.13",
          "form": "밥이 양념된 걸 좋아하는 쪽일듯",
          "original_form": "밥이 양념된 걸 좋아하는 쪽일듯",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100032580.1.14",
          "form": "난 흰쌀밥이 좋더라..ㅋㅋㅋ",
          "original_form": "난 흰쌀밥이 좋더라..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100032580.1.15",
          "form": "치즈들어간 전주비빔은 또 맛잇도르",
          "original_form": "치즈들어간 전주비빔은 또 맛잇도르",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032580.1.16",
          "form": "맞아 흰쌀밥 깔끔쓰",
          "original_form": "맞아 흰쌀밥 깔끔쓰",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032580.1.17",
          "form": "치즈든 것도 있나...사실 요즘은 삼김 잘 안먹은듯",
          "original_form": "치즈든 것도 있나...사실 요즘은 삼김 잘 안먹은듯",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032580.1.18",
          "form": "씅에 안차서 ㅋㅋㅋㅋ",
          "original_form": "씅에 안차서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032580.1.19",
          "form": "나도그려ㅋㅋㅋㅋ",
          "original_form": "나도그려ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032580.1.20",
          "form": "김밥 한줄도 부족한디",
          "original_form": "김밥 한줄도 부족한디",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032580.1.21",
          "form": "당연 라면이란 먹지...",
          "original_form": "당연 라면이란 먹지...",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032580.1.22",
          "form": "그때 육개장이 최고여",
          "original_form": "그때 육개장이 최고여",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100032580.1.23",
