      "utterance": [
        {
          "id": "MDRW2100036344.1.1",
          "form": "여행 언제 갈 수 있을까욥ㅜ",
          "original_form": "여행 언제 갈 수 있을까욥ㅜ",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036344.1.2",
          "form": "국내는 그래도 마스크 잘하고 갈수있을것같아요",
          "original_form": "국내는 그래도 마스크 잘하고 갈수있을것같아요",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036344.1.3",
          "form": "그건 그래용!",
          "original_form": "그건 그래용!",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036344.1.4",
          "form": "제주도 가고싶으네요",
          "original_form": "제주도 가고싶으네요",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036344.1.5",
          "form": "맞아요 저는 경주",
          "original_form": "맞아요 저는 경주",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100036344.1.6",
          "form": "오 경주!",
          "original_form": "오 경주!",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036344.1.7",
          "form": "경리단길이 그리 이쁘다던데요",
          "original_form": "경리단길이 그리 이쁘다던데요",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100036344.1.8",
          "form": "거기일까요?",
          "original_form": "거기일까요?",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100036344.1.9",
          "form": "황리단길이랑",
          "original_form": "황리단길이랑",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036344.1.10",
          "form": "대릉원",
          "original_form": "대릉원",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036344.1.11",
          "form": "자전거도 타고파요",
          "original_form": "자전거도 타고파요",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036344.1.12",
          "form": "첨성대 공원 넓고좋아요ㅠ",
          "original_form": "첨성대 공원 넓고좋아요ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036344.1.13",
          "form": "우와.. 거기 공원이 있었군요!",
          "original_form": "우와.. 거기 공원이 있었군요!",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036344.1.14",
          "form": "새로 생긴건가요?",
          "original_form": "새로 생긴건가요?",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036344.1.15",
          "form": "아니용 거기 좀 넓어서 공원같더라구요 ㅋㅋ",
          "original_form": "아니용 거기 좀 넓어서 공원같더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 06:37"
        },
        {
          "id": "MDRW2100036344.1.16",
          "form": "꽃밭도 있고",
          "original_form": "꽃밭도 있고",
          "speaker_id": "1",
          "time": "20210831 06:37"
        },
        {
          "id": "MDRW2100036344.1.17",
          "form": "오.. 수학여행때 가보고 못가본거같아요",
          "original_form": "오.. 수학여행때 가보고 못가본거같아요",
          "speaker_id": "2",
          "time": "20210831 06:39"
        },
        {
          "id": "MDRW2100036344.1.18",
          "form": "저는 코로나 전엔 매년 간것같아요",
          "original_form": "저는 코로나 전엔 매년 간것같아요",
          "speaker_id": "1",
          "time": "20210831 06:40"
        },
        {
          "id": "MDRW2100036344.1.19",
          "form": "연도 날리고 자전거도 타고 좋아보이더라구요",
          "original_form": "연도 날리고 자전거도 타고 좋아보이더라구요",
          "speaker_id": "1",
          "time": "20210831 06:40"
        },
        {
          "id": "MDRW2100036344.1.20",
          "form": "우와~ 경주 좋아하시나봐요!",
          "original_form": "우와~ 경주 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210831 06:48"
        },
        {
          "id": "MDRW2100036344.1.21",
          "form": "들어보니 좋은곳도 많네요",
          "original_form": "들어보니 좋은곳도 많네요",
          "speaker_id": "2",
          "time": "20210831 06:49"
        },
        {
          "id": "MDRW2100036344.1.22",
          "form": "그쵸 경쥬 가꺼워서 자주가요",
          "original_form": "그쵸 경쥬 가꺼워서 자주가요",
          "speaker_id": "1",
          "time": "20210831 07:44"
        },
        {
          "id": "MDRW2100036344.1.23",
          "form": "통영도 좋어해요",
          "original_form": "통영도 좋어해요",
          "speaker_id": "1",
          "time": "20210831 07:44"
        }
      ]
    }
