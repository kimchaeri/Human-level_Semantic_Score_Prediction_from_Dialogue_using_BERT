      "utterance": [
        {
          "id": "MDRW2100003837.1.1",
          "form": "이제 준비하려구요",
          "original_form": "이제 준비하려구요",
          "speaker_id": "2",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003837.1.2",
          "form": "아침을 늦게 먹어서 ㅎㅎ",
          "original_form": "아침을 늦게 먹어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003837.1.3",
          "form": "메뉴는 뭔가요?",
          "original_form": "메뉴는 뭔가요?",
          "speaker_id": "1",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003837.1.4",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210821 12:48"
        },
        {
          "id": "MDRW2100003837.1.5",
          "form": "불고기 남은거랑 시래국 있는거 먹으려구요 ㅎ",
          "original_form": "불고기 남은거랑 시래국 있는거 먹으려구요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003837.1.6",
          "form": "불고기 좋네요 전 제육 먹을까했는데",
          "original_form": "불고기 좋네요 전 제육 먹을까했는데",
          "speaker_id": "1",
          "time": "20210821 12:49"
        },
        {
          "id": "MDRW2100003837.1.7",
          "form": "냉동실에 있어서 녹여야돼서 저녁에 먹어야할듯요",
          "original_form": "냉동실에 있어서 녹여야돼서 저녁에 먹어야할듯요",
          "speaker_id": "1",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003837.1.8",
          "form": "아이들이 아직 매운걸 못먹으니 제육을 못먹네요 ㅎ",
          "original_form": "아이들이 아직 매운걸 못먹으니 제육을 못먹네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003837.1.9",
          "form": "물에담아놓으면 좀 빨리 해동되지않을까요?",
          "original_form": "물에담아놓으면 좀 빨리 해동되지않을까요?",
          "speaker_id": "2",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003837.1.10",
          "form": "불고기가 더맛있는거같아요 소고기자나요ㅎㅎ",
          "original_form": "불고기가 더맛있는거같아요 소고기자나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003837.1.11",
          "form": "정원샵 막창 오늘배송한다는데 문열어보고 왔음 그거먼저 먹을까봐요",
          "original_form": "정원샵 막창 오늘배송한다는데 문열어보고 왔음 그거먼저 먹을까봐요",
          "speaker_id": "1",
          "time": "20210821 12:51"
        },
        {
          "id": "MDRW2100003837.1.12",
          "form": "제육도 맛있을듯해요 저녁되기전에 머라도 드셔야되는거아니예요?",
          "original_form": "제육도 맛있을듯해요 저녁되기전에 머라도 드셔야되는거아니예요?",
          "speaker_id": "2",
          "time": "20210821 12:51"
        },
        {
          "id": "MDRW2100003837.1.13",
          "form": "오 막창...진짜 맛나는데",
          "original_form": "오 막창...진짜 맛나는데",
          "speaker_id": "2",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003837.1.14",
          "form": "안주야 불막창샀는데 맛있겠죠?",
          "original_form": "안주야 불막창샀는데 맛있겠죠?",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003837.1.15",
          "form": "아 불막창...많이 맵겠네요 ㅎ",
          "original_form": "아 불막창...많이 맵겠네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003837.1.16",
          "form": "아무래도ㅋ 대구가 막창이 유명하죠?",
          "original_form": "아무래도ㅋ 대구가 막창이 유명하죠?",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003837.1.17",
          "form": "네 대구막창 진짜 맛나요",
          "original_form": "네 대구막창 진짜 맛나요",
          "speaker_id": "2",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003837.1.18",
          "form": "대구오시면 막창집에 한번 모실께요 ㅎ",
          "original_form": "대구오시면 막창집에 한번 모실께요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003837.1.19",
          "form": "직접가서 먹어보고싶은데 넘머네요ㅎ",
          "original_form": "직접가서 먹어보고싶은데 넘머네요ㅎ",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003837.1.20",
          "form": "맛있다고 하던데 유명한 곳이어디였죠? 동성로?",
          "original_form": "맛있다고 하던데 유명한 곳이어디였죠? 동성로?",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003837.1.21",
          "form": "마루막창이요 ㅎㅎ",
          "original_form": "마루막창이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003837.1.22",
          "form": "아 유명맛집인가 보네요",
          "original_form": "아 유명맛집인가 보네요",
          "speaker_id": "1",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003837.1.23",
          "form": "저는 점심좀 먹고올께요 애들이 배고프다고 난리 ㅎㅎ",
          "original_form": "저는 점심좀 먹고올께요 애들이 배고프다고 난리 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003837.1.24",
          "form": "맛점하시고 또뵈요",
          "original_form": "맛점하시고 또뵈요",
          "speaker_id": "2",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003837.1.25",
          "form": "네 저도 밥좀 먹어야겠네요",
          "original_form": "네 저도 밥좀 먹어야겠네요",
          "speaker_id": "1",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003837.1.26",
          "form": "맛점하세요",
          "original_form": "맛점하세요",
          "speaker_id": "1",
          "time": "20210821 12:55"
        }
      ]
    }
  ]
