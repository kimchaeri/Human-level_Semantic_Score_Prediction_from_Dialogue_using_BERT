      "utterance": [
        {
          "id": "MDRW2100013086.1.1",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013086.1.2",
          "form": "전 동원샘물도 가끔",
          "original_form": "전 동원샘물도 가끔",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013086.1.3",
          "form": "먹었는데 날벼락이예요ㅋㅋㅋ",
          "original_form": "먹었는데 날벼락이예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013086.1.4",
          "form": "저는 동원샘물 집에 40병있고",
          "original_form": "저는 동원샘물 집에 40병있고",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013086.1.5",
          "form": "60병 지금 오고 있는데.. ㅜㅜ 이제 알았어요",
          "original_form": "60병 지금 오고 있는데.. ㅜㅜ 이제 알았어요",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013086.1.6",
          "form": "싸서 좋다고 먹었는데",
          "original_form": "싸서 좋다고 먹었는데",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013086.1.7",
          "form": "헐ㄷㄷ",
          "original_form": "헐ㄷㄷ",
          "speaker_id": "1",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100013086.1.8",
          "form": "60병이나요?ㅠ 대박이네요",
          "original_form": "60병이나요?ㅠ 대박이네요",
          "speaker_id": "1",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100013086.1.9",
          "form": "동ㅈ원샘물",
          "original_form": "동ㅈ원샘물",
          "speaker_id": "1",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100013086.1.10",
          "form": "물맛은 괜찮던데ㅠ",
          "original_form": "물맛은 괜찮던데ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100013086.1.11",
          "form": "점심은 뭐드시나요?",
          "original_form": "점심은 뭐드시나요?",
          "speaker_id": "2",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100013086.1.12",
          "form": "저 이미먹었답니다",
          "original_form": "저 이미먹었답니다",
          "speaker_id": "1",
          "time": "20210825 12:36"
        },
        {
          "id": "MDRW2100013086.1.13",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210825 12:36"
        },
        {
          "id": "MDRW2100013086.1.14",
          "form": "아주아주 맛난거 드셨네요^^",
          "original_form": "아주아주 맛난거 드셨네요^^",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013086.1.15",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:44"
        },
        {
          "id": "MDRW2100013086.1.16",
          "form": "로제떡볶이 좋아해요",
          "original_form": "로제떡볶이 좋아해요",
          "speaker_id": "2",
          "time": "20210825 12:45"
        },
        {
          "id": "MDRW2100013086.1.17",
          "form": "배떡vs태리로제?",
          "original_form": "배떡vs태리로제?",
          "speaker_id": "1",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100013086.1.18",
          "form": "배떡을 아직 못먹어봐서 태리로제요 ㅋ",
          "original_form": "배떡을 아직 못먹어봐서 태리로제요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 13:00"
        },
        {
          "id": "MDRW2100013086.1.19",
          "form": "ㅋㅋㅋ 배떡보다",
          "original_form": "ㅋㅋㅋ 배떡보다",
          "speaker_id": "1",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100013086.1.20",
          "form": "태리로제가 더 맛있더라고요",
          "original_form": "태리로제가 더 맛있더라고요",
          "speaker_id": "1",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100013086.1.21",
          "form": "오오 다행이네요 ㅋ",
          "original_form": "오오 다행이네요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100013086.1.22",
          "form": "떡군이네",
          "original_form": "떡군이네",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013086.1.23",
          "form": "먹어보셨나요?",
          "original_form": "먹어보셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013086.1.24",
          "form": "네^^ 맛있더라구요",
          "original_form": "네^^ 맛있더라구요",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013086.1.25",
          "form": "북문로 로제 드셔보셨어요~?",
          "original_form": "북문로 로제 드셔보셨어요~?",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100013086.1.26",
