      "utterance": [
        {
          "id": "MDRW2100036468.1.1",
          "form": "엄청 빠르네",
          "original_form": "엄청 빠르네",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036468.1.2",
          "form": "그러게 엄청 빨리 만들어서 먹고 치우면 한시간도 안걸림",
          "original_form": "그러게 엄청 빨리 만들어서 먹고 치우면 한시간도 안걸림",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036468.1.3",
          "form": "어떻게 만드는데 넌",
          "original_form": "어떻게 만드는데 넌",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036468.1.4",
          "form": "일단 팬에 올리브오일에 마늘을 넣어서 마늘향을 낸다",
          "original_form": "일단 팬에 올리브오일에 마늘을 넣어서 마늘향을 낸다",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036468.1.5",
          "form": "그렇지 올리브오일파스타처럼",
          "original_form": "그렇지 올리브오일파스타처럼",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036468.1.6",
          "form": "애들이 있어서 이건 패스",
          "original_form": "애들이 있어서 이건 패스",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036468.1.7",
          "form": "나쁜말 ㅜ.ㅜ 맵게 할려고 청 양 고 추도 약간",
          "original_form": "나쁜말 ㅜ.ㅜ 맵게 할려고 청 양 고 추도 약간",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036468.1.8",
          "form": "엉 일단 만드는건 생각해보고",
          "original_form": "엉 일단 만드는건 생각해보고",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036468.1.9",
          "form": "양파하고 당근 넣고 같이 뽁아",
          "original_form": "양파하고 당근 넣고 같이 뽁아",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036468.1.10",
          "form": "응 그리고",
          "original_form": "응 그리고",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036468.1.11",
          "form": "조갯살을 넣고 비린대 제거로 청주도 넣어",
          "original_form": "조갯살을 넣고 비린대 제거로 청주도 넣어",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036468.1.12",
          "form": "청주 아니라도 되겠네",
          "original_form": "청주 아니라도 되겠네",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036468.1.13",
          "form": "엉 청주말고 다른것도 되지 알콜을 날리도록 볶고",
          "original_form": "엉 청주말고 다른것도 되지 알콜을 날리도록 볶고",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036468.1.14",
          "form": "응 술 용도가 그렇지",
          "original_form": "응 술 용도가 그렇지",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036468.1.15",
          "form": "파스타면은 소금 조금 넣고 끓이는데 얀간 단단하게",
          "original_form": "파스타면은 소금 조금 넣고 끓이는데 얀간 단단하게",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100036468.1.16",
          "form": "다시 볶아야하니까",
          "original_form": "다시 볶아야하니까",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100036468.1.17",
          "form": "맞아 면수도 사용할꺼니 조금 남겨놓고",
          "original_form": "맞아 면수도 사용할꺼니 조금 남겨놓고",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100036468.1.18",
          "form": "알음",
          "original_form": "알음",
          "speaker_id": "2",
