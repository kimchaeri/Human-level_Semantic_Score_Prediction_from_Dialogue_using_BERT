        },
        {
          "id": "MDRW2100047268.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100047268.1.3",
          "form": "무슨 게임 자주하시나요?",
          "original_form": "무슨 게임 자주하시나요?",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100047268.1.4",
          "form": "데바데 했었어요 님은요?",
          "original_form": "데바데 했었어요 님은요?",
          "speaker_id": "1",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100047268.1.5",
          "form": "저는 폰으로 카트라이더 게임 해요 ㅋㅋ",
          "original_form": "저는 폰으로 카트라이더 게임 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100047268.1.6",
          "form": "카트 초딩때 많이했네요ㅎㅎ",
          "original_form": "카트 초딩때 많이했네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100047268.1.7",
          "form": "요즘은 닌텐도 스위치로 링피트도 하고있어요",
          "original_form": "요즘은 닌텐도 스위치로 링피트도 하고있어요",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100047268.1.8",
          "form": "오 링피트 장바구니에 있어요ㅎㅎ",
          "original_form": "오 링피트 장바구니에 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100047268.1.9",
          "form": "어떤가요",
          "original_form": "어떤가요",
          "speaker_id": "1",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100047268.1.10",
          "form": "생각보다 재미있는데 운동 효과도 괜찮아서 만족하고있어요!",
          "original_form": "생각보다 재미있는데 운동 효과도 괜찮아서 만족하고있어요!",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100047268.1.11",
          "form": "메이플 해보셨나요?",
          "original_form": "메이플 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100047268.1.12",
          "form": "해보진 않았는데 구경은 많이 했어요 ㅋㅋㅋ",
          "original_form": "해보진 않았는데 구경은 많이 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100047268.1.13",
          "form": "크레이지 아케이드는요?",
          "original_form": "크레이지 아케이드는요?",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100047268.1.14",
          "form": "오 크아는 많이 해봤죠!",
          "original_form": "오 크아는 많이 해봤죠!",
          "speaker_id": "2",
          "time": "20210826 11:58"
        }
