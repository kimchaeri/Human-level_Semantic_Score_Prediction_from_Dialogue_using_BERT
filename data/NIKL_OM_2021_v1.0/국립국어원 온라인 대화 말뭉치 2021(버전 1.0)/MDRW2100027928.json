      "utterance": [
        {
          "id": "MDRW2100027928.1.1",
          "form": "전독시 대만판 온 거 같은데",
          "original_form": "전독시 대만판 온 거 같은데",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027928.1.2",
          "form": "읽을 수 없는 책 왔다",
          "original_form": "읽을 수 없는 책 왔다",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027928.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027928.1.4",
          "form": "이제부터 너도 중국어 배우는 거야",
          "original_form": "이제부터 너도 중국어 배우는 거야",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027928.1.5",
          "form": "시러욥...",
          "original_form": "시러욥...",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027928.1.6",
          "form": "굿즈를 사기위해 해외판 산거라구요",
          "original_form": "굿즈를 사기위해 해외판 산거라구요",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027928.1.7",
          "form": "그럼 책이 온게 아니잖어",
          "original_form": "그럼 책이 온게 아니잖어",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027928.1.8",
          "form": "굿즈가 같이 온거지",
          "original_form": "굿즈가 같이 온거지",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027928.1.9",
          "form": "부록으로 온 책이 읽을수없는 언어로 써있음",
          "original_form": "부록으로 온 책이 읽을수없는 언어로 써있음",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027928.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027928.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ집 가면 인증샷 찍어죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ집 가면 인증샷 찍어죠",
          "speaker_id": "2",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027928.1.12",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100027928.1.13",
          "form": "이거 1부만 이었던가?",
          "original_form": "이거 1부만 이었던가?",
          "speaker_id": "1",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027928.1.14",
          "form": "내돈... 엄청 깨진다",
          "original_form": "내돈... 엄청 깨진다",
          "speaker_id": "1",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027928.1.15",
          "form": "아마....",
          "original_form": "아마....",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027928.1.16",
          "form": "전독시가 세권에 끝나진 않았을 것....",
          "original_form": "전독시가 세권에 끝나진 않았을 것....",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027928.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100027928.1.18",
          "form": "ㅠㅠ텀두고 나올테니깐...아마...",
          "original_form": "ㅠㅠ텀두고 나올테니깐...아마...",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027928.1.19",
          "form": "그렇겠지",
          "original_form": "그렇겠지",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027928.1.20",
          "form": "그런데 이제 한국판이 나온다면?",
          "original_form": "그런데 이제 한국판이 나온다면?",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027928.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027928.1.22",
          "form": "사야지ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사야지ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027928.1.23",
          "form": "짜잔 돈이 사라졌습니다",
          "original_form": "짜잔 돈이 사라졌습니다",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027928.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027928.1.25",
          "form": "몇년 굿즈도 안사고 살았더니 이런일이...",
          "original_form": "몇년 굿즈도 안사고 살았더니 이런일이...",
          "speaker_id": "1",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027928.1.26",
          "form": "해외판 책 사는사람들 신기했는데",
          "original_form": "해외판 책 사는사람들 신기했는데",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027928.1.27",
          "form": "그 짓을 내가 하다니?",
          "original_form": "그 짓을 내가 하다니?",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027928.1.28",
          "form": "셋쇼마루 동상을 이렇게 세우는 구나",
          "original_form": "셋쇼마루 동상을 이렇게 세우는 구나",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027928.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027928.1.30",
          "form": "한국 단행본 나오긴 나오는거 맞아?",
          "original_form": "한국 단행본 나오긴 나오는거 맞아?",
          "speaker_id": "1",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027928.1.31",
          "form": "통장사정은 별개로 갖고싶은데.. 한글로 된거...",
          "original_form": "통장사정은 별개로 갖고싶은데.. 한글로 된거...",
          "speaker_id": "1",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027928.1.32",
          "form": "먼가 연말 쯤 나온다 하지 않았어?",
          "original_form": "먼가 연말 쯤 나온다 하지 않았어?",
          "speaker_id": "2",
          "time": "20210902 16:41"
        },
        {
          "id": "MDRW2100027928.1.33",
          "form": "소식을 좀 던져줬음 좋겠다",
          "original_form": "소식을 좀 던져줬음 좋겠다",
          "speaker_id": "1",
