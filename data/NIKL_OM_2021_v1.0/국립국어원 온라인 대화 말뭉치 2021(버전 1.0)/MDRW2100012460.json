      "utterance": [
        {
          "id": "MDRW2100012460.1.1",
          "form": "여행 어디 가보셨어요?",
          "original_form": "여행 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012460.1.2",
          "form": "국내는 최근이라면 지리산..일려나..",
          "original_form": "국내는 최근이라면 지리산..일려나..",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012460.1.3",
          "form": "여행많이하세요?",
          "original_form": "여행많이하세요?",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012460.1.4",
          "form": "그리 많이 하지는 않아요",
          "original_form": "그리 많이 하지는 않아요",
          "speaker_id": "2",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012460.1.5",
          "form": "최근에 갔다온데 있나요?",
          "original_form": "최근에 갔다온데 있나요?",
          "speaker_id": "1",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100012460.1.6",
          "form": "제작년에 여수갔었어요",
          "original_form": "제작년에 여수갔었어요",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012460.1.7",
          "form": "여수밤바다...",
          "original_form": "여수밤바다...",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012460.1.8",
          "form": "재작년이라니... 엄청 오래전이네요..",
          "original_form": "재작년이라니... 엄청 오래전이네요..",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012460.1.9",
          "form": "코로나 때문이죠...",
          "original_form": "코로나 때문이죠...",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012460.1.10",
          "form": "그러게요.. 어딜가질 못해요..",
          "original_form": "그러게요.. 어딜가질 못해요..",
          "speaker_id": "1",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012460.1.11",
          "form": "근데 따지고보면 출근길 지하철이나 식당이 더 북적거림.. ㅋㅋㅋ",
          "original_form": "근데 따지고보면 출근길 지하철이나 식당이 더 북적거림.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012460.1.12",
          "form": "글킨해요... 그래도 거주지역한정 북적거리는게 낫죠...",
          "original_form": "글킨해요... 그래도 거주지역한정 북적거리는게 낫죠...",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100012460.1.13",
          "form": "해외여행은 자주가시나여?",
          "original_form": "해외여행은 자주가시나여?",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100012460.1.14",
          "form": "한번도 몬 가봤어요",
          "original_form": "한번도 몬 가봤어요",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100012460.1.15",
          "form": "코로나 풀림 가보고 싶은데 있어요?",
          "original_form": "코로나 풀림 가보고 싶은데 있어요?",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100012460.1.16",
          "form": "러시아요!!",
          "original_form": "러시아요!!",
          "speaker_id": "2",
          "time": "20210825 19:55"
        }
      ]
