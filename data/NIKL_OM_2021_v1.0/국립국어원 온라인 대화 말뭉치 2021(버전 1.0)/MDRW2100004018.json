      "utterance": [
        {
          "id": "MDRW2100004018.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004018.1.2",
          "form": "안녕하세요 여행 최근에 다녀오셨어요?",
          "original_form": "안녕하세요 여행 최근에 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004018.1.3",
          "form": "아뇨 코로나라서 아무데도 못갔어요ㅜㅜ",
          "original_form": "아뇨 코로나라서 아무데도 못갔어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004018.1.4",
          "form": "아 아쉽네요ㅠㅠ저는 짧게 다녀왔어요ㅜㅜ 해외 가고 싶었는데",
          "original_form": "아 아쉽네요ㅠㅠ저는 짧게 다녀왔어요ㅜㅜ 해외 가고 싶었는데",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004018.1.5",
          "form": "어디 다녀오셨어요??",
          "original_form": "어디 다녀오셨어요??",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004018.1.6",
          "form": "속초다녀왔어요ㅎㅎ",
          "original_form": "속초다녀왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004018.1.7",
          "form": "오오 재밌었겠네용",
          "original_form": "오오 재밌었겠네용",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004018.1.8",
          "form": "근데 거의 숙소에만 있어서 아쉬워요ㅠㅠ",
          "original_form": "근데 거의 숙소에만 있어서 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004018.1.9",
          "form": "해외 어디 가고싶으세요?",
          "original_form": "해외 어디 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004018.1.10",
          "form": "저 동남아 가고싶었는데...",
          "original_form": "저 동남아 가고싶었는데...",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004018.1.11",
          "form": "가고싶은곳 있으세요?",
          "original_form": "가고싶은곳 있으세요?",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004018.1.12",
          "form": "저는 일본이랑 대만 가보고 싶어요",
          "original_form": "저는 일본이랑 대만 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004018.1.13",
          "form": "일본이랑 대만 좋죠 얼른 코로나 끝나면ㅜㅠ",
          "original_form": "일본이랑 대만 좋죠 얼른 코로나 끝나면ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004018.1.14",
          "form": "옛날에 유럽 갔다온적 있는데 넘 옛날이라서 사진도 별로 못찍었고 기억도 잘 안나서 아쉬워요ㅜㅜ",
          "original_form": "옛날에 유럽 갔다온적 있는데 넘 옛날이라서 사진도 별로 못찍었고 기억도 잘 안나서 아쉬워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004018.1.15",
          "form": "저도 유럽 그립네요",
          "original_form": "저도 유럽 그립네요",
          "speaker_id": "2",
          "time": "20210821 14:38"
        }
      ]
