      "utterance": [
        {
          "id": "MDRW2100036828.1.1",
          "form": "점심먹으니 후식땡기네욤",
          "original_form": "점심먹으니 후식땡기네욤",
          "speaker_id": "1",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100036828.1.2",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036828.1.3",
          "form": "김치전이요!!!",
          "original_form": "김치전이요!!!",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036828.1.4",
          "form": "우와 사신건가요?",
          "original_form": "우와 사신건가요?",
          "speaker_id": "2",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100036828.1.5",
          "form": "엄마표로 먹었어용",
          "original_form": "엄마표로 먹었어용",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036828.1.6",
          "form": "엄청 맛있겠네요ㅠㅠㅠ",
          "original_form": "엄청 맛있겠네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036828.1.7",
          "form": "넵 진짜 맛있더라구요!!",
          "original_form": "넵 진짜 맛있더라구요!!",
          "speaker_id": "1",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100036828.1.8",
          "form": "좋아하시는 음식 있으신가요?",
          "original_form": "좋아하시는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 13:40"
        },
        {
          "id": "MDRW2100036828.1.9",
          "form": "저는 갈비 좋아해요!",
          "original_form": "저는 갈비 좋아해요!",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036828.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036828.1.11",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "2",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036828.1.12",
          "form": "오.. 어디 떡볶이가 최애세요?",
          "original_form": "오.. 어디 떡볶이가 최애세요?",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036828.1.13",
          "form": "네 name1님은 갈비가 최애신가요?",
          "original_form": "네 &name1&님은 갈비가 최애신가요?",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036828.1.14",
          "form": "넷ㅋㅋㅋ",
          "original_form": "넷ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100036828.1.15",
          "form": "갈비는 진짜 다 좋네요",
          "original_form": "갈비는 진짜 다 좋네요",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100036828.1.16",
          "form": "최근은 햇잎갈비가 맛있어용",
          "original_form": "최근은 햇잎갈비가 맛있어용",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100036828.1.17",
          "form": "햇잎이라는 브랜드가 있나요?",
          "original_form": "햇잎이라는 브랜드가 있나요?",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036828.1.18",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036828.1.19",
          "form": "명륜진사는 지점마다 맛이 달라서 아쉽더라구요",
          "original_form": "명륜진사는 지점마다 맛이 달라서 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036828.1.20",
          "form": "가격은 싸도 아쉬워요",
          "original_form": "가격은 싸도 아쉬워요",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036828.1.21",
          "form": "한번 찾아봐야겠어요",
          "original_form": "한번 찾아봐야겠어요",
