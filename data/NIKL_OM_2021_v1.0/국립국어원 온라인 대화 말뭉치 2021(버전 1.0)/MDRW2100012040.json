      "utterance": [
        {
          "id": "MDRW2100012040.1.1",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012040.1.2",
          "form": "저 이제 먹으려구요!",
          "original_form": "저 이제 먹으려구요!",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012040.1.3",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012040.1.4",
          "form": "저도 아직안먹었는데",
          "original_form": "저도 아직안먹었는데",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012040.1.5",
          "form": "떡볶이 만들어먹을까 고민중이에요",
          "original_form": "떡볶이 만들어먹을까 고민중이에요",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012040.1.6",
          "form": "뭐드실거에요?",
          "original_form": "뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012040.1.7",
          "form": "헐 떡볶이 좋아해요 맛있겠다",
          "original_form": "헐 떡볶이 좋아해요 맛있겠다",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012040.1.8",
          "form": "로제떡볶이 먹고싶네요",
          "original_form": "로제떡볶이 먹고싶네요",
          "speaker_id": "2",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012040.1.9",
          "form": "로제떡볶이 저도 완전좋아해요",
          "original_form": "로제떡볶이 저도 완전좋아해요",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012040.1.10",
          "form": "저는 아직 메뉴를 못골랐는데 그냥 집밥될꺼같아요!",
          "original_form": "저는 아직 메뉴를 못골랐는데 그냥 집밥될꺼같아요!",
          "speaker_id": "2",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012040.1.11",
          "form": "국에다가 불고기 해동해서요",
          "original_form": "국에다가 불고기 해동해서요",
          "speaker_id": "2",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012040.1.12",
          "form": "오 맛있겠네요",
          "original_form": "오 맛있겠네요",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012040.1.13",
          "form": "전집에 먹을게 떡밖에없어서 지금 ㅋㅋㅋ",
          "original_form": "전집에 먹을게 떡밖에없어서 지금 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012040.1.14",
          "form": "떡볶이 밀떡파세요쌀떡파세요",
          "original_form": "떡볶이 밀떡파세요쌀떡파세요",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.15",
          "form": "저 지금은 밀떡이요!",
          "original_form": "저 지금은 밀떡이요!",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.16",
          "form": "전엔 쌀떡이였는데",
          "original_form": "전엔 쌀떡이였는데",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.17",
          "form": "크면서 취향이 바뀌었어요",
          "original_form": "크면서 취향이 바뀌었어요",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.18",
          "form": "ㅎㅎ저도 자주바뀌기는하는데 요즘은 밀떡이에요",
          "original_form": "ㅎㅎ저도 자주바뀌기는하는데 요즘은 밀떡이에요",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.19",
          "form": "어떤걸 더 좋아하세요?",
          "original_form": "어떤걸 더 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.20",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.21",
          "form": "소스가 잘 베여들어서",
          "original_form": "소스가 잘 베여들어서",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.22",
          "form": "맞아요 그게 또 무시못해요",
          "original_form": "맞아요 그게 또 무시못해요",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.23",
          "form": "쌀떡은 못하는집은 따로놀때가 많더라구요",
          "original_form": "쌀떡은 못하는집은 따로놀때가 많더라구요",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012040.1.24",
          "form": "당면이나 이런건 안넣으세요?",
          "original_form": "당면이나 이런건 안넣으세요?",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012040.1.25",
          "form": "중국당면넣을때도있는데 집에서만들때는 떡이랑오뎅 야채이렇게만 넣어요",
          "original_form": "중국당면넣을때도있는데 집에서만들때는 떡이랑오뎅 야채이렇게만 넣어요",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012040.1.26",
          "form": "뚜또님은 당면넣어드시나요",
          "original_form": "뚜또님은 당면넣어드시나요",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012040.1.27",
          "form": "저 중국당면 많이 넣어먹어요!",
          "original_form": "저 중국당면 많이 넣어먹어요!",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012040.1.28",
          "form": "근데 불리기가 귀찮아서",
          "original_form": "근데 불리기가 귀찮아서",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012040.1.29",
          "form": "맞아요 불리기넘귀찮아요",
          "original_form": "맞아요 불리기넘귀찮아요",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012040.1.30",
          "form": "먹을땐 떡보다 맛있는데 ㅎㅎㅎㅎ",
          "original_form": "먹을땐 떡보다 맛있는데 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012040.1.31",
          "form": "그래서 맘먹고 넣어야해요..",
          "original_form": "그래서 맘먹고 넣어야해요..",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012040.1.32",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210824 18:42"
        }
      ]
    }
  ]
