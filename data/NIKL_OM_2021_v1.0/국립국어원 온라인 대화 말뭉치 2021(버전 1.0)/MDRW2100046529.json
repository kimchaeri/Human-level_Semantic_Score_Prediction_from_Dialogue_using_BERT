      "utterance": [
        {
          "id": "MDRW2100046529.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211028 17:07"
        },
        {
          "id": "MDRW2100046529.1.2",
          "form": "안녕하세요! 좋아하는 콘텐츠 있으신가요?",
          "original_form": "안녕하세요! 좋아하는 콘텐츠 있으신가요?",
          "speaker_id": "2",
          "time": "20211028 17:07"
        },
        {
          "id": "MDRW2100046529.1.3",
          "form": "유튜그 낚시채널 자주봐요",
          "original_form": "유튜그 낚시채널 자주봐요",
          "speaker_id": "1",
          "time": "20211028 17:08"
        },
        {
          "id": "MDRW2100046529.1.4",
          "form": "유튜브",
          "original_form": "유튜브",
          "speaker_id": "1",
          "time": "20211028 17:08"
        },
        {
          "id": "MDRW2100046529.1.5",
          "form": "와 낚시 좋아하시나봐요",
          "original_form": "와 낚시 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20211028 17:08"
        },
        {
          "id": "MDRW2100046529.1.6",
          "form": "직접 가기도 하시나요",
          "original_form": "직접 가기도 하시나요",
          "speaker_id": "2",
          "time": "20211028 17:08"
        },
        {
          "id": "MDRW2100046529.1.7",
          "form": "네 자주 가요 갯바위 낚시랑 배낚시 둘다 즐겨해요",
          "original_form": "네 자주 가요 갯바위 낚시랑 배낚시 둘다 즐겨해요",
          "speaker_id": "1",
          "time": "20211028 17:09"
        },
        {
          "id": "MDRW2100046529.1.8",
          "form": "오오.. 저는 낚시를 한번도 안해봐서..",
          "original_form": "오오.. 저는 낚시를 한번도 안해봐서..",
          "speaker_id": "2",
          "time": "20211028 17:10"
        },
        {
          "id": "MDRW2100046529.1.9",
          "form": "요즘 갈치낚시랑 쭈꾸미 갑오징어 잡고 있어요",
          "original_form": "요즘 갈치낚시랑 쭈꾸미 갑오징어 잡고 있어요",
          "speaker_id": "1",
          "time": "20211028 17:10"
        },
        {
          "id": "MDRW2100046529.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20211028 17:10"
        },
        {
          "id": "MDRW2100046529.1.11",
          "form": "철따라 잡히는게 다른가보군요",
          "original_form": "철따라 잡히는게 다른가보군요",
          "speaker_id": "2",
          "time": "20211028 17:10"
        },
        {
          "id": "MDRW2100046529.1.12",
          "form": "여수 바닷가라 쉽게 갈수있어서 자주 갑니다",
          "original_form": "여수 바닷가라 쉽게 갈수있어서 자주 갑니다",
          "speaker_id": "1",
          "time": "20211028 17:10"
        },
        {
          "id": "MDRW2100046529.1.13",
          "form": "재미가 쏠쏠하겠어요",
          "original_form": "재미가 쏠쏠하겠어요",
          "speaker_id": "2",
          "time": "20211028 17:11"
        },
        {
          "id": "MDRW2100046529.1.14",
          "form": "네 계절따라 손맛이 다르죠",
          "original_form": "네 계절따라 손맛이 다르죠",
          "speaker_id": "1",
          "time": "20211028 17:11"
        },
        {
          "id": "MDRW2100046529.1.15",
          "form": "되게 실용적이고 재밌는 취미를 가지셨네요",
          "original_form": "되게 실용적이고 재밌는 취미를 가지셨네요",
          "speaker_id": "2",
          "time": "20211028 17:11"
        },
        {
          "id": "MDRW2100046529.1.16",
          "form": "돈이  그만큼 많이 듭니다",
          "original_form": "돈이  그만큼 많이 듭니다",
          "speaker_id": "1",
          "time": "20211028 17:11"
        },
        {
          "id": "MDRW2100046529.1.17",
          "form": "ㅋㅋ그래도 한번 손맛들이면 벗어날 수 없다던데요",
          "original_form": "ㅋㅋ그래도 한번 손맛들이면 벗어날 수 없다던데요",
          "speaker_id": "2",
          "time": "20211028 17:11"
        },
        {
          "id": "MDRW2100046529.1.18",
          "form": "배 한번 타는데...마누라 눈치를 많이 보네요",
          "original_form": "배 한번 타는데...마누라 눈치를 많이 보네요",
          "speaker_id": "1",
          "time": "20211028 17:11"
        },
        {
          "id": "MDRW2100046529.1.19",
          "form": "그맛에 계속 갑니다",
          "original_form": "그맛에 계속 갑니다",
          "speaker_id": "1",
          "time": "20211028 17:12"
        },
        {
          "id": "MDRW2100046529.1.20",
          "form": "어유 그래도 맛있는거 많이 잡으시는데",
          "original_form": "어유 그래도 맛있는거 많이 잡으시는데",
          "speaker_id": "2",
          "time": "20211028 17:12"
        }
      ]
