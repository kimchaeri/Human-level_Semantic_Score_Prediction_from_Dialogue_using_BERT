      "utterance": [
        {
          "id": "MDRW2100004084.1.1",
          "form": "난 요즘 건강을 챙기고 있어...",
          "original_form": "난 요즘 건강을 챙기고 있어...",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004084.1.2",
          "form": "위가 약해졌거든..",
          "original_form": "위가 약해졌거든..",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004084.1.3",
          "form": "헉 ~ 위가안좋을땐.. 양배추환인가?",
          "original_form": "헉 ~ 위가안좋을땐.. 양배추환인가?",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004084.1.4",
          "form": "건강 조심혀~",
          "original_form": "건강 조심혀~",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004084.1.5",
          "form": "그래 너도~~!! 너는 건강하니????",
          "original_form": "그래 너도~~!! 너는 건강하니????",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004084.1.6",
          "form": "나는 눈건강이 안좋아..",
          "original_form": "나는 눈건강이 안좋아..",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004084.1.7",
          "form": "컴퓨터랑 핸드폰을 많이봐서.. 더그런거같아 ㅋㅋㅋ",
          "original_form": "컴퓨터랑 핸드폰을 많이봐서.. 더그런거같아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004084.1.8",
          "form": "눈을 자주 깜빡여줘야해",
          "original_form": "눈을 자주 깜빡여줘야해",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004084.1.9",
          "form": "아하 ㅠㅠㅠ 안구가 건조하구나...",
          "original_form": "아하 ㅠㅠㅠ 안구가 건조하구나...",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004084.1.10",
          "form": "나도 휴대폰을 진짜 많이 보거든",
          "original_form": "나도 휴대폰을 진짜 많이 보거든",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004084.1.11",
          "form": "안구 건강엔 뭐가 좋을까?",
          "original_form": "안구 건강엔 뭐가 좋을까?",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004084.1.12",
          "form": "블루베리??",
          "original_form": "블루베리??",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004084.1.13",
          "form": "루테인 요새 많이들 사먹잖아",
          "original_form": "루테인 요새 많이들 사먹잖아",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004084.1.14",
          "form": "근데 사실 그거 별 효과없다더라",
          "original_form": "근데 사실 그거 별 효과없다더라",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004084.1.15",
          "form": "어제 유튜브에서봣어.. ㅋㅋㅋㅋㅋ",
          "original_form": "어제 유튜브에서봣어.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004084.1.16",
          "form": "그냥 먼 곳을 자주 봐주는게 제일좋대...^_T",
          "original_form": "그냥 먼 곳을 자주 봐주는게 제일좋대...^_T",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004084.1.17",
          "form": "앗 ㅠㅠㅠ",
          "original_form": "앗 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004084.1.18",
          "form": "방금 너말 듣고 먼곳을 한번 봤다...",
          "original_form": "방금 너말 듣고 먼곳을 한번 봤다...",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004084.1.19",
          "form": "하하...눈아 건강해져라...",
          "original_form": "하하...눈아 건강해져라...",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004084.1.20",
          "form": "코로나때문에 밖에 잘 안나가서 그런지",
          "original_form": "코로나때문에 밖에 잘 안나가서 그런지",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004084.1.21",
          "form": "미묘하게 건강이 나빠지는거같아",
          "original_form": "미묘하게 건강이 나빠지는거같아",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004084.1.22",
          "form": "그렇지않니?",
          "original_form": "그렇지않니?",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004084.1.23",
          "form": "맞아 .... 확실히 그래...",
          "original_form": "맞아 .... 확실히 그래...",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004084.1.24",
          "form": "건강이 평생인데 말이지",
          "original_form": "건강이 평생인데 말이지",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004084.1.25",
          "form": "어제도  건강을 위해 조깅해야지 했는데",
          "original_form": "어제도  건강을 위해 조깅해야지 했는데",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004084.1.26",
          "form": "지금도 누워잇다..",
          "original_form": "지금도 누워잇다..",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004084.1.27",
          "form": "어쩔수없다..",
          "original_form": "어쩔수없다..",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004084.1.28",
          "form": "비가오니깐.. 오늘만 쉬는거야^^",
          "original_form": "비가오니깐.. 오늘만 쉬는거야^^",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004084.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 여기는...비가 그쳤는걸...?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 여기는...비가 그쳤는걸...?",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004084.1.30",
          "form": "너는 건강을 위해 하고있는게 있니???",
          "original_form": "너는 건강을 위해 하고있는게 있니???",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004084.1.31",
          "form": "건강이라...",
          "original_form": "건강이라...",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004084.1.32",
          "form": "따로 운동하는게 없어서 그냥 강변을 걷거나 뛰는편인데",
          "original_form": "따로 운동하는게 없어서 그냥 강변을 걷거나 뛰는편인데",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004084.1.33",
          "form": "런데이라는 어플 알아?",
          "original_form": "런데이라는 어플 알아?",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004084.1.34",
          "form": "오~~알지알지!",
          "original_form": "오~~알지알지!",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004084.1.35",
          "form": "항상 깔고 몇 번 하다가",
          "original_form": "항상 깔고 몇 번 하다가",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004084.1.36",
          "form": "...삭제되는 ㅠㅠ",
          "original_form": "...삭제되는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004084.1.37",
          "form": "요즘 날씨가 좋아져서 런데이하기 좋겠다~!",
          "original_form": "요즘 날씨가 좋아져서 런데이하기 좋겠다~!",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004084.1.38",
          "form": "ㅋㅋㅋㅋㅋ삭제되는",
          "original_form": "ㅋㅋㅋㅋㅋ삭제되는",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004084.1.39",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004084.1.40",
          "form": "우리 이 대화를 계기로 ... 내일부터는 열심히 운동해보자고",
          "original_form": "우리 이 대화를 계기로 ... 내일부터는 열심히 운동해보자고",
