          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031995.1.4",
          "form": "컴터가",
          "original_form": "컴터가",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031995.1.5",
          "form": "엄청 멍청한거 알아요?",
          "original_form": "엄청 멍청한거 알아요?",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031995.1.6",
          "form": "사람이 다 해줘야",
          "original_form": "사람이 다 해줘야",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031995.1.7",
          "form": "돌아가요",
          "original_form": "돌아가요",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031995.1.8",
          "form": "로봇청소기 청소 시켜놔도 결국 본인이 청소하게되는",
          "original_form": "로봇청소기 청소 시켜놔도 결국 본인이 청소하게되는",
          "speaker_id": "2",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031995.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031995.1.10",
          "form": "제가 컴공 가려고 간게 아니라",
          "original_form": "제가 컴공 가려고 간게 아니라",
          "speaker_id": "1",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031995.1.11",
          "form": "점수 맞춰서 간건데",
          "original_form": "점수 맞춰서 간건데",
          "speaker_id": "1",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031995.1.12",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031995.1.13",
          "form": "그 컴퓨터언어들 공부하면서",
          "original_form": "그 컴퓨터언어들 공부하면서",
          "speaker_id": "1",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031995.1.14",
          "form": "느낀건",
          "original_form": "느낀건",
          "speaker_id": "1",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031995.1.15",
          "form": "아 컴터 계산빠른거 빼고ㆍ",
          "original_form": "아 컴터 계산빠른거 빼고ㆍ",
          "speaker_id": "1",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031995.1.16",
          "form": "할수있는게 없구나",
          "original_form": "할수있는게 없구나",
          "speaker_id": "1",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031995.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031995.1.18",
          "form": "그쵸 전문가들 필요하져",
          "original_form": "그쵸 전문가들 필요하져",
          "speaker_id": "2",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031995.1.19",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "1",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031995.1.20",
          "form": "근데 전공으로 배우면서 좀 컴퓨터처럼 생각하게 배우지 않앗나여",
          "original_form": "근데 전공으로 배우면서 좀 컴퓨터처럼 생각하게 배우지 않앗나여",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.21",
          "form": "나이 60먹고 코딩이 가능한직업인지",
          "original_form": "나이 60먹고 코딩이 가능한직업인지",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.22",
          "form": "전 전남친이 너무 알고리즘같아서 웃겼는데",
          "original_form": "전 전남친이 너무 알고리즘같아서 웃겼는데",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.24",
          "form": "생각을 컴터 입장에서 하니까",
          "original_form": "생각을 컴터 입장에서 하니까",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.25",
          "form": "기계같죠",
          "original_form": "기계같죠",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.26",
          "form": "아 전남친도 한 45살까지가 최선같다고 하더라구요",
          "original_form": "아 전남친도 한 45살까지가 최선같다고 하더라구요",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.27",
          "form": "전 도망친게",
          "original_form": "전 도망친게",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.28",
          "form": "네ㅋㅋㅋㅋㅋ 입력된대로 출력을 해서",
          "original_form": "네ㅋㅋㅋㅋㅋ 입력된대로 출력을 해서",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.29",
          "form": "그 이후에 뭘할까",
          "original_form": "그 이후에 뭘할까",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031995.1.30",
          "form": "생각해보면",
          "original_form": "생각해보면",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031995.1.31",
          "form": "결국 다른분야 도전인데",
          "original_form": "결국 다른분야 도전인데",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031995.1.32",
          "form": "그럴바엔 빨리 가자",
          "original_form": "그럴바엔 빨리 가자",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031995.1.33",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031995.1.34",
          "form": "공무원이 그런면에서 좋은거 같아요",
          "original_form": "공무원이 그런면에서 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031995.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031995.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031995.1.37",
          "form": "가늘고 길게",
          "original_form": "가늘고 길게",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031995.1.38",
          "form": "그쵸 최상은 공기업",
          "original_form": "그쵸 최상은 공기업",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031995.1.39",
          "form": "이게 계속 컴터보고하니까",
          "original_form": "이게 계속 컴터보고하니까",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031995.1.40",
          "form": "노안도 빨리와요",
          "original_form": "노안도 빨리와요",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031995.1.41",
          "form": "내눈 ㅜㅜ",
          "original_form": "내눈 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031995.1.42",
          "form": "여러모로 체력이 빨리 소진되는 직업같긴 했어요",
          "original_form": "여러모로 체력이 빨리 소진되는 직업같긴 했어요",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031995.1.43",
          "form": "그래도 전남친이 돈을 진짜 마니 벌긴해서 너무 부럽던데",
          "original_form": "그래도 전남친이 돈을 진짜 마니 벌긴해서 너무 부럽던데",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031995.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031995.1.45",
          "form": "내가 몇년이나 벌어야 하는 연봉인데",
          "original_form": "내가 몇년이나 벌어야 하는 연봉인데",
          "speaker_id": "2",
          "time": "20210905 10:41"
        },
        {
          "id": "MDRW2100031995.1.46",
          "form": "많이 부러우셨나여",
          "original_form": "많이 부러우셨나여",
          "speaker_id": "1",
          "time": "20210905 10:41"
        },
        {
          "id": "MDRW2100031995.1.47",
          "form": "한 해에 다 버네...",
          "original_form": "한 해에 다 버네...",
          "speaker_id": "2",
          "time": "20210905 10:41"
        },
        {
          "id": "MDRW2100031995.1.48",
          "form": "이런 느낌",
          "original_form": "이런 느낌",
          "speaker_id": "2",
          "time": "20210905 10:41"
        },
        {
          "id": "MDRW2100031995.1.49",
          "form": "나이들면서 느낀거지만",
          "original_form": "나이들면서 느낀거지만",
          "speaker_id": "1",
          "time": "20210905 10:41"
        },
        {
          "id": "MDRW2100031995.1.50",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100031995.1.51",
          "form": "점점 나이들면서 일을 너무  많이하면",
          "original_form": "점점 나이들면서 일을 너무  많이하면",
          "speaker_id": "1",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100031995.1.52",
          "form": "번돈이 다 병원비로 나가요",
          "original_form": "번돈이 다 병원비로 나가요",
          "speaker_id": "1",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100031995.1.53",
          "form": "그건 그래요 ㅋㅋㅋㅋ",
          "original_form": "그건 그래요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100031995.1.54",
          "form": "근데 쌤들도 다 병원가고 개발자도 다 병원가니깐",
          "original_form": "근데 쌤들도 다 병원가고 개발자도 다 병원가니깐",
