        },
        {
          "id": "MDRW2100036598.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 01:36"
        },
        {
          "id": "MDRW2100036598.1.3",
          "form": "저녁 아예 안드셧아요?",
          "original_form": "저녁 아예 안드셧아요?",
          "speaker_id": "1",
          "time": "20210831 01:36"
        },
        {
          "id": "MDRW2100036598.1.4",
          "form": "아니요 9시쯤 간단하게 조금 먹었어요ㅋㅋ",
          "original_form": "아니요 9시쯤 간단하게 조금 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:37"
        },
        {
          "id": "MDRW2100036598.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\n아하 뭐드셧어여?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\n아하 뭐드셧어여?",
          "speaker_id": "1",
          "time": "20210831 01:37"
        },
        {
          "id": "MDRW2100036598.1.6",
          "form": "밥 반공기+계란 스크램블+간장 양념 조금 넣어서요",
          "original_form": "밥 반공기+계란 스크램블+간장 양념 조금 넣어서요",
          "speaker_id": "2",
          "time": "20210831 01:39"
        },
        {
          "id": "MDRW2100036598.1.7",
          "form": "오... 배 안부르세여? 지금?",
          "original_form": "오... 배 안부르세여? 지금?",
          "speaker_id": "1",
          "time": "20210831 01:39"
        },
        {
          "id": "MDRW2100036598.1.8",
          "form": "오후 4~5시 이후로 아무것도 안 먹기에는 넘 배고프니까요ㅋㅋㅋㅋ",
          "original_form": "오후 4~5시 이후로 아무것도 안 먹기에는 넘 배고프니까요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:40"
        },
        {
          "id": "MDRW2100036598.1.9",
          "form": "지금 다시 배가 조금씩 고파지고 있어요",
          "original_form": "지금 다시 배가 조금씩 고파지고 있어요",
          "speaker_id": "2",
          "time": "20210831 01:40"
        },
        {
          "id": "MDRW2100036598.1.10",
          "form": "옼ㅋㅋㅋ 원래 밥 분량이 어떠신데여",
          "original_form": "옼ㅋㅋㅋ 원래 밥 분량이 어떠신데여",
          "speaker_id": "1",
          "time": "20210831 01:40"
        },
        {
          "id": "MDRW2100036598.1.11",
          "form": "그냥 1공기요",
          "original_form": "그냥 1공기요",
          "speaker_id": "2",
          "time": "20210831 01:41"
        },
        {
          "id": "MDRW2100036598.1.12",
          "form": "꽉 채우지 않고 허술하게 넣은 1공기랄까요?",
          "original_form": "꽉 채우지 않고 허술하게 넣은 1공기랄까요?",
          "speaker_id": "2",
          "time": "20210831 01:42"
        },
        {
          "id": "MDRW2100036598.1.13",
          "form": "오..많이드시는데여?? 밥그릇 코렐 아니죠?",
          "original_form": "오..많이드시는데여?? 밥그릇 코렐 아니죠?",
          "speaker_id": "1",
          "time": "20210831 01:42"
        },
        {
          "id": "MDRW2100036598.1.14",
          "form": "밥 1공기가 많이 먹는건가요?",
          "original_form": "밥 1공기가 많이 먹는건가요?",
          "speaker_id": "2",
          "time": "20210831 01:42"
        },
        {
          "id": "MDRW2100036598.1.15",
          "form": "어느 제품인지는 모르겠네요",
          "original_form": "어느 제품인지는 모르겠네요",
          "speaker_id": "2",
          "time": "20210831 01:42"
        },
        {
          "id": "MDRW2100036598.1.16",
          "form": "아..그냥 소화가 잘되는것같아여",
          "original_form": "아..그냥 소화가 잘되는것같아여",
          "speaker_id": "1",
          "time": "20210831 01:43"
        },
        {
          "id": "MDRW2100036598.1.17",
          "form": "밥 공기가 좀 작은 편이긴 해요",
          "original_form": "밥 공기가 좀 작은 편이긴 해요",
          "speaker_id": "2",
          "time": "20210831 01:43"
        },
        {
          "id": "MDRW2100036598.1.18",
          "form": "조금먹고 그만큼 소화가 또 빨리 되는 편이긴 하죠",
          "original_form": "조금먹고 그만큼 소화가 또 빨리 되는 편이긴 하죠",
          "speaker_id": "2",
          "time": "20210831 01:43"
        },
        {
          "id": "MDRW2100036598.1.19",
          "form": "ㅋㅋㅋㅋ 그렇군요. 잡곡밥드세여?",
          "original_form": "ㅋㅋㅋㅋ 그렇군요. 잡곡밥드세여?",
          "speaker_id": "1",
          "time": "20210831 01:43"
        },
        {
          "id": "MDRW2100036598.1.20",
          "form": "저도 소식해야하는데..",
          "original_form": "저도 소식해야하는데..",
          "speaker_id": "1",
          "time": "20210831 01:44"
        }
      ]
