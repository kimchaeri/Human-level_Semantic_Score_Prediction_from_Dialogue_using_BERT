      "utterance": [
        {
          "id": "MDRW2100004125.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004125.1.2",
          "form": "혹시 즐겁게 읽은 책이 있으신가요?",
          "original_form": "혹시 즐겁게 읽은 책이 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004125.1.3",
          "form": "네 그리스로마신화를 재밌게 읽었어요!",
          "original_form": "네 그리스로마신화를 재밌게 읽었어요!",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004125.1.4",
          "form": "신화속 이야기들이 너무 신기하고 재밌더라구요",
          "original_form": "신화속 이야기들이 너무 신기하고 재밌더라구요",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004125.1.5",
          "form": "name2님은 재밌게 읽은 도서가 있나요?",
          "original_form": "&name2&님은 재밌게 읽은 도서가 있나요?",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004125.1.6",
          "form": "그리스로마신화 어릴때 재밌게 읽은 기억이 있어여",
          "original_form": "그리스로마신화 어릴때 재밌게 읽은 기억이 있어여",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004125.1.7",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004125.1.8",
          "form": "나미야 잡화점의 기적이요",
          "original_form": "나미야 잡화점의 기적이요",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004125.1.9",
          "form": "영화화도 된 책이네요!!",
          "original_form": "영화화도 된 책이네요!!",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004125.1.10",
          "form": "한번도 읽어본적 없는데 재밌었나요?",
          "original_form": "한번도 읽어본적 없는데 재밌었나요?",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004125.1.11",
          "form": "저도 이야기만 들어보다가",
          "original_form": "저도 이야기만 들어보다가",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004125.1.12",
          "form": "최근에 읽었는데",
          "original_form": "최근에 읽었는데",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004125.1.13",
          "form": "베스트셀러인 이유가 있더라고요",
          "original_form": "베스트셀러인 이유가 있더라고요",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004125.1.14",
          "form": "name1님도 꼭 읽어보세요",
          "original_form": "&name1&님도 꼭 읽어보세요",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004125.1.15",
          "form": "한 번도 안 쉬고 다 읽었어요",
          "original_form": "한 번도 안 쉬고 다 읽었어요",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004125.1.16",
          "form": "오 네 재밌을거같아요",
          "original_form": "오 네 재밌을거같아요",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004125.1.17",
          "form": "추천해주셔서 감사해요!!",
          "original_form": "추천해주셔서 감사해요!!",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004125.1.18",
          "form": "상도 받은 작품인가요?",
          "original_form": "상도 받은 작품인가요?",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004125.1.19",
          "form": "잘 모르겠는데 검색해보고 오겠습니다",
          "original_form": "잘 모르겠는데 검색해보고 오겠습니다",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004125.1.20",
          "form": "네. 뭔가... 상을 많이 받았네요ㅋㅋㅋㅋ",
          "original_form": "네. 뭔가... 상을 많이 받았네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100004125.1.21",
          "form": "그래도 히가시노 게이고하면 추리소설이 인기가 많잖아요?",
          "original_form": "그래도 히가시노 게이고하면 추리소설이 인기가 많잖아요?",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004125.1.22",
          "form": "개인적으로 저는 추리소설을 선호하진 않는데",
          "original_form": "개인적으로 저는 추리소설을 선호하진 않는데",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004125.1.23",
          "form": "추리소설 좋아하시나요?",
          "original_form": "추리소설 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004125.1.24",
          "form": "오 맞아요 저도 들어봤어요",
          "original_form": "오 맞아요 저도 들어봤어요",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004125.1.25",
          "form": "아니요 저도 선호하진않아요ㅜㅠ",
          "original_form": "아니요 저도 선호하진않아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004125.1.26",
          "form": "주변친구들은 추리소설을 좋아하더라고요",
          "original_form": "주변친구들은 추리소설을 좋아하더라고요",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004125.1.27",
          "form": "요즘도 추리소설은 꾸준히 인기가 많은편인가봐요?!",
          "original_form": "요즘도 추리소설은 꾸준히 인기가 많은편인가봐요?!",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004125.1.28",
          "form": "아마도 그렇겠죠? 인스타그램같은곳에서도",
          "original_form": "아마도 그렇겠죠? 인스타그램같은곳에서도",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004125.1.29",
          "form": "추리소설 광고가 많이 뜨더라구요.",
          "original_form": "추리소설 광고가 많이 뜨더라구요.",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004125.1.30",
          "form": "최근에는 [1인 가구 돈 관리]나",
          "original_form": "최근에는 [1인 가구 돈 관리]나",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004125.1.31",
          "form": "주식 서적같은 실생활에 도움되는 책을",
          "original_form": "주식 서적같은 실생활에 도움되는 책을",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004125.1.32",
          "form": "많이 보고 있는데",
          "original_form": "많이 보고 있는데",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004125.1.33",
          "form": "좋은 것 같아요.",
          "original_form": "좋은 것 같아요.",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004125.1.34",
          "form": "오 재테크 관련 책이시군요",
          "original_form": "오 재테크 관련 책이시군요",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004125.1.35",
          "form": "저도 관심있는데 간단히 책추천 아무거나 해주실수있나요?",
          "original_form": "저도 관심있는데 간단히 책추천 아무거나 해주실수있나요?",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004125.1.36",
          "form": "유수진의 부자언니 부자연습",
          "original_form": "유수진의 부자언니 부자연습",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004125.1.37",
          "form": "좋은 것 같아요ㅎㅎ",
          "original_form": "좋은 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004125.1.38",
          "form": "감사해요 구매해서 읽어보고 재테크로 관리해봐야겠어요",
          "original_form": "감사해요 구매해서 읽어보고 재테크로 관리해봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004125.1.39",
          "form": "쉽게 쉽게 넘어가는 재테크 책이라",
          "original_form": "쉽게 쉽게 넘어가는 재테크 책이라",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004125.1.40",
          "form": "추천이에요ㅎㅎ",
          "original_form": "추천이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004125.1.41",
          "form": "오늘 대화 즐거웠습니다!!",
          "original_form": "오늘 대화 즐거웠습니다!!",
