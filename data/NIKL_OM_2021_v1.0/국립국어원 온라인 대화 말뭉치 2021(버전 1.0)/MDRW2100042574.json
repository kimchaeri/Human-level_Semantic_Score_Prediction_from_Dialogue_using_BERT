      "utterance": [
        {
          "id": "MDRW2100042574.1.1",
          "form": "당분덕에 힘 팡팡 나고요 미식가시네요",
          "original_form": "당분덕에 힘 팡팡 나고요 미식가시네요",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.2",
          "form": "동물 좋아하세요??",
          "original_form": "동물 좋아하세요??",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.3",
          "form": "보는거좋아해요",
          "original_form": "보는거좋아해요",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.4",
          "form": "ㅎㅎㅎㅇ 미식가라니..감사합니다^^",
          "original_form": "ㅎㅎㅎㅇ 미식가라니..감사합니다^^",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.5",
          "form": "ㅎㅎ 저도 반려동물 키우지는 못하고",
          "original_form": "ㅎㅎ 저도 반려동물 키우지는 못하고",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.6",
          "form": "고양이 좋아해요",
          "original_form": "고양이 좋아해요",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.7",
          "form": "키우는건 힘들어서 못할듯 ㅠㅠ",
          "original_form": "키우는건 힘들어서 못할듯 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.8",
          "form": "5살 딸아이가 동물을 너무 좋아하고 \r\n그중에서",
          "original_form": "5살 딸아이가 동물을 너무 좋아하고 \r\n그중에서",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.9",
          "form": "고양이를 가장 사랑하거든요",
          "original_form": "고양이를 가장 사랑하거든요",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042574.1.10",
          "form": "덕분에 동물을 별로 좋아하지 않는 저까지",
          "original_form": "덕분에 동물을 별로 좋아하지 않는 저까지",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.11",
          "form": "고양이를 좋아하게 됐어요^^",
          "original_form": "고양이를 좋아하게 됐어요^^",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.12",
          "form": "오오 도도한고영이",
          "original_form": "오오 도도한고영이",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.13",
          "form": "멋지죠",
          "original_form": "멋지죠",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.14",
          "form": "네\r\n너무 귀여운거 같아요",
          "original_form": "네\r\n너무 귀여운거 같아요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.15",
          "form": "키우고 싶은데",
          "original_form": "키우고 싶은데",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.16",
          "form": "아직 엄두가 안나네요",
          "original_form": "아직 엄두가 안나네요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.17",
          "form": "좋아하는 이유가 따님이 좋아해서라니 스윗하세요",
          "original_form": "좋아하는 이유가 따님이 좋아해서라니 스윗하세요",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.18",
          "form": "키우는건 넘 어렵죠",
          "original_form": "키우는건 넘 어렵죠",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042574.1.19",
          "form": "손이 엄청 가서...",
          "original_form": "손이 엄청 가서...",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042574.1.20",
          "form": "네 키우는건 힘들거같은데",
          "original_form": "네 키우는건 힘들거같은데",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042574.1.21",
          "form": "너무 키우고 싶어하니",
          "original_form": "너무 키우고 싶어하니",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042574.1.22",
          "form": "일단 8살 초등학교 가서 스스로 할 수 있는일이 많아지면 다시 생각해보려구요^^",
          "original_form": "일단 8살 초등학교 가서 스스로 할 수 있는일이 많아지면 다시 생각해보려구요^^",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042574.1.23",
          "form": "전 고양이 유튜브보면서 느낀건데  넘 부지런해야겠더라구요",
          "original_form": "전 고양이 유튜브보면서 느낀건데  넘 부지런해야겠더라구요",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042574.1.24",
          "form": "아플때도 넘 맘아프고 ㅠㅠ",
          "original_form": "아플때도 넘 맘아프고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
