      "utterance": [
        {
          "id": "MDRW2100044365.1.1",
          "form": "영화좋아하세요?",
          "original_form": "영화좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044365.1.2",
          "form": "엄청 좋아해요!",
          "original_form": "엄청 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044365.1.3",
          "form": "최근에 헥소고지라는 영화 티비에서 해줘서 봤는데 좋았어요",
          "original_form": "최근에 헥소고지라는 영화 티비에서 해줘서 봤는데 좋았어요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044365.1.4",
          "form": "무슨장르인가요",
          "original_form": "무슨장르인가요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044365.1.5",
          "form": "전쟁영화에요 2차 세계대전이 배경인가 그럴거에요",
          "original_form": "전쟁영화에요 2차 세계대전이 배경인가 그럴거에요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044365.1.6",
          "form": "슬픈가요ㅜ",
          "original_form": "슬픈가요ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044365.1.7",
          "form": "감동적인? 내용이에요",
          "original_form": "감동적인? 내용이에요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044365.1.8",
          "form": "고지 점령전이 계속 되는게",
          "original_form": "고지 점령전이 계속 되는게",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044365.1.9",
          "form": "다들 후퇴한 뒤에 한 의무병이 남아서 다친 병사들 몇십명 혼자 살려내는",
          "original_form": "다들 후퇴한 뒤에 한 의무병이 남아서 다친 병사들 몇십명 혼자 살려내는",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044365.1.10",
          "form": "ㅠㅜ멋지네요",
          "original_form": "ㅠㅜ멋지네요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044365.1.11",
          "form": "요새는 티비에서도 영화 많이해주니까요",
          "original_form": "요새는 티비에서도 영화 많이해주니까요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044365.1.12",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044365.1.13",
          "form": "액션도좋아하세요?",
          "original_form": "액션도좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044365.1.14",
          "form": "너무 좋아해요ㅋㅋㅋ",
          "original_form": "너무 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044365.1.15",
          "form": "로맨스보다 훨씬 좋아하지요",
          "original_form": "로맨스보다 훨씬 좋아하지요",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044365.1.16",
          "form": "최근에 분노의 질주 시리즈 다 해줘서 쭉봤어요",
          "original_form": "최근에 분노의 질주 시리즈 다 해줘서 쭉봤어요",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044365.1.17",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044365.1.18",
          "form": "많이보시네요",
          "original_form": "많이보시네요",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044365.1.19",
          "form": "ㅋㅋㅋ코로나에 재택이 겹치니",
          "original_form": "ㅋㅋㅋ코로나에 재택이 겹치니",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044365.1.20",
          "form": "집순이력이 크게 증가했어요",
          "original_form": "집순이력이 크게 증가했어요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044365.1.21",
          "form": "ㅎㅅㅎㅎ맞아요",
