      "utterance": [
        {
          "id": "MDRW2100045744.1.1",
          "form": "배달 음식도 자주 시켜드세요?",
          "original_form": "배달 음식도 자주 시켜드세요?",
          "speaker_id": "1",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045744.1.2",
          "form": "저는 점심때 다같이 시킬때는 많이 시켜도",
          "original_form": "저는 점심때 다같이 시킬때는 많이 시켜도",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045744.1.3",
          "form": "집에서 혼자의 의지로는 잘 안시키는 것 같아요ㅋㅋㅋㅋ",
          "original_form": "집에서 혼자의 의지로는 잘 안시키는 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045744.1.4",
          "form": "그쵸ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045744.1.5",
          "form": "냉동실에 비축해둔 냉동음식을 데워먹는 파에요!",
          "original_form": "냉동실에 비축해둔 냉동음식을 데워먹는 파에요!",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045744.1.6",
          "form": "맨날 바깥음식만 먹으니까",
          "original_form": "맨날 바깥음식만 먹으니까",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045744.1.7",
          "form": "굳이 배달까지 시키긴 싫은ㅜㅜ",
          "original_form": "굳이 배달까지 시키긴 싫은ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045744.1.8",
          "form": "그런데 왜 다들 자주 시키는지는 알 것 같아요 배달료 별로 안비싸고 선택지도 넓더라구욬ㅋㅋㅋ",
          "original_form": "그런데 왜 다들 자주 시키는지는 알 것 같아요 배달료 별로 안비싸고 선택지도 넓더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045744.1.9",
          "form": "오 저도요!",
          "original_form": "오 저도요!",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045744.1.10",
          "form": "가끔 싸게 나오면 왕창 쟁여요",
          "original_form": "가끔 싸게 나오면 왕창 쟁여요",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045744.1.11",
          "form": "아 요즘 배달료 내렸더라고요",
          "original_form": "아 요즘 배달료 내렸더라고요",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045744.1.12",
          "form": "그리고 직접 조리 안해도 되니까 편하구요!ㅋㅋㅋ",
          "original_form": "그리고 직접 조리 안해도 되니까 편하구요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045744.1.13",
          "form": "진짜 저렴해서 놀랐어욬ㅋㅋㅋㅋ",
          "original_form": "진짜 저렴해서 놀랐어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045744.1.14",
          "form": "최소 배달금액도 이렇게 내린줄 몰랐는데",
          "original_form": "최소 배달금액도 이렇게 내린줄 몰랐는데",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045744.1.15",
          "form": "1인 메뉴도 가볍게 시킬만 하겠더라구요",
          "original_form": "1인 메뉴도 가볍게 시킬만 하겠더라구요",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045744.1.16",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045744.1.17",
          "form": "1인메뉴도 엄청 다양하더라고요",
          "original_form": "1인메뉴도 엄청 다양하더라고요",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045744.1.18",
          "form": "맞아요 그리고 여름에 너무 더워서 못나가니까",
          "original_form": "맞아요 그리고 여름에 너무 더워서 못나가니까",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045744.1.19",
          "form": "다들 그 전까지는 나가서 먹다가",
          "original_form": "다들 그 전까지는 나가서 먹다가",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045744.1.20",
          "form": "배달에 맛들여버렸어요ㅋㅋㅋㅋ",
          "original_form": "배달에 맛들여버렸어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045744.1.21",
          "form": "그죠그죠ㅋㅋㅋ",
          "original_form": "그죠그죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045744.1.22",
          "form": "요즘 여름 너무 매서워요",
          "original_form": "요즘 여름 너무 매서워요",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045744.1.23",
          "form": "햇살도 짱짱하구ㅠㅠ",
          "original_form": "햇살도 짱짱하구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045744.1.24",
          "form": "햇볕이 그렇게 내리쬘지 몰랐어요",
          "original_form": "햇볕이 그렇게 내리쬘지 몰랐어요",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045744.1.25",
          "form": "사람을 찐만두로 만드는 날씨....",
          "original_form": "사람을 찐만두로 만드는 날씨....",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045744.1.26",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045744.1.27",
          "form": "2018년부터 진짜...",
          "original_form": "2018년부터 진짜...",
          "speaker_id": "1",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045744.1.28",
          "form": "저 그때 에어컨 샀잖아요ㅋㅋ",
          "original_form": "저 그때 에어컨 샀잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045744.1.29",
          "form": "2018년은 진짴ㅋㅋㅋㅋㅋ",
          "original_form": "2018년은 진짴ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045744.1.30",
          "form": "제대로 만두가 되었죠",
          "original_form": "제대로 만두가 되었죠",
          "speaker_id": "2",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045744.1.31",
          "form": "잘 익은 만두가",
          "original_form": "잘 익은 만두가",
          "speaker_id": "2",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045744.1.32",
          "form": "찐만두하니까",
          "original_form": "찐만두하니까",
          "speaker_id": "1",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045744.1.33",
          "form": "또 만두먹고싶네요ㅋㅋㅋ",
          "original_form": "또 만두먹고싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:41"
        }
      ]
