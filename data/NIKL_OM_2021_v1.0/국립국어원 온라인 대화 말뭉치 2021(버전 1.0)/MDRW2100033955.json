        },
        {
          "id": "MDRW2100033955.1.3",
          "form": "밀키트 먹어 봤어?",
          "original_form": "밀키트 먹어 봤어?",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100033955.1.4",
          "form": "밀푀유 전골 먹어봤어",
          "original_form": "밀푀유 전골 먹어봤어",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100033955.1.5",
          "form": "그거 유명한가봐",
          "original_form": "그거 유명한가봐",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100033955.1.6",
          "form": "꽤 괜찮았어",
          "original_form": "꽤 괜찮았어",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100033955.1.7",
          "form": "채소도 골고루 들어있다며",
          "original_form": "채소도 골고루 들어있다며",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100033955.1.8",
          "form": "응 배추랑 청경채랑 깻잎?",
          "original_form": "응 배추랑 청경채랑 깻잎?",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100033955.1.9",
          "form": "오 고기는 어땠어?",
          "original_form": "오 고기는 어땠어?",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100033955.1.10",
          "form": "좀 적긴 했는데 그래도 나쁘지 않아",
          "original_form": "좀 적긴 했는데 그래도 나쁘지 않아",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100033955.1.11",
          "form": "고기만 추가해서 먹으면 든든하게 먹을 듯?",
          "original_form": "고기만 추가해서 먹으면 든든하게 먹을 듯?",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100033955.1.12",
          "form": "다음에 마트 가면 한번 사봐",
          "original_form": "다음에 마트 가면 한번 사봐",
          "speaker_id": "1",
