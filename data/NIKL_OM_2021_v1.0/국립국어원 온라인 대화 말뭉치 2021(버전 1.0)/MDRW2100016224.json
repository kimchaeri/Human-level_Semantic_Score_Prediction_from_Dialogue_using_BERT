      "utterance": [
        {
          "id": "MDRW2100016224.1.1",
          "form": "작은 아들 공기시합 끝났나?",
          "original_form": "작은 아들 공기시합 끝났나?",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016224.1.2",
          "form": "ㅇㅇ 끝났는데 역시나 0살로 꼴찌야 ㅋㅋ",
          "original_form": "ㅇㅇ 끝났는데 역시나 0살로 꼴찌야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016224.1.3",
          "form": "이런이런.. 안울었어?",
          "original_form": "이런이런.. 안울었어?",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016224.1.4",
          "form": "ㅎㅎ 낄낄거리며 나오던데",
          "original_form": "ㅎㅎ 낄낄거리며 나오던데",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016224.1.5",
          "form": "역시 긍정적이구만 ㅋㅋ",
          "original_form": "역시 긍정적이구만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016224.1.6",
          "form": "0살인 친구들 많았대",
          "original_form": "0살인 친구들 많았대",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100016224.1.7",
          "form": "그럴 것 같더라.. 우리 아들만 못하는 건 아니었겠지",
          "original_form": "그럴 것 같더라.. 우리 아들만 못하는 건 아니었겠지",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016224.1.8",
          "form": "100살 넘어간 친구도 있던데",
          "original_form": "100살 넘어간 친구도 있던데",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016224.1.9",
          "form": "편차가 크네 ㅋㅋ",
          "original_form": "편차가 크네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016224.1.10",
          "form": "그래도 하나도 못했다고 하니 괜히 엄마만 속상하네",
          "original_form": "그래도 하나도 못했다고 하니 괜히 엄마만 속상하네",
          "speaker_id": "1",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016224.1.11",
          "form": "다 잘할 순 없으니까 ㅎㅎ",
          "original_form": "다 잘할 순 없으니까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016224.1.12",
          "form": "아이 기르다보니 내가 잘하는게 많았구나 새삼 느낌 ㅋㅋ",
          "original_form": "아이 기르다보니 내가 잘하는게 많았구나 새삼 느낌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016224.1.13",
          "form": "ㅎㅎ 우리때는 활동량이 많았으니",
          "original_form": "ㅎㅎ 우리때는 활동량이 많았으니",
          "speaker_id": "2",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016224.1.14",
          "form": "공기도 훌라우프도 줄넘기도 아무도 안알려줘도 다 잘했는데...",
          "original_form": "공기도 훌라우프도 줄넘기도 아무도 안알려줘도 다 잘했는데...",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016224.1.15",
          "form": "요즘은 학원에서만 하잖아 ㅎㅎ",
          "original_form": "요즘은 학원에서만 하잖아 ㅎㅎ",
