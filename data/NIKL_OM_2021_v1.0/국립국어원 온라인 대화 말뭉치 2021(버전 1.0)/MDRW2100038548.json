      "utterance": [
        {
          "id": "MDRW2100038548.1.1",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.2",
          "form": "저도 손톱 꼭 칠하고",
          "original_form": "저도 손톱 꼭 칠하고",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.3",
          "form": "어느순간 화장못하면 불안하고 이런게 이상하게느껴질때가 생기더라구여",
          "original_form": "어느순간 화장못하면 불안하고 이런게 이상하게느껴질때가 생기더라구여",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.4",
          "form": "뺨에다가 블러셔 올리고...",
          "original_form": "뺨에다가 블러셔 올리고...",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.5",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.6",
          "form": "맞아요 맨얼굴 보여지는 게",
          "original_form": "맞아요 맨얼굴 보여지는 게",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.7",
          "form": "민망한 일처럼...",
          "original_form": "민망한 일처럼...",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.8",
          "form": "그래서 아 이게 진짜 내가 원해서한건가?",
          "original_form": "그래서 아 이게 진짜 내가 원해서한건가?",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.9",
          "form": "근데 남자들은 그렇게 안살잖아요",
          "original_form": "근데 남자들은 그렇게 안살잖아요",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.10",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.11",
          "form": "맞아여ㅜ",
          "original_form": "맞아여ㅜ",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.12",
          "form": "name1님 혹시 탈코르셋 운동 아시나요...?",
          "original_form": "&name1&님 혹시 탈코르셋 운동 아시나요...?",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.13",
          "form": "자각하게되니까 눈에보이네여",
          "original_form": "자각하게되니까 눈에보이네여",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.14",
          "form": "예..",
          "original_form": "예..",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.15",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038548.1.16",
          "form": "오 대박!",
          "original_form": "오 대박!",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.17",
          "form": "사실 저는 탈코인입니다 ㅋㅋㅋㅋㅋ",
          "original_form": "사실 저는 탈코인입니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.18",
          "form": "응원합니다 저는 아직 탈코까진못갔어요",
          "original_form": "응원합니다 저는 아직 탈코까진못갔어요",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.19",
          "form": "그럴 수 있죠!!!",
          "original_form": "그럴 수 있죠!!!",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.20",
          "form": "머리도 숏컷이고 화장도 안하긴하는데",
          "original_form": "머리도 숏컷이고 화장도 안하긴하는데",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.21",
          "form": "근데 자각하는 것 자체가 중요하다고 생각해요!",
          "original_form": "근데 자각하는 것 자체가 중요하다고 생각해요!",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.22",
          "form": "네네 그럼 거의",
          "original_form": "네네 그럼 거의",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.23",
          "form": "다 하신거 아닌가요?!",
          "original_form": "다 하신거 아닌가요?!",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.24",
          "form": "뭔가 운동한다고하는만큼은 아닌것같아여",
          "original_form": "뭔가 운동한다고하는만큼은 아닌것같아여",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.25",
          "form": "에이 그거 자체가",
          "original_form": "에이 그거 자체가",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.26",
          "form": "name1님 존재 자체가",
          "original_form": "&name1&님 존재 자체가",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100038548.1.27",
          "form": "운동인거예요 ㅎㅎ",
          "original_form": "운동인거예요 ㅎㅎ",
