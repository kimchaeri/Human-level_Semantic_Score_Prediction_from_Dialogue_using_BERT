      "utterance": [
        {
          "id": "MDRW2100020985.1.1",
          "form": "신라면투움바 유행하더라구요",
          "original_form": "신라면투움바 유행하더라구요",
          "speaker_id": "1",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020985.1.2",
          "form": "오 안먹어봤어요",
          "original_form": "오 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020985.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020985.1.4",
          "form": "들어만봤엉6",
          "original_form": "들어만봤엉6",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020985.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020985.1.6",
          "form": "봤어요 ㅋㅋㅋ",
          "original_form": "봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020985.1.7",
          "form": "궁금하긴해요",
          "original_form": "궁금하긴해요",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020985.1.8",
          "form": "소스는 산걸로 하는거예요?",
          "original_form": "소스는 산걸로 하는거예요?",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020985.1.9",
          "form": "음 아뇨!",
          "original_form": "음 아뇨!",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.10",
          "form": "신라면 수프 넣는거같아요",
          "original_form": "신라면 수프 넣는거같아요",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.11",
          "form": "거기다가 우유랑 치즈요!",
          "original_form": "거기다가 우유랑 치즈요!",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.12",
          "form": "오 소스도 만들면",
          "original_form": "오 소스도 만들면",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.13",
          "form": "힘들거같은데",
          "original_form": "힘들거같은데",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.14",
          "form": "제가 똥손이라 ㅎ",
          "original_form": "제가 똥손이라 ㅎ",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.15",
          "form": "생각보다",
          "original_form": "생각보다",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.16",
          "form": "간단해보이더라구요",
          "original_form": "간단해보이더라구요",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.17",
          "form": "저두여 ㅋㅋㅋㅋ",
          "original_form": "저두여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.18",
          "form": "오 찾아봐야겠네요",
          "original_form": "오 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.19",
          "form": "ㅋㅋㅋㅋ 신기하더라구요",
          "original_form": "ㅋㅋㅋㅋ 신기하더라구요",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.20",
          "form": "신라면 볶음면은 드셔보셨어요?",
          "original_form": "신라면 볶음면은 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020985.1.21",
          "form": "신라면을 파스타로 만들다니",
          "original_form": "신라면을 파스타로 만들다니",
