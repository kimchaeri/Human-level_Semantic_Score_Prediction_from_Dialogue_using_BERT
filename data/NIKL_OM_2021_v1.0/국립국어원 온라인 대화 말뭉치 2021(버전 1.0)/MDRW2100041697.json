      "utterance": [
        {
          "id": "MDRW2100041697.1.1",
          "form": "언니 매번 맘 조리면서 어떻게 만나요ㅋㅋㅋㅋ",
          "original_form": "언니 매번 맘 조리면서 어떻게 만나요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041697.1.2",
          "form": "아놔 내가 걱정되고ㅋㅋㅋ",
          "original_form": "아놔 내가 걱정되고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041697.1.3",
          "form": "그러게 말이다",
          "original_form": "그러게 말이다",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041697.1.4",
          "form": "매주 만나지는 않았으면 좋겠는데",
          "original_form": "매주 만나지는 않았으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041697.1.5",
          "form": "일단 이 주에 한 번씩 캠핑을 가니까",
          "original_form": "일단 이 주에 한 번씩 캠핑을 가니까",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041697.1.6",
          "form": "격주로 놀기는 하거든",
          "original_form": "격주로 놀기는 하거든",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041697.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041697.1.8",
          "form": "걱정이 된다 정말ㅋㅋㅋㅋ",
          "original_form": "걱정이 된다 정말ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041697.1.9",
          "form": "어느 지점에서 핀트가 나갈지 몰라서ㅋㅋㅋ",
          "original_form": "어느 지점에서 핀트가 나갈지 몰라서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041697.1.10",
          "form": "그러니",
          "original_form": "그러니",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041697.1.11",
          "form": "그래서 어제도 주말 둘 다 만나는 건 안 된다고",
          "original_form": "그래서 어제도 주말 둘 다 만나는 건 안 된다고",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041697.1.12",
          "form": "일주일에 한 번만 만나서 놀아라",
          "original_form": "일주일에 한 번만 만나서 놀아라",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041697.1.13",
          "form": "라고 얘기해 놨다",
          "original_form": "라고 얘기해 놨다",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041697.1.14",
          "form": "거리 두기ㅋㅋㅋㅋ",
          "original_form": "거리 두기ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.15",
          "form": "어제는 또 놀다가 비 오니까",
          "original_form": "어제는 또 놀다가 비 오니까",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.16",
          "form": "name3이 집에 가서 놀았는 거야",
          "original_form": "&name3&이 집에 가서 놀았는 거야",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.17",
          "form": "마음에 안정 거리 유지ㅋㅋㅋㅋ",
          "original_form": "마음에 안정 거리 유지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.18",
          "form": "아 근데 누구 집에 가서 노는 건 싫거든",
          "original_form": "아 근데 누구 집에 가서 노는 건 싫거든",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.19",
          "form": "그 엄마 아빠가 계속 지켜보고 있을 거 아니야",
          "original_form": "그 엄마 아빠가 계속 지켜보고 있을 거 아니야",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.20",
          "form": "거리 두기가 정말 필요하다",
          "original_form": "거리 두기가 정말 필요하다",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.21",
          "form": "맞아요 언니",
          "original_form": "맞아요 언니",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.22",
          "form": "집은 정말 친한 거 아니고는",
          "original_form": "집은 정말 친한 거 아니고는",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.23",
          "form": "내 안 가는 게 서로에게 좋음",
          "original_form": "내 안 가는 게 서로에게 좋음",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041697.1.24",
          "form": "그렇지 나도 집에 가는 거는 별로",
          "original_form": "그렇지 나도 집에 가는 거는 별로",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041697.1.25",
          "form": "그래서 계속 전화하면서 인사 잘하고 싸우지 말라고",
          "original_form": "그래서 계속 전화하면서 인사 잘하고 싸우지 말라고",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041697.1.26",
          "form": "얘기는 했는데",
          "original_form": "얘기는 했는데",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041697.1.27",
          "form": "찝찝함ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "찝찝함ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041697.1.28",
          "form": "맞아요 뭐 알 수가 없으니 우리가~",
          "original_form": "맞아요 뭐 알 수가 없으니 우리가~",
          "speaker_id": "2",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041697.1.29",
          "form": "만날 때마다 자꾸 싸우지 마라 할 수도 없는 거고",
          "original_form": "만날 때마다 자꾸 싸우지 마라 할 수도 없는 거고",
          "speaker_id": "2",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100041697.1.30",
          "form": "그러니",
          "original_form": "그러니",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041697.1.31",
