      "utterance": [
        {
          "id": "MDRW2100011968.1.1",
          "form": "영화나 드라마 좋아하시나요!",
          "original_form": "영화나 드라마 좋아하시나요!",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100011968.1.2",
          "form": "ㅋㅋ드라마는 가끔 봐요",
          "original_form": "ㅋㅋ드라마는 가끔 봐요",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100011968.1.3",
          "form": "보시는 프로그램 있어요?",
          "original_form": "보시는 프로그램 있어요?",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100011968.1.4",
          "form": "요즘엔 보는 게 별로 없네요 ㅠㅠㅠ",
          "original_form": "요즘엔 보는 게 별로 없네요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100011968.1.5",
          "form": "가끔 유튜브 돌아다니는 클립만 봐요",
          "original_form": "가끔 유튜브 돌아다니는 클립만 봐요",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100011968.1.6",
          "form": "예전에 지정생존자 재미있게 봤어요! 스토브리그랑!",
          "original_form": "예전에 지정생존자 재미있게 봤어요! 스토브리그랑!",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100011968.1.7",
          "form": "저두 스토브리그 재밌게 봤어요",
          "original_form": "저두 스토브리그 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210824 21:48"
        },
        {
          "id": "MDRW2100011968.1.8",
          "form": "요샌 티빙 오리지널 드라마 보고있어요",
          "original_form": "요샌 티빙 오리지널 드라마 보고있어요",
          "speaker_id": "1",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100011968.1.9",
          "form": "재미있는 드라마 있나요? 저는 티빙 볼 땐 주로 예능을 많이 봤던 것 같아요",
          "original_form": "재미있는 드라마 있나요? 저는 티빙 볼 땐 주로 예능을 많이 봤던 것 같아요",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100011968.1.10",
          "form": "저두 예능을 많이 보긴 해요.",
          "original_form": "저두 예능을 많이 보긴 해요.",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100011968.1.11",
          "form": "드라마는 요새 마녀식당이란 거 보고있어요",
          "original_form": "드라마는 요새 마녀식당이란 거 보고있어요",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100011968.1.12",
          "form": "송지효 나오는 드라마 맞죠!",
          "original_form": "송지효 나오는 드라마 맞죠!",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100011968.1.13",
          "form": "재미있나요?",
          "original_form": "재미있나요?",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100011968.1.14",
          "form": "맞아요! 재미있어요.",
          "original_form": "맞아요! 재미있어요.",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100011968.1.15",
          "form": "설정도 독특하고 송지효님도 예쁘구요ㅋㅋ",
          "original_form": "설정도 독특하고 송지효님도 예쁘구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100011968.1.16",
          "form": "다음에 날잡아서 몰아봐야겠네요!",
          "original_form": "다음에 날잡아서 몰아봐야겠네요!",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100011968.1.17",
          "form": "살짝 판타지스러운가요?",
          "original_form": "살짝 판타지스러운가요?",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100011968.1.18",
          "form": "네 판타지에요.",
          "original_form": "네 판타지에요.",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100011968.1.19",
          "form": "먹방도 좀 있구요ㅋㅋ",
          "original_form": "먹방도 좀 있구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100011968.1.20",
          "form": "재밌을 거 같아요! 다음에 볼 드라마는 마녀식당으로 하겠습니다",
          "original_form": "재밌을 거 같아요! 다음에 볼 드라마는 마녀식당으로 하겠습니다",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100011968.1.21",
          "form": "혹시 식스센스 보셨나요?",
          "original_form": "혹시 식스센스 보셨나요?",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100011968.1.22",
          "form": "시즌 1 재밌게 봤는데 이번에 시즌2도 하고 있더라고요!",
          "original_form": "시즌 1 재밌게 봤는데 이번에 시즌2도 하고 있더라고요!",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100011968.1.23",
          "form": "네 보고있어요 멤버 케미가 좋잖아요",
          "original_form": "네 보고있어요 멤버 케미가 좋잖아요",
          "speaker_id": "1",
          "time": "20210824 22:14"
        }
      ]
    }
