      "utterance": [
        {
          "id": "MDRW2100043779.1.1",
          "form": "요즘 운동해요?ㅎ",
          "original_form": "요즘 운동해요?ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043779.1.2",
          "form": "팩 자주하세요?",
          "original_form": "팩 자주하세요?",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043779.1.3",
          "form": "집에 많은데 귀찮아서 ㅋ",
          "original_form": "집에 많은데 귀찮아서 ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043779.1.4",
          "form": "아주가끔요",
          "original_form": "아주가끔요",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043779.1.5",
          "form": "저도 귀찮아서 안하게 되는데",
          "original_form": "저도 귀찮아서 안하게 되는데",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043779.1.6",
          "form": "1일1팩 하는 사람들은 진짜 효과가 있는건가요?",
          "original_form": "1일1팩 하는 사람들은 진짜 효과가 있는건가요?",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043779.1.7",
          "form": "나이는 먹는데 트러블이 안없어져요ㅎ",
          "original_form": "나이는 먹는데 트러블이 안없어져요ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043779.1.8",
          "form": "ㅎㅎㅎ 점점 신경을 덜쓰게 되요",
          "original_form": "ㅎㅎㅎ 점점 신경을 덜쓰게 되요",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.9",
          "form": "그냥 수분공급해줘서 도움은 될듯요",
          "original_form": "그냥 수분공급해줘서 도움은 될듯요",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.10",
          "form": "나이들수록",
          "original_form": "나이들수록",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.11",
          "form": "안하는것보단ㅎ",
          "original_form": "안하는것보단ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.12",
          "form": "수분크림 듬뿍으로",
          "original_form": "수분크림 듬뿍으로",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.13",
          "form": "바르긴하는데 ㅎㅎㅎ",
          "original_form": "바르긴하는데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.14",
          "form": "너무오래붙이는것도 안좋은가봐요",
          "original_form": "너무오래붙이는것도 안좋은가봐요",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.15",
          "form": "오늘은 생각난김에 팩하나 해야겟네요 ㅎ",
          "original_form": "오늘은 생각난김에 팩하나 해야겟네요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.16",
          "form": "전 마를때까지 붙이는데",
          "original_form": "전 마를때까지 붙이는데",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.17",
          "form": "그렇다고 하더라구요",
          "original_form": "그렇다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043779.1.18",
          "form": "저는 붙이고 잔적도 있어요 ㅎ",
          "original_form": "저는 붙이고 잔적도 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:22"
        }
      ]
