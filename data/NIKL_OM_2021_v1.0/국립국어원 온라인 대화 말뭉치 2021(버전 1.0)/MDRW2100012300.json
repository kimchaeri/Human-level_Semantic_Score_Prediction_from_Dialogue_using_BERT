      "utterance": [
        {
          "id": "MDRW2100012300.1.1",
          "form": "너 몇학점 들어?",
          "original_form": "너 몇학점 들어?",
          "speaker_id": "1",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012300.1.2",
          "form": "나 17학점!",
          "original_form": "나 17학점!",
          "speaker_id": "2",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012300.1.3",
          "form": "2학점짜리 듣는거야?",
          "original_form": "2학점짜리 듣는거야?",
          "speaker_id": "1",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012300.1.4",
          "form": "2학점이랑 3학점 번갈아가면서 들어!",
          "original_form": "2학점이랑 3학점 번갈아가면서 들어!",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012300.1.5",
          "form": "그렇구나 나는 3학점만 들어",
          "original_form": "그렇구나 나는 3학점만 들어",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012300.1.6",
          "form": "넌 몇 학점 듣는데?",
          "original_form": "넌 몇 학점 듣는데?",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012300.1.7",
          "form": "나는 18학점",
          "original_form": "나는 18학점",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012300.1.8",
          "form": "3학점짜리 6개",
          "original_form": "3학점짜리 6개",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012300.1.9",
          "form": "이번에 기대되는 강좌있어?",
          "original_form": "이번에 기대되는 강좌있어?",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012300.1.10",
          "form": "나는 게임 메이킹",
          "original_form": "나는 게임 메이킹",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012300.1.11",
          "form": "우와 재밌겠다",
          "original_form": "우와 재밌겠다",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012300.1.12",
          "form": "맞아 기대된다",
          "original_form": "맞아 기대된다",
          "speaker_id": "1",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012300.1.13",
          "form": "너는 기대되는 강좌있어?",
          "original_form": "너는 기대되는 강좌있어?",
          "speaker_id": "1",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012300.1.14",
          "form": "나는 디자인 표현기법!",
          "original_form": "나는 디자인 표현기법!",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012300.1.15",
          "form": "오 재밌겠다. 너네 등록금은 얼마야?",
          "original_form": "오 재밌겠다. 너네 등록금은 얼마야?",
          "speaker_id": "1",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100012300.1.16",
          "form": "한 417만원 정도 나왔어ㅠ",
          "original_form": "한 417만원 정도 나왔어ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100012300.1.17",
          "form": "혹시 미대야?",
          "original_form": "혹시 미대야?",
          "speaker_id": "1",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100012300.1.18",
          "form": "앗 맞아!",
          "original_form": "앗 맞아!",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100012300.1.19",
          "form": "너는 공대?",
          "original_form": "너는 공대?",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100012300.1.20",
          "form": "아니야 나는 인문계야",
          "original_form": "아니야 나는 인문계야",
          "speaker_id": "1",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100012300.1.21",
          "form": "등록금은 냈어?",
          "original_form": "등록금은 냈어?",
          "speaker_id": "1",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100012300.1.22",
          "form": "게임 메이킹 이라며!!!",
          "original_form": "게임 메이킹 이라며!!!",
          "speaker_id": "2",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012300.1.23",
          "form": "콘텐츠 학과라그래",
          "original_form": "콘텐츠 학과라그래",
          "speaker_id": "1",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012300.1.24",
          "form": "아 그렇구나ㅎㅎㅎ",
          "original_form": "아 그렇구나ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012300.1.25",
          "form": "등록금냈어?",
          "original_form": "등록금냈어?",
          "speaker_id": "1",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012300.1.26",
          "form": "웅 오늘 냈어!",
          "original_form": "웅 오늘 냈어!",
          "speaker_id": "2",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012300.1.27",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012300.1.28",
          "form": "오 나도 오늘 냈어!",
          "original_form": "오 나도 오늘 냈어!",
          "speaker_id": "1",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012300.1.29",
          "form": "넌 얼마 나왔어?",
          "original_form": "넌 얼마 나왔어?",
          "speaker_id": "2",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012300.1.30",
          "form": "너네 과 과제 많아?",
          "original_form": "너네 과 과제 많아?",
          "speaker_id": "1",
          "time": "20210826 00:05"
        }
      ]
    }
