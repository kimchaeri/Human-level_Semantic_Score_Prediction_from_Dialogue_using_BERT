      "utterance": [
        {
          "id": "MDRW2100016618.1.1",
          "form": "네, 요즘 많이 좋아진(?) 것 같아요 ㅋㅋ",
          "original_form": "네, 요즘 많이 좋아진(?) 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016618.1.2",
          "form": "얼마 전만 해도 인스턴트와 직접 만드는 차이가 너무 커서 못 먹겠었거든요.",
          "original_form": "얼마 전만 해도 인스턴트와 직접 만드는 차이가 너무 커서 못 먹겠었거든요.",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016618.1.3",
          "form": "샘표 꺼도 어머니께서 괜찮다고 하셔서 시도해보고 놀랐어요 ㅎㅎ",
          "original_form": "샘표 꺼도 어머니께서 괜찮다고 하셔서 시도해보고 놀랐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016618.1.4",
          "form": "오우 좋네요ㅋㅋㅋㅋ",
          "original_form": "오우 좋네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016618.1.5",
          "form": "저도 밀푀유나베먹어봤어요",
          "original_form": "저도 밀푀유나베먹어봤어요",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016618.1.6",
          "form": "마이쉐프꺼 밀키트",
          "original_form": "마이쉐프꺼 밀키트",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016618.1.7",
          "form": "아 저 밀푀유나베 좋아하는데 밀키트가 몇인분이 되나요?",
          "original_form": "아 저 밀푀유나베 좋아하는데 밀키트가 몇인분이 되나요?",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100016618.1.8",
          "form": "2인분이요ㅋㅋㅋㅋ",
          "original_form": "2인분이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100016618.1.9",
          "form": "근데 좀 부족해요",
          "original_form": "근데 좀 부족해요",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100016618.1.10",
          "form": "오 그럼 저 혼자 먹기 딱 좋네요!",
          "original_form": "오 그럼 저 혼자 먹기 딱 좋네요!",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100016618.1.11",
          "form": "감사합니다 호호",
          "original_form": "감사합니다 호호",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100016618.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100016618.1.13",
          "form": "아주맛있어요진짜로",
          "original_form": "아주맛있어요진짜로",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100016618.1.14",
          "form": "밀키트중에 손에꼽는답니다",
          "original_form": "밀키트중에 손에꼽는답니다",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100016618.1.15",
          "form": "우오아앙 적어놓겠어요",
          "original_form": "우오아앙 적어놓겠어요",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100016618.1.16",
          "form": "가을을 맞이하며 먹기 딱 좋은 음식이네요",
          "original_form": "가을을 맞이하며 먹기 딱 좋은 음식이네요",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100016618.1.17",
          "form": "ㅋㅋㅋ 꿀팁이 하나있어요!",
          "original_form": "ㅋㅋㅋ 꿀팁이 하나있어요!",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100016618.1.18",
          "form": "무엇이죠",
          "original_form": "무엇이죠",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100016618.1.19",
          "form": "칼국수사리는",
          "original_form": "칼국수사리는",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100016618.1.20",
          "form": "따로 더구매해두세요",
          "original_form": "따로 더구매해두세요",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100016618.1.21",
          "form": "당신이 면킬러라면말이죠!!!!!",
          "original_form": "당신이 면킬러라면말이죠!!!!!",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100016618.1.22",
          "form": "오오 저는 우동사리를 쟁여두고 있는 사람이므로 몹시 반가운 꿀팁입니다 ㅋㅋ",
          "original_form": "오오 저는 우동사리를 쟁여두고 있는 사람이므로 몹시 반가운 꿀팁입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100016618.1.23",
          "form": "생면으로 언제나 구비하기 때문에",
          "original_form": "생면으로 언제나 구비하기 때문에",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100016618.1.24",
          "form": "기억하겠어요 ! ㅋㅋ",
          "original_form": "기억하겠어요 ! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100016618.1.25",
          "form": "네네 국물이 맛있는데 칼국수사리가",
          "original_form": "네네 국물이 맛있는데 칼국수사리가",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100016618.1.26",
          "form": "작아서 아쉽거든요ㅋㅋㅋㅋ",
          "original_form": "작아서 아쉽거든요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100016618.1.27",
          "form": "저도먹고싶네요 갑자기",
          "original_form": "저도먹고싶네요 갑자기",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100016618.1.28",
          "form": "혹시 그거 이마트에 파나요?",
          "original_form": "혹시 그거 이마트에 파나요?",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100016618.1.29",
          "form": "으음...",
          "original_form": "으음...",
          "speaker_id": "2",
          "time": "20210826 19:21"
        }
      ]
    }
