      "utterance": [
        {
          "id": "MDRW2100031009.1.1",
          "form": "다람쥐 좋아하세요?",
          "original_form": "다람쥐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100031009.1.2",
          "form": "네, 다람쥐도 좋아하고 하늘 다람쥐도 좋아해요.",
          "original_form": "네, 다람쥐도 좋아하고 하늘 다람쥐도 좋아해요.",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100031009.1.3",
          "form": "하늘 다람쥐는 뭐죠",
          "original_form": "하늘 다람쥐는 뭐죠",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031009.1.4",
          "form": "검색해볼게요",
          "original_form": "검색해볼게요",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031009.1.5",
          "form": "날 수 있는 다람쥐인데 귀여워요.",
          "original_form": "날 수 있는 다람쥐인데 귀여워요.",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031009.1.6",
          "form": "어머 하늘다람쥐",
          "original_form": "어머 하늘다람쥐",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031009.1.7",
          "form": "얘구나",
          "original_form": "얘구나",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031009.1.8",
          "form": "귀여워요",
          "original_form": "귀여워요",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031009.1.9",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031009.1.10",
          "form": "그쵸. 날아다니는 모습도 귀여워요. name1님은 어떤 동물을 좋아하시나요?",
          "original_form": "그쵸. 날아다니는 모습도 귀여워요. &name1&님은 어떤 동물을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100031009.1.11",
          "form": "저는 고양이요!",
          "original_form": "저는 고양이요!",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031009.1.12",
          "form": "덩치크고 통통한 냥이들이요",
          "original_form": "덩치크고 통통한 냥이들이요",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031009.1.13",
          "form": "뱃살 두툼하고!",
          "original_form": "뱃살 두툼하고!",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031009.1.14",
          "form": "와 고양이도 귀엽죠! 뚱냥이들! 정말 귀여워요!",
          "original_form": "와 고양이도 귀엽죠! 뚱냥이들! 정말 귀여워요!",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031009.1.15",
          "form": "맞아요 건강만하면",
          "original_form": "맞아요 건강만하면",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031009.1.16",
          "form": "너무귀여운 친구들이에요!!",
          "original_form": "너무귀여운 친구들이에요!!",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100031009.1.17",
          "form": "그럼 name1님은 반려동물이 있으신가요?",
          "original_form": "그럼 &name1&님은 반려동물이 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100031009.1.18",
          "form": "아니요 없어요!!!",
          "original_form": "아니요 없어요!!!",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031009.1.19",
          "form": "랜선집사랍니다",
          "original_form": "랜선집사랍니다",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031009.1.20",
          "form": "키울 상황이 아니어서요ㅠ",
          "original_form": "키울 상황이 아니어서요ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031009.1.21",
          "form": "아 저도요! 아직까지는 반려동물을 책임지지 못 할 것 같더라구요.",
          "original_form": "아 저도요! 아직까지는 반려동물을 책임지지 못 할 것 같더라구요.",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031009.1.22",
          "form": "맞아요 제가 책임질수있을때가 오면",
          "original_form": "맞아요 제가 책임질수있을때가 오면",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031009.1.23",
          "form": "꼭 키울거에요!!",
          "original_form": "꼭 키울거에요!!",
          "speaker_id": "1",
          "time": "20210904 17:51"
        }
      ]
