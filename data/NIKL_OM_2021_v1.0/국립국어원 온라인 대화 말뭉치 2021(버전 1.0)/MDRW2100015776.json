      "utterance": [
        {
          "id": "MDRW2100015776.1.1",
          "form": "좀 파슬파슬한 버터쿠키좋아하면 생미쉘 팔레트? 그거도 맛있어",
          "original_form": "좀 파슬파슬한 버터쿠키좋아하면 생미쉘 팔레트? 그거도 맛있어",
          "speaker_id": "1",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015776.1.2",
          "form": "단단이 좋앙",
          "original_form": "단단이 좋앙",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015776.1.3",
          "form": "파스파슬 가루싫음",
          "original_form": "파스파슬 가루싫음",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015776.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015776.1.5",
          "form": "단단은 워커스야 단연코",
          "original_form": "단단은 워커스야 단연코",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015776.1.6",
          "form": "ㅋㅋㅋㅋ나 이번에 치약바꿧는데.",
          "original_form": "ㅋㅋㅋㅋ나 이번에 치약바꿧는데.",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
