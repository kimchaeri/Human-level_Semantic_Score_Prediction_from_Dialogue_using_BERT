      "utterance": [
        {
          "id": "MDRW2100020679.1.1",
          "form": "그리고 치킨을 넣겠죠",
          "original_form": "그리고 치킨을 넣겠죠",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020679.1.2",
          "form": "감자들어가야 맛있더라구요",
          "original_form": "감자들어가야 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020679.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020679.1.4",
          "form": "하 어릴 적에 치킨 패티 아니라",
          "original_form": "하 어릴 적에 치킨 패티 아니라",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020679.1.5",
          "form": "탄수화물 탄수화물조합이라 맛이 없을 수가 없는",
          "original_form": "탄수화물 탄수화물조합이라 맛이 없을 수가 없는",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.6",
          "form": "치킨 텐더를 넣어주는 토스트집이 있었는데",
          "original_form": "치킨 텐더를 넣어주는 토스트집이 있었는데",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.7",
          "form": "망했어요",
          "original_form": "망했어요",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.8",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.9",
          "form": "돌아와요",
          "original_form": "돌아와요",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.10",
          "form": "떡볶이도 엄청 달달하고 특이했는데..",
          "original_form": "떡볶이도 엄청 달달하고 특이했는데..",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.11",
          "form": "근데\r\n텐\r\n더\r\n넣\r\n음\r\n진짜 맛있겠다",
          "original_form": "근데\r\n텐\r\n더\r\n넣\r\n음\r\n진짜 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.12",
          "form": "name1님은 이삭에서 뭐드시나요",
          "original_form": "&name1&님은 이삭에서 뭐드시나요",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.13",
          "form": "저 그",
          "original_form": "저 그",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.14",
          "form": "네 진짜 바삭바삭",
          "original_form": "네 진짜 바삭바삭",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020679.1.15",
          "form": "이름을까먹었네",
          "original_form": "이름을까먹었네",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020679.1.16",
          "form": "그 있어요 약간 매콤한거",
          "original_form": "그 있어요 약간 매콤한거",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020679.1.17",
          "form": "ㅋㅋㅋㅋ 너무 성의없었나요",
          "original_form": "ㅋㅋㅋㅋ 너무 성의없었나요",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020679.1.18",
          "form": "하 저도 맛은 기억나는데",
          "original_form": "하 저도 맛은 기억나는데",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020679.1.19",
          "form": "제가 또 매콤한거만 먹거든요 어디가면",
          "original_form": "제가 또 매콤한거만 먹거든요 어디가면",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020679.1.20",
          "form": "우리 가면 알잖아요",
          "original_form": "우리 가면 알잖아요",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020679.1.21",
          "form": "이름은 모르는데",
          "original_form": "이름은 모르는데",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020679.1.22",
          "form": "그쵸 저도 감자 들어간거라고만",
          "original_form": "그쵸 저도 감자 들어간거라고만",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100020679.1.23",
          "form": "맛을 기억하잖아요",
          "original_form": "맛을 기억하잖아요",
