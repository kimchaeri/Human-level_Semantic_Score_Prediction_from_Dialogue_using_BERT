        },
        {
          "id": "MDRW2100006054.1.2",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100006054.1.3",
          "form": "양박쌍용",
          "original_form": "양박쌍용",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100006054.1.4",
          "form": "기억난다..",
          "original_form": "기억난다..",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100006054.1.5",
          "form": "1박은 누구야",
          "original_form": "1박은 누구야",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100006054.1.6",
          "form": "그때가 젤재밌었지",
          "original_form": "그때가 젤재밌었지",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100006054.1.7",
          "form": "박주양",
          "original_form": "박주양",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006054.1.8",
          "form": "영",
          "original_form": "영",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006054.1.9",
          "form": "아 걘 뭐해",
          "original_form": "아 걘 뭐해",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006054.1.10",
          "form": "서울가지안ㄹ았나",
          "original_form": "서울가지안ㄹ았나",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006054.1.11",
          "form": "살아있네",
          "original_form": "살아있네",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006054.1.12",
          "form": "ㅇㅇ 현역이야",
          "original_form": "ㅇㅇ 현역이야",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006054.1.13",
          "form": "아직도..?",
          "original_form": "아직도..?",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006054.1.14",
          "form": "박지성빼고는 다 현역이야",
          "original_form": "박지성빼고는 다 현역이야",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006054.1.15",
          "form": "안늙었나",
          "original_form": "안늙었나",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006054.1.16",
          "form": "이천수는 아니라며",
          "original_form": "이천수는 아니라며",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006054.1.17",
          "form": "이천수는 나이대가 안맞아",
          "original_form": "이천수는 나이대가 안맞아",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006054.1.18",
          "form": "박지성이 2002 막내",
          "original_form": "박지성이 2002 막내",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006054.1.19",
          "form": "그래? 지성이랑 비슷한줄",
          "original_form": "그래? 지성이랑 비슷한줄",
          "speaker_id": "1",
