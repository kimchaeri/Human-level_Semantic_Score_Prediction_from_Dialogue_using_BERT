      "utterance": [
        {
          "id": "MDRW2100004083.1.1",
          "form": "반혀동물키우시나요?",
          "original_form": "반혀동물키우시나요?",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004083.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004083.1.3",
          "form": "초등학교때",
          "original_form": "초등학교때",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004083.1.4",
          "form": "저도 아니요",
          "original_form": "저도 아니요",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004083.1.5",
          "form": "사슴벌레, 장수풍뎅이 애벌레랑",
          "original_form": "사슴벌레, 장수풍뎅이 애벌레랑",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004083.1.6",
          "form": "아 질병이나 알러지이야기도 가능하네요",
          "original_form": "아 질병이나 알러지이야기도 가능하네요",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004083.1.7",
          "form": "전 털 알러지 있음",
          "original_form": "전 털 알러지 있음",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004083.1.8",
          "form": "계곡에서 잡아온 가재랑",
          "original_form": "계곡에서 잡아온 가재랑",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004083.1.9",
          "form": "그래서 물고기 키웠어요",
          "original_form": "그래서 물고기 키웠어요",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004083.1.10",
          "form": "키워본 적 있어요",
          "original_form": "키워본 적 있어요",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004083.1.11",
          "form": "아 저도",
          "original_form": "아 저도",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004083.1.12",
          "form": "약간 물에서 사는 친구들이랑 친한듯",
          "original_form": "약간 물에서 사는 친구들이랑 친한듯",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.14",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.15",
          "form": "구피 번식률 미쳤더라구요",
          "original_form": "구피 번식률 미쳤더라구요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.16",
          "form": "자고일어나면 물고기가 두배가 됨",
          "original_form": "자고일어나면 물고기가 두배가 됨",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.17",
          "form": "애벌레들은 장수풍뎅이랑 사슴벌레 되기 전에",
          "original_form": "애벌레들은 장수풍뎅이랑 사슴벌레 되기 전에",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.18",
          "form": "죽어서 젤리 먹이지도 못하고",
          "original_form": "죽어서 젤리 먹이지도 못하고",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.19",
          "form": "죽었나요",
          "original_form": "죽었나요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.21",
          "form": "물고기랑 가재랑은",
          "original_form": "물고기랑 가재랑은",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004083.1.22",
          "form": "같은 수조에 넣어놓긴 했는데",
          "original_form": "같은 수조에 넣어놓긴 했는데",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004083.1.23",
          "form": "초딩",
          "original_form": "초딩",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004083.1.24",
          "form": "어뭐야 나쁜말이라면서",
          "original_form": "어뭐야 나쁜말이라면서",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004083.1.25",
          "form": "분명 물의 수위를 놓여놨음에도",
          "original_form": "분명 물의 수위를 놓여놨음에도",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004083.1.26",
          "form": "물고기가 낮게 수영해서",
          "original_form": "물고기가 낮게 수영해서",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004083.1.27",
          "form": "물의 수위 중요하지",
          "original_form": "물의 수위 중요하지",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004083.1.28",
          "form": "가재한테 배 뜯어먹혀 무지개 다리를 건넜고",
          "original_form": "가재한테 배 뜯어먹혀 무지개 다리를 건넜고",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004083.1.29",
          "form": "그 가재는 배가 터져서 무지개 다리를 건넜죠...",
          "original_form": "그 가재는 배가 터져서 무지개 다리를 건넜죠...",
          "speaker_id": "2",
          "time": "20210821 14:55"
        }
      ]
    }
