        },
        {
          "id": "MDRW2100040458.1.2",
          "form": "근데 비계가 더 맛나긴 함 ㅋ",
          "original_form": "근데 비계가 더 맛나긴 함 ㅋ",
          "speaker_id": "1",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040458.1.3",
          "form": "살이 찌니까 문제지",
          "original_form": "살이 찌니까 문제지",
          "speaker_id": "2",
          "time": "20210904 20:43"
        },
        {
          "id": "MDRW2100040458.1.4",
          "form": "살 별루 안찐다 고기만 먹으면 ㅋㅋ",
          "original_form": "살 별루 안찐다 고기만 먹으면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040458.1.5",
          "form": "지방인데 살이 왜 안 쪄",
          "original_form": "지방인데 살이 왜 안 쪄",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040458.1.6",
          "form": "지방 만 먹으면 살 원래 빠진다 ㅎ",
          "original_form": "지방 만 먹으면 살 원래 빠진다 ㅎ",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040458.1.7",
          "form": "그러면 고기만 먹어야겠네",
          "original_form": "그러면 고기만 먹어야겠네",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040458.1.8",
          "form": "웅웅 탄수화물 없이 ㅎㅎ",
          "original_form": "웅웅 탄수화물 없이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040458.1.9",
          "form": "그러면 고기만 많이 구워 봐라",
          "original_form": "그러면 고기만 많이 구워 봐라",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040458.1.10",
          "form": "옹옹 많이 있나여",
          "original_form": "옹옹 많이 있나여",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040458.1.11",
          "form": "많이 사 놨다",
          "original_form": "많이 사 놨다",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100040458.1.12",
          "form": "좋구만 ㅋ",
