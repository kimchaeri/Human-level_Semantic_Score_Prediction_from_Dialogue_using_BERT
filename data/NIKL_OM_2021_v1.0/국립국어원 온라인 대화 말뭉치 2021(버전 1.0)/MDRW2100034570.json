      "utterance": [
        {
          "id": "MDRW2100034570.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034570.1.2",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034570.1.3",
          "form": "점심 삼아 간식을 엄청 먹었어요",
          "original_form": "점심 삼아 간식을 엄청 먹었어요",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034570.1.4",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034570.1.5",
          "form": "저는 오리고기 먹었어요!",
          "original_form": "저는 오리고기 먹었어요!",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034570.1.6",
          "form": "간식 좋아하시나봐요 어떤거 드셨어요?",
          "original_form": "간식 좋아하시나봐요 어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100034570.1.7",
          "form": "버터와플 먹었어요!",
          "original_form": "버터와플 먹었어요!",
          "speaker_id": "2",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100034570.1.8",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100034570.1.9",
          "form": "오 저도 버터와플 짱 좋아해요ㅋㅋ",
          "original_form": "오 저도 버터와플 짱 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100034570.1.10",
          "form": "저는 빵 좋아해요!",
          "original_form": "저는 빵 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100034570.1.11",
          "form": "빵 뭐 좋아하세요?",
          "original_form": "빵 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034570.1.12",
          "form": "거의 다 좋아해요 단맛 짠맛 매콤한거 ..",
          "original_form": "거의 다 좋아해요 단맛 짠맛 매콤한거 ..",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034570.1.13",
          "form": "어떤 빵 자주 드시나요?",
          "original_form": "어떤 빵 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034570.1.14",
          "form": "마요네즈빵인데, 미토콘드리아랑 똑같이 생긴 빵. 이름을 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "마요네즈빵인데, 미토콘드리아랑 똑같이 생긴 빵. 이름을 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100034570.1.15",
          "form": "모닝빵에 꼬부랑 모양 있는건데...",
          "original_form": "모닝빵에 꼬부랑 모양 있는건데...",
          "speaker_id": "2",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100034570.1.16",
          "form": "그게 뭘까요...? 안에 야채도 들었나요?",
          "original_form": "그게 뭘까요...? 안에 야채도 들었나요?",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100034570.1.17",
          "form": "아니요! 딱 모닝빵처럼 생겼고, 안에 아무것도 없는데",
          "original_form": "아니요! 딱 모닝빵처럼 생겼고, 안에 아무것도 없는데",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100034570.1.18",
          "form": "일반 모닝빵보다 촉촉하고 진짜 맛있거든요ㅠ",
          "original_form": "일반 모닝빵보다 촉촉하고 진짜 맛있거든요ㅠ",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100034570.1.19",
          "form": "아.. 저 가면 맨날 사장님이 봉투에 그것부터 담아주셔서 이름표 볼 생각을 안했네요..",
          "original_form": "아.. 저 가면 맨날 사장님이 봉투에 그것부터 담아주셔서 이름표 볼 생각을 안했네요..",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100034570.1.20",
          "form": "이렇게 추천할 기회가 생길지도 모르고..",
          "original_form": "이렇게 추천할 기회가 생길지도 모르고..",
          "speaker_id": "2",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100034570.1.21",
          "form": "뭘까요 되게 궁금해요...ㅋㅋ 개인빵집인가요?",
          "original_form": "뭘까요 되게 궁금해요...ㅋㅋ 개인빵집인가요?",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100034570.1.22",
          "form": "네 동네빵집이에요!\r\n근데 다른 빵집 다녀도 이런 빵 봤거든요ㅠ\r\n특별한 거 아니고 기본빵인데ㅠㅠ",
          "original_form": "네 동네빵집이에요!\r\n근데 다른 빵집 다녀도 이런 빵 봤거든요ㅠ\r\n특별한 거 아니고 기본빵인데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100034570.1.23",
          "form": "궁금하네요 돌려있는빵은 알아도 마요네즈는 잘 모르겠어요 이름 알게되면 꼭 알려주세용 ㅎㅍ",
          "original_form": "궁금하네요 돌려있는빵은 알아도 마요네즈는 잘 모르겠어요 이름 알게되면 꼭 알려주세용 ㅎㅍ",
          "speaker_id": "1",
          "time": "20210828 16:00"
        }
      ]
    }
