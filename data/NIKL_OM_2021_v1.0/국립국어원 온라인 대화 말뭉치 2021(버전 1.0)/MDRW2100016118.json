        {
          "id": "MDRW2100016118.1.2",
          "form": "군대 가신분 있나요",
          "original_form": "군대 가신분 있나요",
          "speaker_id": "1",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100016118.1.3",
          "form": "신랑이랑 남동생들이 군대다녀왔어요.",
          "original_form": "신랑이랑 남동생들이 군대다녀왔어요.",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016118.1.4",
          "form": "군대 다녀오셨어요?",
          "original_form": "군대 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
