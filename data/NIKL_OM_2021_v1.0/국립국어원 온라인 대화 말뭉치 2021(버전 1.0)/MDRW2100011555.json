      "utterance": [
        {
          "id": "MDRW2100011555.1.1",
          "form": "너네학교어학연수있음?",
          "original_form": "너네학교어학연수있음?",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011555.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011555.1.3",
          "form": "교환학생",
          "original_form": "교환학생",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011555.1.4",
          "form": "착각함",
          "original_form": "착각함",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011555.1.5",
          "form": "아미 어학연수 찾고 있었는데",
          "original_form": "아미 어학연수 찾고 있었는데",
          "speaker_id": "2",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100011555.1.6",
          "form": "잠시만 다시 찾고 옴",
          "original_form": "잠시만 다시 찾고 옴",
          "speaker_id": "2",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100011555.1.7",
          "form": "알겠음",
          "original_form": "알겠음",
          "speaker_id": "1",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100011555.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:54"
        },
        {
          "id": "MDRW2100011555.1.9",
          "form": "중국 일본 어학연수 미국 편입 있대",
          "original_form": "중국 일본 어학연수 미국 편입 있대",
          "speaker_id": "2",
          "time": "20210824 13:55"
        },
        {
          "id": "MDRW2100011555.1.10",
          "form": "오 미국편입하면",
          "original_form": "오 미국편입하면",
          "speaker_id": "1",
          "time": "20210824 13:55"
        },
        {
          "id": "MDRW2100011555.1.11",
          "form": "4년제로가는건가?",
          "original_form": "4년제로가는건가?",
          "speaker_id": "1",
          "time": "20210824 13:55"
        },
        {
          "id": "MDRW2100011555.1.12",
          "form": "아니 근데 예술 학과도 되는 지는 모르겠음",
          "original_form": "아니 근데 예술 학과도 되는 지는 모르겠음",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011555.1.13",
          "form": "우리 학교에 일반 학과도 있어서",
          "original_form": "우리 학교에 일반 학과도 있어서",
          "speaker_id": "2",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100011555.1.14",
          "form": "너네학과사이트에서",
          "original_form": "너네학과사이트에서",
          "speaker_id": "1",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100011555.1.15",
          "form": "찾아보면",
          "original_form": "찾아보면",
          "speaker_id": "1",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100011555.1.16",
          "form": "될걸",
          "original_form": "될걸",
          "speaker_id": "1",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100011555.1.17",
          "form": "공지사항",
          "original_form": "공지사항",
          "speaker_id": "1",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100011555.1.18",
          "form": "게시판에검색해보ㅓ",
          "original_form": "게시판에검색해보ㅓ",
          "speaker_id": "1",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100011555.1.19",
          "form": "오 괜찮대 그냥 해도",
          "original_form": "오 괜찮대 그냥 해도",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011555.1.20",
          "form": "그러면편입으로가는거임?",
          "original_form": "그러면편입으로가는거임?",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.21",
          "form": "아니 근데",
          "original_form": "아니 근데",
          "speaker_id": "2",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.22",
          "form": "호텔",
          "original_form": "호텔",
          "speaker_id": "2",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.23",
          "form": "아니면그냥1년교환정도",
          "original_form": "아니면그냥1년교환정도",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.24",
          "form": "요리 쪽",
          "original_form": "요리 쪽",
          "speaker_id": "2",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.25",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.26",
          "form": "호텔요리??",
          "original_form": "호텔요리??",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.27",
          "form": "전문인 갓 같아",
          "original_form": "전문인 갓 같아",
          "speaker_id": "2",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.28",
          "form": "응응 그걸로 유명하대",
          "original_form": "응응 그걸로 유명하대",
          "speaker_id": "2",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.29",
          "form": "전혀안맞는거아님?",
          "original_form": "전혀안맞는거아님?",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.30",
          "form": "아 ㅡㄱ걸로유명",
          "original_form": "아 ㅡㄱ걸로유명",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.31",
          "form": "그냥 따로 찾는게 나을 듯",
          "original_form": "그냥 따로 찾는게 나을 듯",
          "speaker_id": "2",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011555.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ교환도괜찮은데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ교환도괜찮은데",
          "speaker_id": "1",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100011555.1.33",
          "form": "교환이나어학연수도은근",
          "original_form": "교환이나어학연수도은근",
          "speaker_id": "1",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100011555.1.34",
          "form": "돈많이들더라",
          "original_form": "돈많이들더라",
          "speaker_id": "1",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100011555.1.35",
          "form": "천은들던데",
          "original_form": "천은들던데",
