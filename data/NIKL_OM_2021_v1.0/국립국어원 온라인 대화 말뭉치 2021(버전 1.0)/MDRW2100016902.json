      "utterance": [
        {
          "id": "MDRW2100016902.1.1",
          "form": "좋아하는 작가 있으세요?",
          "original_form": "좋아하는 작가 있으세요?",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016902.1.2",
          "form": "음.. 좋아하는 만화가는 많고요",
          "original_form": "음.. 좋아하는 만화가는 많고요",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016902.1.3",
          "form": "소설가는 지금은 딱히 없어요.",
          "original_form": "소설가는 지금은 딱히 없어요.",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016902.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016902.1.5",
          "form": "저도 닥치는대로 읽어요ㅎ",
          "original_form": "저도 닥치는대로 읽어요ㅎ",
          "speaker_id": "1",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016902.1.6",
          "form": "어떤 장르를 많이 보세요?",
          "original_form": "어떤 장르를 많이 보세요?",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016902.1.7",
          "form": "전 소설이나 육아 많이 봐요",
          "original_form": "전 소설이나 육아 많이 봐요",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016902.1.8",
          "form": "아하 전 좀 잡학쪽을 봐요 최근 본건 세계미식여행이었어요",
          "original_form": "아하 전 좀 잡학쪽을 봐요 최근 본건 세계미식여행이었어요",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016902.1.9",
          "form": "아하 재밌을것같아요",
          "original_form": "아하 재밌을것같아요",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016902.1.10",
          "form": "네 ㅋㅋㅋ 책으로 여행간 기분도 느끼고..좋았어요",
          "original_form": "네 ㅋㅋㅋ 책으로 여행간 기분도 느끼고..좋았어요",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016902.1.11",
          "form": "아하 그렇군여",
          "original_form": "아하 그렇군여",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016902.1.12",
          "form": "좋아하는 소설가는 누군가요?",
          "original_form": "좋아하는 소설가는 누군가요?",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016902.1.13",
          "form": "전 예전에 김진명 좋아했어요",
          "original_form": "전 예전에 김진명 좋아했어요",
          "speaker_id": "1",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100016902.1.14",
          "form": "무궁화 꽃이 작가요?",
          "original_form": "무궁화 꽃이 작가요?",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
