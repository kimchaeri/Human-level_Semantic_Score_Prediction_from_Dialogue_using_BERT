      "utterance": [
        {
          "id": "MDRW2100002644.1.1",
          "form": "가을옷은 사셨어요?",
          "original_form": "가을옷은 사셨어요?",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002644.1.2",
          "form": "이제 슬슬 사야 할 것 같아요 날이 선선해져서",
          "original_form": "이제 슬슬 사야 할 것 같아요 날이 선선해져서",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002644.1.3",
          "form": "근데 가을옷은 너무 짧게 입어서 아쉬워요",
          "original_form": "근데 가을옷은 너무 짧게 입어서 아쉬워요",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002644.1.4",
          "form": "그쵸 이번여름은 생각보다 짧은거같아요",
          "original_form": "그쵸 이번여름은 생각보다 짧은거같아요",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002644.1.5",
          "form": "마자요 ㅠㅠ",
          "original_form": "마자요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002644.1.6",
          "form": "맞아요 덥긴 더웠는데 생각보다 금방 지나간 거 같아요",
          "original_form": "맞아요 덥긴 더웠는데 생각보다 금방 지나간 거 같아요",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002644.1.7",
          "form": "어떤 계절을 좋아하세요",
          "original_form": "어떤 계절을 좋아하세요",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002644.1.8",
          "form": "저는 봄겨울이요 !",
          "original_form": "저는 봄겨울이요 !",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002644.1.9",
          "form": "쌀쌀한 날씨 좋아해요 ~",
          "original_form": "쌀쌀한 날씨 좋아해요 ~",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002644.1.10",
          "form": "시원하고 선선해서 좋죠",
          "original_form": "시원하고 선선해서 좋죠",
          "speaker_id": "2",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002644.1.11",
          "form": "특히 겨울을 벌레가 없어서 더 좋아요",
          "original_form": "특히 겨울을 벌레가 없어서 더 좋아요",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002644.1.12",
          "form": "여름은 벌레가 너무 많아서..",
          "original_form": "여름은 벌레가 너무 많아서..",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002644.1.13",
          "form": "여름은 너무 끈적끈적해서 ㅠ",
          "original_form": "여름은 너무 끈적끈적해서 ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002644.1.14",
          "form": "집밖으로 나가기도 싫어용",
          "original_form": "집밖으로 나가기도 싫어용",
          "speaker_id": "1",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002644.1.15",
          "form": "그것도 맞아요",
          "original_form": "그것도 맞아요",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002644.1.16",
          "form": "습도가 높아서 불쾌지수도 상승",
          "original_form": "습도가 높아서 불쾌지수도 상승",
          "speaker_id": "2",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002644.1.17",
          "form": "무슨계절 좋아하세요?",
          "original_form": "무슨계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002644.1.18",
          "form": "저는 가을직전 늦여름 지금같은 날씨가 좋아요.\r\n겨울도 좋구요",
          "original_form": "저는 가을직전 늦여름 지금같은 날씨가 좋아요.\r\n겨울도 좋구요",
          "speaker_id": "2",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002644.1.19",
          "form": "여름좋아하는사람은 좀 드문거같아용",
          "original_form": "여름좋아하는사람은 좀 드문거같아용",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002644.1.20",
          "form": "그건 그래요.. 여름은 더워서 그런가..",
          "original_form": "그건 그래요.. 여름은 더워서 그런가..",
          "speaker_id": "2",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002644.1.21",
          "form": "과일많이 먹을 수 있는 건 좋은데 더위가 너무 큰 단점이라..",
          "original_form": "과일많이 먹을 수 있는 건 좋은데 더위가 너무 큰 단점이라..",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002644.1.22",
          "form": "아무래도.. 그렇죠..?",
          "original_form": "아무래도.. 그렇죠..?",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002644.1.23",
          "form": "과일은 진짜맞아요 ㅠㅋㅋㅋㅋ 수분가득",
          "original_form": "과일은 진짜맞아요 ㅠㅋㅋㅋㅋ 수분가득",
          "speaker_id": "1",
          "time": "20210820 14:48"
        }
      ]
