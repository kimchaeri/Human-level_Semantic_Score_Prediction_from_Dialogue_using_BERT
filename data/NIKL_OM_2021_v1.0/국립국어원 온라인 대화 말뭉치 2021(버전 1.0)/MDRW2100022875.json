      "utterance": [
        {
          "id": "MDRW2100022875.1.1",
          "form": "하 진짜",
          "original_form": "하 진짜",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022875.1.2",
          "form": "슈붕은",
          "original_form": "슈붕은",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022875.1.3",
          "form": "수치에요",
          "original_form": "수치에요",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022875.1.4",
          "form": "어디서 혼종이",
          "original_form": "어디서 혼종이",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022875.1.5",
          "form": "원조에게 깝치죠",
          "original_form": "원조에게 깝치죠",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022875.1.6",
          "form": "팥붕최고",
          "original_form": "팥붕최고",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022875.1.7",
          "form": "붕어빵의 계절이 오는군요",
          "original_form": "붕어빵의 계절이 오는군요",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022875.1.8",
          "form": "저기요",
          "original_form": "저기요",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022875.1.9",
          "form": "나대지 마시지요",
          "original_form": "나대지 마시지요",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022875.1.10",
          "form": "저희초면인데요",
          "original_form": "저희초면인데요",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022875.1.11",
          "form": "마라탕 먹고 싶어요",
          "original_form": "마라탕 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022875.1.12",
          "form": "ㅋㅋㅋㅋ저희 자주 봣는데요",
          "original_form": "ㅋㅋㅋㅋ저희 자주 봣는데요",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022875.1.13",
          "form": "전 떡볶뀌",
          "original_form": "전 떡볶뀌",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022875.1.14",
          "form": "떡볶뀌는 무슨 혼종이죠",
          "original_form": "떡볶뀌는 무슨 혼종이죠",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022875.1.15",
          "form": "밀떡파세요 쌀떡파세요?",
          "original_form": "밀떡파세요 쌀떡파세요?",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022875.1.16",
          "form": "밀쌀떡이요",
          "original_form": "밀쌀떡이요",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022875.1.17",
          "form": "전 안 가리고 다 잘먹어요",
          "original_form": "전 안 가리고 다 잘먹어요",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022875.1.18",
          "form": "거짓말하지마세요",
          "original_form": "거짓말하지마세요",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.19",
          "form": "밀떡파들은",
          "original_form": "밀떡파들은",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.20",
          "form": "쌀떡 상종안해요",
          "original_form": "쌀떡 상종안해요",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.21",
          "form": "당신은 고로 쌀떡파지",
          "original_form": "당신은 고로 쌀떡파지",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.22",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.23",
          "form": "당심 편견이 가득하시네요",
          "original_form": "당심 편견이 가득하시네요",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.24",
          "form": "맞습니다 전 원래 쌀떡파였는데요",
          "original_form": "맞습니다 전 원래 쌀떡파였는데요",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.25",
          "form": "걍 다 먹어요",
          "original_form": "걍 다 먹어요",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.26",
          "form": "착하니까요",
          "original_form": "착하니까요",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022875.1.27",
          "form": "그거랑 착한건 뭔 상관이죠 ?",
          "original_form": "그거랑 착한건 뭔 상관이죠 ?",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022875.1.28",
          "form": "당심처럼 편견없이",
          "original_form": "당심처럼 편견없이",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022875.1.29",
          "form": "다 잘먹으니까요",
          "original_form": "다 잘먹으니까요",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022875.1.30",
          "form": "아니 당신과 달리",
          "original_form": "아니 당신과 달리",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022875.1.31",
          "form": "저도 다 잘먹는데욥",
          "original_form": "저도 다 잘먹는데욥",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100022875.1.32",
          "form": "잘 모루겟네요",
          "original_form": "잘 모루겟네요",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022875.1.33",
          "form": "꼬북칩 먹고싶어요",
          "original_form": "꼬북칩 먹고싶어요",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022875.1.34",
          "form": "사주실래요 ?",
          "original_form": "사주실래요 ?",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022875.1.35",
          "form": "츄러스맛 좋아하세요?",
          "original_form": "츄러스맛 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022875.1.36",
          "form": "이슬톡톡이랑 꼬북칩 같이 먹으면 음 굿",
          "original_form": "이슬톡톡이랑 꼬북칩 같이 먹으면 음 굿",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022875.1.37",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022875.1.38",
          "form": "저 사주세요 전 좋아해요",
          "original_form": "저 사주세요 전 좋아해요",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022875.1.39",
          "form": "근데 츄러스보다 다른맛 더 좋아해요",
          "original_form": "근데 츄러스보다 다른맛 더 좋아해요",
          "speaker_id": "2",
          "time": "20210829 18:55"
        }
      ]
    }
