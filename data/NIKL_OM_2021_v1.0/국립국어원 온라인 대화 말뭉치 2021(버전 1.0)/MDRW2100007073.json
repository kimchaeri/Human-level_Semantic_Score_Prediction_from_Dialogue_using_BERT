      "utterance": [
        {
          "id": "MDRW2100007073.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007073.1.2",
          "form": "네 여행 너무좋죠",
          "original_form": "네 여행 너무좋죠",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007073.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007073.1.4",
          "form": "저도 좋아하는데 요즘 못가니 답답해요",
          "original_form": "저도 좋아하는데 요즘 못가니 답답해요",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007073.1.5",
          "form": "해외도 가고싶고 답답",
          "original_form": "해외도 가고싶고 답답",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007073.1.6",
          "form": "그나마 제주도 가는것같아여",
          "original_form": "그나마 제주도 가는것같아여",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007073.1.7",
          "form": "그니까요 제주도라도 갈랬는데 4단계격상ㅠㅠ",
          "original_form": "그니까요 제주도라도 갈랬는데 4단계격상ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007073.1.8",
          "form": "어디 다녀오셨어요?",
          "original_form": "어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007073.1.9",
          "form": "올초에 제주도 지난달에 삼척 다녀오고..별거없어요",
          "original_form": "올초에 제주도 지난달에 삼척 다녀오고..별거없어요",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007073.1.10",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007073.1.11",
          "form": "2년전 다낭갔다온게 마지막이네요",
          "original_form": "2년전 다낭갔다온게 마지막이네요",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007073.1.12",
          "form": "다낭  말레이시아였나요?ㅅ",
          "original_form": "다낭  말레이시아였나요?ㅅ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007073.1.13",
          "form": "헷갈려라",
          "original_form": "헷갈려라",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007073.1.14",
          "form": "갑자기 저도 헷갈리네요",
          "original_form": "갑자기 저도 헷갈리네요",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007073.1.15",
          "form": "코타키나발루 있는 나라 아닌가여  전 말레이시아 거기 다녀온듯",
          "original_form": "코타키나발루 있는 나라 아닌가여  전 말레이시아 거기 다녀온듯",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007073.1.16",
          "form": "베트남 다낭",
          "original_form": "베트남 다낭",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007073.1.17",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007073.1.18",
          "form": "코타가 말레이시아요ㅎ",
          "original_form": "코타가 말레이시아요ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007073.1.19",
          "form": "베트남은 하노이밖에",
          "original_form": "베트남은 하노이밖에",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007073.1.20",
          "form": "전 마지막으로 필핀 다녀옴",
          "original_form": "전 마지막으로 필핀 다녀옴",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007073.1.21",
          "form": "그리워라",
          "original_form": "그리워라",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007073.1.22",
          "form": "다낭도 많이 가시던데",
          "original_form": "다낭도 많이 가시던데",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007073.1.23",
          "form": "급하게 패키지로 갔다와서",
          "original_form": "급하게 패키지로 갔다와서",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007073.1.24",
          "form": "자유로 다시가고싶더라구요",
          "original_form": "자유로 다시가고싶더라구요",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007073.1.25",
          "form": "이런 베트남 치안 갠챠나서 자유도 좋아여",
          "original_form": "이런 베트남 치안 갠챠나서 자유도 좋아여",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007073.1.26",
          "form": "음식도 맛나고  반미 먹고싶네여",
          "original_form": "음식도 맛나고  반미 먹고싶네여",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007073.1.27",
          "form": "베트남도 음식천국이죠",
          "original_form": "베트남도 음식천국이죠",
          "speaker_id": "1",
          "time": "20210822 00:36"
        }
      ]
    }
