      "utterance": [
        {
          "id": "MDRW2100018485.1.1",
          "form": "안녕하세요 좋은 아침입니다~!",
          "original_form": "안녕하세요 좋은 아침입니다~!",
          "speaker_id": "1",
          "time": "20210827 11:12"
        },
        {
          "id": "MDRW2100018485.1.2",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "2",
          "time": "20210827 11:12"
        },
        {
          "id": "MDRW2100018485.1.3",
          "form": "그러게요 오늘 날씨가 좋네요",
          "original_form": "그러게요 오늘 날씨가 좋네요",
          "speaker_id": "2",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018485.1.4",
          "form": "식사는 하셨어요?",
          "original_form": "식사는 하셨어요?",
          "speaker_id": "2",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018485.1.5",
          "form": "네! 저는 방금 먹었어요",
          "original_form": "네! 저는 방금 먹었어요",
          "speaker_id": "1",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018485.1.6",
          "form": "아하 전 아직이네요ㅠㅠ",
          "original_form": "아하 전 아직이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018485.1.7",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018485.1.8",
          "form": "저 떡볶이랑 순대요 ㅎㅎ",
          "original_form": "저 떡볶이랑 순대요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018485.1.9",
          "form": "오랜만에 먹으니 넘 맛있네요",
          "original_form": "오랜만에 먹으니 넘 맛있네요",
          "speaker_id": "1",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018485.1.10",
          "form": "헐~~ 맛있었겠다",
          "original_form": "헐~~ 맛있었겠다",
          "speaker_id": "2",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018485.1.11",
          "form": "저도 오늘은 분식이 끌리네요",
          "original_form": "저도 오늘은 분식이 끌리네요",
          "speaker_id": "2",
          "time": "20210827 11:14"
        },
        {
          "id": "MDRW2100018485.1.12",
          "form": "아직 식사 안하셨으면 오늘 분식 드세영ㅋㅋ",
          "original_form": "아직 식사 안하셨으면 오늘 분식 드세영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018485.1.13",
          "form": "그럴까요 ㅎㅎ",
          "original_form": "그럴까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018485.1.14",
          "form": "저는 오늘 쫄면이 땡기네요",
          "original_form": "저는 오늘 쫄면이 땡기네요",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018485.1.15",
          "form": "쫄면에 만두~",
          "original_form": "쫄면에 만두~",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018485.1.16",
          "form": "크 진짜 맛있죠 쫄면... 저도 쫄면 완전 좋아해요",
          "original_form": "크 진짜 맛있죠 쫄면... 저도 쫄면 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018485.1.17",
          "form": "만두도 좋죠 전 김밥도 추가요 ㅋㅋㅋㅋ",
          "original_form": "만두도 좋죠 전 김밥도 추가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018485.1.18",
          "form": "뭘 아시네요 ㅎㅎ",
          "original_form": "뭘 아시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018485.1.19",
          "form": "떡볶이랑 순대는 만들어 드신거에요?",
          "original_form": "떡볶이랑 순대는 만들어 드신거에요?",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018485.1.20",
          "form": "순대는 시켰고 떡볶이는 만들었어요",
          "original_form": "순대는 시켰고 떡볶이는 만들었어요",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018485.1.21",
          "form": "요즘엔 반조리식품으로 잘 나와가지고",
          "original_form": "요즘엔 반조리식품으로 잘 나와가지고",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018485.1.22",
          "form": "그냥 끓이기만 했어요 헤헤",
          "original_form": "그냥 끓이기만 했어요 헤헤",
          "speaker_id": "1",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018485.1.23",
          "form": "아하 맞아요 요즘은 간편하게 잘 나오더라고요",
          "original_form": "아하 맞아요 요즘은 간편하게 잘 나오더라고요",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018485.1.24",
          "form": "그쵸 그래서 집에서 간단하게 해 먹기 좋은거 같아요",
          "original_form": "그쵸 그래서 집에서 간단하게 해 먹기 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018485.1.25",
          "form": "배달문화가 발달해서 더 편해졌어요",
          "original_form": "배달문화가 발달해서 더 편해졌어요",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018485.1.26",
          "form": "배달료는 좀 아깝지만ㅋㅋ",
          "original_form": "배달료는 좀 아깝지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018485.1.27",
          "form": "그렇긴 해요 ㅠㅠ 배달음식이 편하긴 한데",
          "original_form": "그렇긴 해요 ㅠㅠ 배달음식이 편하긴 한데",
          "speaker_id": "1",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018485.1.28",
          "form": "배달료가 신경쓰이더라구요",
          "original_form": "배달료가 신경쓰이더라구요",
          "speaker_id": "1",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018485.1.29",
          "form": "그런 면에서 중국집이 짱인거 같아요ㅋㅋㅋ",
          "original_form": "그런 면에서 중국집이 짱인거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018485.1.30",
          "form": "아 그렇네요 빠른 배송에 배달료도 없고!",
          "original_form": "아 그렇네요 빠른 배송에 배달료도 없고!",
          "speaker_id": "1",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018485.1.31",
          "form": "메뉴도 꽤 다양해서 밥 메뉴 고르기",
          "original_form": "메뉴도 꽤 다양해서 밥 메뉴 고르기",
          "speaker_id": "2",
          "time": "20210827 11:21"
        },
        {
          "id": "MDRW2100018485.1.32",
          "form": "귀찮을 때는 중국집 자주 시켜요",
          "original_form": "귀찮을 때는 중국집 자주 시켜요",
          "speaker_id": "2",
          "time": "20210827 11:21"
        },
        {
          "id": "MDRW2100018485.1.33",
          "form": "아 갑자기 탕수육에 짜장면 먹고싶어지네요ㅠㅠㅠ",
          "original_form": "아 갑자기 탕수육에 짜장면 먹고싶어지네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 11:21"
        }
      ]
