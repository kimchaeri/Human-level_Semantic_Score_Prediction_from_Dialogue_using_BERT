      "utterance": [
        {
          "id": "MDRW2100001015.1.1",
          "form": "평소에 운동 즐겨하시나요??",
          "original_form": "평소에 운동 즐겨하시나요??",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100001015.1.2",
          "form": "운동 하고싶은마음은 드는데 몸이안따라줘요ㅜ 그리고 넘 귀찮..",
          "original_form": "운동 하고싶은마음은 드는데 몸이안따라줘요ㅜ 그리고 넘 귀찮..",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100001015.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100001015.1.4",
          "form": "저도요... 코로나전엔 했었는데",
          "original_form": "저도요... 코로나전엔 했었는데",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100001015.1.5",
          "form": "집에서 하기는 더더욱 싫은..",
          "original_form": "집에서 하기는 더더욱 싫은..",
          "speaker_id": "2",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100001015.1.6",
          "form": "그니까여 집에선 누워있는것만...",
          "original_form": "그니까여 집에선 누워있는것만...",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100001015.1.7",
          "form": "코로나전엔 어떤운동하셨어요",
          "original_form": "코로나전엔 어떤운동하셨어요",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100001015.1.8",
          "form": "요가랑 필라테스 했었어요 ㅋㅋㅋ",
          "original_form": "요가랑 필라테스 했었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100001015.1.9",
          "form": "수영도 배우고 싶었는데...",
          "original_form": "수영도 배우고 싶었는데...",
          "speaker_id": "2",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100001015.1.10",
          "form": "오 저랑반대시네요 ㅋㅋㅋㅋ",
          "original_form": "오 저랑반대시네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100001015.1.11",
          "form": "전수영했었고 요가랑 필테배우고싶어요",
          "original_form": "전수영했었고 요가랑 필테배우고싶어요",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100001015.1.12",
          "form": "두개하면 자세교정이 좀 되나요?",
          "original_form": "두개하면 자세교정이 좀 되나요?",
          "speaker_id": "1",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100001015.1.13",
          "form": "교정에는 확실히 효과가 있긴해요",
          "original_form": "교정에는 확실히 효과가 있긴해요",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100001015.1.14",
          "form": "다만 체중에는 영향이 없더라구요.",
          "original_form": "다만 체중에는 영향이 없더라구요.",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100001015.1.15",
          "form": "약간 기대를 했었는데 ㅎㅎ",
          "original_form": "약간 기대를 했었는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100001015.1.16",
          "form": "오 저는 몸이 뒤틀린게 넘심해서 교정이필요할것같았거든요",
          "original_form": "오 저는 몸이 뒤틀린게 넘심해서 교정이필요할것같았거든요",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100001015.1.17",
          "form": "효과가있다니..",
          "original_form": "효과가있다니..",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100001015.1.18",
          "form": "체중은 확실히 식이가 답인가봐요..",
          "original_form": "체중은 확실히 식이가 답인가봐요..",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100001015.1.19",
          "form": "그리고 한번 그래도 다니다보면",
          "original_form": "그리고 한번 그래도 다니다보면",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100001015.1.20",
          "form": "그 전 자세보다 괜히 의식해서 억지로 자세를 바꾸려고 하는거같아요 ㅋㅋ",
          "original_form": "그 전 자세보다 괜히 의식해서 억지로 자세를 바꾸려고 하는거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100001015.1.21",
          "form": "점점 나이들수록 식이는 어려워요..ㅎ",
          "original_form": "점점 나이들수록 식이는 어려워요..ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100001015.1.22",
          "form": "맞아요..식이안되는만큼 운동빡세게해야되는데",
          "original_form": "맞아요..식이안되는만큼 운동빡세게해야되는데",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100001015.1.23",
          "form": "그것도 힘들구여",
          "original_form": "그것도 힘들구여",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100001015.1.24",
          "form": "뭔가 운동 열심히하면",
          "original_form": "뭔가 운동 열심히하면",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100001015.1.25",
          "form": "계속 그걸 유지해야할거같더라구요....",
          "original_form": "계속 그걸 유지해야할거같더라구요....",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100001015.1.26",
          "form": "사실 핑계 맞지만 ㅎㅎ 늘 열심히할 자신 없어서 시작을 안하는듯 ㅠㅠ",
          "original_form": "사실 핑계 맞지만 ㅎㅎ 늘 열심히할 자신 없어서 시작을 안하는듯 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100001015.1.27",
          "form": "맞아요 유지하기가 힘들어요",
          "original_form": "맞아요 유지하기가 힘들어요",
          "speaker_id": "1",
          "time": "20210820 16:53"
        }
      ]
