      "utterance": [
        {
          "id": "MDRW2100020208.1.1",
          "form": "다이어트 지금 그럼 휴식기이신가요",
          "original_form": "다이어트 지금 그럼 휴식기이신가요",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020208.1.2",
          "form": "아니면 아예 종료하신건가요",
          "original_form": "아니면 아예 종료하신건가요",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020208.1.3",
          "form": "휴식기이긴 한데",
          "original_form": "휴식기이긴 한데",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020208.1.4",
          "form": "언제 다시 시작할지 모르겟어요 ㅋㅋㅋㅋㅋ",
          "original_form": "언제 다시 시작할지 모르겟어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020208.1.5",
          "form": "꼭 중요한건 건강을 유지하면서 감량하는것",
          "original_form": "꼭 중요한건 건강을 유지하면서 감량하는것",
          "speaker_id": "1",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020208.1.6",
          "form": "저는 그렇게 생각하면서",
          "original_form": "저는 그렇게 생각하면서",
          "speaker_id": "1",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020208.1.7",
          "form": "천천히 빼고있어요",
          "original_form": "천천히 빼고있어요",
          "speaker_id": "1",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020208.1.8",
          "form": "맞아요 건강유지 정말 중요해요",
          "original_form": "맞아요 건강유지 정말 중요해요",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020208.1.9",
          "form": "전 다이어트하면서 입을수 있는 옷이 늘어난게",
          "original_form": "전 다이어트하면서 입을수 있는 옷이 늘어난게",
          "speaker_id": "1",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020208.1.10",
          "form": "정말 신기하고 좋았어요",
          "original_form": "정말 신기하고 좋았어요",
          "speaker_id": "1",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020208.1.11",
          "form": "맞아요 저두요",
          "original_form": "맞아요 저두요",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020208.1.12",
          "form": "바지들 버릴 때의",
          "original_form": "바지들 버릴 때의",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020208.1.13",
          "form": "쾌감이란",
          "original_form": "쾌감이란",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020208.1.14",
          "form": "다이어트 성공하시고 어떤 옷을 사는것이 목표세요?",
          "original_form": "다이어트 성공하시고 어떤 옷을 사는것이 목표세요?",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020208.1.15",
          "form": "전 크롭티...입니다!",
          "original_form": "전 크롭티...입니다!",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020208.1.16",
          "form": "저도 크롭티엿어요!",
          "original_form": "저도 크롭티엿어요!",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020208.1.17",
          "form": "제가 같은 체중대에 비해서도",
          "original_form": "제가 같은 체중대에 비해서도",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020208.1.18",
          "form": "똥배가 어마어마햇거든요",
          "original_form": "똥배가 어마어마햇거든요",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020208.1.19",
          "form": "다이어트 중단상태라 아직 못입긴하지만",
          "original_form": "다이어트 중단상태라 아직 못입긴하지만",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020208.1.20",
          "form": "으악 지금의 저도요",
          "original_form": "으악 지금의 저도요",
          "speaker_id": "1",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020208.1.21",
          "form": "언젠가 입고싶어요!",
          "original_form": "언젠가 입고싶어요!",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020208.1.22",
          "form": "그리고 하이웨스트 청바지도",
          "original_form": "그리고 하이웨스트 청바지도",
          "speaker_id": "1",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020208.1.23",
          "form": "멋있게 소화하고 싶어요",
          "original_form": "멋있게 소화하고 싶어요",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020208.1.24",
          "form": "저는 딱붙는옷이요",
          "original_form": "저는 딱붙는옷이요",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020208.1.25",
          "form": "건강도 잃지않으면서 마음에 드는 옷을 마음껏입도록",
          "original_form": "건강도 잃지않으면서 마음에 드는 옷을 마음껏입도록",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020208.1.26",
          "form": "저희 다이어트 성공해요!",
          "original_form": "저희 다이어트 성공해요!",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
