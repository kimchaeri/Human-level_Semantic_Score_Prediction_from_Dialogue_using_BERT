      "utterance": [
        {
          "id": "MDRW2100043106.1.1",
          "form": "운동하는거있으세요?",
          "original_form": "운동하는거있으세요?",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100043106.1.2",
          "form": "오늘부터 홈트라도 할까 생각중이에요 ㅋㅋ 너무 운동을 안해서요",
          "original_form": "오늘부터 홈트라도 할까 생각중이에요 ㅋㅋ 너무 운동을 안해서요",
          "speaker_id": "2",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100043106.1.3",
          "form": "ㅎㅎㅎ산책은어떠세요",
          "original_form": "ㅎㅎㅎ산책은어떠세요",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043106.1.4",
          "form": "밖에 나가는건 귀찮아해서요",
          "original_form": "밖에 나가는건 귀찮아해서요",
          "speaker_id": "2",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043106.1.5",
          "form": "그리고 오늘은 비가 와요",
          "original_form": "그리고 오늘은 비가 와요",
          "speaker_id": "2",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043106.1.6",
          "form": "그춍",
          "original_form": "그춍",
          "speaker_id": "1",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043106.1.7",
          "form": "건강하..시죠?",
          "original_form": "건강하..시죠?",
          "speaker_id": "1",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043106.1.8",
          "form": "ㅎㅎ 산책하는거 많이 좋아하시는구나",
          "original_form": "ㅎㅎ 산책하는거 많이 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043106.1.9",
          "form": "ㅎㅎㅎ뛰는건싫어해요",
          "original_form": "ㅎㅎㅎ뛰는건싫어해요",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043106.1.10",
          "form": "맞아요 뛰는 건 힘드니깐 그래도 천천히 거니는 건 괜찮죠",
          "original_form": "맞아요 뛰는 건 힘드니깐 그래도 천천히 거니는 건 괜찮죠",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043106.1.11",
          "form": "산책하실때 음악 들으면서 하세요?",
          "original_form": "산책하실때 음악 들으면서 하세요?",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043106.1.12",
          "form": "네 잔잔한거듣고 아님그냥걷기도해요ㅎㅎ",
          "original_form": "네 잔잔한거듣고 아님그냥걷기도해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043106.1.13",
          "form": "자주 가는 산책로가 있으신가요?",
          "original_form": "자주 가는 산책로가 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043106.1.14",
          "form": "사실 저는 산책한다고 하면 어딜가야할지 몰라서 못가는것도 있거든요",
          "original_form": "사실 저는 산책한다고 하면 어딜가야할지 몰라서 못가는것도 있거든요",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043106.1.15",
          "form": "저는동네걷는거좋아해요",
          "original_form": "저는동네걷는거좋아해요",
          "speaker_id": "1",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043106.1.16",
          "form": "운동장도는거는지겨워서ㅎㅎ",
          "original_form": "운동장도는거는지겨워서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043106.1.17",
          "form": "맞아요 저도 어릴 때 운동장 많이 돌아봤는데 그건 그냥 훈련하는 느낌이긴 하죠",
          "original_form": "맞아요 저도 어릴 때 운동장 많이 돌아봤는데 그건 그냥 훈련하는 느낌이긴 하죠",
          "speaker_id": "2",
          "time": "20210907 14:35"
        },
        {
          "id": "MDRW2100043106.1.18",
          "form": "동네가 걷기 좋게 조성되어 있으신가 봐요",
          "original_form": "동네가 걷기 좋게 조성되어 있으신가 봐요",
          "speaker_id": "2",
          "time": "20210907 14:35"
        },
        {
          "id": "MDRW2100043106.1.19",
          "form": "ㅎㅎㅎ치안은괜찮아요",
          "original_form": "ㅎㅎㅎ치안은괜찮아요",
          "speaker_id": "1",
          "time": "20210907 14:35"
        },
        {
          "id": "MDRW2100043106.1.20",
          "form": "저희 동네는 가게가 많아서 산책하긴 안좋거든요 ㅜ",
          "original_form": "저희 동네는 가게가 많아서 산책하긴 안좋거든요 ㅜ",
          "speaker_id": "2",
          "time": "20210907 14:35"
        },
        {
          "id": "MDRW2100043106.1.21",
          "form": "해볼게요 지압",
          "original_form": "해볼게요 지압",
          "speaker_id": "1",
