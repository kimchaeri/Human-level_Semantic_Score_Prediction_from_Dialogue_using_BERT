      "utterance": [
        {
          "id": "MDRW2100007997.1.1",
          "form": "좋아하는 계절이 어떻게 되세요?",
          "original_form": "좋아하는 계절이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007997.1.2",
          "form": "전 겨울이요!!!",
          "original_form": "전 겨울이요!!!",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007997.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007997.1.4",
          "form": "전 원래 여름파인데 올해 여름 진짜 너무 더워요 ㅋㅋㅋㅋ",
          "original_form": "전 원래 여름파인데 올해 여름 진짜 너무 더워요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007997.1.5",
          "form": "이번여름 너무 장난아니에요ㅠㅜ",
          "original_form": "이번여름 너무 장난아니에요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007997.1.6",
          "form": "진짜 병원에만 있어서 다행이지",
          "original_form": "진짜 병원에만 있어서 다행이지",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007997.1.7",
          "form": "출 퇴근할때마다 너무 힘들었어요ㅜ",
          "original_form": "출 퇴근할때마다 너무 힘들었어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007997.1.8",
          "form": "진짜.. 밖에 나가면 녹아내릴거 같더라고요..",
          "original_form": "진짜.. 밖에 나가면 녹아내릴거 같더라고요..",
          "speaker_id": "1",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007997.1.9",
          "form": "그래도 요며칠 비가 와서 그나마 나아요 ㅎㅎ",
          "original_form": "그래도 요며칠 비가 와서 그나마 나아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100007997.1.10",
          "form": "근데 비가 진짜 하늘 구멍난 듯이 오지않았어요???",
          "original_form": "근데 비가 진짜 하늘 구멍난 듯이 오지않았어요???",
          "speaker_id": "2",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007997.1.11",
          "form": "요즘 소나기도 너무 자주내리고",
          "original_form": "요즘 소나기도 너무 자주내리고",
          "speaker_id": "2",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007997.1.12",
          "form": "그러게요 특히 밤에...",
          "original_form": "그러게요 특히 밤에...",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007997.1.13",
          "form": "담주는 내내 비온다고 ㅠㅠ",
          "original_form": "담주는 내내 비온다고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007997.1.14",
          "form": "빨래를 미리 해야겠어요ㅎㅎㅎ",
          "original_form": "빨래를 미리 해야겠어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100007997.1.15",
          "form": "전 겨울이 좋은것중에 하나가ㅋㅋㅋ",
          "original_form": "전 겨울이 좋은것중에 하나가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100007997.1.16",
          "form": "비가 안와서ㅋㅋㅋ",
          "original_form": "비가 안와서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100007997.1.17",
          "form": "아 건조한거 좋아하시는군요 ㅋㅋㅋ",
          "original_form": "아 건조한거 좋아하시는군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100007997.1.18",
          "form": "우산 들고 다니는거 싫어해서....ㅋㅋㅋ",
          "original_form": "우산 들고 다니는거 싫어해서....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007997.1.19",
          "form": "축축하고 찝찝한것도.....ㅋㅋ",
          "original_form": "축축하고 찝찝한것도.....ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007997.1.20",
          "form": "일단 여름은 아무리 벗어도 더운데",
          "original_form": "일단 여름은 아무리 벗어도 더운데",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007997.1.21",
          "form": "겨울은 뭐라도 입으면 따뜻하니까ㅎㅎㅎ 그런거같아요ㅎㅎ",
          "original_form": "겨울은 뭐라도 입으면 따뜻하니까ㅎㅎㅎ 그런거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007997.1.22",
          "form": "ㅋㅋㅋㅋㅋ 아 그건 그래요",
          "original_form": "ㅋㅋㅋㅋㅋ 아 그건 그래요",
          "speaker_id": "1",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007997.1.23",
          "form": "겨울은 그냥 롱패딩 입고 나가면 최고",
          "original_form": "겨울은 그냥 롱패딩 입고 나가면 최고",
          "speaker_id": "1",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100007997.1.24",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100007997.1.25",
          "form": "롱패딩 최고",
          "original_form": "롱패딩 최고",
          "speaker_id": "2",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100007997.1.26",
          "form": "코디 신경안써도 되고ㅎㅎ",
          "original_form": "코디 신경안써도 되고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100007997.1.27",
          "form": "나이들수록 얇은 코트는 못 입겠어요 ㅠㅠ",
          "original_form": "나이들수록 얇은 코트는 못 입겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100007997.1.28",
          "form": "그래서 문제예요 ㅋㅋㅋ",
          "original_form": "그래서 문제예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100007997.1.29",
          "form": "코트가 이쁘긴 한데ㅋㅋㅋㅋㅋ",
          "original_form": "코트가 이쁘긴 한데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100007997.1.30",
          "form": "너무 추우면 패딩만한게 없더라구요",
          "original_form": "너무 추우면 패딩만한게 없더라구요",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007997.1.31",
          "form": "네 남이 입은거 보면 이쁜데 나는 못 입겠는 ㅋㅋㅋㅋ",
          "original_form": "네 남이 입은거 보면 이쁜데 나는 못 입겠는 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007997.1.32",
          "form": "숏패딩 혹시 있으세요?",
          "original_form": "숏패딩 혹시 있으세요?",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007997.1.33",
          "form": "작년에 유행해서 하나 사긴했죠...ㅋㅋㅋ",
          "original_form": "작년에 유행해서 하나 사긴했죠...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007997.1.34",
          "form": "근데 전 잘 안입게 되더라구요ㅎㅎㅎ",
          "original_form": "근데 전 잘 안입게 되더라구요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007997.1.35",
          "form": "그쵸 저도 유행하는거 보고 계속 사고는 싶은데 하반신이 추울까봐 ㅋㅋㅋㅋ",
          "original_form": "그쵸 저도 유행하는거 보고 계속 사고는 싶은데 하반신이 추울까봐 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007997.1.36",
          "form": "많이 추워요ㅋㅋㅋㅋ",
          "original_form": "많이 추워요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100007997.1.37",
          "form": "ㅋㅋㅋㅋㅋ 역시 전 포기해야겠네요 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 역시 전 포기해야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007997.1.38",
          "form": "추위많이 타시는 분에겐 비추천합니다 ㅎㅎㅎㅎ",
          "original_form": "추위많이 타시는 분에겐 비추천합니다 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007997.1.39",
          "form": "역시 그냥 롱패딩이 최고인걸로 ㅋㅋㅋ",
          "original_form": "역시 그냥 롱패딩이 최고인걸로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007997.1.40",
          "form": "겨울엔 롱패딩ㅋㅋㅋ",
          "original_form": "겨울엔 롱패딩ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007997.1.41",
          "form": "이제곧 롱패팅의 계절이 옵니다ㄹㅎ",
          "original_form": "이제곧 롱패팅의 계절이 옵니다ㄹㅎ",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007997.1.42",
          "form": "추석 지나기전에 한발 더 사둘까봐요 ㅋㅋㅋ 추석전까지가 역시즌이라 하더라고요",
          "original_form": "추석 지나기전에 한발 더 사둘까봐요 ㅋㅋㅋ 추석전까지가 역시즌이라 하더라고요",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100007997.1.43",
          "form": "ㅋㅋㅋ저도 검은색뿐인데",
          "original_form": "ㅋㅋㅋ저도 검은색뿐인데",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100007997.1.44",
          "form": "이번엔 밝은색으로 도전해볼까해요ㅎ",
          "original_form": "이번엔 밝은색으로 도전해볼까해요ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100007997.1.45",
          "form": "밝은 색 있음 나름 기분전환도 되고 좋은거 같아요 ㅎㅎ",
          "original_form": "밝은 색 있음 나름 기분전환도 되고 좋은거 같아요 ㅎㅎ",
