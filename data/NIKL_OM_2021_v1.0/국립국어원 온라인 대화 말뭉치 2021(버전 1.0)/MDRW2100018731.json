      "utterance": [
        {
          "id": "MDRW2100018731.1.1",
          "form": "그래 진짜 말로는 못 당한다",
          "original_form": "그래 진짜 말로는 못 당한다",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.2",
          "form": "그래도 동생들이 와서 장난감 만지는 걸로는 별 말 안 함",
          "original_form": "그래도 동생들이 와서 장난감 만지는 걸로는 별 말 안 함",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.3",
          "form": "name3가 만졌음 난리 났을건데",
          "original_form": "&name3&가 만졌음 난리 났을건데",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.4",
          "form": "근데 애들도 놀다가 형 오니까 다들 움찔해서",
          "original_form": "근데 애들도 놀다가 형 오니까 다들 움찔해서",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그땐 나이많음  짱이져 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그땐 나이많음  짱이져 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.6",
          "form": "굽신거리며",
          "original_form": "굽신거리며",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.7",
          "form": "초등학생인데 ㅋㅋㅋㅋㅋ",
          "original_form": "초등학생인데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.8",
          "form": "아욱겨 ㅋㅋㅋㅋ",
          "original_form": "아욱겨 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:33"
        },
        {
          "id": "MDRW2100018731.1.10",
          "form": "남자애들 둘이 계속 싸우니까",
          "original_form": "남자애들 둘이 계속 싸우니까",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.11",
          "form": "name4이 가서",
          "original_form": "&name4&이 가서",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.12",
          "form": "자꾸 싸우면 장난감 못 가지고 놀게 한다",
          "original_form": "자꾸 싸우면 장난감 못 가지고 놀게 한다",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.13",
          "form": "라고 멋있게 말하고 옴",
          "original_form": "라고 멋있게 말하고 옴",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.14",
          "form": "name4이 서열정리 잘하네여 ㅋㅋㅋㅋ",
          "original_form": "&name4&이 서열정리 잘하네여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.16",
          "form": "든든허네 ㅋㅋㅋㅋㅋ",
          "original_form": "든든허네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.17",
          "form": "없었음 싸움 길어졌을듯 ㅋㅋㅋㅋ",
          "original_form": "없었음 싸움 길어졌을듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.18",
          "form": "어제 좀 했다",
          "original_form": "어제 좀 했다",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.19",
          "form": "어후 다시는 부르기 싫음",
          "original_form": "어후 다시는 부르기 싫음",
          "speaker_id": "2",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.20",
          "form": "글쵸ㅠㅠ",
          "original_form": "글쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.21",
          "form": "난 요즘 애들 집에 안불러요 ㅋㅋㅋㅋ",
          "original_form": "난 요즘 애들 집에 안불러요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018731.1.22",
          "form": "집에 안갈라캐가 ㅋㅋㅋㅋ",
          "original_form": "집에 안갈라캐가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.23",
          "form": "집은 무조건 일대일",
          "original_form": "집은 무조건 일대일",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.24",
          "form": "아 ㅋㅋㅋㅋㅋㅋㄱㅋ 그래 피곤타",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㄱㅋ 그래 피곤타",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.25",
          "form": "우린 7시되믄 씻고 잘 준비해야되는데 ㅋ",
          "original_form": "우린 7시되믄 씻고 잘 준비해야되는데 ㅋ",
          "speaker_id": "1",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.26",
          "form": "저녁까지 먹고 갈라고 맨날",
          "original_form": "저녁까지 먹고 갈라고 맨날",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.27",
          "form": "8시되가도 안갈라고;",
          "original_form": "8시되가도 안갈라고;",
          "speaker_id": "1",
          "time": "20210827 11:35"
