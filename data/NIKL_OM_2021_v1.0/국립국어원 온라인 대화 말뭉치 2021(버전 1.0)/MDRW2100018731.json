        {
          "id": "MDRW2100018731.1.28",
          "form": "육퇴가",
          "original_form": "육퇴가",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.29",
          "form": "빠르니...",
          "original_form": "빠르니...",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.30",
          "form": "웁스 8시....",
          "original_form": "웁스 8시....",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.31",
          "form": "그럼 둘찌가 힘들지",
          "original_form": "그럼 둘찌가 힘들지",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.32",
          "form": "다꼬인다",
          "original_form": "다꼬인다",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.33",
          "form": "전에 name5 언니랑 둘이 왔는데ㅋㅋㅋㅋㅋ",
          "original_form": "전에 &name5& 언니랑 둘이 왔는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018731.1.34",
          "form": "둘다 안가서 ㅋㅋㅋ",
          "original_form": "둘다 안가서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018731.1.35",
          "form": "신랑도 빨리 밥먹고 애들 씻기고해야되는데 ㅋㅋ",
          "original_form": "신랑도 빨리 밥먹고 애들 씻기고해야되는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018731.1.36",
          "form": "6시에 놀러가도 되냐고 ㅋㅋㅋㅋ안돼 ㅋㅋㅋㅋ",
          "original_form": "6시에 놀러가도 되냐고 ㅋㅋㅋㅋ안돼 ㅋㅋㅋㅋ",
