      },
      "utterance": [
        {
          "id": "MDRW2100026426.1.1",
          "form": "그게 그렇게 가금 생각난다",
          "original_form": "그게 그렇게 가금 생각난다",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026426.1.2",
          "form": "머가??",
          "original_form": "머가??",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026426.1.3",
          "form": "보름달이요",
          "original_form": "보름달이요",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026426.1.4",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.5",
          "form": "그거 우유랑먹으면",
          "original_form": "그거 우유랑먹으면",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.6",
          "form": "진짜 맛있는데",
          "original_form": "진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.7",
          "form": "근데 크림이 가끔 모래씹는맛남",
          "original_form": "근데 크림이 가끔 모래씹는맛남",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.8",
          "form": "그래도 막잇어서 봐준다",
          "original_form": "그래도 막잇어서 봐준다",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.9",
          "form": "난 그래도",
          "original_form": "난 그래도",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.10",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.11",
          "form": "그게 매력인거같아",
          "original_form": "그게 매력인거같아",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.12",
          "form": "빵 겉에",
          "original_form": "빵 겉에",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.13",
          "form": "땅콩향같은거",
          "original_form": "땅콩향같은거",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.14",
          "form": "살짝 나고..",
          "original_form": "살짝 나고..",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026426.1.15",
          "form": "음 그리고...",
          "original_form": "음 그리고...",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.16",
          "form": "하 가끔 이렇게",
          "original_form": "하 가끔 이렇게",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.17",
          "form": "편의점빵이 또 먹고싶을때가 잇네",
          "original_form": "편의점빵이 또 먹고싶을때가 잇네",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.18",
          "form": "편의점에서",
          "original_form": "편의점에서",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.19",
          "form": "뭐사먹음",
          "original_form": "뭐사먹음",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.20",
          "form": "둥그런 네모 치즈빵같은거",
          "original_form": "둥그런 네모 치즈빵같은거",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.21",
          "form": "뭐 사먹냐고요?",
          "original_form": "뭐 사먹냐고요?",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.22",
          "form": "맛있는디",
          "original_form": "맛있는디",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026426.1.25",
          "form": "그거 그냥 치즈 가운데에",
          "original_form": "그거 그냥 치즈 가운데에",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026426.1.26",
          "form": "선처럼 그어진 빵",
          "original_form": "선처럼 그어진 빵",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026426.1.27",
          "form": "말하나",
          "original_form": "말하나",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026426.1.28",
          "form": "그게머지?",
          "original_form": "그게머지?",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026426.1.29",
          "form": "둥그런 네모치즈방은 뭐야,",
          "original_form": "둥그런 네모치즈방은 뭐야,",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026426.1.30",
          "form": "가운데에보통",
          "original_form": "가운데에보통",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026426.1.31",
          "form": "캐릭터그림그려져잇음",
          "original_form": "캐릭터그림그려져잇음",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026426.1.32",
          "form": "뭔지 모르겠어요",
          "original_form": "뭔지 모르겠어요",
          "speaker_id": "2",
          "time": "20210901 22:12"
        }
      ]
    }
  ]
