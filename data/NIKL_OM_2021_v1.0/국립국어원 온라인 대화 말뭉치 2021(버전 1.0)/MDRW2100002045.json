      "utterance": [
        {
          "id": "MDRW2100002045.1.1",
          "form": "여행가고싶다아~~~ 코로나 언제끝나니 ㅠㅠ",
          "original_form": "여행가고싶다아~~~ 코로나 언제끝나니 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100002045.1.2",
          "form": "하...나도",
          "original_form": "하...나도",
          "speaker_id": "2",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100002045.1.3",
          "form": "해외여행 가고싶은데ㅜㅜ",
          "original_form": "해외여행 가고싶은데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002045.1.4",
          "form": "근데 어제보니까 백신맞고나서",
          "original_form": "근데 어제보니까 백신맞고나서",
          "speaker_id": "2",
          "time": "20210821 09:48"
        },
        {
          "id": "MDRW2100002045.1.5",
          "form": "이제 다니는사람은 다니나봐",
          "original_form": "이제 다니는사람은 다니나봐",
          "speaker_id": "2",
          "time": "20210821 09:49"
        },
        {
          "id": "MDRW2100002045.1.6",
          "form": "우리도 가볼까?",
          "original_form": "우리도 가볼까?",
          "speaker_id": "2",
          "time": "20210821 09:49"
        },
        {
          "id": "MDRW2100002045.1.7",
          "form": "근데 아직은 좀 무서움..",
          "original_form": "근데 아직은 좀 무서움..",
          "speaker_id": "1",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100002045.1.8",
          "form": "그렇긴한데..",
          "original_form": "그렇긴한데..",
          "speaker_id": "2",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100002045.1.9",
          "form": "근데 이탈리아 그런데 가보고 싶긴 해...",
          "original_form": "근데 이탈리아 그런데 가보고 싶긴 해...",
          "speaker_id": "1",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100002045.1.10",
          "form": "나도...",
          "original_form": "나도...",
          "speaker_id": "2",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100002045.1.11",
          "form": "그리고 난 스위스",
          "original_form": "그리고 난 스위스",
          "speaker_id": "2",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100002045.1.12",
          "form": "난 옛날부터 그렇게 스위스가 가보고 싶더라",
          "original_form": "난 옛날부터 그렇게 스위스가 가보고 싶더라",
          "speaker_id": "1",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100002045.1.13",
          "form": "그냥 이상하게 스위스가 땡겨",
          "original_form": "그냥 이상하게 스위스가 땡겨",
          "speaker_id": "1",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100002045.1.14",
          "form": "자긴 스페인 갔다왔ㅈ?",
          "original_form": "자긴 스페인 갔다왔ㅈ?",
          "speaker_id": "2",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100002045.1.15",
          "form": "응 친구들이랑",
          "original_form": "응 친구들이랑",
          "speaker_id": "1",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100002045.1.16",
          "form": "그때가 벌써 몇년전인가....",
          "original_form": "그때가 벌써 몇년전인가....",
          "speaker_id": "1",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100002045.1.17",
          "form": "국내여행만 가니까 해외가 너무 고파",
          "original_form": "국내여행만 가니까 해외가 너무 고파",
          "speaker_id": "1",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100002045.1.18",
          "form": "휴가쓰고 해외여행 계획해놓고",
          "original_form": "휴가쓰고 해외여행 계획해놓고",
          "speaker_id": "2",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100002045.1.19",
          "form": "그날만 바라보면",
          "original_form": "그날만 바라보면",
          "speaker_id": "2",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100002045.1.20",
          "form": "일하는데",
          "original_form": "일하는데",
          "speaker_id": "2",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100002045.1.21",
          "form": "그게 없네 요즘",
          "original_form": "그게 없네 요즘",
          "speaker_id": "2",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100002045.1.22",
          "form": "원래 여행으로 리프레시 하는건데",
          "original_form": "원래 여행으로 리프레시 하는건데",
          "speaker_id": "1",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100002045.1.23",
          "form": "쉽지 않다 요즘 그게",
          "original_form": "쉽지 않다 요즘 그게",
          "speaker_id": "1",
          "time": "20210821 09:52"
        },
        {
          "id": "MDRW2100002045.1.24",
          "form": "그나마 제주도 가는게",
          "original_form": "그나마 제주도 가는게",
          "speaker_id": "2",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100002045.1.25",
          "form": "제일 해외가는거 같지",
          "original_form": "제일 해외가는거 같지",
          "speaker_id": "2",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100002045.1.26",
          "form": "비행기타니까 ㅋㅋㅋ",
          "original_form": "비행기타니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 09:53"
        },
        {
          "id": "MDRW2100002045.1.27",
          "form": "자긴 비행기도 무서워 하면서 ㅋㅋㅋ",
          "original_form": "자긴 비행기도 무서워 하면서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:54"
        },
        {
          "id": "MDRW2100002045.1.28",
          "form": "근데 타고싶어 ㅋㅋㅋ",
          "original_form": "근데 타고싶어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 09:54"
        }
      ]
    }
