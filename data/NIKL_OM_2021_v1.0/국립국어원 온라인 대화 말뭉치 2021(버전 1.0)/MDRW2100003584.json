      "utterance": [
        {
          "id": "MDRW2100003584.1.1",
          "form": "싸이월드 하셨었나요?",
          "original_form": "싸이월드 하셨었나요?",
          "speaker_id": "2",
          "time": "20210821 02:52"
        },
        {
          "id": "MDRW2100003584.1.2",
          "form": "예전에 싸이월드 다이어리",
          "original_form": "예전에 싸이월드 다이어리",
          "speaker_id": "1",
          "time": "20210821 02:52"
        },
        {
          "id": "MDRW2100003584.1.3",
          "form": "엄청 열심히 썼던 기억이나요",
          "original_form": "엄청 열심히 썼던 기억이나요",
          "speaker_id": "1",
          "time": "20210821 02:52"
        },
        {
          "id": "MDRW2100003584.1.4",
          "form": "그럼 부활하면 그게 다 남아있는 건가요?",
          "original_form": "그럼 부활하면 그게 다 남아있는 건가요?",
          "speaker_id": "2",
          "time": "20210821 02:53"
        },
        {
          "id": "MDRW2100003584.1.5",
          "form": "근데 저도 그런줄 알았는데",
          "original_form": "근데 저도 그런줄 알았는데",
          "speaker_id": "1",
          "time": "20210821 02:53"
        },
        {
          "id": "MDRW2100003584.1.6",
          "form": "다들 부활시켰다 이런말이",
          "original_form": "다들 부활시켰다 이런말이",
          "speaker_id": "1",
          "time": "20210821 02:53"
        },
        {
          "id": "MDRW2100003584.1.7",
          "form": "없더라구요?",
          "original_form": "없더라구요?",
          "speaker_id": "1",
          "time": "20210821 02:53"
        },
        {
          "id": "MDRW2100003584.1.8",
          "form": "헉 그럼 부활 안하는 건가요?",
          "original_form": "헉 그럼 부활 안하는 건가요?",
          "speaker_id": "2",
          "time": "20210821 02:54"
        },
        {
          "id": "MDRW2100003584.1.9",
          "form": "싸이월드 부활 이슈였던건 아는데",
          "original_form": "싸이월드 부활 이슈였던건 아는데",
          "speaker_id": "2",
          "time": "20210821 02:54"
        },
        {
          "id": "MDRW2100003584.1.10",
          "form": "이게 개인정보 위반 이런이슈때문에",
          "original_form": "이게 개인정보 위반 이런이슈때문에",
          "speaker_id": "1",
          "time": "20210821 02:54"
        },
        {
          "id": "MDRW2100003584.1.11",
          "form": "엄청 이슈더라구요",
          "original_form": "엄청 이슈더라구요",
          "speaker_id": "1",
          "time": "20210821 02:54"
        },
        {
          "id": "MDRW2100003584.1.12",
          "form": "싸이월드 부활하면",
          "original_form": "싸이월드 부활하면",
          "speaker_id": "1",
          "time": "20210821 02:55"
        },
        {
          "id": "MDRW2100003584.1.13",
          "form": "흑역사 엄청날거같아요",
          "original_form": "흑역사 엄청날거같아요",
          "speaker_id": "1",
          "time": "20210821 02:55"
        },
        {
          "id": "MDRW2100003584.1.14",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 02:55"
        },
        {
          "id": "MDRW2100003584.1.15",
          "form": "그게 지금까지 보존될수 있다는것도 신기하네요",
          "original_form": "그게 지금까지 보존될수 있다는것도 신기하네요",
          "speaker_id": "2",
          "time": "20210821 02:55"
        },
        {
          "id": "MDRW2100003584.1.16",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003584.1.17",
          "form": "저는 제 흑역사가 부활하는게",
          "original_form": "저는 제 흑역사가 부활하는게",
          "speaker_id": "1",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003584.1.18",
          "form": "너무 싫어요 ㅋㅋㅋㅋ",
          "original_form": "너무 싫어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003584.1.19",
          "form": "저라도 그럴거같아요ㅠㅠ",
          "original_form": "저라도 그럴거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003584.1.20",
          "form": "저는 싸이월드를 안해봤어서",
          "original_form": "저는 싸이월드를 안해봤어서",
          "speaker_id": "2",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003584.1.21",
          "form": "싸이월드 그래도 궁금하긴해요",
          "original_form": "싸이월드 그래도 궁금하긴해요",
          "speaker_id": "2",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003584.1.22",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210821 02:58"
        },
        {
          "id": "MDRW2100003584.1.23",
          "form": "저두 엄청 예전에 했던거라",
          "original_form": "저두 엄청 예전에 했던거라",
          "speaker_id": "1",
          "time": "20210821 02:58"
        },
        {
          "id": "MDRW2100003584.1.24",
          "form": "기억이 잘 안나요 ㅋㅋ",
          "original_form": "기억이 잘 안나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:58"
        },
        {
          "id": "MDRW2100003584.1.25",
          "form": "도토리 같은거 있고ㅋㅋㅋ",
          "original_form": "도토리 같은거 있고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 02:58"
        },
        {
          "id": "MDRW2100003584.1.26",
          "form": "캡쳐 꽤 봤는데 감성이 있더라구요 나름",
          "original_form": "캡쳐 꽤 봤는데 감성이 있더라구요 나름",
          "speaker_id": "2",
          "time": "20210821 02:59"
        },
        {
          "id": "MDRW2100003584.1.27",
          "form": "맞아요 몽환의 숲 노래가",
          "original_form": "맞아요 몽환의 숲 노래가",
          "speaker_id": "1",
          "time": "20210821 02:59"
        },
        {
          "id": "MDRW2100003584.1.28",
          "form": "싸이월드 BGM으로 대박이었어요 ㅋㅋ",
          "original_form": "싸이월드 BGM으로 대박이었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 03:00"
        },
        {
          "id": "MDRW2100003584.1.29",
          "form": "오 방금 검색해서 들어봤는데",
          "original_form": "오 방금 검색해서 들어봤는데",
          "speaker_id": "2",
          "time": "20210821 03:01"
        },
        {
          "id": "MDRW2100003584.1.30",
          "form": "많이 들어봤던 노래인 것 같아요",
          "original_form": "많이 들어봤던 노래인 것 같아요",
          "speaker_id": "2",
          "time": "20210821 03:01"
        },
        {
          "id": "MDRW2100003584.1.31",
          "form": "쉿 몽환의 숲",
          "original_form": "쉿 몽환의 숲",
          "speaker_id": "1",
          "time": "20210821 03:03"
        }
      ]
