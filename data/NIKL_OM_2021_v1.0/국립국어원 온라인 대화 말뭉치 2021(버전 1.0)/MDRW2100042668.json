      "utterance": [
        {
          "id": "MDRW2100042668.1.1",
          "form": "방금 신랑 나왔다가 드갔으 ㅋㅋㅋ",
          "original_form": "방금 신랑 나왔다가 드갔으 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:45"
        },
        {
          "id": "MDRW2100042668.1.2",
          "form": "양치안하고 잤다고 양치하고 드가네",
          "original_form": "양치안하고 잤다고 양치하고 드가네",
          "speaker_id": "2",
          "time": "20210907 00:45"
        },
        {
          "id": "MDRW2100042668.1.3",
          "form": "안자나?/",
          "original_form": "안자나?/",
          "speaker_id": "1",
          "time": "20210907 00:45"
        },
        {
          "id": "MDRW2100042668.1.4",
          "form": "아 ㅋㅋㅋ 나도 양치아직인데ㅠ자기전에 해야지",
          "original_form": "아 ㅋㅋㅋ 나도 양치아직인데ㅠ자기전에 해야지",
          "speaker_id": "1",
          "time": "20210907 00:45"
        },
        {
          "id": "MDRW2100042668.1.5",
          "form": "잠와서 이 닦고 자면 잠 다 깸ㅋㅋ",
          "original_form": "잠와서 이 닦고 자면 잠 다 깸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:46"
        },
        {
          "id": "MDRW2100042668.1.6",
          "form": "자다가 깼어 ㅋㅋ\r\n난 아까 양치질 했쥐~~~\r\n늦어서 자야된당 이제",
          "original_form": "자다가 깼어 ㅋㅋ\r\n난 아까 양치질 했쥐~~~\r\n늦어서 자야된당 이제",
          "speaker_id": "2",
          "time": "20210907 00:46"
        },
        {
          "id": "MDRW2100042668.1.7",
          "form": "그래야 내일 출근하지 ㅠㅠ",
          "original_form": "그래야 내일 출근하지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 00:46"
        },
        {
          "id": "MDRW2100042668.1.8",
          "form": "응응 ㅠㅠ 어여자 ㅠㅠ",
          "original_form": "응응 ㅠㅠ 어여자 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 00:46"
        },
        {
          "id": "MDRW2100042668.1.9",
          "form": "내일은 열안나서 백신 맞을 수 있기를 ㅠㅠ",
          "original_form": "내일은 열안나서 백신 맞을 수 있기를 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 00:47"
        },
        {
          "id": "MDRW2100042668.1.10",
          "form": "낼 잔여가 있어야 맞는거 아니고?",
          "original_form": "낼 잔여가 있어야 맞는거 아니고?",
          "speaker_id": "1",
          "time": "20210907 00:47"
        },
        {
          "id": "MDRW2100042668.1.11",
          "form": "보통 매일 잔여 생기더라 ㅋㅋ\r\n최소 한개 정도는",
          "original_form": "보통 매일 잔여 생기더라 ㅋㅋ\r\n최소 한개 정도는",
          "speaker_id": "2",
          "time": "20210907 00:48"
        },
        {
          "id": "MDRW2100042668.1.12",
          "form": "오 그럼 낼 열안남 맞겠네. 짐 갠찮아?",
          "original_form": "오 그럼 낼 열안남 맞겠네. 짐 갠찮아?",
          "speaker_id": "1",
          "time": "20210907 00:50"
        },
        {
          "id": "MDRW2100042668.1.13",
          "form": "일어났지?ㅋㅋ",
          "original_form": "일어났지?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:15"
        },
        {
          "id": "MDRW2100042668.1.14",
          "form": "아침에 채팅이 안갔네",
          "original_form": "아침에 채팅이 안갔네",
          "speaker_id": "2",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100042668.1.15",
          "form": "오늘아침에 일어나서 재봤는데",
          "original_form": "오늘아침에 일어나서 재봤는데",
          "speaker_id": "2",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100042668.1.16",
          "form": "정상 ㅋ",
          "original_form": "정상 ㅋ",
          "speaker_id": "2",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100042668.1.17",
          "form": "제발 좀 백신맞구 싶다",
          "original_form": "제발 좀 백신맞구 싶다",
          "speaker_id": "2",
          "time": "20210907 08:18"
        },
        {
          "id": "MDRW2100042668.1.18",
          "form": "오 다행!!!",
          "original_form": "오 다행!!!",
          "speaker_id": "1",
          "time": "20210907 08:18"
        },
        {
          "id": "MDRW2100042668.1.19",
          "form": "퇴근 3시에하고 가는거지?",
          "original_form": "퇴근 3시에하고 가는거지?",
          "speaker_id": "1",
          "time": "20210907 08:18"
        },
        {
          "id": "MDRW2100042668.1.20",
          "form": "아니 그전에 연락오면 나왔다가 맞고 다시출근ㅋㅋ",
          "original_form": "아니 그전에 연락오면 나왔다가 맞고 다시출근ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042668.1.21",
          "form": "아 좋다 그럼 되겠네 좋은 직장.!",
          "original_form": "아 좋다 그럼 되겠네 좋은 직장.!",
          "speaker_id": "1",
          "time": "20210907 08:34"
        },
        {
          "id": "MDRW2100042668.1.22",
          "form": "애들은 다갔어?? 밥은>",
          "original_form": "애들은 다갔어?? 밥은>",
          "speaker_id": "1",
          "time": "20210907 08:36"
        },
        {
          "id": "MDRW2100042668.1.23",
          "form": "갔지 오늘은 코코볼ㅋㅋ",
          "original_form": "갔지 오늘은 코코볼ㅋㅋ",
          "speaker_id": "2",
