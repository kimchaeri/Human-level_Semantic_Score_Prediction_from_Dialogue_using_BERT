      "utterance": [
        {
          "id": "MDRW2100020977.1.1",
          "form": "파스타는 무슨맛 제일 좋아하세요?",
          "original_form": "파스타는 무슨맛 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020977.1.2",
          "form": "전 예전엔 로제인데",
          "original_form": "전 예전엔 로제인데",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.3",
          "form": "요샌 크림요!",
          "original_form": "요샌 크림요!",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.4",
          "form": "로제를 하도 먹어서..ㅋㅋㅋㅋ",
          "original_form": "로제를 하도 먹어서..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.5",
          "form": "아 저도 로제가 최애",
          "original_form": "아 저도 로제가 최애",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.6",
          "form": "하 침나오네요",
          "original_form": "하 침나오네요",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.7",
          "form": "그리고 투움바요",
          "original_form": "그리고 투움바요",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.8",
          "form": "오 저는 투움바파스타는 못먹어봤어요",
          "original_form": "오 저는 투움바파스타는 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.9",
          "form": "그 아웃백",
          "original_form": "그 아웃백",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.11",
          "form": "너무 맛있어요",
          "original_form": "너무 맛있어요",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020977.1.12",
          "form": "오 먹어봐야겠네요",
          "original_form": "오 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020977.1.13",
          "form": "아웃백 비싸긴한데 추천해요 투움바",
          "original_form": "아웃백 비싸긴한데 추천해요 투움바",
          "speaker_id": "1",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020977.1.14",
          "form": "비싼덴 이유가 있겠죠??",
          "original_form": "비싼덴 이유가 있겠죠??",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020977.1.15",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020977.1.16",
          "form": "맛있긴하니까요",
          "original_form": "맛있긴하니까요",
