      "utterance": [
        {
          "id": "MDRW2100003845.1.1",
          "form": "저두 그런거 있어요",
          "original_form": "저두 그런거 있어요",
          "speaker_id": "2",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003845.1.2",
          "form": "그러고 참다가 터지는거예요",
          "original_form": "그러고 참다가 터지는거예요",
          "speaker_id": "1",
          "time": "20210821 12:50"
        },
        {
          "id": "MDRW2100003845.1.3",
          "form": "연애때 잠깐 헤어졌던적이 있는데",
          "original_form": "연애때 잠깐 헤어졌던적이 있는데",
          "speaker_id": "2",
          "time": "20210821 12:51"
        },
        {
          "id": "MDRW2100003845.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210821 12:51"
        },
        {
          "id": "MDRW2100003845.1.5",
          "form": "진짜 아무것도 아니고 남사친이랑 일적으로 만난적이 있었거든요 딱 1번",
          "original_form": "진짜 아무것도 아니고 남사친이랑 일적으로 만난적이 있었거든요 딱 1번",
          "speaker_id": "2",
          "time": "20210821 12:51"
        },
        {
          "id": "MDRW2100003845.1.6",
          "form": "근데 그걸 나중에 제 카톡보고 알아가지고는 그때부터 지금까지도 그얘기 해요",
          "original_form": "근데 그걸 나중에 제 카톡보고 알아가지고는 그때부터 지금까지도 그얘기 해요",
          "speaker_id": "2",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003845.1.7",
          "form": "ㅋ대박 나 뭔지 알거같아",
          "original_form": "ㅋ대박 나 뭔지 알거같아",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003845.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003845.1.9",
          "form": "저도 똑같은거 있어요",
          "original_form": "저도 똑같은거 있어요",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003845.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003845.1.11",
          "form": "정말 싫아요ㅠ",
          "original_form": "정말 싫아요ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003845.1.12",
          "form": "사심없는 관계고 고등학교 동창이라서",
          "original_form": "사심없는 관계고 고등학교 동창이라서",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003845.1.13",
          "form": "그냥 오랜 친구거든요?",
          "original_form": "그냥 오랜 친구거든요?",
          "speaker_id": "1",
          "time": "20210821 12:52"
        },
        {
          "id": "MDRW2100003845.1.14",
          "form": "걔랑 일 때문에 도와달라고 한거가지고",
          "original_form": "걔랑 일 때문에 도와달라고 한거가지고",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.15",
          "form": "네네~그런사이 있죠",
          "original_form": "네네~그런사이 있죠",
          "speaker_id": "2",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.16",
          "form": "아주 썸이니 뭐니ㅡㅡ",
          "original_form": "아주 썸이니 뭐니ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.17",
          "form": "지는 없나ㅋㅋㅋ",
          "original_form": "지는 없나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.18",
          "form": "헐 어쩜",
          "original_form": "헐 어쩜",
          "speaker_id": "2",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.19",
          "form": "남자들은 다 이런건가",
          "original_form": "남자들은 다 이런건가",
          "speaker_id": "2",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.20",
          "form": "와 진짜 돌아버리겠더라고요",
          "original_form": "와 진짜 돌아버리겠더라고요",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.21",
          "form": "걔가 맘이 있을거다",
          "original_form": "걔가 맘이 있을거다",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.22",
          "form": "이러면서",
          "original_form": "이러면서",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.23",
          "form": "헐 저희 남편이랑 비슷하시다",
          "original_form": "헐 저희 남편이랑 비슷하시다",
          "speaker_id": "2",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.24",
          "form": "그냥~~~~오랜 친구고 일 얘기하고 부탁한게 끝인데",
          "original_form": "그냥~~~~오랜 친구고 일 얘기하고 부탁한게 끝인데",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100003845.1.25",
          "form": "뭔 사심ㅡㅡ",
          "original_form": "뭔 사심ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.26",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.27",
          "form": "자기가 여사친한테 사심있으니 그런말하는거겠죠ㅋㅋㅋ",
          "original_form": "자기가 여사친한테 사심있으니 그런말하는거겠죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.28",
          "form": "이렇게 받아쳤더니",
          "original_form": "이렇게 받아쳤더니",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.29",
          "form": "남자는 다 똑같네어쩌고ㅡㅡ",
          "original_form": "남자는 다 똑같네어쩌고ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.30",
          "form": "참내...",
          "original_form": "참내...",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.31",
          "form": "헉 맙소사",
          "original_form": "헉 맙소사",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.32",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.33",
          "form": "남편분도 그얘기했어요?",
          "original_form": "남편분도 그얘기했어요?",
          "speaker_id": "1",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.34",
          "form": "너무 비슷해서요",
          "original_form": "너무 비슷해서요",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.35",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 12:54"
        },
        {
          "id": "MDRW2100003845.1.36",
          "form": "ㅋㅋㅋㅋ어우 남자들 다 똑같은거 맞네",
          "original_form": "ㅋㅋㅋㅋ어우 남자들 다 똑같은거 맞네",
          "speaker_id": "1",
          "time": "20210821 12:55"
        },
        {
          "id": "MDRW2100003845.1.37",
          "form": "그런가봐요 ㅠㅠ",
          "original_form": "그런가봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 12:55"
        }
      ]
    }
