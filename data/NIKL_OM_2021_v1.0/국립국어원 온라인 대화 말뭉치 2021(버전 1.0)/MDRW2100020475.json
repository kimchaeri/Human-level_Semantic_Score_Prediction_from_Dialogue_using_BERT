        {
          "id": "MDRW2100020475.1.1",
          "form": "가정 주부 입니다\r\n일하세요?",
          "original_form": "가정 주부 입니다\r\n일하세요?",
          "speaker_id": "2",
          "time": "20210829 06:08"
        },
        {
          "id": "MDRW2100020475.1.2",
          "form": "네 일해요",
          "original_form": "네 일해요",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100020475.1.3",
          "form": "바쁘겠네요 저야 살림 하면 바쁜데 일다니는 것만 못하겠죠",
          "original_form": "바쁘겠네요 저야 살림 하면 바쁜데 일다니는 것만 못하겠죠",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100020475.1.4",
          "form": "가정일도 많죠",
          "original_form": "가정일도 많죠",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100020475.1.5",
          "form": "집안일은 끝이 없는 것 같아요",
          "original_form": "집안일은 끝이 없는 것 같아요",
          "speaker_id": "2",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100020475.1.6",
          "form": "해도해도 똑같은 걸 계속 해야 하죠",
          "original_form": "해도해도 똑같은 걸 계속 해야 하죠",
          "speaker_id": "2",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100020475.1.7",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100020475.1.8",
          "form": "그리고 딱히 보람도 없고",
          "original_form": "그리고 딱히 보람도 없고",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100020475.1.9",
          "form": "그래서 간단한 알바라도 할까 생각중이에요",
          "original_form": "그래서 간단한 알바라도 할까 생각중이에요",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100020475.1.10",
          "form": "부업이요?",
          "original_form": "부업이요?",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100020475.1.11",
          "form": "공공근로요",
          "original_form": "공공근로요",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100020475.1.12",
          "form": "아 편한가요?",
          "original_form": "아 편한가요?",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100020475.1.13",
