      "utterance": [
        {
          "id": "MDRW2100034808.1.1",
          "form": "네 올해는 햇빛이 세서 양산유행했었죠",
          "original_form": "네 올해는 햇빛이 세서 양산유행했었죠",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100034808.1.2",
          "form": "맞아요 너무 세더라구요 ..",
          "original_form": "맞아요 너무 세더라구요 ..",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034808.1.3",
          "form": "저도 살걸 그랬어요 지났지만",
          "original_form": "저도 살걸 그랬어요 지났지만",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034808.1.4",
          "form": "ㅋㅋ 내년에 쓰실수있어요",
          "original_form": "ㅋㅋ 내년에 쓰실수있어요",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034808.1.5",
          "form": "오 생각지도 못한 ㅋㅋㅋㅋ",
          "original_form": "오 생각지도 못한 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034808.1.6",
          "form": "그치만 일단 당장 안 필요하니 내년에 살래요 ㅋㅋㅋ",
          "original_form": "그치만 일단 당장 안 필요하니 내년에 살래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034808.1.7",
          "form": "그것도 좋죠",
          "original_form": "그것도 좋죠",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034808.1.8",
          "form": "싸고이쁜거 보이면 킵하세요",
          "original_form": "싸고이쁜거 보이면 킵하세요",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034808.1.9",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034808.1.10",
          "form": "오히려 시즌오프라 쌀수도 있겠네요 ㅋㅋ",
          "original_form": "오히려 시즌오프라 쌀수도 있겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034808.1.11",
          "form": "네여름옷 싸듯이 ㅎㅎ",
          "original_form": "네여름옷 싸듯이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034808.1.12",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034808.1.13",
          "form": "저는 수리하면서 양산 오래 써요",
          "original_form": "저는 수리하면서 양산 오래 써요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034808.1.14",
          "form": "오 수리도 되나요??",
          "original_form": "오 수리도 되나요??",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034808.1.15",
          "form": "가벼운건제가 고쳐요",
          "original_form": "가벼운건제가 고쳐요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034808.1.16",
          "form": "손재주가 좋으신가봐요!",
          "original_form": "손재주가 좋으신가봐요!",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034808.1.17",
          "form": "아니요 부실하게 고쳐져서 저만쓸수있어요ㅋㅜㅜㅜ",
          "original_form": "아니요 부실하게 고쳐져서 저만쓸수있어요ㅋㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 19:56"
        }
