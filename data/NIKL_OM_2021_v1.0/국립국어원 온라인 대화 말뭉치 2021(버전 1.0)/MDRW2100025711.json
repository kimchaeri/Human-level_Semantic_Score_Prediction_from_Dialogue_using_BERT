      "utterance": [
        {
          "id": "MDRW2100025711.1.1",
          "form": "오 어떤 국밥?",
          "original_form": "오 어떤 국밥?",
          "speaker_id": "2",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025711.1.2",
          "form": "거기 국밥보다 수육이 진짜 제대로야",
          "original_form": "거기 국밥보다 수육이 진짜 제대로야",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025711.1.3",
          "form": "돼지국밥",
          "original_form": "돼지국밥",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025711.1.4",
          "form": "아 진짜 맛있겠다",
          "original_form": "아 진짜 맛있겠다",
          "speaker_id": "2",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025711.1.5",
          "form": "그치 수육을 항정살로 해주더라고",
          "original_form": "그치 수육을 항정살로 해주더라고",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025711.1.6",
          "form": "허어어얼 완전 맛있겠다ㅠㅠㅠ",
          "original_form": "허어어얼 완전 맛있겠다ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:18"
        },
        {
          "id": "MDRW2100025711.1.7",
          "form": "나 돼지국밥 진짜 좋아해",
          "original_form": "나 돼지국밥 진짜 좋아해",
          "speaker_id": "2",
          "time": "20210901 16:18"
        },
        {
          "id": "MDRW2100025711.1.8",
          "form": "진짜 맛있어ㅠㅠ 김치도 생김치 주는데... 9천원 밖에 안해",
          "original_form": "진짜 맛있어ㅠㅠ 김치도 생김치 주는데... 9천원 밖에 안해",
          "speaker_id": "1",
          "time": "20210901 16:18"
        },
        {
          "id": "MDRW2100025711.1.9",
          "form": "헐 진짜 꼭 먹으러가고싶다...",
          "original_form": "헐 진짜 꼭 먹으러가고싶다...",
          "speaker_id": "2",
          "time": "20210901 16:18"
        },
        {
          "id": "MDRW2100025711.1.10",
          "form": "집근처에 맛집없어?",
          "original_form": "집근처에 맛집없어?",
          "speaker_id": "1",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025711.1.11",
          "form": "그런데 가면 밥 세그릇씩 먹어줘야된다구...",
          "original_form": "그런데 가면 밥 세그릇씩 먹어줘야된다구...",
          "speaker_id": "2",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025711.1.12",
          "form": "나는 혜화에 뼈해장국 맛집 찾았어",
          "original_form": "나는 혜화에 뼈해장국 맛집 찾았어",
          "speaker_id": "2",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025711.1.13",
          "form": "ㅋㅋㅋㅋ맞어 여기도 맨날 점심시간 가면 아저씨들 그득..",
          "original_form": "ㅋㅋㅋㅋ맞어 여기도 맨날 점심시간 가면 아저씨들 그득..",
          "speaker_id": "1",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025711.1.14",
          "form": "아 뼈해장국 진짜 좋아하는데",
          "original_form": "아 뼈해장국 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025711.1.15",
          "form": "우리집까지 배달도 오는데 진짜 맛있어...",
          "original_form": "우리집까지 배달도 오는데 진짜 맛있어...",
          "speaker_id": "2",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025711.1.16",
          "form": "헐..김치도 줘?",
          "original_form": "헐..김치도 줘?",
          "speaker_id": "1",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100025711.1.17",
          "form": "김치는 안줬던거 같은데 김치 생각 안나는 맛...",
          "original_form": "김치는 안줬던거 같은데 김치 생각 안나는 맛...",
          "speaker_id": "2",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100025711.1.18",
          "form": "뼈해장국은 겉절이랑 먹어야 진짜 맛있는데",
          "original_form": "뼈해장국은 겉절이랑 먹어야 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100025711.1.19",
          "form": "아 호떡 먹고싶어",
          "original_form": "아 호떡 먹고싶어",
          "speaker_id": "1",
          "time": "20210901 16:27"
        }
      ]
