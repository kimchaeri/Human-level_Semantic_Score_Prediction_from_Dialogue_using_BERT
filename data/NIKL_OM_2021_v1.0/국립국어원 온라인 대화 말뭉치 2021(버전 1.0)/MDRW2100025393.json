      "utterance": [
        {
          "id": "MDRW2100025393.1.1",
          "form": "어떤 음료 좋아하세요",
          "original_form": "어떤 음료 좋아하세요",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025393.1.2",
          "form": "저 에이드류 좋어해요",
          "original_form": "저 에이드류 좋어해요",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025393.1.3",
          "form": "에이드는 자몽 맛있던데 어떤 종류용?",
          "original_form": "에이드는 자몽 맛있던데 어떤 종류용?",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025393.1.4",
          "form": "저는 오렌지가 요즘 땡기더라구요",
          "original_form": "저는 오렌지가 요즘 땡기더라구요",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025393.1.5",
          "form": "오렌지 상큼하니 좋지요",
          "original_form": "오렌지 상큼하니 좋지요",
          "speaker_id": "1",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025393.1.6",
          "form": "자몽은너무 시그럽더라구요",
          "original_form": "자몽은너무 시그럽더라구요",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025393.1.7",
          "form": "커피는 안좋아하세요",
          "original_form": "커피는 안좋아하세요",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025393.1.8",
          "form": "좋아요 하는데",
          "original_form": "좋아요 하는데",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025393.1.9",
          "form": "즐겨먹는편은아니예요",
          "original_form": "즐겨먹는편은아니예요",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025393.1.10",
          "form": "저는 달달한 카라멜 먹었었는데 요즘 아메리카노 좋아하네요",
          "original_form": "저는 달달한 카라멜 먹었었는데 요즘 아메리카노 좋아하네요",
          "speaker_id": "1",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100025393.1.11",
          "form": "배달음식은 주로 뭐시켜드세요",
          "original_form": "배달음식은 주로 뭐시켜드세요",
          "speaker_id": "1",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100025393.1.12",
          "form": "저는 집밥을 주로먹어서 안시켜먹는데 구지 먹을때는",
          "original_form": "저는 집밥을 주로먹어서 안시켜먹는데 구지 먹을때는",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025393.1.13",
          "form": "치킨이나 떡볶이??이런거 시켜요",
          "original_form": "치킨이나 떡볶이??이런거 시켜요",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025393.1.14",
          "form": "저도 차돌 떡볶이 자주 시켜먹어요",
          "original_form": "저도 차돌 떡볶이 자주 시켜먹어요",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025393.1.15",
          "form": "배달음식 고르는 것도 일이에요",
          "original_form": "배달음식 고르는 것도 일이에요",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025393.1.16",
          "form": "헐 저오늘 차돌떡볶이 먹으러 가요",
          "original_form": "헐 저오늘 차돌떡볶이 먹으러 가요",
          "speaker_id": "2",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025393.1.17",
          "form": "어떠케 제마음을 아시고...",
          "original_form": "어떠케 제마음을 아시고...",
          "speaker_id": "2",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025393.1.18",
          "form": "역시 맛있는 메뉴",
          "original_form": "역시 맛있는 메뉴",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025393.1.19",
          "form": "튀김 필수지요?",
          "original_form": "튀김 필수지요?",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025393.1.20",
          "form": "맞어요 갈릭 감튀가 최고예요...ㅎ",
          "original_form": "맞어요 갈릭 감튀가 최고예요...ㅎ",
          "speaker_id": "2",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025393.1.21",
          "form": "맞어요 저도 메뉴는정해노코",
          "original_form": "맞어요 저도 메뉴는정해노코",
          "speaker_id": "2",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025393.1.22",
          "form": "어디시킬지가 어렵더라구요 ㅠㅠ",
          "original_form": "어디시킬지가 어렵더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:53"
        }
      ]
