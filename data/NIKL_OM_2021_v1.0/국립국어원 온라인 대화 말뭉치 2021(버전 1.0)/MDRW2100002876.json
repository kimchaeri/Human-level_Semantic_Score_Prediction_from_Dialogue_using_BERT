      "utterance": [
        {
          "id": "MDRW2100002876.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002876.1.2",
          "form": "네 저는 플스4 있어요ㅎㅎ",
          "original_form": "네 저는 플스4 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002876.1.3",
          "form": "저는 오빠랑 플스2 해봤었는데~",
          "original_form": "저는 오빠랑 플스2 해봤었는데~",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002876.1.4",
          "form": "모바일게임도하시나요?",
          "original_form": "모바일게임도하시나요?",
          "speaker_id": "1",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002876.1.5",
          "form": "네 저는 클래시 오브 클랜 좀 했었어요",
          "original_form": "네 저는 클래시 오브 클랜 좀 했었어요",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002876.1.6",
          "form": "아하 그거 인기 많던데",
          "original_form": "아하 그거 인기 많던데",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002876.1.7",
          "form": "도미네이션즈 아시나요?",
          "original_form": "도미네이션즈 아시나요?",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002876.1.8",
          "form": "처음 들어봤어요. 어떤 게임인가요?",
          "original_form": "처음 들어봤어요. 어떤 게임인가요?",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002876.1.9",
          "form": "클래시 오브 클랜은 안해봤는데",
          "original_form": "클래시 오브 클랜은 안해봤는데",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002876.1.10",
          "form": "그게임이랑 비슷한걸로 알고있어요~",
          "original_form": "그게임이랑 비슷한걸로 알고있어요~",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002876.1.11",
          "form": "새로 나온 게임인가요?",
          "original_form": "새로 나온 게임인가요?",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002876.1.12",
          "form": "나온지 한 6년 됐는데 제가 오래 하고 있어요!",
          "original_form": "나온지 한 6년 됐는데 제가 오래 하고 있어요!",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002876.1.13",
          "form": "와 그럼 랭커시겠어요ㅋㅋ",
          "original_form": "와 그럼 랭커시겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002876.1.14",
          "form": "아뇨 랭커는 아니예요 제가 현질을 안해서 랭커까지는.. 레벨은 높지만",
          "original_form": "아뇨 랭커는 아니예요 제가 현질을 안해서 랭커까지는.. 레벨은 높지만",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002876.1.15",
          "form": "컴퓨터게임도 하시나요?",
          "original_form": "컴퓨터게임도 하시나요?",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002876.1.16",
          "form": "결국 랭커는 현질이군요",
          "original_form": "결국 랭커는 현질이군요",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002876.1.17",
          "form": "롤 좋아해요!",
          "original_form": "롤 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002876.1.18",
          "form": "아하 롤은 안해봤네요",
          "original_form": "아하 롤은 안해봤네요",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002876.1.19",
          "form": "컴퓨터 게임 하세요?",
          "original_form": "컴퓨터 게임 하세요?",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002876.1.20",
          "form": "스마트폰 이전까지는 많이 한 편인데 스마트폰 생긴 이후로는 모바일 게임 위주로 해요~",
          "original_form": "스마트폰 이전까지는 많이 한 편인데 스마트폰 생긴 이후로는 모바일 게임 위주로 해요~",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002876.1.21",
          "form": "아 좋은 폰 사셨나보다",
          "original_form": "아 좋은 폰 사셨나보다",
          "speaker_id": "2",
          "time": "20210820 16:23"
        }
