      "utterance": [
        {
          "id": "MDRW2100000605.1.1",
          "form": "안녕하세여 이상형이 어떻게 되시나요?",
          "original_form": "안녕하세여 이상형이 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000605.1.2",
          "form": "다정하고 성실하고 남자다운사람이요ㅋㅋ",
          "original_form": "다정하고 성실하고 남자다운사람이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000605.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000605.1.4",
          "form": "저랑 비슷하시네요 저도 자기주관 또렷하고 말보단 행동으로 보여주는 믿음직한 사람이요 ㅎㅎ",
          "original_form": "저랑 비슷하시네요 저도 자기주관 또렷하고 말보단 행동으로 보여주는 믿음직한 사람이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000605.1.5",
          "form": "다정하구요!!",
          "original_form": "다정하구요!!",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000605.1.6",
          "form": "name1님은 20대신가요?",
          "original_form": "&name1&님은 20대신가요?",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000605.1.7",
          "form": "미팅같은거 해보셨어요?",
          "original_form": "미팅같은거 해보셨어요?",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000605.1.8",
          "form": "저는 30대예요!",
          "original_form": "저는 30대예요!",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000605.1.9",
          "form": "미팅 예전에 해봤는데 저런 사람은 없더라구요ㅠㅠ",
          "original_form": "미팅 예전에 해봤는데 저런 사람은 없더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000605.1.10",
          "form": "아아! 전 대학교에서 미팅해본 적은 없구 소개팅해서 만났어요ㅎ",
          "original_form": "아아! 전 대학교에서 미팅해본 적은 없구 소개팅해서 만났어요ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000605.1.11",
          "form": "확실히 이상형에 맞는 사람 만나기가 어렵죠",
          "original_form": "확실히 이상형에 맞는 사람 만나기가 어렵죠",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000605.1.12",
          "form": "소개팅에서 괜찮은 분 만나셨나요??",
          "original_form": "소개팅에서 괜찮은 분 만나셨나요??",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000605.1.13",
          "form": "네ㅋㅋ3번 실패 후 지금 잘 만나고있어요ㅎ",
          "original_form": "네ㅋㅋ3번 실패 후 지금 잘 만나고있어요ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000605.1.14",
          "form": "저도 소개팅 몇번 해봤는데 운이 없었나봐요",
          "original_form": "저도 소개팅 몇번 해봤는데 운이 없었나봐요",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000605.1.15",
          "form": "ㅜㅠ 좋은 분 곧 나타나실거에요",
          "original_form": "ㅜㅠ 좋은 분 곧 나타나실거에요",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000605.1.16",
          "form": "지금만나는분은 이상형에 가까우신가요? 행운이네요ㅠㅠ 부러우ㅓ요",
          "original_form": "지금만나는분은 이상형에 가까우신가요? 행운이네요ㅠㅠ 부러우ㅓ요",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000605.1.17",
          "form": "이상형에 가까우면서도...음...안맞는 것도 있구요ㅋㅋㅋㅋㅋ",
          "original_form": "이상형에 가까우면서도...음...안맞는 것도 있구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000605.1.18",
          "form": "그래도 만날 운명이었나봐요 저도 얼른 그런사람이 오기를..",
          "original_form": "그래도 만날 운명이었나봐요 저도 얼른 그런사람이 오기를..",
