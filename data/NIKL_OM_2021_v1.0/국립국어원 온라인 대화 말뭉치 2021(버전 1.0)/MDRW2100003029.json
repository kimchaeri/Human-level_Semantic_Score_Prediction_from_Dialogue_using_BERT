      "utterance": [
        {
          "id": "MDRW2100003029.1.1",
          "form": "퇴근하셨습니까?ㅋ",
          "original_form": "퇴근하셨습니까?ㅋ",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003029.1.2",
          "form": "전 퇴근이 없네요",
          "original_form": "전 퇴근이 없네요",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003029.1.3",
          "form": "주부 입니다 ㅎㅎ",
          "original_form": "주부 입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003029.1.4",
          "form": "으아 업무스트레스가 굉장하시겠어요ㅎㅎ",
          "original_form": "으아 업무스트레스가 굉장하시겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003029.1.5",
          "form": "꼭 그렇진 않아요",
          "original_form": "꼭 그렇진 않아요",
          "speaker_id": "2",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003029.1.6",
          "form": "집에서 소소하게 주식해서 벌어요",
          "original_form": "집에서 소소하게 주식해서 벌어요",
          "speaker_id": "2",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003029.1.7",
          "form": "와 저도 주식하는데",
          "original_form": "와 저도 주식하는데",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003029.1.8",
          "form": "요즘 장이 안 좋아서ㅋ",
          "original_form": "요즘 장이 안 좋아서ㅋ",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003029.1.9",
          "form": "저도 완전 말아먹고 공모주만 해요",
          "original_form": "저도 완전 말아먹고 공모주만 해요",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003029.1.10",
          "form": "아직 정리 안한것도 있어요",
          "original_form": "아직 정리 안한것도 있어요",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003029.1.11",
          "form": "ㅋㅋㅋ 주식이 좋은 알바이긴 한데",
          "original_form": "ㅋㅋㅋ 주식이 좋은 알바이긴 한데",
          "speaker_id": "1",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003029.1.12",
          "form": "공모주는 안해봤네요",
          "original_form": "공모주는 안해봤네요",
          "speaker_id": "1",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003029.1.13",
          "form": "넘소소해요",
          "original_form": "넘소소해요",
          "speaker_id": "2",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003029.1.14",
          "form": "저는 삼성 떨어지길래 괜히 들어갔다가",
          "original_form": "저는 삼성 떨어지길래 괜히 들어갔다가",
          "speaker_id": "1",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003029.1.15",
          "form": "큰돈 잃으니 소소하게 해요",
          "original_form": "큰돈 잃으니 소소하게 해요",
          "speaker_id": "2",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003029.1.16",
          "form": "오늘도 소소하게 수입이 생겼네요",
          "original_form": "오늘도 소소하게 수입이 생겼네요",
          "speaker_id": "2",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003029.1.17",
          "form": "어떤일 하세요?",
          "original_form": "어떤일 하세요?",
          "speaker_id": "2",
          "time": "20210820 17:47"
        },
        {
          "id": "MDRW2100003029.1.18",
          "form": "ㅎㅎ 저는 일반 회사원이에요",
          "original_form": "ㅎㅎ 저는 일반 회사원이에요",
          "speaker_id": "1",
          "time": "20210820 17:47"
        },
        {
          "id": "MDRW2100003029.1.19",
          "form": "요샌 회사원이 젤좋아요",
          "original_form": "요샌 회사원이 젤좋아요",
          "speaker_id": "2",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003029.1.20",
          "form": "주는 월급 받는게 젤좋아요",
          "original_form": "주는 월급 받는게 젤좋아요",
          "speaker_id": "2",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003029.1.21",
          "form": "ㅎㅎ 그렇다고는 하는데",
          "original_form": "ㅎㅎ 그렇다고는 하는데",
          "speaker_id": "1",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003029.1.22",
          "form": "코로나에 위기 극복하고 더 잘되는 사람도 있더라그요",
          "original_form": "코로나에 위기 극복하고 더 잘되는 사람도 있더라그요",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003029.1.23",
          "form": "저희집도 남편이 자영업하다  회사가니 매달 일정해서 좋은거 같아요",
          "original_form": "저희집도 남편이 자영업하다  회사가니 매달 일정해서 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003029.1.24",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003029.1.25",
          "form": "의외의 곳에서 대박나고",
          "original_form": "의외의 곳에서 대박나고",
          "speaker_id": "2",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003029.1.26",
          "form": "자영업 하다 취직하시기 힘드셨을텐데",
          "original_form": "자영업 하다 취직하시기 힘드셨을텐데",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003029.1.27",
          "form": "큰 결심 하셨네요",
          "original_form": "큰 결심 하셨네요",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003029.1.28",
          "form": "네 정말 폐업해서",
          "original_form": "네 정말 폐업해서",
          "speaker_id": "2",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003029.1.29",
          "form": "ㅠㅠ 속상하셨겠어요",
          "original_form": "ㅠㅠ 속상하셨겠어요",
          "speaker_id": "1",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003029.1.30",
          "form": "어떨수 없이 했는데 월급 걱정안해돞되고 그냥 좋아요",
          "original_form": "어떨수 없이 했는데 월급 걱정안해돞되고 그냥 좋아요",
          "speaker_id": "2",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003029.1.31",
          "form": "맘비웠어요",
          "original_form": "맘비웠어요",
          "speaker_id": "2",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003029.1.32",
          "form": "월급인상만 노리고있어요 ㅋㅋ",
          "original_form": "월급인상만 노리고있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:51"
        },
        {
          "id": "MDRW2100003029.1.33",
          "form": "요즘엔 폐업도 힘들다는 얘기가 있던데",
          "original_form": "요즘엔 폐업도 힘들다는 얘기가 있던데",
          "speaker_id": "1",
          "time": "20210820 17:51"
        },
        {
          "id": "MDRW2100003029.1.34",
          "form": "힘든 상황은 넘기셨겠어요",
          "original_form": "힘든 상황은 넘기셨겠어요",
          "speaker_id": "1",
          "time": "20210820 17:51"
        },
        {
          "id": "MDRW2100003029.1.35",
          "form": "네 정리할 세금이 폭탄으로 날라와요",
          "original_form": "네 정리할 세금이 폭탄으로 날라와요",
          "speaker_id": "2",
          "time": "20210820 17:51"
        },
        {
          "id": "MDRW2100003029.1.36",
          "form": "이젠 연말정산 폭탄이 오네요",
          "original_form": "이젠 연말정산 폭탄이 오네요",
          "speaker_id": "2",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003029.1.37",
          "form": "집기는 죄다 버리고 헐값에 넘기고 집에도 사무용품 있어요",
          "original_form": "집기는 죄다 버리고 헐값에 넘기고 집에도 사무용품 있어요",
          "speaker_id": "2",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003029.1.38",
          "form": "세금과의 싸움이네요ㅠ",
          "original_form": "세금과의 싸움이네요ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:53"
        }
      ]
    }
