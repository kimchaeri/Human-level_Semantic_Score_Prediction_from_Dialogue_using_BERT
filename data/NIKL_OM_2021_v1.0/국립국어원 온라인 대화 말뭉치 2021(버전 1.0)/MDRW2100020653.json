      "utterance": [
        {
          "id": "MDRW2100020653.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ아녜요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아녜요",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020653.1.2",
          "form": "대박이고",
          "original_form": "대박이고",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020653.1.3",
          "form": "박찬열다리도 대박이고ㅋㅋㅋㅋㅋ",
          "original_form": "박찬열다리도 대박이고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 ...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 ...",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.5",
          "form": "겉모습만보고 판단해서는 안되나봐요",
          "original_form": "겉모습만보고 판단해서는 안되나봐요",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.6",
          "form": "와 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.7",
          "form": "돈도 잘버면서 왜 그럴까요?ㅠㅠ",
          "original_form": "돈도 잘버면서 왜 그럴까요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.8",
          "form": "박찬열명 ...",
          "original_form": "박찬열명 ...",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.9",
          "form": "돈 잘버니까 그런듯요 .... 휴",
          "original_form": "돈 잘버니까 그런듯요 .... 휴",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.10",
          "form": "새벽에 뜬거 보셨어요?",
          "original_form": "새벽에 뜬거 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.11",
          "form": "팬들은 대체 뭔 죄인지 ㅠ ㅠ",
          "original_form": "팬들은 대체 뭔 죄인지 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.12",
          "form": "헐 아뇨",
          "original_form": "헐 아뇨",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.13",
          "form": "또 떴어요 ??",
          "original_form": "또 떴어요 ??",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.14",
          "form": "고양이 윈윈한테 버리듯이 떠넘기고 튀었어요",
          "original_form": "고양이 윈윈한테 버리듯이 떠넘기고 튀었어요",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.16",
          "form": "아 너무 싫어요",
          "original_form": "아 너무 싫어요",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020653.1.17",
          "form": "중국 펫샵에서 고양이 분양받아가지고",
          "original_form": "중국 펫샵에서 고양이 분양받아가지고",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.18",
          "form": "그걸로 이성한테 환심사고",
          "original_form": "그걸로 이성한테 환심사고",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.19",
          "form": "아 진짜 .. 인류애 상실이네요",
          "original_form": "아 진짜 .. 인류애 상실이네요",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.20",
          "form": "필요없어지니까윈윈한테",
          "original_form": "필요없어지니까윈윈한테",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.21",
          "form": "미친게 아닐까요 ...",
          "original_form": "미친게 아닐까요 ...",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.22",
          "form": "넘기고 한국으로 튀었다고ㅠㅠ",
          "original_form": "넘기고 한국으로 튀었다고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.23",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.24",
          "form": "와 입이 안다물어지네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 입이 안다물어지네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.25",
          "form": "근데 댓글중에",
          "original_form": "근데 댓글중에",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.26",
          "form": "와 그 많은 폭로중에 유일하게 돈썼네",
          "original_form": "와 그 많은 폭로중에 유일하게 돈썼네",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.27",
          "form": "이 댓글보고",
          "original_form": "이 댓글보고",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.28",
          "form": "웃안웃이었어요ㅋㅋㅋㅋ",
          "original_form": "웃안웃이었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020653.1.29",
          "form": "웃긴데 안웃겨... 근데 웃겨...",
          "original_form": "웃긴데 안웃겨... 근데 웃겨...",
          "speaker_id": "1",
          "time": "20210828 10:52"
        }
      ]
