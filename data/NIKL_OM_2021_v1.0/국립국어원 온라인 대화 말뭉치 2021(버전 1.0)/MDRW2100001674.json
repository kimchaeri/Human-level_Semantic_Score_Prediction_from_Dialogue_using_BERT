      "utterance": [
        {
          "id": "MDRW2100001674.1.1",
          "form": "요즘 운동하는거있나",
          "original_form": "요즘 운동하는거있나",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001674.1.2",
          "form": "요새 링피트하는데 너무 힘들다",
          "original_form": "요새 링피트하는데 너무 힘들다",
          "speaker_id": "2",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001674.1.3",
          "form": "운동 뭐 하나?",
          "original_form": "운동 뭐 하나?",
          "speaker_id": "2",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001674.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ링피트는 그래도 재밌다아이사",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ링피트는 그래도 재밌다아이사",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.5",
          "form": "아이가.",
          "original_form": "아이가.",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.6",
          "form": "운동...",
          "original_form": "운동...",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.7",
          "form": "매일 20분씩 걸으면 그게 운동이지",
          "original_form": "매일 20분씩 걸으면 그게 운동이지",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.8",
          "form": "그렇제?",
          "original_form": "그렇제?",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.9",
          "form": "맞지... 근데 요새 그래도 좀 덥지않나...",
          "original_form": "맞지... 근데 요새 그래도 좀 덥지않나...",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.10",
          "form": "야 나오늘 걸어오다가",
          "original_form": "야 나오늘 걸어오다가",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.11",
          "form": "그리고 밖에 나가기가 힘들더라..ㅎ",
          "original_form": "그리고 밖에 나가기가 힘들더라..ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.12",
          "form": "땀 줄줄 흘렸다",
          "original_form": "땀 줄줄 흘렸다",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.13",
          "form": "안더운줄 알았는데",
          "original_form": "안더운줄 알았는데",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001674.1.14",
          "form": "흐려서 속은거였다",
          "original_form": "흐려서 속은거였다",
          "speaker_id": "1",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100001674.1.15",
          "form": "오늘 좀 덥긴하더라",
          "original_form": "오늘 좀 덥긴하더라",
          "speaker_id": "2",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100001674.1.16",
          "form": "아직은 운동 안에서 해야겠드라...",
          "original_form": "아직은 운동 안에서 해야겠드라...",
          "speaker_id": "1",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100001674.1.17",
          "form": "맞아 흐린데 더워",
          "original_form": "맞아 흐린데 더워",
          "speaker_id": "2",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100001674.1.18",
          "form": "가을에 뭐 운동 할거가? 걷기말고.",
          "original_form": "가을에 뭐 운동 할거가? 걷기말고.",
          "speaker_id": "2",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100001674.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어어 사실 날씨는 핑계다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어어 사실 날씨는 핑계다",
          "speaker_id": "1",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100001674.1.20",
          "form": "가을도 똑같지머...",
          "original_form": "가을도 똑같지머...",
          "speaker_id": "1",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001674.1.21",
          "form": "학교안에 걸어다니면 그게 운동이다",
          "original_form": "학교안에 걸어다니면 그게 운동이다",
          "speaker_id": "1",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001674.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ좀 고되긴하다 캠퍼스 걷기",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ좀 고되긴하다 캠퍼스 걷기",
          "speaker_id": "2",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001674.1.23",
          "form": "나는 필테 배워보고싶다",
          "original_form": "나는 필테 배워보고싶다",
          "speaker_id": "2",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001674.1.24",
          "form": "필테 그거 너무 비싸다",
          "original_form": "필테 그거 너무 비싸다",
          "speaker_id": "1",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100001674.1.25",
          "form": "나 머리 자를까말까 넘 고민됨",
          "original_form": "나 머리 자를까말까 넘 고민됨",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100001674.1.26",
          "form": "머리 자르고 싶은 날은 꼭 머리스타일이 괜찮아",
          "original_form": "머리 자르고 싶은 날은 꼭 머리스타일이 괜찮아",
          "speaker_id": "2",
          "time": "20210819 20:31"
        }
      ]
    }
  ]
