      "utterance": [
        {
          "id": "MDRW2100046201.1.1",
          "form": "쭈꾸미 땡기는데요 ㅎ",
          "original_form": "쭈꾸미 땡기는데요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046201.1.2",
          "form": "쭈꾸미 맛나죠",
          "original_form": "쭈꾸미 맛나죠",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046201.1.3",
          "form": "무슨 유명한게 있던데요",
          "original_form": "무슨 유명한게 있던데요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046201.1.4",
          "form": "직화라 불맛도 나요",
          "original_form": "직화라 불맛도 나요",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046201.1.5",
          "form": "쭈꾸미도",
          "original_form": "쭈꾸미도",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046201.1.6",
          "form": "하남쭈꾸미요",
          "original_form": "하남쭈꾸미요",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046201.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046201.1.8",
          "form": "팩사서 먹는데 맛있어요",
          "original_form": "팩사서 먹는데 맛있어요",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046201.1.9",
          "form": "그거 맛있다고 하더라구요",
          "original_form": "그거 맛있다고 하더라구요",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046201.1.10",
          "form": "팩추천해주시더라구요",
          "original_form": "팩추천해주시더라구요",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046201.1.11",
          "form": "콩나물넣고 무쌈이랑",
          "original_form": "콩나물넣고 무쌈이랑",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046201.1.12",
          "form": "추천해요",
          "original_form": "추천해요",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046201.1.13",
          "form": "얼마정도해요?",
          "original_form": "얼마정도해요?",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046201.1.14",
          "form": "오늘은 이사님이 어디가셔야돼서 간단한거 먹어야 될듯",
          "original_form": "오늘은 이사님이 어디가셔야돼서 간단한거 먹어야 될듯",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046201.1.15",
          "form": "요즘비싸요 9천원인가",
          "original_form": "요즘비싸요 9천원인가",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046201.1.16",
          "form": "비싸다고 하긴하더라구요",
          "original_form": "비싸다고 하긴하더라구요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046201.1.17",
          "form": "그래도 맛있다면야 ㅎ",
          "original_form": "그래도 맛있다면야 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046201.1.18",
          "form": "맵지는않아요?",
          "original_form": "맵지는않아요?",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046201.1.19",
          "form": "쿠폰줄때 사면 괜찮은듯요",
          "original_form": "쿠폰줄때 사면 괜찮은듯요",
          "speaker_id": "1",
          "time": "20210908 11:15"
        }
