        {
          "id": "MDRW2100014904.1.1",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014904.1.2",
          "form": "닭갈비요^^",
          "original_form": "닭갈비요^^",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014904.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014904.1.4",
          "form": "볶음밥은 볶으셨나요?",
          "original_form": "볶음밥은 볶으셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014904.1.5",
          "form": "당연하죠ㅎㅎ",
          "original_form": "당연하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014904.1.6",
          "form": "^^볶음밥은 상식이죠",
          "original_form": "^^볶음밥은 상식이죠",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014904.1.7",
          "form": "저는 name1 먹었어요",
          "original_form": "저는 &name1& 먹었어요",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014904.1.8",
          "form": "앗 이름값을 하셨네요",
          "original_form": "앗 이름값을 하셨네요",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014904.1.9",
          "form": "또 무슨음식좋아하세요",
          "original_form": "또 무슨음식좋아하세요",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014904.1.10",
          "form": "저 빵순이예요 ㅋㅋ",
          "original_form": "저 빵순이예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014904.1.11",
          "form": "밀가루 러버입니다",
          "original_form": "밀가루 러버입니다",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014904.1.12",
          "form": "ㅋㅋㅋㅋㅋ빵 최고...인정입니다",
          "original_form": "ㅋㅋㅋㅋㅋ빵 최고...인정입니다",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014904.1.13",
          "form": "질리지가않아요",
          "original_form": "질리지가않아요",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014904.1.14",
          "form": "주식이 빵이었다면 달랐을까요?",
          "original_form": "주식이 빵이었다면 달랐을까요?",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014904.1.15",
          "form": "전 밥없인 살아도 빵없인 안 될것같아요",
          "original_form": "전 밥없인 살아도 빵없인 안 될것같아요",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014904.1.16",
          "form": "다르지 않았을거에요..ㅋㅋㅋㅋ",
          "original_form": "다르지 않았을거에요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014904.1.17",
          "form": "맛있는건 언제먹어도 맛있는법",
          "original_form": "맛있는건 언제먹어도 맛있는법",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014904.1.18",
          "form": "식사빵 잘라다가 과카몰리 싹싹 얹어먹고 싶네요",
          "original_form": "식사빵 잘라다가 과카몰리 싹싹 얹어먹고 싶네요",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014904.1.19",
          "form": "ㅋㅋㅋ아직 배가 덜 차신거같은데여",
          "original_form": "ㅋㅋㅋ아직 배가 덜 차신거같은데여",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014904.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ 배불러도 빵 배는 따로 있어서요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 배불러도 빵 배는 따로 있어서요",
