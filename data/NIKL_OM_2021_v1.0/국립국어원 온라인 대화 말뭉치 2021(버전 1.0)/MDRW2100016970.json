      "utterance": [
        {
          "id": "MDRW2100016970.1.1",
          "form": "아 실화기반이에요",
          "original_form": "아 실화기반이에요",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016970.1.2",
          "form": "조승우 나오죠?",
          "original_form": "조승우 나오죠?",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016970.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016970.1.4",
          "form": "시즌1까지 방송했나요?",
          "original_form": "시즌1까지 방송했나요?",
          "speaker_id": "2",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016970.1.5",
          "form": "시즌1은 실화가 많이 들어간 걸로 알고 있어요",
          "original_form": "시즌1은 실화가 많이 들어간 걸로 알고 있어요",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016970.1.6",
          "form": "2까지요",
          "original_form": "2까지요",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016970.1.7",
          "form": "아 3도 나오나 봐요",
          "original_form": "아 3도 나오나 봐요",
          "speaker_id": "2",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016970.1.8",
          "form": "현실은 더 하겠지만어ㅗ",
          "original_form": "현실은 더 하겠지만어ㅗ",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016970.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016970.1.10",
          "form": "무서운내용인가요??",
          "original_form": "무서운내용인가요??",
          "speaker_id": "2",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016970.1.11",
          "form": "검경비리를 주로 다루고 있으니까",
          "original_form": "검경비리를 주로 다루고 있으니까",
          "speaker_id": "1",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016970.1.12",
          "form": "성매매 관련 실태도 나오고요",
          "original_form": "성매매 관련 실태도 나오고요",
          "speaker_id": "1",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016970.1.13",
          "form": "무섭다기보단",
          "original_form": "무섭다기보단",
          "speaker_id": "1",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016970.1.14",
          "form": "아 ㅜ 어떤내용인지 상상이좀되네요",
          "original_form": "아 ㅜ 어떤내용인지 상상이좀되네요",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016970.1.15",
          "form": "남자로 태어나지 않아서 아주 더러운 기분이예요",
          "original_form": "남자로 태어나지 않아서 아주 더러운 기분이예요",
          "speaker_id": "1",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016970.1.16",
          "form": "현실은 더할거에요 ㅠ",
          "original_form": "현실은 더할거에요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016970.1.17",
          "form": "맞아요 ㅜ",
          "original_form": "맞아요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016970.1.18",
          "form": "그런데 늘 다뤄온 시선이 아니라서",
          "original_form": "그런데 늘 다뤄온 시선이 아니라서",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016970.1.19",
          "form": "주연도 배두나이고",
          "original_form": "주연도 배두나이고",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016970.1.20",
          "form": "아 궁금하네요 찜해놓았어요 ㅎ",
          "original_form": "아 궁금하네요 찜해놓았어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016970.1.21",
          "form": "알탕 느낌은 뒤로 빠져있어요.",
          "original_form": "알탕 느낌은 뒤로 빠져있어요.",
          "speaker_id": "1",
          "time": "20210826 16:39"
        }
      ]
