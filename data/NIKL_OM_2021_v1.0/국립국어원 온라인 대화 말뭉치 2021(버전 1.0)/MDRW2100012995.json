      "utterance": [
        {
          "id": "MDRW2100012995.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 10:12"
        },
        {
          "id": "MDRW2100012995.1.2",
          "form": "안녕하세요 이번 학기에 학교 다니시나요?",
          "original_form": "안녕하세요 이번 학기에 학교 다니시나요?",
          "speaker_id": "2",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100012995.1.3",
          "form": "네 다닙니다",
          "original_form": "네 다닙니다",
          "speaker_id": "1",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100012995.1.4",
          "form": "이번 수강신청은 어떠셨나요?",
          "original_form": "이번 수강신청은 어떠셨나요?",
          "speaker_id": "2",
          "time": "20210825 12:02"
        },
        {
          "id": "MDRW2100012995.1.5",
          "form": "음 수월했습니다ㅎㅎ",
          "original_form": "음 수월했습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100012995.1.6",
          "form": "정말 다행이에요!",
          "original_form": "정말 다행이에요!",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100012995.1.7",
          "form": "저는 수강신청을 망쳐서 시간표를 다시 짜느라 공강이 없거든요",
          "original_form": "저는 수강신청을 망쳐서 시간표를 다시 짜느라 공강이 없거든요",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100012995.1.8",
          "form": "공강은 있으신가요?",
          "original_form": "공강은 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100012995.1.9",
          "form": "네네 있어요 담학기부터 대면하시나요?",
          "original_form": "네네 있어요 담학기부터 대면하시나요?",
          "speaker_id": "1",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100012995.1.10",
          "form": "지금은 코로나 단계가 올라가서 대면을 못한다고 공지가 왔는데",
          "original_form": "지금은 코로나 단계가 올라가서 대면을 못한다고 공지가 왔는데",
          "speaker_id": "2",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100012995.1.11",
          "form": "단계가 내려가면 대면으로 전환될 것 같아요!",
          "original_form": "단계가 내려가면 대면으로 전환될 것 같아요!",
          "speaker_id": "2",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100012995.1.12",
          "form": "참 코로나땜에 엠티도못가고ㅠ",
          "original_form": "참 코로나땜에 엠티도못가고ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100012995.1.13",
          "form": "나중에 코로나가 끝나면 엠티를 한 번 연다는 학교도 있는걸 보니",
          "original_form": "나중에 코로나가 끝나면 엠티를 한 번 연다는 학교도 있는걸 보니",
          "speaker_id": "2",
          "time": "20210825 12:34"
        },
        {
          "id": "MDRW2100012995.1.14",
          "form": "나중에 학교 엠티 기다려보셔도 될 것 같아요!",
          "original_form": "나중에 학교 엠티 기다려보셔도 될 것 같아요!",
          "speaker_id": "2",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100012995.1.15",
          "form": "물론 그 전에 수업과 시험을 해야겠지만요 ㅠㅠ",
          "original_form": "물론 그 전에 수업과 시험을 해야겠지만요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100012995.1.16",
          "form": "코로나가 언제끝날지ㅠ",
          "original_form": "코로나가 언제끝날지ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100012995.1.17",
          "form": "얼른 끝나서 동아리 활동을 했으면 좋겠네요~",
          "original_form": "얼른 끝나서 동아리 활동을 했으면 좋겠네요~",
          "speaker_id": "2",
          "time": "20210825 12:45"
        }
      ]
