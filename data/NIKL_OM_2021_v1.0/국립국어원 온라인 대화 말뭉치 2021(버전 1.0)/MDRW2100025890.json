      "utterance": [
        {
          "id": "MDRW2100025890.1.1",
          "form": "엄마 나 머리 자른 거 어때",
          "original_form": "엄마 나 머리 자른 거 어때",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100025890.1.2",
          "form": "더 잘라야지 티도 안나",
          "original_form": "더 잘라야지 티도 안나",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025890.1.3",
          "form": "왜 처음 생각보다 많이 잘랐는데!",
          "original_form": "왜 처음 생각보다 많이 잘랐는데!",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025890.1.4",
          "form": "맞아 그렇긴해",
          "original_form": "맞아 그렇긴해",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025890.1.5",
          "form": "그래도 더 잘랐음 좋겠다",
          "original_form": "그래도 더 잘랐음 좋겠다",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025890.1.6",
          "form": "왜 난 머리도 별로 안 빠지는데",
          "original_form": "왜 난 머리도 별로 안 빠지는데",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025890.1.7",
          "form": "너는 몇가닥 안 빠져도 머리가 길어서 많이 빠져보여",
          "original_form": "너는 몇가닥 안 빠져도 머리가 길어서 많이 빠져보여",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025890.1.8",
          "form": "헐 그렇게 우기기있음?",
          "original_form": "헐 그렇게 우기기있음?",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025890.1.9",
          "form": "너무한다 진짜",
          "original_form": "너무한다 진짜",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025890.1.10",
          "form": "시원하게 확 짤라",
          "original_form": "시원하게 확 짤라",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025890.1.11",
          "form": "어깨정도로",
          "original_form": "어깨정도로",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025890.1.12",
          "form": "안돼 기르기 귀찮아서",
          "original_form": "안돼 기르기 귀찮아서",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025890.1.13",
          "form": "엄청 힘들다구",
          "original_form": "엄청 힘들다구",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025890.1.14",
          "form": "아니야 짧아야 이쁘다",
          "original_form": "아니야 짧아야 이쁘다",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025890.1.15",
          "form": "뭐 맨날 자르래ㅡㅡ",
          "original_form": "뭐 맨날 자르래ㅡㅡ",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025890.1.16",
          "form": "니 동생 봐라 얼마나 시원하냐",
          "original_form": "니 동생 봐라 얼마나 시원하냐",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025890.1.17",
          "form": "쟤 저렇게 자르고 엄청 산발되잖아",
          "original_form": "쟤 저렇게 자르고 엄청 산발되잖아",
