      "utterance": [
        {
          "id": "MDRW2100029468.1.1",
          "form": "저도 빵집 투에 꽤 다녔어요",
          "original_form": "저도 빵집 투에 꽤 다녔어요",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100029468.1.2",
          "form": "이성당 가겠다고 새벽에 출발하고ㅋㅋ",
          "original_form": "이성당 가겠다고 새벽에 출발하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100029468.1.3",
          "form": "오~빵사랑 짱!",
          "original_form": "오~빵사랑 짱!",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029468.1.4",
          "form": "코로나끝나면 또 해야죠",
          "original_form": "코로나끝나면 또 해야죠",
          "speaker_id": "2",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029468.1.5",
          "form": "맞아요 끝나면 다시 달려갑니다",
          "original_form": "맞아요 끝나면 다시 달려갑니다",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100029468.1.6",
          "form": "가끔 추천도 받을수 있었으면 좋겠네요^^",
          "original_form": "가끔 추천도 받을수 있었으면 좋겠네요^^",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029468.1.7",
          "form": "혹시 서울권 사시고 단거 좋아하시면",
          "original_form": "혹시 서울권 사시고 단거 좋아하시면",
          "speaker_id": "1",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029468.1.8",
          "form": "꽈페라구 꽈배기 파는 집이 있는데",
          "original_form": "꽈페라구 꽈배기 파는 집이 있는데",
          "speaker_id": "1",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029468.1.9",
          "form": "거기 얼그레이 꽈배기 추천해요",
          "original_form": "거기 얼그레이 꽈배기 추천해요",
          "speaker_id": "1",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029468.1.10",
          "form": "여긴 이성당이 더 가깝죠.^^",
          "original_form": "여긴 이성당이 더 가깝죠.^^",
          "speaker_id": "2",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100029468.1.11",
          "form": "서울가게되면 거기도 가볼께요.",
          "original_form": "서울가게되면 거기도 가볼께요.",
          "speaker_id": "2",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100029468.1.12",
          "form": "이성당이 가까우면 좋으시겠어요",
          "original_form": "이성당이 가까우면 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029468.1.13",
          "form": "거기 아침메뉴가 기가 막혀서",
          "original_form": "거기 아침메뉴가 기가 막혀서",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029468.1.14",
          "form": "음..가깝다고는 했으나 상상하시는것보다는 멀어서 그 기만힌 아침메뉴를 구경할수가 없네요.ㅠ",
          "original_form": "음..가깝다고는 했으나 상상하시는것보다는 멀어서 그 기만힌 아침메뉴를 구경할수가 없네요.ㅠ",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029468.1.15",
          "form": "ㅠ 아쉽네요",
          "original_form": "ㅠ 아쉽네요",
          "speaker_id": "1",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029468.1.16",
          "form": "많이요..",
          "original_form": "많이요..",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029468.1.17",
          "form": "주변에 빵 맛집 없으신가요",
          "original_form": "주변에 빵 맛집 없으신가요",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029468.1.18",
          "form": "몇군데 있긴 해요.^^",
          "original_form": "몇군데 있긴 해요.^^",
          "speaker_id": "2",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029468.1.19",
          "form": "어떤 빵 제일 좋아하세요?",
          "original_form": "어떤 빵 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029468.1.20",
          "form": "옛날사람인가 꽈배기랑 도넛쪽을 선호해요",
          "original_form": "옛날사람인가 꽈배기랑 도넛쪽을 선호해요",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029468.1.21",
          "form": "그 다음엔 밤식빵",
          "original_form": "그 다음엔 밤식빵",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029468.1.22",
          "form": "저도 꽈배기랑 도넛 엄청 좋아해요",
          "original_form": "저도 꽈배기랑 도넛 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029468.1.23",
          "form": "가끔 시장 가서 보던 도넛이 그렇게 좋았는데",
          "original_form": "가끔 시장 가서 보던 도넛이 그렇게 좋았는데",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029468.1.24",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029468.1.25",
          "form": "어릴때 시장에서 먹었던 그맛을 잊을수가없어요",
          "original_form": "어릴때 시장에서 먹었던 그맛을 잊을수가없어요",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029468.1.26",
          "form": "아무리 맛집이라도 그 맛은 못따라가는거 같아요",
          "original_form": "아무리 맛집이라도 그 맛은 못따라가는거 같아요",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029468.1.27",
          "form": "글쵸?",
          "original_form": "글쵸?",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029468.1.28",
          "form": "갑자기 배고파지네요",
          "original_form": "갑자기 배고파지네요",
          "speaker_id": "2",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029468.1.29",
