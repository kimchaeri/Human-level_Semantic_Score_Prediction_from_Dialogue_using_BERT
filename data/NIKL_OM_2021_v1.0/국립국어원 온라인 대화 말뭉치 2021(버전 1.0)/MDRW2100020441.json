      "utterance": [
        {
          "id": "MDRW2100020441.1.1",
          "form": "네 다행히 저는 아직 그런 학부모님 못만나봤지만",
          "original_form": "네 다행히 저는 아직 그런 학부모님 못만나봤지만",
          "speaker_id": "2",
          "time": "20210828 05:51"
        },
        {
          "id": "MDRW2100020441.1.2",
          "form": "저희때랑은 또 달라서 세상이 흉흉해진것도 있지만",
          "original_form": "저희때랑은 또 달라서 세상이 흉흉해진것도 있지만",
          "speaker_id": "1",
          "time": "20210828 05:51"
        },
        {
          "id": "MDRW2100020441.1.3",
          "form": "과보호가 너무 심한것같아요",
          "original_form": "과보호가 너무 심한것같아요",
          "speaker_id": "1",
          "time": "20210828 05:51"
        },
        {
          "id": "MDRW2100020441.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 05:52"
        },
        {
          "id": "MDRW2100020441.1.5",
          "form": "지도할때 부모님땜에 힘들때도 있어요",
          "original_form": "지도할때 부모님땜에 힘들때도 있어요",
          "speaker_id": "2",
          "time": "20210828 05:52"
        },
        {
          "id": "MDRW2100020441.1.6",
          "form": "ㅠㅠ 교육계 종사하시는분들 그래서 존경해요...",
          "original_form": "ㅠㅠ 교육계 종사하시는분들 그래서 존경해요...",
          "speaker_id": "1",
          "time": "20210828 05:52"
        },
        {
          "id": "MDRW2100020441.1.7",
          "form": "완전 서비스직이에요",
          "original_form": "완전 서비스직이에요",
          "speaker_id": "1",
          "time": "20210828 05:52"
        },
        {
          "id": "MDRW2100020441.1.8",
          "form": "애들이 아무래도 예민할땐데",
          "original_form": "애들이 아무래도 예민할땐데",
          "speaker_id": "2",
          "time": "20210828 05:52"
        },
        {
          "id": "MDRW2100020441.1.9",
          "form": "부모님의 영향이 엄청 또 클때니깐 ㅜㅠ",
          "original_form": "부모님의 영향이 엄청 또 클때니깐 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 05:52"
        },
        {
          "id": "MDRW2100020441.1.10",
          "form": "애들만 보시기도 힘드신데",
          "original_form": "애들만 보시기도 힘드신데",
          "speaker_id": "1",
          "time": "20210828 05:52"
        },
        {
          "id": "MDRW2100020441.1.11",
          "form": "ㅠㅠ 저는 고객사 대하는데",
          "original_form": "ㅠㅠ 저는 고객사 대하는데",
          "speaker_id": "1",
          "time": "20210828 05:53"
        },
        {
          "id": "MDRW2100020441.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 05:53"
        },
        {
          "id": "MDRW2100020441.1.13",
          "form": "자기 회사 이름 걸고 미팅해도",
          "original_form": "자기 회사 이름 걸고 미팅해도",
          "speaker_id": "1",
          "time": "20210828 05:53"
        },
        {
          "id": "MDRW2100020441.1.14",
          "form": "갑질하거나 태도불량인 사람 많더라구요",
          "original_form": "갑질하거나 태도불량인 사람 많더라구요",
          "speaker_id": "1",
          "time": "20210828 05:53"
        },
        {
          "id": "MDRW2100020441.1.15",
          "form": "아ㅋㅋㅋ 갑질 뭐야 진짜",
          "original_form": "아ㅋㅋㅋ 갑질 뭐야 진짜",
          "speaker_id": "2",
          "time": "20210828 05:53"
        },
        {
          "id": "MDRW2100020441.1.16",
          "form": "일반 소비자도 아닌데",
          "original_form": "일반 소비자도 아닌데",
          "speaker_id": "1",
          "time": "20210828 05:53"
        },
        {
          "id": "MDRW2100020441.1.17",
          "form": "회사 누구누구 이름 직책 다 나와있는데도 그래요",
          "original_form": "회사 누구누구 이름 직책 다 나와있는데도 그래요",
          "speaker_id": "1",
          "time": "20210828 05:53"
        },
        {
          "id": "MDRW2100020441.1.18",
          "form": "소비자 민원도 힘든데 고객사 민원도 은근",
          "original_form": "소비자 민원도 힘든데 고객사 민원도 은근",
          "speaker_id": "2",
          "time": "20210828 05:53"
        },
        {
          "id": "MDRW2100020441.1.19",
          "form": "스트레스인것 같더라구요",
          "original_form": "스트레스인것 같더라구요",
          "speaker_id": "2",
          "time": "20210828 05:54"
        },
        {
          "id": "MDRW2100020441.1.20",
          "form": "친구가 맨날 어디 회사 욕하는 얘기만 해섴ㅋㅋㅋ",
          "original_form": "친구가 맨날 어디 회사 욕하는 얘기만 해섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:54"
        },
        {
          "id": "MDRW2100020441.1.21",
          "form": "네네ㅠㅠ 뭐든 사람이 힘들게 하는게 큰것같아요 정말...",
          "original_form": "네네ㅠㅠ 뭐든 사람이 힘들게 하는게 큰것같아요 정말...",
          "speaker_id": "1",
          "time": "20210828 05:54"
        },
        {
          "id": "MDRW2100020441.1.22",
          "form": "ㅋㅋㅋㅋ 저 보는것같네요ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 저 보는것같네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:54"
        },
        {
          "id": "MDRW2100020441.1.23",
          "form": "맞아요 업무야 그냥 하면 되는데 사람이 중요하죠ㅠㅠ",
          "original_form": "맞아요 업무야 그냥 하면 되는데 사람이 중요하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 05:54"
        },
        {
          "id": "MDRW2100020441.1.24",
          "form": "반대로 일이 힘들어도",
          "original_form": "반대로 일이 힘들어도",
          "speaker_id": "1",
          "time": "20210828 05:54"
        },
        {
          "id": "MDRW2100020441.1.25",
          "form": "사람이 좋으면",
          "original_form": "사람이 좋으면",
          "speaker_id": "1",
          "time": "20210828 05:55"
        },
        {
          "id": "MDRW2100020441.1.26",
          "form": "계속 가게되는것같아용",
          "original_form": "계속 가게되는것같아용",
