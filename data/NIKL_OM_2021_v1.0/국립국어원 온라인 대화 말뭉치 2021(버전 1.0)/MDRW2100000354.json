      "utterance": [
        {
          "id": "MDRW2100000354.1.1",
          "form": "가을이 오는데 비염은 아직 괜찮네",
          "original_form": "가을이 오는데 비염은 아직 괜찮네",
          "speaker_id": "1",
          "time": "20210819 15:30"
        },
        {
          "id": "MDRW2100000354.1.2",
          "form": "비 많이와서 안건조해서 그런듯ㅋㅋ",
          "original_form": "비 많이와서 안건조해서 그런듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:31"
        },
        {
          "id": "MDRW2100000354.1.3",
          "form": "하지만 피부트러블이 장난이 아님",
          "original_form": "하지만 피부트러블이 장난이 아님",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000354.1.4",
          "form": "나도 얼굴에 이상한거 많이 생김...ㅠㅠ",
          "original_form": "나도 얼굴에 이상한거 많이 생김...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000354.1.5",
          "form": "가라앉을 생각을 안 하고 더더더 많이 생기는 중",
          "original_form": "가라앉을 생각을 안 하고 더더더 많이 생기는 중",
          "speaker_id": "1",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000354.1.6",
          "form": "난 눈꺼플에 너무 심해서 항생제 연고까지 발랐다",
          "original_form": "난 눈꺼플에 너무 심해서 항생제 연고까지 발랐다",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000354.1.7",
          "form": "오늘은 팩이라도 해봐야겠어",
          "original_form": "오늘은 팩이라도 해봐야겠어",
          "speaker_id": "1",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000354.1.8",
          "form": "난 관리가 아니라 치료를 받아야....",
          "original_form": "난 관리가 아니라 치료를 받아야....",
          "speaker_id": "2",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000354.1.9",
          "form": "나도 치료 받아야 될 수준인듯한 느낌",
          "original_form": "나도 치료 받아야 될 수준인듯한 느낌",
          "speaker_id": "1",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000354.1.10",
          "form": "근데 피부과가도 약을 못먹으니 무용지물",
          "original_form": "근데 피부과가도 약을 못먹으니 무용지물",
          "speaker_id": "2",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000354.1.11",
          "form": "그래도 연고 발랐더니 쩜 덜 하긴 한데",
          "original_form": "그래도 연고 발랐더니 쩜 덜 하긴 한데",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000354.1.12",
          "form": "안바르면 또 도질듯",
          "original_form": "안바르면 또 도질듯",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000354.1.13",
          "form": "카페인 끊기는 잘 하고 있나?",
          "original_form": "카페인 끊기는 잘 하고 있나?",
          "speaker_id": "1",
          "time": "20210819 15:39"
        },
        {
          "id": "MDRW2100000354.1.14",
          "form": "완전히 끊지는 못하고",
          "original_form": "완전히 끊지는 못하고",
          "speaker_id": "2",
          "time": "20210819 15:39"
        },
        {
          "id": "MDRW2100000354.1.15",
          "form": "이틀에 한잔 수준으로 유지...",
          "original_form": "이틀에 한잔 수준으로 유지...",
          "speaker_id": "2",
          "time": "20210819 15:39"
        },
        {
          "id": "MDRW2100000354.1.16",
          "form": "난 거의 한달째 유지중",
          "original_form": "난 거의 한달째 유지중",
          "speaker_id": "1",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000354.1.17",
          "form": "다다나다",
          "original_form": "다다나다",
          "speaker_id": "2",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000354.1.18",
          "form": "이제 역류성 식도염 좀 좋아진것 같아서",
          "original_form": "이제 역류성 식도염 좀 좋아진것 같아서",
          "speaker_id": "1",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000354.1.19",
          "form": "대다나다",
          "original_form": "대다나다",
          "speaker_id": "2",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000354.1.20",
          "form": "유산균 먹는거 다 먹으면 커피 마셔봐야지",
          "original_form": "유산균 먹는거 다 먹으면 커피 마셔봐야지",
          "speaker_id": "1",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000354.1.21",
          "form": "난 식도염은 그냥 영원히 함께가는 운명이라 여기기로 했다",
          "original_form": "난 식도염은 그냥 영원히 함께가는 운명이라 여기기로 했다",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000354.1.22",
          "form": "나도 늘 함께 간다고 생각은 해 ㅋㅋ별 증상 없을때는 카페인 마구 섭취하니까",
          "original_form": "나도 늘 함께 간다고 생각은 해 ㅋㅋ별 증상 없을때는 카페인 마구 섭취하니까",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000354.1.23",
          "form": "난 어차피 카페인 끊고 자극적인거 안먹어도 면역억제제가 원인인것을...",
          "original_form": "난 어차피 카페인 끊고 자극적인거 안먹어도 면역억제제가 원인인것을...",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000354.1.24",
          "form": "비 오니 요즘은 산책도 못 가고 아쉽네",
          "original_form": "비 오니 요즘은 산책도 못 가고 아쉽네",
          "speaker_id": "1",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000354.1.25",
          "form": "안그래도 날 시원해져서 걷고싶은디 시도때도 없이 비 쏟아져서 짜증ㅋㅋ",
          "original_form": "안그래도 날 시원해져서 걷고싶은디 시도때도 없이 비 쏟아져서 짜증ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000354.1.26",
          "form": "운동해서 건강 절대 지켜!",
          "original_form": "운동해서 건강 절대 지켜!",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000354.1.27",
          "form": "이미 건강은 물건너갔고",
          "original_form": "이미 건강은 물건너갔고",
          "speaker_id": "2",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000354.1.28",
          "form": "더 나빠지지않기를ㅋㅋㅋ",
          "original_form": "더 나빠지지않기를ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000354.1.29",
          "form": "비염없이 환절기 보내고싶다",
          "original_form": "비염없이 환절기 보내고싶다",
          "speaker_id": "1",
          "time": "20210819 15:48"
        }
      ]
    }
