      "utterance": [
        {
          "id": "MDRW2100013084.1.1",
          "form": "헐... 면에서 밥으로 바뀌면 좀 아쉬운데...",
          "original_form": "헐... 면에서 밥으로 바뀌면 좀 아쉬운데...",
          "speaker_id": "2",
          "time": "20210825 12:44"
        },
        {
          "id": "MDRW2100013084.1.2",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100013084.1.3",
          "form": "그냥 라면 끓여먹었어요",
          "original_form": "그냥 라면 끓여먹었어요",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013084.1.4",
          "form": "급식 스파게티",
          "original_form": "급식 스파게티",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013084.1.5",
          "form": "기대햇는데 아쉽긴 했어요 ㅋㅋㅋ",
          "original_form": "기대햇는데 아쉽긴 했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013084.1.6",
          "form": "라면 뭐좋아하시나요",
          "original_form": "라면 뭐좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013084.1.7",
          "form": "신라면 좋아하지만 오늘은 진라면 순한맛 먹었어요",
          "original_form": "신라면 좋아하지만 오늘은 진라면 순한맛 먹었어요",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013084.1.8",
          "form": "진라면 순한맛 좋아하시나요 그거 호불호 엄청 갈리던데!",
          "original_form": "진라면 순한맛 좋아하시나요 그거 호불호 엄청 갈리던데!",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013084.1.9",
          "form": "저는 좋아해요ㅎㅎ 한개만 먹을거면 진순이 좋고",
          "original_form": "저는 좋아해요ㅎㅎ 한개만 먹을거면 진순이 좋고",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013084.1.10",
          "form": "같이 먹을 사람 있으면 진매 진순 섞는게 맛있어요",
          "original_form": "같이 먹을 사람 있으면 진매 진순 섞는게 맛있어요",
          "speaker_id": "2",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013084.1.11",
          "form": "헐 저 섞어볼 생각 한번도 못했어요",
          "original_form": "헐 저 섞어볼 생각 한번도 못했어요",
          "speaker_id": "1",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013084.1.12",
          "form": "다음에 도전해보겠습니다 ㅋㅋㅋㅋ",
          "original_form": "다음에 도전해보겠습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013084.1.13",
          "form": "ㅋㅋ좋아요",
          "original_form": "ㅋㅋ좋아요",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013084.1.14",
          "form": "마크정식 아시나요",
          "original_form": "마크정식 아시나요",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013084.1.15",
          "form": "편의점 떡볶이랑 콕콕콕 스파게티 섞는건데",
          "original_form": "편의점 떡볶이랑 콕콕콕 스파게티 섞는건데",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013084.1.16",
          "form": "그거 되게 불량식품맛이고 맛잇어요",
          "original_form": "그거 되게 불량식품맛이고 맛잇어요",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013084.1.17",
          "form": "아직 못 먹어봤어요",
          "original_form": "아직 못 먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013084.1.18",
          "form": "떡볶이랑 콕콕콕 각각 사먹어 본적도 없네요",
          "original_form": "떡볶이랑 콕콕콕 각각 사먹어 본적도 없네요",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013084.1.19",
          "form": "헐 거기에 치즈도 넣고 소시지도 넣고 하면",
          "original_form": "헐 거기에 치즈도 넣고 소시지도 넣고 하면",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013084.1.20",
          "form": "자극적이고 맛있어요",
          "original_form": "자극적이고 맛있어요",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013084.1.21",
          "form": "재료들이 심상치 않네요ㅋㅋ 맛이 보장된 조합",
          "original_form": "재료들이 심상치 않네요ㅋㅋ 맛이 보장된 조합",
          "speaker_id": "2",
          "time": "20210825 15:12"
        }
      ]
