      "utterance": [
        {
          "id": "MDRW2100016573.1.1",
          "form": "요번엔 안아프고 넘어 가야 할텐데요~",
          "original_form": "요번엔 안아프고 넘어 가야 할텐데요~",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.2",
          "form": "난도 겁나여ㅠ",
          "original_form": "난도 겁나여ㅠ",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.3",
          "form": "어 화이자",
          "original_form": "어 화이자",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.4",
          "form": "아까 까먹어가",
          "original_form": "아까 까먹어가",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.5",
          "form": "뭐했는지도모르겠다",
          "original_form": "뭐했는지도모르겠다",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.6",
          "form": "요즘 화이자가 많이 남나?? 잔여도 화이자가 많데요~",
          "original_form": "요즘 화이자가 많이 남나?? 잔여도 화이자가 많데요~",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.7",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.8",
          "form": "아까~우리 날씨얘기하다가 옆으로 빠짐 ㅋㅋㅋ",
          "original_form": "아까~우리 날씨얘기하다가 옆으로 빠짐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.9",
          "form": "아젠안들어오는갑네",
          "original_form": "아젠안들어오는갑네",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016573.1.10",
          "form": "헉ㅜ",
          "original_form": "헉ㅜ",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016573.1.11",
          "form": "날씨였군ㅡㅡ",
          "original_form": "날씨였군ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016573.1.12",
          "form": "난 그건 잔여 떠도 안잡았아요 ㅋㅋㅋㅋ",
          "original_form": "난 그건 잔여 떠도 안잡았아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016573.1.13",
          "form": "모더나가 잘 없데여~",
          "original_form": "모더나가 잘 없데여~",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016573.1.14",
          "form": "알수 없음 ㅋㅋㅋㅋ",
          "original_form": "알수 없음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016573.1.15",
          "form": "여튼 아프지 말고 잘 지나가입시더~",
          "original_form": "여튼 아프지 말고 잘 지나가입시더~",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016573.1.16",
          "form": "엉ㅜ 떨림",
          "original_form": "엉ㅜ 떨림",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016573.1.17",
          "form": "건강하자",
          "original_form": "건강하자",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016573.1.18",
          "form": "맞아여~기승전 건강입니데이~",
          "original_form": "맞아여~기승전 건강입니데이~",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016573.1.19",
          "form": "진짜 안아프고 병원안가는 삶 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 안아프고 병원안가는 삶 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016573.1.20",
          "form": "그래 영양제돚먹고",
          "original_form": "그래 영양제돚먹고",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016573.1.21",
          "form": "애들껀 챙겨줘도 난 잘 안먹게 되여ㅠ",
          "original_form": "애들껀 챙겨줘도 난 잘 안먹게 되여ㅠ",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016573.1.22",
          "form": "집에가믄 약 먹어야지 ㅋㅋㅋㅋ",
          "original_form": "집에가믄 약 먹어야지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016573.1.23",
          "form": "난도 오메가ㅋ",
          "original_form": "난도 오메가ㅋ",
          "speaker_id": "2",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016573.1.24",
          "form": "걷기운동하자",
          "original_form": "걷기운동하자",
          "speaker_id": "2",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016573.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ안그래도 신랑 첫찌데리러 가서ㅜ열씨미 걸어서 집에 가야하네요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ안그래도 신랑 첫찌데리러 가서ㅜ열씨미 걸어서 집에 가야하네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016573.1.26",
          "form": "걷기랑은 뗄수없는 몸 ㅋㅋㅋㅋ",
          "original_form": "걷기랑은 뗄수없는 몸 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016573.1.27",
          "form": "언니 살빼는 한약 먹어봤어여??",
          "original_form": "언니 살빼는 한약 먹어봤어여??",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016573.1.28",
          "form": "먹어봤지",
          "original_form": "먹어봤지",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016573.1.29",
          "form": "방금도 받았다",
          "original_form": "방금도 받았다",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016573.1.30",
          "form": "소개다이어트한약",
          "original_form": "소개다이어트한약",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016573.1.31",
