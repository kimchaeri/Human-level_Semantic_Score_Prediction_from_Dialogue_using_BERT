      "utterance": [
        {
          "id": "MDRW2100020349.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 01:51"
        },
        {
          "id": "MDRW2100020349.1.2",
          "form": "최근에 구매한 물건잇으세요?",
          "original_form": "최근에 구매한 물건잇으세요?",
          "speaker_id": "1",
          "time": "20210828 01:51"
        },
        {
          "id": "MDRW2100020349.1.3",
          "form": "실리콘 밥주걱을 샀어요",
          "original_form": "실리콘 밥주걱을 샀어요",
          "speaker_id": "2",
          "time": "20210828 02:01"
        },
        {
          "id": "MDRW2100020349.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210828 02:18"
        },
        {
          "id": "MDRW2100020349.1.5",
          "form": "저는 그릭요거트 만드려고 면보자기 삿어요",
          "original_form": "저는 그릭요거트 만드려고 면보자기 삿어요",
          "speaker_id": "1",
          "time": "20210828 02:18"
        },
        {
          "id": "MDRW2100020349.1.6",
          "form": "그릭요거트 자주 만드시나요?",
          "original_form": "그릭요거트 자주 만드시나요?",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020349.1.7",
          "form": "아 이번에 처음 만들어먹어봤는데",
          "original_form": "아 이번에 처음 만들어먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100020349.1.8",
          "form": "맛잇어서 자주 만들거같아요",
          "original_form": "맛잇어서 자주 만들거같아요",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100020349.1.9",
          "form": "저는 요거트 메이커 있어서 종종 요거트 만드는데 그릭은 꽤 손이 많이 가서 버겁더라고요",
          "original_form": "저는 요거트 메이커 있어서 종종 요거트 만드는데 그릭은 꽤 손이 많이 가서 버겁더라고요",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100020349.1.10",
          "form": "굳이 우유로 발효 안하시구",
          "original_form": "굳이 우유로 발효 안하시구",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100020349.1.11",
          "form": "그냥 플레인요거트로 유청만 거르면",
          "original_form": "그냥 플레인요거트로 유청만 거르면",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100020349.1.12",
          "form": "간단하더라구요",
          "original_form": "간단하더라구요",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100020349.1.13",
          "form": "오 그렇군요ㅎ 그릭요거트 드실때 어떤 조합으로 드세요?",
          "original_form": "오 그렇군요ㅎ 그릭요거트 드실때 어떤 조합으로 드세요?",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100020349.1.14",
          "form": "저 그래놀라욤",
          "original_form": "저 그래놀라욤",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100020349.1.15",
          "form": "저는 꿀만 타먹어요 요거트 고유의 맛을 워낙 좋아해서ㅎ",
          "original_form": "저는 꿀만 타먹어요 요거트 고유의 맛을 워낙 좋아해서ㅎ",
          "speaker_id": "2",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100020349.1.16",
          "form": "아하 그렇게 먹어도 맛은 잇죠!",
          "original_form": "아하 그렇게 먹어도 맛은 잇죠!",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100020349.1.17",
          "form": "네 전 요거트를 건강땜에 먹기보단 좋아해서 먹는 편이에요ㅋㅋ",
          "original_form": "네 전 요거트를 건강땜에 먹기보단 좋아해서 먹는 편이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:21"
        },
        {
          "id": "MDRW2100020349.1.18",
          "form": "좋아하시는 편에 건강도 좋으면 완전 일석이조이네요",
          "original_form": "좋아하시는 편에 건강도 좋으면 완전 일석이조이네요",
          "speaker_id": "1",
          "time": "20210829 12:39"
        }
      ]
