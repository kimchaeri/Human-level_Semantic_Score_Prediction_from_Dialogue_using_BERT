      "utterance": [
        {
          "id": "MDRW2100043512.1.1",
          "form": "몇번 안먹어봤는데 맛있더라고요",
          "original_form": "몇번 안먹어봤는데 맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043512.1.2",
          "form": "아 저는 속초에 만석닭강정 먹어봤는데요",
          "original_form": "아 저는 속초에 만석닭강정 먹어봤는데요",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043512.1.3",
          "form": "네 맛있나요?",
          "original_form": "네 맛있나요?",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043512.1.4",
          "form": "엄청요! 왜 방송에도 나왔는지 알겠어요",
          "original_form": "엄청요! 왜 방송에도 나왔는지 알겠어요",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043512.1.5",
          "form": "음 특별히 더 맛있는 이유가 있어요?",
          "original_form": "음 특별히 더 맛있는 이유가 있어요?",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043512.1.6",
          "form": "닭강정을 양념에 버무려주는데",
          "original_form": "닭강정을 양념에 버무려주는데",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043512.1.7",
          "form": "보통은 그렇죠?",
          "original_form": "보통은 그렇죠?",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043512.1.8",
          "form": "택배로 받은건데도 하루지낫는데 바삭하고",
          "original_form": "택배로 받은건데도 하루지낫는데 바삭하고",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043512.1.9",
          "form": "헉 신기하네요",
          "original_form": "헉 신기하네요",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043512.1.10",
          "form": "그쵸그쵸 그리고 양념이 엄청 달다구리 맛있어요",
          "original_form": "그쵸그쵸 그리고 양념이 엄청 달다구리 맛있어요",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043512.1.11",
          "form": "치킨류는 양념이 중요해요",
          "original_form": "치킨류는 양념이 중요해요",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043512.1.12",
          "form": "네 달달하고 매콤하고 맛있어요",
          "original_form": "네 달달하고 매콤하고 맛있어요",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043512.1.13",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210907 19:23"
        }
      ]
