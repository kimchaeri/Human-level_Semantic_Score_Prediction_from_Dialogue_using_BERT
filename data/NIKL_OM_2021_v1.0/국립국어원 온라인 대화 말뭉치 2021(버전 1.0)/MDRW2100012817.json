        {
          "id": "MDRW2100012817.1.1",
          "form": "중식 좋아하세요?",
          "original_form": "중식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012817.1.2",
          "form": "라면 좋아하세요?",
          "original_form": "라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 02:03"
        },
        {
          "id": "MDRW2100012817.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210825 07:22"
        },
        {
          "id": "MDRW2100012817.1.4",
          "form": "점심드셨나요?!",
          "original_form": "점심드셨나요?!",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100012817.1.5",
          "form": "이제 먹을까해요",
          "original_form": "이제 먹을까해요",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100012817.1.6",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100012817.1.7",
          "form": "전먹었답니다ㅎㅎㅎ",
          "original_form": "전먹었답니다ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100012817.1.8",
          "form": "뭐 드셨어요",
          "original_form": "뭐 드셨어요",
          "speaker_id": "2",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100012817.1.9",
          "form": "저 치킨이요!",
          "original_form": "저 치킨이요!",
          "speaker_id": "1",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100012817.1.10",
          "form": "점심을요?",
          "original_form": "점심을요?",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100012817.1.11",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100012817.1.12",
          "form": "양념치킨이요",
          "original_form": "양념치킨이요",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100012817.1.13",
          "form": "오? 직딩 아니신감",
          "original_form": "오? 직딩 아니신감",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100012817.1.14",
          "form": "ㅋㅋㅋ아니랍니다ㅎㅎ",
          "original_form": "ㅋㅋㅋ아니랍니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100012817.1.15",
          "form": "학생도아니지만요",
          "original_form": "학생도아니지만요",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100012817.1.16",
          "form": "아 취준이신가요.",
          "original_form": "아 취준이신가요.",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100012817.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100012817.1.18",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100012817.1.19",
          "form": "네 신전 짱이죠",
          "original_form": "네 신전 짱이죠",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100012817.1.20",
          "form": "로제는요?",
          "original_form": "로제는요?",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100012817.1.21",
          "form": "로제보다는 오리지날이!",
          "original_form": "로제보다는 오리지날이!",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100012817.1.22",
          "form": "ㅋㅋㅋ전",
          "original_form": "ㅋㅋㅋ전",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100012817.1.23",
          "form": "요즘 로제가 너무좋아요",
          "original_form": "요즘 로제가 너무좋아요",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100012817.1.24",
          "form": "로제 여성들에게 인기가",
          "original_form": "로제 여성들에게 인기가",
          "speaker_id": "2",
