      "utterance": [
        {
          "id": "MDRW2100047126.1.1",
          "form": "아이스크림 좋아하죠 ㅋㅋㅋㅋ",
          "original_form": "아이스크림 좋아하죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100047126.1.2",
          "form": "오늘도 빠삐코 밀크맛 먹었어요",
          "original_form": "오늘도 빠삐코 밀크맛 먹었어요",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100047126.1.3",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100047126.1.4",
          "form": "이거 맛있어요!",
          "original_form": "이거 맛있어요!",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100047126.1.5",
          "form": "빠삐코는 밀크죠",
          "original_form": "빠삐코는 밀크죠",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.6",
          "form": "진짜 맛을 아시네요",
          "original_form": "진짜 맛을 아시네요",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.7",
          "form": "밀크쉐이크맛의 정석이잖아요",
          "original_form": "밀크쉐이크맛의 정석이잖아요",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.8",
          "form": "맛잘알이셔 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맛잘알이셔 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.9",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.10",
          "form": "맞아요 맞아요 ㅠㅠㅋㅋㅋ",
          "original_form": "맞아요 맞아요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.11",
          "form": "그럼 이쯤에서 뻔한질문",
          "original_form": "그럼 이쯤에서 뻔한질문",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.12",
          "form": "난 민초다 반민초다",
          "original_form": "난 민초다 반민초다",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.14",
          "form": "전 민초 먹어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 민초 먹어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.15",
          "form": "말해봐요 어서",
          "original_form": "말해봐요 어서",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.16",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047126.1.17",
          "form": "우리 잠시 멀어져요",
          "original_form": "우리 잠시 멀어져요",
          "speaker_id": "1",
          "time": "20210822 16:28"
