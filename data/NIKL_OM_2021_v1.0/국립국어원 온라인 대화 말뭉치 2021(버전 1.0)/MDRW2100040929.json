      "utterance": [
        {
          "id": "MDRW2100040929.1.1",
          "form": "저녁은 맛있게 드셨나요?",
          "original_form": "저녁은 맛있게 드셨나요?",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100040929.1.2",
          "form": "네네 ㅋㅋ 벌써 아침이네요ㅠ",
          "original_form": "네네 ㅋㅋ 벌써 아침이네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100040929.1.3",
          "form": "그러네요 아침 드셨어요?",
          "original_form": "그러네요 아침 드셨어요?",
          "speaker_id": "2",
          "time": "20210906 08:49"
        },
        {
          "id": "MDRW2100040929.1.4",
          "form": "네네 오늘은 동료분 아가가 백일이라 백설기를 먹었어요!",
          "original_form": "네네 오늘은 동료분 아가가 백일이라 백설기를 먹었어요!",
          "speaker_id": "1",
          "time": "20210906 08:49"
        },
        {
          "id": "MDRW2100040929.1.5",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "1",
          "time": "20210906 08:49"
        },
        {
          "id": "MDRW2100040929.1.6",
          "form": "백설기 맛있었겠네요",
          "original_form": "백설기 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210906 08:50"
        },
        {
          "id": "MDRW2100040929.1.7",
          "form": "저는 아침 먹었어요",
          "original_form": "저는 아침 먹었어요",
          "speaker_id": "2",
          "time": "20210906 08:50"
        },
        {
          "id": "MDRW2100040929.1.8",
          "form": "우리 식구들은 아침 꼭 먹어서요",
          "original_form": "우리 식구들은 아침 꼭 먹어서요",
          "speaker_id": "2",
          "time": "20210906 08:51"
        },
        {
          "id": "MDRW2100040929.1.9",
          "form": "오 좋아요 밥으로 챙겨드셨어요?",
          "original_form": "오 좋아요 밥으로 챙겨드셨어요?",
          "speaker_id": "1",
          "time": "20210906 08:53"
        },
        {
          "id": "MDRW2100040929.1.10",
          "form": "보통 밥 국 차려서 먹어요",
          "original_form": "보통 밥 국 차려서 먹어요",
          "speaker_id": "2",
          "time": "20210906 08:58"
        },
        {
          "id": "MDRW2100040929.1.11",
          "form": "그러면 아침에 진짜 일찍일어나시겠어요",
          "original_form": "그러면 아침에 진짜 일찍일어나시겠어요",
          "speaker_id": "1",
          "time": "20210906 10:21"
        },
        {
          "id": "MDRW2100040929.1.12",
          "form": "그렇죠 6시 이전에 일어나야 해요",
          "original_form": "그렇죠 6시 이전에 일어나야 해요",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100040929.1.13",
          "form": "아침에 일찍일어나는건 안힘드세요?",
          "original_form": "아침에 일찍일어나는건 안힘드세요?",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100040929.1.14",
          "form": "저는 사실 아침에 잘 못일어나는데 나머지 식구들이 엄청 잘 일어나요",
          "original_form": "저는 사실 아침에 잘 못일어나는데 나머지 식구들이 엄청 잘 일어나요",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100040929.1.15",
          "form": "그래서 저도 일어나다 보니까 습관이 돼서인지 이제는 잘 일어나요",
          "original_form": "그래서 저도 일어나다 보니까 습관이 돼서인지 이제는 잘 일어나요",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100040929.1.16",
          "form": "우와 그래도 아침잠 많은게 건강에 더좋으실것같아요!!",
          "original_form": "우와 그래도 아침잠 많은게 건강에 더좋으실것같아요!!",
          "speaker_id": "1",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100040929.1.17",
          "form": "이제는 밥 국 말고 간단히 먹는 거 하고 있는데",
          "original_form": "이제는 밥 국 말고 간단히 먹는 거 하고 있는데",
          "speaker_id": "2",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100040929.1.18",
          "form": "슬슬 알아서 잘 차려먹더라구요",
          "original_form": "슬슬 알아서 잘 차려먹더라구요",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100040929.1.19",
          "form": "그래서 가끔 늦잠 잘때도 있어요 ㅎㅎ",
          "original_form": "그래서 가끔 늦잠 잘때도 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100040929.1.20",
          "form": "어제밤에 오류가 많았다던데 그거때문일까요 ㅋㅋ 제가 다시 바꿨습니다",
          "original_form": "어제밤에 오류가 많았다던데 그거때문일까요 ㅋㅋ 제가 다시 바꿨습니다",
          "speaker_id": "1",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100040929.1.21",
          "form": "비오니까 칼국수땡기네요 ㅠ",
          "original_form": "비오니까 칼국수땡기네요 ㅠ",
