          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017296.1.3",
          "form": "요가요",
          "original_form": "요가요",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017296.1.4",
          "form": "우와 집에서하시나요",
          "original_form": "우와 집에서하시나요",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017296.1.5",
          "form": "네네 유튜브 보면서요",
          "original_form": "네네 유튜브 보면서요",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017296.1.6",
          "form": "오 저도요 방송댄스해요",
          "original_form": "오 저도요 방송댄스해요",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017296.1.7",
          "form": "아스트로 애프터미드나잇",
          "original_form": "아스트로 애프터미드나잇",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017296.1.8",
          "form": "오 멋있어요!",
          "original_form": "오 멋있어요!",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017296.1.9",
          "form": "재밌는것같아요 요가가 더 멋져요",
          "original_form": "재밌는것같아요 요가가 더 멋져요",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017296.1.10",
          "form": "체형이 비틀어져서 요가해야하는데",
          "original_form": "체형이 비틀어져서 요가해야하는데",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017296.1.11",
          "form": "유튜브보고",
          "original_form": "유튜브보고",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017296.1.12",
          "form": "겨우 따라하는 수준이에요ㅋㅋ",
          "original_form": "겨우 따라하는 수준이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017296.1.13",
          "form": "그래두멋져요",
          "original_form": "그래두멋져요",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
