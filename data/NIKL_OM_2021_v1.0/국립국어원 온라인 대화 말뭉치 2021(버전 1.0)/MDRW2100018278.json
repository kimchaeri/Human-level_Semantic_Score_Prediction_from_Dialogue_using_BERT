      "utterance": [
        {
          "id": "MDRW2100018278.1.1",
          "form": "학교생활 재밌나요",
          "original_form": "학교생활 재밌나요",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018278.1.2",
          "form": "학교생활이요? ㅋㅋ 벌써 2년전 일인데",
          "original_form": "학교생활이요? ㅋㅋ 벌써 2년전 일인데",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018278.1.3",
          "form": "ㅋㅋㅋㅋㅋ재밌었었나요?",
          "original_form": "ㅋㅋㅋㅋㅋ재밌었었나요?",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018278.1.4",
          "form": "학교 다니시나용?",
          "original_form": "학교 다니시나용?",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018278.1.5",
          "form": "저도 졸업한지 2년째...",
          "original_form": "저도 졸업한지 2년째...",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018278.1.6",
          "form": "ㅋㅋㅋㅋㅋ 전 대학생활은 2학년때쯤에 힘들었었는데",
          "original_form": "ㅋㅋㅋㅋㅋ 전 대학생활은 2학년때쯤에 힘들었었는데",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018278.1.7",
          "form": "나중에 수월하더라고요",
          "original_form": "나중에 수월하더라고요",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018278.1.8",
          "form": "저는 매년 다른방법으로 힘들었던 것 같아요",
          "original_form": "저는 매년 다른방법으로 힘들었던 것 같아요",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018278.1.9",
          "form": "2학년은 수업 많아서...3학년은 인턴쉽...4학년은 실습....",
          "original_form": "2학년은 수업 많아서...3학년은 인턴쉽...4학년은 실습....",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018278.1.10",
          "form": "어떤 과 나오셨나요??",
          "original_form": "어떤 과 나오셨나요??",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018278.1.11",
          "form": "아동학 전공했어요!!!",
          "original_form": "아동학 전공했어요!!!",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018278.1.12",
          "form": "어느과세용??",
          "original_form": "어느과세용??",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018278.1.13",
          "form": "저 화학공학과요 근데",
          "original_form": "저 화학공학과요 근데",
          "speaker_id": "2",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018278.1.14",
          "form": "취업은 확실히 열심히 한 애들이 잘했더라고요 그래서 전 그냥 딴 일 하고 있어요.. ㅋㅋ",
          "original_form": "취업은 확실히 열심히 한 애들이 잘했더라고요 그래서 전 그냥 딴 일 하고 있어요.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018278.1.15",
          "form": "ㅋㅋㅋ어느 과나 다 그런가봐요",
          "original_form": "ㅋㅋㅋ어느 과나 다 그런가봐요",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018278.1.16",
          "form": "저두 다른 일 하고있어요",
          "original_form": "저두 다른 일 하고있어요",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018278.1.17",
          "form": "완전 다른쪽으로...",
          "original_form": "완전 다른쪽으로...",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018278.1.18",
          "form": "지금하는일도 완전 적성에 맞진 않아서",
          "original_form": "지금하는일도 완전 적성에 맞진 않아서",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018278.1.19",
          "form": "다시 대학원 갈까도 생각하고 있어요 ㅋㅋ",
          "original_form": "다시 대학원 갈까도 생각하고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018278.1.20",
          "form": "우와 대학원...",
          "original_form": "우와 대학원...",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018278.1.21",
          "form": "상상도 하고싶지 않은곳...",
          "original_form": "상상도 하고싶지 않은곳...",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018278.1.22",
          "form": "하하...",
          "original_form": "하하...",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018278.1.23",
          "form": "아무래도 대학원 나오면 취업은 잘 되다보니..",
          "original_form": "아무래도 대학원 나오면 취업은 잘 되다보니..",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018278.1.24",
          "form": "저희 과 애들도 반정도는 대학원 가더라구요",
          "original_form": "저희 과 애들도 반정도는 대학원 가더라구요",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018278.1.25",
          "form": "대학원 많이 가는구나ㅠㅠ",
          "original_form": "대학원 많이 가는구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018278.1.26",
          "form": "갔다가 박사까지 하고..",
          "original_form": "갔다가 박사까지 하고..",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018278.1.27",
          "form": "2년만 고생한다는 생각으로",
          "original_form": "2년만 고생한다는 생각으로",
          "speaker_id": "2",
          "time": "20210826 23:45"
        }
      ]
    }
  ]
