      "utterance": [
        {
          "id": "MDRW2100036965.1.1",
          "form": "자주보시는 요리관련유투버있나요?",
          "original_form": "자주보시는 요리관련유투버있나요?",
          "speaker_id": "2",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100036965.1.2",
          "form": "백종원 많이 봐요",
          "original_form": "백종원 많이 봐요",
          "speaker_id": "1",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036965.1.3",
          "form": "그저 갓 백선생님ㅋㅋ",
          "original_form": "그저 갓 백선생님ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100036965.1.4",
          "form": "오오 저두요 ㅋㅋㅋ",
          "original_form": "오오 저두요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100036965.1.5",
          "form": "백주부님 넘잼나쥬",
          "original_form": "백주부님 넘잼나쥬",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100036965.1.6",
          "form": "말도 잘하시고 레시피도 좋고ㅎㅎ",
          "original_form": "말도 잘하시고 레시피도 좋고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100036965.1.7",
          "form": "백선생님 덕분에 잘먹고 삽니다",
          "original_form": "백선생님 덕분에 잘먹고 삽니다",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100036965.1.8",
          "form": "맞아여 ㅋㅋㅋㅋ 백주부님 리치가이 센스도넘치시고 ㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋㅋ 백주부님 리치가이 센스도넘치시고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100036965.1.9",
          "form": "그래서 방송 많이 하시나봐요",
          "original_form": "그래서 방송 많이 하시나봐요",
          "speaker_id": "1",
          "time": "20210902 07:38"
        },
        {
          "id": "MDRW2100036965.1.10",
          "form": "맞아여ㅋㅋㅋ",
          "original_form": "맞아여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100036965.1.11",
          "form": "예전에 골목식당 많이 봤었어요",
          "original_form": "예전에 골목식당 많이 봤었어요",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100036965.1.12",
          "form": "아하! 저는 조보아님? 나오셧을때 잠깐봣어요",
          "original_form": "아하! 저는 조보아님? 나오셧을때 잠깐봣어요",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100036965.1.13",
          "form": "저도요ㅋㅋ 조보아님 되게 일 잘하시던데ㅋㅋ",
          "original_form": "저도요ㅋㅋ 조보아님 되게 일 잘하시던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100036965.1.14",
          "form": "맞아여!! 성격도 좋으신거같구 무엇보다 너무 예쁘신 ㅜㅜ",
          "original_form": "맞아여!! 성격도 좋으신거같구 무엇보다 너무 예쁘신 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100036965.1.15",
          "form": "맞아요 진짜 다 갖추신분",
          "original_form": "맞아요 진짜 다 갖추신분",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100036965.1.16",
          "form": "부러워요 게다가 연기도잘하시더라구요!",
          "original_form": "부러워요 게다가 연기도잘하시더라구요!",
          "speaker_id": "2",
          "time": "20210902 14:15"
        }
