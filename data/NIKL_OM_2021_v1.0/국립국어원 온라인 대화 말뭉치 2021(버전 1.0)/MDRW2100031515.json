      "utterance": [
        {
          "id": "MDRW2100031515.1.1",
          "form": "저는 오는 황리단길 갔는데",
          "original_form": "저는 오는 황리단길 갔는데",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031515.1.2",
          "form": "경주 황리단길",
          "original_form": "경주 황리단길",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031515.1.3",
          "form": "거기서 오스테리아 밀즈라는 레스토랑에서 리조토랑 파스타 먹었는데 꿀맛이었어요",
          "original_form": "거기서 오스테리아 밀즈라는 레스토랑에서 리조토랑 파스타 먹었는데 꿀맛이었어요",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100031515.1.4",
          "form": "요리 자주 하시나요?",
          "original_form": "요리 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100031515.1.5",
          "form": "환상적인맛이었겠네요",
          "original_form": "환상적인맛이었겠네요",
          "speaker_id": "1",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100031515.1.6",
          "form": "저는 리조토가 맛있었는데",
          "original_form": "저는 리조토가 맛있었는데",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031515.1.7",
          "form": "아내는 파스타가 맛있었다고 하더라고요",
          "original_form": "아내는 파스타가 맛있었다고 하더라고요",
          "speaker_id": "2",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100031515.1.8",
          "form": "크리스님은 오늘 밖에서 식사할 계획 있으신가요?",
          "original_form": "크리스님은 오늘 밖에서 식사할 계획 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100031515.1.9",
          "form": "오오!!!맛있겠다",
          "original_form": "오오!!!맛있겠다",
          "speaker_id": "1",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100031515.1.10",
          "form": "저는 어제는KFC맛있는거 먹었어요",
          "original_form": "저는 어제는KFC맛있는거 먹었어요",
          "speaker_id": "1",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100031515.1.11",
          "form": "햄버거랑 치킨....",
          "original_form": "햄버거랑 치킨....",
          "speaker_id": "1",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100031515.1.12",
          "form": "그리고 또 치킨",
          "original_form": "그리고 또 치킨",
          "speaker_id": "1",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100031515.1.13",
          "form": "오늘은 머먹지",
          "original_form": "오늘은 머먹지",
          "speaker_id": "1",
          "time": "20210905 08:59"
        },
        {
          "id": "MDRW2100031515.1.14",
          "form": "이디야들러서 음료 마실예정이에요",
          "original_form": "이디야들러서 음료 마실예정이에요",
          "speaker_id": "1",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031515.1.15",
          "form": "밀키트 사다가 요리해보는거 어때요",
          "original_form": "밀키트 사다가 요리해보는거 어때요",
          "speaker_id": "2",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031515.1.16",
          "form": "오후에요",
          "original_form": "오후에요",
          "speaker_id": "1",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031515.1.17",
          "form": "그게 머에요?",
          "original_form": "그게 머에요?",
          "speaker_id": "1",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031515.1.18",
          "form": "재료랑 양념이랑 다 손질되있어서",
          "original_form": "재료랑 양념이랑 다 손질되있어서",
          "speaker_id": "2",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031515.1.19",
          "form": "집에서 볶고 굽고 삶기만 하면되는거",
          "original_form": "집에서 볶고 굽고 삶기만 하면되는거",
          "speaker_id": "2",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031515.1.20",
          "form": "그냥 바로 해먹으면 되는거에요?",
          "original_form": "그냥 바로 해먹으면 되는거에요?",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031515.1.21",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031515.1.22",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031515.1.23",
          "form": "바로먹을수있으니깐",
          "original_form": "바로먹을수있으니깐",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031515.1.24",
          "form": "간편하겠다",
          "original_form": "간편하겠다",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031515.1.25",
          "form": "쓰레기도 덜 나오고",
          "original_form": "쓰레기도 덜 나오고",
          "speaker_id": "2",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031515.1.26",
          "form": "그럴것같아요",
          "original_form": "그럴것같아요",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031515.1.27",
          "form": "먹는거먹을때",
          "original_form": "먹는거먹을때",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031515.1.28",
          "form": "쓰레기많이 나오면 좀 그렇죠",
          "original_form": "쓰레기많이 나오면 좀 그렇죠",
          "speaker_id": "1",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100031515.1.29",
          "form": "맞아요 그것때문에 요리하기 꺼리거돼요",
          "original_form": "맞아요 그것때문에 요리하기 꺼리거돼요",
          "speaker_id": "2",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100031515.1.30",
          "form": "카페는 자주 가세요?",
          "original_form": "카페는 자주 가세요?",
          "speaker_id": "2",
          "time": "20210905 09:02"
        }
      ]
    }
