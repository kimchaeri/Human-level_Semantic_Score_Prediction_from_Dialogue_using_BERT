      "utterance": [
        {
          "id": "MDRW2100030251.1.1",
          "form": "요즘 뭐 보는거 있어?",
          "original_form": "요즘 뭐 보는거 있어?",
          "speaker_id": "2",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100030251.1.2",
          "form": "나 요즘 중국드라마~",
          "original_form": "나 요즘 중국드라마~",
          "speaker_id": "1",
          "time": "20210904 10:56"
        },
        {
          "id": "MDRW2100030251.1.3",
          "form": "보던거 다 보고 요즘은 청춘물 봄?",
          "original_form": "보던거 다 보고 요즘은 청춘물 봄?",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030251.1.4",
          "form": "물음표 잘만누름 ㅋㅋ",
          "original_form": "물음표 잘만누름 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030251.1.5",
          "form": "중국드라마 다봤네",
          "original_form": "중국드라마 다봤네",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030251.1.6",
          "form": "못볼까 걱정했었는데 다행이네ㅎ",
          "original_form": "못볼까 걱정했었는데 다행이네ㅎ",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030251.1.7",
          "form": "ㅇㅇ 오랜만에 재미난 중드봤엉 ㅋㅋ",
          "original_form": "ㅇㅇ 오랜만에 재미난 중드봤엉 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030251.1.8",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030251.1.9",
          "form": "현대물 별로였는데 요즘은 쟤네도 많이 발전한듯",
          "original_form": "현대물 별로였는데 요즘은 쟤네도 많이 발전한듯",
          "speaker_id": "1",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030251.1.10",
          "form": "설명 부탁해",
          "original_form": "설명 부탁해",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030251.1.11",
          "form": "내가 재밌어할만해?",
          "original_form": "내가 재밌어할만해?",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030251.1.12",
          "form": "글쎄 로맨스라서 이게... 메인몰빵 로맨스?",
          "original_form": "글쎄 로맨스라서 이게... 메인몰빵 로맨스?",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030251.1.13",
          "form": "남여주 잘생겼고 간질간질 달달 로맨스",
          "original_form": "남여주 잘생겼고 간질간질 달달 로맨스",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030251.1.14",
          "form": "오ㅎㅎ",
          "original_form": "오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100030251.1.15",
          "form": "현대 로맨스?",
          "original_form": "현대 로맨스?",
          "speaker_id": "2",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100030251.1.16",
          "form": "ㅇㅇ 요즘은 현대물도 볼만하더라 연출이 볼만했음 단조롭지않고",
          "original_form": "ㅇㅇ 요즘은 현대물도 볼만하더라 연출이 볼만했음 단조롭지않고",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100030251.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030251.1.18",
          "form": "잘 골라봤다이ㅎ",
          "original_form": "잘 골라봤다이ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030251.1.19",
          "form": "ㅇㅇ 근데 다른 현대물은 유치하거나 지루해서 보다 하차한게 한트럭이긴함",
          "original_form": "ㅇㅇ 근데 다른 현대물은 유치하거나 지루해서 보다 하차한게 한트럭이긴함",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030251.1.20",
          "form": "이드라마는 별 내용 없어도 연출좋고 메인 커플 계속 나와서 잼드라",
          "original_form": "이드라마는 별 내용 없어도 연출좋고 메인 커플 계속 나와서 잼드라",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030251.1.21",
          "form": "맞어 현대물 유치하고 지루하다고 했었다 그지ㅎ\r\n이번 중드 제목은 뭐야?",
          "original_form": "맞어 현대물 유치하고 지루하다고 했었다 그지ㅎ\r\n이번 중드 제목은 뭐야?",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030251.1.22",
          "form": "니시아적영요",
          "original_form": "니시아적영요",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030251.1.23",
          "form": "너는 나의 영광",
          "original_form": "너는 나의 영광",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030251.1.24",
          "form": "여주랑 남주 캐릭터 둘다 좋더라",
          "original_form": "여주랑 남주 캐릭터 둘다 좋더라",
          "speaker_id": "1",
          "time": "20210904 15:12"
        }
      ]
    }
  ]
