      "utterance": [
        {
          "id": "MDRW2100006383.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006383.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006383.1.3",
          "form": "아직 안주무시네요",
          "original_form": "아직 안주무시네요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006383.1.4",
          "form": "네.. 저녁을 너무 많이먹어서 배 좀 꺼지면 자려구요",
          "original_form": "네.. 저녁을 너무 많이먹어서 배 좀 꺼지면 자려구요",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006383.1.5",
          "form": "아하 집에서 드셨나요?",
          "original_form": "아하 집에서 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006383.1.6",
          "form": "네~ 아무래도 코로나때문에 외식은 거의 안하고 있네요",
          "original_form": "네~ 아무래도 코로나때문에 외식은 거의 안하고 있네요",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006383.1.7",
          "form": "원래 늦게 주무시나요?",
          "original_form": "원래 늦게 주무시나요?",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006383.1.8",
          "form": "그쵸 ㅠㅠ 망할 코로나",
          "original_form": "그쵸 ㅠㅠ 망할 코로나",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006383.1.9",
          "form": "낼 출근안해서 늦장부리고있네요",
          "original_form": "낼 출근안해서 늦장부리고있네요",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006383.1.10",
          "form": "아 부럽네요.. 전 일하러갑니다..",
          "original_form": "아 부럽네요.. 전 일하러갑니다..",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006383.1.11",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006383.1.12",
          "form": "네네 2차까지 다맞았어요 님은요?",
          "original_form": "네네 2차까지 다맞았어요 님은요?",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006383.1.13",
          "form": "전 1차도 아직..ㅎ 추석전에 맞고 연휴 때 쉬려구요",
          "original_form": "전 1차도 아직..ㅎ 추석전에 맞고 연휴 때 쉬려구요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006383.1.14",
          "form": "주사 맞고 컨디션 어떠셨어요?",
          "original_form": "주사 맞고 컨디션 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006383.1.15",
          "form": "아하 전 화이자 2번맞았는데",
          "original_form": "아하 전 화이자 2번맞았는데",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006383.1.16",
          "form": "1차는 아무렇지않고",
          "original_form": "1차는 아무렇지않고",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006383.1.17",
          "form": "2차는 아파서 누워있었어요 ㅠ",
          "original_form": "2차는 아파서 누워있었어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006383.1.18",
          "form": "화이자 두번맞으면 두번째가 아프대요 보통..",
          "original_form": "화이자 두번맞으면 두번째가 아프대요 보통..",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006383.1.19",
          "form": "헉... 몸살 같은 증상이었어요?",
          "original_form": "헉... 몸살 같은 증상이었어요?",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006383.1.20",
          "form": "네네 몸에힘이 안들어가고",
          "original_form": "네네 몸에힘이 안들어가고",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006383.1.21",
          "form": "속도안좋고 하더라구여",
          "original_form": "속도안좋고 하더라구여",
          "speaker_id": "1",
          "time": "20210907 23:44"
        }
      ]
