      "utterance": [
        {
          "id": "MDRW2100002838.1.1",
          "form": "덥네요~",
          "original_form": "덥네요~",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002838.1.2",
          "form": "네 내일부터 비라던데 그래보이지가 않네요",
          "original_form": "네 내일부터 비라던데 그래보이지가 않네요",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002838.1.3",
          "form": "잠깐 오고 말지 않겠어요",
          "original_form": "잠깐 오고 말지 않겠어요",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002838.1.4",
          "form": "늦은 장마시작이래요 ㅜㅠ",
          "original_form": "늦은 장마시작이래요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002838.1.5",
          "form": "토요일부터 온다고 하긴하는데 믿음이 ~ㅋㅋ",
          "original_form": "토요일부터 온다고 하긴하는데 믿음이 ~ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002838.1.6",
          "form": "지금 담주내내 비그림이예요",
          "original_form": "지금 담주내내 비그림이예요",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002838.1.7",
          "form": "꼼짝말고 집에 있어야겠네요",
          "original_form": "꼼짝말고 집에 있어야겠네요",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002838.1.8",
          "form": "저희회사는 벌써 공문내러왔어요 대비하라고",
          "original_form": "저희회사는 벌써 공문내러왔어요 대비하라고",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002838.1.9",
          "form": "그렇겠어요 \r\n부산에 대비안해서 지하도 사망 있었잖아요",
          "original_form": "그렇겠어요 \r\n부산에 대비안해서 지하도 사망 있었잖아요",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002838.1.10",
          "form": "근데 비님오시는데 저희가 막을길이 있나요? 대비는 무슨 ㅋㅋㅋ",
          "original_form": "근데 비님오시는데 저희가 막을길이 있나요? 대비는 무슨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002838.1.11",
          "form": "지금 중국 일본은 난리라는데요?",
          "original_form": "지금 중국 일본은 난리라는데요?",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002838.1.12",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002838.1.13",
          "form": "비가 많이와서요 잠겼나봐요",
          "original_form": "비가 많이와서요 잠겼나봐요",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002838.1.14",
          "form": "어머나 어째요?",
          "original_form": "어머나 어째요?",
          "speaker_id": "2",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002838.1.15",
          "form": "그래서 저희회사도 공문내려온거레요",
          "original_form": "그래서 저희회사도 공문내려온거레요",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002838.1.16",
          "form": "대비해서 나쁠건 없지요뭐 몸이 고생해서 그렇지",
          "original_form": "대비해서 나쁠건 없지요뭐 몸이 고생해서 그렇지",
