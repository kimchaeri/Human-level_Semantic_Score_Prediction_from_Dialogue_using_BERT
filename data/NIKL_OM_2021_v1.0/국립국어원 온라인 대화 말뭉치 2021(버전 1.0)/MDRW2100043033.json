      "utterance": [
        {
          "id": "MDRW2100043033.1.1",
          "form": "오늘 뭐 드셨어요?",
          "original_form": "오늘 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043033.1.2",
          "form": "저는 방금 피자랑 스파게티 먹었어요",
          "original_form": "저는 방금 피자랑 스파게티 먹었어요",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100043033.1.3",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100043033.1.4",
          "form": "저는 그거 머야 편의점 김밥 먹었어요!",
          "original_form": "저는 그거 머야 편의점 김밥 먹었어요!",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043033.1.5",
          "form": "저는 저녁에 비빔밥 먹었어요",
          "original_form": "저는 저녁에 비빔밥 먹었어요",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043033.1.6",
          "form": "김밥 하나면 양 괜찮으신가요?",
          "original_form": "김밥 하나면 양 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043033.1.7",
          "form": "아니요 안괜찮아서 물 한병 원샷 했어요..",
          "original_form": "아니요 안괜찮아서 물 한병 원샷 했어요..",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043033.1.8",
          "form": "삼겹살 김밥이래서 기대했는데",
          "original_form": "삼겹살 김밥이래서 기대했는데",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043033.1.9",
          "form": "맛도 진짜 짜증났어요 그냥 비계맛만 났어요 ㅠ",
          "original_form": "맛도 진짜 짜증났어요 그냥 비계맛만 났어요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043033.1.10",
          "form": "비빔밥 재료 뭐 넣으셨어요? 알려주세요 상상으로 먹을래요 ㅠ",
          "original_form": "비빔밥 재료 뭐 넣으셨어요? 알려주세요 상상으로 먹을래요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043033.1.11",
          "form": "편의점은 삼각김밥만 맛나더라구요ㅜ",
          "original_form": "편의점은 삼각김밥만 맛나더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043033.1.12",
          "form": "콩나물 버섯 무생채 고사리 오이 이렇게 넣어서 먹었어요",
          "original_form": "콩나물 버섯 무생채 고사리 오이 이렇게 넣어서 먹었어요",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043033.1.13",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 양념은요!",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 양념은요!",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043033.1.14",
          "form": "고추장이랑 참기름요~ㅎㅎ",
          "original_form": "고추장이랑 참기름요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043033.1.15",
          "form": "하 저는 재료중에 고사리가 제일 땡겨요",
          "original_form": "하 저는 재료중에 고사리가 제일 땡겨요",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043033.1.16",
          "form": "포도도 후식으로 먹었어요",
          "original_form": "포도도 후식으로 먹었어요",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043033.1.17",
          "form": "ㅠㅠㅠㅠ 오늘 잘드셨네요",
          "original_form": "ㅠㅠㅠㅠ 오늘 잘드셨네요",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043033.1.18",
          "form": "고사리  좋아하시군요~~",
          "original_form": "고사리  좋아하시군요~~",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043033.1.19",
          "form": "사실 저는 친정엄마가 살림 봐주셔서 가능해요~ㅎㅎ",
          "original_form": "사실 저는 친정엄마가 살림 봐주셔서 가능해요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
