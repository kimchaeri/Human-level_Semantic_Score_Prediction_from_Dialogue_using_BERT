      "utterance": [
        {
          "id": "MDRW2100012994.1.1",
          "form": "여행 자주 다니셨어요?",
          "original_form": "여행 자주 다니셨어요?",
          "speaker_id": "1",
          "time": "20210825 10:11"
        },
        {
          "id": "MDRW2100012994.1.2",
          "form": "요샌 코로나로 못가요",
          "original_form": "요샌 코로나로 못가요",
          "speaker_id": "2",
          "time": "20210825 10:15"
        },
        {
          "id": "MDRW2100012994.1.3",
          "form": "다행이 시골이라",
          "original_form": "다행이 시골이라",
          "speaker_id": "2",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012994.1.4",
          "form": "자주 드라이브가요",
          "original_form": "자주 드라이브가요",
          "speaker_id": "2",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012994.1.5",
          "form": "바람쐬고 숨쉬고 살아요",
          "original_form": "바람쐬고 숨쉬고 살아요",
          "speaker_id": "2",
          "time": "20210825 10:17"
        },
        {
          "id": "MDRW2100012994.1.6",
          "form": "휴가 어디 가셨어요",
          "original_form": "휴가 어디 가셨어요",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012994.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012994.1.8",
          "form": "그쵸ㅠ 전 원래 여행 많이 안 다녔는데 요즘은 너무 가고 싶더라고요.",
          "original_form": "그쵸ㅠ 전 원래 여행 많이 안 다녔는데 요즘은 너무 가고 싶더라고요.",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012994.1.9",
          "form": "이번에 지리산 쪽에 펜션 하루 다녀왔어요.",
          "original_form": "이번에 지리산 쪽에 펜션 하루 다녀왔어요.",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012994.1.10",
          "form": "오자마자 애들 감기로 고생해서ㅋㅋㅋ 당분간은 어디 안 가려구요",
          "original_form": "오자마자 애들 감기로 고생해서ㅋㅋㅋ 당분간은 어디 안 가려구요",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012994.1.11",
          "form": "안가도 이리가치면 가고싶게돼요",
          "original_form": "안가도 이리가치면 가고싶게돼요",
          "speaker_id": "2",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012994.1.12",
          "form": "못하게 하면 괜히 하고싶죠",
          "original_form": "못하게 하면 괜히 하고싶죠",
          "speaker_id": "2",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012994.1.13",
          "form": "오 지리산 넘좋은데요",
          "original_form": "오 지리산 넘좋은데요",
          "speaker_id": "2",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012994.1.14",
          "form": "전 숙박은 못했네요 ㅜㅜ",
          "original_form": "전 숙박은 못했네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012994.1.15",
          "form": "감기 맞아요",
          "original_form": "감기 맞아요",
          "speaker_id": "2",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012994.1.16",
          "form": "애들은 놀면 걸려서 힘들어요  고생하셨어요 ㅜㅜ",
          "original_form": "애들은 놀면 걸려서 힘들어요  고생하셨어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012994.1.17",
          "form": "아빠가 하루 놀다 가라고 잡아주셨는데 고생만 하다 왔어요ㅋㅋ",
          "original_form": "아빠가 하루 놀다 가라고 잡아주셨는데 고생만 하다 왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012994.1.18",
          "form": "애들 어릴 땐 집이 최고...ㅋㅋ",
          "original_form": "애들 어릴 땐 집이 최고...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012994.1.19",
          "form": "네 애들과 사실 집이 최고죠",
          "original_form": "네 애들과 사실 집이 최고죠",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012994.1.20",
          "form": "아버님이 신경써주셨는데 아파서 아쉬운 여행됐네여",
          "original_form": "아버님이 신경써주셨는데 아파서 아쉬운 여행됐네여",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012994.1.21",
          "form": "그래도 아빠가 최곱니다",
          "original_form": "그래도 아빠가 최곱니다",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012994.1.22",
          "form": "넹 맞아요 아빠가 최고ㅎㅎ",
          "original_form": "넹 맞아요 아빠가 최고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100012994.1.23",
          "form": "신혼여행은 어디로 다녀오셨어요?",
          "original_form": "신혼여행은 어디로 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100012994.1.24",
          "form": "태국이요",
          "original_form": "태국이요",
          "speaker_id": "2",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100012994.1.25",
          "form": "윽 정말 별로였어요 ㅋㅋ",
          "original_form": "윽 정말 별로였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100012994.1.26",
          "form": "어디 가셨어요?",
          "original_form": "어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210825 11:15"
        },
        {
          "id": "MDRW2100012994.1.27",
          "form": "이상한 쇼를 대리고 가고 침대도 안좋고",
          "original_form": "이상한 쇼를 대리고 가고 침대도 안좋고",
          "speaker_id": "2",
          "time": "20210825 11:19"
        },
        {
          "id": "MDRW2100012994.1.28",
          "form": "전.하와이요~",
          "original_form": "전.하와이요~",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100012994.1.29",
          "form": "근데...저도 숙소가 별로였어서 ㅠㅠ",
          "original_form": "근데...저도 숙소가 별로였어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100012994.1.30",
          "form": "오좋은데 가셨네요",
          "original_form": "오좋은데 가셨네요",
          "speaker_id": "2",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100012994.1.31",
          "form": "저도 숙소가 ㅜㅜ완전 꽝요",
          "original_form": "저도 숙소가 ㅜㅜ완전 꽝요",
          "speaker_id": "2",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100012994.1.32",
          "form": "속았어요",
          "original_form": "속았어요",
          "speaker_id": "2",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100012994.1.33",
          "form": "ㅋㅋㅋ이때 아니면 언제 멀리 가나 싶어서 갔어요",
          "original_form": "ㅋㅋㅋ이때 아니면 언제 멀리 가나 싶어서 갔어요",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100012994.1.34",
          "form": "잘하셨어요 애크고야 겨우갔어요",
          "original_form": "잘하셨어요 애크고야 겨우갔어요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100012994.1.35",
          "form": "무조건 좋은데 가야해요",
          "original_form": "무조건 좋은데 가야해요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100012994.1.36",
          "form": "담번에 여행가면 숙소는 무조건 좋은데로 잡을거에요!",
          "original_form": "담번에 여행가면 숙소는 무조건 좋은데로 잡을거에요!",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100012994.1.37",
          "form": "맞아요 숙소 젤좋게요 ㅎㅎ",
          "original_form": "맞아요 숙소 젤좋게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:13"
        }
      ]
    }
  ]
