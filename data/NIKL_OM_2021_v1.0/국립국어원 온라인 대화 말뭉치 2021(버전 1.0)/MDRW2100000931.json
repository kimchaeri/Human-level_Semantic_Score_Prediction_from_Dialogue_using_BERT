      "utterance": [
        {
          "id": "MDRW2100000931.1.1",
          "form": "요즘 애들 자는 시간이 좀 늦어졌어",
          "original_form": "요즘 애들 자는 시간이 좀 늦어졌어",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000931.1.2",
          "form": "몇 시에 자는데?",
          "original_form": "몇 시에 자는데?",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000931.1.3",
          "form": "10시에 누우면 잠드는데 30분은 걸림",
          "original_form": "10시에 누우면 잠드는데 30분은 걸림",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000931.1.4",
          "form": "이제 스스로 자야 할텐데",
          "original_form": "이제 스스로 자야 할텐데",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000931.1.5",
          "form": "언제쯤 스스로 잘지",
          "original_form": "언제쯤 스스로 잘지",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000931.1.6",
          "form": "엄마가 너무 잘해줘서 그래",
          "original_form": "엄마가 너무 잘해줘서 그래",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000931.1.7",
          "form": "ㅎㅎ 다른 집도 자다 그래",
          "original_form": "ㅎㅎ 다른 집도 자다 그래",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000931.1.8",
          "form": "이제 고학년인데 언제까지 재워줘 ㅋ",
          "original_form": "이제 고학년인데 언제까지 재워줘 ㅋ",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000931.1.9",
          "form": "그냥 옆에 있는 게 편한가봐",
          "original_form": "그냥 옆에 있는 게 편한가봐",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000931.1.10",
          "form": "언젠가 혼자 자겠지",
          "original_form": "언젠가 혼자 자겠지",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000931.1.11",
          "form": "훌 그러면 안돼",
          "original_form": "훌 그러면 안돼",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000931.1.12",
          "form": "때 되면 떼어놔야지",
          "original_form": "때 되면 떼어놔야지",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000931.1.13",
          "form": "겁도 많도 혼자 있으면 무서워하니까",
          "original_form": "겁도 많도 혼자 있으면 무서워하니까",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000931.1.14",
          "form": "마음이 약해서 탈이네",
          "original_form": "마음이 약해서 탈이네",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000931.1.15",
          "form": "다른 집은 12시에도 잔대 그에 비하면 양반이지 ㅎㅎ",
          "original_form": "다른 집은 12시에도 잔대 그에 비하면 양반이지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000931.1.16",
          "form": "일찍 불 꺼버려야지 12시에 자면 키 안 커",
          "original_form": "일찍 불 꺼버려야지 12시에 자면 키 안 커",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000931.1.17",
          "form": "그렇다더만~ 일찍자야 성장호르몬 뿜뿜 ㅎㅎ",
          "original_form": "그렇다더만~ 일찍자야 성장호르몬 뿜뿜 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000931.1.18",
          "form": "응 운동하고 일찍 자고",
          "original_form": "응 운동하고 일찍 자고",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000931.1.19",
          "form": "그래야 다음 날도 덜 피곤하지",
          "original_form": "그래야 다음 날도 덜 피곤하지",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000931.1.20",
          "form": "그렇지 두뇌회전도 잘되고",
          "original_form": "그렇지 두뇌회전도 잘되고",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000931.1.21",
          "form": "ㅎㅎ 혼자 자고 싶다 편하게",
          "original_form": "ㅎㅎ 혼자 자고 싶다 편하게",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000931.1.22",
          "form": "근데 이런 말 하면 애들이 서운해해 ㅋㅋㅋㅋ",
          "original_form": "근데 이런 말 하면 애들이 서운해해 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000931.1.23",
          "form": "서운하긴~~ 아빠가 더 서운하다고 해 ㅋ",
          "original_form": "서운하긴~~ 아빠가 더 서운하다고 해 ㅋ",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000931.1.24",
          "form": "왜 아빠는 혼자 잠들어서? ㅋㅋ",
          "original_form": "왜 아빠는 혼자 잠들어서? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000931.1.25",
          "form": "응 아빠도 혼자 자면 무섭거든!",
          "original_form": "응 아빠도 혼자 자면 무섭거든!",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000931.1.26",
          "form": "ㅋㅋㅋ 넘 웃경",
          "original_form": "ㅋㅋㅋ 넘 웃경",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000931.1.27",
          "form": "흥~ 진심임",
          "original_form": "흥~ 진심임",
          "speaker_id": "1",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000931.1.28",
          "form": "눼눼",
          "original_form": "눼눼",
          "speaker_id": "2",
