          "original_form": "단우산파",
          "speaker_id": "2",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042206.1.6",
          "form": "너는어때",
          "original_form": "너는어때",
          "speaker_id": "2",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042206.1.7",
          "form": "어 나도 ㅋㅋㅋㅋㅋ",
          "original_form": "어 나도 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042206.1.8",
          "form": "장우산이 쓰기 편하고 이쁜데",
          "original_form": "장우산이 쓰기 편하고 이쁜데",
          "speaker_id": "1",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042206.1.9",
          "form": "아무래도 갖고 다니기엔 단우산이지",
          "original_form": "아무래도 갖고 다니기엔 단우산이지",
          "speaker_id": "1",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042206.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210907 08:15"
        },
        {
          "id": "MDRW2100042206.1.11",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "2",
          "time": "20210907 08:15"
        },
        {
          "id": "MDRW2100042206.1.12",
          "form": "차를산다면어떨까",
          "original_form": "차를산다면어떨까",
          "speaker_id": "2",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100042206.1.13",
          "form": "장우산들고다녀도",
          "original_form": "장우산들고다녀도",
          "speaker_id": "2",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100042206.1.14",
          "form": "무겁지않아",
          "original_form": "무겁지않아",
          "speaker_id": "2",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100042206.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 차 사는돈이 어마무지하잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 차 사는돈이 어마무지하잖아",
          "speaker_id": "1",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042206.1.16",
          "form": "ㅋㅋㅋㅋ우리회사사람들",
          "original_form": "ㅋㅋㅋㅋ우리회사사람들",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042206.1.17",
          "form": "차할부갚으려고",
          "original_form": "차할부갚으려고",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042206.1.18",
          "form": "회사다니잖앜ㅋㅋ",
          "original_form": "회사다니잖앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042206.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 강제적 자발근무 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 강제적 자발근무 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042206.1.20",
          "form": "내 친구도 신용카드",
          "original_form": "내 친구도 신용카드",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042206.1.21",
          "form": "할부 갚으려고 회사 다닌다고",
          "original_form": "할부 갚으려고 회사 다닌다고",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042206.1.22",
          "form": "우스갯소리 하던데 ㅋㅋㅋㅋ",
          "original_form": "우스갯소리 하던데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042206.1.23",
          "form": "ㅋㅋㅋㅋㅋ그거맏따",
          "original_form": "ㅋㅋㅋㅋㅋ그거맏따",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042206.1.24",
          "form": "사면 할부노예..",
          "original_form": "사면 할부노예..",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042206.1.25",
          "form": "다나가욧",
          "original_form": "다나가욧",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042206.1.26",
          "form": "나도 이번에 신용카 하나 만들라궁..",
          "original_form": "나도 이번에 신용카 하나 만들라궁..",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042206.1.27",
          "form": "오오 국민이겠지?",
          "original_form": "오오 국민이겠지?",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042206.1.28",
          "form": "아니 신한이나 삼성 생각중?!",
          "original_form": "아니 신한이나 삼성 생각중?!",
          "speaker_id": "1",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042206.1.29",
          "form": "오호 삼성도괜찮지",
          "original_form": "오호 삼성도괜찮지",
          "speaker_id": "2",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100042206.1.30",
          "form": "근데나신카한달써봤는데",
          "original_form": "근데나신카한달써봤는데",
          "speaker_id": "2",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100042206.1.31",
          "form": "30만채우기 개힘들더라..",
          "original_form": "30만채우기 개힘들더라..",
          "speaker_id": "2",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100042206.1.32",
          "form": "ㅠㅠ 하 하필 우리 밥도 대표가 내고 카카오로 전달하는 방식이라",
          "original_form": "ㅠㅠ 하 하필 우리 밥도 대표가 내고 카카오로 전달하는 방식이라",
          "speaker_id": "1",
          "time": "20210907 10:22"
        },
        {
          "id": "MDRW2100042206.1.33",
          "form": "나도 30만 못채울거 같긴한데...습",
          "original_form": "나도 30만 못채울거 같긴한데...습",
          "speaker_id": "1",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100042206.1.34",
          "form": "그래서 네페 신용 만들까 싶음",
          "original_form": "그래서 네페 신용 만들까 싶음",
          "speaker_id": "1",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100042206.1.35",
          "form": "그게나을거같기도ㅜㅜ",
          "original_form": "그게나을거같기도ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042206.1.36",
          "form": "진짜 캥거루족이 쓰기힘들더라",
