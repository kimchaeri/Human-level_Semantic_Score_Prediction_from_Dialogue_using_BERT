      "utterance": [
        {
          "id": "MDRW2100046379.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211020 14:59"
        },
        {
          "id": "MDRW2100046379.1.2",
          "form": "안녕하세요! 영화보는거 좋아하시나요?",
          "original_form": "안녕하세요! 영화보는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211020 15:00"
        },
        {
          "id": "MDRW2100046379.1.3",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20211020 15:00"
        },
        {
          "id": "MDRW2100046379.1.4",
          "form": "스릴러물",
          "original_form": "스릴러물",
          "speaker_id": "1",
          "time": "20211020 15:00"
        },
        {
          "id": "MDRW2100046379.1.5",
          "form": "오 요즘 넷플릭스에 스릴러 많이 나오지 않았나요?!",
          "original_form": "오 요즘 넷플릭스에 스릴러 많이 나오지 않았나요?!",
          "speaker_id": "2",
          "time": "20211020 15:00"
        },
        {
          "id": "MDRW2100046379.1.6",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "1",
          "time": "20211020 15:01"
        },
        {
          "id": "MDRW2100046379.1.7",
          "form": "오징어게임 잼나게 봤어요",
          "original_form": "오징어게임 잼나게 봤어요",
          "speaker_id": "1",
          "time": "20211020 15:01"
        },
        {
          "id": "MDRW2100046379.1.8",
          "form": "오징어 게임 요즘 엄청 유명하잖아요",
          "original_form": "오징어 게임 요즘 엄청 유명하잖아요",
          "speaker_id": "2",
          "time": "20211020 15:02"
        },
        {
          "id": "MDRW2100046379.1.9",
          "form": "거기 나온 게임들도 요즘 유행이던데",
          "original_form": "거기 나온 게임들도 요즘 유행이던데",
          "speaker_id": "2",
          "time": "20211020 15:02"
        },
        {
          "id": "MDRW2100046379.1.10",
          "form": "달고나 뽑기 같은거 해보셨어요?",
          "original_form": "달고나 뽑기 같은거 해보셨어요?",
          "speaker_id": "2",
          "time": "20211020 15:02"
        },
        {
          "id": "MDRW2100046379.1.11",
          "form": "어릴때 해보고 안해봤죠 ㅋㅋ",
          "original_form": "어릴때 해보고 안해봤죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 15:03"
        },
        {
          "id": "MDRW2100046379.1.12",
          "form": "앗 ㅋㅋㅋ 그래도 요즘 오겜 덕분에",
          "original_form": "앗 ㅋㅋㅋ 그래도 요즘 오겜 덕분에",
          "speaker_id": "2",
          "time": "20211020 15:03"
        },
        {
          "id": "MDRW2100046379.1.13",
          "form": "길에서 많이 보이는 것 같아요",
          "original_form": "길에서 많이 보이는 것 같아요",
          "speaker_id": "2",
          "time": "20211020 15:03"
        },
        {
          "id": "MDRW2100046379.1.14",
          "form": "맞아요 근데 너무 비싸던데요 ㅋ",
          "original_form": "맞아요 근데 너무 비싸던데요 ㅋ",
          "speaker_id": "1",
          "time": "20211020 15:03"
        },
        {
          "id": "MDRW2100046379.1.15",
          "form": "헉 그런가요",
          "original_form": "헉 그런가요",
          "speaker_id": "2",
          "time": "20211020 15:04"
        },
        {
          "id": "MDRW2100046379.1.16",
          "form": "가격은 잘 안보고 지나쳐서 몰랐네요",
          "original_form": "가격은 잘 안보고 지나쳐서 몰랐네요",
          "speaker_id": "2",
          "time": "20211020 15:04"
        },
        {
          "id": "MDRW2100046379.1.17",
          "form": "네 물들어올때 노젓는듯",
          "original_form": "네 물들어올때 노젓는듯",
          "speaker_id": "1",
          "time": "20211020 15:04"
        },
        {
          "id": "MDRW2100046379.1.18",
          "form": "ㅋㅋ모든 콘텐츠가 다 그렇죠 뭐..",
          "original_form": "ㅋㅋ모든 콘텐츠가 다 그렇죠 뭐..",
          "speaker_id": "2",
          "time": "20211020 15:04"
        },
        {
          "id": "MDRW2100046379.1.19",
          "form": "맞아요 ㅋ",
          "original_form": "맞아요 ㅋ",
          "speaker_id": "1",
          "time": "20211020 15:05"
        },
        {
          "id": "MDRW2100046379.1.20",
          "form": "그래도 옛날에 보던걸 다시보니 반갑기도 하고 그러네요!",
          "original_form": "그래도 옛날에 보던걸 다시보니 반갑기도 하고 그러네요!",
          "speaker_id": "2",
          "time": "20211020 15:05"
        },
        {
          "id": "MDRW2100046379.1.21",
          "form": "네 옛날 기억이 새록새록나요",
          "original_form": "네 옛날 기억이 새록새록나요",
          "speaker_id": "1",
          "time": "20211020 15:05"
        },
        {
          "id": "MDRW2100046379.1.22",
          "form": "저도 국자 태워먹던 기억이..ㅋㅋ",
          "original_form": "저도 국자 태워먹던 기억이..ㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 15:06"
        },
        {
          "id": "MDRW2100046379.1.23",
          "form": "많이 태워먹었죠 ㅋㅋ",
          "original_form": "많이 태워먹었죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 15:06"
        }
      ]
