      "utterance": [
        {
          "id": "MDRW2100039601.1.1",
          "form": "네 문 닫고 자요",
          "original_form": "네 문 닫고 자요",
          "speaker_id": "2",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.2",
          "form": "저도 문닫고",
          "original_form": "저도 문닫고",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.3",
          "form": "이불덮고 자요 ㅎㅎ",
          "original_form": "이불덮고 자요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.4",
          "form": "네 이제 문열고 자면 춥더라구요",
          "original_form": "네 이제 문열고 자면 춥더라구요",
          "speaker_id": "2",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.5",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.6",
          "form": "급 가을.. ㅜㅜ",
          "original_form": "급 가을.. ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.7",
          "form": "가을 싫으세요?",
          "original_form": "가을 싫으세요?",
          "speaker_id": "2",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.8",
          "form": "좋은데",
          "original_form": "좋은데",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.9",
          "form": "시간이 많이 간 것 같아서요 ㅠㅠ",
          "original_form": "시간이 많이 간 것 같아서요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.10",
          "form": "아 그런건 있죠",
          "original_form": "아 그런건 있죠",
          "speaker_id": "2",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039601.1.11",
          "form": "네 확실히 센치해져요",
          "original_form": "네 확실히 센치해져요",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039601.1.12",
          "form": "네 뭔가 씁쓸한??",
          "original_form": "네 뭔가 씁쓸한??",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039601.1.13",
          "form": "네 완전 센티멘털.. ㅜㅜ",
          "original_form": "네 완전 센티멘털.. ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039601.1.14",
          "form": "멜랑꼴리..",
          "original_form": "멜랑꼴리..",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039601.1.15",
          "form": "가을이 그런게 있나봐요",
          "original_form": "가을이 그런게 있나봐요",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039601.1.16",
          "form": "네 분위기가 좀 그래요 ㅎㅎ",
          "original_form": "네 분위기가 좀 그래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039601.1.17",
          "form": "그래도 날은 딱 좋아요",
          "original_form": "그래도 날은 딱 좋아요",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039601.1.18",
          "form": "네 산책하기 좋은 날씨",
          "original_form": "네 산책하기 좋은 날씨",
          "speaker_id": "1",
          "time": "20210903 19:29"
        }
