      "utterance": [
        {
          "id": "MDRW2100043612.1.1",
          "form": "다들 다급해졌어요",
          "original_form": "다들 다급해졌어요",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043612.1.2",
          "form": "많이 하셨나요?",
          "original_form": "많이 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043612.1.3",
          "form": "그니까요 ㅋㅋㅋㅋ아니 어쩐지 진짜 다들 글리젠 ..엄청 빠르더라..",
          "original_form": "그니까요 ㅋㅋㅋㅋ아니 어쩐지 진짜 다들 글리젠 ..엄청 빠르더라..",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043612.1.4",
          "form": "저 지금 백삼십몇개",
          "original_form": "저 지금 백삼십몇개",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043612.1.5",
          "form": "아...300개는 무리겟죠..흑",
          "original_form": "아...300개는 무리겟죠..흑",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043612.1.6",
          "form": "모든사람한테 대화를 걸어보는건 어때요?",
          "original_form": "모든사람한테 대화를 걸어보는건 어때요?",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043612.1.7",
          "form": "ㅋㅋㅋㅋ저 오늘 말 많이걸넜는데",
          "original_form": "ㅋㅋㅋㅋ저 오늘 말 많이걸넜는데",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043612.1.8",
          "form": "아 족금 힘드내요..ㅎ",
          "original_form": "아 족금 힘드내요..ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043612.1.9",
          "form": "너무 많이하면 제 기가 빨리는거같아욬ㅋㅋㅋㅋ",
          "original_form": "너무 많이하면 제 기가 빨리는거같아욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043612.1.10",
          "form": "떼잉...쩔 수 없져 머",
          "original_form": "떼잉...쩔 수 없져 머",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043612.1.11",
          "form": "저도 많은 사람 만나면",
          "original_form": "저도 많은 사람 만나면",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043612.1.12",
          "form": "기가빨리는편이에요ㅋㅋ",
          "original_form": "기가빨리는편이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043612.1.13",
          "form": "ㅋㅋㅋㅋ비슷한 주제로 ㄱㅖ속 말하여니까",
          "original_form": "ㅋㅋㅋㅋ비슷한 주제로 ㄱㅖ속 말하여니까",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043612.1.14",
          "form": "로봇된거같고..ㅋㅋㅋ",
          "original_form": "로봇된거같고..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043612.1.15",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043612.1.16",
          "form": "오늘이 시작한이후로",
          "original_form": "오늘이 시작한이후로",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043612.1.17",
          "form": "최고 많이 대화하는 날이에요",
          "original_form": "최고 많이 대화하는 날이에요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043612.1.18",
          "form": "저도욬ㅋㅋㅋㅋ",
          "original_form": "저도욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043612.1.19",
          "form": "거의 들디님이랑만 햇는데",
          "original_form": "거의 들디님이랑만 햇는데",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043612.1.20",
          "form": "막 새로운 사람도 엄청 말걸어요 ㅋㅋㅋㅋㅋ",
          "original_form": "막 새로운 사람도 엄청 말걸어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043612.1.21",
          "form": "다들 마음이 급한갑네요",
          "original_form": "다들 마음이 급한갑네요",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043612.1.22",
          "form": "비대면으로 정말 많은 사람을 만나네요",
          "original_form": "비대면으로 정말 많은 사람을 만나네요",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043612.1.23",
          "form": "그니가요 ㅋㅋㅋ",
          "original_form": "그니가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043612.1.24",
          "form": "아 요즘도 학교 수업 줌인가요?",
          "original_form": "아 요즘도 학교 수업 줌인가요?",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043612.1.25",
          "form": "네!!! 실험과목 제외하고요",
          "original_form": "네!!! 실험과목 제외하고요",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043612.1.26",
          "form": "오 실험은 가서 하는군요",
          "original_form": "오 실험은 가서 하는군요",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043612.1.27",
          "form": "그래도 그렇게라도 학교가는게 요즘의 낙일듯.",
          "original_form": "그래도 그렇게라도 학교가는게 요즘의 낙일듯.",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043612.1.28",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043612.1.29",
          "form": "전 실험이없어서ㅋㅋ",
          "original_form": "전 실험이없어서ㅋㅋ",
