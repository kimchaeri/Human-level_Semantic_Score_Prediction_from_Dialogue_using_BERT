      "utterance": [
        {
          "id": "MDRW2100023892.1.1",
          "form": "봉지채로 사야겠네요ㅋㅋㅋ",
          "original_form": "봉지채로 사야겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023892.1.2",
          "form": "생필품같은건 어디서 구매하세요?",
          "original_form": "생필품같은건 어디서 구매하세요?",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023892.1.3",
          "form": "저는 주로 네이버랑 11번가 많이 이용해요",
          "original_form": "저는 주로 네이버랑 11번가 많이 이용해요",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023892.1.4",
          "form": "오리덕님은요?",
          "original_form": "오리덕님은요?",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023892.1.5",
          "form": "저도 네이버페이로 사면 포인트 적립해줘서 네이버쇼핑 많이 이용하고",
          "original_form": "저도 네이버페이로 사면 포인트 적립해줘서 네이버쇼핑 많이 이용하고",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023892.1.6",
          "form": "그때그때 원플러스원하거나",
          "original_form": "그때그때 원플러스원하거나",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023892.1.7",
          "form": "세일품목 많은 쇼핑몰로 가요ㅋㅋㅋ",
          "original_form": "세일품목 많은 쇼핑몰로 가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023892.1.8",
          "form": "저도 거의 그런편이에요ㅋㅋㅋ",
          "original_form": "저도 거의 그런편이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023892.1.9",
          "form": "쿠폰에 따라서 가격차이 너무 심하더라고요",
          "original_form": "쿠폰에 따라서 가격차이 너무 심하더라고요",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023892.1.10",
          "form": "그래서 각종 마트 다 가입되어있어요ㅋㅋㅋ",
          "original_form": "그래서 각종 마트 다 가입되어있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023892.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023892.1.12",
          "form": "저도요ㅋㅋㅋㅋ다해요",
          "original_form": "저도요ㅋㅋㅋㅋ다해요",
          "speaker_id": "2",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023892.1.13",
          "form": "요즘 이마트에서 주말마다 가끔",
          "original_form": "요즘 이마트에서 주말마다 가끔",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023892.1.14",
          "form": "원플러스원 세일 품목 몇개씩 잡아서 세일하던데",
          "original_form": "원플러스원 세일 품목 몇개씩 잡아서 세일하던데",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023892.1.15",
          "form": "그때 좀 많이 사는것같아요",
          "original_form": "그때 좀 많이 사는것같아요",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023892.1.16",
          "form": "오 그런게 있군요..!!!!",
          "original_form": "오 그런게 있군요..!!!!",
          "speaker_id": "2",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023892.1.17",
          "form": "저는 11번가 메인에 할인하는 품목위주로 사요ㅋㅋㅋㅋ",
          "original_form": "저는 11번가 메인에 할인하는 품목위주로 사요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023892.1.18",
          "form": "앗 저두요ㅋㅋㅋㅋ 베스트 품목 가끔 들어가서봐요",
          "original_form": "앗 저두요ㅋㅋㅋㅋ 베스트 품목 가끔 들어가서봐요",
          "speaker_id": "1",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023892.1.19",
          "form": "11절에도 사고ㅋㅋㅋ",
          "original_form": "11절에도 사고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023892.1.20",
          "form": "저도요ㅋㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023892.1.21",
          "form": "9월1일부터",
          "original_form": "9월1일부터",
          "speaker_id": "1",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023892.1.22",
          "form": "빅스마일데이 시작한대요!",
          "original_form": "빅스마일데이 시작한대요!",
          "speaker_id": "1",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023892.1.23",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023892.1.24",
          "form": "살거많았는데ㅎㅎ",
          "original_form": "살거많았는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023892.1.25",
          "form": "알려주셔서 감사해요!",
          "original_form": "알려주셔서 감사해요!",
          "speaker_id": "2",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023892.1.26",
          "form": "보통 첫날에는 세일 품목이 많은데",
          "original_form": "보통 첫날에는 세일 품목이 많은데",
          "speaker_id": "1",
          "time": "20210830 19:54"
        },
        {
          "id": "MDRW2100023892.1.27",
