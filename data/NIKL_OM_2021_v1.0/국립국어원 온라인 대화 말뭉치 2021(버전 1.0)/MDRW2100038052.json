      "utterance": [
        {
          "id": "MDRW2100038052.1.1",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100038052.1.2",
          "form": "너 그거 좋앟9?",
          "original_form": "너 그거 좋앟9?",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038052.1.3",
          "form": "너 핫도그 싫어하지않아?",
          "original_form": "너 핫도그 싫어하지않아?",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038052.1.4",
          "form": "아니 뭘 잘못알고있구나",
          "original_form": "아니 뭘 잘못알고있구나",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038052.1.5",
          "form": "나 핫도그라면 환장해",
          "original_form": "나 핫도그라면 환장해",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038052.1.6",
          "form": "근데 안먹은지 너무 오래되었어",
          "original_form": "근데 안먹은지 너무 오래되었어",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038052.1.7",
          "form": "너는 최근에 언제 먹었어?",
          "original_form": "너는 최근에 언제 먹었어?",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100038052.1.8",
          "form": "아 정말?",
          "original_form": "아 정말?",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.9",
          "form": "나도 안먹은지 오래됐어",
          "original_form": "나도 안먹은지 오래됐어",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.10",
          "form": "핫도그를 배달해먹기 좀 힘들어",
          "original_form": "핫도그를 배달해먹기 좀 힘들어",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.11",
          "form": "너무 여러개를 시켜야하는데 눈치보여",
          "original_form": "너무 여러개를 시켜야하는데 눈치보여",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.12",
          "form": "맞아 많이시켜야하지?",
          "original_form": "맞아 많이시켜야하지?",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.13",
          "form": "나는 맵구마릉 좋아해",
          "original_form": "나는 맵구마릉 좋아해",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.14",
          "form": "너는 뭐 좋아해?",
          "original_form": "너는 뭐 좋아해?",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.15",
          "form": "사실 난 다좋아",
          "original_form": "사실 난 다좋아",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.16",
          "form": "우리 곧 만나니까 그때먹을까??",
          "original_form": "우리 곧 만나니까 그때먹을까??",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.17",
          "form": "나도 맵구마 좋아해!",
          "original_form": "나도 맵구마 좋아해!",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.18",
          "form": "그 안에 치즈든것도 맛있잔항",
          "original_form": "그 안에 치즈든것도 맛있잔항",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.19",
          "form": "너 천재구나",
          "original_form": "너 천재구나",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.20",
          "form": "그거 은근히 맵더라",
          "original_form": "그거 은근히 맵더라",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100038052.1.21",
          "form": "엽떡이랑 같이 먹자",
          "original_form": "엽떡이랑 같이 먹자",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.22",
          "form": "맞아!!!@",
          "original_form": "맞아!!!@",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.23",
          "form": "겉에 포테이토 있는 것도 맛있어",
          "original_form": "겉에 포테이토 있는 것도 맛있어",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.24",
          "form": "냉동실에 넣어두고 담날 아침까지먹자",
          "original_form": "냉동실에 넣어두고 담날 아침까지먹자",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.25",
          "form": "맞아 치즈포테이토도 맛있어",
          "original_form": "맞아 치즈포테이토도 맛있어",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.26",
          "form": "우리 하루종일 먹는거랴?",
          "original_form": "우리 하루종일 먹는거랴?",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.27",
          "form": "응 그럼 뭐하려고?",
          "original_form": "응 그럼 뭐하려고?",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.28",
          "form": "맞아. 빨리 먹고싶다",
          "original_form": "맞아. 빨리 먹고싶다",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.29",
          "form": "먹는게 최고야",
          "original_form": "먹는게 최고야",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100038052.1.30",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "2",
          "time": "20210903 17:44"
        }
      ]
    }
