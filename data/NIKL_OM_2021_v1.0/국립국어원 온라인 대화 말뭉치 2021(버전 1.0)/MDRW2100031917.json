      "utterance": [
        {
          "id": "MDRW2100031917.1.1",
          "form": "우리 아버지 어머님께 잘해드려야해요ㅠ",
          "original_form": "우리 아버지 어머님께 잘해드려야해요ㅠ",
          "speaker_id": "1",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100031917.1.2",
          "form": "이제야 철들었어요ㅎㅎ 애가있다보니깐ㅎㅎ",
          "original_form": "이제야 철들었어요ㅎㅎ 애가있다보니깐ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031917.1.3",
          "form": "ㅋㅋ 다들 그렇더라고요",
          "original_form": "ㅋㅋ 다들 그렇더라고요",
          "speaker_id": "2",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031917.1.4",
          "form": "애기낳고나면 ㅋㅋ",
          "original_form": "애기낳고나면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031917.1.5",
          "form": "맞아요ㅎ 엄청들어갈때가 많아요ㅎㅎ",
          "original_form": "맞아요ㅎ 엄청들어갈때가 많아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031917.1.6",
          "form": "옷은 다 새로사야하지",
          "original_form": "옷은 다 새로사야하지",
          "speaker_id": "1",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031917.1.7",
          "form": "먹는건 아무거나 먹일수 있나요?ㅎ",
          "original_form": "먹는건 아무거나 먹일수 있나요?ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100031917.1.8",
          "form": "최고급은 아니어도 중간급 이상은 먹여야지ㅎ",
          "original_form": "최고급은 아니어도 중간급 이상은 먹여야지ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031917.1.9",
          "form": "ㅋㅋ아 진짜 애환이ㄱㅋ",
          "original_form": "ㅋㅋ아 진짜 애환이ㄱㅋ",
          "speaker_id": "2",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031917.1.10",
          "form": "그래서 꼭 싱글때 많이 쓰셔요ㅎㅎㅎㅎㅎㅎ",
          "original_form": "그래서 꼭 싱글때 많이 쓰셔요ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031917.1.11",
          "form": "애. 교육비 들기시작하면",
          "original_form": "애. 교육비 들기시작하면",
          "speaker_id": "2",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031917.1.12",
          "form": "나중에는 아이에게 남편에게만 들어갈꺼에요ㅎ",
          "original_form": "나중에는 아이에게 남편에게만 들어갈꺼에요ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031917.1.13",
          "form": "그러더라고요",
          "original_form": "그러더라고요",
          "speaker_id": "2",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031917.1.14",
          "form": "맞네요ㅠ 아직저는 애기가 어려서ㅠ 교육비는 안드는데",
          "original_form": "맞네요ㅠ 아직저는 애기가 어려서ㅠ 교육비는 안드는데",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100031917.1.15",
          "form": "자기옷 살거면 애기옷 사고",
          "original_form": "자기옷 살거면 애기옷 사고",
          "speaker_id": "2",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031917.1.16",
          "form": "어린이집ㄷㅎ 요즘 영어배우고 체육하고 그러네요",
          "original_form": "어린이집ㄷㅎ 요즘 영어배우고 체육하고 그러네요",
          "speaker_id": "1",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031917.1.17",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031917.1.18",
          "form": "슬프네요ㅠ",
          "original_form": "슬프네요ㅠ",
          "speaker_id": "1",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031917.1.19",
          "form": "커피도 카페안가고ㅜ편의점",
          "original_form": "커피도 카페안가고ㅜ편의점",
          "speaker_id": "2",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031917.1.20",
          "form": "맞습니다ㅎ",
          "original_form": "맞습니다ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031917.1.21",
          "form": "친구네는 쌍둥인데 인당 100인가 원비",
          "original_form": "친구네는 쌍둥인데 인당 100인가 원비",
          "speaker_id": "2",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031917.1.22",
          "form": "아이는 4살이에요ㅎㅎ",
          "original_form": "아이는 4살이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:29"
        },
        {
          "id": "MDRW2100031917.1.23",
          "form": "정말 살꺼 많아요ㅠㅠ",
          "original_form": "정말 살꺼 많아요ㅠㅠ",
