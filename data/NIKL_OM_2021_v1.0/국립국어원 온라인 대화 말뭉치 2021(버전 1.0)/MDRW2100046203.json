      "utterance": [
        {
          "id": "MDRW2100046203.1.1",
          "form": "반려동물 주제 재밌어요",
          "original_form": "반려동물 주제 재밌어요",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046203.1.2",
          "form": "저도 재밌네요 ㅎㅎ",
          "original_form": "저도 재밌네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046203.1.3",
          "form": "햄스터 너무 귀엽습니다...",
          "original_form": "햄스터 너무 귀엽습니다...",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046203.1.4",
          "form": "ㅎ키우면서 힐링이시겠어요",
          "original_form": "ㅎ키우면서 힐링이시겠어요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046203.1.5",
          "form": "맞아요 완전 힐링돼요",
          "original_form": "맞아요 완전 힐링돼요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046203.1.6",
          "form": "주인은 알아보나요?",
          "original_form": "주인은 알아보나요?",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046203.1.7",
          "form": "아마 못알아볼거예요",
          "original_form": "아마 못알아볼거예요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046203.1.8",
          "form": "화장실 가리는거보고 생각보다 똑똑한줄 알았는데",
          "original_form": "화장실 가리는거보고 생각보다 똑똑한줄 알았는데",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046203.1.9",
          "form": "지능이 그정도는 안되나봐요",
          "original_form": "지능이 그정도는 안되나봐요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046203.1.10",
          "form": "그렇군요...",
          "original_form": "그렇군요...",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046203.1.11",
          "form": "그래도 핸들링이라는게 있어요",
          "original_form": "그래도 핸들링이라는게 있어요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046203.1.12",
          "form": "핸들링은 뭔가요?",
          "original_form": "핸들링은 뭔가요?",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046203.1.13",
          "form": "손에 길들이는거예요",
          "original_form": "손에 길들이는거예요",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046203.1.14",
          "form": "오 그런것도 있군요",
          "original_form": "오 그런것도 있군요",
          "speaker_id": "1",
          "time": "20210908 11:12"
