      "utterance": [
        {
          "id": "MDRW2100005266.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005266.1.2",
          "form": "아니요 님은요?",
          "original_form": "아니요 님은요?",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005266.1.3",
          "form": "저도 아직",
          "original_form": "저도 아직",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005266.1.4",
          "form": "자녀있으시면 보통  애기들이 키우고싶어하던데",
          "original_form": "자녀있으시면 보통  애기들이 키우고싶어하던데",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005266.1.5",
          "form": "자녀도 힘드네요",
          "original_form": "자녀도 힘드네요",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005266.1.6",
          "form": "애완견 키우면 정서발달에도 좋대요",
          "original_form": "애완견 키우면 정서발달에도 좋대요",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005266.1.7",
          "form": "네 그렇다거 하더라구요",
          "original_form": "네 그렇다거 하더라구요",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005266.1.8",
          "form": "향후 키우실 생각은요?",
          "original_form": "향후 키우실 생각은요?",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005266.1.9",
          "form": "일하면 혼자 외로워요..동물이",
          "original_form": "일하면 혼자 외로워요..동물이",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005266.1.10",
          "form": "아내분 계시니까 괜찮지않나요",
          "original_form": "아내분 계시니까 괜찮지않나요",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005266.1.11",
          "form": "여행가면 데리고 가거나 맡겨야해서",
          "original_form": "여행가면 데리고 가거나 맡겨야해서",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005266.1.12",
          "form": "같이 일해서",
          "original_form": "같이 일해서",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005266.1.13",
          "form": "맞벌이시군요  그럼 동물키우는건 힘드실듯",
          "original_form": "맞벌이시군요  그럼 동물키우는건 힘드실듯",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005266.1.14",
          "form": "네 아무래도 그렇죠",
          "original_form": "네 아무래도 그렇죠",
          "speaker_id": "1",
