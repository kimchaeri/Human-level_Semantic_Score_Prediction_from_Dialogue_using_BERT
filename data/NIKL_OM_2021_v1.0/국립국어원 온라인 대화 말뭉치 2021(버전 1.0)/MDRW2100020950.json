      "utterance": [
        {
          "id": "MDRW2100020950.1.1",
          "form": "디저트 좋아하세요?",
          "original_form": "디저트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100020950.1.2",
          "form": "단것은 별로 안 좋아해요",
          "original_form": "단것은 별로 안 좋아해요",
          "speaker_id": "2",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100020950.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100020950.1.4",
          "form": "어 저랑 반대네요",
          "original_form": "어 저랑 반대네요",
          "speaker_id": "1",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100020950.1.5",
          "form": "단것 무척 좋아해요",
          "original_form": "단것 무척 좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100020950.1.6",
          "form": "자주 먹는 디저트가 있나요",
          "original_form": "자주 먹는 디저트가 있나요",
          "speaker_id": "2",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100020950.1.7",
          "form": "그때그때 달라요",
          "original_form": "그때그때 달라요",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100020950.1.8",
          "form": "요샌 레몬마들렌이 맛있어요",
          "original_form": "요샌 레몬마들렌이 맛있어요",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100020950.1.9",
          "form": "카페에 직접 가서 드시나요?",
          "original_form": "카페에 직접 가서 드시나요?",
          "speaker_id": "2",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100020950.1.10",
          "form": "아님 빵집서 파나요",
          "original_form": "아님 빵집서 파나요",
          "speaker_id": "2",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100020950.1.11",
          "form": "배달앱에서",
          "original_form": "배달앱에서",
          "speaker_id": "2",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100020950.1.12",
          "form": "배달앱에서 시켜먹어요",
          "original_form": "배달앱에서 시켜먹어요",
          "speaker_id": "1",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100020950.1.13",
          "form": "오 근처에 디저트 맛집이 있나봐요",
          "original_form": "오 근처에 디저트 맛집이 있나봐요",
          "speaker_id": "1",
          "time": "20210828 16:20"
        },
        {
          "id": "MDRW2100020950.1.14",
          "form": "검색하면 리뷰 좋은 곳이 나와요",
          "original_form": "검색하면 리뷰 좋은 곳이 나와요",
          "speaker_id": "2",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100020950.1.15",
          "form": "? 아니면 원래 알던 맛집인가요",
          "original_form": "? 아니면 원래 알던 맛집인가요",
          "speaker_id": "2",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100020950.1.16",
          "form": "별점 찜 높은 곳 시키면 실패는 잘 안하더라고요",
          "original_form": "별점 찜 높은 곳 시키면 실패는 잘 안하더라고요",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100020950.1.17",
          "form": "최근에 뭘 주문했어요?",
          "original_form": "최근에 뭘 주문했어요?",
          "speaker_id": "2",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100020950.1.18",
          "form": "흑당밀크티를 주문했어요",
          "original_form": "흑당밀크티를 주문했어요",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100020950.1.19",
          "form": "와플과 함께요",
          "original_form": "와플과 함께요",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100020950.1.20",
          "form": "크로플로 주문하셨어요",
          "original_form": "크로플로 주문하셨어요",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100020950.1.21",
          "form": "아님 기본으로?",
          "original_form": "아님 기본으로?",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100020950.1.22",
          "form": "와플은 기본이 진리입니다ㅎㅎ",
          "original_form": "와플은 기본이 진리입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100020950.1.23",
          "form": "요새 와플이 너무 변했어요",
          "original_form": "요새 와플이 너무 변했어요",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100020950.1.24",
          "form": "크로플은 특히 최악",
          "original_form": "크로플은 특히 최악",
          "speaker_id": "1",
          "time": "20210828 16:25"
        }
      ]
