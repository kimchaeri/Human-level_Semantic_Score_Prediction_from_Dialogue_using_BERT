      "utterance": [
        {
          "id": "MDRW2100026162.1.1",
          "form": "name1이에용",
          "original_form": "&name1&이에용",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026162.1.2",
          "form": "안녕 안녕",
          "original_form": "안녕 안녕",
          "speaker_id": "2",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100026162.1.3",
          "form": "안녕~",
          "original_form": "안녕~",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026162.1.4",
          "form": "name1 너 아르바이트는 할 생각 없댔지",
          "original_form": "&name1& 너 아르바이트는 할 생각 없댔지",
          "speaker_id": "2",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026162.1.5",
          "form": "아 아파요",
          "original_form": "아 아파요",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026162.1.6",
          "form": "아니 근데 할 생각은 있는데 시간이 없어",
          "original_form": "아니 근데 할 생각은 있는데 시간이 없어",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026162.1.7",
          "form": "하긴 name1 요즘 바쁘니까",
          "original_form": "하긴 &name1& 요즘 바쁘니까",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026162.1.8",
          "form": "나도 올해는 불가능일 듯",
          "original_form": "나도 올해는 불가능일 듯",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026162.1.9",
          "form": "내년에 바리스타 자격증 따서 카페 알바라도 도전해볼까봐",
          "original_form": "내년에 바리스타 자격증 따서 카페 알바라도 도전해볼까봐",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026162.1.10",
          "form": "내년에 휴학하면 알바",
          "original_form": "내년에 휴학하면 알바",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026162.1.11",
          "form": "하려고?",
          "original_form": "하려고?",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026162.1.12",
          "form": "멋져",
          "original_form": "멋져",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026162.1.13",
          "form": "나는 부업처럼 그림 그려서 용돈 벌이",
          "original_form": "나는 부업처럼 그림 그려서 용돈 벌이",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026162.1.14",
          "form": "하는정도로 만족하려고",
          "original_form": "하는정도로 만족하려고",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026162.1.15",
          "form": "멋져 멋져",
          "original_form": "멋져 멋져",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026162.1.16",
          "form": "나도 이번주에 바쁜 거 다 해결하면",
          "original_form": "나도 이번주에 바쁜 거 다 해결하면",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026162.1.17",
          "form": "너 하는 것처럼 용돈 벌이 하려고",
          "original_form": "너 하는 것처럼 용돈 벌이 하려고",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026162.1.18",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026162.1.19",
          "form": "돈 벌면 뭐하고싶어?",
          "original_form": "돈 벌면 뭐하고싶어?",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026162.1.20",
          "form": "요즘 알바 자리 없긴하지만",
          "original_form": "요즘 알바 자리 없긴하지만",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026162.1.21",
          "form": "만약에 알바하거나 내가 하는 것처럼 용돈 벌면",
          "original_form": "만약에 알바하거나 내가 하는 것처럼 용돈 벌면",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026162.1.22",
          "form": "돈 벌면 일단 맛있는 걸 많이 사먹고 싶어",
          "original_form": "돈 벌면 일단 맛있는 걸 많이 사먹고 싶어",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026162.1.23",
          "form": "그러고 책 사야지",
          "original_form": "그러고 책 사야지",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026162.1.24",
          "form": "아~~ 나도 먹을거 사고 싶어",
          "original_form": "아~~ 나도 먹을거 사고 싶어",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.25",
          "form": "그치 요즘 시국 때문에 알바 자리 없긴 해",
          "original_form": "그치 요즘 시국 때문에 알바 자리 없긴 해",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.26",
          "form": "그리고 나는 그림 그리니까",
          "original_form": "그리고 나는 그림 그리니까",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.27",
          "form": "나도 나도",
          "original_form": "나도 나도",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.28",
          "form": "3D모델링",
          "original_form": "3D모델링",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.29",
          "form": "사고싶어...",
          "original_form": "사고싶어...",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.30",
          "form": "좋네",
          "original_form": "좋네",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.31",
          "form": "나는 살짝",
          "original_form": "나는 살짝",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.32",
          "form": "작법서 같은 거 사고 싶어",
          "original_form": "작법서 같은 거 사고 싶어",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.33",
          "form": "헐~",
          "original_form": "헐~",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.34",
          "form": "멋있다",
          "original_form": "멋있다",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.35",
          "form": "이제와 물어보는거 웃긴데",
          "original_form": "이제와 물어보는거 웃긴데",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026162.1.36",
          "form": "장래희망이 뭐야?",
          "original_form": "장래희망이 뭐야?",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026162.1.37",
          "form": "웃기다 나 일단은 공무원",
          "original_form": "웃기다 나 일단은 공무원",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026162.1.38",
          "form": "일이 좀 반복적이고 안정적이잖아",
          "original_form": "일이 좀 반복적이고 안정적이잖아",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026162.1.39",
          "form": "반복적인 일이 좋아??",
          "original_form": "반복적인 일이 좋아??",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026162.1.40",
          "form": "비꼬는거 아님",
          "original_form": "비꼬는거 아님",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026162.1.41",
          "form": "궁금한거임",
          "original_form": "궁금한거임",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026162.1.42",
          "form": "좋진 않지...",
          "original_form": "좋진 않지...",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026162.1.43",
          "form": "근데 막 회사보단 비교적",
          "original_form": "근데 막 회사보단 비교적",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026162.1.44",
          "form": "뭐라고 해야 하지",
          "original_form": "뭐라고 해야 하지",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026162.1.45",
          "form": "돌발적인 업무? 가 없다고 해야 하나",
          "original_form": "돌발적인 업무? 가 없다고 해야 하나",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026162.1.46",
          "form": "아~~",
          "original_form": "아~~",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026162.1.47",
          "form": "뭔지 알겠다",
          "original_form": "뭔지 알겠다",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026162.1.48",
          "form": "그럼 너도 공시...",
          "original_form": "그럼 너도 공시...",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026162.1.49",
          "form": "쳐야겠네",
          "original_form": "쳐야겠네",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026162.1.50",
          "form": "내가 좀 너무 예상치 못한 업무를 받아버리면",
          "original_form": "내가 좀 너무 예상치 못한 업무를 받아버리면",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026162.1.51",
          "form": "스트레스를 많이 받아서",
          "original_form": "스트레스를 많이 받아서",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026162.1.52",
          "form": "그치...",
          "original_form": "그치...",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026162.1.53",
          "form": "맞아 너는 그런거 엄청",
          "original_form": "맞아 너는 그런거 엄청",
          "speaker_id": "1",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026162.1.54",
          "form": "힘들어하더라",
          "original_form": "힘들어하더라",
          "speaker_id": "1",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026162.1.55",
          "form": "차라리 규칙적이고 큰 변화없는 업무가 낫긴하겠다",
          "original_form": "차라리 규칙적이고 큰 변화없는 업무가 낫긴하겠다",
          "speaker_id": "1",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026162.1.56",
          "form": "맞어 어차피 지금 뭐...",
          "original_form": "맞어 어차피 지금 뭐...",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026162.1.57",
          "form": "진로 생각해봤자",
          "original_form": "진로 생각해봤자",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026162.1.58",
          "form": "공무원 아님 회사쪽이라서",
          "original_form": "공무원 아님 회사쪽이라서",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026162.1.59",
          "form": "그렇게 놓고 보면 차라리 공무원이 나을 것 같아",
          "original_form": "그렇게 놓고 보면 차라리 공무원이 나을 것 같아",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026162.1.60",
          "form": "내 기준!",
          "original_form": "내 기준!",
          "speaker_id": "2",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026162.1.61",
          "form": "그래도 멋져 확실한 주관으로 정했구나",
          "original_form": "그래도 멋져 확실한 주관으로 정했구나",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026162.1.62",
          "form": "너무 꿈과 희망이 없는 느낌이긴 한데",
          "original_form": "너무 꿈과 희망이 없는 느낌이긴 한데",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100026162.1.63",
          "form": "어른이니까 적당히 없어도 괜찮아",
          "original_form": "어른이니까 적당히 없어도 괜찮아",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100026162.1.64",
          "form": "멋진 말이다",
          "original_form": "멋진 말이다",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100026162.1.65",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100026162.1.66",
          "form": "너무 허황된 것보다야 낫다고 생각해",
          "original_form": "너무 허황된 것보다야 낫다고 생각해",
