      "utterance": [
        {
          "id": "MDRW2100011770.1.1",
          "form": "종교있으신가요?",
          "original_form": "종교있으신가요?",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011770.1.2",
          "form": "전 무교에요.",
          "original_form": "전 무교에요.",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011770.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011770.1.4",
          "form": "전 천주교 믿다 현재는 쉬는중이요~",
          "original_form": "전 천주교 믿다 현재는 쉬는중이요~",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011770.1.5",
          "form": "종교있으면 어떤점이 좋나요?",
          "original_form": "종교있으면 어떤점이 좋나요?",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011770.1.6",
          "form": "그냥 믿음이 생기는거죠ㅎㅎ",
          "original_form": "그냥 믿음이 생기는거죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011770.1.7",
          "form": "저는 아직은 뭔가에 의지해 믿음을 얻고싶진않아요",
          "original_form": "저는 아직은 뭔가에 의지해 믿음을 얻고싶진않아요",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011770.1.8",
          "form": "혹시 종교관련 안좋은 기억이 있으신가요?",
          "original_form": "혹시 종교관련 안좋은 기억이 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011770.1.9",
          "form": "네ㅜㅜ전남자친구가 교회를 좀 강요해서요",
          "original_form": "네ㅜㅜ전남자친구가 교회를 좀 강요해서요",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011770.1.10",
          "form": "에고 어떤식으로요",
          "original_form": "에고 어떤식으로요",
          "speaker_id": "1",
          "time": "20210824 15:59"
        },
        {
          "id": "MDRW2100011770.1.11",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210824 15:59"
        },
        {
          "id": "MDRW2100011770.1.12",
          "form": "하느님 안믿는 죄가 가장크다고 지옥간다하더라고요",
          "original_form": "하느님 안믿는 죄가 가장크다고 지옥간다하더라고요",
          "speaker_id": "2",
          "time": "20210824 15:59"
        },
        {
          "id": "MDRW2100011770.1.13",
          "form": "종교인이든지 아니든지 서로 존중해야하는데",
          "original_form": "종교인이든지 아니든지 서로 존중해야하는데",
          "speaker_id": "1",
          "time": "20210824 15:59"
        },
        {
          "id": "MDRW2100011770.1.14",
          "form": "그러니까요 성경도 자꾸 읽으라그러고",
          "original_form": "그러니까요 성경도 자꾸 읽으라그러고",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011770.1.15",
          "form": "조금 강압적이었네요",
          "original_form": "조금 강압적이었네요",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011770.1.16",
          "form": "네 읽기싫음 따라 쓰기라도 하랬어요",
          "original_form": "네 읽기싫음 따라 쓰기라도 하랬어요",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011770.1.17",
          "form": "아직도 만나시나요?",
          "original_form": "아직도 만나시나요?",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011770.1.18",
          "form": "어유 안만나죠",
          "original_form": "어유 안만나죠",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011770.1.19",
          "form": "이제 종교있는사람 안만나려구요",
