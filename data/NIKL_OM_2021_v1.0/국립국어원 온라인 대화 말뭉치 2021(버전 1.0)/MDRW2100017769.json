      "utterance": [
        {
          "id": "MDRW2100017769.1.1",
          "form": "나 오늘 퇴근하면서 진짜 귀여운 하얀강아지 봄",
          "original_form": "나 오늘 퇴근하면서 진짜 귀여운 하얀강아지 봄",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017769.1.2",
          "form": "멈무들 산책하는거 보는거 넘 좋아...",
          "original_form": "멈무들 산책하는거 보는거 넘 좋아...",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017769.1.3",
          "form": "나도출근때마다 산책하는",
          "original_form": "나도출근때마다 산책하는",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017769.1.4",
          "form": "뽀메 마주치는데",
          "original_form": "뽀메 마주치는데",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017769.1.5",
          "form": "진짜 요물이더라",
          "original_form": "진짜 요물이더라",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017769.1.6",
          "form": "하 뽀메 너무 좋아",
          "original_form": "하 뽀메 너무 좋아",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017769.1.7",
          "form": "뽀메 진짜 걸어다니는 솜사탕이야",
          "original_form": "뽀메 진짜 걸어다니는 솜사탕이야",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017769.1.8",
          "form": "걔가 성격더럽기로 유명한데",
          "original_form": "걔가 성격더럽기로 유명한데",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017769.1.9",
          "form": "ㅋㅋㅋㅋ진짜 얼굴이",
          "original_form": "ㅋㅋㅋㅋ진짜 얼굴이",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017769.1.10",
          "form": "너무귀여워 젠장",
          "original_form": "너무귀여워 젠장",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017769.1.11",
          "form": "사실 아침에보는걔도",
          "original_form": "사실 아침에보는걔도",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017769.1.12",
          "form": "나보고 맨날으르렁거림",
          "original_form": "나보고 맨날으르렁거림",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017769.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 타인을 굉장히 싫어하시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 타인을 굉장히 싫어하시네요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017769.1.14",
          "form": "근데 생긴게 넘 천사같아 ㅠ",
          "original_form": "근데 생긴게 넘 천사같아 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.15",
          "form": "ㅋㅋㅋㅋ 맨날 주인분이 말림",
          "original_form": "ㅋㅋㅋㅋ 맨날 주인분이 말림",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.16",
          "form": "뽀얗고 둥실둥실해",
          "original_form": "뽀얗고 둥실둥실해",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.17",
          "form": "넌 강아지파야?",
          "original_form": "넌 강아지파야?",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ 녀석 성깔있네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 녀석 성깔있네",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.19",
          "form": "고양이보다?",
          "original_form": "고양이보다?",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.20",
          "form": "어 난 강아지파!!",
          "original_form": "어 난 강아지파!!",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.21",
          "form": "ㅋㅋㅋ확고하네",
          "original_form": "ㅋㅋㅋ확고하네",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.22",
          "form": "원래 고양이는 별로 안좋아했는데",
          "original_form": "원래 고양이는 별로 안좋아했는데",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.23",
          "form": "친구한테 7년간 세뇌당해서",
          "original_form": "친구한테 7년간 세뇌당해서",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.24",
          "form": "키운다고하면 닥 강아지?",
          "original_form": "키운다고하면 닥 강아지?",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.25",
          "form": "이제 고양이도 좋아함 ㅋㅋㅋ",
          "original_form": "이제 고양이도 좋아함 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.26",
          "form": "어 닥 강아지",
          "original_form": "어 닥 강아지",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.27",
          "form": "ㅋㅋㅋㅋㅋ오..",
          "original_form": "ㅋㅋㅋㅋㅋ오..",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017769.1.28",
          "form": "근데 키우면",
          "original_form": "근데 키우면",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017769.1.29",
          "form": "근데 나 한몸 건사하기 힘들어서 반려동물은...한참 뒤에나 가능항거 같은 ㅠ",
          "original_form": "근데 나 한몸 건사하기 힘들어서 반려동물은...한참 뒤에나 가능항거 같은 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017769.1.30",
          "form": "산책시켜주기가",
          "original_form": "산책시켜주기가",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017769.1.31",
          "form": "힘들거같아서ㅜㅜ",
          "original_form": "힘들거같아서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017769.1.32",
          "form": "ㅁㅈ 글구 강아지는 혼자 오래 두면 애가 힘들어하잖아 ㅠ",
          "original_form": "ㅁㅈ 글구 강아지는 혼자 오래 두면 애가 힘들어하잖아 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017769.1.33",
          "form": "난 사랑과돈을..그만큼투자해줄수가없다",
          "original_form": "난 사랑과돈을..그만큼투자해줄수가없다",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017769.1.34",
          "form": "너는 개파? 고양이파?",
          "original_form": "너는 개파? 고양이파?",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017769.1.35",
          "form": "ㅁㅈㅁㅈ...그만큼 책임지기가 힘들지 ㅠ",
          "original_form": "ㅁㅈㅁㅈ...그만큼 책임지기가 힘들지 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:07"
        }
      ]
