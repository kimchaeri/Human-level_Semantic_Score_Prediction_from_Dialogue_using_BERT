      "utterance": [
        {
          "id": "MDRW2100041794.1.1",
          "form": "무즙이 좋다",
          "original_form": "무즙이 좋다",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041794.1.2",
          "form": "오.. 집에 무있어요?",
          "original_form": "오.. 집에 무있어요?",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041794.1.3",
          "form": "있다",
          "original_form": "있다",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041794.1.4",
          "form": "그럼 만들어줘용~",
          "original_form": "그럼 만들어줘용~",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041794.1.5",
          "form": "네가 만들어 봐라",
          "original_form": "네가 만들어 봐라",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041794.1.6",
          "form": "어떻게 만드는지 모르는뎅",
          "original_form": "어떻게 만드는지 모르는뎅",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041794.1.7",
          "form": "그냥 갈아서 물 넣고 끓이면 된다",
          "original_form": "그냥 갈아서 물 넣고 끓이면 된다",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041794.1.8",
          "form": "오오 해봐야겠다 콧물이 주륵주륵",
          "original_form": "오오 해봐야겠다 콧물이 주륵주륵",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041794.1.9",
          "form": "빨리 해서 먹어라",
          "original_form": "빨리 해서 먹어라",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041794.1.10",
          "form": "웅웅~! 초기에 잡아야지",
          "original_form": "웅웅~! 초기에 잡아야지",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041794.1.11",
          "form": "무즙 먹으면 빨리 낫는다",
          "original_form": "무즙 먹으면 빨리 낫는다",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041794.1.12",
          "form": "웅 기대해본다",
          "original_form": "웅 기대해본다",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041794.1.13",
          "form": "먹고 일찍 자라",
          "original_form": "먹고 일찍 자라",
