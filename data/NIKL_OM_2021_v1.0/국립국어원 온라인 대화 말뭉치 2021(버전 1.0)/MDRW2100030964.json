      "utterance": [
        {
          "id": "MDRW2100030964.1.1",
          "form": "올리브영에서도 물건 구경하고 사고 싶은게 있고",
          "original_form": "올리브영에서도 물건 구경하고 사고 싶은게 있고",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030964.1.2",
          "form": "생리 컵은 왜 그걸 골랐어요",
          "original_form": "생리 컵은 왜 그걸 골랐어요",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030964.1.3",
          "form": "화방 들러서 볼펜도 좀 사야 하거든요",
          "original_form": "화방 들러서 볼펜도 좀 사야 하거든요",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030964.1.4",
          "form": "생리 컵도 종류가 엄청 많아요",
          "original_form": "생리 컵도 종류가 엄청 많아요",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030964.1.5",
          "form": "그냥 여러 군데서 보다가 그게 마음에 들었어요",
          "original_form": "그냥 여러 군데서 보다가 그게 마음에 들었어요",
          "speaker_id": "2",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030964.1.6",
          "form": "써보니까 괜찮나요",
          "original_form": "써보니까 괜찮나요",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030964.1.7",
          "form": "추천할 만한 가요",
          "original_form": "추천할 만한 가요",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030964.1.8",
          "form": "저는 다음에 한번 다른 거 써 보려고요",
          "original_form": "저는 다음에 한번 다른 거 써 보려고요",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030964.1.9",
          "form": "다른 제품을 안써봐서 비교는 어렵지만 전 괜찮은 거 같아요",
          "original_form": "다른 제품을 안써봐서 비교는 어렵지만 전 괜찮은 거 같아요",
          "speaker_id": "2",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030964.1.10",
          "form": "쉽게 적응 하셨네요",
          "original_form": "쉽게 적응 하셨네요",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030964.1.11",
          "form": "보관 용기도 같이 주고요",
          "original_form": "보관 용기도 같이 주고요",
          "speaker_id": "2",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030964.1.12",
          "form": "진작 썼으면 좋았을 것 같아요",
          "original_form": "진작 썼으면 좋았을 것 같아요",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030964.1.13",
          "form": "탐폰에 있어봐서 그게 도움이 되었던 것 같아요",
          "original_form": "탐폰에 있어봐서 그게 도움이 되었던 것 같아요",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030964.1.14",
          "form": "생리대 쓰레기가 안 나와서 좋아요",
          "original_form": "생리대 쓰레기가 안 나와서 좋아요",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030964.1.15",
          "form": "맞아요 올리브영 세일 할 때마다 탐폰 J 느라고 정말 힘들었거든요",
          "original_form": "맞아요 올리브영 세일 할 때마다 탐폰 J 느라고 정말 힘들었거든요",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030964.1.16",
          "form": "탐폰 안 써 봤는데 간편하겠어요",
          "original_form": "탐폰 안 써 봤는데 간편하겠어요",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030964.1.17",
          "form": "쟁이 느라고",
          "original_form": "쟁이 느라고",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100030964.1.18",
          "form": "탐폰이 편하긴 하죠",
          "original_form": "탐폰이 편하긴 하죠",
          "speaker_id": "2",
          "time": "20210904 14:53"
        }
