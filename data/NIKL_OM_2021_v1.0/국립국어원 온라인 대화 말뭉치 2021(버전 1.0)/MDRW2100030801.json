      "utterance": [
        {
          "id": "MDRW2100030801.1.1",
          "form": "새 주제 메타버스 어때",
          "original_form": "새 주제 메타버스 어때",
          "speaker_id": "1",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030801.1.2",
          "form": "9모 국어 지문으로 메타버스 나왔는데",
          "original_form": "9모 국어 지문으로 메타버스 나왔는데",
          "speaker_id": "1",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030801.1.3",
          "form": "풀면어 레플원 생각났어ㅋㅋㅋ",
          "original_form": "풀면어 레플원 생각났어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030801.1.4",
          "form": "풀면서",
          "original_form": "풀면서",
          "speaker_id": "1",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030801.1.5",
          "form": "헐 요즘은 메타버스가 문제에 나오는구나",
          "original_form": "헐 요즘은 메타버스가 문제에 나오는구나",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030801.1.6",
          "form": "나사실 메타버스가 뭔지 잘 모르겠어",
          "original_form": "나사실 메타버스가 뭔지 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030801.1.7",
          "form": "설명해줄수있엉?",
          "original_form": "설명해줄수있엉?",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030801.1.8",
          "form": "vr같은건데",
          "original_form": "vr같은건데",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030801.1.9",
          "form": "장갑 트레드밀 풀로 착용하는",
          "original_form": "장갑 트레드밀 풀로 착용하는",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030801.1.10",
          "form": "그런거 같던데(사실 나도 잘 모름)",
          "original_form": "그런거 같던데(사실 나도 잘 모름)",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030801.1.11",
          "form": "레디 플레이어 원처럼",
          "original_form": "레디 플레이어 원처럼",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030801.1.12",
          "form": "ㅋㅋㅋ그영화아직안봤다(놀라운사실)",
          "original_form": "ㅋㅋㅋ그영화아직안봤다(놀라운사실)",
          "speaker_id": "2",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030801.1.13",
          "form": "헐 의외네...?",
          "original_form": "헐 의외네...?",
          "speaker_id": "1",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030801.1.14",
          "form": "9모 지문 읽어보니까 신기하더라고...",
          "original_form": "9모 지문 읽어보니까 신기하더라고...",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030801.1.15",
          "form": "막 트레드밀이 배경에 따라 속도나 기울기도 변하고 한대",
          "original_form": "막 트레드밀이 배경에 따라 속도나 기울기도 변하고 한대",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030801.1.16",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030801.1.17",
          "form": "짱이다",
          "original_form": "짱이다",
          "speaker_id": "2",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030801.1.18",
          "form": "그런걸로 게임하면 재밌을 듯..;",
          "original_form": "그런걸로 게임하면 재밌을 듯..;",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030801.1.19",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030801.1.20",
          "form": "어쌔신크리드 썁가능",
          "original_form": "어쌔신크리드 썁가능",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030801.1.21",
          "form": "옛날에 그런 vr로 엑스윙 조종 게임 있으면 좋겠다 생각했는데...",
          "original_form": "옛날에 그런 vr로 엑스윙 조종 게임 있으면 좋겠다 생각했는데...",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100030801.1.22",
          "form": "냅다 데스스타에 들이박기",
          "original_form": "냅다 데스스타에 들이박기",
          "speaker_id": "2",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030801.1.23",
          "form": "ㅋㅋㅋㅋ 그래도 재밌을듯...",
          "original_form": "ㅋㅋㅋㅋ 그래도 재밌을듯...",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030801.1.24",
          "form": "예아....",
          "original_form": "예아....",
          "speaker_id": "2",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030801.1.25",
          "form": "메타버스로그럼",
          "original_form": "메타버스로그럼",
          "speaker_id": "2",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030801.1.26",
          "form": "우리가좍들도만날수있게되는건가",
          "original_form": "우리가좍들도만날수있게되는건가",
          "speaker_id": "2",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030801.1.27",
          "form": "와 그럼 좋겠다...",
          "original_form": "와 그럼 좋겠다...",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030801.1.28",
          "form": "만나서 그 안에서도 트위터하는거지",
          "original_form": "만나서 그 안에서도 트위터하는거지",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030801.1.29",
          "form": "아니 그럼 메타버스 하는 이유가 없잖아 ㅋㅋㅋ",
          "original_form": "아니 그럼 메타버스 하는 이유가 없잖아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030801.1.30",
          "form": "서로 서바이벌 게임 하면서 죽이고 그래야지(?)",
          "original_form": "서로 서바이벌 게임 하면서 죽이고 그래야지(?)",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030801.1.31",
          "form": "흐에에에엥ㄱ",
          "original_form": "흐에에에엥ㄱ",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030801.1.32",
          "form": "갑자기 헝거게임",
          "original_form": "갑자기 헝거게임",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030801.1.33",
          "form": "트레드밀 속도도 변한다고 하니까",
          "original_form": "트레드밀 속도도 변한다고 하니까",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030801.1.34",
          "form": "술래잡기 해도 재밌겠다",
          "original_form": "술래잡기 해도 재밌겠다",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030801.1.35",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030801.1.36",
          "form": "개빨리뛰어야겠네",
          "original_form": "개빨리뛰어야겠네",
          "speaker_id": "2",
          "time": "20210904 13:19"
        }
      ]
