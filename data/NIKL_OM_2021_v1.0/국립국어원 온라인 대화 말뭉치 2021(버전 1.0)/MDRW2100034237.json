      "utterance": [
        {
          "id": "MDRW2100034237.1.1",
          "form": "좋아하는 프로그램 있나요?",
          "original_form": "좋아하는 프로그램 있나요?",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034237.1.2",
          "form": "티비는 잘 안 보고 유튜브만 좀 봐요!",
          "original_form": "티비는 잘 안 보고 유튜브만 좀 봐요!",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034237.1.3",
          "form": "name2 님은 좋아하는 프로그램 있으세요?",
          "original_form": "&name2& 님은 좋아하는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100034237.1.4",
          "form": "저는 꼬꼬무나 무도 좋아해요 !",
          "original_form": "저는 꼬꼬무나 무도 좋아해요 !",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034237.1.5",
          "form": "아 저도 꼬꼬무는 유튜브 클립으로 자주 봐요!",
          "original_form": "아 저도 꼬꼬무는 유튜브 클립으로 자주 봐요!",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034237.1.6",
          "form": "재밌지않나요 ㅎㅎㅎ",
          "original_form": "재밌지않나요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034237.1.7",
          "form": "친구가 말해주는 듯한 특유의 분위기도 좋고 내용도 흥미로워요!",
          "original_form": "친구가 말해주는 듯한 특유의 분위기도 좋고 내용도 흥미로워요!",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034237.1.8",
          "form": "맞아요 ! 더 이해가 잘되고",
          "original_form": "맞아요 ! 더 이해가 잘되고",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034237.1.9",
          "form": "게스트들 반응도 좋아서 ㅋㅋ",
          "original_form": "게스트들 반응도 좋아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100034237.1.10",
          "form": "게스트 반응 완전 제 반응! 같이 놀라고 화내고ㅋㅋㅋ",
          "original_form": "게스트 반응 완전 제 반응! 같이 놀라고 화내고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034237.1.11",
          "form": "맞아요 ㅋㅋㅋ 가끔 웃기기도 하지않나욬ㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 가끔 웃기기도 하지않나욬ㅋ",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100034237.1.12",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034237.1.13",
          "form": "어떤 편 제일 재밌게 보셨어요?",
          "original_form": "어떤 편 제일 재밌게 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034237.1.14",
          "form": "전 이태원 살인사건도 기억에 많이 남구요",
          "original_form": "전 이태원 살인사건도 기억에 많이 남구요",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034237.1.15",
          "form": "그그 오대양도 재밌게봤어요",
          "original_form": "그그 오대양도 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034237.1.16",
          "form": "헉 저도 오대양 기억에 남아요... 너무 충격적이었어요",
          "original_form": "헉 저도 오대양 기억에 남아요... 너무 충격적이었어요",
          "speaker_id": "1",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034237.1.17",
          "form": "맞죠 ㅜㅜ 저는 그 사건 아예 몰랐었는데",
          "original_form": "맞죠 ㅜㅜ 저는 그 사건 아예 몰랐었는데",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034237.1.18",
          "form": "충격이였어요!",
          "original_form": "충격이였어요!",
          "speaker_id": "2",
          "time": "20210828 19:56"
        }
