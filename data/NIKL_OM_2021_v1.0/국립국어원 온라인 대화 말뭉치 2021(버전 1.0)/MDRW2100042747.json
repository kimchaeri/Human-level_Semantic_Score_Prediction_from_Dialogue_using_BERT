      "utterance": [
        {
          "id": "MDRW2100042747.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100042747.1.2",
          "form": "넹넹!!",
          "original_form": "넹넹!!",
          "speaker_id": "2",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100042747.1.3",
          "form": "근데 실천은 못햐여..",
          "original_form": "근데 실천은 못햐여..",
          "speaker_id": "2",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100042747.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100042747.1.5",
          "form": "ㅋㅋㅋㅋ저도 그래요 어떤 운동 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋ저도 그래요 어떤 운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042747.1.6",
          "form": "만약한다면 걷는거??",
          "original_form": "만약한다면 걷는거??",
          "speaker_id": "2",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042747.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042747.1.8",
          "form": "좋아한다가 맞을까요 ㅠㅠ",
          "original_form": "좋아한다가 맞을까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:29"
        },
        {
          "id": "MDRW2100042747.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아한다기보다는 필요성을 느껴서 하는 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아한다기보다는 필요성을 느껴서 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100042747.1.10",
          "form": "맞아요 거의 그러초 ㅋㅋㅋ",
          "original_form": "맞아요 거의 그러초 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100042747.1.11",
          "form": "그래도 꾸준히 하면 뭐든 좋은 것 같아요",
          "original_form": "그래도 꾸준히 하면 뭐든 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100042747.1.12",
          "form": "요즘 비가 와서 걷기는 잘 못 하시겠어요ㅜㅜ",
          "original_form": "요즘 비가 와서 걷기는 잘 못 하시겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100042747.1.13",
          "form": "맞아요 맨날 잔바리 비가와요 ㅠ",
          "original_form": "맞아요 맨날 잔바리 비가와요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100042747.1.14",
          "form": "그쵸 그래도 오늘부터는 개나봐요!",
          "original_form": "그쵸 그래도 오늘부터는 개나봐요!",
          "speaker_id": "1",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100042747.1.15",
          "form": "그러게요 날이좋아졌어요!",
          "original_form": "그러게요 날이좋아졌어요!",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100042747.1.16",
          "form": "다행이에요ㅎㅎㅎ저도 훨씬 가볍게 입고 나왔어요",
          "original_form": "다행이에요ㅎㅎㅎ저도 훨씬 가볍게 입고 나왔어요",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100042747.1.17",
          "form": "저는 긴바지에 반팔입었어요",
          "original_form": "저는 긴바지에 반팔입었어요",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100042747.1.18",
          "form": "와 반팔",
          "original_form": "와 반팔",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100042747.1.19",
          "form": "안 추우세요?",
          "original_form": "안 추우세요?",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100042747.1.20",
          "form": "넹넹 아직은",
          "original_form": "넹넹 아직은",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100042747.1.21",
          "form": "추울정돈아니더라규요",
          "original_form": "추울정돈아니더라규요",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100042747.1.22",
          "form": "다행이네요ㅎㅎㅎ저는 셔츠에 자켓도 걸쳤어요ㅋㅋㅋㅋ",
          "original_form": "다행이네요ㅎㅎㅎ저는 셔츠에 자켓도 걸쳤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100042747.1.23",
          "form": "추위를 잘타시나바용",
          "original_form": "추위를 잘타시나바용",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100042747.1.24",
          "form": "저희는버스에서 에어컨도 틀어준답니다 ㅋㅋㅋ",
          "original_form": "저희는버스에서 에어컨도 틀어준답니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100042747.1.25",
          "form": "네네ㅎㅎㅎ 그래도 훨씬 가볍게 입었다고 생각했는데 아니었네요ㅋㅋㅋㅋㅋ",
          "original_form": "네네ㅎㅎㅎ 그래도 훨씬 가볍게 입었다고 생각했는데 아니었네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:54"
        }
      ]
