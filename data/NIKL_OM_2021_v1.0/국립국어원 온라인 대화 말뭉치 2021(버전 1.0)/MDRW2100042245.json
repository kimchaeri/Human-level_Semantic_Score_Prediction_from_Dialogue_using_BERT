      "utterance": [
        {
          "id": "MDRW2100042245.1.1",
          "form": "사과샀어요",
          "original_form": "사과샀어요",
          "speaker_id": "1",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100042245.1.2",
          "form": "가을사과 맛있죠",
          "original_form": "가을사과 맛있죠",
          "speaker_id": "2",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100042245.1.3",
          "form": "밤도맛있어요ㅎㅎ",
          "original_form": "밤도맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042245.1.4",
          "form": "네 밤도 맛있죠 그냥 먹어도 맛있고 밤식빵으로 먹어도 좋더라구여",
          "original_form": "네 밤도 맛있죠 그냥 먹어도 맛있고 밤식빵으로 먹어도 좋더라구여",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042245.1.5",
          "form": "밤식빵너무좋아해요ㅎㅎ",
          "original_form": "밤식빵너무좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042245.1.6",
          "form": "사실빵순이에요",
          "original_form": "사실빵순이에요",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042245.1.7",
          "form": "오 특별히 좋아하는 빵 있으세요?",
          "original_form": "오 특별히 좋아하는 빵 있으세요?",
          "speaker_id": "2",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100042245.1.8",
          "form": "저는크림빵이요",
          "original_form": "저는크림빵이요",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042245.1.9",
          "form": "생크림인가요?",
          "original_form": "생크림인가요?",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042245.1.10",
          "form": "넹넹ㅎㅎ고런거요",
          "original_form": "넹넹ㅎㅎ고런거요",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042245.1.11",
          "form": "빵좋아하세요?",
          "original_form": "빵좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042245.1.12",
          "form": "좋아하는 편이에요 ㅎㅎ 주로 건강빵 쪽으로 먹어요",
          "original_form": "좋아하는 편이에요 ㅎㅎ 주로 건강빵 쪽으로 먹어요",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042245.1.13",
          "form": "치아바타나 쌀빵 글루텐프리 같은 거요",
          "original_form": "치아바타나 쌀빵 글루텐프리 같은 거요",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042245.1.14",
          "form": "치아바타 저도 좋아해요",
          "original_form": "치아바타 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042245.1.15",
          "form": "글루텐 드시면 아프신가요?ㅠ",
          "original_form": "글루텐 드시면 아프신가요?ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042245.1.16",
          "form": "검사는 안해봐서 모르겠고 밀가루를 안 먹으면 낫더라구요",
          "original_form": "검사는 안해봐서 모르겠고 밀가루를 안 먹으면 낫더라구요",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042245.1.17",
          "form": "그래서 유기농밀 통밀 쌀빵 글루텐프리쪽으로 먹게 되요",
          "original_form": "그래서 유기농밀 통밀 쌀빵 글루텐프리쪽으로 먹게 되요",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042245.1.18",
          "form": "ㅜㅜ그러시군요",
          "original_form": "ㅜㅜ그러시군요",
          "speaker_id": "1",
