      "utterance": [
        {
          "id": "MDRW2100035442.1.1",
          "form": "꾸미는거 좋아하세요?",
          "original_form": "꾸미는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100035442.1.2",
          "form": "좋아하긴 하는데 돈이 부족해서요 ㅎㅎ",
          "original_form": "좋아하긴 하는데 돈이 부족해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100035442.1.3",
          "form": "어떤 종류에 관심있으세요? 옷? 머리? 소품? 화장?",
          "original_form": "어떤 종류에 관심있으세요? 옷? 머리? 소품? 화장?",
          "speaker_id": "2",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100035442.1.4",
          "form": "옷이랑 소품요",
          "original_form": "옷이랑 소품요",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100035442.1.5",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100035442.1.6",
          "form": "돈도 그렇고 시간도요...",
          "original_form": "돈도 그렇고 시간도요...",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100035442.1.7",
          "form": "많아야하는거같아요",
          "original_form": "많아야하는거같아요",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100035442.1.8",
          "form": "맞아요 ㅜㅜㅜ 가방 신발 이런거 좋아하시겠어요!",
          "original_form": "맞아요 ㅜㅜㅜ 가방 신발 이런거 좋아하시겠어요!",
          "speaker_id": "2",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100035442.1.9",
          "form": "네 그런거 잘사고싶어요",
          "original_form": "네 그런거 잘사고싶어요",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035442.1.10",
          "form": "저도 돈만있다면 명품가방 척척사고싶네요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 돈만있다면 명품가방 척척사고싶네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100035442.1.11",
          "form": "네 돈이 문제에요",
          "original_form": "네 돈이 문제에요",
          "speaker_id": "1",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100035442.1.12",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100035442.1.13",
          "form": "옷도그렇고 모든게 ㅜㅜ",
          "original_form": "옷도그렇고 모든게 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100035442.1.14",
          "form": "없으면 그중 좋아보이는거 고르느라 쓰는시간이",
          "original_form": "없으면 그중 좋아보이는거 고르느라 쓰는시간이",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100035442.1.15",
          "form": "꽤많은거같어요",
          "original_form": "꽤많은거같어요",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100035442.1.16",
          "form": "와 완전 공감이에요 한정된 예산안에서 최선을 고르기란 시간이 많이 드는 일이죠 ㅜㅜ",
          "original_form": "와 완전 공감이에요 한정된 예산안에서 최선을 고르기란 시간이 많이 드는 일이죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100035442.1.17",
          "form": "옷 주로 어디서 사세요?",
          "original_form": "옷 주로 어디서 사세요?",
          "speaker_id": "2",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100035442.1.18",
          "form": "요즘은 그것마저도 못샀는데",
          "original_form": "요즘은 그것마저도 못샀는데",
          "speaker_id": "1",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100035442.1.19",
          "form": "예전엔 인터넷 쇼핑이나 백화점 아울렛 행사요",
          "original_form": "예전엔 인터넷 쇼핑이나 백화점 아울렛 행사요",
          "speaker_id": "1",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100035442.1.20",
          "form": "아울렛 사랑이지요 ㅜㅜ",
          "original_form": "아울렛 사랑이지요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100035442.1.21",
          "form": "어떤 스타일 즐겨입으세요?",
          "original_form": "어떤 스타일 즐겨입으세요?",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100035442.1.22",
          "form": "음 그냥 얌전해보이는 스타일로요",
          "original_form": "음 그냥 얌전해보이는 스타일로요",
          "speaker_id": "1",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100035442.1.23",
          "form": "전 원피스 주로 입어요 ㅋㅋ",
          "original_form": "전 원피스 주로 입어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:28"
        }
      ]
    }
