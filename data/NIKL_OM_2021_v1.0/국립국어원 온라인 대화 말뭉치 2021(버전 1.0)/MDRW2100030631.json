      "utterance": [
        {
          "id": "MDRW2100030631.1.1",
          "form": "오늘 두 아기들은",
          "original_form": "오늘 두 아기들은",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030631.1.2",
          "form": "언제부터 키우셧어요?",
          "original_form": "언제부터 키우셧어요?",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030631.1.3",
          "form": "4살과 13살! 4살짜리는 키운지 반년밖에 안 됐어요ㅎ",
          "original_form": "4살과 13살! 4살짜리는 키운지 반년밖에 안 됐어요ㅎ",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030631.1.4",
          "form": "아이와 놀이터서 잘 노셨나요",
          "original_form": "아이와 놀이터서 잘 노셨나요",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100030631.1.5",
          "form": "내 미끄럼틀 타고 타고 그네 타고",
          "original_form": "내 미끄럼틀 타고 타고 그네 타고",
          "speaker_id": "2",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030631.1.6",
          "form": "저는 땀이 납니다",
          "original_form": "저는 땀이 납니다",
          "speaker_id": "2",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030631.1.7",
          "form": "육아가 쉽지가 않네요 육아",
          "original_form": "육아가 쉽지가 않네요 육아",
          "speaker_id": "2",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030631.1.8",
          "form": "애기들은 무슨 종이에요?",
          "original_form": "애기들은 무슨 종이에요?",
          "speaker_id": "2",
          "time": "20210904 12:29"
        },
        {
          "id": "MDRW2100030631.1.9",
          "form": "열정적으로 노셨군요ㅎㅎ",
          "original_form": "열정적으로 노셨군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100030631.1.10",
          "form": "오늘 놀이터에 애들 많더라구요",
          "original_form": "오늘 놀이터에 애들 많더라구요",
          "speaker_id": "1",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100030631.1.11",
          "form": "둘다 세상에 하나뿐인 믹스입니당",
          "original_form": "둘다 세상에 하나뿐인 믹스입니당",
          "speaker_id": "1",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100030631.1.12",
          "form": "진짜 귀여워요ㅎ",
          "original_form": "진짜 귀여워요ㅎ",
          "speaker_id": "1",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100030631.1.13",
          "form": "너무 귀엽겠습니다",
          "original_form": "너무 귀엽겠습니다",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030631.1.14",
          "form": "사진전송되면",
          "original_form": "사진전송되면",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030631.1.15",
          "form": "꼭 보고싶네요",
          "original_form": "꼭 보고싶네요",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030631.1.16",
          "form": "두 애기들",
          "original_form": "두 애기들",
          "speaker_id": "2",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100030631.1.17",
          "form": "ㅜㅜ 저 얼마나 짤이나 사진 보내고 싶었음",
          "original_form": "ㅜㅜ 저 얼마나 짤이나 사진 보내고 싶었음",
          "speaker_id": "1",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100030631.1.18",
          "form": "간밤에 갑자기 이미지 첨부 칸이 생긴 거예요!",
          "original_form": "간밤에 갑자기 이미지 첨부 칸이 생긴 거예요!",
          "speaker_id": "1",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100030631.1.19",
          "form": "너무 좋아서 보내려는데 계속 오류 나고",
          "original_form": "너무 좋아서 보내려는데 계속 오류 나고",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100030631.1.20",
          "form": "잠에서 깼네요",
          "original_form": "잠에서 깼네요",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100030631.1.21",
          "form": "ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030631.1.22",
          "form": "귀여우십니다",
          "original_form": "귀여우십니다",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030631.1.23",
          "form": "짤 좀 구경해보고싶어요 ㅋㅋ",
          "original_form": "짤 좀 구경해보고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030631.1.24",
          "form": "강아지 사진도 엄청 많은데ㅜ",
          "original_form": "강아지 사진도 엄청 많은데ㅜ",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030631.1.25",
          "form": "님 사진폴더도 애기로 한가득이쥬?",
          "original_form": "님 사진폴더도 애기로 한가득이쥬?",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030631.1.26",
          "form": "강아지 보고싶습니나!",
          "original_form": "강아지 보고싶습니나!",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030631.1.27",
          "form": "아기 사진으로 꽉찾어요 ㅋㅋ",
          "original_form": "아기 사진으로 꽉찾어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030631.1.28",
          "form": "책상에도 애기 액자사진 있고요?",
          "original_form": "책상에도 애기 액자사진 있고요?",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030631.1.29",
          "form": "아녀 ㅋㅋㅋ",
          "original_form": "아녀 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030631.1.30",
          "form": "책상에는 겜할때만 앉아서",
          "original_form": "책상에는 겜할때만 앉아서",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030631.1.31",
          "form": "사진있으면 겜못합니다 ㅋㅋ",
          "original_form": "사진있으면 겜못합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030631.1.32",
          "form": "아니ㅋㅋ",
          "original_form": "아니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030631.1.33",
          "form": "겜할때 최적의 환경을 위해ㅋㅋ",
          "original_form": "겜할때 최적의 환경을 위해ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030631.1.34",
          "form": "네 ㅋㅋ사실 겜을 잘못해서(실력)",
          "original_form": "네 ㅋㅋ사실 겜을 잘못해서(실력)",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030631.1.35",
          "form": "집중안하면 쌍욬 먹어요",
          "original_form": "집중안하면 쌍욬 먹어요",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030631.1.36",
          "form": "제 별명이 쌍욬제조기입니다",
          "original_form": "제 별명이 쌍욬제조기입니다",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030631.1.37",
          "form": "생산적이시군요!! ㅋㅋ",
          "original_form": "생산적이시군요!! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:23"
        }
      ]
    }
