      "utterance": [
        {
          "id": "MDRW2100030831.1.1",
          "form": "안녕하셍ㆍ",
          "original_form": "안녕하셍ㆍ",
          "speaker_id": "1",
          "time": "20210904 13:11"
        },
        {
          "id": "MDRW2100030831.1.2",
          "form": "세요",
          "original_form": "세요",
          "speaker_id": "1",
          "time": "20210904 13:11"
        },
        {
          "id": "MDRW2100030831.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030831.1.4",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030831.1.5",
          "form": "날씨가 좋죠~",
          "original_form": "날씨가 좋죠~",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030831.1.6",
          "form": "앗 저는 직장인이에요",
          "original_form": "앗 저는 직장인이에요",
          "speaker_id": "1",
          "time": "20210904 13:15"
        },
        {
          "id": "MDRW2100030831.1.7",
          "form": "내 날 너무 좋아요 오늘 ㅜㅜ",
          "original_form": "내 날 너무 좋아요 오늘 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030831.1.8",
          "form": "아하 저는 학생이에요!",
          "original_form": "아하 저는 학생이에요!",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030831.1.9",
          "form": "개강했어요~ 9월에!!",
          "original_form": "개강했어요~ 9월에!!",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030831.1.10",
          "form": "앗 그렇군요ㅎㅎㅎ",
          "original_form": "앗 그렇군요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030831.1.11",
          "form": "요샌 줌으로듣나요?",
          "original_form": "요샌 줌으로듣나요?",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030831.1.12",
          "form": "네 수업은 거의 줌이나 동영상강의에요ㅜㅜ",
          "original_form": "네 수업은 거의 줌이나 동영상강의에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 14:49"
        },
        {
          "id": "MDRW2100030831.1.13",
          "form": "실기, 실험수업은 대면이 필요하긴 한데 그것도 쉽지 않죠ㅜㅜ",
          "original_form": "실기, 실험수업은 대면이 필요하긴 한데 그것도 쉽지 않죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100030831.1.14",
          "form": "아휴ㅜㅜ그러겠어요",
          "original_form": "아휴ㅜㅜ그러겠어요",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100030831.1.15",
          "form": "학업도어렵겠지만 학교사람 만날기회가 적겠어요",
          "original_form": "학업도어렵겠지만 학교사람 만날기회가 적겠어요",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100030831.1.16",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100030831.1.17",
          "form": "맞아요 20학번이 제일 안됐죠...ㅜㅜ 학교도 못와보고",
          "original_form": "맞아요 20학번이 제일 안됐죠...ㅜㅜ 학교도 못와보고",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100030831.1.18",
          "form": "학교앞 맛집도 못가보고ㅠㅠ",
          "original_form": "학교앞 맛집도 못가보고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100030831.1.19",
          "form": "ㅜㅜㅂ그르게요",
          "original_form": "ㅜㅜㅂ그르게요",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100030831.1.20",
          "form": "저도 휴학하고 그래서 1년 넘게 온라인으로 다니긴 했는데... 그래서 더 동아리고 뭐고 못했던 것같아요",
          "original_form": "저도 휴학하고 그래서 1년 넘게 온라인으로 다니긴 했는데... 그래서 더 동아리고 뭐고 못했던 것같아요",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100030831.1.21",
          "form": "코로나가 여러모도 너무 힘들게 하네요ㅜㅜ",
          "original_form": "코로나가 여러모도 너무 힘들게 하네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100030831.1.22",
          "form": "그러게요ㅜ많이달라졌어요",
          "original_form": "그러게요ㅜ많이달라졌어요",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100030831.1.23",
          "form": "코로나 전에 졸업하셨나요?",
          "original_form": "코로나 전에 졸업하셨나요?",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100030831.1.24",
          "form": "네 졸업때터졌어요",
          "original_form": "네 졸업때터졌어요",
          "speaker_id": "1",
          "time": "20210908 09:08"
        }
      ]
