      "utterance": [
        {
          "id": "MDRW2100039455.1.1",
          "form": "우리 졸업 여행 꼭 가",
          "original_form": "우리 졸업 여행 꼭 가",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039455.1.2",
          "form": "일본으로 갈거야?",
          "original_form": "일본으로 갈거야?",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039455.1.3",
          "form": "가자 함 갈겨 가보자고",
          "original_form": "가자 함 갈겨 가보자고",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039455.1.4",
          "form": "돈이 문제긴 한데",
          "original_form": "돈이 문제긴 한데",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039455.1.5",
          "form": "배타고 가면 그나마 저렴할걸",
          "original_form": "배타고 가면 그나마 저렴할걸",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039455.1.6",
          "form": "호오 근데 요즘",
          "original_form": "호오 근데 요즘",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039455.1.7",
          "form": "비행기 값도 괜찮지 않나",
          "original_form": "비행기 값도 괜찮지 않나",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039455.1.8",
          "form": "그런가? 궈러나 안 괜찮아지면 괜찮을수도",
          "original_form": "그런가? 궈러나 안 괜찮아지면 괜찮을수도",
          "speaker_id": "2",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100039455.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 여행 가야해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 여행 가야해",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039455.1.10",
          "form": "우디 가지 도쿄 가?",
          "original_form": "우디 가지 도쿄 가?",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039455.1.11",
          "form": "덕질 여행 가야해!",
          "original_form": "덕질 여행 가야해!",
          "speaker_id": "1",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039455.1.12",
          "form": "가야지!!!",
          "original_form": "가야지!!!",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039455.1.13",
          "form": "너는 어디로 가고 싶어?",
          "original_form": "너는 어디로 가고 싶어?",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039455.1.14",
          "form": "나도 도쿄가 좋지 그 근처는 앵간해선 볼거리정도는 알고있고",
          "original_form": "나도 도쿄가 좋지 그 근처는 앵간해선 볼거리정도는 알고있고",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039455.1.15",
          "form": "관광만 하겠다 하면 오사카도 나쁘진 않은데",
          "original_form": "관광만 하겠다 하면 오사카도 나쁘진 않은데",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039455.1.16",
          "form": "웅웅 그럼 어디?",
          "original_form": "웅웅 그럼 어디?",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039455.1.17",
          "form": "골라 난 어디든 상관없어",
          "original_form": "골라 난 어디든 상관없어",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039455.1.18",
          "form": "아예 다들 모르는 곳으로 가서 개고생 하는것도 나쁘진 않고",
          "original_form": "아예 다들 모르는 곳으로 가서 개고생 하는것도 나쁘진 않고",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039455.1.19",
          "form": "오오오 당신 멋져",
          "original_form": "오오오 당신 멋져",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039455.1.20",
          "form": "도쿄가자",
          "original_form": "도쿄가자",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039455.1.21",
          "form": "나 한번도 못가봤어",
          "original_form": "나 한번도 못가봤어",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039455.1.22",
          "form": "도쿄 어때?",
          "original_form": "도쿄 어때?",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039455.1.23",
          "form": "도쿄 굿~~~",
          "original_form": "도쿄 굿~~~",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039455.1.24",
          "form": "도쿄는",
          "original_form": "도쿄는",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039455.1.25",
          "form": "시끌벅적하지",
          "original_form": "시끌벅적하지",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039455.1.26",
          "form": "난 네온사인 많은 거리 좋아해서 도쿄 좋아",
          "original_form": "난 네온사인 많은 거리 좋아해서 도쿄 좋아",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039455.1.27",
          "form": "완전 도쿄라기 보단 신주쿠에서 주로 보내긴 했는데",
          "original_form": "완전 도쿄라기 보단 신주쿠에서 주로 보내긴 했는데",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039455.1.28",
          "form": "옹 웅웅 그래서?",
          "original_form": "옹 웅웅 그래서?",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039455.1.29",
          "form": "거기는 진짜",
          "original_form": "거기는 진짜",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039455.1.30",
          "form": "쩔었지",
          "original_form": "쩔었지",
          "speaker_id": "2",
          "time": "20210903 21:45"
        }
      ]
    }
  ]
