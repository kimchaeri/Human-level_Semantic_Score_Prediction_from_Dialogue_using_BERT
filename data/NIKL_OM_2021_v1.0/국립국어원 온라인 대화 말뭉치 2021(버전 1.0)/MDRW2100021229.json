      "utterance": [
        {
          "id": "MDRW2100021229.1.1",
          "form": "사브르 선수들 얼굴도 즐겁더라고요 ㅋㅋㅋ",
          "original_form": "사브르 선수들 얼굴도 즐겁더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021229.1.2",
          "form": "아 맞아요 다들 훈훈하시더라구요",
          "original_form": "아 맞아요 다들 훈훈하시더라구요",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021229.1.3",
          "form": "뮤지컬 배우인줄 알았어요 ㅋㅋ",
          "original_form": "뮤지컬 배우인줄 알았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021229.1.4",
          "form": "맞아요 ㅋㅋㅋㅋㅋ진짜 뮤배느낌 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ진짜 뮤배느낌 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021229.1.5",
          "form": "사진도 잘 찍었더라고요 ㅋㅋㅋ",
          "original_form": "사진도 잘 찍었더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021229.1.6",
          "form": "이번 국대 선수들 사진들이 ㅋㅋㅋㅋㅋ",
          "original_form": "이번 국대 선수들 사진들이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021229.1.7",
          "form": "뽀샤시하게 찍으셧더라구요 ㅋㅋㅋㅋ",
          "original_form": "뽀샤시하게 찍으셧더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021229.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021229.1.9",
          "form": "다른나라 선수들이랑 비교하면ㅋㅋㅋㅋ 엄청 뽀샤시해요",
          "original_form": "다른나라 선수들이랑 비교하면ㅋㅋㅋㅋ 엄청 뽀샤시해요",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021229.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ진짜 뮤지컬 팜플렛 사진 느낌으로 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ진짜 뮤지컬 팜플렛 사진 느낌으로 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021229.1.11",
          "form": "맞아요 ㅋㅋㅋ 잘만드셨더라고요",
          "original_form": "맞아요 ㅋㅋㅋ 잘만드셨더라고요",
