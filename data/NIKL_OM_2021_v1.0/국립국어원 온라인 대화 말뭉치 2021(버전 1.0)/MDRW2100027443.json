      "utterance": [
        {
          "id": "MDRW2100027443.1.1",
          "form": "오늘 날씨 완전 가을인거 같아요!",
          "original_form": "오늘 날씨 완전 가을인거 같아요!",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027443.1.2",
          "form": "맞아요! 벌써 가을이 돼버렸네요",
          "original_form": "맞아요! 벌써 가을이 돼버렸네요",
          "speaker_id": "2",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100027443.1.3",
          "form": "바람불면 선선하니 딱 좋아요~",
          "original_form": "바람불면 선선하니 딱 좋아요~",
          "speaker_id": "1",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027443.1.4",
          "form": "구름도 많아서 햇빛도 안쎄고ㅋㅋ 다음주엔 더워질까요?",
          "original_form": "구름도 많아서 햇빛도 안쎄고ㅋㅋ 다음주엔 더워질까요?",
          "speaker_id": "1",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027443.1.5",
          "form": "이젠 더워지진 않을거 같아요",
          "original_form": "이젠 더워지진 않을거 같아요",
          "speaker_id": "2",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027443.1.6",
          "form": "그럼 슬슬 가디건도 꺼내놔야겠네요 환절기 감기 조심!",
          "original_form": "그럼 슬슬 가디건도 꺼내놔야겠네요 환절기 감기 조심!",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027443.1.7",
          "form": "name1도 감기 조심하세요",
          "original_form": "&name1&도 감기 조심하세요",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027443.1.8",
          "form": "쨌든.. 날씨가 풀리면 슬슬 가을 모기가 기승이겠네요",
          "original_form": "쨌든.. 날씨가 풀리면 슬슬 가을 모기가 기승이겠네요",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027443.1.9",
          "form": "요즘은 가을모기가 더 독한거같아요",
          "original_form": "요즘은 가을모기가 더 독한거같아요",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027443.1.10",
          "form": "맞아요 안그래도 지금 발에 한군데 물려서ㅋㅋ",
          "original_form": "맞아요 안그래도 지금 발에 한군데 물려서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027443.1.11",
          "form": "일어설 자신이 없어요",
          "original_form": "일어설 자신이 없어요",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027443.1.12",
          "form": "헉 발등요? 발바닥요?",
          "original_form": "헉 발등요? 발바닥요?",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027443.1.13",
          "form": "옆구리에 물렸어요 애매하게",
          "original_form": "옆구리에 물렸어요 애매하게",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027443.1.14",
          "form": "으아 듣기만해도 가렵다",
          "original_form": "으아 듣기만해도 가렵다",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027443.1.15",
          "form": "스티커같이 붙이는 모기약이 제일 괜찮던데 추천드립니다",
          "original_form": "스티커같이 붙이는 모기약이 제일 괜찮던데 추천드립니다",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027443.1.16",
          "form": "그런 것도 있었군요",
          "original_form": "그런 것도 있었군요",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027443.1.17",
          "form": "이참에 하나 사야겠어요",
          "original_form": "이참에 하나 사야겠어요",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027443.1.18",
          "form": "저도 지난주애 처음 써봤는데 무심결에 긁는것도 방지돼서 괜찮더라구요ㅎㅎ",
          "original_form": "저도 지난주애 처음 써봤는데 무심결에 긁는것도 방지돼서 괜찮더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 14:25"
        }
