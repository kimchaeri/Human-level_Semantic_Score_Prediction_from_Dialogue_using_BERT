      "utterance": [
        {
          "id": "MDRW2100020370.1.1",
          "form": "보드게임 좋아해여?",
          "original_form": "보드게임 좋아해여?",
          "speaker_id": "2",
          "time": "20210828 02:22"
        },
        {
          "id": "MDRW2100020370.1.2",
          "form": "그럼여",
          "original_form": "그럼여",
          "speaker_id": "1",
          "time": "20210828 02:22"
        },
        {
          "id": "MDRW2100020370.1.3",
          "form": "보드게임 재밋음",
          "original_form": "보드게임 재밋음",
          "speaker_id": "1",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020370.1.4",
          "form": "보드게임카페 가보신적잇으신가요",
          "original_form": "보드게임카페 가보신적잇으신가요",
          "speaker_id": "1",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020370.1.5",
          "form": "많죠ㅋㅋ",
          "original_form": "많죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020370.1.6",
          "form": "뭐좋아해여",
          "original_form": "뭐좋아해여",
          "speaker_id": "2",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020370.1.7",
          "form": "스플랜더 아십니까",
          "original_form": "스플랜더 아십니까",
          "speaker_id": "1",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020370.1.8",
          "form": "옛날에 해본거같은데",
          "original_form": "옛날에 해본거같은데",
          "speaker_id": "2",
          "time": "20210828 02:24"
        },
        {
          "id": "MDRW2100020370.1.9",
          "form": "매번 규칙봐야됨",
          "original_form": "매번 규칙봐야됨",
          "speaker_id": "2",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020370.1.10",
          "form": "저희회사는 왜 동호회없나요",
          "original_form": "저희회사는 왜 동호회없나요",
          "speaker_id": "1",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020370.1.11",
          "form": "다른회사는 매주 목요일마다 2시간씩 동호회모임 갖던데",
          "original_form": "다른회사는 매주 목요일마다 2시간씩 동호회모임 갖던데",
          "speaker_id": "1",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020370.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020370.1.13",
          "form": "업무시간에요?",
          "original_form": "업무시간에요?",
          "speaker_id": "2",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020370.1.14",
          "form": "보드게임 동호회 만들죠 회사에",
          "original_form": "보드게임 동호회 만들죠 회사에",
          "speaker_id": "1",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020370.1.15",
          "form": "넹.쉬는거임",
          "original_form": "넹.쉬는거임",
          "speaker_id": "1",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020370.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020370.1.17",
          "form": "그 저",
          "original_form": "그 저",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020370.1.18",
          "form": "보드게임 친구들하고",
          "original_form": "보드게임 친구들하고",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020370.1.19",
          "form": "안해본게임 대여해서",
          "original_form": "안해본게임 대여해서",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020370.1.20",
          "form": "자취방에서 가끔 하그든요ㅋㅋ",
          "original_form": "자취방에서 가끔 하그든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020370.1.21",
          "form": "대여료 쌈",
          "original_form": "대여료 쌈",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020370.1.22",
          "form": "넹어디서 대여함",
          "original_form": "넹어디서 대여함",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020370.1.23",
          "form": "얼마임",
          "original_form": "얼마임",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020370.1.24",
          "form": "거기 동호회네요",
          "original_form": "거기 동호회네요",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.25",
          "form": "인터넷에 시키니까 배송오던데요?ㅋㅋ",
          "original_form": "인터넷에 시키니까 배송오던데요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.26",
          "form": "오 신기하다",
          "original_form": "오 신기하다",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.27",
          "form": "게임 4개에 만 얼마했던듯",
          "original_form": "게임 4개에 만 얼마했던듯",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.28",
          "form": "전 생일선물로 스플렌더 받음",
          "original_form": "전 생일선물로 스플렌더 받음",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.29",
          "form": "달무티 아시나요",
          "original_form": "달무티 아시나요",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.30",
          "form": "지난주에 많이했조",
          "original_form": "지난주에 많이했조",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.31",
          "form": "몰라여",
          "original_form": "몰라여",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.32",
          "form": "왜 혼자 재밋는거하심",
          "original_form": "왜 혼자 재밋는거하심",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.33",
          "form": "게임 넘 많음",
          "original_form": "게임 넘 많음",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.34",
          "form": "저도꺼주삼",
          "original_form": "저도꺼주삼",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.35",
          "form": "껴주삼",
          "original_form": "껴주삼",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.36",
          "form": "호호",
          "original_form": "호호",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020370.1.37",
          "form": "서울대입구와야됨",
          "original_form": "서울대입구와야됨",
