          "id": "MDRW2100025962.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025962.1.2",
          "form": "아뇨ㅜ",
          "original_form": "아뇨ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025962.1.3",
          "form": "저도 여건이",
          "original_form": "저도 여건이",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100025962.1.4",
          "form": "돈이많이들것같더라구요",
          "original_form": "돈이많이들것같더라구요",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100025962.1.5",
          "form": "사람보다 더",
          "original_form": "사람보다 더",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100025962.1.6",
          "form": "맞아요 그래서 기르고 싶지만 못 길러요ㅜ",
          "original_form": "맞아요 그래서 기르고 싶지만 못 길러요ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100025962.1.7",
          "form": "케어가어렵죠",
          "original_form": "케어가어렵죠",
          "speaker_id": "1",
          "time": "20210902 07:20"
        },
        {
          "id": "MDRW2100025962.1.8",
          "form": "맞아요 같이 하루종일 있지도못하고",
          "original_form": "맞아요 같이 하루종일 있지도못하고",
          "speaker_id": "2",
          "time": "20210902 07:28"
        },
        {
          "id": "MDRW2100025962.1.9",
          "form": "시간이 문제죠",
          "original_form": "시간이 문제죠",
          "speaker_id": "1",
          "time": "20210902 07:30"
        },
        {
          "id": "MDRW2100025962.1.10",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 07:54"
        },
        {
          "id": "MDRW2100025962.1.11",
          "form": "좋아는 하지만 힘들거 같아서",
          "original_form": "좋아는 하지만 힘들거 같아서",
          "speaker_id": "1",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100025962.1.12",
          "form": "그렇군요 ㅜ 아쉽겠어요",
          "original_form": "그렇군요 ㅜ 아쉽겠어요",
          "speaker_id": "2",
          "time": "20210902 08:04"
