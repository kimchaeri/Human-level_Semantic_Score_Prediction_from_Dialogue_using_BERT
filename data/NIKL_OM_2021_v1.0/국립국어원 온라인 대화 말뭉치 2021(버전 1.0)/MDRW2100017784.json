      "utterance": [
        {
          "id": "MDRW2100017784.1.1",
          "form": "잡지 같은거 구독하시나요?",
          "original_form": "잡지 같은거 구독하시나요?",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017784.1.2",
          "form": "구독하는 건 없어요",
          "original_form": "구독하는 건 없어요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017784.1.3",
          "form": "간혹 부록 좋은 패션지 정도 사서 봐요",
          "original_form": "간혹 부록 좋은 패션지 정도 사서 봐요",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017784.1.4",
          "form": "요즘도 부록 많이 껴주나여?",
          "original_form": "요즘도 부록 많이 껴주나여?",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017784.1.5",
          "form": "간혹 정품 기초나 메이크업제품 이 나오더라구요",
          "original_form": "간혹 정품 기초나 메이크업제품 이 나오더라구요",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017784.1.6",
          "form": "정가가 잡지 몇배가 되는...",
          "original_form": "정가가 잡지 몇배가 되는...",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017784.1.7",
          "form": "예전엔 진짜 부록에 혹해서 잡지 많이샀었는데..",
          "original_form": "예전엔 진짜 부록에 혹해서 잡지 많이샀었는데..",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017784.1.8",
          "form": "요즘도하는줄 몰랐네여..",
          "original_form": "요즘도하는줄 몰랐네여..",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017784.1.9",
          "form": "요즘도 하더라구요",
          "original_form": "요즘도 하더라구요",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017784.1.10",
          "form": "예전엔 일본잡지도 많이 샀는데",
          "original_form": "예전엔 일본잡지도 많이 샀는데",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017784.1.11",
          "form": "이쁜 에코백같은거 많이 줬거든요",
          "original_form": "이쁜 에코백같은거 많이 줬거든요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017784.1.12",
          "form": "캰캰?",
          "original_form": "캰캰?",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017784.1.13",
          "form": "오~ 아시는 군요",
          "original_form": "오~ 아시는 군요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017784.1.14",
          "form": "근데 그 잡지는 부록 별로",
          "original_form": "근데 그 잡지는 부록 별로",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017784.1.15",
          "form": "일본잡지 많이 봤는데.. 다른건 기억이 하나도 안나네요...",
          "original_form": "일본잡지 많이 봤는데.. 다른건 기억이 하나도 안나네요...",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017784.1.16",
          "form": "린넨 이나 s카와이 이런건 꽤 괜찮았다능",
          "original_form": "린넨 이나 s카와이 이런건 꽤 괜찮았다능",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017784.1.17",
          "form": "요즘도ㅜ일본은 잡지 많이나오겠죠?\n한귝은 이제 잡지 볼려고 사는 사람은 거의 엄는듯..",
          "original_form": "요즘도ㅜ일본은 잡지 많이나오겠죠?\n한귝은 이제 잡지 볼려고 사는 사람은 거의 엄는듯..",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017784.1.18",
          "form": "그쵸 내용이 다 광고고",
          "original_form": "그쵸 내용이 다 광고고",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017784.1.19",
          "form": "인터넷 찾아보면 소식이나 정보는 다 있고",
          "original_form": "인터넷 찾아보면 소식이나 정보는 다 있고",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017784.1.20",
          "form": "급 궁금해서 잡지나 한번 찾아좌야겟네요.. ㅋㅋ",
          "original_form": "급 궁금해서 잡지나 한번 찾아좌야겟네요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:39"
        }
      ]
