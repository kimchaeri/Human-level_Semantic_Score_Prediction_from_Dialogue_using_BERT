      "utterance": [
        {
          "id": "MDRW2100010834.1.1",
          "form": "이번달에 살 거 있어?",
          "original_form": "이번달에 살 거 있어?",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010834.1.2",
          "form": "11일에 파자마 사서 더 필요한 건 없어",
          "original_form": "11일에 파자마 사서 더 필요한 건 없어",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010834.1.3",
          "form": "파자마는 잘 맞아? 마음에 들고?",
          "original_form": "파자마는 잘 맞아? 마음에 들고?",
          "speaker_id": "1",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010834.1.4",
          "form": "응 사길 잘한 것 같아",
          "original_form": "응 사길 잘한 것 같아",
          "speaker_id": "2",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010834.1.5",
          "form": "입으면 기분이 좋아~",
          "original_form": "입으면 기분이 좋아~",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010834.1.6",
          "form": "엄마도 옷 사고 싶은데 마음에 드는 게 없네.",
          "original_form": "엄마도 옷 사고 싶은데 마음에 드는 게 없네.",
          "speaker_id": "1",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010834.1.7",
          "form": "어떤 옷 사고 싶은데?",
          "original_form": "어떤 옷 사고 싶은데?",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010834.1.8",
          "form": "요즘 원피스가 입고 싶어.",
          "original_form": "요즘 원피스가 입고 싶어.",
          "speaker_id": "1",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010834.1.9",
          "form": "예쁜 거 있으면 찾아 봐줘.",
          "original_form": "예쁜 거 있으면 찾아 봐줘.",
          "speaker_id": "1",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010834.1.10",
          "form": "알겠어 장바구니에 넣어놓을게~",
          "original_form": "알겠어 장바구니에 넣어놓을게~",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010834.1.11",
          "form": "너 다음달에 사야할 건 없어?",
          "original_form": "너 다음달에 사야할 건 없어?",
          "speaker_id": "1",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010834.1.12",
          "form": "미리 리스트 만들어 놓게.",
          "original_form": "미리 리스트 만들어 놓게.",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010834.1.13",
          "form": "음 세제 다 떨어져서 사야 되고, 치약도 사야 돼",
          "original_form": "음 세제 다 떨어져서 사야 되고, 치약도 사야 돼",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010834.1.14",
          "form": "알겠어 그럼 쇼핑몰에서 최저가 찾아서 카드 결제 해.",
          "original_form": "알겠어 그럼 쇼핑몰에서 최저가 찾아서 카드 결제 해.",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010834.1.15",
          "form": "오케이~ 근데 세일은 11일에 하니까 그때가서 결제하는게 좋을 것 같아",
          "original_form": "오케이~ 근데 세일은 11일에 하니까 그때가서 결제하는게 좋을 것 같아",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010834.1.16",
          "form": "5만원 이상 사면 7천원 할인쿠폰 줘!",
          "original_form": "5만원 이상 사면 7천원 할인쿠폰 줘!",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010834.1.17",
          "form": "그래 그럼 5만원에 맞춰서 결제하면 되겠다.",
          "original_form": "그래 그럼 5만원에 맞춰서 결제하면 되겠다.",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010834.1.18",
          "form": "집에서 먹을 과자도 같이 주문해줘.",
          "original_form": "집에서 먹을 과자도 같이 주문해줘.",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010834.1.19",
          "form": "응 저번에 사먹은 버터쿠키 괜찮았어?",
          "original_form": "응 저번에 사먹은 버터쿠키 괜찮았어?",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010834.1.20",
          "form": "맛있었으면 그걸로 살게",
          "original_form": "맛있었으면 그걸로 살게",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010834.1.21",
          "form": "응 버터쿠키 맛있었어. 그거랑 생필품이랄 같이 시켜주면 고맙겠어~.",
          "original_form": "응 버터쿠키 맛있었어. 그거랑 생필품이랄 같이 시켜주면 고맙겠어~.",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010834.1.22",
          "form": "응 쇼핑몰에서 주문하고 다시 알려줄게!",
          "original_form": "응 쇼핑몰에서 주문하고 다시 알려줄게!",
          "speaker_id": "2",
          "time": "20210823 22:29"
        }
      ]
    }
