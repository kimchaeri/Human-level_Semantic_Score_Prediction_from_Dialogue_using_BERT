      "utterance": [
        {
          "id": "MDRW2100026442.1.1",
          "form": "30분안에",
          "original_form": "30분안에",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026442.1.2",
          "form": "주무실수있으세용???",
          "original_form": "주무실수있으세용???",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026442.1.3",
          "form": "비소리가 들려도 ㅠ",
          "original_form": "비소리가 들려도 ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026442.1.4",
          "form": "사실 그렇게 막 불면증 있고 그러지누않아서",
          "original_form": "사실 그렇게 막 불면증 있고 그러지누않아서",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026442.1.5",
          "form": "기본적으로 잘 자긴해요",
          "original_form": "기본적으로 잘 자긴해요",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026442.1.6",
          "form": "아.... ㅎㅎ 그래도",
          "original_form": "아.... ㅎㅎ 그래도",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026442.1.7",
          "form": "빗소리가 도움이 된다니 다행이네요 ㅎㅎ",
          "original_form": "빗소리가 도움이 된다니 다행이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026442.1.8",
          "form": "뭐라도 자는데 도움을 주니 다행이죠ㅠ",
          "original_form": "뭐라도 자는데 도움을 주니 다행이죠ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026442.1.9",
          "form": "비 오고 나면 또 조금 쌀쌀해질거라고 생각했는데",
          "original_form": "비 오고 나면 또 조금 쌀쌀해질거라고 생각했는데",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026442.1.10",
          "form": "비그치면 또 후덥지간하고 비오면 갑갑하고",
          "original_form": "비그치면 또 후덥지간하고 비오면 갑갑하고",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026442.1.11",
          "form": "무한반복이네요",
          "original_form": "무한반복이네요",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026442.1.12",
          "form": "아 그니까요...진짜..진짜...진짜...... 습한거만 럾으면 살텐데",
          "original_form": "아 그니까요...진짜..진짜...진짜...... 습한거만 럾으면 살텐데",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026442.1.13",
          "form": "3면이 바다인 우리나라에선 불가하겠죠 ㅠ",
          "original_form": "3면이 바다인 우리나라에선 불가하겠죠 ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026442.1.14",
          "form": "후.. 맞아요",
          "original_form": "후.. 맞아요",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026442.1.15",
          "form": "초기하고 살아야하는데 포기가 안되네요",
          "original_form": "초기하고 살아야하는데 포기가 안되네요",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026442.1.16",
          "form": "안되조 ㅠㅠ 포기가 될리가없어요 ㅠㅠ",
          "original_form": "안되조 ㅠㅠ 포기가 될리가없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026442.1.17",
          "form": "할머니되도 불평불만할듯",
          "original_form": "할머니되도 불평불만할듯",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026442.1.18",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 저 미래 꿈이 욕쟁이 할머니에요..",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 저 미래 꿈이 욕쟁이 할머니에요..",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026442.1.19",
          "form": "불평불만 다하고 살거야..",
          "original_form": "불평불만 다하고 살거야..",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026442.1.20",
          "form": "왘ㅋㅋㅋㅋㅋㅋ 좋은 꿈인거같아요!",
          "original_form": "왘ㅋㅋㅋㅋㅋㅋ 좋은 꿈인거같아요!",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026442.1.21",
          "form": "진짜 그때라도 다해야죠!",
          "original_form": "진짜 그때라도 다해야죠!",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026442.1.22",
          "form": "치매만안걸리면 딱 좋아요 ㅎㅎ",
          "original_form": "치매만안걸리면 딱 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026442.1.23",
          "form": "또 욕은 맨정신에 해야죠",
          "original_form": "또 욕은 맨정신에 해야죠",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026442.1.24",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026442.1.25",
          "form": "타당한 욕은 괜찮지만 마구잡이로 할 순 없으니",
          "original_form": "타당한 욕은 괜찮지만 마구잡이로 할 순 없으니",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026442.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ그쵸그춐ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그쵸그춐ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026442.1.27",
          "form": "그럼찐으로 나뿐사람",
          "original_form": "그럼찐으로 나뿐사람",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026442.1.28",
          "form": "사업하나 하면서 간판 하나 내걸려구요",
          "original_form": "사업하나 하면서 간판 하나 내걸려구요",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026442.1.29",
          "form": "욕쟁이할머니카페 ㅎㅎ",
          "original_form": "욕쟁이할머니카페 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026442.1.30",
          "form": "욕쟁이할머니 국밥집",
          "original_form": "욕쟁이할머니 국밥집",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026442.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026442.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ메뉴는 아메 하나 뿐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ메뉴는 아메 하나 뿐",
          "speaker_id": "2",
