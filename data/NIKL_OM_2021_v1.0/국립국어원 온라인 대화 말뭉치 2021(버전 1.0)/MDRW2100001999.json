      "utterance": [
        {
          "id": "MDRW2100001999.1.1",
          "form": "해외여행갈수 있는 날이 올까요?",
          "original_form": "해외여행갈수 있는 날이 올까요?",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001999.1.2",
          "form": "그러게요 작년부터 못가니 미칠거 같아요",
          "original_form": "그러게요 작년부터 못가니 미칠거 같아요",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001999.1.3",
          "form": "몇년동안 못가겠죠?",
          "original_form": "몇년동안 못가겠죠?",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001999.1.4",
          "form": "백신다맞고 내후년쯤은 괜찮아질려나요?",
          "original_form": "백신다맞고 내후년쯤은 괜찮아질려나요?",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001999.1.5",
          "form": "결혼하는사람들 신혼여행 못가서 넘 우울할거같아요",
          "original_form": "결혼하는사람들 신혼여행 못가서 넘 우울할거같아요",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001999.1.6",
          "form": "그러게요 결혼식도 꽝  신혼여행도 꽝",
          "original_form": "그러게요 결혼식도 꽝  신혼여행도 꽝",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001999.1.7",
          "form": "name2님은 휴가계획없으세요?",
          "original_form": "&name2&님은 휴가계획없으세요?",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001999.1.8",
          "form": "일도 안하는데 휴가가 필요없는거 같아요 ㅋㅋ",
          "original_form": "일도 안하는데 휴가가 필요없는거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001999.1.9",
          "form": "ㅋㅋ 그래도 리프레시 차원에서 힐링하러가는거죠",
          "original_form": "ㅋㅋ 그래도 리프레시 차원에서 힐링하러가는거죠",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001999.1.10",
          "form": "매일 휴가요 ㅋㅈㅋ",
          "original_form": "매일 휴가요 ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001999.1.11",
          "form": "제주도 가고싶은데 4단계라 이제 그것도 못가겠어요",
          "original_form": "제주도 가고싶은데 4단계라 이제 그것도 못가겠어요",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001999.1.12",
          "form": "백신 안맞을거라 꼼짝마라예요",
          "original_form": "백신 안맞을거라 꼼짝마라예요",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001999.1.13",
          "form": "진짜 태국가서 1일1마사지 받고싶은데 기약도 없고",
          "original_form": "진짜 태국가서 1일1마사지 받고싶은데 기약도 없고",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001999.1.14",
          "form": "어깨마시지는 좋던데",
          "original_form": "어깨마시지는 좋던데",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001999.1.15",
          "form": "3년전 캄보디아도 넘 좋았거든요",
          "original_form": "3년전 캄보디아도 넘 좋았거든요",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001999.1.16",
          "form": "캄보디아 가보고 싶어요 추천할곳 있나요?",
          "original_form": "캄보디아 가보고 싶어요 추천할곳 있나요?",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100001999.1.17",
          "form": "앙코르와트는 꼭 가보세요 일출 일몰보는거 추천이요",
          "original_form": "앙코르와트는 꼭 가보세요 일출 일몰보는거 추천이요",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100001999.1.18",
          "form": "친구가 가보고 싶다고 같이 가자했는데",
          "original_form": "친구가 가보고 싶다고 같이 가자했는데",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001999.1.19",
          "form": "공부는 하고 가야 멋있고 좋은데 그냥가서보면 돌덩이에 불과해요ㅎ",
          "original_form": "공부는 하고 가야 멋있고 좋은데 그냥가서보면 돌덩이에 불과해요ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001999.1.20",
          "form": "공부는 싫으니 그냥 보고 와야겠어요",
          "original_form": "공부는 싫으니 그냥 보고 와야겠어요",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100001999.1.21",
          "form": "현지 가이드 연결해서 같이보면 설명도 듣고 좋더라구요 한국말도 엄청 잘하고ㅋ",
          "original_form": "현지 가이드 연결해서 같이보면 설명도 듣고 좋더라구요 한국말도 엄청 잘하고ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001999.1.22",
          "form": "좋은 방법이네요",
          "original_form": "좋은 방법이네요",
          "speaker_id": "2",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001999.1.23",
          "form": "일출은 강추요 넘 멋있어요",
          "original_form": "일출은 강추요 넘 멋있어요",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001999.1.24",
          "form": "일찍 못일어나서 일몰로 가야겠어요",
          "original_form": "일찍 못일어나서 일몰로 가야겠어요",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001999.1.25",
          "form": "물가도 싸서 걍 개인적으로 가이드 신청해서 다녀도 편하고 좋아요",
          "original_form": "물가도 싸서 걍 개인적으로 가이드 신청해서 다녀도 편하고 좋아요",
          "speaker_id": "1",
          "time": "20210819 22:55"
        }
      ]
