      "utterance": [
        {
          "id": "MDRW2100047619.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100047619.1.2",
          "form": "혹시 연애중이신가요?",
          "original_form": "혹시 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100047619.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100047619.1.4",
          "form": "아뇨 안하고 있어요~",
          "original_form": "아뇨 안하고 있어요~",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100047619.1.5",
          "form": "name1님은 연애중이신가요?",
          "original_form": "&name1&님은 연애중이신가요?",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100047619.1.6",
          "form": "네 연애중이예요",
          "original_form": "네 연애중이예요",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100047619.1.7",
          "form": "연애생각은 있으신가요?",
          "original_form": "연애생각은 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100047619.1.8",
          "form": "요즘 아예 생각없는분들도 많아서 물어봐요",
          "original_form": "요즘 아예 생각없는분들도 많아서 물어봐요",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100047619.1.9",
          "form": "당장은 연애에 대해서 별 생각은 없는데 좋은 사람이 있음 만나야겠다는 생각은 있어요",
          "original_form": "당장은 연애에 대해서 별 생각은 없는데 좋은 사람이 있음 만나야겠다는 생각은 있어요",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047619.1.10",
          "form": "근데 주변엔 영 없네요 ㅠㅠ",
          "original_form": "근데 주변엔 영 없네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047619.1.11",
          "form": "이상형있으세요?",
          "original_form": "이상형있으세요?",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047619.1.12",
          "form": "두루뭉술 하긴한데 코드 잘 맞고 볼수록 괜찮은 사람이요",
          "original_form": "두루뭉술 하긴한데 코드 잘 맞고 볼수록 괜찮은 사람이요",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100047619.1.13",
          "form": "약간 이성볼 때 잘 안맞는 점이나 단점이 보이면 확 싫어지더라구요",
          "original_form": "약간 이성볼 때 잘 안맞는 점이나 단점이 보이면 확 싫어지더라구요",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100047619.1.14",
          "form": "코드가 잘맞는다는 점이 진짜 많은걸 포함하누 말인거 같아요",
          "original_form": "코드가 잘맞는다는 점이 진짜 많은걸 포함하누 말인거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047619.1.15",
          "form": "관점 가치관 관심사 등등 이요",
          "original_form": "관점 가치관 관심사 등등 이요",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047619.1.16",
          "form": "맞아요 저것들이 맞는게 쉽지 않으니까요~",
          "original_form": "맞아요 저것들이 맞는게 쉽지 않으니까요~",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100047619.1.17",
          "form": "지금 만나시는 분이랑은 코드가 잘 맞으세요?",
          "original_form": "지금 만나시는 분이랑은 코드가 잘 맞으세요?",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100047619.1.18",
          "form": "네 생활패턴도 가치관도 취미도 비슷해요",
          "original_form": "네 생활패턴도 가치관도 취미도 비슷해요",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100047619.1.19",
          "form": "꽤 맞는편인거 같아요",
          "original_form": "꽤 맞는편인거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100047619.1.20",
          "form": "잘 만나셨네요! 그렇게 잘 맞기도 쉽지 않은데~",
          "original_form": "잘 만나셨네요! 그렇게 잘 맞기도 쉽지 않은데~",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100047619.1.21",
          "form": "식성도 비슷하신가요?",
          "original_form": "식성도 비슷하신가요?",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100047619.1.22",
          "form": "저는 단것을 안좋아하는데 애인은 단거 좋아하는거만 빼면 비슷해요",
          "original_form": "저는 단것을 안좋아하는데 애인은 단거 좋아하는거만 빼면 비슷해요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100047619.1.23",
          "form": "단거는 대부분 디저트류니까 식성 잘 맞으시면 같이 식사하실때 좋을 것 같아요!",
          "original_form": "단거는 대부분 디저트류니까 식성 잘 맞으시면 같이 식사하실때 좋을 것 같아요!",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100047619.1.24",
          "form": "name1님은 결혼 생각은 있으신가요?",
          "original_form": "&name1&님은 결혼 생각은 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100047619.1.25",
          "form": "아직 없어요",
          "original_form": "아직 없어요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        }
      ]
