      "utterance": [
        {
          "id": "MDRW2100041933.1.1",
          "form": "맞나 할려고하나",
          "original_form": "맞나 할려고하나",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041933.1.2",
          "form": "응 어제도 내가 까먹고있는데",
          "original_form": "응 어제도 내가 까먹고있는데",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041933.1.3",
          "form": "지가 밤되니 엄마 숙제해야지 이러더라",
          "original_form": "지가 밤되니 엄마 숙제해야지 이러더라",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041933.1.4",
          "form": "이학습터도 내가 말안해도 다 듣고",
          "original_form": "이학습터도 내가 말안해도 다 듣고",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041933.1.5",
          "form": "맞나 점점좋아지면 좋겠네",
          "original_form": "맞나 점점좋아지면 좋겠네",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041933.1.6",
          "form": "좋아지고는 있지",
          "original_form": "좋아지고는 있지",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041933.1.7",
          "form": "남들보다 더디니 문제지",
          "original_form": "남들보다 더디니 문제지",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041933.1.8",
          "form": "그래 그래도ㅠ좋아지고있다는게 중요하지",
          "original_form": "그래 그래도ㅠ좋아지고있다는게 중요하지",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041933.1.9",
          "form": "좋아지고있는건 확실하다",
          "original_form": "좋아지고있는건 확실하다",
          "speaker_id": "1",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041933.1.10",
          "form": "작년엔 내가 이학습터 들었다ㅜ쌤이 무서워서ㅜ",
          "original_form": "작년엔 내가 이학습터 들었다ㅜ쌤이 무서워서ㅜ",
          "speaker_id": "1",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041933.1.11",
          "form": "저녁에 name3이가 다시 듣고",
          "original_form": "저녁에 &name3&이가 다시 듣고",
          "speaker_id": "1",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041933.1.12",
          "form": "맞나 대단하다",
          "original_form": "맞나 대단하다",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041933.1.13",
          "form": "name4이한테 잘해줘라",
          "original_form": "&name4&이한테 잘해줘라",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041933.1.14",
          "form": "name3이같은 얘도 있는데ㅜ",
          "original_form": "&name3&이같은 얘도 있는데ㅜ",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041933.1.15",
          "form": "그래 ㅜ 먹는것만 잘챙겨줘도 행복한아이",
          "original_form": "그래 ㅜ 먹는것만 잘챙겨줘도 행복한아이",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041933.1.16",
          "form": "먹는것도 잘 안먹는 아이ㅜ",
          "original_form": "먹는것도 잘 안먹는 아이ㅜ",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041933.1.17",
          "form": "그렇네 ㅠ",
          "original_form": "그렇네 ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041933.1.18",
          "form": "뭐 괜찮은게 하나도 없노ㅜ",
          "original_form": "뭐 괜찮은게 하나도 없노ㅜ",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100041933.1.19",
          "form": "다른복이있을거다",
          "original_form": "다른복이있을거다",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041933.1.20",
          "form": "저녁은 먹었나?\r\n우린일찍먹고 설거지 다해간다",
          "original_form": "저녁은 먹었나?\r\n우린일찍먹고 설거지 다해간다",
