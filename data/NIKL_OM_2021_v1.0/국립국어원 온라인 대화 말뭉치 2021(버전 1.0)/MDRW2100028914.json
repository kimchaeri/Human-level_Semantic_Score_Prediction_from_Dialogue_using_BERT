      "utterance": [
        {
          "id": "MDRW2100028914.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028914.1.2",
          "form": "오늘은 청소기 한번 돌려써요",
          "original_form": "오늘은 청소기 한번 돌려써요",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028914.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028914.1.4",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100028914.1.5",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210903 08:12"
        },
        {
          "id": "MDRW2100028914.1.6",
          "form": "저는 설거지가 제일 싫어요",
          "original_form": "저는 설거지가 제일 싫어요",
          "speaker_id": "2",
          "time": "20210903 08:15"
        },
        {
          "id": "MDRW2100028914.1.7",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210903 08:16"
        },
        {
          "id": "MDRW2100028914.1.8",
          "form": "저는 수영장 냄새라 약하게 맡으면 좋아하는 ㅜㅜㅋㅋ",
          "original_form": "저는 수영장 냄새라 약하게 맡으면 좋아하는 ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100028914.1.9",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100028914.1.10",
          "form": "독하면 어지럽긴해요",
          "original_form": "독하면 어지럽긴해요",
          "speaker_id": "2",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100028914.1.11",
          "form": "마스크껴도안됨",
          "original_form": "마스크껴도안됨",
          "speaker_id": "1",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100028914.1.12",
          "form": "ㄷㄷ 환기 필수네요",
          "original_form": "ㄷㄷ 환기 필수네요",
          "speaker_id": "2",
          "time": "20210903 08:18"
        },
        {
          "id": "MDRW2100028914.1.13",
          "form": "곰팡이안 생기게 자주햐죠",
          "original_form": "곰팡이안 생기게 자주햐죠",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100028914.1.14",
          "form": "저는 바닥 닦기 귀찮아서",
          "original_form": "저는 바닥 닦기 귀찮아서",
          "speaker_id": "2",
          "time": "20210903 09:34"
        },
        {
