      "utterance": [
        {
          "id": "MDRW2100020226.1.1",
          "form": "약간 만들기 겁나힘든데",
          "original_form": "약간 만들기 겁나힘든데",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020226.1.2",
          "form": "강릉시장에서",
          "original_form": "강릉시장에서",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020226.1.3",
          "form": "바로 갈아서 해주시는",
          "original_form": "바로 갈아서 해주시는",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020226.1.4",
          "form": "감자전 먹고싶네요",
          "original_form": "감자전 먹고싶네요",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020226.1.5",
          "form": "감자 가느라 와 성격버리겠다 하는데 먹으면 다 풀리는",
          "original_form": "감자 가느라 와 성격버리겠다 하는데 먹으면 다 풀리는",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020226.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020226.1.7",
          "form": "강릉 가야겠네",
          "original_form": "강릉 가야겠네",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020226.1.8",
          "form": "감자전을 팔다니",
          "original_form": "감자전을 팔다니",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020226.1.9",
          "form": "거기 감자전 어땠어요",
          "original_form": "거기 감자전 어땠어요",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020226.1.10",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020226.1.11",
          "form": "말잇못입니다",
          "original_form": "말잇못입니다",
          "speaker_id": "1",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020226.1.12",
          "form": "청 양 고 추 를 넣 어 주 시 던 가 요",
          "original_form": "청 양 고 추 를 넣 어 주 시 던 가 요",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020226.1.13",
          "form": "주문하면 바로 갈아서 부쳐주세요",
          "original_form": "주문하면 바로 갈아서 부쳐주세요",
          "speaker_id": "1",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020226.1.14",
          "form": "아뇨ㅠ 그저 간장에 잇엇을 뿐...",
          "original_form": "아뇨ㅠ 그저 간장에 잇엇을 뿐...",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020226.1.15",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020226.1.16",
          "form": "괜찮아요 간장에라도 있으면 된거죠",
          "original_form": "괜찮아요 간장에라도 있으면 된거죠",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020226.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020226.1.18",
          "form": "메밀전 드셔보셧나요?",
          "original_form": "메밀전 드셔보셧나요?",
          "speaker_id": "1",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020226.1.19",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020226.1.20",
          "form": "메밀전도 맛있잖아요",
          "original_form": "메밀전도 맛있잖아요",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020226.1.21",
          "form": "메밀전 메밀전병 이런거 아주 좋아합니다",
          "original_form": "메밀전 메밀전병 이런거 아주 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020226.1.22",
          "form": "맞아요 메밀전 모르는사람",
          "original_form": "맞아요 메밀전 모르는사람",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020226.1.23",
          "form": "은근잇더라구요",
          "original_form": "은근잇더라구요",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020226.1.24",
          "form": "그 맛있는걸 모르다니",
          "original_form": "그 맛있는걸 모르다니",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020226.1.25",
          "form": "인생을 헛살았다",
          "original_form": "인생을 헛살았다",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020226.1.26",
          "form": "당장 손잡고가서 먹여주고싶어지는",
          "original_form": "당장 손잡고가서 먹여주고싶어지는",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020226.1.27",
          "form": "그니까요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020226.1.28",
          "form": "맛있는건 다 먹어봐야해요",
          "original_form": "맛있는건 다 먹어봐야해요",
          "speaker_id": "2",
          "time": "20210828 00:33"
        }
      ]
    }
  ]
