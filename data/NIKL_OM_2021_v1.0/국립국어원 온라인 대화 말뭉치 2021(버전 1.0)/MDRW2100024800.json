        {
          "id": "MDRW2100024800.1.1",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100024800.1.2",
          "form": "동물완전사랑하죠",
          "original_form": "동물완전사랑하죠",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100024800.1.3",
          "form": "글쿤요 전 동물무서워요",
          "original_form": "글쿤요 전 동물무서워요",
          "speaker_id": "2",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100024800.1.4",
          "form": "저는 동물키워보고싶은데아직안키워봤어요",
          "original_form": "저는 동물키워보고싶은데아직안키워봤어요",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100024800.1.5",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100024800.1.6",
          "form": "저도요ㅋ 키울 생각도없어요ㅜ",
          "original_form": "저도요ㅋ 키울 생각도없어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100024800.1.7",
          "form": "나중에 기회되면 키우고싶어요",
          "original_form": "나중에 기회되면 키우고싶어요",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100024800.1.8",
          "form": "동물은좋아하는데",
          "original_form": "동물은좋아하는데",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100024800.1.9",
          "form": "키울능력이안되서",
          "original_form": "키울능력이안되서",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100024800.1.10",
          "form": "돌봐줄사람도없고",
          "original_form": "돌봐줄사람도없고",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100024800.1.11",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100024800.1.12",
          "form": "혼자두면 안되나요?",
          "original_form": "혼자두면 안되나요?",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100024800.1.13",
          "form": "동물은 혼자놔두몃안되자나나요",
          "original_form": "동물은 혼자놔두몃안되자나나요",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100024800.1.14",
          "form": "몰라요ㅋ 혼자두는거봣는뎅...",
          "original_form": "몰라요ㅋ 혼자두는거봣는뎅...",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100024800.1.15",
          "form": "혼자놔둬도 되면 당장 키우고싶어요!!!",
          "original_form": "혼자놔둬도 되면 당장 키우고싶어요!!!",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100024800.1.16",
          "form": "저는 안 키우고 싶어요ㅋ",
          "original_form": "저는 안 키우고 싶어요ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100024800.1.17",
          "form": "동물 보는거 좋아하세요?",
          "original_form": "동물 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 07:06"
        },
        {
          "id": "MDRW2100024800.1.18",
          "form": "보는것도 그다지.. 음 작은 동물이 귀엽긴해요",
          "original_form": "보는것도 그다지.. 음 작은 동물이 귀엽긴해요",
