      "utterance": [
        {
          "id": "MDRW2100038865.1.1",
          "form": "맞아요 혼자 막 허우적대고 있어ㅋㅋ",
          "original_form": "맞아요 혼자 막 허우적대고 있어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038865.1.2",
          "form": "그러니까요 보고 있음 정말",
          "original_form": "그러니까요 보고 있음 정말",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038865.1.3",
          "form": "어떤 날은 전선 다 감고 있고",
          "original_form": "어떤 날은 전선 다 감고 있고",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038865.1.4",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038865.1.5",
          "form": "살림없는 면적 넓은 집에서는",
          "original_form": "살림없는 면적 넓은 집에서는",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038865.1.6",
          "form": "나름 괜찮을수도있겠지만",
          "original_form": "나름 괜찮을수도있겠지만",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038865.1.7",
          "form": "그런 집이 얼마나 있을까요ㅋㅋㅋㅋ",
          "original_form": "그런 집이 얼마나 있을까요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038865.1.8",
          "form": "저희집에서는 일이 더 많아지더라고요 오히려",
          "original_form": "저희집에서는 일이 더 많아지더라고요 오히려",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038865.1.9",
          "form": "저흰 애기가",
          "original_form": "저흰 애기가",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038865.1.10",
          "form": "로봇 청소기 만 보면 울어서",
          "original_form": "로봇 청소기 만 보면 울어서",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038865.1.11",
          "form": "그러니까 말이에요",
          "original_form": "그러니까 말이에요",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038865.1.12",
          "form": "정말 슬펐어요ㅋㅋ",
          "original_form": "정말 슬펐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038865.1.13",
          "form": "아이고 저런ㅜㅜ",
          "original_form": "아이고 저런ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038865.1.14",
          "form": "끝없는 집안일",
          "original_form": "끝없는 집안일",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038865.1.15",
          "form": "해도 티도 안 나는",
          "original_form": "해도 티도 안 나는",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038865.1.16",
          "form": "티안나는게 진짜 허무하죠",
          "original_form": "티안나는게 진짜 허무하죠",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038865.1.17",
