      "utterance": [
        {
          "id": "MDRW2100009893.1.1",
          "form": "점심메뉴 뭔가요?",
          "original_form": "점심메뉴 뭔가요?",
          "speaker_id": "1",
          "time": "20210823 11:23"
        },
        {
          "id": "MDRW2100009893.1.2",
          "form": "저는 떡볶이 먹으려고요",
          "original_form": "저는 떡볶이 먹으려고요",
          "speaker_id": "2",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009893.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009893.1.4",
          "form": "떡볶이 맛있겠다 저는 초밥이요",
          "original_form": "떡볶이 맛있겠다 저는 초밥이요",
          "speaker_id": "1",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009893.1.5",
          "form": "떡볶이 배달시켜드시나요",
          "original_form": "떡볶이 배달시켜드시나요",
          "speaker_id": "1",
          "time": "20210823 11:24"
        },
        {
          "id": "MDRW2100009893.1.6",
          "form": "아마 조리해서 먹을 것 같아요",
          "original_form": "아마 조리해서 먹을 것 같아요",
          "speaker_id": "2",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009893.1.7",
          "form": "초밥은 배달시켜 드시나요?",
          "original_form": "초밥은 배달시켜 드시나요?",
          "speaker_id": "2",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009893.1.8",
          "form": "네 상무초밥 배달시키려구요",
          "original_form": "네 상무초밥 배달시키려구요",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009893.1.9",
          "form": "어떤 스타일의 떡볶이를 만들어드시나요",
          "original_form": "어떤 스타일의 떡볶이를 만들어드시나요",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009893.1.10",
          "form": "전 로제떡볶이 먹으려구요",
          "original_form": "전 로제떡볶이 먹으려구요",
          "speaker_id": "2",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009893.1.11",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009893.1.12",
          "form": "추가하는 재료있으신가요",
          "original_form": "추가하는 재료있으신가요",
          "speaker_id": "1",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009893.1.13",
          "form": "요즘 소스가 잘 나와서 조리만 하면 맛있어요ㅋㅋ",
          "original_form": "요즘 소스가 잘 나와서 조리만 하면 맛있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009893.1.14",
          "form": "떡볶이에 계란넣으시나요",
          "original_form": "떡볶이에 계란넣으시나요",
          "speaker_id": "1",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009893.1.15",
          "form": "있으면 넣는데 안넣을 때가 더 많아요",
          "original_form": "있으면 넣는데 안넣을 때가 더 많아요",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009893.1.16",
          "form": "상무초밥은 프랜차이즌가요?",
          "original_form": "상무초밥은 프랜차이즌가요?",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009893.1.17",
          "form": "네 프랜차이즈에요",
          "original_form": "네 프랜차이즈에요",
          "speaker_id": "1",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009893.1.18",
          "form": "어떤 초밥 제일 좋아하세요?",
          "original_form": "어떤 초밥 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 11:38"
        },
        {
          "id": "MDRW2100009893.1.19",
          "form": "새우초밥 좋아해요",
          "original_form": "새우초밥 좋아해요",
          "speaker_id": "1",
          "time": "20210823 11:38"
        },
        {
          "id": "MDRW2100009893.1.20",
          "form": "새우가 가장맛있더라구요",
          "original_form": "새우가 가장맛있더라구요",
          "speaker_id": "1",
          "time": "20210823 11:38"
        },
        {
          "id": "MDRW2100009893.1.21",
          "form": "새우는 다 좋아해요",
          "original_form": "새우는 다 좋아해요",
