      "utterance": [
        {
          "id": "MDRW2100019956.1.1",
          "form": "거긴 진짜 가부장 심하겠어요",
          "original_form": "거긴 진짜 가부장 심하겠어요",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019956.1.2",
          "form": "그르게요 군대가 가부장의 원흉이 되기도 하네요",
          "original_form": "그르게요 군대가 가부장의 원흉이 되기도 하네요",
          "speaker_id": "1",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019956.1.3",
          "form": "남초 직군도 그런 식의 문화가 심한거같고",
          "original_form": "남초 직군도 그런 식의 문화가 심한거같고",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019956.1.4",
          "form": "저는 요새 남자 못잃는 여자들 보면 마음이 복잡헙니다",
          "original_form": "저는 요새 남자 못잃는 여자들 보면 마음이 복잡헙니다",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019956.1.5",
          "form": "맞아요 특히 남친한테 의지 많이 하는 사람들보면",
          "original_form": "맞아요 특히 남친한테 의지 많이 하는 사람들보면",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019956.1.6",
          "form": "더 그렇더라구요",
          "original_form": "더 그렇더라구요",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019956.1.7",
          "form": "남친 없는 시기를 못견뎌서",
          "original_form": "남친 없는 시기를 못견뎌서",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019956.1.8",
          "form": "계속 남친 사귀거나",
          "original_form": "계속 남친 사귀거나",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019956.1.9",
          "form": "나는 남자 싫긴 한데 유니콘 만나고싶다 이런..",
          "original_form": "나는 남자 싫긴 한데 유니콘 만나고싶다 이런..",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019956.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019956.1.11",
          "form": "굳이 따질것 없지만",
          "original_form": "굳이 따질것 없지만",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019956.1.12",
          "form": "전자의 경우가 더 심각하더라구요",
          "original_form": "전자의 경우가 더 심각하더라구요",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019956.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ막상막하지만 전자가 심하다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ막상막하지만 전자가 심하다",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019956.1.14",
          "form": "남친외에 딱 고립되기 쉬워서",
          "original_form": "남친외에 딱 고립되기 쉬워서",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019956.1.15",
          "form": "맞는말씀이십니다",
          "original_form": "맞는말씀이십니다",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019956.1.16",
          "form": "맞아요 남 친없으면 아무도없어서 계속사귀게되고",
          "original_form": "맞아요 남 친없으면 아무도없어서 계속사귀게되고",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019956.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019956.1.18",
          "form": "외로운거 못견뎌서 그렇기도 하더라구요",
          "original_form": "외로운거 못견뎌서 그렇기도 하더라구요",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019956.1.19",
          "form": "친구 있는데도 외롭다고 그러는데",
          "original_form": "친구 있는데도 외롭다고 그러는데",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019956.1.20",
          "form": "그럴땐 친구인 나는? 이런 생각도 들구요",
          "original_form": "그럴땐 친구인 나는? 이런 생각도 들구요",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019956.1.21",
          "form": "뭐 그렇다고 남친처럼 친구랑 지낼수도없어서",
          "original_form": "뭐 그렇다고 남친처럼 친구랑 지낼수도없어서",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019956.1.22",
          "form": "그런가보다 하고 지내고있어요",
          "original_form": "그런가보다 하고 지내고있어요",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019956.1.23",
          "form": "맞아요 ㅋㅋㅋㅋ 24시간 연락해야할 사람이 있어야 안외로운거면",
          "original_form": "맞아요 ㅋㅋㅋㅋ 24시간 연락해야할 사람이 있어야 안외로운거면",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019956.1.24",
          "form": "본질적으로 외로운사람인거같아요",
          "original_form": "본질적으로 외로운사람인거같아요",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019956.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019956.1.26",
          "form": "그런 사람이 연애를 쉽게 접하긴하죠",
          "original_form": "그런 사람이 연애를 쉽게 접하긴하죠",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019956.1.27",
          "form": "차라리 뭐 행복해보이기라도 하면 다행인데",
          "original_form": "차라리 뭐 행복해보이기라도 하면 다행인데",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019956.1.28",
          "form": "싸우면서도 못헤어지는 게 제일 힘든것같아요",
          "original_form": "싸우면서도 못헤어지는 게 제일 힘든것같아요",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019956.1.29",
          "form": "그런식으로 발목잡히는 것 같아서요",
          "original_form": "그런식으로 발목잡히는 것 같아서요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019956.1.30",
          "form": "비극적이네요",
          "original_form": "비극적이네요",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019956.1.31",
          "form": "약간 사회적 지지가 모자란 사람들이 연애에 빠지는 것 같아요",
          "original_form": "약간 사회적 지지가 모자란 사람들이 연애에 빠지는 것 같아요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019956.1.32",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019956.1.33",
          "form": "좀 부족한걸 채워넣으려고 하다보니",
          "original_form": "좀 부족한걸 채워넣으려고 하다보니",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019956.1.34",
          "form": "그걸 연애로 하나봐요",
          "original_form": "그걸 연애로 하나봐요",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019956.1.35",
          "form": "사실 여혐을 떠나서 전 연애가 그렇게 필요하지않는 사람이거든요",
          "original_form": "사실 여혐을 떠나서 전 연애가 그렇게 필요하지않는 사람이거든요",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019956.1.36",
          "form": "그래서 좀 궁금하긴해요 왜 하는지",
          "original_form": "그래서 좀 궁금하긴해요 왜 하는지",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019956.1.37",
          "form": "오 저두요 저두요",
          "original_form": "오 저두요 저두요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019956.1.38",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019956.1.39",
          "form": "시간 감정 돈 다 저한테 쓰고싶어요 ㅋㅋㅋㅋ",
          "original_form": "시간 감정 돈 다 저한테 쓰고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019956.1.40",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019956.1.41",
          "form": "자신이 제일이라ㅋㅋㅋ",
          "original_form": "자신이 제일이라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019956.1.42",
          "form": "나를 향한 확고한 지지? 이런걸 바라는것같아요",
          "original_form": "나를 향한 확고한 지지? 이런걸 바라는것같아요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019956.1.43",
          "form": "아 뭔지 알겠네요",
          "original_form": "아 뭔지 알겠네요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019956.1.44",
          "form": "인간은 다 저런면을 원하긴하죠",
          "original_form": "인간은 다 저런면을 원하긴하죠",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019956.1.45",
          "form": "그중 더 원해서 적극적으로 움직이는건가봐요",
          "original_form": "그중 더 원해서 적극적으로 움직이는건가봐요",
