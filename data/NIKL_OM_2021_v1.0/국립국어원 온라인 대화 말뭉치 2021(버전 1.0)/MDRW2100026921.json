      "utterance": [
        {
          "id": "MDRW2100026921.1.1",
          "form": "날씨가 많이 선선해졌네요",
          "original_form": "날씨가 많이 선선해졌네요",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100026921.1.2",
          "form": "그러게요 무더위가 가셔서 날씨가 좋아요",
          "original_form": "그러게요 무더위가 가셔서 날씨가 좋아요",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026921.1.3",
          "form": "이런 날씨에는 딱 여행이 좋은데ㅠㅠ",
          "original_form": "이런 날씨에는 딱 여행이 좋은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026921.1.4",
          "form": "요즘따라 여행이 너무 가고 싶어져요",
          "original_form": "요즘따라 여행이 너무 가고 싶어져요",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026921.1.5",
          "form": "여행 좋죠~! 만약 간다면 어디 가고 싶으세요?",
          "original_form": "여행 좋죠~! 만약 간다면 어디 가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026921.1.6",
          "form": "저는 유럽이요...",
          "original_form": "저는 유럽이요...",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026921.1.7",
          "form": "유럽 14박 15일로 다녀오고 싶네요ㅜㅜㅜ",
          "original_form": "유럽 14박 15일로 다녀오고 싶네요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026921.1.8",
          "form": "유럽도 좋죠ㅠㅠ",
          "original_form": "유럽도 좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026921.1.9",
          "form": "그런데 전 전에 유럽 갔다가 호되게 당해서요ㅠㅠ",
          "original_form": "그런데 전 전에 유럽 갔다가 호되게 당해서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026921.1.10",
          "form": "아 진짜요...? 아이고 여행갔다가 봉변이셨겠다...",
          "original_form": "아 진짜요...? 아이고 여행갔다가 봉변이셨겠다...",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026921.1.11",
          "form": "네ㅠㅠ 이상한 사람도 많구,,,,",
          "original_form": "네ㅠㅠ 이상한 사람도 많구,,,,",
          "speaker_id": "2",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100026921.1.12",
          "form": "그치만 명소들은 예뻤어요!",
          "original_form": "그치만 명소들은 예뻤어요!",
          "speaker_id": "2",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100026921.1.13",
          "form": "그쵸 잔 유럽 명소나 관광지들이 너무 예뻐서 너무 가고 싶더라구요",
          "original_form": "그쵸 잔 유럽 명소나 관광지들이 너무 예뻐서 너무 가고 싶더라구요",
          "speaker_id": "1",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100026921.1.14",
          "form": "그치만 비추천하신다니 다시 생각해봐야겠어요ㅠㅠ",
          "original_form": "그치만 비추천하신다니 다시 생각해봐야겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100026921.1.15",
          "form": "네네 전 요즘 제주도 가고 싶더라구요",
          "original_form": "네네 전 요즘 제주도 가고 싶더라구요",
          "speaker_id": "2",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100026921.1.16",
          "form": "저 이번 여름 휴가에 다녀왔어용",
          "original_form": "저 이번 여름 휴가에 다녀왔어용",
          "speaker_id": "1",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100026921.1.17",
          "form": "바다도 맑고 정말 좋았어요",
          "original_form": "바다도 맑고 정말 좋았어요",
          "speaker_id": "1",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100026921.1.18",
          "form": "좋았겠다ㅠㅠ 기념품으로는 뭐 사오셨나요?",
          "original_form": "좋았겠다ㅠㅠ 기념품으로는 뭐 사오셨나요?",
          "speaker_id": "2",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100026921.1.19",
          "form": "오메기떡이랑 감귤파이요ㅋㅋㅋㅋ",
          "original_form": "오메기떡이랑 감귤파이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100026921.1.20",
          "form": "넘 흔한가 싶은데 무난해서 좋더라구여",
          "original_form": "넘 흔한가 싶은데 무난해서 좋더라구여",
          "speaker_id": "1",
          "time": "20210902 01:23"
        }
      ]
