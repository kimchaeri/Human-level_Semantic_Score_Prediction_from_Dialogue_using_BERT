      "utterance": [
        {
          "id": "MDRW2100006174.1.1",
          "form": "여행좋아하시나여",
          "original_form": "여행좋아하시나여",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006174.1.2",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006174.1.3",
          "form": "기억에남는곳잇으신가용",
          "original_form": "기억에남는곳잇으신가용",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006174.1.4",
          "form": "신행으로 간 몰디브요",
          "original_form": "신행으로 간 몰디브요",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100006174.1.5",
          "form": "아ㅜㅜ",
          "original_form": "아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006174.1.6",
          "form": "저는 신행 미루고잇어요 ㅜㅜ",
          "original_form": "저는 신행 미루고잇어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006174.1.7",
          "form": "멀리갈수잇을 그날에 갈꺼에여",
          "original_form": "멀리갈수잇을 그날에 갈꺼에여",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006174.1.8",
          "form": "신행으로 가고싶으셨던 곳 있나요?",
          "original_form": "신행으로 가고싶으셨던 곳 있나요?",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006174.1.9",
          "form": "축구보러",
          "original_form": "축구보러",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006174.1.10",
          "form": "유럽가기로햇어여",
          "original_form": "유럽가기로햇어여",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006174.1.11",
          "form": "전에 유럽여행갓을때 축구보러 못갓거등요ㅎㅎ",
          "original_form": "전에 유럽여행갓을때 축구보러 못갓거등요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006174.1.12",
          "form": "영국?스페인?어디요?ㅎㅎ",
          "original_form": "영국?스페인?어디요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006174.1.13",
          "form": "그 두곳 입니다",
          "original_form": "그 두곳 입니다",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006174.1.14",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006174.1.15",
          "form": "신행 회사에서 2주정도 갈수잇어서",
          "original_form": "신행 회사에서 2주정도 갈수잇어서",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006174.1.16",
          "form": "갈수잇는날까지 존버할꺼에여ㅠㅠ",
          "original_form": "갈수잇는날까지 존버할꺼에여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006174.1.17",
          "form": "꼭 가실 수 있길요 \r\n또 가고싶은 여행지 있나요",
          "original_form": "꼭 가실 수 있길요 \r\n또 가고싶은 여행지 있나요",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100006174.1.18",
          "form": "에휴 멀리라면 다 좋아요 ㅜ",
          "original_form": "에휴 멀리라면 다 좋아요 ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006174.1.19",
          "form": "이제 국내여행은 그만하고싶어요ㅜ",
          "original_form": "이제 국내여행은 그만하고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006174.1.20",
          "form": "최근에 여행 다녀오셧나욤",
          "original_form": "최근에 여행 다녀오셧나욤",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006174.1.21",
          "form": "네 부산.경주 다녀왔어요",
          "original_form": "네 부산.경주 다녀왔어요",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006174.1.22",
          "form": "좋으셧나용?",
          "original_form": "좋으셧나용?",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006174.1.23",
          "form": "네 둘은 여행지로 여러번가도 항상 좋네요",
