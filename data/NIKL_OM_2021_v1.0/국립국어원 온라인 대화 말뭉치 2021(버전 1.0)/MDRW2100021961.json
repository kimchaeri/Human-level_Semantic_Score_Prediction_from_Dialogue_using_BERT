      "utterance": [
        {
          "id": "MDRW2100021961.1.1",
          "form": "ㅎㅎ 책고르셔야하는거아니에요?ㅎ",
          "original_form": "ㅎㅎ 책고르셔야하는거아니에요?ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021961.1.2",
          "form": "ㅎㅎ 시간뺏기기 좀 그러네요ㅎㅎ 언능 쇼핑하셔요ㅎㅎ",
          "original_form": "ㅎㅎ 시간뺏기기 좀 그러네요ㅎㅎ 언능 쇼핑하셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021961.1.3",
          "form": "아 쿠폰만 받은거에ㄴ요 ㅎㅎ",
          "original_form": "아 쿠폰만 받은거에ㄴ요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021961.1.4",
          "form": "선착이라 ㅎㅎㅎ",
          "original_form": "선착이라 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021961.1.5",
          "form": "아침에 받음 없어용 ㅋ",
          "original_form": "아침에 받음 없어용 ㅋ",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021961.1.6",
          "form": "ㅎㅎ 역시 대단하셔ㅎㅎ",
          "original_form": "ㅎㅎ 역시 대단하셔ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021961.1.7",
          "form": "구매하시는것도 합리적으로 하시고ㅎ",
          "original_form": "구매하시는것도 합리적으로 하시고ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021961.1.8",
          "form": "오늘 반성 했습니당ㅠㅠ",
          "original_form": "오늘 반성 했습니당ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021961.1.9",
          "form": "ㅎㅎ 저도 충동구매 많이 해여 ㅋ",
          "original_form": "ㅎㅎ 저도 충동구매 많이 해여 ㅋ",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021961.1.10",
          "form": "에이 그런것같진 않아요ㅎㅎ",
          "original_form": "에이 그런것같진 않아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021961.1.11",
          "form": "오늘도 전 택배를 뜯네요ㅎㅎㅎㅎ",
          "original_form": "오늘도 전 택배를 뜯네요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021961.1.12",
          "form": "ㅋㅋ 택배는 사랑이쥬~",
          "original_form": "ㅋㅋ 택배는 사랑이쥬~",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021961.1.13",
          "form": "육계장 사발면에 이벤트로 냉동식품2개왔어요ㅎㅎ",
          "original_form": "육계장 사발면에 이벤트로 냉동식품2개왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021961.1.14",
          "form": "어디서 소리안들려요,?",
          "original_form": "어디서 소리안들려요,?",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021961.1.15",
          "form": "머리 벗겨지는 소리요?",
          "original_form": "머리 벗겨지는 소리요?",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021961.1.16",
          "form": "빠지는건가 ㅋ",
          "original_form": "빠지는건가 ㅋ",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021961.1.17",
          "form": "살찌는소리죠ㅎㅎㅎㅎㅎㅎㄹ",
          "original_form": "살찌는소리죠ㅎㅎㅎㅎㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021961.1.18",
          "form": "맨날 식품만 엄청 사니깐 안되겟어요ㅎㅎ",
          "original_form": "맨날 식품만 엄청 사니깐 안되겟어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021961.1.19",
          "form": "옛날로 돌아가야지ㅎㅎ",
          "original_form": "옛날로 돌아가야지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021961.1.20",
          "form": "헉 전 머리 벗겨진단 소리 많이 들어여ㅠㅋ",
          "original_form": "헉 전 머리 벗겨진단 소리 많이 들어여ㅠㅋ",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021961.1.21",
          "form": "아아님음 최근에 온 택배 뭐에요?",
          "original_form": "아아님음 최근에 온 택배 뭐에요?",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021961.1.22",
          "form": "전 식품은 좀 자제해요 ㅎㅎ",
          "original_form": "전 식품은 좀 자제해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021961.1.23",
          "form": "저도 자제해야하는데ㅎ",
          "original_form": "저도 자제해야하는데ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021961.1.24",
          "form": "선착순잘됬어요?",
          "original_form": "선착순잘됬어요?",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021961.1.25",
          "form": "최근에  온거 없네요 ㅋ",
          "original_form": "최근에  온거 없네요 ㅋ",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021961.1.26",
          "form": "나 남줘서요 ㅋㅋㅋ",
          "original_form": "나 남줘서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021961.1.27",
          "form": "아뇨 타미 안됐어요",
          "original_form": "아뇨 타미 안됐어요",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021961.1.28",
          "form": "까먹엇어요",
          "original_form": "까먹엇어요",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021961.1.29",
          "form": "ㅠㅠ 어뜩해요ㅎㅎ",
          "original_form": "ㅠㅠ 어뜩해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021961.1.30",
          "form": "괜찮아요 ㅎㅎ",
          "original_form": "괜찮아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:25"
        }
      ]
