      "utterance": [
        {
          "id": "MDRW2100038118.1.1",
          "form": "나 사은품으로 플립스트랩을 받았어",
          "original_form": "나 사은품으로 플립스트랩을 받았어",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038118.1.2",
          "form": "플립 알지?",
          "original_form": "플립 알지?",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038118.1.3",
          "form": "요즘 광고하는 휴대폰이야. 졉을 수 있는!",
          "original_form": "요즘 광고하는 휴대폰이야. 졉을 수 있는!",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038118.1.4",
          "form": "응응 요즘 기술이 너무 좋라",
          "original_form": "응응 요즘 기술이 너무 좋라",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038118.1.5",
          "form": "그런데 나는 심슨 끈을 받았거든",
          "original_form": "그런데 나는 심슨 끈을 받았거든",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.6",
          "form": "인기가 많은 끈은 노티드 거야",
          "original_form": "인기가 많은 끈은 노티드 거야",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.7",
          "form": "응 귀엽겠다!",
          "original_form": "응 귀엽겠다!",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.8",
          "form": "무슨색이야??",
          "original_form": "무슨색이야??",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.9",
          "form": "근데 아니 글쎄 너 당근하니?",
          "original_form": "근데 아니 글쎄 너 당근하니?",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.10",
          "form": "심슨은 파란색이야",
          "original_form": "심슨은 파란색이야",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.11",
          "form": "응 가끔 찾아보기도해",
          "original_form": "응 가끔 찾아보기도해",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.12",
          "form": "당근에서 교환을 구하는거야?",
          "original_form": "당근에서 교환을 구하는거야?",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.13",
          "form": "당근마켓에 보니 누가 노티드스투랩을 무나했더라",
          "original_form": "당근마켓에 보니 누가 노티드스투랩을 무나했더라",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.14",
          "form": "우와 그걸 무료로???",
          "original_form": "우와 그걸 무료로???",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.15",
          "form": "쇼핑앱을 잘 들여다봐야 복이 오는거같야",
          "original_form": "쇼핑앱을 잘 들여다봐야 복이 오는거같야",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.16",
          "form": "교환하는게아니라??",
          "original_form": "교환하는게아니라??",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.17",
          "form": "응 근데 펀매하는 사람들은 이만오천원에 팔거든",
          "original_form": "응 근데 펀매하는 사람들은 이만오천원에 팔거든",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.18",
          "form": "완전 대인배구나",
          "original_form": "완전 대인배구나",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.19",
          "form": "그러니까... 대단해",
          "original_form": "그러니까... 대단해",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.20",
          "form": "나같으면 이만원에 팔꺼야",
          "original_form": "나같으면 이만원에 팔꺼야",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.21",
          "form": "나도야.",
          "original_form": "나도야.",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038118.1.22",
          "form": "쏠쏠하잖아 이만원.",
          "original_form": "쏠쏠하잖아 이만원.",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038118.1.23",
          "form": "역시 부지런해야 좋은 뮬건을 찾을 수 있어",
          "original_form": "역시 부지런해야 좋은 뮬건을 찾을 수 있어",
          "speaker_id": "1",
          "time": "20210901 23:26"
        }
      ]
    }
