      "utterance": [
        {
          "id": "MDRW2100002136.1.1",
          "form": "회사에 다니시나요?",
          "original_form": "회사에 다니시나요?",
          "speaker_id": "1",
          "time": "20210819 23:52"
        },
        {
          "id": "MDRW2100002136.1.2",
          "form": "전 회사다니다가 지금은 육아휴직중이에요",
          "original_form": "전 회사다니다가 지금은 육아휴직중이에요",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002136.1.3",
          "form": "회사다닐 땐 주로 어떤 업무를 하셨나요?\r\n저는 아직 직장을 구하지 못해서 알바중이거든요",
          "original_form": "회사다닐 땐 주로 어떤 업무를 하셨나요?\r\n저는 아직 직장을 구하지 못해서 알바중이거든요",
          "speaker_id": "1",
          "time": "20210819 23:54"
        },
        {
          "id": "MDRW2100002136.1.4",
          "form": "전 경리직이엿어요",
          "original_form": "전 경리직이엿어요",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100002136.1.5",
          "form": "그러셨군요!  저는 영화관 알바하고있어요",
          "original_form": "그러셨군요!  저는 영화관 알바하고있어요",
          "speaker_id": "1",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002136.1.6",
          "form": "오 영화관 알바시면 영화많이 보실수잇나요?",
          "original_form": "오 영화관 알바시면 영화많이 보실수잇나요?",
          "speaker_id": "2",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100002136.1.7",
          "form": "직원 복지라면 복지인게 영화 공짜로 보는게 좋아요ㅎㅎ",
          "original_form": "직원 복지라면 복지인게 영화 공짜로 보는게 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002136.1.8",
          "form": "그건 좋네요ㅎ영화보는거 좋아하는사람한테는",
          "original_form": "그건 좋네요ㅎ영화보는거 좋아하는사람한테는",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002136.1.9",
          "form": "그건 그래요ㅎㅎ",
          "original_form": "그건 그래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002136.1.10",
          "form": "직장은 어느업무 원하세요?",
          "original_form": "직장은 어느업무 원하세요?",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002136.1.11",
          "form": "사무직이요.. 서비스나 영업직은 안맞는거같아오",
          "original_form": "사무직이요.. 서비스나 영업직은 안맞는거같아오",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002136.1.12",
          "form": "저도 그렇더라구요",
          "original_form": "저도 그렇더라구요",
          "speaker_id": "2",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002136.1.13",
          "form": "뭔가 고객비위맞추는게 감정소모가 심하더라구요",
          "original_form": "뭔가 고객비위맞추는게 감정소모가 심하더라구요",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002136.1.14",
          "form": "못하는 사람은 진짜 절대 못할 거 같은..",
          "original_form": "못하는 사람은 진짜 절대 못할 거 같은..",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002136.1.15",
          "form": "근데 사무직도 비위맞추는건..",
          "original_form": "근데 사무직도 비위맞추는건..",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002136.1.16",
          "form": "비슷해요ㅋㅋ직장사람들 비위",
          "original_form": "비슷해요ㅋㅋ직장사람들 비위",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002136.1.17",
          "form": "그쳐.. 어딜가나 도라이질량법칙이 존재하니까..",
          "original_form": "그쳐.. 어딜가나 도라이질량법칙이 존재하니까..",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002136.1.18",
          "form": "그쳐 하나씩은 꼭잇어요",
          "original_form": "그쳐 하나씩은 꼭잇어요",
          "speaker_id": "2",
          "time": "20210820 00:18"
        }
      ]
