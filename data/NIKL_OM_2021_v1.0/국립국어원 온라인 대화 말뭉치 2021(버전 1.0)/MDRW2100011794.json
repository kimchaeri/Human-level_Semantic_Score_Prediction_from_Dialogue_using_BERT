      "utterance": [
        {
          "id": "MDRW2100011794.1.1",
          "form": "행복한 몇안되는 가정을 미디어에 전시하고",
          "original_form": "행복한 몇안되는 가정을 미디어에 전시하고",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011794.1.2",
          "form": "예전엔 참 남자 살기 편했겠어요",
          "original_form": "예전엔 참 남자 살기 편했겠어요",
          "speaker_id": "2",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011794.1.3",
          "form": "희대의 사기극..",
          "original_form": "희대의 사기극..",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011794.1.4",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011794.1.5",
          "form": "얼마나 좋았을까요..",
          "original_form": "얼마나 좋았을까요..",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011794.1.6",
          "form": "왜 1020남들이 그러는지 알것같기도 해요",
          "original_form": "왜 1020남들이 그러는지 알것같기도 해요",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011794.1.7",
          "form": "자기 아빠뻘만해도 여자들이 알아서 잘 섬기고 그랬는데",
          "original_form": "자기 아빠뻘만해도 여자들이 알아서 잘 섬기고 그랬는데",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011794.1.8",
          "form": "ㅋㅋㅋㅋㅋ 진짜 생각도못해봣는데 그렇네요",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜 생각도못해봣는데 그렇네요",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011794.1.9",
          "form": "불안하겠어요",
          "original_form": "불안하겠어요",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011794.1.10",
          "form": "자기들도 그렇게 살아야하는뎈ㅋㅋㅋ",
          "original_form": "자기들도 그렇게 살아야하는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011794.1.11",
          "form": "여자한테 이제 경쟁에서 밀리고 ㅋㅋㅋ",
          "original_form": "여자한테 이제 경쟁에서 밀리고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011794.1.12",
          "form": "집에 노예 못들이고",
          "original_form": "집에 노예 못들이고",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011794.1.13",
          "form": "더 퇴화되고",
          "original_form": "더 퇴화되고",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011794.1.14",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011794.1.15",
          "form": "아 자기들이 누릴수없게되니까",
          "original_form": "아 자기들이 누릴수없게되니까",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011794.1.16",
          "form": "가성비라도 더 붙잡고 늘어지는 걸까요",
          "original_form": "가성비라도 더 붙잡고 늘어지는 걸까요",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011794.1.17",
          "form": "최저가부장제",
          "original_form": "최저가부장제",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011794.1.18",
          "form": "그런것같기도 하네요",
          "original_form": "그런것같기도 하네요",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011794.1.19",
          "form": "더 찍어 누르려는 느낌?",
          "original_form": "더 찍어 누르려는 느낌?",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011794.1.20",
          "form": "보통은 자기관리를 더 하려고 하지않나요",
          "original_form": "보통은 자기관리를 더 하려고 하지않나요",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011794.1.21",
          "form": "그러게욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러게욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011794.1.22",
          "form": "경쟁률을 높이려고 할텐데",
          "original_form": "경쟁률을 높이려고 할텐데",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011794.1.23",
          "form": "이슬람과 비슷한 원리같아요",
          "original_form": "이슬람과 비슷한 원리같아요",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011794.1.24",
          "form": "자기 경쟁률 높이지않고 여자 더 후려치기",
          "original_form": "자기 경쟁률 높이지않고 여자 더 후려치기",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011794.1.25",
          "form": "이슬람 그렇죠",
          "original_form": "이슬람 그렇죠",
