      "utterance": [
        {
          "id": "MDRW2100024766.1.1",
          "form": "우와 무슨휴가인가요??",
          "original_form": "우와 무슨휴가인가요??",
          "speaker_id": "2",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024766.1.2",
          "form": "휴가주는 좋은직장다니시나봐요",
          "original_form": "휴가주는 좋은직장다니시나봐요",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024766.1.3",
          "form": "제 직장ㅋㅋㅋ",
          "original_form": "제 직장ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024766.1.4",
          "form": "이백 버는 작은 회사예요^^",
          "original_form": "이백 버는 작은 회사예요^^",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024766.1.5",
          "form": "그래도 작지만 소중한 그대의월급^^",
          "original_form": "그래도 작지만 소중한 그대의월급^^",
          "speaker_id": "2",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024766.1.6",
          "form": "월급으로는  어떻게 관리하세요?",
          "original_form": "월급으로는  어떻게 관리하세요?",
          "speaker_id": "2",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100024766.1.7",
          "form": "저는 그냥 대출 갚아요",
          "original_form": "저는 그냥 대출 갚아요",
          "speaker_id": "1",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100024766.1.8",
          "form": "그전에는 저축했어요",
          "original_form": "그전에는 저축했어요",
          "speaker_id": "1",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100024766.1.9",
          "form": "아ㅋ주식도 해요",
          "original_form": "아ㅋ주식도 해요",
          "speaker_id": "1",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100024766.1.10",
          "form": "저도 주식하는데ㅋㅋㅋ",
          "original_form": "저도 주식하는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100024766.1.11",
          "form": "저축만큼의이율만큼도못벌어서",
          "original_form": "저축만큼의이율만큼도못벌어서",
          "speaker_id": "2",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100024766.1.12",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100024766.1.13",
          "form": "돈까먹는중ㅋ",
          "original_form": "돈까먹는중ㅋ",
          "speaker_id": "1",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100024766.1.14",
          "form": "이제뭐하는건가싶어요ㅋㅋ ㅋㅋ",
          "original_form": "이제뭐하는건가싶어요ㅋㅋ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100024766.1.15",
          "form": "ㅋㅋ돈삭제기능이있더라구요",
          "original_form": "ㅋㅋ돈삭제기능이있더라구요",
          "speaker_id": "1",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100024766.1.16",
          "form": "네 주식으로1억만들기 정말쉬워요",
          "original_form": "네 주식으로1억만들기 정말쉬워요",
          "speaker_id": "2",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100024766.1.17",
          "form": "ㅋㅋㅋ저도알죠",
          "original_form": "ㅋㅋㅋ저도알죠",
          "speaker_id": "1",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100024766.1.18",
          "form": "2억으로시작하면됨^^",
          "original_form": "2억으로시작하면됨^^",
          "speaker_id": "1",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100024766.1.19",
          "form": "2억을넣음되거든요^^",
          "original_form": "2억을넣음되거든요^^",
          "speaker_id": "2",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100024766.1.20",
          "form": "은 내이야기ㅋㅋ",
