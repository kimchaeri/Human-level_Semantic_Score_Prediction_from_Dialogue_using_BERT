      "utterance": [
        {
          "id": "MDRW2100023914.1.1",
          "form": "오늘은 홈트 안해요?",
          "original_form": "오늘은 홈트 안해요?",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023914.1.2",
          "form": "하.. 홈트...",
          "original_form": "하.. 홈트...",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023914.1.3",
          "form": "말도 마세요 저 죽는 줄",
          "original_form": "말도 마세요 저 죽는 줄",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023914.1.4",
          "form": "오 벌써 하셨구나",
          "original_form": "오 벌써 하셨구나",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023914.1.5",
          "form": "저 홈트 말고 교트 했어요 교트 ㅋㅋㅋㅋ",
          "original_form": "저 홈트 말고 교트 했어요 교트 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023914.1.6",
          "form": "와 교트",
          "original_form": "와 교트",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023914.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023914.1.8",
          "form": "온라인으로 했어요...",
          "original_form": "온라인으로 했어요...",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023914.1.9",
          "form": "애들 체육수업이요?",
          "original_form": "애들 체육수업이요?",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023914.1.10",
          "form": "네네 ㅋㅋㅋ체육 지도하면서 같이 했네요",
          "original_form": "네네 ㅋㅋㅋ체육 지도하면서 같이 했네요",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023914.1.11",
          "form": "와아ㅋㅋㅋㅋ",
          "original_form": "와아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.12",
          "form": "런지 스쿼트 플랭크 버피 등등 다 했어요",
          "original_form": "런지 스쿼트 플랭크 버피 등등 다 했어요",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.13",
          "form": "같이 하려면 힘들겠네요",
          "original_form": "같이 하려면 힘들겠네요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.14",
          "form": "애들 벌써 징징 거려서",
          "original_form": "애들 벌써 징징 거려서",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.15",
          "form": "담임반만 하는건가요? 아님 과목 전담?",
          "original_form": "담임반만 하는건가요? 아님 과목 전담?",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.16",
          "form": "아나\n나도 같이한다고",
          "original_form": "아나\n나도 같이한다고",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.17",
          "form": "말해서 겨우 했네요",
          "original_form": "말해서 겨우 했네요",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.19",
          "form": "저 담임 맡았어요",
          "original_form": "저 담임 맡았어요",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023914.1.20",
          "form": "저희학교 체육쌤은 그냥 애들보고 시키던뎈ㅋㅋㅋㅋ",
          "original_form": "저희학교 체육쌤은 그냥 애들보고 시키던뎈ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023914.1.21",
          "form": "플랭크 하니까 애들 카메라 밖에서 눕고;;",
          "original_form": "플랭크 하니까 애들 카메라 밖에서 눕고;;",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023914.1.22",
          "form": "하루에 여러번 수업 다 하기 힘들어서..",
          "original_form": "하루에 여러번 수업 다 하기 힘들어서..",
          "speaker_id": "2",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023914.1.23",
          "form": "앜ㅋㅋㅋㅋ플랭크가 휴식이었겠네요",
          "original_form": "앜ㅋㅋㅋㅋ플랭크가 휴식이었겠네요",
          "speaker_id": "2",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023914.1.24",
          "form": "저도 그냥 운동할 겸 같이 했어요",
          "original_form": "저도 그냥 운동할 겸 같이 했어요",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023914.1.25",
          "form": "노래 틀어놓고 각자 페이스에 맞게 같이 하자! 하고 했네요",
          "original_form": "노래 틀어놓고 각자 페이스에 맞게 같이 하자! 하고 했네요",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023914.1.26",
          "form": "신청곡 받으면서..",
          "original_form": "신청곡 받으면서..",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023914.1.27",
          "form": "ㅋㅋㅋㅋㅋ귀엽다",
          "original_form": "ㅋㅋㅋㅋㅋ귀엽다",
          "speaker_id": "2",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023914.1.28",
          "form": "전 이제 운동하러 갈거에요!!!",
          "original_form": "전 이제 운동하러 갈거에요!!!",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.29",
          "form": "어떤 친구 안하고 멍하니 있길래",
          "original_form": "어떤 친구 안하고 멍하니 있길래",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.30",
          "form": "단독으로 하고 싶어서 안달났나냐까 바로 벌떡 일어나네요",
          "original_form": "단독으로 하고 싶어서 안달났나냐까 바로 벌떡 일어나네요",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.32",
          "form": "카리스마있으신 쌤이시당",
          "original_form": "카리스마있으신 쌤이시당",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.33",
          "form": "저 되게 좋게 얘기했어요 ㅠㅠ",
          "original_form": "저 되게 좋게 얘기했어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.34",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.35",
          "form": "네네 다정할거 같아요ㅋㅋㅋㅋ",
          "original_form": "네네 다정할거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.36",
          "form": "아이들하고 같이 운동하니까 저 되게 좋았어요",
          "original_form": "아이들하고 같이 운동하니까 저 되게 좋았어요",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023914.1.37",
          "form": "집에 갈땐 다리가 후들후들거렸지만 ㅜㅜ",
          "original_form": "집에 갈땐 다리가 후들후들거렸지만 ㅜㅜ",
          "speaker_id": "1",
