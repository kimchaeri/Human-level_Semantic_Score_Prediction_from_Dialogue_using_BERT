      "utterance": [
        {
          "id": "MDRW2100015634.1.1",
          "form": "좋아하시는 책 장르 있나요?",
          "original_form": "좋아하시는 책 장르 있나요?",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015634.1.2",
          "form": "전 책과 친하질 않아서^^",
          "original_form": "전 책과 친하질 않아서^^",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015634.1.3",
          "form": "책좋아하세요?",
          "original_form": "책좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015634.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015634.1.5",
          "form": "저는 가끔 읽어요",
          "original_form": "저는 가끔 읽어요",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015634.1.6",
          "form": "뭔가 바쁠때 읽어야 제일 재밌어요",
          "original_form": "뭔가 바쁠때 읽어야 제일 재밌어요",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015634.1.7",
          "form": "어떤 장르좋아하시나요?",
          "original_form": "어떤 장르좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015634.1.8",
          "form": "폰이 발열이 심해서 답이 느려도 이해해주세요",
          "original_form": "폰이 발열이 심해서 답이 느려도 이해해주세요",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100015634.1.9",
          "form": "헉 아니에요 속도가 중요하나요ㅎㅎ",
          "original_form": "헉 아니에요 속도가 중요하나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015634.1.10",
          "form": "전 추리나 스릴러 좋아해요!",
          "original_form": "전 추리나 스릴러 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015634.1.11",
          "form": "흥미진진해서요ㅎㅎ",
          "original_form": "흥미진진해서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015634.1.12",
          "form": "저도 그런장르는 좋아해요",
          "original_form": "저도 그런장르는 좋아해요",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015634.1.13",
          "form": "범죄스릴러",
          "original_form": "범죄스릴러",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015634.1.14",
          "form": "오옹 좋아하는 작가 있으세요?",
          "original_form": "오옹 좋아하는 작가 있으세요?",
          "speaker_id": "1",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015634.1.15",
          "form": "아는작가는 없어요ㅋ 말해주심 알수도ㅋ",
          "original_form": "아는작가는 없어요ㅋ 말해주심 알수도ㅋ",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015634.1.16",
          "form": "저재부팅하고올게요 폰이 맛이갔네요ㅠ",
          "original_form": "저재부팅하고올게요 폰이 맛이갔네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015634.1.17",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015634.1.18",
          "form": "전 히가시노 게이고 좋아해요",
          "original_form": "전 히가시노 게이고 좋아해요",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015634.1.19",
          "form": "아 그작가 ㅋ 이름이생각이 안나서",
          "original_form": "아 그작가 ㅋ 이름이생각이 안나서",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015634.1.20",
          "form": "베스트셀러 많던데요",
          "original_form": "베스트셀러 많던데요",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015634.1.21",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015634.1.22",
          "form": "제가 추리 소설이나 스릴러 장르 좋아해서",
          "original_form": "제가 추리 소설이나 스릴러 장르 좋아해서",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015634.1.23",
          "form": "그 작가를 좋아하게 됐어요",
          "original_form": "그 작가를 좋아하게 됐어요",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015634.1.24",
          "form": "무슨 나무 제목있었는디",
          "original_form": "무슨 나무 제목있었는디",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015634.1.25",
          "form": "엇 뭐지뭐지",
          "original_form": "엇 뭐지뭐지",
          "speaker_id": "1",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015634.1.26",
          "form": "기억력 참~~~ㅋㅋ",
          "original_form": "기억력 참~~~ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015634.1.27",
          "form": "저도 기억력이 안좋아서 기억이 안나네요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "저도 기억력이 안좋아서 기억이 안나네요ㅠㅋㅋㅋㅋㅋ",
