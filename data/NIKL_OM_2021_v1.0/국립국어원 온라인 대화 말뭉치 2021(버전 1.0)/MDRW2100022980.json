      "utterance": [
        {
          "id": "MDRW2100022980.1.1",
          "form": "점심 집에서 드셨나요",
          "original_form": "점심 집에서 드셨나요",
          "speaker_id": "1",
          "time": "20210829 19:59"
        },
        {
          "id": "MDRW2100022980.1.2",
          "form": "네 집에서 해먹었어요",
          "original_form": "네 집에서 해먹었어요",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022980.1.3",
          "form": "밥이 들어와 나요",
          "original_form": "밥이 들어와 나요",
          "speaker_id": "1",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022980.1.4",
          "form": "들어가나요?",
          "original_form": "들어가나요?",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022980.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022980.1.6",
          "form": "아니요 쌀이 들어가지 않아요",
          "original_form": "아니요 쌀이 들어가지 않아요",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022980.1.7",
          "form": "그럼 빵이 들어가나요?",
          "original_form": "그럼 빵이 들어가나요?",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022980.1.8",
          "form": "아뇨! 빵도 안 들어간답니다",
          "original_form": "아뇨! 빵도 안 들어간답니다",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022980.1.9",
          "form": "그럼 야채가 들어가나요?",
          "original_form": "그럼 야채가 들어가나요?",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022980.1.10",
          "form": "음 대답하기 어려운 질문이예요",
          "original_form": "음 대답하기 어려운 질문이예요",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022980.1.11",
          "form": "네 야채는 확실해요",
          "original_form": "네 야채는 확실해요",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022980.1.12",
          "form": "정답 샐러드!",
          "original_form": "정답 샐러드!",
          "speaker_id": "1",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022980.1.13",
          "form": "핫하!",
          "original_form": "핫하!",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022980.1.14",
          "form": "땡!",
          "original_form": "땡!",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022980.1.15",
          "form": "덤벼라!",
          "original_form": "덤벼라!",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022980.1.16",
          "form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022980.1.17",
          "form": "아니 음식에 빵도 안 들어가고 밥도 안 들어갈 수가 있어요? ㅋㅋㅋㅋ",
          "original_form": "아니 음식에 빵도 안 들어가고 밥도 안 들어갈 수가 있어요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022980.1.18",
          "form": "힌트 드릴까요 후후후",
          "original_form": "힌트 드릴까요 후후후",
          "speaker_id": "2",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022980.1.19",
          "form": "식물입니다",
          "original_form": "식물입니다",
          "speaker_id": "2",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022980.1.20",
          "form": "과일만 깎아서 드셨나요?",
          "original_form": "과일만 깎아서 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022980.1.21",
          "form": "단품으로 먹었지만 과일이 아닙니다",
          "original_form": "단품으로 먹었지만 과일이 아닙니다",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022980.1.22",
          "form": "토마토 드셨나요?",
          "original_form": "토마토 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022980.1.23",
