      "utterance": [
        {
          "id": "MDRW2100036099.1.1",
          "form": "백신은 맞으셨나요?",
          "original_form": "백신은 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100036099.1.2",
          "form": "이번주에 맞아요",
          "original_form": "이번주에 맞아요",
          "speaker_id": "2",
          "time": "20210830 16:21"
        },
        {
          "id": "MDRW2100036099.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210830 16:21"
        },
        {
          "id": "MDRW2100036099.1.4",
          "form": "저는 추석 전주요",
          "original_form": "저는 추석 전주요",
          "speaker_id": "1",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036099.1.5",
          "form": "이번에 1차 맞나요?",
          "original_form": "이번에 1차 맞나요?",
          "speaker_id": "1",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036099.1.6",
          "form": "네네 뭐 맞으세요?",
          "original_form": "네네 뭐 맞으세요?",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100036099.1.7",
          "form": "저는 미정이래요.. 그떄가서 알려준대요",
          "original_form": "저는 미정이래요.. 그떄가서 알려준대요",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100036099.1.8",
          "form": "아 저도 예약하고 좀 지나서 종류 확정됐어요",
          "original_form": "아 저도 예약하고 좀 지나서 종류 확정됐어요",
          "speaker_id": "2",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100036099.1.9",
          "form": "사실 모더나이길 바랐는데ㅎ",
          "original_form": "사실 모더나이길 바랐는데ㅎ",
          "speaker_id": "2",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100036099.1.10",
          "form": "저 화이자 아니면 모더나라는데 저도 화이자겠죠?",
          "original_form": "저 화이자 아니면 모더나라는데 저도 화이자겠죠?",
          "speaker_id": "1",
          "time": "20210830 16:47"
        },
        {
          "id": "MDRW2100036099.1.11",
          "form": "어..전 화이자였어요ㅜ",
          "original_form": "어..전 화이자였어요ㅜ",
          "speaker_id": "2",
          "time": "20210830 16:48"
        },
        {
          "id": "MDRW2100036099.1.12",
          "form": "아무래도  지금은 화이자가 많은듯",
          "original_form": "아무래도  지금은 화이자가 많은듯",
          "speaker_id": "2",
          "time": "20210830 16:48"
        },
        {
          "id": "MDRW2100036099.1.13",
          "form": "모더나는 물량이 딸리니까 아무래도 화이지일 거 같은데",
          "original_form": "모더나는 물량이 딸리니까 아무래도 화이지일 거 같은데",
          "speaker_id": "1",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100036099.1.14",
          "form": "화이자가 대기업이라 또 안정성은 가장 좋지 않나요?",
          "original_form": "화이자가 대기업이라 또 안정성은 가장 좋지 않나요?",
          "speaker_id": "1",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100036099.1.15",
          "form": "아 델타변이 방어율이 모더나가 더 좋다 그래서요",
          "original_form": "아 델타변이 방어율이 모더나가 더 좋다 그래서요",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036099.1.16",
          "form": "사실 화이자도 좋아요ㅎ",
          "original_form": "사실 화이자도 좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100036099.1.17",
          "form": "그렇군요 근데 선택권이 없다보니 일단 뭐라도 맞고 부작용만 없어도 감사할거 같아요",
          "original_form": "그렇군요 근데 선택권이 없다보니 일단 뭐라도 맞고 부작용만 없어도 감사할거 같아요",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100036099.1.18",
          "form": "ㅎㅎ 그쵸 부작용 제발 없엏으면",
          "original_form": "ㅎㅎ 그쵸 부작용 제발 없엏으면",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036099.1.19",
          "form": "타이레놀 구비해셨나요",
          "original_form": "타이레놀 구비해셨나요",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036099.1.20",
          "form": "네 미리 사놨어요",
          "original_form": "네 미리 사놨어요",
          "speaker_id": "1",
          "time": "20210830 22:03"
        }
      ]
