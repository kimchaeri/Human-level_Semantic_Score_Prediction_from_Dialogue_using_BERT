      "utterance": [
        {
          "id": "MDRW2100003712.1.1",
          "form": "반쪽이 2009년노래네요",
          "original_form": "반쪽이 2009년노래네요",
          "speaker_id": "1",
          "time": "20210821 11:04"
        },
        {
          "id": "MDRW2100003712.1.2",
          "form": "네 오래됐어요",
          "original_form": "네 오래됐어요",
          "speaker_id": "2",
          "time": "20210821 11:04"
        },
        {
          "id": "MDRW2100003712.1.3",
          "form": "요새 다시 보고싶은 가수가 많아요",
          "original_form": "요새 다시 보고싶은 가수가 많아요",
          "speaker_id": "2",
          "time": "20210821 11:05"
        },
        {
          "id": "MDRW2100003712.1.4",
          "form": "전 빅뱅 2ne1 이 다시 보고 싶어요",
          "original_form": "전 빅뱅 2ne1 이 다시 보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 11:05"
        },
        {
          "id": "MDRW2100003712.1.5",
          "form": "이승철 연인도좋아요",
          "original_form": "이승철 연인도좋아요",
          "speaker_id": "2",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.6",
          "form": "제작년부터 핑클..옛날가수들 소환하는거 보고 과거 회상하고 좋드라구요",
          "original_form": "제작년부터 핑클..옛날가수들 소환하는거 보고 과거 회상하고 좋드라구요",
          "speaker_id": "1",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.7",
          "form": "불새 ㅜㅜ이은주 생각나요",
          "original_form": "불새 ㅜㅜ이은주 생각나요",
          "speaker_id": "2",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.9",
          "form": "저도 저도요!",
          "original_form": "저도 저도요!",
          "speaker_id": "1",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.10",
          "form": "저도 핑클봤어요",
          "original_form": "저도 핑클봤어요",
          "speaker_id": "2",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.11",
          "form": "이은주생각나요 그노래들으면ㅠ",
          "original_form": "이은주생각나요 그노래들으면ㅠ",
          "speaker_id": "1",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.12",
          "form": "저 불새 몇번봤네요",
          "original_form": "저 불새 몇번봤네요",
          "speaker_id": "2",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.13",
          "form": "그때 넘 눈빛이 아련했어요",
          "original_form": "그때 넘 눈빛이 아련했어요",
          "speaker_id": "2",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003712.1.14",
          "form": "정말 좋아한 배우인데",
          "original_form": "정말 좋아한 배우인데",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003712.1.15",
          "form": "가끔 생각나요",
          "original_form": "가끔 생각나요",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003712.1.16",
          "form": "네ㅠㅡ 그런 좋은 영화배우를 잃은게 너무 슬프네요",
          "original_form": "네ㅠㅡ 그런 좋은 영화배우를 잃은게 너무 슬프네요",
          "speaker_id": "1",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003712.1.17",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003712.1.18",
          "form": "아 슬프네요",
          "original_form": "아 슬프네요",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003712.1.19",
          "form": "저 고현정 히트도 몇번봤어요",
          "original_form": "저 고현정 히트도 몇번봤어요",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003712.1.20",
          "form": "고련정이 거기서 연기가 넘좋았어요 옛사고에 갇혀 사는모습 넘 아련하고",
          "original_form": "고련정이 거기서 연기가 넘좋았어요 옛사고에 갇혀 사는모습 넘 아련하고",
          "speaker_id": "2",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003712.1.21",
          "form": "고현정",
          "original_form": "고현정",
          "speaker_id": "2",
          "time": "20210821 11:09"
        },
        {
          "id": "MDRW2100003712.1.22",
          "form": "저도 히트 재밌게 봤어요 ㅎㅎ",
          "original_form": "저도 히트 재밌게 봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003712.1.23",
          "form": "또 다시 한번 봐야겠어요",
          "original_form": "또 다시 한번 봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003712.1.24",
          "form": "네 저도 또볼래요",
          "original_form": "네 저도 또볼래요",
          "speaker_id": "2",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003712.1.25",
          "form": "요새봐도 잼나요",
          "original_form": "요새봐도 잼나요",
          "speaker_id": "2",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003712.1.26",
          "form": "마동석의 재발견이죠",
          "original_form": "마동석의 재발견이죠",
          "speaker_id": "2",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003712.1.27",
          "form": "그때 그리 대성할줄 꿈에도 몰랐어요",
          "original_form": "그때 그리 대성할줄 꿈에도 몰랐어요",
          "speaker_id": "2",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003712.1.28",
          "form": "의리의 사나이",
          "original_form": "의리의 사나이",
          "speaker_id": "2",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003712.1.29",
          "form": "캬!! name2님 덕분에 보고 싶은것들 듣고싶은 것들 생겼어요 ㅎㅎ",
          "original_form": "캬!! &name2&님 덕분에 보고 싶은것들 듣고싶은 것들 생겼어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003712.1.30",
          "form": "그러게요 그땐,, 확 보이지 않았었는데",
          "original_form": "그러게요 그땐,, 확 보이지 않았었는데",
          "speaker_id": "1",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003712.1.31",
          "form": "그죠 세상사 아무도 몰라요",
          "original_form": "그죠 세상사 아무도 몰라요",
          "speaker_id": "2",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003712.1.32",
          "form": "펜트하우스는 어제 충격의 도가니 였어요",
          "original_form": "펜트하우스는 어제 충격의 도가니 였어요",
