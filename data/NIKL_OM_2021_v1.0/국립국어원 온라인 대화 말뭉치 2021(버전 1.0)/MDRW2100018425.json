      "utterance": [
        {
          "id": "MDRW2100018425.1.1",
          "form": "요즘 관심있는 종교가 있으신가요?",
          "original_form": "요즘 관심있는 종교가 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018425.1.2",
          "form": "아니요ㅎ",
          "original_form": "아니요ㅎ",
          "speaker_id": "2",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018425.1.3",
          "form": "무교입니다!",
          "original_form": "무교입니다!",
          "speaker_id": "2",
          "time": "20210827 01:38"
        },
        {
          "id": "MDRW2100018425.1.4",
          "form": "저도 무교입니다! name2은 왜 종교를 안 믿으시나요??",
          "original_form": "저도 무교입니다! &name2&은 왜 종교를 안 믿으시나요??",
          "speaker_id": "1",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018425.1.5",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018425.1.6",
          "form": "신앙심이 생기지않아서요!",
          "original_form": "신앙심이 생기지않아서요!",
          "speaker_id": "2",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018425.1.7",
          "form": "저도요! 비슷하네요~",
          "original_form": "저도요! 비슷하네요~",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.9",
          "form": "과연",
          "original_form": "과연",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.10",
          "form": "그들이 진짜존재하나",
          "original_form": "그들이 진짜존재하나",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.11",
          "form": "믿음이 전혀가지않죠",
          "original_form": "믿음이 전혀가지않죠",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.12",
          "form": "저는 신이 없다고 믿는데 누누님도 그러신가요??",
          "original_form": "저는 신이 없다고 믿는데 누누님도 그러신가요??",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.13",
          "form": "눈꼽만큼도요",
          "original_form": "눈꼽만큼도요",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.14",
          "form": "종교를 전혀가질수가없죠ㅋㅋㅋ",
          "original_form": "종교를 전혀가질수가없죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.15",
          "form": "맞아요ㅋㅋㅋ!!",
          "original_form": "맞아요ㅋㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.16",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.17",
          "form": "말이안돼요",
          "original_form": "말이안돼요",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.18",
          "form": "진짜로",
          "original_form": "진짜로",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018425.1.19",
          "form": "그 창조론이 너무 소설 같아서...ㅋㅋㅋ",
          "original_form": "그 창조론이 너무 소설 같아서...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018425.1.20",
          "form": "성경 같은 것도 그렇고...",
          "original_form": "성경 같은 것도 그렇고...",
          "speaker_id": "1",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018425.1.21",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018425.1.22",
          "form": "신이있다면",
          "original_form": "신이있다면",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018425.1.23",
          "form": "지구가",
          "original_form": "지구가",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018425.1.24",
          "form": "이모양 이꼴일리가없죠",
          "original_form": "이모양 이꼴일리가없죠",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018425.1.25",
          "form": "진정 그들이 말하는 그 신이 있다면말이예요",
          "original_form": "진정 그들이 말하는 그 신이 있다면말이예요",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018425.1.26",
          "form": "맞아요!! 종교는 그냥 필요에 의해 만들어진 허상에 불과하다고 생각해요",
          "original_form": "맞아요!! 종교는 그냥 필요에 의해 만들어진 허상에 불과하다고 생각해요",
          "speaker_id": "1",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018425.1.27",
          "form": "강압적인 포교할동도 별로고...",
          "original_form": "강압적인 포교할동도 별로고...",
          "speaker_id": "1",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018425.1.28",
          "form": "맞아요ㅋㅋㅋㅋ진짜이해안돼요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ진짜이해안돼요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018425.1.29",
          "form": "종교는 참 부정적인이미지가너무강해서ㅋㅋㅋㅋ",
          "original_form": "종교는 참 부정적인이미지가너무강해서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018425.1.30",
          "form": "다말하자면 썰리겠네요",
          "original_form": "다말하자면 썰리겠네요",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018425.1.31",
          "form": "맞아요... 종교 특유의 음침한 분위기도 별로고...ㅠㅠ!!",
          "original_form": "맞아요... 종교 특유의 음침한 분위기도 별로고...ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018425.1.32",
          "form": "코로나",
          "original_form": "코로나",
          "speaker_id": "2",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018425.1.33",
          "form": "이후로",
          "original_form": "이후로",
          "speaker_id": "2",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018425.1.34",
          "form": "특히 모여서 함께 해야한다는 생각이 정말 별로인 것 같아요",
          "original_form": "특히 모여서 함께 해야한다는 생각이 정말 별로인 것 같아요",
          "speaker_id": "1",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018425.1.35",
          "form": "이미지가 더더욱안좋아졌죠ㅋㅋㅋ",
          "original_form": "이미지가 더더욱안좋아졌죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018425.1.36",
          "form": "맞아요 참",
          "original_form": "맞아요 참",
          "speaker_id": "2",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018425.1.37",
          "form": "아 아프간도 난리죠",
          "original_form": "아 아프간도 난리죠",
          "speaker_id": "2",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018425.1.38",
          "form": "맞아요... 진짜 과도한 종교는 사회에도 악영향을 끼치니...",
          "original_form": "맞아요... 진짜 과도한 종교는 사회에도 악영향을 끼치니...",
          "speaker_id": "1",
          "time": "20210827 01:59"
        }
      ]
