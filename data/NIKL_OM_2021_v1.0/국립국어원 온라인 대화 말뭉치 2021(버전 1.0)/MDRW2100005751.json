        },
        {
          "id": "MDRW2100005751.1.2",
          "form": "다 잘먹어서",
          "original_form": "다 잘먹어서",
          "speaker_id": "2",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005751.1.3",
          "form": "와 다행이다",
          "original_form": "와 다행이다",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005751.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005751.1.5",
          "form": "근데 다 아시아권이라서",
          "original_form": "근데 다 아시아권이라서",
          "speaker_id": "2",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005751.1.6",
          "form": "나는 예전에 어디였지",
          "original_form": "나는 예전에 어디였지",
          "speaker_id": "1",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005751.1.7",
          "form": "?.?",
          "original_form": "?.?",
          "speaker_id": "2",
          "time": "20210907 10:45"
        },
        {
          "id": "MDRW2100005751.1.8",
          "form": "동남아 어디갔는데 배탈나서 가족들 액티비티하는데 해먹에 누워있었음",
          "original_form": "동남아 어디갔는데 배탈나서 가족들 액티비티하는데 해먹에 누워있었음",
          "speaker_id": "1",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100005751.1.9",
          "form": "억울했지...",
          "original_form": "억울했지...",
          "speaker_id": "1",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100005751.1.10",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "2",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100005751.1.11",
          "form": "힘들었겠구만",
          "original_form": "힘들었겠구만",
          "speaker_id": "2",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100005751.1.12",
          "form": "여행가서 탈 나는게 제일 억울",
          "original_form": "여행가서 탈 나는게 제일 억울",
          "speaker_id": "2",
          "time": "20210907 10:46"
        },
        {
          "id": "MDRW2100005751.1.13",
          "form": "내가 이때 서핑못한거 너무",
          "original_form": "내가 이때 서핑못한거 너무",
          "speaker_id": "1",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.14",
          "form": "이때까지 후회스러움",
          "original_form": "이때까지 후회스러움",
          "speaker_id": "1",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.15",
          "form": "서핑 잘해??",
          "original_form": "서핑 잘해??",
          "speaker_id": "2",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.16",
          "form": "그럼 하와이나 이런쪽도",
          "original_form": "그럼 하와이나 이런쪽도",
          "speaker_id": "2",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.17",
          "form": "아니 배울수 있는 기회였지",
          "original_form": "아니 배울수 있는 기회였지",
          "speaker_id": "1",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.18",
          "form": "괜찮겠다",
          "original_form": "괜찮겠다",
          "speaker_id": "2",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.19",
          "form": "....ㅜ",
          "original_form": "....ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.20",
          "form": "헉 그렇구만",
          "original_form": "헉 그렇구만",
          "speaker_id": "2",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.21",
          "form": "나중에 여행가서 배우면 되니까",
          "original_form": "나중에 여행가서 배우면 되니까",
          "speaker_id": "2",
          "time": "20210907 10:47"
        },
        {
          "id": "MDRW2100005751.1.22",
          "form": "넘 후회하지마~~",
          "original_form": "넘 후회하지마~~",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005751.1.23",
          "form": "글치글치",
          "original_form": "글치글치",
          "speaker_id": "1",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005751.1.24",
          "form": "기념품하니까 생각난다",
          "original_form": "기념품하니까 생각난다",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005751.1.25",
          "form": "제주도 기념품이 엄청 바뀌었다던대",
          "original_form": "제주도 기념품이 엄청 바뀌었다던대",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005751.1.26",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "1",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005751.1.27",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005751.1.28",
          "form": "내 마지막 기억은 복분자초콜릿 한라봉초콜릿이었는데",
          "original_form": "내 마지막 기억은 복분자초콜릿 한라봉초콜릿이었는데",
          "speaker_id": "2",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100005751.1.29",
          "form": "앜 나도 한라봉 초콜릿 말하려 했는데",
          "original_form": "앜 나도 한라봉 초콜릿 말하려 했는데",
          "speaker_id": "1",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100005751.1.30",
          "form": "과자도 나오고, 여러가지 다양하게 판다고 하더라",
          "original_form": "과자도 나오고, 여러가지 다양하게 판다고 하더라",
          "speaker_id": "2",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100005751.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ우리세대는...그게 제주도의 상징이었어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ우리세대는...그게 제주도의 상징이었어",
