      "utterance": [
        {
          "id": "MDRW2100010446.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:19"
        },
        {
          "id": "MDRW2100010446.1.2",
          "form": "오늘 비와서 그런가 전을 붙이고 싶네요 ㅋㅋ",
          "original_form": "오늘 비와서 그런가 전을 붙이고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010446.1.3",
          "form": "저녁 드셨나요 ㅎㅎ",
          "original_form": "저녁 드셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010446.1.4",
          "form": "전 종류 좋아 하세요?",
          "original_form": "전 종류 좋아 하세요?",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010446.1.5",
          "form": "아뇨 ㅡㅡ",
          "original_form": "아뇨 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010446.1.6",
          "form": "좀있다 먹으려고요",
          "original_form": "좀있다 먹으려고요",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010446.1.7",
          "form": "고민중이예요",
          "original_form": "고민중이예요",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010446.1.8",
          "form": "부추가 있는데 전을 부칠지 말지요",
          "original_form": "부추가 있는데 전을 부칠지 말지요",
          "speaker_id": "1",
          "time": "20210823 18:20"
        },
        {
          "id": "MDRW2100010446.1.9",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010446.1.10",
          "form": "저 사실 ㅋㅋ전 엄청 좋아해요",
          "original_form": "저 사실 ㅋㅋ전 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010446.1.11",
          "form": "근데 전 좀 시켜 먹자 해도 다들 호응을 안해요",
          "original_form": "근데 전 좀 시켜 먹자 해도 다들 호응을 안해요",
          "speaker_id": "1",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010446.1.12",
          "form": "비오는데 뭐 드셨나요?",
          "original_form": "비오는데 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010446.1.13",
          "form": "맛난 정보 듣고 싶네요 ㅎㅎ",
          "original_form": "맛난 정보 듣고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010446.1.14",
          "form": "아니 요리 힘든데",
          "original_form": "아니 요리 힘든데",
          "speaker_id": "2",
          "time": "20210823 18:21"
        },
        {
          "id": "MDRW2100010446.1.15",
          "form": "왜 요리시킨대요 ㅋㅋ",
          "original_form": "왜 요리시킨대요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010446.1.16",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010446.1.17",
          "form": "저 가끔 마트 있는데",
          "original_form": "저 가끔 마트 있는데",
          "speaker_id": "1",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010446.1.18",
          "form": "거기 반찬코너가 있어요",
          "original_form": "거기 반찬코너가 있어요",
          "speaker_id": "1",
          "time": "20210823 18:22"
        },
        {
          "id": "MDRW2100010446.1.19",
          "form": "거기 전 부쳐놓은거 사와서 혼자 먹어요 ㅋㅋ",
          "original_form": "거기 전 부쳐놓은거 사와서 혼자 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010446.1.20",
          "form": "전 좋아하세요",
          "original_form": "전 좋아하세요",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010446.1.21",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010446.1.22",
          "form": "반찬집 싸고 나름 맛집이예요",
          "original_form": "반찬집 싸고 나름 맛집이예요",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010446.1.23",
          "form": "전 좋아해요",
          "original_form": "전 좋아해요",
          "speaker_id": "2",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010446.1.24",
          "form": "오 그러시군요",
          "original_form": "오 그러시군요",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010446.1.25",
          "form": "싫어하는 음식이 있긴할까요 ㅋㅋ",
          "original_form": "싫어하는 음식이 있긴할까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010446.1.26",
          "form": "다 잘먹어 ㅋㅋ",
          "original_form": "다 잘먹어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010446.1.27",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010446.1.28",
          "form": "근데 저희집은 제가 외롭네요",
          "original_form": "근데 저희집은 제가 외롭네요",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010446.1.29",
          "form": "전 좋아하는데",
          "original_form": "전 좋아하는데",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010446.1.30",
          "form": "한번을 호응을 안해줘요",
          "original_form": "한번을 호응을 안해줘요",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.31",
          "form": "저는 저녁 절제좀 하려고요",
          "original_form": "저는 저녁 절제좀 하려고요",
          "speaker_id": "2",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.32",
          "form": "저만 먹기도 그렇고",
          "original_form": "저만 먹기도 그렇고",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.33",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.34",
          "form": "근데 자꾸 뭐 사먹고프네요",
          "original_form": "근데 자꾸 뭐 사먹고프네요",
          "speaker_id": "2",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.35",
          "form": "큰결심하셨네요",
          "original_form": "큰결심하셨네요",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.36",
          "form": "그렇죠 ㅜㅜ",
          "original_form": "그렇죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.37",
          "form": "전 저녁에 전이나 부쳐 먹으려고요",
          "original_form": "전 저녁에 전이나 부쳐 먹으려고요",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.38",
          "form": "어제 월욜부터 건강 메뉴로 먹는다고 한만큼  ㅋㅋ",
          "original_form": "어제 월욜부터 건강 메뉴로 먹는다고 한만큼  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.39",
          "form": "까짓거 혼자 실컷먹게요",
          "original_form": "까짓거 혼자 실컷먹게요",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.40",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010446.1.41",
          "form": "다 잊은걸로",
          "original_form": "다 잊은걸로",
          "speaker_id": "1",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010446.1.42",
          "form": "건강메뉴 뭐드시게요?",
          "original_form": "건강메뉴 뭐드시게요?",
          "speaker_id": "1",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010446.1.43",
          "form": "샐러드?",
          "original_form": "샐러드?",
          "speaker_id": "1",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010446.1.44",
          "form": "단백질 쉐이크 먹으려고요",
          "original_form": "단백질 쉐이크 먹으려고요",
          "speaker_id": "2",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100010446.1.45",
          "form": "하다못해 저녁이라도 참아보자며 ㅋㅋ",
          "original_form": "하다못해 저녁이라도 참아보자며 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100010446.1.46",
          "form": "아 그것만 먹음 안좋지 않을까요",
          "original_form": "아 그것만 먹음 안좋지 않을까요",
          "speaker_id": "1",
          "time": "20210823 18:28"
        }
      ]
    }
