      "utterance": [
        {
          "id": "MDRW2100014079.1.1",
          "form": "요즘 관심있는 물건 있으세요?",
          "original_form": "요즘 관심있는 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014079.1.2",
          "form": "저는 요즘",
          "original_form": "저는 요즘",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014079.1.3",
          "form": "워치류...",
          "original_form": "워치류...",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014079.1.4",
          "form": "갤럭시 워치 너무 갖고 싶어서 ㅠㅠ",
          "original_form": "갤럭시 워치 너무 갖고 싶어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014079.1.5",
          "form": "오 마쟈요",
          "original_form": "오 마쟈요",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014079.1.6",
          "form": "새로 나온거 너무 예쁘고 좋던데요??",
          "original_form": "새로 나온거 너무 예쁘고 좋던데요??",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014079.1.7",
          "form": "그래서 너무 갖고 싶더라고요 ㅠㅠㅠㅠ",
          "original_form": "그래서 너무 갖고 싶더라고요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014079.1.8",
          "form": "Z플립도 너무 예쁘구요ㅠ",
          "original_form": "Z플립도 너무 예쁘구요ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014079.1.9",
          "form": "마자요 ㅠㅠㅠㅜㅜㅜㅜㅠ",
          "original_form": "마자요 ㅠㅠㅠㅜㅜㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014079.1.10",
          "form": "근데 가격이 아직 조금 부담돼서 못살것 같긴한데",
          "original_form": "근데 가격이 아직 조금 부담돼서 못살것 같긴한데",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014079.1.11",
          "form": "사람들 후기만 구경해요ㅋㅋㅋ",
          "original_form": "사람들 후기만 구경해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014079.1.12",
          "form": "마자요 근데 진짜 너무 예쁘더라고요 ㅠㅠㅠ",
          "original_form": "마자요 근데 진짜 너무 예쁘더라고요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014079.1.13",
          "form": "사실 그전까진 그냥 새로운거 나와도 뜨뜻미지근했는데",
          "original_form": "사실 그전까진 그냥 새로운거 나와도 뜨뜻미지근했는데",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014079.1.14",
          "form": "이번거 너무 예뻐서 계속 눈이 가요",
          "original_form": "이번거 너무 예뻐서 계속 눈이 가요",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014079.1.15",
          "form": "마 자 요 ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "마 자 요 ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014079.1.16",
          "form": "진짜 감성...",
          "original_form": "진짜 감성...",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014079.1.17",
          "form": "휴 사고싶은거 너무 많아서 큰일이네요ㅋㅋㅋㅋ\n최근에는 사신물건 있으세요?",
          "original_form": "휴 사고싶은거 너무 많아서 큰일이네요ㅋㅋㅋㅋ\n최근에는 사신물건 있으세요?",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014079.1.18",
          "form": "최근에 마스크팩이여",
          "original_form": "최근에 마스크팩이여",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014079.1.19",
          "form": "피부가 아작나서 ㅋㅋㅋ",
          "original_form": "피부가 아작나서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014079.1.20",
          "form": "요즘 마스크도 하고 날씨도 습하고 더워서 피부가 가라앉을날이 없어요ㅜㅜ",
          "original_form": "요즘 마스크도 하고 날씨도 습하고 더워서 피부가 가라앉을날이 없어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014079.1.21",
          "form": "좋은 마스크팩 있나용?",
          "original_form": "좋은 마스크팩 있나용?",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014079.1.22",
          "form": "아비브꺼 좋아욥",
          "original_form": "아비브꺼 좋아욥",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014079.1.23",
          "form": "옹ㅎㅎ아비브 정유미님이 광고하셔서 왠지 한번 사보고싶었는데",
          "original_form": "옹ㅎㅎ아비브 정유미님이 광고하셔서 왠지 한번 사보고싶었는데",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014079.1.24",
          "form": "한번 사봐야겠어요",
          "original_form": "한번 사봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 17:29"
        }
      ]
    }
