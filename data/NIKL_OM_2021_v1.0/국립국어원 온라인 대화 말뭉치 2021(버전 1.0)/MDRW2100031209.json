      "utterance": [
        {
          "id": "MDRW2100031209.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031209.1.2",
          "form": "아뇨 아직요! name1님은요?",
          "original_form": "아뇨 아직요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031209.1.3",
          "form": "저는 방금 먹었어요",
          "original_form": "저는 방금 먹었어요",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031209.1.4",
          "form": "뭐 드실예정이세요?",
          "original_form": "뭐 드실예정이세요?",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031209.1.5",
          "form": "아직 못정했어요 뭐 드셨나요?",
          "original_form": "아직 못정했어요 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031209.1.6",
          "form": "저는 치킨사다 먹었어요",
          "original_form": "저는 치킨사다 먹었어요",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031209.1.7",
          "form": "오! 치킨 좋아하세요?",
          "original_form": "오! 치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100031209.1.8",
          "form": "치킨 좋죠ㅎㅎ name2님은 무슨 음식 좋아하시나요?",
          "original_form": "치킨 좋죠ㅎㅎ &name2&님은 무슨 음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100031209.1.9",
          "form": "저는 가리는거 없이 다 잘 먹고 다 좋아해요ㅋㅋㅋㅋㅋ 너무 뻔한 답변 같지만 진짜 그래가지규...ㅋㅋㅋㅋ",
          "original_form": "저는 가리는거 없이 다 잘 먹고 다 좋아해요ㅋㅋㅋㅋㅋ 너무 뻔한 답변 같지만 진짜 그래가지규...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100031209.1.10",
          "form": "그럼 질문있어요!",
          "original_form": "그럼 질문있어요!",
          "speaker_id": "2",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100031209.1.11",
          "form": "그게 좋죠",
          "original_form": "그게 좋죠",
          "speaker_id": "1",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100031209.1.12",
          "form": "튀김옷 샥샥 붙은 튀긴친 바삭한 구운치킨",
          "original_form": "튀김옷 샥샥 붙은 튀긴친 바삭한 구운치킨",
          "speaker_id": "2",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031209.1.13",
          "form": "어떤게 좋으세요?",
          "original_form": "어떤게 좋으세요?",
          "speaker_id": "2",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031209.1.14",
          "form": "튀긴 치킨쪽이 좋긴한데",
          "original_form": "튀긴 치킨쪽이 좋긴한데",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031209.1.15",
          "form": "가끔은 구운것도 땡겨요",
          "original_form": "가끔은 구운것도 땡겨요",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031209.1.16",
          "form": "저는 구운쪽이 조금더 아주 약간 더 많이 좋아요 ㅋㅋㅋㅋ",
          "original_form": "저는 구운쪽이 조금더 아주 약간 더 많이 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100031209.1.17",
          "form": "그럼 최애 치킨 메뉴는 뭔가요",
          "original_form": "그럼 최애 치킨 메뉴는 뭔가요",
          "speaker_id": "2",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100031209.1.18",
          "form": "오 건강을 생각하시는건가요?",
          "original_form": "오 건강을 생각하시는건가요?",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100031209.1.19",
          "form": "최애는 그냥 기본 후라이드요!",
          "original_form": "최애는 그냥 기본 후라이드요!",
          "speaker_id": "1",
          "time": "20210904 18:26"
        }
      ]
