      "utterance": [
        {
          "id": "MDRW2100012403.1.1",
          "form": "벌써 대학교 2학년 2학기네~개강한게지?",
          "original_form": "벌써 대학교 2학년 2학기네~개강한게지?",
          "speaker_id": "2",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100012403.1.2",
          "form": "아뇨아직안했어요",
          "original_form": "아뇨아직안했어요",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012403.1.3",
          "form": "개강이라니...그렇게슬픈...",
          "original_form": "개강이라니...그렇게슬픈...",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012403.1.4",
          "form": "표현을...",
          "original_form": "표현을...",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012403.1.5",
          "form": "쏘리 ㅋ 그럼..수강신청은 아직 안하거야?",
          "original_form": "쏘리 ㅋ 그럼..수강신청은 아직 안하거야?",
          "speaker_id": "2",
          "time": "20210825 08:50"
        },
        {
          "id": "MDRW2100012403.1.6",
          "form": "다 끝났어요ㅋㅋ중요한거 못잡아서 교수님한테 메일보냈는데 답장이 없으시네요",
          "original_form": "다 끝났어요ㅋㅋ중요한거 못잡아서 교수님한테 메일보냈는데 답장이 없으시네요",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100012403.1.7",
          "form": "전공필수도 못잡았는데 이건 마지막학기 아니면 따로 안받아준대요ㅜㅜ",
          "original_form": "전공필수도 못잡았는데 이건 마지막학기 아니면 따로 안받아준대요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100012403.1.8",
          "form": "수강신청이 정말 하늘의별따기구나 ㅠ",
          "original_form": "수강신청이 정말 하늘의별따기구나 ㅠ",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100012403.1.9",
          "form": "본전공 복수전공 둘다 수강신청 빡세기로 유명해요 하필..ㅋㅋㅋ",
          "original_form": "본전공 복수전공 둘다 수강신청 빡세기로 유명해요 하필..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100012403.1.10",
          "form": "복수전공 하는구나",
          "original_form": "복수전공 하는구나",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100012403.1.11",
          "form": "학업 스트레스보다 수강신청 스트레스가 더 크겠다~",
          "original_form": "학업 스트레스보다 수강신청 스트레스가 더 크겠다~",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100012403.1.12",
          "form": "대박 저 강의하나 교수님이 받아주신대용",
          "original_form": "대박 저 강의하나 교수님이 받아주신대용",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100012403.1.13",
          "form": "아싸 계절학기 안들어도 된다",
          "original_form": "아싸 계절학기 안들어도 된다",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100012403.1.14",
          "form": "오예에에",
          "original_form": "오예에에",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100012403.1.15",
          "form": "축하축하",
          "original_form": "축하축하",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100012403.1.16",
          "form": "보답으로 그 수업 열강해야겠군",
          "original_form": "보답으로 그 수업 열강해야겠군",
          "speaker_id": "2",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100012403.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100012403.1.18",
          "form": "학기중에 질문있을때 메일보내는데",
          "original_form": "학기중에 질문있을때 메일보내는데",
          "speaker_id": "1",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100012403.1.19",
          "form": "그때 겸사겸사 감사하다고 하려구요",
          "original_form": "그때 겸사겸사 감사하다고 하려구요",
          "speaker_id": "1",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100012403.1.20",
          "form": "그랴..엄청 감사하다고 열심히 강의 듣겠다고..",
          "original_form": "그랴..엄청 감사하다고 열심히 강의 듣겠다고..",
          "speaker_id": "2",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100012403.1.21",
          "form": "여전히 비대면  온라인강의인게지?",
          "original_form": "여전히 비대면  온라인강의인게지?",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100012403.1.22",
          "form": "7주차까지는 전면 비대면이고",
          "original_form": "7주차까지는 전면 비대면이고",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100012403.1.23",
          "form": "이후는 상황봐서+교수재량이래요",
          "original_form": "이후는 상황봐서+교수재량이래요",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100012403.1.24",
          "form": "지금 보면 교수님 두분정도는 오프라인 원하시는듯....",
          "original_form": "지금 보면 교수님 두분정도는 오프라인 원하시는듯....",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100012403.1.25",
          "form": "오프라인으로 수업하는 날이 멀지 않았구나",
          "original_form": "오프라인으로 수업하는 날이 멀지 않았구나",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100012403.1.26",
          "form": "그러면 좀 대학생 실감나려나..",
          "original_form": "그러면 좀 대학생 실감나려나..",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100012403.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100012403.1.28",
          "form": "근데 다들 수업듣고 해산할것같아섴ㅋㅋㅋ",
          "original_form": "근데 다들 수업듣고 해산할것같아섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100012403.1.29",
          "form": "그냥 학교나간다 이외의 의미는 없을것같아어",
          "original_form": "그냥 학교나간다 이외의 의미는 없을것같아어",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100012403.1.30",
          "form": "ㅠㅠ 동아리 활동도 하고",
          "original_form": "ㅠㅠ 동아리 활동도 하고",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100012403.1.31",
          "form": "과선배..동기 사람들도 사귀고..그래야 하는데...",
          "original_form": "과선배..동기 사람들도 사귀고..그래야 하는데...",
