      "utterance": [
        {
          "id": "MDRW2100013513.1.1",
          "form": "최근에 본 영화 있어요?",
          "original_form": "최근에 본 영화 있어요?",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013513.1.2",
          "form": "아니요 영화는 본 적 없고",
          "original_form": "아니요 영화는 본 적 없고",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100013513.1.3",
          "form": "슬의 재밌게 보고 있습니다",
          "original_form": "슬의 재밌게 보고 있습니다",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100013513.1.4",
          "form": "코로나 이후로 영화관을 간 적이 없네요",
          "original_form": "코로나 이후로 영화관을 간 적이 없네요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100013513.1.5",
          "form": "완전 바람직하시네요",
          "original_form": "완전 바람직하시네요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100013513.1.6",
          "form": "저는 슬의는 안보고있고 최근에 본드라마는.. 옛날꺼 봐가지고ㅎㅎ",
          "original_form": "저는 슬의는 안보고있고 최근에 본드라마는.. 옛날꺼 봐가지고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100013513.1.7",
          "form": "최근드라마는 괴물이 제일 최근인 것 같아요",
          "original_form": "최근드라마는 괴물이 제일 최근인 것 같아요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100013513.1.8",
          "form": "괴물 재밌나요??",
          "original_form": "괴물 재밌나요??",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100013513.1.9",
          "form": "괴물안보셨구나",
          "original_form": "괴물안보셨구나",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100013513.1.10",
          "form": "진짜 제발 봐주세요",
          "original_form": "진짜 제발 봐주세요",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100013513.1.11",
          "form": "제가 그렇게  믿을만하진 않겠지만 저믿고 보시면",
          "original_form": "제가 그렇게  믿을만하진 않겠지만 저믿고 보시면",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100013513.1.12",
          "form": "후회하지 않으실거예요",
          "original_form": "후회하지 않으실거예요",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100013513.1.13",
          "form": "ㅎㅎ 네 알겠습니다^^",
          "original_form": "ㅎㅎ 네 알겠습니다^^",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100013513.1.14",
          "form": "추천 감사해요^^",
          "original_form": "추천 감사해요^^",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100013513.1.15",
          "form": "넷플로 봐야겠네요",
          "original_form": "넷플로 봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100013513.1.16",
          "form": "저도 넷플 추천해주세여ㅋㅋㅋ",
          "original_form": "저도 넷플 추천해주세여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100013513.1.17",
          "form": "슬의 재밌습니다 ㅎㅎㅎ",
          "original_form": "슬의 재밌습니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100013513.1.18",
          "form": "시즌1부터 보시면 재밌을거예요",
          "original_form": "시즌1부터 보시면 재밌을거예요",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100013513.1.19",
          "form": "전 류준열을 좋아해서",
          "original_form": "전 류준열을 좋아해서",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100013513.1.20",
          "form": "조만간 류준열 전도연 나오는 드라마 기다리고 있어요^^",
          "original_form": "조만간 류준열 전도연 나오는 드라마 기다리고 있어요^^",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100013513.1.21",
          "form": "시즌 2개는 좀 길긴한뎈ㅋㅋㅋㅋ",
          "original_form": "시즌 2개는 좀 길긴한뎈ㅋㅋㅋㅋ",
