        },
        {
          "id": "MDRW2100013513.1.22",
          "form": "알겠습니다",
          "original_form": "알겠습니다",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100013513.1.23",
