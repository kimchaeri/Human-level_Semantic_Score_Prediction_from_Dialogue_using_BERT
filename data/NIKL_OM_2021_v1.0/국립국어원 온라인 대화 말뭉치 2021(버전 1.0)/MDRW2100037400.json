      "utterance": [
        {
          "id": "MDRW2100037400.1.1",
          "form": "나는 가을",
          "original_form": "나는 가을",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.2",
          "form": "겨울은 발시려",
          "original_form": "겨울은 발시려",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.3",
          "form": "이유는 뭐야?",
          "original_form": "이유는 뭐야?",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.5",
          "form": "나는 가을을 좋아해",
          "original_form": "나는 가을을 좋아해",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.6",
          "form": "아  겨울도 좀 괜찮아",
          "original_form": "아  겨울도 좀 괜찮아",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.7",
          "form": "나는 귤을 좋아하거든",
          "original_form": "나는 귤을 좋아하거든",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.8",
          "form": "낙옆 밟는 소리가좋아서",
          "original_form": "낙옆 밟는 소리가좋아서",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.9",
          "form": "올해는 맛있는 귤이 많이 났으면 좋겠어",
          "original_form": "올해는 맛있는 귤이 많이 났으면 좋겠어",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.10",
          "form": "겨울은 정말 귤에 감사해야해",
          "original_form": "겨울은 정말 귤에 감사해야해",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.11",
          "form": "낙엽사이에 벌레가 많던데 조심해",
          "original_form": "낙엽사이에 벌레가 많던데 조심해",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037400.1.12",
          "form": "그러게 달달한 귤은 너무 좋아",
          "original_form": "그러게 달달한 귤은 너무 좋아",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037400.1.13",
          "form": "맞아. 그리고 낭만에게 감사해야해.",
          "original_form": "맞아. 그리고 낭만에게 감사해야해.",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037400.1.14",
          "form": "눈은 정말.. 낭만빨이야",
          "original_form": "눈은 정말.. 낭만빨이야",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037400.1.15",
          "form": "너는 눈내리는건 좋아하니?",
          "original_form": "너는 눈내리는건 좋아하니?",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037400.1.16",
          "form": "조금 오는 건 좋아",
          "original_form": "조금 오는 건 좋아",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037400.1.17",
          "form": "어릴땐 참 좋아했는데",
          "original_form": "어릴땐 참 좋아했는데",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037400.1.18",
          "form": "많이 오면 교통이 불편해져",
          "original_form": "많이 오면 교통이 불편해져",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037400.1.19",
          "form": "그치 많이오면 너무 미끄러워",
          "original_form": "그치 많이오면 너무 미끄러워",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037400.1.20",
          "form": "질퍽한 것도 싫더라",
          "original_form": "질퍽한 것도 싫더라",
          "speaker_id": "2",
          "time": "20210831 23:17"
        }
      ]
    }
