      "utterance": [
        {
          "id": "MDRW2100010284.1.1",
          "form": "안녕하세요 !!",
          "original_form": "안녕하세요 !!",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010284.1.2",
          "form": "오늘 날씨 완전 가을 같네요~",
          "original_form": "오늘 날씨 완전 가을 같네요~",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010284.1.3",
          "form": "거기는",
          "original_form": "거기는",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010284.1.4",
          "form": "비가 안오나요?!",
          "original_form": "비가 안오나요?!",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010284.1.5",
          "form": "비가 아까는 내리다가 지금은 멈췄어요!",
          "original_form": "비가 아까는 내리다가 지금은 멈췄어요!",
          "speaker_id": "1",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010284.1.6",
          "form": "거기는 비오나요?",
          "original_form": "거기는 비오나요?",
          "speaker_id": "1",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010284.1.7",
          "form": "네네 여기는 비가오네요",
          "original_form": "네네 여기는 비가오네요",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010284.1.8",
          "form": "아무래도 비가오니",
          "original_form": "아무래도 비가오니",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010284.1.9",
          "form": "많이추워졌네요",
          "original_form": "많이추워졌네요",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010284.1.10",
          "form": "많이 쌀쌀해졌어요 바람도 많이불고",
          "original_form": "많이 쌀쌀해졌어요 바람도 많이불고",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010284.1.11",
          "form": "오늘은 긴팔입어도 될 날씨네요",
          "original_form": "오늘은 긴팔입어도 될 날씨네요",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010284.1.12",
          "form": "그러네요 기온이",
          "original_form": "그러네요 기온이",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010284.1.13",
          "form": "십도나 내려갔네요..!",
          "original_form": "십도나 내려갔네요..!",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010284.1.14",
          "form": "가을이 오려나봐요!",
          "original_form": "가을이 오려나봐요!",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010284.1.15",
          "form": "이번 여름에 진짜 더웠는데 갑자기 기온이 이렇게 떨어졌네요",
          "original_form": "이번 여름에 진짜 더웠는데 갑자기 기온이 이렇게 떨어졌네요",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010284.1.16",
          "form": "그니까요ㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:41"
        },
        {
          "id": "MDRW2100010284.1.17",
          "form": "일주일 사이에 훅 이네요",
          "original_form": "일주일 사이에 훅 이네요",
          "speaker_id": "2",
          "time": "20210823 16:41"
        },
        {
          "id": "MDRW2100010284.1.18",
          "form": "가을도 이렇게 훅 지나고 갑자기 겨울올까봐 무섭네요",
          "original_form": "가을도 이렇게 훅 지나고 갑자기 겨울올까봐 무섭네요",
          "speaker_id": "1",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100010284.1.19",
          "form": "가을이 점점 짧아져서요 ..ㅠㅠ",
          "original_form": "가을이 점점 짧아져서요 ..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100010284.1.20",
          "form": "아쉬워요ㅠ",
          "original_form": "아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010284.1.21",
          "form": "저는 가을이 제일 좋거든요",
          "original_form": "저는 가을이 제일 좋거든요",
          "speaker_id": "2",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010284.1.22",
          "form": "저도 계절 중에 가을이 제일 좋아요~",
          "original_form": "저도 계절 중에 가을이 제일 좋아요~",
          "speaker_id": "1",
          "time": "20210823 16:58"
        },
        {
          "id": "MDRW2100010284.1.23",
          "form": "날씨도 좋고 단풍도 예쁜데 .. 올해에는 가을이 조금 길었으면 좋겠네요",
          "original_form": "날씨도 좋고 단풍도 예쁜데 .. 올해에는 가을이 조금 길었으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210823 16:58"
        },
        {
          "id": "MDRW2100010284.1.24",
          "form": "저도요 가을이 참 놀기좋은날씬데",
          "original_form": "저도요 가을이 참 놀기좋은날씬데",
          "speaker_id": "2",
          "time": "20210823 17:03"
        },
        {
          "id": "MDRW2100010284.1.25",
          "form": "코로나땜에 어디가기도 그래요ㅠㅠ",
          "original_form": "코로나땜에 어디가기도 그래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 17:03"
        },
        {
          "id": "MDRW2100010284.1.26",
          "form": "맞아요 ㅠㅠ 단풍구경도 가면 좋을텐데 아쉽네요",
          "original_form": "맞아요 ㅠㅠ 단풍구경도 가면 좋을텐데 아쉽네요",
          "speaker_id": "1",
          "time": "20210823 17:04"
        }
      ]
    }
