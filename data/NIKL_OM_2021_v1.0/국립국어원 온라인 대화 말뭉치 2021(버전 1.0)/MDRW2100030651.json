      "utterance": [
        {
          "id": "MDRW2100030651.1.1",
          "form": "요즘 김밥 가격이 많이 올랐어요",
          "original_form": "요즘 김밥 가격이 많이 올랐어요",
          "speaker_id": "2",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.2",
          "form": "김밥만 드시나요",
          "original_form": "김밥만 드시나요",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.3",
          "form": "다른 음식은 없나요",
          "original_form": "다른 음식은 없나요",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.4",
          "form": "아까 전에 예감 과자랑 커피 먹었어요",
          "original_form": "아까 전에 예감 과자랑 커피 먹었어요",
          "speaker_id": "2",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.5",
          "form": "모든 물가가 올랐습니다",
          "original_form": "모든 물가가 올랐습니다",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.6",
          "form": "오 저도 커피랑 먹었어요",
          "original_form": "오 저도 커피랑 먹었어요",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.7",
          "form": "역시 남이 해준게 맛있네요",
          "original_form": "역시 남이 해준게 맛있네요",
          "speaker_id": "2",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.8",
          "form": "찐 감자도 있어서 감자를 더 먹었지요",
          "original_form": "찐 감자도 있어서 감자를 더 먹었지요",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.9",
          "form": "몸에서 감자 냄새가납니다",
          "original_form": "몸에서 감자 냄새가납니다",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.10",
          "form": "찐감자 맛있겠어요 소금 찍어 먹나요",
          "original_form": "찐감자 맛있겠어요 소금 찍어 먹나요",
          "speaker_id": "2",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030651.1.11",
          "form": "신 김치랑 먹었습니다",
          "original_form": "신 김치랑 먹었습니다",
          "speaker_id": "1",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030651.1.12",
          "form": "좋은 선택입니다 맛있어 보이네요",
          "original_form": "좋은 선택입니다 맛있어 보이네요",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030651.1.13",
          "form": "괜찮은 조합이네요",
          "original_form": "괜찮은 조합이네요",
          "speaker_id": "1",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030651.1.14",
          "form": "감자가 아직 많이 있습니다",
          "original_form": "감자가 아직 많이 있습니다",
          "speaker_id": "1",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030651.1.15",
          "form": "물김치도같이 드셔 보세요",
          "original_form": "물김치도같이 드셔 보세요",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030651.1.16",
          "form": "놀러 오시면 드릴 텐데",
          "original_form": "놀러 오시면 드릴 텐데",
          "speaker_id": "1",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030651.1.17",
          "form": "요즘 코로나 4단계로 놀러 가기 힘드네요",
          "original_form": "요즘 코로나 4단계로 놀러 가기 힘드네요",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030651.1.18",
          "form": "김치는 백김치 지요",
          "original_form": "김치는 백김치 지요",
          "speaker_id": "1",
