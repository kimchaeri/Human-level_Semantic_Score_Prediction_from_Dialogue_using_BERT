          "id": "MDRW2100042888.1.1",
          "form": "28일?",
          "original_form": "28일?",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042888.1.2",
          "form": "아 본인 수업하는데 실강은 힘들대",
          "original_form": "아 본인 수업하는데 실강은 힘들대",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042888.1.3",
          "form": "그래놓고 자차로 오기만 해",
          "original_form": "그래놓고 자차로 오기만 해",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042888.1.4",
          "form": "진심 열받게",
          "original_form": "진심 열받게",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042888.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042888.1.6",
          "form": "아니실강이나",
          "original_form": "아니실강이나",
          "speaker_id": "1",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042888.1.7",
          "form": "대면이나뭐가다람",
          "original_form": "대면이나뭐가다람",
          "speaker_id": "1",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042888.1.8",
          "form": "름",
          "original_form": "름",
          "speaker_id": "1",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042888.1.9",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "2",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042888.1.10",
          "form": "실강도쨔증나는데",
          "original_form": "실강도쨔증나는데",
          "speaker_id": "1",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042888.1.11",
          "form": "ㄱㄴㄲ 진짜",
          "original_form": "ㄱㄴㄲ 진짜",
          "speaker_id": "2",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042888.1.12",
          "form": "8시 30분 부터 수업 받는게 진짜 얼마나 어려운데",
          "original_form": "8시 30분 부터 수업 받는게 진짜 얼마나 어려운데",
          "speaker_id": "2",
          "time": "20210907 10:52"
        },
        {
          "id": "MDRW2100042888.1.13",
          "form": "사람 소리를 안내",
          "original_form": "사람 소리를 안내",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042888.1.14",
          "form": "9시부터아님?",
          "original_form": "9시부터아님?",
          "speaker_id": "1",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042888.1.15",
          "form": "ㄴㄴ8세 30분 부터",
          "original_form": "ㄴㄴ8세 30분 부터",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042888.1.16",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042888.1.17",
          "form": "8시30분에일러나야하네",
          "original_form": "8시30분에일러나야하네",
          "speaker_id": "1",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042888.1.18",
          "form": "실강으로해도",
          "original_form": "실강으로해도",
          "speaker_id": "1",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042888.1.19",
          "form": "ㄹㅇㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㄹㅇㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.20",
          "form": "대면이면",
          "original_form": "대면이면",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.21",
          "form": "6시 40분에 나가야함",
          "original_form": "6시 40분에 나가야함",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.22",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.23",
          "form": "고등학교때도",
          "original_form": "고등학교때도",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.24",
          "form": "안이러지않았음?",
          "original_form": "안이러지않았음?",
          "speaker_id": "1",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.25",
          "form": "ㅇㅈ 나 고등학생때 8시 30분에 나감",
          "original_form": "ㅇㅈ 나 고등학생때 8시 30분에 나감",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.26",
          "form": "애들한테 쌤 오나 알려달라 그러고",
          "original_form": "애들한테 쌤 오나 알려달라 그러고",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.27",
          "form": "그렇게 나갔는데",
          "original_form": "그렇게 나갔는데",
          "speaker_id": "2",
          "time": "20210907 10:54"
        },
        {
          "id": "MDRW2100042888.1.28",
          "form": "장난? 진심? 나랑?",
          "original_form": "장난? 진심? 나랑?",
          "speaker_id": "2",
          "time": "20210907 10:55"
        },
