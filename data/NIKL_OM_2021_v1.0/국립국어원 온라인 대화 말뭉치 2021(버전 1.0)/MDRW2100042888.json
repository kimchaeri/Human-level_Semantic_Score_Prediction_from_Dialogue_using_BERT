          "id": "MDRW2100042888.1.29",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100042888.1.30",
          "form": "무슨고등학교때보다",
          "original_form": "무슨고등학교때보다",
          "speaker_id": "1",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100042888.1.31",
          "form": "일찍나감?",
          "original_form": "일찍나감?",
          "speaker_id": "1",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100042888.1.32",
          "form": "6시40분에일어나는거부터가",
          "original_form": "6시40분에일어나는거부터가",
          "speaker_id": "1",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100042888.1.33",
          "form": "불가능인데",
          "original_form": "불가능인데",
          "speaker_id": "1",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100042888.1.34",
          "form": "억지로",
          "original_form": "억지로",
          "speaker_id": "2",
          "time": "20210907 10:55"
        },
        {
          "id": "MDRW2100042888.1.35",
          "form": "성실해지기",
          "original_form": "성실해지기",
          "speaker_id": "2",
          "time": "20210907 10:55"
        },
        {
