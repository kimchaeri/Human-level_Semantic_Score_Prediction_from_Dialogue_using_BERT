      "utterance": [
        {
          "id": "MDRW2100015255.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015255.1.2",
          "form": "아니요ㅜㅜ학생이고싶네요ㅎㅎ",
          "original_form": "아니요ㅜㅜ학생이고싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015255.1.3",
          "form": "학생때 수업은 잘 들으셧나요? ㅎㅎㅎ",
          "original_form": "학생때 수업은 잘 들으셧나요? ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015255.1.4",
          "form": "ㅋㅋㅋ 잘안들어서 후회해요ㅜㅜ..초등학생부터",
          "original_form": "ㅋㅋㅋ 잘안들어서 후회해요ㅜㅜ..초등학생부터",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015255.1.5",
          "form": "다시시작하고싶어요ㅋㅋ",
          "original_form": "다시시작하고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015255.1.6",
          "form": "근데 초딩 돌아가도 그대로이지 않을까요? ㅋㅋㅋ",
          "original_form": "근데 초딩 돌아가도 그대로이지 않을까요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015255.1.7",
          "form": "사람이 그래도 인데.. ㅎㅎ",
          "original_form": "사람이 그래도 인데.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015255.1.8",
          "form": "안돼....ㅋㅋㅋㅋ기",
          "original_form": "안돼....ㅋㅋㅋㅋ기",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015255.1.9",
          "form": "name1님 인생2회차이신가요ㅋㅋ",
          "original_form": "&name1&님 인생2회차이신가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015255.1.10",
          "form": "통달하셨네요",
          "original_form": "통달하셨네요",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015255.1.11",
          "form": "ㅋㅋㅋ 근데 진짜 초딩때 수업자세가 인생좌우하는거 같아요",
          "original_form": "ㅋㅋㅋ 근데 진짜 초딩때 수업자세가 인생좌우하는거 같아요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015255.1.12",
          "form": "아...맞아요.....",
          "original_form": "아...맞아요.....",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015255.1.13",
          "form": "그때부터가 시작이었어요ㅜㅜㅜㅜ",
          "original_form": "그때부터가 시작이었어요ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015255.1.14",
          "form": "초딩때 기초가 튼튼하면 중딩때 살짝 놀아도 금방 따라가는데...",
          "original_form": "초딩때 기초가 튼튼하면 중딩때 살짝 놀아도 금방 따라가는데...",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015255.1.15",
          "form": "기초가 없으면 엄청 힘들죠..",
          "original_form": "기초가 없으면 엄청 힘들죠..",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015255.1.16",
          "form": "맞아요ㅜㅜ근데 초딩때는 어설픈 기초가 통하니깓",
          "original_form": "맞아요ㅜㅜ근데 초딩때는 어설픈 기초가 통하니깓",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015255.1.17",
          "form": "더 힘들어지는거같아요",
          "original_form": "더 힘들어지는거같아요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015255.1.18",
          "form": "초딩때는 티가안나서ㅜㅜ",
