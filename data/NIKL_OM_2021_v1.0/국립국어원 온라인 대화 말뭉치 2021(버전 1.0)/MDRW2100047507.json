      "utterance": [
        {
          "id": "MDRW2100047507.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ그럴 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럴 것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100047507.1.2",
          "form": "햄스터도 귀여운데",
          "original_form": "햄스터도 귀여운데",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047507.1.3",
          "form": "수명이 너무 짧아서 자신이 없어요...",
          "original_form": "수명이 너무 짧아서 자신이 없어요...",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047507.1.4",
          "form": "햄스터두 귀여워요",
          "original_form": "햄스터두 귀여워요",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047507.1.5",
          "form": "근데 탈출해요ㅜ",
          "original_form": "근데 탈출해요ㅜ",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047507.1.6",
          "form": "앜ㅋㅋㅋㅋ맞아요 그렇더라구요",
          "original_form": "앜ㅋㅋㅋㅋ맞아요 그렇더라구요",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047507.1.7",
          "form": "햄스터는 너무 작아서 탈출하면 못찾을까봐",
          "original_form": "햄스터는 너무 작아서 탈출하면 못찾을까봐",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100047507.1.8",
          "form": "무섭긴해요...",
          "original_form": "무섭긴해요...",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100047507.1.9",
          "form": "구피는 탈출을 몰라요ㅎㅎ",
          "original_form": "구피는 탈출을 몰라요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047507.1.10",
          "form": "잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047507.1.11",
          "form": "구피 몇마리 키우세요??",
          "original_form": "구피 몇마리 키우세요??",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047507.1.12",
          "form": "다섯마리정도에요",
          "original_form": "다섯마리정도에요",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100047507.1.13",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100047507.1.14",
          "form": "그래도어항작아요30센티",
          "original_form": "그래도어항작아요30센티",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100047507.1.15",
          "form": "오 생각보다 작네요",
          "original_form": "오 생각보다 작네요",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047507.1.16",
          "form": "제 친구네는 구피가 막 너무 수가 불어나서 곤란해하고 그랬던 기억이 있는데",
          "original_form": "제 친구네는 구피가 막 너무 수가 불어나서 곤란해하고 그랬던 기억이 있는데",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047507.1.17",
          "form": "모든 구피가 그렇진 않은거죠?_?",
          "original_form": "모든 구피가 그렇진 않은거죠?_?",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047507.1.18",
          "form": "음 암컷 한마린데 임신한거 같진않아요",
          "original_form": "음 암컷 한마린데 임신한거 같진않아요",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100047507.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100047507.1.20",
          "form": "구피는 성별구별 어떻게 하나요?",
          "original_form": "구피는 성별구별 어떻게 하나요?",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100047507.1.21",
          "form": "모양이 쫌 달라요",
          "original_form": "모양이 쫌 달라요",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100047507.1.22",
          "form": "한마린데 응아만 하네요",
          "original_form": "한마린데 응아만 하네요",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100047507.1.23",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100047507.1.24",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100047507.1.25",
          "form": "건강하게 응아하는게 중요하죠",
          "original_form": "건강하게 응아하는게 중요하죠",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100047507.1.26",
          "form": "마자요 구피는 건강한게",
          "original_form": "마자요 구피는 건강한게",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
