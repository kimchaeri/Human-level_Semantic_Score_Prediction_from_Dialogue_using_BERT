          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100010327.1.12",
          "form": "이겼어요!",
          "original_form": "이겼어요!",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100010327.1.13",
