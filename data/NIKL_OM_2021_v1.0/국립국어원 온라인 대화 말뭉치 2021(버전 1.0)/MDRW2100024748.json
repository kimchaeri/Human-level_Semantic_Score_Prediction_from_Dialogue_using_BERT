      "utterance": [
        {
          "id": "MDRW2100024748.1.1",
          "form": "최근에 뭐 사셨어요~?",
          "original_form": "최근에 뭐 사셨어요~?",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024748.1.2",
          "form": "저 그 외장하드 샀는데",
          "original_form": "저 그 외장하드 샀는데",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024748.1.3",
          "form": "요즘껀 엄청 가볍고 작더라고요",
          "original_form": "요즘껀 엄청 가볍고 작더라고요",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024748.1.4",
          "form": "저는 울샴푸 세일하길래 샀어요 \r\n거의 다 썼거든요",
          "original_form": "저는 울샴푸 세일하길래 샀어요 \r\n거의 다 썼거든요",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024748.1.5",
          "form": "외장하드 용량 정하기 어렵던데 얼마짜리로 사셨어요?",
          "original_form": "외장하드 용량 정하기 어렵던데 얼마짜리로 사셨어요?",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024748.1.6",
          "form": "울샴푸!",
          "original_form": "울샴푸!",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024748.1.7",
          "form": "저 1테라로 샀어요",
          "original_form": "저 1테라로 샀어요",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024748.1.8",
          "form": "ssd",
          "original_form": "ssd",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024748.1.9",
          "form": "게임하려고ㅋㅋ",
          "original_form": "게임하려고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024748.1.10",
          "form": "게임저장용인가요?ㅋㅋ",
          "original_form": "게임저장용인가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024748.1.11",
          "form": "네네 ㅋㅋ 게임 즐겨하는건 아닌데",
          "original_form": "네네 ㅋㅋ 게임 즐겨하는건 아닌데",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024748.1.12",
          "form": "심즈 어쩌다가 하거든요",
          "original_form": "심즈 어쩌다가 하거든요",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024748.1.13",
          "form": "근데 cc템 때문에 용량이 너무 커져서 어쩔수없이 샀어요 ㅋ",
          "original_form": "근데 cc템 때문에 용량이 너무 커져서 어쩔수없이 샀어요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024748.1.14",
          "form": "외장하드는 돈 조금 더 주면 용량이 훅 커져서 살 때 고민되더라구요ㅋㅋ",
          "original_form": "외장하드는 돈 조금 더 주면 용량이 훅 커져서 살 때 고민되더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024748.1.15",
          "form": "맞아요 500기가 할까하다가 산김에 사자 싶어서 1테라 샀어요 충분하다해서",
          "original_form": "맞아요 500기가 할까하다가 산김에 사자 싶어서 1테라 샀어요 충분하다해서",
          "speaker_id": "2",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024748.1.16",
          "form": "생각보다 금방 차더라구요",
          "original_form": "생각보다 금방 차더라구요",
          "speaker_id": "1",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024748.1.17",
          "form": "어디 꺼 사셨어요?",
          "original_form": "어디 꺼 사셨어요?",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024748.1.18",
          "form": "저 그 씨게이트요!!!!",
          "original_form": "저 그 씨게이트요!!!!",
          "speaker_id": "2",
          "time": "20210831 19:12"
        },
        {
          "id": "MDRW2100024748.1.19",
          "form": "씨게이트도 유명하죠. 저장장치는 유명한 거 사는 게 맘 편한 것 같아요",
          "original_form": "씨게이트도 유명하죠. 저장장치는 유명한 거 사는 게 맘 편한 것 같아요",
          "speaker_id": "1",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024748.1.20",
          "form": "맞아요 그거 유명하다해서 비싸도 그거샀어요 ㅎ",
          "original_form": "맞아요 그거 유명하다해서 비싸도 그거샀어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 19:20"
        },
        {
          "id": "MDRW2100024748.1.21",
          "form": "사고싶은건 왤케 많은지 모르겠어요ㅋㅋ",
          "original_form": "사고싶은건 왤케 많은지 모르겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100024748.1.22",
          "form": "일단 장바구니에 이것저것 담아만 두죠ㅋㅋ",
          "original_form": "일단 장바구니에 이것저것 담아만 두죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100024748.1.23",
          "form": "맞아요ㅎ 충동적으로 살때도 있고",
          "original_form": "맞아요ㅎ 충동적으로 살때도 있고",
          "speaker_id": "2",
          "time": "20210831 19:53"
        },
        {
          "id": "MDRW2100024748.1.24",
          "form": "좀 생각해보고 안살때도 있고",
          "original_form": "좀 생각해보고 안살때도 있고",
          "speaker_id": "2",
          "time": "20210831 19:53"
        },
        {
          "id": "MDRW2100024748.1.25",
          "form": "꼭 필요한 것만 사야지 하는데 그래도 가끔은 충동구매 하게 돼요 ㅎㅎ",
