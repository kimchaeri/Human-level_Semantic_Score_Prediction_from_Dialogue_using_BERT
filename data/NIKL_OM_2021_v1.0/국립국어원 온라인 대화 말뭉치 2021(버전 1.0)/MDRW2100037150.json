      "utterance": [
        {
          "id": "MDRW2100037150.1.1",
          "form": "전 ㅠ.^ 겁없이 산책 나갔다가",
          "original_form": "전 ㅠ.^ 겁없이 산책 나갔다가",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037150.1.2",
          "form": "쫄딱 맞고 들어왔어요...",
          "original_form": "쫄딱 맞고 들어왔어요...",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037150.1.3",
          "form": "헉 진짜요?ㅠㅠ",
          "original_form": "헉 진짜요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100037150.1.4",
          "form": "근데 요새 날씨가 진짜 우산 챙기면 비 안오고",
          "original_form": "근데 요새 날씨가 진짜 우산 챙기면 비 안오고",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100037150.1.5",
          "form": "고민하다 우산 안가져가면 비와요ㅠㅠ",
          "original_form": "고민하다 우산 안가져가면 비와요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100037150.1.6",
          "form": "맞아요 -.- 엄청 얄밉다님가요",
          "original_form": "맞아요 -.- 엄청 얄밉다님가요",
          "speaker_id": "2",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100037150.1.7",
          "form": "비 막 오다가 중간에 뚝 끊겨서",
          "original_form": "비 막 오다가 중간에 뚝 끊겨서",
          "speaker_id": "2",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100037150.1.8",
          "form": "우산 괜히 가져온게 돼고 ㅠ.",
          "original_form": "우산 괜히 가져온게 돼고 ㅠ.",
          "speaker_id": "2",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100037150.1.9",
          "form": "그쵸ㅠㅠ오늘도 점심시간에",
          "original_form": "그쵸ㅠㅠ오늘도 점심시간에",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100037150.1.10",
          "form": "우산 안 가지고 나갔다가",
          "original_form": "우산 안 가지고 나갔다가",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100037150.1.11",
          "form": "한두방울 와서 돌아왔어요ㅋㅋㅋ",
          "original_form": "한두방울 와서 돌아왔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100037150.1.12",
          "form": "허얼 ㅠ!!!!! 그럴 땐 괜히 허무한 생각 들잖아요 ㅠ.^",
          "original_form": "허얼 ㅠ!!!!! 그럴 땐 괜히 허무한 생각 들잖아요 ㅠ.^",
          "speaker_id": "2",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100037150.1.13",
          "form": "그래도 봄이랑 달리",
          "original_form": "그래도 봄이랑 달리",
          "speaker_id": "2",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100037150.1.14",
          "form": "미세먼지 없는 거에 만족중이에요",
          "original_form": "미세먼지 없는 거에 만족중이에요",
          "speaker_id": "2",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100037150.1.15",
          "form": "맞아요ㅠㅠㅠ미세먼지 진짜 힘들었어요",
          "original_form": "맞아요ㅠㅠㅠ미세먼지 진짜 힘들었어요",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100037150.1.16",
          "form": "그래도 진짜 가을인가봐요 이제",
          "original_form": "그래도 진짜 가을인가봐요 이제",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100037150.1.17",
          "form": "그러니까요!! 슬슬 시원하더라고요",
          "original_form": "그러니까요!! 슬슬 시원하더라고요",
          "speaker_id": "2",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100037150.1.18",
          "form": "저는 문 열어놓고 자다가 기온차 때문에 감기까지 걸렸어요",
          "original_form": "저는 문 열어놓고 자다가 기온차 때문에 감기까지 걸렸어요",
          "speaker_id": "2",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100037150.1.19",
          "form": "헉 진짜요?ㅠㅠ",
          "original_form": "헉 진짜요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100037150.1.20",
          "form": "환절기에 감기 걸리면 너무 고생인거같아요",
          "original_form": "환절기에 감기 걸리면 너무 고생인거같아요",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100037150.1.21",
          "form": "그니께요 ㅠ.^ 아직 여름인줄 알고 반팔도 잔뜩 입고 다녔는대",
          "original_form": "그니께요 ㅠ.^ 아직 여름인줄 알고 반팔도 잔뜩 입고 다녔는대",
          "speaker_id": "2",
          "time": "20210901 16:29"
        },
        {
          "id": "MDRW2100037150.1.22",
          "form": "어느새 가을이더라고요",
          "original_form": "어느새 가을이더라고요",
          "speaker_id": "2",
          "time": "20210901 16:29"
        },
        {
          "id": "MDRW2100037150.1.23",
          "form": "name1님은 어느 계절이 제엘 좋으셔요?",
          "original_form": "&name1&님은 어느 계절이 제엘 좋으셔요?",
          "speaker_id": "2",
          "time": "20210901 16:29"
        },
        {
          "id": "MDRW2100037150.1.24",
          "form": "저는 봄가을 좋아해요!",
          "original_form": "저는 봄가을 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037150.1.25",
          "form": "원랜 봄을 더 좋아했던거 같은데 미세먼지때문에 이젠 가을이 더 좋아요",
          "original_form": "원랜 봄을 더 좋아했던거 같은데 미세먼지때문에 이젠 가을이 더 좋아요",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037150.1.26",
          "form": "오 저두요!! 가을이 그리고 뭘 껴입기 시작하는 날씨라",
          "original_form": "오 저두요!! 가을이 그리고 뭘 껴입기 시작하는 날씨라",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037150.1.27",
          "form": "멋낼 수 있어서 좋지 않나요",
          "original_form": "멋낼 수 있어서 좋지 않나요",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037150.1.28",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100037150.1.29",
          "form": "여름은 한계가 있어서ㅠㅠ",
          "original_form": "여름은 한계가 있어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100037150.1.30",
          "form": "맞아요 하나만 입어도",
          "original_form": "맞아요 하나만 입어도",
