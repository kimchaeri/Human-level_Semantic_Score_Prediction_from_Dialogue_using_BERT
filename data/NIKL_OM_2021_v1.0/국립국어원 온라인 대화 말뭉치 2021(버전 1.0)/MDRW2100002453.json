      "utterance": [
        {
          "id": "MDRW2100002453.1.1",
          "form": "아 나도 진짜",
          "original_form": "아 나도 진짜",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.2",
          "form": "너무 안먹은지 오래됨",
          "original_form": "너무 안먹은지 오래됨",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.3",
          "form": "글고 치즈볼두",
          "original_form": "글고 치즈볼두",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.4",
          "form": "맞다 우리전에",
          "original_form": "맞다 우리전에",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.5",
          "form": "치즈볼 안먹었자너",
          "original_form": "치즈볼 안먹었자너",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.6",
          "form": "ㅠㅠ 근데 너무 비싸 진짜",
          "original_form": "ㅠㅠ 근데 너무 비싸 진짜",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.7",
          "form": "인간적으로 3만원까진 좀 아니지",
          "original_form": "인간적으로 3만원까진 좀 아니지",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.8",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.9",
          "form": "너무비싸서 못사먹었잖아",
          "original_form": "너무비싸서 못사먹었잖아",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.10",
          "form": "27000원 이정도였는듯??",
          "original_form": "27000원 이정도였는듯??",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.11",
          "form": "뿌링클말고도",
          "original_form": "뿌링클말고도",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.12",
          "form": "다른치킨들도 너무비싸",
          "original_form": "다른치킨들도 너무비싸",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002453.1.13",
          "form": "예전엔 안그랬는데...",
          "original_form": "예전엔 안그랬는데...",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.14",
          "form": "그니께",
          "original_form": "그니께",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.15",
          "form": "교촌이 배달비 붙이면서",
          "original_form": "교촌이 배달비 붙이면서",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.16",
          "form": "그이후로 다른 치킨들도 다 붙이더니만",
          "original_form": "그이후로 다른 치킨들도 다 붙이더니만",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.17",
          "form": "이제 치킨값도 다 올리고있어...",
          "original_form": "이제 치킨값도 다 올리고있어...",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.18",
          "form": "악 내가 젤 좋아하는 치킨이 더비싸지면",
          "original_form": "악 내가 젤 좋아하는 치킨이 더비싸지면",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.19",
          "form": "이제 많이 못먹을지두몰러",
          "original_form": "이제 많이 못먹을지두몰러",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.20",
          "form": "ㅜㅜㅠ",
          "original_form": "ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.21",
          "form": "너무슬프다",
          "original_form": "너무슬프다",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.22",
          "form": "그래도 돈 많이벌어서",
          "original_form": "그래도 돈 많이벌어서",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002453.1.23",
          "form": "사묵자 언니야",
          "original_form": "사묵자 언니야",
          "speaker_id": "2",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.24",
          "form": "그려.....",
          "original_form": "그려.....",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.25",
          "form": "진짜 치킨 아무걱정 안하고 사먹을",
          "original_form": "진짜 치킨 아무걱정 안하고 사먹을",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.26",
          "form": "만큼 돈 많이벌어야지",
          "original_form": "만큼 돈 많이벌어야지",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.27",
          "form": "나드 그래야짓",
          "original_form": "나드 그래야짓",
          "speaker_id": "2",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.28",
          "form": "아자아자 팟팅이닷!",
          "original_form": "아자아자 팟팅이닷!",
          "speaker_id": "2",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.30",
          "form": "해쭈여 니가?",
          "original_form": "해쭈여 니가?",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.31",
          "form": "아 담엔 해쭈얘기하자",
          "original_form": "아 담엔 해쭈얘기하자",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002453.1.32",
          "form": "치킨 얘기하다가 갑자기ㅋㅋㅋㅋㅋ",
          "original_form": "치킨 얘기하다가 갑자기ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002453.1.33",
          "form": "오키오키",
          "original_form": "오키오키",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002453.1.34",
          "form": "해쭈도 음식 잘묵자나",
          "original_form": "해쭈도 음식 잘묵자나",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002453.1.35",
          "form": "맞어 먹방시리즈",
          "original_form": "맞어 먹방시리즈",
          "speaker_id": "1",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002453.1.36",
          "form": "그 먹는 브이로그 내가 젤 좋아하자나",
          "original_form": "그 먹는 브이로그 내가 젤 좋아하자나",
          "speaker_id": "1",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002453.1.37",
          "form": "나도 그거 몇번이나봤어",
          "original_form": "나도 그거 몇번이나봤어",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002453.1.38",
          "form": "완전꿀꿀잼",
          "original_form": "완전꿀꿀잼",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002453.1.39",
          "form": "집에",
          "original_form": "집에",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100002453.1.40",
          "form": "너가시킨 우유사탕왓다 기대돼",
          "original_form": "너가시킨 우유사탕왓다 기대돼",
          "speaker_id": "1",
          "time": "20210825 13:32"
        }
      ]
