      "utterance": [
        {
          "id": "MDRW2100024944.1.1",
          "form": "샐러드나 샌드위치는 알바들 아침으로 주셨었죠 ㅋㅋㅋ",
          "original_form": "샐러드나 샌드위치는 알바들 아침으로 주셨었죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024944.1.2",
          "form": "파바 오픈조 알바가 그렇게 힘들다면서요ㅠㅠ",
          "original_form": "파바 오픈조 알바가 그렇게 힘들다면서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024944.1.3",
          "form": "저는 오픈조 나쁘지 않았는데 매장이크면 빵 진열이 빡세서",
          "original_form": "저는 오픈조 나쁘지 않았는데 매장이크면 빵 진열이 빡세서",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024944.1.4",
          "form": "그게힘든것 같아요",
          "original_form": "그게힘든것 같아요",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024944.1.5",
          "form": "전 청소하고 그런게 힘들어서 차라리 진열하는게 좋았어요 ㅋㅋㅋㄱ",
          "original_form": "전 청소하고 그런게 힘들어서 차라리 진열하는게 좋았어요 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024944.1.6",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024944.1.7",
          "form": "장단점이 있군요",
          "original_form": "장단점이 있군요",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024944.1.8",
          "form": "카페는 무료라서",
          "original_form": "카페는 무료라서",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024944.1.9",
          "form": "ㅠㅠㅠ좋으셨겠어요 매일",
          "original_form": "ㅠㅠㅠ좋으셨겠어요 매일",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024944.1.10",
          "form": "맛있는 빵냄새에 둘러싸인 직업환경..",
          "original_form": "맛있는 빵냄새에 둘러싸인 직업환경..",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024944.1.11",
          "form": "커피는 무한정먹을수 있어서 그것도 좋았고",
          "original_form": "커피는 무한정먹을수 있어서 그것도 좋았고",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024944.1.12",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024944.1.13",
          "form": "빙수도요?",
          "original_form": "빙수도요?",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024944.1.14",
          "form": "빙수는 먹어본적이 없어서 모르겠어요",
          "original_form": "빙수는 먹어본적이 없어서 모르겠어요",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024944.1.15",
          "form": "근데 빙수는 사먹어야햇을거에요",
          "original_form": "근데 빙수는 사먹어야햇을거에요",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024944.1.16",
          "form": "파바 카페에서 뭐가 제일 맛있나요?",
          "original_form": "파바 카페에서 뭐가 제일 맛있나요?",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024944.1.17",
          "form": "전 한지 좀 오래되서 메뉴가 많이 바뀌었는데",
          "original_form": "전 한지 좀 오래되서 메뉴가 많이 바뀌었는데",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024944.1.18",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024944.1.19",
          "form": "밀크티라떼도 맛있고 아메리카노도 원두바뀌고 괜찮더라구요",
          "original_form": "밀크티라떼도 맛있고 아메리카노도 원두바뀌고 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024944.1.20",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024944.1.21",
          "form": "근데 아이스티는 드시지마세용",
          "original_form": "근데 아이스티는 드시지마세용",
          "speaker_id": "2",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024944.1.22",
          "form": "밀크티라떼도 파는줄 몰랐어요",
          "original_form": "밀크티라떼도 파는줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024944.1.23",
          "form": "아이스티 왜요?",
          "original_form": "아이스티 왜요?",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024944.1.24",
          "form": "아이스티는 공산품페트에서 따라주는 제품이에요",
          "original_form": "아이스티는 공산품페트에서 따라주는 제품이에요",
          "speaker_id": "2",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024944.1.25",
          "form": "가격대비 별로..",
          "original_form": "가격대비 별로..",
          "speaker_id": "2",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024944.1.26",
          "form": "맞아요 어느 카페를 가도",
          "original_form": "맞아요 어느 카페를 가도",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024944.1.27",
          "form": "큰마트가서 사서먹는거랑 똑같아요",
          "original_form": "큰마트가서 사서먹는거랑 똑같아요",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024944.1.28",
          "form": "아이스티는 돈아까운느낌 ㅠㅠ",
          "original_form": "아이스티는 돈아까운느낌 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:18"
        }
      ]
