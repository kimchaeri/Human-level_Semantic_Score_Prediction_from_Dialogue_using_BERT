        {
          "id": "MDRW2100029534.1.1",
          "form": "아침은 시리얼 먹었네요",
          "original_form": "아침은 시리얼 먹었네요",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029534.1.2",
          "form": "아 전 점심이 시리얼인데ㅋ",
          "original_form": "아 전 점심이 시리얼인데ㅋ",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100029534.1.3",
          "form": "그른가요",
          "original_form": "그른가요",
          "speaker_id": "2",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029534.1.4",
          "form": "네 점심은 간단히",
          "original_form": "네 점심은 간단히",
          "speaker_id": "1",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029534.1.5",
          "form": "전 생각중",
          "original_form": "전 생각중",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029534.1.6",
          "form": "메뉴고르기가 힘들죠",
          "original_form": "메뉴고르기가 힘들죠",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029534.1.7",
          "form": "그렇져",
          "original_form": "그렇져",
          "speaker_id": "2",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029534.1.8",
          "form": "구내식당 있음 편할거같아여",
          "original_form": "구내식당 있음 편할거같아여",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029534.1.9",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "2",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100029534.1.10",
          "form": "근데 요즘은 식당가서 먹기도 그래요",
          "original_form": "근데 요즘은 식당가서 먹기도 그래요",
          "speaker_id": "1",
          "time": "20210903 10:28"
        },
        {
          "id": "MDRW2100029534.1.11",
          "form": "거리두기때문에",
          "original_form": "거리두기때문에",
          "speaker_id": "2",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100029534.1.12",
          "form": "네 맞아요 조심스러워서",
          "original_form": "네 맞아요 조심스러워서",
          "speaker_id": "1",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100029534.1.13",
          "form": "아무래도 그렇죠 사람이 많아서",
          "original_form": "아무래도 그렇죠 사람이 많아서",
