      "utterance": [
        {
          "id": "MDRW2100036516.1.1",
          "form": "공주 부자떡집에 쑥떡팔아서 사봤는데\n손바닥만한 사이즈 좋더라구요",
          "original_form": "공주 부자떡집에 쑥떡팔아서 사봤는데\n손바닥만한 사이즈 좋더라구요",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100036516.1.2",
          "form": "아무래도 수공비가 더 드니 그런가봐요 ㅎ",
          "original_form": "아무래도 수공비가 더 드니 그런가봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100036516.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036516.1.4",
          "form": "그냥 댕강댕강 잘라서 콩고물 묻혀먹어요 ㅎ",
          "original_form": "그냥 댕강댕강 잘라서 콩고물 묻혀먹어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036516.1.5",
          "form": "티비에 카레우동 나오네요 ㅎ",
          "original_form": "티비에 카레우동 나오네요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036516.1.6",
          "form": "저런건 첨보는데 맛나겠어요",
          "original_form": "저런건 첨보는데 맛나겠어요",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036516.1.7",
          "form": "아 조합이..ㅋㅋㅋ 맛이없을수 없겟네요 ㅎㅎ",
          "original_form": "아 조합이..ㅋㅋㅋ 맛이없을수 없겟네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036516.1.8",
          "form": "나중에 한번 해봐야겠어요",
          "original_form": "나중에 한번 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036516.1.9",
          "form": "오호호.. 카레는 좋아하세요??",
          "original_form": "오호호.. 카레는 좋아하세요??",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036516.1.10",
          "form": "네 카레좋아해요",
          "original_form": "네 카레좋아해요",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036516.1.11",
          "form": "카레어디꺼 드세요??",
          "original_form": "카레어디꺼 드세요??",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036516.1.12",
          "form": "근데 첫째가 자꾸 맵다고 안먹어서",
          "original_form": "근데 첫째가 자꾸 맵다고 안먹어서",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036516.1.13",
          "form": "저는 별로인 짜장을 하게되요",
          "original_form": "저는 별로인 짜장을 하게되요",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036516.1.14",
          "form": "저는 여기저기 다 해요",
          "original_form": "저는 여기저기 다 해요",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036516.1.15",
          "form": "순한맛 안되나요ㅠㅠ",
          "original_form": "순한맛 안되나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036516.1.16",
          "form": "카레의 여왕이랑",
          "original_form": "카레의 여왕이랑",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036516.1.17",
          "form": "고형카레 좋아해요",
          "original_form": "고형카레 좋아해요",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036516.1.18",
          "form": "오뚜기 3일숙성 드셔보셨나요?",
          "original_form": "오뚜기 3일숙성 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036516.1.19",
          "form": "순한맛도 후추같은 화한맛이 싫은가봐요",
          "original_form": "순한맛도 후추같은 화한맛이 싫은가봐요",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036516.1.20",
          "form": "아뇨 그건 안먹어봤어요",
          "original_form": "아뇨 그건 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036516.1.21",
          "form": "맛나나요?",
          "original_form": "맛나나요?",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036516.1.22",
          "form": "약간 하이라이스삘나요 갈색같이 맛있더라구요 저는",
          "original_form": "약간 하이라이스삘나요 갈색같이 맛있더라구요 저는",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036516.1.23",
          "form": "아., .그건 좀 덜맵겠네요?",
          "original_form": "아., .그건 좀 덜맵겠네요?",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036516.1.24",
          "form": "네 그것도 순한맛 있으니 한번 해보세요 ㅎㅎ",
          "original_form": "네 그것도 순한맛 있으니 한번 해보세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036516.1.25",
          "form": "담에 장볼때 추가해야겠어요 감사해요",
          "original_form": "담에 장볼때 추가해야겠어요 감사해요",
