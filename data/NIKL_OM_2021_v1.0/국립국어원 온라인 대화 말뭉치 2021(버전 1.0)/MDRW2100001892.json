      "utterance": [
        {
          "id": "MDRW2100001892.1.1",
          "form": "현장에서 본 공연은 꽤 오래전 같아",
          "original_form": "현장에서 본 공연은 꽤 오래전 같아",
          "speaker_id": "2",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100001892.1.2",
          "form": "코로나 전엔 갔어",
          "original_form": "코로나 전엔 갔어",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001892.1.3",
          "form": "난 아이돌 온라인 콘서트 봤어",
          "original_form": "난 아이돌 온라인 콘서트 봤어",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001892.1.4",
          "form": "역시 현장감은 부족하더라",
          "original_form": "역시 현장감은 부족하더라",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001892.1.5",
          "form": "그치..ㅠㅠ",
          "original_form": "그치..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001892.1.6",
          "form": "최근에 누가 온라인 콘서트 했더라?세븐틴이었나?",
          "original_form": "최근에 누가 온라인 콘서트 했더라?세븐틴이었나?",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001892.1.7",
          "form": "아하 난 방탄 팬미팅",
          "original_form": "아하 난 방탄 팬미팅",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001892.1.8",
          "form": "근데 영 별로였어",
          "original_form": "근데 영 별로였어",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001892.1.9",
          "form": "헐 어땠는데??",
          "original_form": "헐 어땠는데??",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001892.1.10",
          "form": "무대가 돈 안 쓴 것 같았거든",
          "original_form": "무대가 돈 안 쓴 것 같았거든",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001892.1.11",
          "form": "야외에서 했어",
          "original_form": "야외에서 했어",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001892.1.12",
          "form": "잠실에서 무관객으로 했을거야",
          "original_form": "잠실에서 무관객으로 했을거야",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001892.1.13",
          "form": "아.......하이브....방시혁...왜이래 아는 사람들끼리ㅠ",
          "original_form": "아.......하이브....방시혁...왜이래 아는 사람들끼리ㅠ",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001892.1.14",
          "form": "아무리 그래도 진짜 서운하다...",
          "original_form": "아무리 그래도 진짜 서운하다...",
          "speaker_id": "1",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001892.1.15",
          "form": "유료였어?",
          "original_form": "유료였어?",
          "speaker_id": "1",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001892.1.16",
          "form": "콘서트가 아니라 그냥 팬미팅이라 그런가...",
          "original_form": "콘서트가 아니라 그냥 팬미팅이라 그런가...",
          "speaker_id": "2",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001892.1.17",
          "form": "응 유료였어",
          "original_form": "응 유료였어",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001892.1.18",
          "form": "원래 옛날엔 팬미팅 무료거나 얼마 안했어",
          "original_form": "원래 옛날엔 팬미팅 무료거나 얼마 안했어",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001892.1.19",
          "form": "근데 돈 받고 하는데 최소한의 회사와 팬간의 예의가 있어야하는거 아닐까....",
          "original_form": "근데 돈 받고 하는데 최소한의 회사와 팬간의 예의가 있어야하는거 아닐까....",
          "speaker_id": "1",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001892.1.20",
          "form": "와 근데 지금 비대면인데도 돈 받는거야??",
          "original_form": "와 근데 지금 비대면인데도 돈 받는거야??",
          "speaker_id": "1",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001892.1.21",
          "form": "아마 폭죽값이 훨 많이 소비되었을거야",
          "original_form": "아마 폭죽값이 훨 많이 소비되었을거야",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001892.1.22",
          "form": "응 아이돌 온라인 콘서트는",
          "original_form": "응 아이돌 온라인 콘서트는",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001892.1.23",
          "form": "오프라인 콘서트보단 싸지만",
          "original_form": "오프라인 콘서트보단 싸지만",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001892.1.24",
          "form": "유료야",
          "original_form": "유료야",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001892.1.25",
          "form": "ㅋㅋㅋㅋ맙소사..ㅠㅠㅠ그치만 돈 아깝다는 생각은 안하게 해줘야하는거 아닌가 생각이 드네...",
          "original_form": "ㅋㅋㅋㅋ맙소사..ㅠㅠㅠ그치만 돈 아깝다는 생각은 안하게 해줘야하는거 아닌가 생각이 드네...",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001892.1.26",
          "form": "당연히 콘서트만큼의 퀄은 바라지도 않는데...",
          "original_form": "당연히 콘서트만큼의 퀄은 바라지도 않는데...",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001892.1.27",
          "form": "근데 동생이랑 같은 팬이라 분할로 내서",
          "original_form": "근데 동생이랑 같은 팬이라 분할로 내서",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001892.1.28",
          "form": "그냥 유료 콘텐츠 값이니 부담은 덜했어",
          "original_form": "그냥 유료 콘텐츠 값이니 부담은 덜했어",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001892.1.29",
          "form": "아무래도 온라인이니 조금은 제약은 있지",
          "original_form": "아무래도 온라인이니 조금은 제약은 있지",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001892.1.30",
          "form": "아 그렇게 같이 나눠보면 괜찮겠네",
          "original_form": "아 그렇게 같이 나눠보면 괜찮겠네",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001892.1.31",
          "form": "그래도 콘서트도 현장감이 진짜 생명인데 코로나 빨리 끝나서 얼른 콘서트 시작했으면 좋겠다.",
          "original_form": "그래도 콘서트도 현장감이 진짜 생명인데 코로나 빨리 끝나서 얼른 콘서트 시작했으면 좋겠다.",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001892.1.32",
          "form": "그래도 온라인이라 티켓팅은 없었겠네!",
          "original_form": "그래도 온라인이라 티켓팅은 없었겠네!",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001892.1.33",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001892.1.34",
          "form": "집에서 편히 봐도 공연장 왔을때",
          "original_form": "집에서 편히 봐도 공연장 왔을때",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001892.1.35",
          "form": "뭔가 두근두근한 기분 있잖아",
          "original_form": "뭔가 두근두근한 기분 있잖아",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001892.1.36",
          "form": "그걸 다시 느껴보고 싶어 집은 조금은 반감되긴 해",
          "original_form": "그걸 다시 느껴보고 싶어 집은 조금은 반감되긴 해",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001892.1.37",
          "form": "맞아 티켓팅 걱정은 없지",
          "original_form": "맞아 티켓팅 걱정은 없지",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001892.1.38",
          "form": "뮤지컬도 인기있는건",
          "original_form": "뮤지컬도 인기있는건",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001892.1.39",
          "form": "치열하더라",
          "original_form": "치열하더라",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001892.1.40",
          "form": "아 맞아!진짜 콘서트날 준비해서 나갔다가 돌아오는 그 경험 전체가 엄청 소중한 추억이 되는 것 같아.",
          "original_form": "아 맞아!진짜 콘서트날 준비해서 나갔다가 돌아오는 그 경험 전체가 엄청 소중한 추억이 되는 것 같아.",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001892.1.41",
          "form": "맞아ㅠㅠ특히 뮤지컬 배우가 인기있는 타임이 당연하지만 엄청나...",
          "original_form": "맞아ㅠㅠ특히 뮤지컬 배우가 인기있는 타임이 당연하지만 엄청나...",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001892.1.42",
          "form": "전날에 설레고 긴장되고",
          "original_form": "전날에 설레고 긴장되고",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001892.1.43",
          "form": "시국이 시국인지라 좀 아쉬워",
          "original_form": "시국이 시국인지라 좀 아쉬워",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001892.1.44",
          "form": "난 뮤지컬 티켓팅 도와준 적 있거든",
          "original_form": "난 뮤지컬 티켓팅 도와준 적 있거든",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001892.1.45",
          "form": "근데 티켓팅 장난아니었어",
          "original_form": "근데 티켓팅 장난아니었어",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001892.1.46",
          "form": "그래도 이제 다들 백신 맞고 11월이면 20대까지 다 맞았을테니 겨울을 기대하고 있어..!",
          "original_form": "그래도 이제 다들 백신 맞고 11월이면 20대까지 다 맞았을테니 겨울을 기대하고 있어..!",
          "speaker_id": "1",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001892.1.47",
          "form": "맞아맞아ㅠㅠ진짜 이선좌....",
          "original_form": "맞아맞아ㅠㅠ진짜 이선좌....",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001892.1.48",
          "form": "포도알 터트리고 싶어.",
          "original_form": "포도알 터트리고 싶어.",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001892.1.49",
          "form": "나도! 그래서 얼른 백신 예약했지",
          "original_form": "나도! 그래서 얼른 백신 예약했지",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001892.1.50",
          "form": "이선좌 ㅋㅋㅋㅋ 이미 선택된 좌석",
          "original_form": "이선좌 ㅋㅋㅋㅋ 이미 선택된 좌석",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001892.1.51",
          "form": "사람을 미쳐버리게 해",
          "original_form": "사람을 미쳐버리게 해",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001892.1.52",
          "form": "딱 잡았는데 결제 하려고 볼때 이선좌 뜨는게 너무 싫어....",
          "original_form": "딱 잡았는데 결제 하려고 볼때 이선좌 뜨는게 너무 싫어....",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001892.1.53",
          "form": "헛된 희망ㅠㅠ",
          "original_form": "헛된 희망ㅠㅠ",
          "speaker_id": "1",
