      "utterance": [
        {
          "id": "MDRW2100013115.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013115.1.2",
          "form": "아뇨 예전에 키운 적은 있어요!",
          "original_form": "아뇨 예전에 키운 적은 있어요!",
          "speaker_id": "1",
          "time": "20210825 12:45"
        },
        {
          "id": "MDRW2100013115.1.3",
          "form": "오 어떤동물이요",
          "original_form": "오 어떤동물이요",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100013115.1.4",
          "form": "햄스터 두마리 키웠어요ㅋㅋㅋ",
          "original_form": "햄스터 두마리 키웠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100013115.1.5",
          "form": "헐 햄스터귀여워요",
          "original_form": "헐 햄스터귀여워요",
          "speaker_id": "2",
          "time": "20210825 12:49"
        },
        {
          "id": "MDRW2100013115.1.6",
          "form": "한 마리는 까매서 까망이었고 다른 한 마리는 하얘서 하양이라는 이름이었어요!",
          "original_form": "한 마리는 까매서 까망이었고 다른 한 마리는 하얘서 하양이라는 이름이었어요!",
          "speaker_id": "1",
          "time": "20210825 12:53"
        },
        {
          "id": "MDRW2100013115.1.7",
          "form": "다른거는 안키우셨나요",
          "original_form": "다른거는 안키우셨나요",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013115.1.8",
          "form": "네! name2 님은 반려동물 키우시나요",
          "original_form": "네! &name2& 님은 반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013115.1.9",
          "form": "아뇨 고양이키워보고싶어요",
          "original_form": "아뇨 고양이키워보고싶어요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100013115.1.10",
          "form": "어떤 종으로 키우시고 싶으세요?",
          "original_form": "어떤 종으로 키우시고 싶으세요?",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100013115.1.11",
          "form": "종은 잘 모르겠어요",
          "original_form": "종은 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100013115.1.12",
          "form": "저도 고양이 키워보고 싶네요ㅋㅋㅋ",
          "original_form": "저도 고양이 키워보고 싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100013115.1.13",
          "form": "종은 코숏인데 치즈로 키워보고 싶어요 너무 귀여울 것 같아요ㅋㅋㅋ",
          "original_form": "종은 코숏인데 치즈로 키워보고 싶어요 너무 귀여울 것 같아요ㅋㅋㅋ",
