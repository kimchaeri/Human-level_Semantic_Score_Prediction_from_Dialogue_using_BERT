      "utterance": [
        {
          "id": "MDRW2100040617.1.1",
          "form": "무슨..맥주 세잔반이었나",
          "original_form": "무슨..맥주 세잔반이었나",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100040617.1.2",
          "form": "기억도 안나 너무 어이없는 기준이라;;",
          "original_form": "기억도 안나 너무 어이없는 기준이라;;",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100040617.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100040617.1.4",
          "form": "아낰ㅋㅋㅋ 아니 맥주 세잔 반이면",
          "original_form": "아낰ㅋㅋㅋ 아니 맥주 세잔 반이면",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100040617.1.5",
          "form": "그 잔 크기가 설마 소주잔인가요?",
          "original_form": "그 잔 크기가 설마 소주잔인가요?",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100040617.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ맥주잔이겜ㅅ죠?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맥주잔이겜ㅅ죠?",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040617.1.7",
          "form": "아니그거면 그 병맥주 두병도 안되는제",
          "original_form": "아니그거면 그 병맥주 두병도 안되는제",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040617.1.8",
          "form": "생맥 2000은 먹는데 무슨 세잔반을...",
          "original_form": "생맥 2000은 먹는데 무슨 세잔반을...",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040617.1.9",
          "form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋ 진짜",
          "original_form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋ 진짜",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040617.1.10",
          "form": "술 잘 드시는 분들에겐",
          "original_form": "술 잘 드시는 분들에겐",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040617.1.11",
          "form": "비현실적인 조언이네요ㅠㅠ",
          "original_form": "비현실적인 조언이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040617.1.12",
          "form": "네 ㅎ",
          "original_form": "네 ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040617.1.13",
          "form": "애휴...",
          "original_form": "애휴...",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040617.1.14",
          "form": "술 좋아하는 의사들도",
          "original_form": "술 좋아하는 의사들도",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040617.1.15",
          "form": "환자들한테 줄이라고 해놓고",
          "original_form": "환자들한테 줄이라고 해놓고",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040617.1.16",
          "form": "그래도 지금은 나름 절주하...니까  ..",
          "original_form": "그래도 지금은 나름 절주하...니까  ..",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040617.1.17",
          "form": "자기들은 마실거얘요 ㅎ",
          "original_form": "자기들은 마실거얘요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040617.1.18",
          "form": "네네내",
          "original_form": "네네내",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040617.1.19",
          "form": "절주가 오래가길 빌어야죠",
          "original_form": "절주가 오래가길 빌어야죠",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040617.1.20",
          "form": "그것도 습관이지 않을까요?!",
          "original_form": "그것도 습관이지 않을까요?!",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040617.1.21",
          "form": "마자요 담배도 겁나피고",
          "original_form": "마자요 담배도 겁나피고",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040617.1.22",
          "form": "맞아요 더 한대요",
          "original_form": "맞아요 더 한대요",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040617.1.23",
          "form": "name1님은 친구들만나면",
          "original_form": "&name1&님은 친구들만나면",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040617.1.24",
          "form": "주로 맛집가고 카페가시나요?",
          "original_form": "주로 맛집가고 카페가시나요?",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040617.1.25",
          "form": "술을 즐기시진 않으니까",
          "original_form": "술을 즐기시진 않으니까",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040617.1.26",
          "form": "네네네 저는 주로 친구들의 취향을 따라요!",
          "original_form": "네네네 저는 주로 친구들의 취향을 따라요!",
          "speaker_id": "1",
