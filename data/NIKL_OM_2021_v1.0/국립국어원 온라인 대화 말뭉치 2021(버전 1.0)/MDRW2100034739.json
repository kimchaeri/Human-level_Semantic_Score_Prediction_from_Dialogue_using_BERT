        },
        {
          "id": "MDRW2100034739.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100034739.1.3",
          "form": "좋아하는 tv프로나 유튜브 있나요?",
          "original_form": "좋아하는 tv프로나 유튜브 있나요?",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034739.1.4",
          "form": "요즘은 대탈출이요!",
          "original_form": "요즘은 대탈출이요!",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034739.1.5",
          "form": "일요일만 기다리고 있어요",
          "original_form": "일요일만 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034739.1.6",
          "form": "도넛 먹지말라는 클립영상 본 것 같아요",
          "original_form": "도넛 먹지말라는 클립영상 본 것 같아요",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034739.1.7",
          "form": "그거 정말 먹으면 안되는 거였나요 ㅋㅋㅋ",
          "original_form": "그거 정말 먹으면 안되는 거였나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034739.1.8",
          "form": "앗 알려드려도 되는건가욧?!",
          "original_form": "앗 알려드려도 되는건가욧?!",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034739.1.9",
          "form": "혹시 엄청난 반전의 요소인가요?!",
          "original_form": "혹시 엄청난 반전의 요소인가요?!",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100034739.1.10",
          "form": "엄청나진 않지만 웃긴장면이였어요ㅋㅋㅋㅋ",
          "original_form": "엄청나진 않지만 웃긴장면이였어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034739.1.11",
          "form": "아 보호해주셔서 감사합니다 ㅋㅋㅋㅋ",
          "original_form": "아 보호해주셔서 감사합니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034739.1.12",
          "form": "대탈출 가끔 보는데 요즘 전성기 된 거 같아요 ㅋㅋㅋ",
          "original_form": "대탈출 가끔 보는데 요즘 전성기 된 거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100034739.1.13",
          "form": "진짜요ㅋㅋㅋ",
          "original_form": "진짜요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100034739.1.14",
          "form": "1ㅡ2화 보고 실망 좀 했는데 이번에 대박이였어요",
          "original_form": "1ㅡ2화 보고 실망 좀 했는데 이번에 대박이였어요",
          "speaker_id": "2",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100034739.1.15",
          "form": "요즘 예능들 시즌제 도입 잘 한것 같아요 ㅋㅋㅋ",
          "original_form": "요즘 예능들 시즌제 도입 잘 한것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100034739.1.16",
          "form": "돌아올 때다 재밌어지는 느낌이에요!",
          "original_form": "돌아올 때다 재밌어지는 느낌이에요!",
          "speaker_id": "1",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100034739.1.17",
          "form": "아 ㅋㅋㅋㅋㅋ 도넛 먹으면 안되는 거였네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ 도넛 먹으면 안되는 거였네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100034739.1.18",
          "form": "보셨군요ㅋㅋㅋㅋ깆ㅋㅋㅋ",
          "original_form": "보셨군요ㅋㅋㅋㅋ깆ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100034739.1.19",
          "form": "전 그거 제일 웃겼어요ㅋㅋㅋㅋ",
          "original_form": "전 그거 제일 웃겼어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100034739.1.20",
          "form": "다들 표정이 ㅋㅋㅋㅋㅋ 너무 허망해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다들 표정이 ㅋㅋㅋㅋㅋ 너무 허망해요 ㅋㅋㅋㅋㅋㅋㅋ",
