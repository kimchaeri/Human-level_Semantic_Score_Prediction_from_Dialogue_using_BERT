      "utterance": [
        {
          "id": "MDRW2100005844.1.1",
          "form": "다회용 컵 갖고다니세요?",
          "original_form": "다회용 컵 갖고다니세요?",
          "speaker_id": "1",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100005844.1.2",
          "form": "아니요~~자주잊어서",
          "original_form": "아니요~~자주잊어서",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100005844.1.3",
          "form": "스벅같은곳에서 텀블러 마케팅 하는것에 대해 어떻게 생각하세요?",
          "original_form": "스벅같은곳에서 텀블러 마케팅 하는것에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100005844.1.4",
          "form": "좋죠.환경생각하면",
          "original_form": "좋죠.환경생각하면",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100005844.1.5",
          "form": "네 근데 한 수천번인가 사용하지않으면 오히려 환경오염이라네요",
          "original_form": "네 근데 한 수천번인가 사용하지않으면 오히려 환경오염이라네요",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100005844.1.6",
          "form": "일회용품보다 못하다고해요",
          "original_form": "일회용품보다 못하다고해요",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100005844.1.7",
          "form": "환경살리기가 쉽지않네요",
          "original_form": "환경살리기가 쉽지않네요",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100005844.1.8",
          "form": "아..집에 텀블러 엄청많은데",
          "original_form": "아..집에 텀블러 엄청많은데",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100005844.1.9",
          "form": "맞는말이네요.잘사용안하니",
          "original_form": "맞는말이네요.잘사용안하니",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100005844.1.10",
          "form": "있는거 잘 활용하라고하더라구요",
          "original_form": "있는거 잘 활용하라고하더라구요",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100005844.1.11",
          "form": "근데 저도 산적은 없는데",
          "original_form": "근데 저도 산적은 없는데",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005844.1.12",
          "form": "집에 엄청 많네요",
          "original_form": "집에 엄청 많네요",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005844.1.13",
          "form": "저두요..",
          "original_form": "저두요..",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005844.1.14",
          "form": "얻은게 많아요",
          "original_form": "얻은게 많아요",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005844.1.15",
          "form": "보틀도많고",
          "original_form": "보틀도많고",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100005844.1.16",
          "form": "맞아요 보틀은 그냥 양념장 같은거 넣어놨어요",
          "original_form": "맞아요 보틀은 그냥 양념장 같은거 넣어놨어요",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005844.1.17",
          "form": "환경호르몬이 신경쓰여서 잘 쓰진않네요",
          "original_form": "환경호르몬이 신경쓰여서 잘 쓰진않네요",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100005844.1.18",
          "form": "저도 거의 안쓰고 보관용이네요",
          "original_form": "저도 거의 안쓰고 보관용이네요",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005844.1.19",
          "form": "기념품 같은걸로 많이받긴한데",
          "original_form": "기념품 같은걸로 많이받긴한데",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005844.1.20",
          "form": "의외로 쓸모가없더라구요",
          "original_form": "의외로 쓸모가없더라구요",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100005844.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005844.1.22",
          "form": "쓸일이 별로없어요",
          "original_form": "쓸일이 별로없어요",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005844.1.23",
          "form": "갖고다니기엔 좀 번거롭고 무겁구요",
          "original_form": "갖고다니기엔 좀 번거롭고 무겁구요",
          "speaker_id": "1",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005844.1.24",
