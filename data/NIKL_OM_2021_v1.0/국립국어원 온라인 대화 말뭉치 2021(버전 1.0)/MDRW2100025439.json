      "utterance": [
        {
          "id": "MDRW2100025439.1.1",
          "form": "게임자주하시나요?",
          "original_form": "게임자주하시나요?",
          "speaker_id": "1",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025439.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025439.1.3",
          "form": "엄청자주해요",
          "original_form": "엄청자주해요",
          "speaker_id": "2",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025439.1.4",
          "form": "겜하세요?",
          "original_form": "겜하세요?",
          "speaker_id": "2",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025439.1.5",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025439.1.6",
          "form": "근데 요새는 안해요",
          "original_form": "근데 요새는 안해요",
          "speaker_id": "1",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025439.1.7",
          "form": "요새 넘 바빠서",
          "original_form": "요새 넘 바빠서",
          "speaker_id": "1",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025439.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.9",
          "form": "오전 열시까지 소소하게 돈버느라ㅋㅋㅋ",
          "original_form": "오전 열시까지 소소하게 돈버느라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.10",
          "form": "전 롤 해요",
          "original_form": "전 롤 해요",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.11",
          "form": "리그오브레전드",
          "original_form": "리그오브레전드",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.12",
          "form": "롤 재밌나요?",
          "original_form": "롤 재밌나요?",
          "speaker_id": "1",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.13",
          "form": "네 재밌어요",
          "original_form": "네 재밌어요",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.14",
          "form": "폰겜하세요.ㅣ",
          "original_form": "폰겜하세요.ㅣ",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.15",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.16",
          "form": "폰겜하면 눈안아픈가요?",
          "original_form": "폰겜하면 눈안아픈가요?",
          "speaker_id": "1",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025439.1.17",
          "form": "아프죠",
          "original_form": "아프죠",
          "speaker_id": "2",
          "time": "20210901 11:24"
        },
        {
          "id": "MDRW2100025439.1.18",
          "form": "블루라이트 어플쓰고 안경쓰고 해요",
          "original_form": "블루라이트 어플쓰고 안경쓰고 해요",
          "speaker_id": "2",
          "time": "20210901 11:24"
        },
        {
          "id": "MDRW2100025439.1.19",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210901 11:24"
        },
        {
          "id": "MDRW2100025439.1.20",
          "form": "재밌나보군요!",
          "original_form": "재밌나보군요!",
          "speaker_id": "1",
          "time": "20210901 11:24"
        },
        {
          "id": "MDRW2100025439.1.21",
          "form": "블루라이트안경은 오래쓰면 콧대가 아파요..",
          "original_form": "블루라이트안경은 오래쓰면 콧대가 아파요..",
          "speaker_id": "1",
          "time": "20210901 11:24"
        },
        {
          "id": "MDRW2100025439.1.22",
          "form": "그래도 재밌어요",
          "original_form": "그래도 재밌어요",
          "speaker_id": "2",
          "time": "20210901 11:25"
        },
        {
          "id": "MDRW2100025439.1.23",
          "form": "손가락 운동이죠!",
          "original_form": "손가락 운동이죠!",
          "speaker_id": "2",
          "time": "20210901 11:25"
        }
      ]
    }
