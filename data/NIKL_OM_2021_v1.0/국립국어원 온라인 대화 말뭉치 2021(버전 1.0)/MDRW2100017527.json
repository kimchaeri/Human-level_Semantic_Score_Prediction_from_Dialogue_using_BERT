        },
        {
          "id": "MDRW2100017527.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017527.1.3",
          "form": "최근에 어떤거 구매하셧어요!?",
          "original_form": "최근에 어떤거 구매하셧어요!?",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017527.1.4",
          "form": "흠 저는 반팔 샀어여",
          "original_form": "흠 저는 반팔 샀어여",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017527.1.5",
          "form": "옹 그렇군요",
          "original_form": "옹 그렇군요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017527.1.6",
          "form": "좋아하는 브랜드있어요?",
          "original_form": "좋아하는 브랜드있어요?",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017527.1.7",
          "form": "딱히 정해져있는건 아니구요",
          "original_form": "딱히 정해져있는건 아니구요",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017527.1.8",
          "form": "디자인 맘에드는거있늠 사요",
          "original_form": "디자인 맘에드는거있늠 사요",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017527.1.9",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017527.1.10",
          "form": "최근 사신거 있나요",
          "original_form": "최근 사신거 있나요",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017527.1.11",
          "form": "저는 버킷햇 샀어요ㅋㅋㅋ",
          "original_form": "저는 버킷햇 샀어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017527.1.12",
          "form": "할인하길래 샀어용",
          "original_form": "할인하길래 샀어용",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017527.1.13",
          "form": "오 저도 최근에 샀네요 생각해보니",
          "original_form": "오 저도 최근에 샀네요 생각해보니",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017527.1.14",
          "form": "아울렛가서요 ㅎ",
          "original_form": "아울렛가서요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017527.1.15",
          "form": "그렇군요ㅋㅋㅋㅋ",
          "original_form": "그렇군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017527.1.16",
          "form": "친구들이랑 선물 주고받으세여?",
          "original_form": "친구들이랑 선물 주고받으세여?",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017527.1.17",
          "form": "생일때아니면 보통은 아니요 ㅎㅎ",
          "original_form": "생일때아니면 보통은 아니요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017527.1.18",
          "form": "그렇군여ㅋㅋㅋㅋ생일때 선물 뭐 사주세요?",
          "original_form": "그렇군여ㅋㅋㅋㅋ생일때 선물 뭐 사주세요?",
