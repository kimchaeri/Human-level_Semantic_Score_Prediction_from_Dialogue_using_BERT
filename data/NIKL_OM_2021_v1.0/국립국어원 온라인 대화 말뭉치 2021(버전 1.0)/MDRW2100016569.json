      "utterance": [
        {
          "id": "MDRW2100016569.1.1",
          "form": "저두 힙으뜸 봤어요!",
          "original_form": "저두 힙으뜸 봤어요!",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016569.1.2",
          "form": "영양제는 드세요?",
          "original_form": "영양제는 드세요?",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016569.1.3",
          "form": "네 소소하게 챙겨먹고있어요",
          "original_form": "네 소소하게 챙겨먹고있어요",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016569.1.4",
          "form": "드시는 영양제 있으세요?",
          "original_form": "드시는 영양제 있으세요?",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016569.1.5",
          "form": "네네 저 영양제 많이챙겨먹어요 ㅋㅋㅋ",
          "original_form": "네네 저 영양제 많이챙겨먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016569.1.6",
          "form": "뭐 드시는지 궁금해요 ㅋㅋ",
          "original_form": "뭐 드시는지 궁금해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016569.1.7",
          "form": "저 유산균 오메가3 비타민c,d 챙겨먹어요ㅋㅋㅋㅋ",
          "original_form": "저 유산균 오메가3 비타민c,d 챙겨먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016569.1.8",
          "form": "면역력이 좀 약한편이라서요 ㅠㅠ",
          "original_form": "면역력이 좀 약한편이라서요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016569.1.9",
          "form": "면역력이 약하시구나 ㅠㅠ",
          "original_form": "면역력이 약하시구나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016569.1.10",
          "form": "저도 비타민 유산균은 챙겨먹어요",
          "original_form": "저도 비타민 유산균은 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016569.1.11",
          "form": "오메가3도 챙겨드세요! 몸에 염증완화에 좋아요",
          "original_form": "오메가3도 챙겨드세요! 몸에 염증완화에 좋아요",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016569.1.12",
          "form": "저는 스트레스 잘 받는편이라그런지 면역력이 약한느낌..?",
          "original_form": "저는 스트레스 잘 받는편이라그런지 면역력이 약한느낌..?",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016569.1.13",
          "form": "오메가3 어디꺼 드세요?",
          "original_form": "오메가3 어디꺼 드세요?",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016569.1.14",
          "form": "스트레스가 만병의 근원이죠 ㅠㅠ",
          "original_form": "스트레스가 만병의 근원이죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016569.1.15",
          "form": "저는 스포츠리서치요! 냄새도안나구 가성비 좋습니다",
          "original_form": "저는 스포츠리서치요! 냄새도안나구 가성비 좋습니다",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016569.1.16",
          "form": "앗 냄새안나는거 중요하죠",
          "original_form": "앗 냄새안나는거 중요하죠",
          "speaker_id": "2",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016569.1.17",
          "form": "추천감사해요",
          "original_form": "추천감사해요",
          "speaker_id": "2",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016569.1.18",
          "form": "아이허브에서 사면 다른 할인이랑 같이되서 저렴해요!",
          "original_form": "아이허브에서 사면 다른 할인이랑 같이되서 저렴해요!",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016569.1.19",
          "form": "영양제는 역시 아이허브...!!",
          "original_form": "영양제는 역시 아이허브...!!",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016569.1.20",
          "form": "유산균은 어디꺼 드세요?",
          "original_form": "유산균은 어디꺼 드세요?",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016569.1.21",
          "form": "아이허브에 캘리포니아골드 어쩌구 유명한거있어요! 좋아요",
          "original_form": "아이허브에 캘리포니아골드 어쩌구 유명한거있어요! 좋아요",
          "speaker_id": "1",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016569.1.22",
          "form": "아니면 그 자로우!",
          "original_form": "아니면 그 자로우!",
          "speaker_id": "1",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016569.1.23",
          "form": "아 그거드시는구나",
          "original_form": "아 그거드시는구나",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016569.1.24",
          "form": "네네 유산균은 몸에 맞는거 먹어야되서",
          "original_form": "네네 유산균은 몸에 맞는거 먹어야되서",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016569.1.25",
          "form": "이거저거 먹어보면서 고르는게 좋대요",
          "original_form": "이거저거 먹어보면서 고르는게 좋대요",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016569.1.26",
          "form": "맞아요 유산균은 정말 유목민처럼 이것저것먹어봐야한대요",
          "original_form": "맞아요 유산균은 정말 유목민처럼 이것저것먹어봐야한대요",
          "speaker_id": "2",
          "time": "20210826 14:07"
        }
      ]
    }
