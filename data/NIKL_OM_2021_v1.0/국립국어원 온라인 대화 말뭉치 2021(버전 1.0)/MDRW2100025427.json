      "utterance": [
        {
          "id": "MDRW2100025427.1.1",
          "form": "식사하셧어요?",
          "original_form": "식사하셧어요?",
          "speaker_id": "1",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025427.1.2",
          "form": "아니요 ㅠ아직점심 시간이라니라서요 ㅠ",
          "original_form": "아니요 ㅠ아직점심 시간이라니라서요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025427.1.3",
          "form": "아침 드시는편이세요??",
          "original_form": "아침 드시는편이세요??",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025427.1.4",
          "form": "아니요 ㅠㅠㅠ",
          "original_form": "아니요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025427.1.5",
          "form": "점심 이랑 저녁만 챙겨먹어요",
          "original_form": "점심 이랑 저녁만 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025427.1.6",
          "form": "오 저랑 같네요!!",
          "original_form": "오 저랑 같네요!!",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025427.1.7",
          "form": "배고프지 않으세요?",
          "original_form": "배고프지 않으세요?",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025427.1.8",
          "form": "맞어요 일하면서 배에 꼬르륵 소리나면 얼마나 민망하던지 ㅋㅋㅋ",
          "original_form": "맞어요 일하면서 배에 꼬르륵 소리나면 얼마나 민망하던지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025427.1.9",
          "form": "그래서 커피를 꼭 마셔줘요!",
          "original_form": "그래서 커피를 꼭 마셔줘요!",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025427.1.10",
          "form": "커피는 어떤거 좋아하세요?",
          "original_form": "커피는 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025427.1.11",
          "form": "아침데는 아아 먹고",
          "original_form": "아침데는 아아 먹고",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025427.1.12",
          "form": "주로 에이드 많이 먹어요",
          "original_form": "주로 에이드 많이 먹어요",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025427.1.13",
          "form": "오오 저는 라떼 좋아해요 !!",
          "original_form": "오오 저는 라떼 좋아해요 !!",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025427.1.14",
          "form": "카페 어디로 자주 가세요?",
          "original_form": "카페 어디로 자주 가세요?",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025427.1.15",
          "form": "저는 숲이나 바다 보이는쪽으로가요",
          "original_form": "저는 숲이나 바다 보이는쪽으로가요",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100025427.1.16",
          "form": "우아 너뮤 좋을거같아요 !!",
          "original_form": "우아 너뮤 좋을거같아요 !!",
          "speaker_id": "1",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100025427.1.17",
          "form": "배가 너무 고프네요 ㅜㅜ",
          "original_form": "배가 너무 고프네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100025427.1.18",
          "form": "점심머드시나요",
          "original_form": "점심머드시나요",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100025427.1.19",
          "form": "돈가스 먹을려구용!!",
          "original_form": "돈가스 먹을려구용!!",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
