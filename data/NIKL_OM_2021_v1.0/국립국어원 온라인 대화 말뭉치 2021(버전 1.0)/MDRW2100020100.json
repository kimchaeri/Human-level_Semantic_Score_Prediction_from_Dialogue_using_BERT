      "utterance": [
        {
          "id": "MDRW2100020100.1.1",
          "form": "몰라 생각 안해봤는데?",
          "original_form": "몰라 생각 안해봤는데?",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020100.1.2",
          "form": "하긴 키울 일이 없으니까",
          "original_form": "하긴 키울 일이 없으니까",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020100.1.3",
          "form": "나 동물 한번도 안키워봤던거 같은데",
          "original_form": "나 동물 한번도 안키워봤던거 같은데",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020100.1.4",
          "form": "우리 사슴벌레 키웠었잖아",
          "original_form": "우리 사슴벌레 키웠었잖아",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020100.1.5",
          "form": "장수풍뎅이 아냐?",
          "original_form": "장수풍뎅이 아냐?",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020100.1.6",
          "form": "장수풍뎅이었나?",
          "original_form": "장수풍뎅이었나?",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020100.1.7",
          "form": "뿔 하나면 장수풍뎅이고 두개면 사슴벌레야",
          "original_form": "뿔 하나면 장수풍뎅이고 두개면 사슴벌레야",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020100.1.8",
          "form": "아 그렇게 구별하는구나",
          "original_form": "아 그렇게 구별하는구나",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020100.1.9",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020100.1.10",
          "form": "아무튼 그때 이름 뭐였지? 기억도 안나",
          "original_form": "아무튼 그때 이름 뭐였지? 기억도 안나",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020100.1.11",
          "form": "나도 이름 생각 안나;;;",
          "original_form": "나도 이름 생각 안나;;;",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020100.1.12",
          "form": "이름이 있긴 있었던 것 같은데",
          "original_form": "이름이 있긴 있었던 것 같은데",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020100.1.13",
          "form": "그래?난 이름도 기억 안나는데",
          "original_form": "그래?난 이름도 기억 안나는데",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020100.1.14",
          "form": "이름은 기억 안 나는데 이름이 있긴 했던 거 같아",
          "original_form": "이름은 기억 안 나는데 이름이 있긴 했던 거 같아",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020100.1.15",
          "form": "아 고양이 키워보고 싶다",
          "original_form": "아 고양이 키워보고 싶다",
