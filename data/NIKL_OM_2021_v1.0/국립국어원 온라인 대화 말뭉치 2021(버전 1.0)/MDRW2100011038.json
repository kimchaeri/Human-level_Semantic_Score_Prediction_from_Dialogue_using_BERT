      "utterance": [
        {
          "id": "MDRW2100011038.1.1",
          "form": "너는 동아리 한적있니?",
          "original_form": "너는 동아리 한적있니?",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011038.1.2",
          "form": "당연하지.",
          "original_form": "당연하지.",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011038.1.3",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011038.1.4",
          "form": "응 나는 보드게임동아리. 너는 어떤 동아리했어?",
          "original_form": "응 나는 보드게임동아리. 너는 어떤 동아리했어?",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011038.1.5",
          "form": "학교다닐 때 동아리를 하는 건 정말 좋은 거 같아.",
          "original_form": "학교다닐 때 동아리를 하는 건 정말 좋은 거 같아.",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.6",
          "form": "나는 다양한 동아리를 했었어.",
          "original_form": "나는 다양한 동아리를 했었어.",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.7",
          "form": "맞아 사람들을 사귀기 좋지",
          "original_form": "맞아 사람들을 사귀기 좋지",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.8",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.9",
          "form": "그리고 보람되더라.",
          "original_form": "그리고 보람되더라.",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.10",
          "form": "어떤 동아리를 했길래 보람도있던거야?",
          "original_form": "어떤 동아리를 했길래 보람도있던거야?",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.11",
          "form": "봉사동아리??",
          "original_form": "봉사동아리??",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.12",
          "form": "뭐 그런 동아리였어",
          "original_form": "뭐 그런 동아리였어",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.13",
          "form": "좋다 정말 정신건강에 좋은 것 같아",
          "original_form": "좋다 정말 정신건강에 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011038.1.14",
          "form": "대학생활은 사실 공부만하는데",
          "original_form": "대학생활은 사실 공부만하는데",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.15",
          "form": "내 허전한 영혼에 생기를 불어넣어준 활동이었어.",
          "original_form": "내 허전한 영혼에 생기를 불어넣어준 활동이었어.",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.16",
          "form": "이 멘트 정말 좋은 거 같다",
          "original_form": "이 멘트 정말 좋은 거 같다",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.17",
          "form": "나중에 면접 때 쓸거야.",
          "original_form": "나중에 면접 때 쓸거야.",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.18",
          "form": "세상에 정말 중요한 경험이었구나",
          "original_form": "세상에 정말 중요한 경험이었구나",
          "speaker_id": "1",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.19",
          "form": "ㅎㅎ좋아",
          "original_form": "ㅎㅎ좋아",
          "speaker_id": "1",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.20",
          "form": "응 꼭 추천해 ㅎ",
          "original_form": "응 꼭 추천해 ㅎ",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.21",
          "form": "동아리 열심히 해",
          "original_form": "동아리 열심히 해",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.22",
          "form": "추억이 같이 남는단다.",
          "original_form": "추억이 같이 남는단다.",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011038.1.23",
          "form": "나도 동아리를 좀 더 찾아봐야겠어",
          "original_form": "나도 동아리를 좀 더 찾아봐야겠어",
          "speaker_id": "1",
          "time": "20210824 00:04"
        }
      ]
