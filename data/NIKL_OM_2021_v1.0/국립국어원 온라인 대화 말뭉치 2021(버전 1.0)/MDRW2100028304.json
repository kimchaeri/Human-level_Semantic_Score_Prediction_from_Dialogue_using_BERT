      "utterance": [
        {
          "id": "MDRW2100028304.1.1",
          "form": "너도 집안일 좀 해라",
          "original_form": "너도 집안일 좀 해라",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028304.1.2",
          "form": "저도 청소 하잖아요 ㅠㅠ",
          "original_form": "저도 청소 하잖아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028304.1.3",
          "form": "빨래도 하고 요리도 하렴",
          "original_form": "빨래도 하고 요리도 하렴",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028304.1.4",
          "form": "오늘도 혼자 했다",
          "original_form": "오늘도 혼자 했다",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028304.1.5",
          "form": "네 알겠어요 ㅠㅠ 고생 많으시네요",
          "original_form": "네 알겠어요 ㅠㅠ 고생 많으시네요",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028304.1.6",
          "form": "아빠는 괜찮으세요??",
          "original_form": "아빠는 괜찮으세요??",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028304.1.7",
          "form": "아빠 열 많이 내렸어",
          "original_form": "아빠 열 많이 내렸어",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028304.1.8",
          "form": "병원까지 갔다왔는데 다행이지",
          "original_form": "병원까지 갔다왔는데 다행이지",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028304.1.9",
          "form": "진짜 큰일이었어요 얘기 듣고 엄청 걱정했는데",
          "original_form": "진짜 큰일이었어요 얘기 듣고 엄청 걱정했는데",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028304.1.10",
          "form": "39도까지 올라갔단다",
          "original_form": "39도까지 올라갔단다",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028304.1.11",
          "form": "헐... 옆에 있었어야 했는데 ㅠㅠ",
          "original_form": "헐... 옆에 있었어야 했는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028304.1.12",
          "form": "링거 맞고 괜찮아졌어",
          "original_form": "링거 맞고 괜찮아졌어",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028304.1.13",
          "form": "다행이지",
          "original_form": "다행이지",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028304.1.14",
          "form": "네 ㅠㅠ 백신이 다 좋은데 맞고 아파서 큰일이에요",
          "original_form": "네 ㅠㅠ 백신이 다 좋은데 맞고 아파서 큰일이에요",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028304.1.15",
          "form": "너도 1차 맞지 않았니?",
          "original_form": "너도 1차 맞지 않았니?",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028304.1.16",
          "form": "엊그제요!! 1차라 그렇게 힘들지 않았어요",
          "original_form": "엊그제요!! 1차라 그렇게 힘들지 않았어요",
          "speaker_id": "2",
          "time": "20210902 18:58"
