      "utterance": [
        {
          "id": "MDRW2100030292.1.1",
          "form": "창조적인 직업 너무 멋져요!",
          "original_form": "창조적인 직업 너무 멋져요!",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030292.1.2",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030292.1.3",
          "form": "개발자나 설계사나...  ㅋㅋ",
          "original_form": "개발자나 설계사나...  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030292.1.4",
          "form": "그래도 고생한만큼 성과가 딱 눈에 보이니 보람될거같아요",
          "original_form": "그래도 고생한만큼 성과가 딱 눈에 보이니 보람될거같아요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030292.1.5",
          "form": "야근만 적다면 보람을 느낄거 같은데",
          "original_form": "야근만 적다면 보람을 느낄거 같은데",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030292.1.6",
          "form": "피곤에 쩔면 그런생각자체가 안들어서 ㅋㅋ",
          "original_form": "피곤에 쩔면 그런생각자체가 안들어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030292.1.7",
          "form": "하긴 야근많이하면 피폐해져요ㅜ 저도 작년까진 업무가 하나 더 있어서 한달쯤 야근한적 있었는데",
          "original_form": "하긴 야근많이하면 피폐해져요ㅜ 저도 작년까진 업무가 하나 더 있어서 한달쯤 야근한적 있었는데",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030292.1.8",
          "form": "어느순간부터는 비몽사몽이에요ㅜ",
          "original_form": "어느순간부터는 비몽사몽이에요ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030292.1.9",
          "form": "피곤에 쩔어있으니까ㅜㅜ",
          "original_form": "피곤에 쩔어있으니까ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030292.1.10",
          "form": "졸리기만하고",
          "original_form": "졸리기만하고",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030292.1.11",
          "form": "맞아요 진짜 비효율인데 또 어떻게 마감전까지 맞춰지니까 계속 반복되는거같아요ㅜㅠ",
          "original_form": "맞아요 진짜 비효율인데 또 어떻게 마감전까지 맞춰지니까 계속 반복되는거같아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030292.1.12",
          "form": "일을 줄여야 야근이 없어지는데",
          "original_form": "일을 줄여야 야근이 없어지는데",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030292.1.13",
          "form": "일은 끊이질 않네요",
          "original_form": "일은 끊이질 않네요",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030292.1.14",
          "form": "회사가 부자되겠어요! 곧 명절이니!!! 보너스!!!!",
          "original_form": "회사가 부자되겠어요! 곧 명절이니!!! 보너스!!!!",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030292.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030292.1.16",
          "form": "보너스 주나여?",
          "original_form": "보너스 주나여?",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030292.1.17",
          "form": "저흰 저번달에 주긴했는데",
          "original_form": "저흰 저번달에 주긴했는데",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030292.1.18",
          "form": "명절엔 상여금?ㅋㅋ",
          "original_form": "명절엔 상여금?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030292.1.19",
          "form": "저흰 요즘 일이...ㅜㅠ",
          "original_form": "저흰 요즘 일이...ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030292.1.20",
          "form": "회사가 힘들어서ㅋㅋ 근데 이번달엔 준다고하더라그요",
          "original_form": "회사가 힘들어서ㅋㅋ 근데 이번달엔 준다고하더라그요",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030292.1.21",
          "form": "아직 직원들에겐 비밀....",
          "original_form": "아직 직원들에겐 비밀....",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030292.1.22",
          "form": "다 알고계시는군여 ㅋㅋㅋ",
          "original_form": "다 알고계시는군여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030292.1.23",
          "form": "작은회사 관리부서는... ㅋㅋ",
          "original_form": "작은회사 관리부서는... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030292.1.24",
          "form": "어차피 곧 알게될텐데 비밀이에요?ㅋㅋ",
          "original_form": "어차피 곧 알게될텐데 비밀이에요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030292.1.25",
          "form": "서프라이즈죠!!!",
          "original_form": "서프라이즈죠!!!",
          "speaker_id": "1",
