        {
          "id": "MDRW2100018430.1.1",
          "form": "아니요 가본적이없어요.",
          "original_form": "아니요 가본적이없어요.",
          "speaker_id": "2",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018430.1.2",
          "form": "너무 가보고 싶어요",
          "original_form": "너무 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018430.1.3",
          "form": "오 가서 뭐하고싶으신가요",
          "original_form": "오 가서 뭐하고싶으신가요",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018430.1.4",
          "form": "관광이요",
          "original_form": "관광이요",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018430.1.5",
          "form": "제일 가고싶은 곳이 어딘가요",
          "original_form": "제일 가고싶은 곳이 어딘가요",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018430.1.6",
          "form": "교토요",
          "original_form": "교토요",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018430.1.7",
          "form": "교토에서 뭐보시려고요?",
          "original_form": "교토에서 뭐보시려고요?",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018430.1.8",
          "form": "옛날 건축물 좋아해요",
          "original_form": "옛날 건축물 좋아해요",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018430.1.9",
          "form": "오 안무서우신가요?",
