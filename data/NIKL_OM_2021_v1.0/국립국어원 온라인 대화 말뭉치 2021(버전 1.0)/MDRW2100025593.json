      "utterance": [
        {
          "id": "MDRW2100025593.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025593.1.2",
          "form": "네 볶음밥에 카레 먹었어요",
          "original_form": "네 볶음밥에 카레 먹었어요",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025593.1.3",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025593.1.4",
          "form": "네 먹었습니다!",
          "original_form": "네 먹었습니다!",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025593.1.5",
          "form": "카레 좋아하시나요?",
          "original_form": "카레 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025593.1.6",
          "form": "그냥그냥요",
          "original_form": "그냥그냥요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025593.1.7",
          "form": "만들기 쉬워서 해먹었어요",
          "original_form": "만들기 쉬워서 해먹었어요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025593.1.8",
          "form": "뭐드셨나요",
          "original_form": "뭐드셨나요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025593.1.9",
          "form": "저는 라면에 김치전 먹었습니다",
          "original_form": "저는 라면에 김치전 먹었습니다",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025593.1.10",
          "form": "김치전 좋아하시나요?",
          "original_form": "김치전 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025593.1.11",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025593.1.12",
          "form": "김치로 만든건 다 맛나죠!",
          "original_form": "김치로 만든건 다 맛나죠!",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025593.1.13",
          "form": "직접 부치신거에요?",
          "original_form": "직접 부치신거에요?",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025593.1.14",
          "form": "아니욯ㅎ",
          "original_form": "아니욯ㅎ",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025593.1.15",
          "form": "마켓컬리에서 산 걸로 해먹었어요!",
          "original_form": "마켓컬리에서 산 걸로 해먹었어요!",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025593.1.16",
          "form": "직접 부치셨나요?",
          "original_form": "직접 부치셨나요?",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025593.1.17",
          "form": "아뇨 전 요릴 잘 못해서요!",
          "original_form": "아뇨 전 요릴 잘 못해서요!",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025593.1.18",
          "form": "배달음식 자주 먹어요 ㅋㅋㅋ",
          "original_form": "배달음식 자주 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025593.1.19",
          "form": "마켓 컬리로 배달하는거 진짜 편하죠?",
          "original_form": "마켓 컬리로 배달하는거 진짜 편하죠?",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025593.1.20",
          "form": "네 엄청 편해요!!",
          "original_form": "네 엄청 편해요!!",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025593.1.21",
          "form": "그냥 가열해서 먹기만하면 되니까 좋더라구요ㅎㅎ",
          "original_form": "그냥 가열해서 먹기만하면 되니까 좋더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025593.1.22",
          "form": "배달음식 어떤거 자주 드세요?",
          "original_form": "배달음식 어떤거 자주 드세요?",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100025593.1.23",
          "form": "전 찜닭이나 중화요리 치킨 많이 시켜먹어요",
          "original_form": "전 찜닭이나 중화요리 치킨 많이 시켜먹어요",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025593.1.24",
          "form": "일하고 집갈때 포장도 종종하구요",
          "original_form": "일하고 집갈때 포장도 종종하구요",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025593.1.25",
          "form": "배달료가 너무 많이 올라서 부담되네요",
          "original_form": "배달료가 너무 많이 올라서 부담되네요",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025593.1.26",
          "form": "또 오른다는 소식이 있어서 슬퍼요 ㅠㅠ",
          "original_form": "또 오른다는 소식이 있어서 슬퍼요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025593.1.27",
          "form": "헉 또 오른대요??",
          "original_form": "헉 또 오른대요??",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025593.1.28",
          "form": "지금도 부담인데ㅠㅠ",
          "original_form": "지금도 부담인데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025593.1.29",
          "form": "그러니까요 ㅠㅠ",
          "original_form": "그러니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025593.1.30",
          "form": "배달을 끊던가 포장을 해와야 할것같아요",
          "original_form": "배달을 끊던가 포장을 해와야 할것같아요",
          "speaker_id": "2",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025593.1.31",
          "form": "편하긴한데 더 비싸진다면 횟수를 줄이긴 해야겠어요ㅠㅠ",
          "original_form": "편하긴한데 더 비싸진다면 횟수를 줄이긴 해야겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:23"
        }
      ]
    }
