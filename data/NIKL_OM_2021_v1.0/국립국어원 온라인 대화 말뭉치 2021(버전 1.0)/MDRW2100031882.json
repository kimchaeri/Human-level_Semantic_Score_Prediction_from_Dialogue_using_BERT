      "utterance": [
        {
          "id": "MDRW2100031882.1.1",
          "form": "운이 안조으셧던거 아닐가요?",
          "original_form": "운이 안조으셧던거 아닐가요?",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031882.1.2",
          "form": "저 만난 여자들은 다 행복해했서요",
          "original_form": "저 만난 여자들은 다 행복해했서요",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031882.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031882.1.4",
          "form": "최선을 다했습니다 ㅋㅋ",
          "original_form": "최선을 다했습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031882.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031882.1.6",
          "form": "최선을 다 하셨으니까 그렇죠 근데 정말 진심으로",
          "original_form": "최선을 다 하셨으니까 그렇죠 근데 정말 진심으로",
          "speaker_id": "1",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031882.1.7",
          "form": "마음에 와닿으셔서 한 거 맞나요",
          "original_form": "마음에 와닿으셔서 한 거 맞나요",
          "speaker_id": "1",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031882.1.8",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.9",
          "form": "안보면 보고싶고",
          "original_form": "안보면 보고싶고",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.10",
          "form": "맛있는거 같이 먹고싶고",
          "original_form": "맛있는거 같이 먹고싶고",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.11",
          "form": "같이 공부하고",
          "original_form": "같이 공부하고",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.12",
          "form": "놀러가고",
          "original_form": "놀러가고",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.13",
          "form": "그랬죵",
          "original_form": "그랬죵",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.14",
          "form": "아 근데 결혼은 왜 말리시는 거예요",
          "original_form": "아 근데 결혼은 왜 말리시는 거예요",
          "speaker_id": "1",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.15",
          "form": "아 해보니까 ㅋㅋ",
          "original_form": "아 해보니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.16",
          "form": "아까 남자로 착하시고 하지 말라고 하셨잖아요",
          "original_form": "아까 남자로 착하시고 하지 말라고 하셨잖아요",
          "speaker_id": "1",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.17",
          "form": "남자의 삶이",
          "original_form": "남자의 삶이",
          "speaker_id": "2",
          "time": "20210905 07:22"
        },
        {
          "id": "MDRW2100031882.1.18",
          "form": "너무 힘들어요 ㅋㅋ",
          "original_form": "너무 힘들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:23"
        },
        {
          "id": "MDRW2100031882.1.19",
          "form": "저는 남자는 싫고 자식을 갖고 싶어요",
          "original_form": "저는 남자는 싫고 자식을 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210905 07:23"
        },
        {
          "id": "MDRW2100031882.1.20",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210905 07:23"
        },
        {
          "id": "MDRW2100031882.1.21",
          "form": "자식은",
          "original_form": "자식은",
          "speaker_id": "2",
          "time": "20210905 07:23"
        },
        {
          "id": "MDRW2100031882.1.22",
          "form": "아버지가 필요합니다",
          "original_form": "아버지가 필요합니다",
          "speaker_id": "2",
          "time": "20210905 07:23"
        },
        {
          "id": "MDRW2100031882.1.23",
          "form": "남자 볼 때도 저거 닮은 자식 나오면 싫겠다 이러면 정이 떨어지더라고요",
          "original_form": "남자 볼 때도 저거 닮은 자식 나오면 싫겠다 이러면 정이 떨어지더라고요",
          "speaker_id": "1",
          "time": "20210905 07:23"
        },
        {
          "id": "MDRW2100031882.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:23"
        },
        {
          "id": "MDRW2100031882.1.25",
          "form": "정우성이면요?",
          "original_form": "정우성이면요?",
          "speaker_id": "2",
          "time": "20210905 07:23"
        },
        {
          "id": "MDRW2100031882.1.26",
          "form": "저는 애가 못생기면 싫을거 같아요",
          "original_form": "저는 애가 못생기면 싫을거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100031882.1.27",
          "form": "사람마다 다를겁니다",
          "original_form": "사람마다 다를겁니다",
          "speaker_id": "2",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100031882.1.28",
          "form": "정말 좋은사람 있으면",
          "original_form": "정말 좋은사람 있으면",
          "speaker_id": "2",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100031882.1.29",
          "form": "좋은사람 많은뎅",
          "original_form": "좋은사람 많은뎅",
          "speaker_id": "2",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100031882.1.30",
          "form": "아 돌아가서",
          "original_form": "아 돌아가서",
          "speaker_id": "2",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100031882.1.31",
          "form": "그죠 사람의 문제인거 같아요",
          "original_form": "그죠 사람의 문제인거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100031882.1.32",
          "form": "음 저도 집안 일 다하고",
          "original_form": "음 저도 집안 일 다하고",
          "speaker_id": "2",
          "time": "20210905 07:24"
        },
        {
          "id": "MDRW2100031882.1.33",
          "form": "육아다바고",
          "original_form": "육아다바고",
          "speaker_id": "2",
          "time": "20210905 07:25"
        },
        {
          "id": "MDRW2100031882.1.34",
          "form": "다하고",
          "original_form": "다하고",
          "speaker_id": "2",
          "time": "20210905 07:25"
        }
      ]
    }
  ]
