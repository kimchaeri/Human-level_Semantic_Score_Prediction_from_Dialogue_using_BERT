      "utterance": [
        {
          "id": "MDRW2100005003.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100005003.1.2",
          "form": "안녕하세요! 근무 중이세요?",
          "original_form": "안녕하세요! 근무 중이세요?",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005003.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005003.1.4",
          "form": "전 집에서 소소하게 알바해요",
          "original_form": "전 집에서 소소하게 알바해요",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005003.1.5",
          "form": "근무하세요?",
          "original_form": "근무하세요?",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005003.1.6",
          "form": "ㅋㅋㅋㅋ잘 모르다보니ㅋㅋㅋ첫 질문이 근무 중이세요 였네요",
          "original_form": "ㅋㅋㅋㅋ잘 모르다보니ㅋㅋㅋ첫 질문이 근무 중이세요 였네요",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005003.1.7",
          "form": "네네 저는 회사 다니고 있어요 ㅎㅎ 재택 식으로 하는 알바 하시나요?",
          "original_form": "네네 저는 회사 다니고 있어요 ㅎㅎ 재택 식으로 하는 알바 하시나요?",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005003.1.8",
          "form": "ㅎㅎㅎ괜찮아요",
          "original_form": "ㅎㅎㅎ괜찮아요",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005003.1.9",
          "form": "소소하게 공모주 청약해서 좀벌어요",
          "original_form": "소소하게 공모주 청약해서 좀벌어요",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005003.1.10",
          "form": "알바하려다 코로나로 맘접고 집에서 쫌씩 하고 있어요",
          "original_form": "알바하려다 코로나로 맘접고 집에서 쫌씩 하고 있어요",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005003.1.11",
          "form": "회사 다니는게 요샌 젤좋아요",
          "original_form": "회사 다니는게 요샌 젤좋아요",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005003.1.12",
          "form": "오늘은 쉬셨죠?",
          "original_form": "오늘은 쉬셨죠?",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005003.1.13",
          "form": "네네 오늘은 몇개월만에 친구들이랑 식당에서 만나고 왔어요ㅋㅋ코로나 무섭..",
          "original_form": "네네 오늘은 몇개월만에 친구들이랑 식당에서 만나고 왔어요ㅋㅋ코로나 무섭..",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005003.1.14",
          "form": "오 공모주 청약!! 유투브나 이런거로도 많이 공부하고 청약하고 하시는거에요?",
          "original_form": "오 공모주 청약!! 유투브나 이런거로도 많이 공부하고 청약하고 하시는거에요?",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005003.1.15",
          "form": "맞아요 코로나 때문에 식당 좀 겁나요",
          "original_form": "맞아요 코로나 때문에 식당 좀 겁나요",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005003.1.16",
          "form": "네 지금도 유튜브 틀고 있어요",
          "original_form": "네 지금도 유튜브 틀고 있어요",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005003.1.17",
          "form": "안보고 투자하면 망해요",
          "original_form": "안보고 투자하면 망해요",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005003.1.18",
          "form": "주식 유투브요? ㅋㅋㅋ",
          "original_form": "주식 유투브요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005003.1.19",
          "form": "여기 망한 케이스 손듭니다....손...",
          "original_form": "여기 망한 케이스 손듭니다....손...",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005003.1.20",
          "form": "그래도 알바고돈벌이 인데 분석 ㅋㅋ해야해요",
          "original_form": "그래도 알바고돈벌이 인데 분석 ㅋㅋ해야해요",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005003.1.21",
          "form": "ㅋㅋㅋ아 해서 다른분들 공유해주시는? 건가요?",
          "original_form": "ㅋㅋㅋ아 해서 다른분들 공유해주시는? 건가요?",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005003.1.22",
          "form": "엥 왜요",
          "original_form": "엥 왜요",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005003.1.23",
          "form": "저희집 다같이 해요",
          "original_form": "저희집 다같이 해요",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005003.1.24",
          "form": "여동생도 회사에서 몰래해요",
          "original_form": "여동생도 회사에서 몰래해요",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005003.1.25",
          "form": "제그 저희집 알바 ㅋㅋ끌여 들였어요",
          "original_form": "제그 저희집 알바 ㅋㅋ끌여 들였어요",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005003.1.26",
          "form": "뭘 사셨는데 망하셨어요?",
          "original_form": "뭘 사셨는데 망하셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005003.1.27",
          "form": "혹시 클래프톤 하셨어요?",
          "original_form": "혹시 클래프톤 하셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005003.1.28",
          "form": "소소하더라도 돈버는 일이라 막사면 망해요",
          "original_form": "소소하더라도 돈버는 일이라 막사면 망해요",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005003.1.29",
          "form": "전 요걸로 벌이를 하는데 여동생은 회사다니며 용돈벌이해요",
          "original_form": "전 요걸로 벌이를 하는데 여동생은 회사다니며 용돈벌이해요",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005003.1.30",
          "form": "요샌 알바 안하면 여유 없다고 다들 하더라구요",
          "original_form": "요샌 알바 안하면 여유 없다고 다들 하더라구요",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005003.1.31",
          "form": "아 가족 알바를 자처하셨군요!! 사실 최근에 저는 아버지가 역할 해주고 계세요ㅋㅋ알바ㅋㅋㅋㅋ",
          "original_form": "아 가족 알바를 자처하셨군요!! 사실 최근에 저는 아버지가 역할 해주고 계세요ㅋㅋ알바ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005003.1.32",
          "form": "오 넘 좋네요 ㅎㅎ",
          "original_form": "오 넘 좋네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005003.1.33",
          "form": "누가 해주는게 젤좋아요",
          "original_form": "누가 해주는게 젤좋아요",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005003.1.34",
          "form": "당일에 어디 상장한대ㅋㅋㅋ10주 사봐!!",
          "original_form": "당일에 어디 상장한대ㅋㅋㅋ10주 사봐!!",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005003.1.35",
          "form": "전 부담돼요",
          "original_form": "전 부담돼요",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005003.1.36",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.37",
          "form": "ㅋㅋㅋ하시면 옴마랑 같이 폰켜서 우루루ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ하시면 옴마랑 같이 폰켜서 우루루ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.38",
          "form": "능력자세요 ㅎㅎ",
          "original_form": "능력자세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.39",
          "form": "저희집도 다하세요",
          "original_form": "저희집도 다하세요",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.40",
          "form": "ㅋㅋㅋ근데 다들 잘 모르면서ㅋㅋㅋㅋ유투브에서 들은걸로 하는거죠",
          "original_form": "ㅋㅋㅋ근데 다들 잘 모르면서ㅋㅋㅋㅋ유투브에서 들은걸로 하는거죠",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋ그래서ㅋㅋㅋ아빠 말듣고 성공하면 치킨을 접대하고ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그래서ㅋㅋㅋ아빠 말듣고 성공하면 치킨을 접대하고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.42",
          "form": "엄마 아빠 요새 수익좋아서 막 용돈주세요",
          "original_form": "엄마 아빠 요새 수익좋아서 막 용돈주세요",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.43",
          "form": "오좋네요",
          "original_form": "오좋네요",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.44",
          "form": "그러고 있어요 소소하자먼 도움은 확실히 되는거 같아요",
          "original_form": "그러고 있어요 소소하자먼 도움은 확실히 되는거 같아요",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.45",
          "form": "오 용돈! name1님도 능력잔데요?!",
          "original_form": "오 용돈! &name1&님도 능력잔데요?!",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005003.1.46",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005003.1.47",
          "form": "쏠쏠해요",
          "original_form": "쏠쏠해요",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005003.1.48",
          "form": "크래프톤운 무서워서 안들어갔습니다",
          "original_form": "크래프톤운 무서워서 안들어갔습니다",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005003.1.49",
          "form": "아빠가 알려주시면서 이건 100프로다 이건 애매하다",
          "original_form": "아빠가 알려주시면서 이건 100프로다 이건 애매하다",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005003.1.50",
          "form": "말씀하시면 저는 100프로라는 것만..ㅋㅋㅋㅋ",
          "original_form": "말씀하시면 저는 100프로라는 것만..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005003.1.51",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005003.1.52",
          "form": "맞아요 그게 답이죠",
          "original_form": "맞아요 그게 답이죠",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005003.1.53",
          "form": "제친구는 클래프톤 확 떨어질때 사서 올랐을때 팔아서 좀벌었더라구요",
          "original_form": "제친구는 클래프톤 확 떨어질때 사서 올랐을때 팔아서 좀벌었더라구요",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005003.1.54",
          "form": "전 겁이 많아서 그렇게는 안했어요 ㅎㅎ",
          "original_form": "전 겁이 많아서 그렇게는 안했어요 ㅎㅎ",
