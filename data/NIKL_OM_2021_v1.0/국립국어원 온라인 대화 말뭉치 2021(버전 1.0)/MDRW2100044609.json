      "utterance": [
        {
          "id": "MDRW2100044609.1.1",
          "form": "고기가 들어가나요?",
          "original_form": "고기가 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044609.1.2",
          "form": "아니요 고기는 안 들어갑니다!",
          "original_form": "아니요 고기는 안 들어갑니다!",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044609.1.3",
          "form": "후후후",
          "original_form": "후후후",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044609.1.4",
          "form": "안 맵고 야채 들어가고 국물 없고 토마토가 들어가요",
          "original_form": "안 맵고 야채 들어가고 국물 없고 토마토가 들어가요",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044609.1.5",
          "form": "양식인가요?",
          "original_form": "양식인가요?",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044609.1.6",
          "form": "아니요 중식이라는데 모르는 분들이 많을 것 같아요",
          "original_form": "아니요 중식이라는데 모르는 분들이 많을 것 같아요",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044609.1.7",
          "form": "저도 아까 다른 분한테 중식인 거 처음 들었어요",
          "original_form": "저도 아까 다른 분한테 중식인 거 처음 들었어요",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044609.1.8",
          "form": "헉 ㅋㅋㅋㅋ 중식에 토마토가 들어간다구요?",
          "original_form": "헉 ㅋㅋㅋㅋ 중식에 토마토가 들어간다구요?",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044609.1.9",
          "form": "고기는 들어가나요?",
          "original_form": "고기는 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044609.1.10",
          "form": "아니요 토마토 안 들어가요!",
          "original_form": "아니요 토마토 안 들어가요!",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044609.1.11",
          "form": "생긴 것만 보면 중식이 아닐 것 같이 생긴 음식이예요ㅋㅋㅋㅋ",
          "original_form": "생긴 것만 보면 중식이 아닐 것 같이 생긴 음식이예요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044609.1.12",
          "form": "양식같이 생겼어요",
          "original_form": "양식같이 생겼어요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044609.1.13",
          "form": "토마토 안들어가요??",
          "original_form": "토마토 안들어가요??",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044609.1.14",
          "form": "아이고 잘못 말했어요",
          "original_form": "아이고 잘못 말했어요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044609.1.15",
          "form": "고기 안 들어가요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "고기 안 들어가요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044609.1.16",
          "form": "기름에 볶나요?",
          "original_form": "기름에 볶나요?",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044609.1.17",
          "form": "네! 볶음요리예요 볶볶",
          "original_form": "네! 볶음요리예요 볶볶",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044609.1.18",
          "form": "안맵고 계란들어가는 중식은 토마토 계란볶음밖에 생각 안나요 ㅋㅋㅋㅋㅋ ㅠ",
          "original_form": "안맵고 계란들어가는 중식은 토마토 계란볶음밖에 생각 안나요 ㅋㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044609.1.19",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044609.1.20",
          "form": "정답 정답 정답",
          "original_form": "정답 정답 정답",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044609.1.21",
          "form": "뿌이뿌이뿌이~~~",
          "original_form": "뿌이뿌이뿌이~~~",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044609.1.22",
          "form": "앗 쉽게 먹는 요리는 아닌데",
          "original_form": "앗 쉽게 먹는 요리는 아닌데",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044609.1.23",
          "form": "집에서 요리 자주하시나봐요 ㅋㅋㅋㅋ",
          "original_form": "집에서 요리 자주하시나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044609.1.24",
          "form": "ㅌㅋㅋㅋㅋ토마토 사두고 안 먹었더니 쭈글해져서",
          "original_form": "ㅌㅋㅋㅋㅋ토마토 사두고 안 먹었더니 쭈글해져서",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044609.1.25",
          "form": "짬처리로 맨들어 먹었답니다",
          "original_form": "짬처리로 맨들어 먹었답니다",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044609.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ 그럼 이제 제 점심 맞춰보실래요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그럼 이제 제 점심 맞춰보실래요?",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044609.1.27",
          "form": "좋아요~!",
          "original_form": "좋아요~!",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044609.1.28",
          "form": "구운 음식인가요?",
          "original_form": "구운 음식인가요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044609.1.29",
          "form": "아뇨 굽지 않았습니다",
          "original_form": "아뇨 굽지 않았습니다",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044609.1.30",
          "form": "쌀이 들어가나요!",
          "original_form": "쌀이 들어가나요!",
