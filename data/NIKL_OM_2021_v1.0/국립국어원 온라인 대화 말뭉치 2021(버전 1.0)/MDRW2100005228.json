      "utterance": [
        {
          "id": "MDRW2100005228.1.1",
          "form": "name2님 동호회 활동 하는 것 있으셔요?",
          "original_form": "&name2&님 동호회 활동 하는 것 있으셔요?",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005228.1.2",
          "form": "요샌 코로나로 못해요",
          "original_form": "요샌 코로나로 못해요",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005228.1.3",
          "form": "코로나 시작과 함께 끝났네요",
          "original_form": "코로나 시작과 함께 끝났네요",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005228.1.4",
          "form": "뭐하세요?",
          "original_form": "뭐하세요?",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005228.1.5",
          "form": "전 예전에 타로 배우고 동화책 구연 과 도서관 봉사 했어요",
          "original_form": "전 예전에 타로 배우고 동화책 구연 과 도서관 봉사 했어요",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005228.1.6",
          "form": "오오 타로와 도서관!!",
          "original_form": "오오 타로와 도서관!!",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005228.1.7",
          "form": "무슨 활동 하세요?",
          "original_form": "무슨 활동 하세요?",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005228.1.8",
          "form": "다 제가 관심있는 분야네요",
          "original_form": "다 제가 관심있는 분야네요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005228.1.9",
          "form": "수료증도 타로 땄는데 다까먹었어요 ㅜㅜ",
          "original_form": "수료증도 타로 땄는데 다까먹었어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005228.1.10",
          "form": "오 그러세요",
          "original_form": "오 그러세요",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005228.1.11",
          "form": "전 코로나 전에 스윙댄스 동호회 잠깐 했는데",
          "original_form": "전 코로나 전에 스윙댄스 동호회 잠깐 했는데",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005228.1.12",
          "form": "저 일년넘게 안하니 백지네여",
          "original_form": "저 일년넘게 안하니 백지네여",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005228.1.13",
          "form": "저도 역시 코로나 이후로는...",
          "original_form": "저도 역시 코로나 이후로는...",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005228.1.14",
          "form": "다시 공부해야겠어요",
          "original_form": "다시 공부해야겠어요",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005228.1.15",
          "form": "세상에 제가 댄스가 젤 배우고 싶어요",
          "original_form": "세상에 제가 댄스가 젤 배우고 싶어요",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005228.1.16",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005228.1.17",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005228.1.18",
          "form": "저희 서로 부러워할만한 동호회 활동을 했었네요",
          "original_form": "저희 서로 부러워할만한 동호회 활동을 했었네요",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005228.1.19",
          "form": "네 저 몸치예요 ㅜㅜ",
          "original_form": "네 저 몸치예요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005228.1.20",
          "form": "저도 몸치예요",
          "original_form": "저도 몸치예요",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005228.1.21",
          "form": "네 그러게요",
          "original_form": "네 그러게요",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005228.1.22",
          "form": "어릴때 나이트 못간게 한이네요 ㅋㅋ",
          "original_form": "어릴때 나이트 못간게 한이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005228.1.23",
          "form": "근데 동호회서 배우고 같이 하니까 재미있더라고요",
          "original_form": "근데 동호회서 배우고 같이 하니까 재미있더라고요",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005228.1.24",
          "form": "저 전혀 못해요",
          "original_form": "저 전혀 못해요",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005228.1.25",
          "form": "ㅋㅋㅋ코로나 끝나면 나이트는 가보실 수 있을텐데",
          "original_form": "ㅋㅋㅋ코로나 끝나면 나이트는 가보실 수 있을텐데",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005228.1.26",
          "form": "뭐든 같이가 중요해요",
          "original_form": "뭐든 같이가 중요해요",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005228.1.27",
          "form": "맞아요 같이 하면 시너지 효과가!!",
          "original_form": "맞아요 같이 하면 시너지 효과가!!",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005228.1.28",
          "form": "아 저 늙어서 못가지 싶네요",
          "original_form": "아 저 늙어서 못가지 싶네요",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005228.1.29",
          "form": "뭔가 서로 뿜뿜하고 좋은데 같이 타로도 보고해야 늘고",
          "original_form": "뭔가 서로 뿜뿜하고 좋은데 같이 타로도 보고해야 늘고",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005228.1.30",
          "form": "춤도 같이해야 늘고 신나죠",
          "original_form": "춤도 같이해야 늘고 신나죠",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005228.1.31",
          "form": "그쵸 동호회 하는 사람들끼리 으쌰으쌰하고",
          "original_form": "그쵸 동호회 하는 사람들끼리 으쌰으쌰하고",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005228.1.32",
          "form": "서로 의지돼요",
          "original_form": "서로 의지돼요",
          "speaker_id": "2",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005228.1.33",
          "form": "직장이나 가족 말고 새로운 사람들도 만나고요",
          "original_form": "직장이나 가족 말고 새로운 사람들도 만나고요",
          "speaker_id": "1",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005228.1.34",
          "form": "맞아요 새로운 인연이 만나고싶네요",
          "original_form": "맞아요 새로운 인연이 만나고싶네요",
          "speaker_id": "2",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005228.1.35",
          "form": "네 코로나 때문에 외부활동도 줄어드니",
          "original_form": "네 코로나 때문에 외부활동도 줄어드니",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005228.1.36",
          "form": "새로운 사람 만나기가 더 어려워졌어요",
          "original_form": "새로운 사람 만나기가 더 어려워졌어요",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005228.1.37",
          "form": "맞아요 이사오니 전에 사람도 못보고 동회도 폐쇄되고 ㅜㅜ",
          "original_form": "맞아요 이사오니 전에 사람도 못보고 동회도 폐쇄되고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005228.1.38",
          "form": "단절이네요",
          "original_form": "단절이네요",
          "speaker_id": "2",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005228.1.39",
          "form": "아 이사하셨구나",
          "original_form": "아 이사하셨구나",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005228.1.40",
          "form": "다시 맘껏 모임도 동회회도 다니고싶어요",
          "original_form": "다시 맘껏 모임도 동회회도 다니고싶어요",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005228.1.41",
          "form": "네 이사오니 코로나 터져서",
          "original_form": "네 이사오니 코로나 터져서",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005228.1.42",
          "form": "아이고 하필 타이밍이",
          "original_form": "아이고 하필 타이밍이",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005228.1.43",
          "form": "도서관 봉사 신청한게 아직 연락이 없어요 ㅋㅌ",
          "original_form": "도서관 봉사 신청한게 아직 연락이 없어요 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005228.1.44",
          "form": "요즘 도서관들도 다 문닫거나 비대면 대출하더라고요",
          "original_form": "요즘 도서관들도 다 문닫거나 비대면 대출하더라고요",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005228.1.45",
          "form": "학교 구연동화ㅜㅜ중단요",
          "original_form": "학교 구연동화ㅜㅜ중단요",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005228.1.46",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005228.1.47",
          "form": "아 그쵸 학교에 가는 건 더더욱...",
          "original_form": "아 그쵸 학교에 가는 건 더더욱...",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005228.1.48",
          "form": "2년 다되도록 연락이 ㅜㅜ",
          "original_form": "2년 다되도록 연락이 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005228.1.49",
          "form": "얼른 다시 동호회/봉사를 활발히 할 수 있는 날이 오면 좋겠네요!!",
