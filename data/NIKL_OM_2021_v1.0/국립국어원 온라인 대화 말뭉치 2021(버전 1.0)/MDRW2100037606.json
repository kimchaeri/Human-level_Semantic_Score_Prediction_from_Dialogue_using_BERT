      "utterance": [
        {
          "id": "MDRW2100037606.1.1",
          "form": "대신에 겨울 난방비는 안든 답니다",
          "original_form": "대신에 겨울 난방비는 안든 답니다",
          "speaker_id": "2",
          "time": "20210901 13:19"
        },
        {
          "id": "MDRW2100037606.1.2",
          "form": "겨울에도 난 덥거든요ㅋㅋㅋ",
          "original_form": "겨울에도 난 덥거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100037606.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그래",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그래",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100037606.1.4",
          "form": "늘 땀흘리지",
          "original_form": "늘 땀흘리지",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100037606.1.5",
          "form": "우리집에 오면 맨날 덥다고",
          "original_form": "우리집에 오면 맨날 덥다고",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100037606.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 니오면 창문열고 ㅋㅋㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 니오면 창문열고 ㅋㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100037606.1.7",
          "form": "살이 쪄서 더한 거 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "살이 쪄서 더한 거 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100037606.1.8",
          "form": "평생 함께 할 더위 인듯ㅋㅋㅋㅋ",
          "original_form": "평생 함께 할 더위 인듯ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100037606.1.9",
          "form": "집안일은 다 해놓고 나왔어요??ㅋㅋㅋㅋ",
          "original_form": "집안일은 다 해놓고 나왔어요??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100037606.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 나 요새 엄마가 아침에 오시잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 나 요새 엄마가 아침에 오시잖아",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100037606.1.11",
          "form": "10시출근할때는 내가 했믄데",
          "original_form": "10시출근할때는 내가 했믄데",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100037606.1.12",
          "form": "9시까지 출근하면서부터",
          "original_form": "9시까지 출근하면서부터",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100037606.1.13",
          "form": "엄마가 일찍 오셔서 투 등원시키고",
          "original_form": "엄마가 일찍 오셔서 투 등원시키고",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100037606.1.14",
          "form": "집치워놓고 가심",
          "original_form": "집치워놓고 가심",
          "speaker_id": "1",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100037606.1.15",
          "form": "너무 좋네요 언니ㅋㅋㅋㅋㅋ",
          "original_form": "너무 좋네요 언니ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100037606.1.16",
          "form": "진짜 친정 엄마가 최고ㅋㅋㅋㅋ",
          "original_form": "진짜 친정 엄마가 최고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:32"
        },
        {
          "id": "MDRW2100037606.1.17",
          "form": "엄니 바쁘시겠어요ㅋㅋㅋ",
          "original_form": "엄니 바쁘시겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:32"
        },
        {
          "id": "MDRW2100037606.1.18",
          "form": "엉 요새는 name3이 학교가니 청소만 해놓고 바로 가시면 되는데",
          "original_form": "엉 요새는 &name3&이 학교가니 청소만 해놓고 바로 가시면 되는데",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100037606.1.19",
          "form": "방학때는 점심까지 줘야되니",
          "original_form": "방학때는 점심까지 줘야되니",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100037606.1.20",
          "form": "고생하셨지 뭐",
          "original_form": "고생하셨지 뭐",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100037606.1.21",
          "form": "엄니도 개학해서 해방됐구만요ㅋㅋㅋㅋ",
          "original_form": "엄니도 개학해서 해방됐구만요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:37"
        },
        {
          "id": "MDRW2100037606.1.22",
          "form": "엌ㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100037606.1.23",
          "form": "엄마도 지쳤음",
          "original_form": "엄마도 지쳤음",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037606.1.24",
          "form": "이제 엄마가 방학",
          "original_form": "이제 엄마가 방학",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037606.1.25",
          "form": "ㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037606.1.26",
          "form": "와따오늘 매장서 땀 한바가지 흘리고 왓디 기력이 없구만요",
          "original_form": "와따오늘 매장서 땀 한바가지 흘리고 왓디 기력이 없구만요",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037606.1.27",
          "form": "빨리 먹어야겠어요 ㅋㅋㅋㅋ",
          "original_form": "빨리 먹어야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037606.1.28",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㄱㅋㅋㅋㅋㄱ 파워냉방도 소용없는 땀샘이네",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㄱㅋㅋㅋㅋㄱ 파워냉방도 소용없는 땀샘이네",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037606.1.29",
          "form": "역시 ㅋㅋㅋㅋㅋ",
          "original_form": "역시 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037606.1.30",
          "form": "언니 혹시 name1이에요?",
          "original_form": "언니 혹시 &name1&이에요?",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037606.1.31",
          "form": "매너 좋은 사람ㅋㅋㅋ",
          "original_form": "매너 좋은 사람ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037606.1.32",
          "form": "어 맞다 맞다",
          "original_form": "어 맞다 맞다",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037606.1.33",
          "form": "신기한데",
          "original_form": "신기한데",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037606.1.34",
          "form": "니도 담소를 나눴구만",
          "original_form": "니도 담소를 나눴구만",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037606.1.35",
          "form": "ㅋㅋㅋㅋㅋ아욱기노 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아욱기노 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037606.1.36",
          "form": "저녁 뭐먹어요??",
          "original_form": "저녁 뭐먹어요??",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037606.1.37",
          "form": "삼계탕 생선",
          "original_form": "삼계탕 생선",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037606.1.38",
          "form": "벌써 육퇴 했겠네",
          "original_form": "벌써 육퇴 했겠네",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037606.1.39",
          "form": "난 이제 양치하러 간다",
          "original_form": "난 이제 양치하러 간다",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037606.1.40",
          "form": "부디 오늘은 9시 전에 끝나 길",
          "original_form": "부디 오늘은 9시 전에 끝나 길",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037606.1.41",
          "form": "깜빡 잠들었다 깼구먼요~",
          "original_form": "깜빡 잠들었다 깼구먼요~",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100037606.1.42",
          "form": "오늘은 육해공을 다 잡샀네여 ㅋㅋㅋ",
          "original_form": "오늘은 육해공을 다 잡샀네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100037606.1.43",
          "form": "ㅋㅋㅋㅋㅋ 잠들었나\r\nㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 잠들었나\r\nㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037606.1.44",
