      "utterance": [
        {
          "id": "MDRW2100042754.1.1",
          "form": "비 금세 그치면 좋겠는데",
          "original_form": "비 금세 그치면 좋겠는데",
          "speaker_id": "1",
          "time": "20210907 08:31"
        },
        {
          "id": "MDRW2100042754.1.2",
          "form": "내일까지 올 수도 있대요ㅜ",
          "original_form": "내일까지 올 수도 있대요ㅜ",
          "speaker_id": "1",
          "time": "20210907 08:31"
        },
        {
          "id": "MDRW2100042754.1.3",
          "form": "세상에나 오늘이 끝이 아니었다니ㅜ",
          "original_form": "세상에나 오늘이 끝이 아니었다니ㅜ",
          "speaker_id": "2",
          "time": "20210907 08:42"
        },
        {
          "id": "MDRW2100042754.1.4",
          "form": "이러다가 비그치면 확 추워지겠죠? ㅠㅠ",
          "original_form": "이러다가 비그치면 확 추워지겠죠? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:43"
        },
        {
          "id": "MDRW2100042754.1.5",
          "form": "그럴것 같아요",
          "original_form": "그럴것 같아요",
          "speaker_id": "1",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100042754.1.6",
          "form": "가을을 재촉하는 비",
          "original_form": "가을을 재촉하는 비",
          "speaker_id": "1",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100042754.1.7",
          "form": "그쵸 ㅋㅋㅋ 근데 너무신기해요 몇일사이에 이렇게 계절이 확 변하는게 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ 근데 너무신기해요 몇일사이에 이렇게 계절이 확 변하는게 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100042754.1.8",
          "form": "네 계절 바뀌는 게 신기하죠",
          "original_form": "네 계절 바뀌는 게 신기하죠",
          "speaker_id": "1",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042754.1.9",
          "form": "날이 추워지니 가디건 사고싶은데 어느정도 두께되는걸 사야하는지 고민되네요 ㅋㅋ",
          "original_form": "날이 추워지니 가디건 사고싶은데 어느정도 두께되는걸 사야하는지 고민되네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:49"
        },
        {
          "id": "MDRW2100042754.1.10",
          "form": "그쵸 가디건도 두께가 다르니까",
          "original_form": "그쵸 가디건도 두께가 다르니까",
          "speaker_id": "1",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042754.1.11",
          "form": "전 지금 살짝 비칠 정도의 두께 가디건 입어요",
          "original_form": "전 지금 살짝 비칠 정도의 두께 가디건 입어요",
          "speaker_id": "1",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042754.1.12",
          "form": "오 지금은 그게 딱이겠어요 ㅋㅋㅋ",
          "original_form": "오 지금은 그게 딱이겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042754.1.13",
          "form": "오늘은 추우니까 살짝 도톰한거 사고싶은데 또 비그치면 덜 추울것같기도하고",
          "original_form": "오늘은 추우니까 살짝 도톰한거 사고싶은데 또 비그치면 덜 추울것같기도하고",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042754.1.14",
          "form": "이제 점점 추워질테니까요",
          "original_form": "이제 점점 추워질테니까요",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042754.1.15",
          "form": "좋아요 ㅋㅋ그럼 두꺼운 옷으로 사야겠네요",
          "original_form": "좋아요 ㅋㅋ그럼 두꺼운 옷으로 사야겠네요",
          "speaker_id": "2",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100042754.1.16",
          "form": "비가 그치는 듯하더니",
          "original_form": "비가 그치는 듯하더니",
          "speaker_id": "1",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100042754.1.17",
          "form": "또 많이 내리네요",
          "original_form": "또 많이 내리네요",
          "speaker_id": "1",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100042754.1.18",
          "form": "내일 야외행사 있는데...",
          "original_form": "내일 야외행사 있는데...",
          "speaker_id": "1",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100042754.1.19",
          "form": "헐 그래도 오늘밤사이에 그치지않을까요? ㅠㅠㅠㅠㅠ",
          "original_form": "헐 그래도 오늘밤사이에 그치지않을까요? ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100042754.1.20",
          "form": "예보엔 뭐래요? ㅠㅠㅠ",
          "original_form": "예보엔 뭐래요? ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100042754.1.21",
          "form": "안온다고는 했는데",
          "original_form": "안온다고는 했는데",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100042754.1.22",
          "form": "오늘도 오전까지만 비온다고 예보에서 그랬거든요",
          "original_form": "오늘도 오전까지만 비온다고 예보에서 그랬거든요",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100042754.1.23",
          "form": "헐랭 ㅠ 맞아요 여기도 그친다더니 많이는아니니만 그래도 꽤오고있어요",
          "original_form": "헐랭 ㅠ 맞아요 여기도 그친다더니 많이는아니니만 그래도 꽤오고있어요",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100042754.1.24",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100042754.1.25",
          "form": "비 그치는 것 같더니",
          "original_form": "비 그치는 것 같더니",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100042754.1.26",
          "form": "왜 계속 오지...",
          "original_form": "왜 계속 오지...",
          "speaker_id": "1",
          "time": "20210907 18:37"
        }
      ]
    }
