      "utterance": [
        {
          "id": "MDRW2100047591.1.1",
          "form": "반려동물 길러보셨나요",
          "original_form": "반려동물 길러보셨나요",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047591.1.2",
          "form": "청거북이길러봤어요 name2님은요?",
          "original_form": "청거북이길러봤어요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047591.1.3",
          "form": "저는 경험이 없네요 얼마나 기르신건가요",
          "original_form": "저는 경험이 없네요 얼마나 기르신건가요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100047591.1.4",
          "form": "3-4년기른거같아요",
          "original_form": "3-4년기른거같아요",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100047591.1.5",
          "form": "와 꽤 오래 기르셨네요",
          "original_form": "와 꽤 오래 기르셨네요",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100047591.1.6",
          "form": "흠 생각해보니 최소 6-7년은 기른거같아요",
          "original_form": "흠 생각해보니 최소 6-7년은 기른거같아요",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100047591.1.7",
          "form": "와..그럼 마지막은 어떻게 됐나요? 거북이는 수명도 기니까..",
          "original_form": "와..그럼 마지막은 어떻게 됐나요? 거북이는 수명도 기니까..",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100047591.1.8",
          "form": "저 고3때죽었어요 ㅠㅠ",
          "original_form": "저 고3때죽었어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047591.1.9",
          "form": "어머 죽었군요 ㅠ 불로장생 느낌이었는데...",
          "original_form": "어머 죽었군요 ㅠ 불로장생 느낌이었는데...",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047591.1.10",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047591.1.11",
          "form": "먹이는 뭘 먹었나요",
          "original_form": "먹이는 뭘 먹었나요",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100047591.1.12",
          "form": "사료랑 생돼지고기요",
          "original_form": "사료랑 생돼지고기요",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100047591.1.13",
          "form": "어머 생돼지고기 신기하네요",
          "original_form": "어머 생돼지고기 신기하네요",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100047591.1.14",
          "form": "네 다른 사람들도 신기하다 하더라구요",
          "original_form": "네 다른 사람들도 신기하다 하더라구요",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100047591.1.15",
          "form": "잘먹어요 야생의본능으로요",
          "original_form": "잘먹어요 야생의본능으로요",
          "speaker_id": "1",
          "time": "20210826 21:44"
        }
      ]
