      "utterance": [
        {
          "id": "MDRW2100032829.1.1",
          "form": "리유저블컵도 있구나",
          "original_form": "리유저블컵도 있구나",
          "speaker_id": "2",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100032829.1.2",
          "form": "요새 뭐가 많네요 ㅡㅎ",
          "original_form": "요새 뭐가 많네요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100032829.1.3",
          "form": "네 제주는 종이컵 없는 매장이 생겼어요",
          "original_form": "네 제주는 종이컵 없는 매장이 생겼어요",
          "speaker_id": "1",
          "time": "20210906 00:34"
        },
        {
          "id": "MDRW2100032829.1.4",
          "form": "공항이나 스벅에서 리유저블컵 반납해요",
          "original_form": "공항이나 스벅에서 리유저블컵 반납해요",
          "speaker_id": "1",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100032829.1.5",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.6",
          "form": "전 잘몰라요ㅎㅎ",
          "original_form": "전 잘몰라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.7",
          "form": "보증금 500원",
          "original_form": "보증금 500원",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.8",
          "form": "우왐ㅠ",
          "original_form": "우왐ㅠ",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.9",
          "form": "이제 그렇게 되군요ㅠ",
          "original_form": "이제 그렇게 되군요ㅠ",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.10",
          "form": "전 마케팅이도 무조건 찬성이요",
          "original_form": "전 마케팅이도 무조건 찬성이요",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.11",
          "form": "그게 지금 계속하는거죠",
          "original_form": "그게 지금 계속하는거죠",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.12",
          "form": "저도 찬성이요",
          "original_form": "저도 찬성이요",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.13",
          "form": "지구는 ㅠㅜ 죽어가고 있다규요",
          "original_form": "지구는 ㅠㅜ 죽어가고 있다규요",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.15",
          "form": "그럼 스벅은?",
          "original_form": "그럼 스벅은?",
          "speaker_id": "2",
          "time": "20210906 00:36"
        },
        {
          "id": "MDRW2100032829.1.16",
          "form": "저 아내때문에 스벅많이가는데",
          "original_form": "저 아내때문에 스벅많이가는데",
          "speaker_id": "2",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032829.1.17",
          "form": "최근에 얼탱이 없는거 있었는데",
          "original_form": "최근에 얼탱이 없는거 있었는데",
          "speaker_id": "1",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032829.1.18",
          "form": "텀블러가져갔는데 코로나가",
          "original_form": "텀블러가져갔는데 코로나가",
          "speaker_id": "1",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032829.1.19",
          "form": "코로나라서",
          "original_form": "코로나라서",
          "speaker_id": "1",
          "time": "20210906 00:37"
        },
        {
          "id": "MDRW2100032829.1.20",
          "form": "텀블러 할인은 해주지만 종이컵쓰라고 한적 있었어요",
          "original_form": "텀블러 할인은 해주지만 종이컵쓰라고 한적 있었어요",
          "speaker_id": "1",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032829.1.21",
          "form": "아 그런것도있네요ㅠ",
          "original_form": "아 그런것도있네요ㅠ",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032829.1.22",
          "form": "뭔 종이컵을쓰래요",
          "original_form": "뭔 종이컵을쓰래요",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032829.1.23",
          "form": "텀블러가 좋은거지",
          "original_form": "텀블러가 좋은거지",
          "speaker_id": "2",
          "time": "20210906 00:38"
        },
        {
          "id": "MDRW2100032829.1.24",
          "form": "알바가 귀찮았는지 ㅠㅜ",
          "original_form": "알바가 귀찮았는지 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210906 00:39"
        }
