      "utterance": [
        {
          "id": "MDRW2100016003.1.1",
          "form": "집안일 분담 어떻게 하시나요",
          "original_form": "집안일 분담 어떻게 하시나요",
          "speaker_id": "1",
          "time": "20210826 07:25"
        },
        {
          "id": "MDRW2100016003.1.2",
          "form": "퇴근이 늦어서 평일에는 거의 손 놓고 있는 것 같아요",
          "original_form": "퇴근이 늦어서 평일에는 거의 손 놓고 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 07:26"
        },
        {
          "id": "MDRW2100016003.1.3",
          "form": "그렇죠 평일엔 힘들죠 ㅠ",
          "original_form": "그렇죠 평일엔 힘들죠 ㅠ",
          "speaker_id": "1",
          "time": "20210826 07:26"
        },
        {
          "id": "MDRW2100016003.1.4",
          "form": "주말에는 세탁 부엌일 보조 설거지 이런 일 정도 하고요",
          "original_form": "주말에는 세탁 부엌일 보조 설거지 이런 일 정도 하고요",
          "speaker_id": "2",
          "time": "20210826 07:26"
        },
        {
          "id": "MDRW2100016003.1.5",
          "form": "name1님은 어떤가요",
          "original_form": "&name1&님은 어떤가요",
          "speaker_id": "2",
          "time": "20210826 07:27"
        },
        {
          "id": "MDRW2100016003.1.6",
          "form": "아 그러시군여 저는 쉬는중이라 제가 많이 하고 있어요",
          "original_form": "아 그러시군여 저는 쉬는중이라 제가 많이 하고 있어요",
          "speaker_id": "1",
          "time": "20210826 07:27"
        },
        {
          "id": "MDRW2100016003.1.7",
          "form": "집안일 해도해도 끝이 없던데 힘드시겠어요",
          "original_form": "집안일 해도해도 끝이 없던데 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100016003.1.8",
          "form": "맞아요 바쁠땐 몰랐는데 집에 있으니 할 일이 더 보이더라구요",
          "original_form": "맞아요 바쁠땐 몰랐는데 집에 있으니 할 일이 더 보이더라구요",
          "speaker_id": "1",
          "time": "20210826 07:31"
        },
        {
          "id": "MDRW2100016003.1.9",
          "form": "집안일 중 뭐가 가장 힘드세요?",
          "original_form": "집안일 중 뭐가 가장 힘드세요?",
          "speaker_id": "2",
          "time": "20210826 07:32"
        },
        {
          "id": "MDRW2100016003.1.10",
          "form": "음.. 요리요? ㅋㅋ 저는 정리가 차라리 편하더라구요",
          "original_form": "음.. 요리요? ㅋㅋ 저는 정리가 차라리 편하더라구요",
          "speaker_id": "1",
          "time": "20210826 07:35"
        },
        {
          "id": "MDRW2100016003.1.11",
          "form": "저랑 반대네요ㅎ",
          "original_form": "저랑 반대네요ㅎ",
          "speaker_id": "2",
          "time": "20210826 07:41"
        },
        {
          "id": "MDRW2100016003.1.12",
          "form": "전 정리는 쥐약이에요",
          "original_form": "전 정리는 쥐약이에요",
          "speaker_id": "2",
          "time": "20210826 07:41"
        },
        {
          "id": "MDRW2100016003.1.13",
          "form": "꼼꼼한 편이신가봐요",
          "original_form": "꼼꼼한 편이신가봐요",
          "speaker_id": "2",
          "time": "20210826 07:42"
        },
        {
          "id": "MDRW2100016003.1.14",
          "form": "네 꼼꼼하니 집안일하기 더 피곤하더라구요 ㅠ",
          "original_form": "네 꼼꼼하니 집안일하기 더 피곤하더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 08:23"
        },
        {
          "id": "MDRW2100016003.1.15",
          "form": "그쵸ㅜ 다른 사람이 한 것도 성에 안 차고",
          "original_form": "그쵸ㅜ 다른 사람이 한 것도 성에 안 차고",
          "speaker_id": "2",
          "time": "20210826 08:24"
        },
        {
          "id": "MDRW2100016003.1.16",
          "form": "맞아요 남편이 해도 손이 다시 가더라구여~",
          "original_form": "맞아요 남편이 해도 손이 다시 가더라구여~",
          "speaker_id": "1",
          "time": "20210826 08:24"
        }
      ]
