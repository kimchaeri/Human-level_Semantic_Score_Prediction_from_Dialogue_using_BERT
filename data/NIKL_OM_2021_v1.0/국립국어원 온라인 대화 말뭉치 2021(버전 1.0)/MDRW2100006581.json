      "utterance": [
        {
          "id": "MDRW2100006581.1.1",
          "form": "어머님 옷같은거요?",
          "original_form": "어머님 옷같은거요?",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100006581.1.2",
          "form": "네 자주 사드리신다기에..",
          "original_form": "네 자주 사드리신다기에..",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100006581.1.3",
          "form": "아 신발가방은 백화점에서 사드리구요",
          "original_form": "아 신발가방은 백화점에서 사드리구요",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100006581.1.4",
          "form": "옷은 lf몰 좋아요",
          "original_form": "옷은 lf몰 좋아요",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100006581.1.5",
          "form": "와 플렉스네요 정말",
          "original_form": "와 플렉스네요 정말",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100006581.1.6",
          "form": "제가 집안일을 안하니까요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 집안일을 안하니까요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100006581.1.7",
          "form": "생필품은요? 어디서사세요?",
          "original_form": "생필품은요? 어디서사세요?",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100006581.1.8",
          "form": "저 온라인쇼핑 좋아해서 그때그때 세일하는거 눈여겨봅니다 제가 늦은밤에는 시간이 좀 여유롭거든요 ㅋㅋㅋ",
          "original_form": "저 온라인쇼핑 좋아해서 그때그때 세일하는거 눈여겨봅니다 제가 늦은밤에는 시간이 좀 여유롭거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100006581.1.9",
          "form": "그럼 지마켓 이런곳 이용하시는건가요?",
          "original_form": "그럼 지마켓 이런곳 이용하시는건가요?",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006581.1.10",
          "form": "네 지마켓 십일번가 쿠팡 다씁니다 ㅋㅋㅋ",
          "original_form": "네 지마켓 십일번가 쿠팡 다씁니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100006581.1.11",
          "form": "물품마다 더 싼곳이 달라요",
          "original_form": "물품마다 더 싼곳이 달라요",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100006581.1.12",
          "form": "그럼 이번 빅스마일도???",
          "original_form": "그럼 이번 빅스마일도???",
          "speaker_id": "1",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100006581.1.13",
          "form": "살거없던데 뭐사셨나요 ㅜㅜ",
          "original_form": "살거없던데 뭐사셨나요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100006581.1.14",
