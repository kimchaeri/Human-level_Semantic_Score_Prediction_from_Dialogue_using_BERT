      "utterance": [
        {
          "id": "MDRW2100043056.1.1",
          "form": "나 플스5",
          "original_form": "나 플스5",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043056.1.2",
          "form": "헐 플스...나도",
          "original_form": "헐 플스...나도",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043056.1.3",
          "form": "그 컨트롤기가 그렇게 신세계라던데",
          "original_form": "그 컨트롤기가 그렇게 신세계라던데",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043056.1.4",
          "form": "폭탄터지는거나 무게감 이런거 다느껴진다던데",
          "original_form": "폭탄터지는거나 무게감 이런거 다느껴진다던데",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043056.1.5",
          "form": "그니까....너무 가지고싶다",
          "original_form": "그니까....너무 가지고싶다",
          "speaker_id": "1",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043056.1.6",
          "form": "나는 소파 사고싶어",
          "original_form": "나는 소파 사고싶어",
          "speaker_id": "1",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043056.1.7",
          "form": "너네집 소파사면 딱이긴하겠다",
          "original_form": "너네집 소파사면 딱이긴하겠다",
          "speaker_id": "2",
          "time": "20210907 13:39"
        },
        {
          "id": "MDRW2100043056.1.8",
          "form": "손님와서 앉을데도없고",
          "original_form": "손님와서 앉을데도없고",
          "speaker_id": "1",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043056.1.9",
          "form": "좌식생활이 무릎에 안좋대서",
          "original_form": "좌식생활이 무릎에 안좋대서",
          "speaker_id": "1",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043056.1.10",
          "form": "그렇다고 하더라...",
          "original_form": "그렇다고 하더라...",
          "speaker_id": "2",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043056.1.11",
          "form": "좌식생활 안좋대",
          "original_form": "좌식생활 안좋대",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043056.1.12",
          "form": "얼른사자",
          "original_form": "얼른사자",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043056.1.13",
          "form": "나도 그러고 싶은데",
          "original_form": "나도 그러고 싶은데",
          "speaker_id": "1",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100043056.1.14",
          "form": "나중에 이사할때 짐일까봐...",
          "original_form": "나중에 이사할때 짐일까봐...",
          "speaker_id": "1",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100043056.1.15",
          "form": "아 맞네.....",
          "original_form": "아 맞네.....",
          "speaker_id": "2",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100043056.1.16",
          "form": "가구는 그런게 걱정이네",
          "original_form": "가구는 그런게 걱정이네",
          "speaker_id": "2",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043056.1.17",
          "form": "그래서 작은것만 사게되나봐",
          "original_form": "그래서 작은것만 사게되나봐",
          "speaker_id": "1",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043056.1.18",
          "form": "중고거래 알아보는것도 괜찮지않을까?",
          "original_form": "중고거래 알아보는것도 괜찮지않을까?",
          "speaker_id": "2",
          "time": "20210907 13:44"
        },
        {
          "id": "MDRW2100043056.1.19",
          "form": "남이쓰던 가구 쓴다는게 조금 찝찝해서..",
          "original_form": "남이쓰던 가구 쓴다는게 조금 찝찝해서..",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043056.1.20",
