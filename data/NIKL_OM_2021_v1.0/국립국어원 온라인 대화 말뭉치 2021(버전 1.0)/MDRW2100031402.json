      "utterance": [
        {
          "id": "MDRW2100031402.1.1",
          "form": "종교 있으세요??",
          "original_form": "종교 있으세요??",
          "speaker_id": "1",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100031402.1.2",
          "form": "저 무교!",
          "original_form": "저 무교!",
          "speaker_id": "2",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100031402.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100031402.1.4",
          "form": "저도 무교랍니닼ㅋㅋ",
          "original_form": "저도 무교랍니닼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031402.1.5",
          "form": "종교있으세요 물어보는 분은 거의 무교 같은데요",
          "original_form": "종교있으세요 물어보는 분은 거의 무교 같은데요",
          "speaker_id": "1",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031402.1.6",
          "form": "왜냐하면 종교 있는 분들은 단어가 달라요",
          "original_form": "왜냐하면 종교 있는 분들은 단어가 달라요",
          "speaker_id": "1",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100031402.1.7",
          "form": "ㅋㅋ 저 지금까지 종교있는분 한번도 못버ㆍㅆ어요",
          "original_form": "ㅋㅋ 저 지금까지 종교있는분 한번도 못버ㆍㅆ어요",
          "speaker_id": "2",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100031402.1.8",
          "form": "궁금하네요!!!",
          "original_form": "궁금하네요!!!",
          "speaker_id": "2",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100031402.1.9",
          "form": "ㅋㅋㅋ 암호같기도",
          "original_form": "ㅋㅋㅋ 암호같기도",
          "speaker_id": "1",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100031402.1.10",
          "form": "그들만 아는 그런 단어가 있었는데",
          "original_form": "그들만 아는 그런 단어가 있었는데",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100031402.1.11",
          "form": "지금은 생각이 안나네요",
          "original_form": "지금은 생각이 안나네요",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100031402.1.12",
          "form": "보통 먼저 밝히시더라고요 ㅋㅋ",
          "original_form": "보통 먼저 밝히시더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100031402.1.13",
          "form": "자매님..? ㅋㅋ 진짜 그분들은 현실에서도 남다르긴해요!",
          "original_form": "자매님..? ㅋㅋ 진짜 그분들은 현실에서도 남다르긴해요!",
          "speaker_id": "2",
          "time": "20210904 21:47"
        },
        {
          "id": "MDRW2100031402.1.14",
          "form": "뭔가 선량한데 핀트가 좀안맞는..",
          "original_form": "뭔가 선량한데 핀트가 좀안맞는..",
          "speaker_id": "2",
          "time": "20210904 21:47"
        },
        {
          "id": "MDRW2100031402.1.15",
          "form": "ㅋㅋㅋㅋ 심한분들 좀 있어요",
          "original_form": "ㅋㅋㅋㅋ 심한분들 좀 있어요",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031402.1.16",
          "form": "저는 교회인들 중에 그런분 많이 봤어요",
          "original_form": "저는 교회인들 중에 그런분 많이 봤어요",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031402.1.17",
          "form": "바깥에서 전단지 나눠주시는 분들",
          "original_form": "바깥에서 전단지 나눠주시는 분들",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031402.1.18",
          "form": "교회 이상한 곳인줄 알았거든요?",
          "original_form": "교회 이상한 곳인줄 알았거든요?",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031402.1.19",
          "form": "근데 전도사라고 그 목사님 되기 전에 전단지 배포도 해야되는 거래요",
          "original_form": "근데 전도사라고 그 목사님 되기 전에 전단지 배포도 해야되는 거래요",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031402.1.20",
          "form": "아 진짜 그 퇴약볕에 누가 시킨것도 아니고\n 믿음 하나로?",
          "original_form": "아 진짜 그 퇴약볕에 누가 시킨것도 아니고\n 믿음 하나로?",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031402.1.21",
          "form": "아 목적이 있군요!",
          "original_form": "아 목적이 있군요!",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031402.1.22",
          "form": "옷도 항상 긴치마에 스타킹 구두",
          "original_form": "옷도 항상 긴치마에 스타킹 구두",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100031402.1.23",
          "form": "진짜 이상한 곳도 가끔 있을테지만",
          "original_form": "진짜 이상한 곳도 가끔 있을테지만",
          "speaker_id": "1",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100031402.1.24",
          "form": "교회 전단지는 나눠주는 과정이 있다고 해서",
          "original_form": "교회 전단지는 나눠주는 과정이 있다고 해서",
          "speaker_id": "1",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100031402.1.25",
          "form": "신기했어요 ㅋㅋㅋㅋ",
          "original_form": "신기했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100031402.1.26",
          "form": "ㅋㅋ 목사가 되면 모든 죄를 용서받고ㅋㅋ 밀양 이었나... 진짜 노답종교라...개인적으로ㅜ",
          "original_form": "ㅋㅋ 목사가 되면 모든 죄를 용서받고ㅋㅋ 밀양 이었나... 진짜 노답종교라...개인적으로ㅜ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031402.1.27",
          "form": "아 그런것도 있어요?",
          "original_form": "아 그런것도 있어요?",
          "speaker_id": "1",
          "time": "20210904 21:56"
        },
        {
          "id": "MDRW2100031402.1.28",
          "form": "진짜 노답도 많네여",
          "original_form": "진짜 노답도 많네여",
          "speaker_id": "1",
          "time": "20210904 21:56"
        },
        {
          "id": "MDRW2100031402.1.29",
          "form": "ㅋㅋ서세원이랑 삼풍백화점 책임자 다 목사ㅋㅋ",
          "original_form": "ㅋㅋ서세원이랑 삼풍백화점 책임자 다 목사ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100031402.1.30",
          "form": "쓰레기들이 세탁하러 많이 가더라구요...ㅜ",
          "original_form": "쓰레기들이 세탁하러 많이 가더라구요...ㅜ",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031402.1.31",
          "form": "서세원은 들었는데 삼풍백화점??",
          "original_form": "서세원은 들었는데 삼풍백화점??",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031402.1.32",
          "form": "와 미.틴.놈들이네여",
          "original_form": "와 미.틴.놈들이네여",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031402.1.33",
          "form": "ㅋㅋ 진짜 속편해요 죄짓고 그 종교만 믿음 용서받는다니ㅋㅋ",
          "original_form": "ㅋㅋ 진짜 속편해요 죄짓고 그 종교만 믿음 용서받는다니ㅋㅋ",
          "speaker_id": "2",
