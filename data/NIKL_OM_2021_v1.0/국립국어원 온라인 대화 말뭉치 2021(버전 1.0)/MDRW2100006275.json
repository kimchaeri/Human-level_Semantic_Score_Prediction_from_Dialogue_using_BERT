      "utterance": [
        {
          "id": "MDRW2100006275.1.1",
          "form": "학교가서 동아리 같은거 해보고 싶은건 없어요?",
          "original_form": "학교가서 동아리 같은거 해보고 싶은건 없어요?",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006275.1.2",
          "form": "지금은 글쓰기 동아리인데",
          "original_form": "지금은 글쓰기 동아리인데",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006275.1.3",
          "form": "꽤 재밌는 거 같아요",
          "original_form": "꽤 재밌는 거 같아요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006275.1.4",
          "form": "나중에도 계속 해보고싶네요",
          "original_form": "나중에도 계속 해보고싶네요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006275.1.5",
          "form": "오 글쓰고 서로 수정해주고 그런건가요?",
          "original_form": "오 글쓰고 서로 수정해주고 그런건가요?",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006275.1.6",
          "form": "네 그렇게 해서 문집 만들고 하는",
          "original_form": "네 그렇게 해서 문집 만들고 하는",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006275.1.7",
          "form": "오 문집 내면 나중에 돌아보기 좋겠는데요",
          "original_form": "오 문집 내면 나중에 돌아보기 좋겠는데요",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006275.1.8",
          "form": "고등학교때 담임샘이랑 한번 반문집? 비슷한거 낸 뒤로",
          "original_form": "고등학교때 담임샘이랑 한번 반문집? 비슷한거 낸 뒤로",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006275.1.9",
          "form": "그런건 많이 못해봤어욬ㅋㅋ",
          "original_form": "그런건 많이 못해봤어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006275.1.10",
          "form": "문집만들기 재밌더라고요",
          "original_form": "문집만들기 재밌더라고요",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006275.1.11",
          "form": "제가 글쓰기를 좋아해서ㅈ그런가",
          "original_form": "제가 글쓰기를 좋아해서ㅈ그런가",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006275.1.12",
          "form": "그 동안 많이 만들었어요?",
          "original_form": "그 동안 많이 만들었어요?",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006275.1.13",
          "form": "여기저기서 하면서",
          "original_form": "여기저기서 하면서",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006275.1.14",
          "form": "만든 문집이 네 권 정도 될거에요",
          "original_form": "만든 문집이 네 권 정도 될거에요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006275.1.15",
          "form": "오 뭐 운동이나 공연 동아니 이런건요?",
          "original_form": "오 뭐 운동이나 공연 동아니 이런건요?",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006275.1.16",
          "form": "헉 몸 쓰는 쪽으론 재능은 없어서...",
          "original_form": "헉 몸 쓰는 쪽으론 재능은 없어서...",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006275.1.17",
          "form": "공연동아리는 하면 재밌을거 같긴 하네요",
          "original_form": "공연동아리는 하면 재밌을거 같긴 하네요",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006275.1.18",
          "form": "근데 공연동아리 들어가도 연극을 하기보단 대본을 쓰는 쪽일거 같아요 ㅋㅋㅋ",
          "original_form": "근데 공연동아리 들어가도 연극을 하기보단 대본을 쓰는 쪽일거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006275.1.19",
          "form": "오 명확하다ㅋㅋㅋ 저는 공연 같은거 해보고 싶은 마음에",
          "original_form": "오 명확하다ㅋㅋㅋ 저는 공연 같은거 해보고 싶은 마음에",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006275.1.20",
          "form": "밴드 클래식기타 춤 이런 동아리 들어갔었거든요",
          "original_form": "밴드 클래식기타 춤 이런 동아리 들어갔었거든요",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006275.1.21",
          "form": "뭔가 합 맞추는겨 이런거도 재미있었어요ㅋㅋ",
          "original_form": "뭔가 합 맞추는겨 이런거도 재미있었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006275.1.22",
          "form": "밴드 동아리는 아니더라도",
          "original_form": "밴드 동아리는 아니더라도",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006275.1.23",
          "form": "학교 음악시간에 친구들이랑 맞춰서 연주해 본 적은 있네요",
          "original_form": "학교 음악시간에 친구들이랑 맞춰서 연주해 본 적은 있네요",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006275.1.24",
          "form": "오 요새는 그런것도 하나요",
          "original_form": "오 요새는 그런것도 하나요",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006275.1.25",
          "form": "엄청 어려울 듯 한데",
          "original_form": "엄청 어려울 듯 한데",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006275.1.26",
          "form": "음악선생님이 적극적인 분이셨던 거 같아요",
          "original_form": "음악선생님이 적극적인 분이셨던 거 같아요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006275.1.27",
          "form": "맞아요 선생님이 열정이 있으셔야 갸능한..!",
          "original_form": "맞아요 선생님이 열정이 있으셔야 갸능한..!",
