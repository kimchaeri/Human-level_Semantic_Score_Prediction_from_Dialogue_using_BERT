      "utterance": [
        {
          "id": "MDRW2100011529.1.1",
          "form": "여기는 비가와요",
          "original_form": "여기는 비가와요",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011529.1.2",
          "form": "여기도 와요 많이는 안오지만 계속 내리네요",
          "original_form": "여기도 와요 많이는 안오지만 계속 내리네요",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011529.1.3",
          "form": "이번주 내내 장마라고 하더라구요",
          "original_form": "이번주 내내 장마라고 하더라구요",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011529.1.4",
          "form": "벌써 가을이 오려나봐요",
          "original_form": "벌써 가을이 오려나봐요",
          "speaker_id": "1",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011529.1.5",
          "form": "날짜론 가을이 올 때가 되긴 했죠~ 시간 빨라요",
          "original_form": "날짜론 가을이 올 때가 되긴 했죠~ 시간 빨라요",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011529.1.6",
          "form": "여름이 그립네요",
          "original_form": "여름이 그립네요",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011529.1.7",
          "form": "환절기인데 감기조심하세요",
          "original_form": "환절기인데 감기조심하세요",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011529.1.8",
          "form": "벌써 그리우세요? 여름 좋아하시나봐요ㅎㅎ",
          "original_form": "벌써 그리우세요? 여름 좋아하시나봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011529.1.9",
          "form": "요즘엔 다들 마스크쓰니까 확실히 감기는 안걸리는거같아요",
          "original_form": "요즘엔 다들 마스크쓰니까 확실히 감기는 안걸리는거같아요",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011529.1.10",
          "form": "감기보단 코로나 조심..ㅎㅎ",
          "original_form": "감기보단 코로나 조심..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011529.1.11",
          "form": "추운게 싫어서요ㅋㅋ",
          "original_form": "추운게 싫어서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011529.1.12",
          "form": "맞아요 코로나 조심!",
          "original_form": "맞아요 코로나 조심!",
          "speaker_id": "1",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011529.1.13",
          "form": "전 추운거보다 건조해서 정전기가 많이 생겨서 싫긴해요ㅠㅠ",
          "original_form": "전 추운거보다 건조해서 정전기가 많이 생겨서 싫긴해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011529.1.14",
          "form": "정전기가 많이 생기는 체질이거든요",
          "original_form": "정전기가 많이 생기는 체질이거든요",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011529.1.15",
          "form": "저도 건조한거 너무싫어요!",
          "original_form": "저도 건조한거 너무싫어요!",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011529.1.16",
          "form": "비염때문에 힘들어요",
          "original_form": "비염때문에 힘들어요",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011529.1.17",
          "form": "오 그런체질도 있군요",
          "original_form": "오 그런체질도 있군요",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011529.1.18",
          "form": "아 비염이 건조해서 생기는거에요? 전 꽃가루 때문인줄 알았어요",
          "original_form": "아 비염이 건조해서 생기는거에요? 전 꽃가루 때문인줄 알았어요",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011529.1.19",
          "form": "몸에 수분이 없으면 정전기가 잘 난대요ㅠㅠ\r\n의식적으로 물을 많이 마시긴하는데 별 차이는 없어요..",
          "original_form": "몸에 수분이 없으면 정전기가 잘 난대요ㅠㅠ\r\n의식적으로 물을 많이 마시긴하는데 별 차이는 없어요..",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011529.1.20",
          "form": "원인이 다양하더라고요",
          "original_form": "원인이 다양하더라고요",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011529.1.21",
          "form": "심할땐 물에서도 정전기가 생겨요",
          "original_form": "심할땐 물에서도 정전기가 생겨요",
          "speaker_id": "2",
          "time": "20210824 13:49"
        }
      ]
    }
