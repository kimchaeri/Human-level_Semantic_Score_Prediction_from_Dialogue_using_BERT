      "utterance": [
        {
          "id": "MDRW2100033022.1.1",
          "form": "나 최근에 갑질방지법 이거 뉴스봤어",
          "original_form": "나 최근에 갑질방지법 이거 뉴스봤어",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100033022.1.2",
          "form": "인터넷으루",
          "original_form": "인터넷으루",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033022.1.3",
          "form": "구글이 원래 30%나 수수료를 떼갔다구 하더라",
          "original_form": "구글이 원래 30%나 수수료를 떼갔다구 하더라",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033022.1.4",
          "form": "나도 크롬 좋아하지만",
          "original_form": "나도 크롬 좋아하지만",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033022.1.5",
          "form": "확실히 한국인이라ㅠ 외국기업 세금안내고 돈버는거 몬가 반갑지는 않은듯",
          "original_form": "확실히 한국인이라ㅠ 외국기업 세금안내고 돈버는거 몬가 반갑지는 않은듯",
          "speaker_id": "1",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033022.1.6",
          "form": "구글?그래?? 떼가겠다고 선포한거 아니었나??",
          "original_form": "구글?그래?? 떼가겠다고 선포한거 아니었나??",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100033022.1.7",
          "form": "근데 참그럼 크롬말고 다른거쓸까? 근데 생각해보니또 어차피 광고차단어플쓰긴하는데",
          "original_form": "근데 참그럼 크롬말고 다른거쓸까? 근데 생각해보니또 어차피 광고차단어플쓰긴하는데",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033022.1.8",
          "form": "아니 그 어플아니고 프로그램",
          "original_form": "아니 그 어플아니고 프로그램",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033022.1.9",
          "form": "그럼 어차피 얘네 돈버는건 아니겠지?",
          "original_form": "그럼 어차피 얘네 돈버는건 아니겠지?",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100033022.1.10",
          "form": "그치 돈을 덜 벌겠지",
          "original_form": "그치 돈을 덜 벌겠지",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033022.1.11",
          "form": "우리가 영향을 줄 수 있을까 싶긴하지만",
          "original_form": "우리가 영향을 줄 수 있을까 싶긴하지만",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033022.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나 웨일도 쓰긴해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나 웨일도 쓰긴해",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033022.1.13",
          "form": "크롬으로 네이버 들어가구",
          "original_form": "크롬으로 네이버 들어가구",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033022.1.14",
          "form": "웨일이 크롬보다 좋긴한데",
          "original_form": "웨일이 크롬보다 좋긴한데",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033022.1.15",
          "form": "익숙해져서 쓰는편이야",
          "original_form": "익숙해져서 쓰는편이야",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033022.1.16",
          "form": "난 엣지밖에 안 써",
          "original_form": "난 엣지밖에 안 써",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033022.1.17",
          "form": "웨일 너무 무겁더라",
          "original_form": "웨일 너무 무겁더라",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033022.1.18",
          "form": "그래? 기능이 많아서 그런가",
          "original_form": "그래? 기능이 많아서 그런가",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033022.1.19",
          "form": "어쨌든 구글이 이제 수수료 못받는다는건가",
          "original_form": "어쨌든 구글이 이제 수수료 못받는다는건가",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033022.1.20",
          "form": "그런걸껄",
          "original_form": "그런걸껄",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033022.1.21",
          "form": "갑질방지법인가 그거",
          "original_form": "갑질방지법인가 그거",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033022.1.22",
          "form": "맞아 잘했더라 그건",
          "original_form": "맞아 잘했더라 그건",
          "speaker_id": "2",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100033022.1.23",
          "form": "응응 근데 제정된건가",
          "original_form": "응응 근데 제정된건가",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100033022.1.24",
          "form": "사실 체감하진못할듯",
          "original_form": "사실 체감하진못할듯",
          "speaker_id": "1",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100033022.1.25",
          "form": "난워낙",
          "original_form": "난워낙",
          "speaker_id": "1",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100033022.1.26",
          "form": "결제를안해서",
          "original_form": "결제를안해서",
          "speaker_id": "1",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100033022.1.27",
          "form": "모르겠다 나도 결제를 안해서",
          "original_form": "모르겠다 나도 결제를 안해서",
          "speaker_id": "2",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100033022.1.28",
          "form": "통과했다는 것만 봤어",
          "original_form": "통과했다는 것만 봤어",
          "speaker_id": "2",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100033022.1.29",
          "form": "나는 오히려 원스토어 많이썼어",
          "original_form": "나는 오히려 원스토어 많이썼어",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033022.1.30",
          "form": "이벤트로 캐시주길래",
          "original_form": "이벤트로 캐시주길래",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033022.1.31",
          "form": "조금더 보태서 쿠키런..",
          "original_form": "조금더 보태서 쿠키런..",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033022.1.32",
          "form": "샀었지",
          "original_form": "샀었지",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033022.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ잘했어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ잘했어",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033022.1.34",
          "form": "맞아 원스토어 좋지 국내거잖아",
          "original_form": "맞아 원스토어 좋지 국내거잖아",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033022.1.35",
          "form": "확실히 그런데가 수수료를 덜받으니까",
          "original_form": "확실히 그런데가 수수료를 덜받으니까",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033022.1.36",
          "form": "세일? 이벤트를 했나봐 싶긴하더라",
          "original_form": "세일? 이벤트를 했나봐 싶긴하더라",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033022.1.37",
          "form": "맞아 그리고 점유율이 낮다보니까 홍보용으로 이벤트도 많이하고 그러더라",
          "original_form": "맞아 그리고 점유율이 낮다보니까 홍보용으로 이벤트도 많이하고 그러더라",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033022.1.38",
          "form": "그런가보당!",
          "original_form": "그런가보당!",
          "speaker_id": "1",
          "time": "20210906 16:42"
        }
      ]
