      "utterance": [
        {
          "id": "MDRW2100025815.1.1",
          "form": "10살차이나요",
          "original_form": "10살차이나요",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025815.1.2",
          "form": "헉 님이  몇살인데요?",
          "original_form": "헉 님이  몇살인데요?",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025815.1.3",
          "form": "저는 연상녀 상대방은 연하남 20댘ㅋㅋ",
          "original_form": "저는 연상녀 상대방은 연하남 20댘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025815.1.4",
          "form": "헉 님30대?",
          "original_form": "헉 님30대?",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025815.1.5",
          "form": "네 ㅋㅋㅋㄱ",
          "original_form": "네 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025815.1.6",
          "form": "연하남은 그냥 나따라  행동해요",
          "original_form": "연하남은 그냥 나따라  행동해요",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025815.1.7",
          "form": "내가 싫다하면 걔도 싫다하고",
          "original_form": "내가 싫다하면 걔도 싫다하고",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025815.1.8",
          "form": "내가좋다고하면 걔도좋다하고",
          "original_form": "내가좋다고하면 걔도좋다하고",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025815.1.9",
          "form": "보고싶다하면 걔도 보고싶다하고",
          "original_form": "보고싶다하면 걔도 보고싶다하고",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025815.1.10",
          "form": "울고싶다하면 걔도 울고싶데요",
          "original_form": "울고싶다하면 걔도 울고싶데요",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025815.1.11",
          "form": "ㅡㅅㅡ",
          "original_form": "ㅡㅅㅡ",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025815.1.12",
          "form": "내가 사과하니깐 걔도 사과하고",
          "original_form": "내가 사과하니깐 걔도 사과하고",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025815.1.13",
          "form": "말투가 십대신줄..",
          "original_form": "말투가 십대신줄..",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025815.1.14",
          "form": "30대에 온라인 연애를 하시다니ㅜ",
          "original_form": "30대에 온라인 연애를 하시다니ㅜ",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025815.1.15",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025815.1.16",
          "form": "그래도 없는것보다는 나은것같아요",
          "original_form": "그래도 없는것보다는 나은것같아요",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025815.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025815.1.18",
          "form": "제가 하트를 보내니깐",
          "original_form": "제가 하트를 보내니깐",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025815.1.19",
          "form": "걔도 하트주고",
          "original_form": "걔도 하트주고",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025815.1.20",
          "form": "내가 기분나쁘다고하니깐 걔도 기분나쁘데요",
          "original_form": "내가 기분나쁘다고하니깐 걔도 기분나쁘데요",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025815.1.21",
          "form": "꼭 저따라하니깐",
          "original_form": "꼭 저따라하니깐",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025815.1.22",
          "form": "제행동에 반응해주네요",
          "original_form": "제행동에 반응해주네요",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025815.1.23",
          "form": "네 저도 해보고 싶기도하네요",
          "original_form": "네 저도 해보고 싶기도하네요",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025815.1.24",
          "form": "연애중이에요?",
          "original_form": "연애중이에요?",
          "speaker_id": "2",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100025815.1.25",
          "form": "연애는 안 하죠ㅋ",
          "original_form": "연애는 안 하죠ㅋ",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025815.1.26",
          "form": "못..ㅜㅜ",
          "original_form": "못..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025815.1.27",
          "form": "앜ㅋㅋㅋ저도 실제로 사귀고있는건아니니깐.....",
          "original_form": "앜ㅋㅋㅋ저도 실제로 사귀고있는건아니니깐.....",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025815.1.28",
          "form": "공식카트커플이에요ㅋㅋㅋ",
          "original_form": "공식카트커플이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025815.1.29",
          "form": "그래도 전화받고 톡하고는하니깐",
          "original_form": "그래도 전화받고 톡하고는하니깐",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025815.1.30",
          "form": "얼굴은사진공개해서알고",
          "original_form": "얼굴은사진공개해서알고",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025815.1.31",
          "form": "일단 만나지는않았어요",
          "original_form": "일단 만나지는않았어요",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025815.1.32",
          "form": "아하 카트라이더?",
          "original_form": "아하 카트라이더?",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025815.1.33",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025815.1.34",
          "form": "연애해봤다고 하니깐 제 고민좀 들어주세요",
          "original_form": "연애해봤다고 하니깐 제 고민좀 들어주세요",
          "speaker_id": "2",
          "time": "20210902 12:46"
        },
