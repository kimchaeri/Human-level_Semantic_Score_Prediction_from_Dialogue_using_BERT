      "utterance": [
        {
          "id": "MDRW2100015555.1.1",
          "form": "아뇨! 안오네요!",
          "original_form": "아뇨! 안오네요!",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015555.1.2",
          "form": "예보엔 내일은 안온다고 하네요",
          "original_form": "예보엔 내일은 안온다고 하네요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015555.1.3",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210826 06:29"
        },
        {
          "id": "MDRW2100015555.1.4",
          "form": "흐리고 비는 안오네요!",
          "original_form": "흐리고 비는 안오네요!",
          "speaker_id": "2",
          "time": "20210826 10:06"
        },
        {
          "id": "MDRW2100015555.1.5",
          "form": "더워요",
          "original_form": "더워요",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100015555.1.6",
          "form": "햇빛 간만에 보는거같아요 ㅠㅠ",
          "original_form": "햇빛 간만에 보는거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015555.1.7",
          "form": "좀 반가운 더위",
          "original_form": "좀 반가운 더위",
          "speaker_id": "2",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015555.1.8",
          "form": "그렇지만 습해서",
          "original_form": "그렇지만 습해서",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015555.1.9",
          "form": "습한것도 나름 있긴하지만",
          "original_form": "습한것도 나름 있긴하지만",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015555.1.10",
          "form": "그래도 간만에 보는 햇빛이라 좋네요",
          "original_form": "그래도 간만에 보는 햇빛이라 좋네요",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015555.1.11",
          "form": "그렇됴",
          "original_form": "그렇됴",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015555.1.12",
          "form": "내일또 비온다고하던데",
          "original_form": "내일또 비온다고하던데",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100015555.1.13",
          "form": "제발 안왔으면 좋겠네요 ㅠㅠ",
          "original_form": "제발 안왔으면 좋겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100015555.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100015555.1.15",
          "form": "오늘같은 날씨가 참 좋은데",
          "original_form": "오늘같은 날씨가 참 좋은데",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100015555.1.16",
          "form": "비오면 골치아프거든요 ㅠㅠ",
          "original_form": "비오면 골치아프거든요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100015555.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100015555.1.18",
          "form": "부디 내일 비 안오길 ㅠ.ㅠ",
          "original_form": "부디 내일 비 안오길 ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100015555.1.19",
          "form": "덥지도 않길",
          "original_form": "덥지도 않길",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100015555.1.20",
          "form": "덥죠?",
          "original_form": "덥죠?",
