          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002527.1.15",
          "form": "아 40대 3명",
          "original_form": "아 40대 3명",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002527.1.16",
          "form": "나 궁금한게 생겼는데",
          "original_form": "나 궁금한게 생겼는데",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002527.1.17",
          "form": "30대 얼마전에 들어옴",
          "original_form": "30대 얼마전에 들어옴",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002527.1.18",
          "form": "예전에 나 소개해주려고 했던 여자 생각나?",
          "original_form": "예전에 나 소개해주려고 했던 여자 생각나?",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002527.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002527.1.20",
          "form": "바람둥이 였음",
          "original_form": "바람둥이 였음",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002527.1.21",
          "form": "중국어한다고 하고 웨딩샵에서 투잡뛰던",
          "original_form": "중국어한다고 하고 웨딩샵에서 투잡뛰던",
          "speaker_id": "1",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002527.1.22",
          "form": "아 그래",
          "original_form": "아 그래",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.23",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.24",
          "form": "바둥이였어",
          "original_form": "바둥이였어",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.25",
          "form": "엄ㅅㅇ",
          "original_form": "엄ㅅㅇ",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.26",
          "form": "이름은 생각이 안나",
          "original_form": "이름은 생각이 안나",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.27",
          "form": "갑자기 남친생겼다고",
          "original_form": "갑자기 남친생겼다고",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.28",
          "form": "연락 그만했던",
          "original_form": "연락 그만했던",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.29",
          "form": "결혼함",
          "original_form": "결혼함",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.30",
          "form": "아 진짜",
          "original_form": "아 진짜",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.31",
          "form": "결국 외국에서 사귀었던 남자랑",
          "original_form": "결국 외국에서 사귀었던 남자랑",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.32",
          "form": "남자가 한국 들어옴",
          "original_form": "남자가 한국 들어옴",
          "speaker_id": "2",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002527.1.33",
          "form": "외국인?",
          "original_form": "외국인?",
          "speaker_id": "1",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002527.1.34",
          "form": "한국사람",
          "original_form": "한국사람",
          "speaker_id": "2",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002527.1.35",
          "form": "나중에 들어보니",
          "original_form": "나중에 들어보니",
          "speaker_id": "2",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002527.1.36",
          "form": "걸러지긜 잘한듯",
          "original_form": "걸러지긜 잘한듯",
          "speaker_id": "2",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002527.1.37",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002527.1.38",
          "form": "외국생활 많이해서 그런지 성적으로 개방",
          "original_form": "외국생활 많이해서 그런지 성적으로 개방",
          "speaker_id": "2",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002527.1.39",
          "form": "야 그건 좋네",
          "original_form": "야 그건 좋네",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002527.1.40",
          "form": "고객사 사람이랑 술먹는데 말하더라 잠시 사귀었는데",
          "original_form": "고객사 사람이랑 술먹는데 말하더라 잠시 사귀었는데",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002527.1.41",
          "form": "좀 통통했던것 같은데",
          "original_form": "좀 통통했던것 같은데",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002527.1.42",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002527.1.43",
          "form": "잠자리하고",
          "original_form": "잠자리하고",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002527.1.44",
