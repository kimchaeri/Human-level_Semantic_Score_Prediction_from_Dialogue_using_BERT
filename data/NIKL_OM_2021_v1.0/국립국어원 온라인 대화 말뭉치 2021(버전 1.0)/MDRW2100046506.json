        {
          "id": "MDRW2100046506.1.2",
          "form": "안녕하세요~! 좋은 아침이에요!",
          "original_form": "안녕하세요~! 좋은 아침이에요!",
          "speaker_id": "2",
          "time": "20211026 09:34"
        },
        {
          "id": "MDRW2100046506.1.3",
          "form": "네 반가워요",
          "original_form": "네 반가워요",
          "speaker_id": "1",
          "time": "20211026 09:35"
        },
        {
          "id": "MDRW2100046506.1.4",
          "form": "아침은 드셨나요~?",
          "original_form": "아침은 드셨나요~?",
          "speaker_id": "2",
          "time": "20211026 09:39"
        },
        {
          "id": "MDRW2100046506.1.5",
          "form": "아침은 잘 안먹는 편이에요",
          "original_form": "아침은 잘 안먹는 편이에요",
          "speaker_id": "1",
          "time": "20211026 09:39"
        },
        {
          "id": "MDRW2100046506.1.6",
          "form": "아침 드시나요?",
          "original_form": "아침 드시나요?",
          "speaker_id": "1",
          "time": "20211026 09:41"
        },
        {
          "id": "MDRW2100046506.1.7",
          "form": "저도 안 먹는 편이에요 ㅎㅎ",
          "original_form": "저도 안 먹는 편이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211026 09:49"
        },
        {
          "id": "MDRW2100046506.1.8",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20211026 09:49"
        },
        {
          "id": "MDRW2100046506.1.9",
          "form": "저는 한식 좋아해요",
          "original_form": "저는 한식 좋아해요",
          "speaker_id": "1",
          "time": "20211026 09:49"
        },
        {
          "id": "MDRW2100046506.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20211026 09:49"
        },
        {
          "id": "MDRW2100046506.1.11",
          "form": "저는 회 좋아해요!",
          "original_form": "저는 회 좋아해요!",
          "speaker_id": "2",
          "time": "20211026 10:14"
        },
        {
          "id": "MDRW2100046506.1.12",
          "form": "어떤 음식이 최야인가요? ㅎㅎ",
          "original_form": "어떤 음식이 최야인가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211026 10:14"
        },
        {
          "id": "MDRW2100046506.1.13",
          "form": "음 저는",
          "original_form": "음 저는",
          "speaker_id": "1",
          "time": "20211026 10:19"
        },
        {
          "id": "MDRW2100046506.1.14",
          "form": "볶음을 좋아해요",
          "original_form": "볶음을 좋아해요",
          "speaker_id": "1",
          "time": "20211026 10:19"
        },
        {
          "id": "MDRW2100046506.1.15",
          "form": "오징어볶음 제육볶음",
          "original_form": "오징어볶음 제육볶음",
          "speaker_id": "1",
          "time": "20211026 10:19"
        },
        {
          "id": "MDRW2100046506.1.16",
          "form": "오 완전 직장인 점심 ㅠ",
          "original_form": "오 완전 직장인 점심 ㅠ",
          "speaker_id": "2",
          "time": "20211026 10:40"
        },
        {
          "id": "MDRW2100046506.1.17",
          "form": "떡볶이도 좋아하세요?",
          "original_form": "떡볶이도 좋아하세요?",
          "speaker_id": "2",
          "time": "20211026 10:40"
        },
        {
          "id": "MDRW2100046506.1.18",
          "form": "네 그럼요",
          "original_form": "네 그럼요",
          "speaker_id": "1",
          "time": "20211026 11:02"
        },
        {
          "id": "MDRW2100046506.1.19",
          "form": "떡볶이는 주말에 꼭 한번은 시켜먹어요",
          "original_form": "떡볶이는 주말에 꼭 한번은 시켜먹어요",
