        },
        {
          "id": "MDRW2100028336.1.2",
          "form": "그런걸 어떻게 알려드려요 ㅎㅎ;",
          "original_form": "그런걸 어떻게 알려드려요 ㅎㅎ;",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028336.1.3",
          "form": "왜 엄마도 잘 하는데",
          "original_form": "왜 엄마도 잘 하는데",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028336.1.4",
          "form": "아무래도 부끄러우니까..ㅡ",
          "original_form": "아무래도 부끄러우니까..ㅡ",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028336.1.5",
          "form": "이런것도 하는데 새삼스레",
          "original_form": "이런것도 하는데 새삼스레",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028336.1.6",
          "form": "이거랑은 다른걸요!!",
          "original_form": "이거랑은 다른걸요!!",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028336.1.7",
          "form": "아무튼 배경음악 설정하고",
          "original_form": "아무튼 배경음악 설정하고",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028336.1.8",
          "form": "친구들하고 교류하는거 재밌었어요 ㅋㅋㅋ",
          "original_form": "친구들하고 교류하는거 재밌었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028336.1.9",
          "form": "부활했으면 다시 하는거니?",
          "original_form": "부활했으면 다시 하는거니?",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028336.1.10",
          "form": "아뇨 이제는 하는 SNS 있으니까요",
          "original_form": "아뇨 이제는 하는 SNS 있으니까요",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028336.1.11",
          "form": "트위터 한다고 했지",
          "original_form": "트위터 한다고 했지",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028336.1.12",
          "form": "네네 ㅋㅋㅋ 트위터 재밌어서 계속 하게 돼요",
          "original_form": "네네 ㅋㅋㅋ 트위터 재밌어서 계속 하게 돼요",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028336.1.13",
          "form": "인터넷 친구 많더라 너",
          "original_form": "인터넷 친구 많더라 너",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028336.1.14",
          "form": "싸이월드에서 사귄 친구들도 있어요 ㅎㅎ 신기하죠",
          "original_form": "싸이월드에서 사귄 친구들도 있어요 ㅎㅎ 신기하죠",
          "speaker_id": "1",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028336.1.15",
          "form": "되게 오래됐어요",
