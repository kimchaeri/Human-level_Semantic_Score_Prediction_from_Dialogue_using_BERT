      "utterance": [
        {
          "id": "MDRW2100013834.1.1",
          "form": "한국오셨을때 제일 먼저산게 무엇인가요?",
          "original_form": "한국오셨을때 제일 먼저산게 무엇인가요?",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013834.1.2",
          "form": "음..............",
          "original_form": "음..............",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013834.1.3",
          "form": "기억이 매우 가물가물한데....",
          "original_form": "기억이 매우 가물가물한데....",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013834.1.4",
          "form": "아마 샀다면 화장품 일것같앙ㅅ",
          "original_form": "아마 샀다면 화장품 일것같앙ㅅ",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013834.1.5",
          "form": "그러시구나 확실히 한국 화장품이 괜찮나요?",
          "original_form": "그러시구나 확실히 한국 화장품이 괜찮나요?",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013834.1.6",
          "form": "기후가 매우 다르니까",
          "original_form": "기후가 매우 다르니까",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013834.1.7",
          "form": "기존에 쓰던게 안맞더라구요..ㅠㅠ",
          "original_form": "기존에 쓰던게 안맞더라구요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013834.1.8",
          "form": "그래서 새로 다시샀어야 했어요",
          "original_form": "그래서 새로 다시샀어야 했어요",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013834.1.9",
          "form": "아 그렇겠네요!",
          "original_form": "아 그렇겠네요!",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013834.1.10",
          "form": "요즘은 그러면 뭐 사고 싶으세요?",
          "original_form": "요즘은 그러면 뭐 사고 싶으세요?",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013834.1.11",
          "form": "네ㅠㅠ 그래서 싹다 바꿨었던것 같아요",
          "original_form": "네ㅠㅠ 그래서 싹다 바꿨었던것 같아요",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013834.1.12",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013834.1.13",
          "form": "스위치 사고싶어서 벼루다가 이번에 샀어요!",
          "original_form": "스위치 사고싶어서 벼루다가 이번에 샀어요!",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013834.1.14",
          "form": "와! 축하드려요 저도 벼르다가 올해 봄에 샀었는데",
          "original_form": "와! 축하드려요 저도 벼르다가 올해 봄에 샀었는데",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013834.1.15",
          "form": "타이틀은 뭐사셨나요?",
          "original_form": "타이틀은 뭐사셨나요?",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013834.1.16",
          "form": "저 젤다요!",
          "original_form": "저 젤다요!",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013834.1.17",
          "form": "젤다랑 모동숲!",
          "original_form": "젤다랑 모동숲!",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013834.1.18",
          "form": "젤다 정말 재밌다라구요...",
          "original_form": "젤다 정말 재밌다라구요...",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013834.1.19",
          "form": "젤다야숨 말씀하시는거죠?",
          "original_form": "젤다야숨 말씀하시는거죠?",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013834.1.20",
          "form": "모험의 재미! 진짜 너무 재밌죠",
          "original_form": "모험의 재미! 진짜 너무 재밌죠",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013834.1.21",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013834.1.22",
          "form": "야숨이요!!",
          "original_form": "야숨이요!!",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013834.1.23",
          "form": "야숨2 빨리 나왔으면 좋겠어요",
          "original_form": "야숨2 빨리 나왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013834.1.24",
          "form": "그죠!",
          "original_form": "그죠!",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013834.1.25",
          "form": "저는 중고거래로 샀어요 타이틀은",
          "original_form": "저는 중고거래로 샀어요 타이틀은",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013834.1.26",
          "form": "타이틀은 중고거래로 살만하다고 하더라구요!",
          "original_form": "타이틀은 중고거래로 살만하다고 하더라구요!",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013834.1.27",
          "form": "지금은 오디세이 뜨길 기다리는데",
          "original_form": "지금은 오디세이 뜨길 기다리는데",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013834.1.28",
          "form": "집근처에서는 매물이 잘 안올라오네요..ㅠㅠ",
          "original_form": "집근처에서는 매물이 잘 안올라오네요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013834.1.29",
          "form": "매물이 얼른 올라와야할텐데ㅠㅠ",
          "original_form": "매물이 얼른 올라와야할텐데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:59"
        }
      ]
    }
