      "utterance": [
        {
          "id": "MDRW2100030807.1.1",
          "form": "차랑 영양제 얘기했었어요 혹시 몰라서 보내놔요 ㅎㅎ",
          "original_form": "차랑 영양제 얘기했었어요 혹시 몰라서 보내놔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030807.1.2",
          "form": "아하~ 감사해용",
          "original_form": "아하~ 감사해용",
          "speaker_id": "2",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030807.1.3",
          "form": "닌텐도도 갑자기 갖고싶네여",
          "original_form": "닌텐도도 갑자기 갖고싶네여",
          "speaker_id": "2",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030807.1.4",
          "form": "저도요 저는 동물의 숲 하고 싶어요",
          "original_form": "저도요 저는 동물의 숲 하고 싶어요",
          "speaker_id": "1",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100030807.1.5",
          "form": "아 동숲 저도 한번 해보고싶어요 ㅎ",
          "original_form": "아 동숲 저도 한번 해보고싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100030807.1.6",
          "form": "한창 붐이었을때 살까말까 했는데",
          "original_form": "한창 붐이었을때 살까말까 했는데",
          "speaker_id": "1",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100030807.1.7",
          "form": "다들 재밌게 하길래 ㅎ 궁금하더라고요",
          "original_form": "다들 재밌게 하길래 ㅎ 궁금하더라고요",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100030807.1.8",
          "form": "안사고 ㅋㅋ 넹 저도요",
          "original_form": "안사고 ㅋㅋ 넹 저도요",
          "speaker_id": "1",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100030807.1.9",
          "form": "맞아요 한창 유행이었죠",
          "original_form": "맞아요 한창 유행이었죠",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100030807.1.10",
          "form": "근데 질릴거 같기도 해요",
          "original_form": "근데 질릴거 같기도 해요",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100030807.1.11",
          "form": "주변에 산 친구는 그만두긴 했어서 ㅋㅋㅋ",
          "original_form": "주변에 산 친구는 그만두긴 했어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100030807.1.12",
          "form": "아 그래요?  그럼 ㅋㅋ포기해야겠네요",
          "original_form": "아 그래요?  그럼 ㅋㅋ포기해야겠네요",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100030807.1.13",
          "form": "게임 금방 질려하는 타입이라",
          "original_form": "게임 금방 질려하는 타입이라",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100030807.1.14",
          "form": "저도 오래 못해요 ㅎㅎ 게임 좋아하세요?",
          "original_form": "저도 오래 못해요 ㅎㅎ 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100030807.1.15",
          "form": "아뇨 ㅎ 그냥 심심하거나 시간때울때 한번씩 하는정도에요",
          "original_form": "아뇨 ㅎ 그냥 심심하거나 시간때울때 한번씩 하는정도에요",
          "speaker_id": "2",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100030807.1.16",
          "form": "저도 그렇게 좋아하는 편은 아니에요 핸드폰 게임도 잠깐 했다가 마는 정도?",
          "original_form": "저도 그렇게 좋아하는 편은 아니에요 핸드폰 게임도 잠깐 했다가 마는 정도?",
          "speaker_id": "1",
          "time": "20210904 20:19"
        },
        {
          "id": "MDRW2100030807.1.17",
          "form": "끝까지를 못하네요 ㅋㅋ",
          "original_form": "끝까지를 못하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:19"
        },
        {
          "id": "MDRW2100030807.1.18",
          "form": "저도요ㅋㅋㅋ 끝장을 내지 못하는",
          "original_form": "저도요ㅋㅋㅋ 끝장을 내지 못하는",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100030807.1.19",
          "form": "ㅎㅎ 끈질기게 끝을 봐야되는데 그걸 못해여",
          "original_form": "ㅎㅎ 끈질기게 끝을 봐야되는데 그걸 못해여",
          "speaker_id": "1",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100030807.1.20",
          "form": "그런데 심심이만큼은 백개는 하겠다는 목표를",
          "original_form": "그런데 심심이만큼은 백개는 하겠다는 목표를",
          "speaker_id": "1",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100030807.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:31"
        },
        {
          "id": "MDRW2100030807.1.22",
          "form": "ㅋㅋㅋㅋ저도요 뭔가 이번에는 목표만큼해보자에요",
          "original_form": "ㅋㅋㅋㅋ저도요 뭔가 이번에는 목표만큼해보자에요",
