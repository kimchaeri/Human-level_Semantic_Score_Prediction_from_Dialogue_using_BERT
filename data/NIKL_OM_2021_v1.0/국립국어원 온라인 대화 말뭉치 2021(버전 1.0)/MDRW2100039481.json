        },
        {
          "id": "MDRW2100039481.1.2",
          "form": "학교 다닐만 하십니까",
          "original_form": "학교 다닐만 하십니까",
          "speaker_id": "1",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039481.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039481.1.4",
          "form": "친구들 있어서 다닐만 해요 ㅎㅎㅎ",
          "original_form": "친구들 있어서 다닐만 해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039481.1.5",
          "form": "다행이네용 ㅎㅎ",
          "original_form": "다행이네용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039481.1.6",
          "form": "혹시 어떤 동아리 하고 계세요?",
          "original_form": "혹시 어떤 동아리 하고 계세요?",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039481.1.7",
          "form": "전 학교 교지 만드는 동아리에요",
          "original_form": "전 학교 교지 만드는 동아리에요",
          "speaker_id": "1",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039481.1.8",
          "form": "우와! 전 그림그리는 동아리 하고있어요",
          "original_form": "우와! 전 그림그리는 동아리 하고있어요",
          "speaker_id": "2",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039481.1.9",
          "form": "헉 미술부신가요",
          "original_form": "헉 미술부신가요",
          "speaker_id": "1",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039481.1.10",
          "form": "네! 미술 배운다는 목적보단 그냥 자유롭게 그리는 분위기에요",
          "original_form": "네! 미술 배운다는 목적보단 그냥 자유롭게 그리는 분위기에요",
          "speaker_id": "2",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039481.1.11",
          "form": "와,,좋네요",
          "original_form": "와,,좋네요",
          "speaker_id": "1",
          "time": "20210903 18:09"
        },
        {
          "id": "MDRW2100039481.1.12",
          "form": "동아리 분위기 어때요?",
          "original_form": "동아리 분위기 어때요?",
          "speaker_id": "1",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039481.1.13",
          "form": "선배님들도 친절하시고 빡세게 굴러가지 않아서 좋아요",
          "original_form": "선배님들도 친절하시고 빡세게 굴러가지 않아서 좋아요",
          "speaker_id": "2",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039481.1.14",
          "form": "자유롭네요..",
          "original_form": "자유롭네요..",
          "speaker_id": "1",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100039481.1.15",
          "form": "약간 미술 쪽 특유의  자유로운 분위기 부러워요",
          "original_form": "약간 미술 쪽 특유의  자유로운 분위기 부러워요",
          "speaker_id": "1",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100039481.1.16",
          "form": "완전 막...회화를 하자!보다는 만화 좋아하는 사람끼리 모인거라 더 그런것 같아요!",
          "original_form": "완전 막...회화를 하자!보다는 만화 좋아하는 사람끼리 모인거라 더 그런것 같아요!",
          "speaker_id": "2",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100039481.1.17",
          "form": "근데 교지는 뭐에요? 학교 홍보지 같은건가요?",
          "original_form": "근데 교지는 뭐에요? 학교 홍보지 같은건가요?",
          "speaker_id": "2",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100039481.1.18",
          "form": "아 약간 만화부 그런 쪽이군요",
          "original_form": "아 약간 만화부 그런 쪽이군요",
          "speaker_id": "1",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100039481.1.19",
          "form": "홍보지는 아니구요",
          "original_form": "홍보지는 아니구요",
          "speaker_id": "1",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100039481.1.20",
          "form": "약간 학교 잡지?",
          "original_form": "약간 학교 잡지?",
          "speaker_id": "1",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100039481.1.21",
          "form": "선생님들 인터뷰도 싣고",
          "original_form": "선생님들 인터뷰도 싣고",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039481.1.22",
          "form": "졸업생 대학 가신분들 학과 관련 인터뷰도 싣는",
          "original_form": "졸업생 대학 가신분들 학과 관련 인터뷰도 싣는",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039481.1.23",
          "form": "그런 거에요!",
          "original_form": "그런 거에요!",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039481.1.24",
          "form": "우와... 편집 힘들겠네요...!!!!!!멋져요!!!",
          "original_form": "우와... 편집 힘들겠네요...!!!!!!멋져요!!!",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039481.1.25",
          "form": "그렇진 않아요..출판은..출판 업체에서 해주기 때문에..^^",
          "original_form": "그렇진 않아요..출판은..출판 업체에서 해주기 때문에..^^",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039481.1.26",
          "form": "약간 입시도 미술 쪽으로 가시나요?",
          "original_form": "약간 입시도 미술 쪽으로 가시나요?",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039481.1.27",
          "form": "앗 그렇군용..",
          "original_form": "앗 그렇군용..",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039481.1.28",
          "form": "그럴 목표로 살고있어요!",
          "original_form": "그럴 목표로 살고있어요!",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039481.1.29",
          "form": "그렇군요..입시 화이팅 하세요 ㅎㅎ",
          "original_form": "그렇군요..입시 화이팅 하세요 ㅎㅎ",
