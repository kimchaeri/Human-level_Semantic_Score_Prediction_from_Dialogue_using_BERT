      "utterance": [
        {
          "id": "MDRW2100027399.1.1",
          "form": "안녕하세요 운동하는거 있으세요?",
          "original_form": "안녕하세요 운동하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027399.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100027399.1.3",
          "form": "숨쉬기만 하고있어요...",
          "original_form": "숨쉬기만 하고있어요...",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027399.1.4",
          "form": "ㅋㅋㅋㅋㅋ 폐운동 하고계시네요",
          "original_form": "ㅋㅋㅋㅋㅋ 폐운동 하고계시네요",
          "speaker_id": "1",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027399.1.5",
          "form": "코로나에 운동도안하니 생명력 깎이는거같아요",
          "original_form": "코로나에 운동도안하니 생명력 깎이는거같아요",
          "speaker_id": "1",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100027399.1.6",
          "form": "네.....",
          "original_form": "네.....",
          "speaker_id": "2",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027399.1.7",
          "form": "운동 따로하는거 있으세요?",
          "original_form": "운동 따로하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027399.1.8",
          "form": "저 다음주부터에 필라테스 등록햇는데",
          "original_form": "저 다음주부터에 필라테스 등록햇는데",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027399.1.9",
          "form": "듣자하니 엄청힘들대서 겁먹엇어요",
          "original_form": "듣자하니 엄청힘들대서 겁먹엇어요",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027399.1.10",
          "form": "오오...",
          "original_form": "오오...",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027399.1.11",
          "form": "제 친구는 하루가고 안가더라구요...",
          "original_form": "제 친구는 하루가고 안가더라구요...",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027399.1.12",
          "form": "필라테스",
          "original_form": "필라테스",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027399.1.13",
          "form": "헐ㅋㅋㅋㅋㅋ그정도에요?미치겟다",
          "original_form": "헐ㅋㅋㅋㅋㅋ그정도에요?미치겟다",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027399.1.14",
          "form": "꽤비싸던데 그분은 정말 가기싫으셧나봐요",
          "original_form": "꽤비싸던데 그분은 정말 가기싫으셧나봐요",
          "speaker_id": "1",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027399.1.15",
          "form": "거기가 주기적으로 행사를 하더라구요ㅋㅋㅋ아마 그때 등록하지않았을까 싶어요ㅋㅋㅋㅋ",
          "original_form": "거기가 주기적으로 행사를 하더라구요ㅋㅋㅋ아마 그때 등록하지않았을까 싶어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027399.1.16",
          "form": "앜ㅋㅋㅋㅋ 저도 회당 15000원에 등록햇는데",
          "original_form": "앜ㅋㅋㅋㅋ 저도 회당 15000원에 등록햇는데",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027399.1.17",
          "form": "한번가고 안가긴 너무아까워요",
          "original_form": "한번가고 안가긴 너무아까워요",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027399.1.18",
          "form": "1회 9900원인가.. 한달99000원인가...",
          "original_form": "1회 9900원인가.. 한달99000원인가...",
          "speaker_id": "2",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100027399.1.19",
          "form": "지나가면서봐서 자세히 기억은 안나지만...",
          "original_form": "지나가면서봐서 자세히 기억은 안나지만...",
          "speaker_id": "2",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100027399.1.20",
          "form": "와 대박싼데욬ㅋㅋㅋㅋ",
          "original_form": "와 대박싼데욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100027399.1.21",
          "form": "친구분 잘 고르셨다..",
          "original_form": "친구분 잘 고르셨다..",
          "speaker_id": "1",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100027399.1.22",
          "form": "필라테스는 가격이 너무 비밀스러워요",
          "original_form": "필라테스는 가격이 너무 비밀스러워요",
