        },
        {
          "id": "MDRW2100006731.1.2",
          "form": "아니요~",
          "original_form": "아니요~",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006731.1.3",
          "form": "맞아요 결혼은 선택이지요",
          "original_form": "맞아요 결혼은 선택이지요",
          "speaker_id": "1",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100006731.1.4",
          "form": "ㅎㅎ 그쵸 요즘엔 결혼 안하는 사람들이 많아졌어요",
          "original_form": "ㅎㅎ 그쵸 요즘엔 결혼 안하는 사람들이 많아졌어요",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100006731.1.5",
          "form": "언제 결혼하셨어요?",
          "original_form": "언제 결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100006731.1.6",
          "form": "한 오년 된거 같아요",
          "original_form": "한 오년 된거 같아요",
          "speaker_id": "1",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100006731.1.7",
          "form": "벌써 가물가물ㅋㅋ",
          "original_form": "벌써 가물가물ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100006731.1.8",
          "form": "헉 !",
          "original_form": "헉 !",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100006731.1.9",
          "form": "그정도면 신혼이시져 ><",
          "original_form": "그정도면 신혼이시져 ><",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100006731.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100006731.1.11",
          "form": "결혼 생각은 없으세요?",
          "original_form": "결혼 생각은 없으세요?",
          "speaker_id": "1",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100006731.1.12",
          "form": "아직은 따로 없어요 ㅎㅎ",
          "original_form": "아직은 따로 없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100006731.1.13",
          "form": "헉 혹시 아기도 있으신가요?",
          "original_form": "헉 혹시 아기도 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100006731.1.14",
          "form": "네ㅎㅎ 신혼 지났죠ㅠ",
          "original_form": "네ㅎㅎ 신혼 지났죠ㅠ",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100006731.1.15",
          "form": "헉 아기 너무 귀여울 것 같아요!!!!",
          "original_form": "헉 아기 너무 귀여울 것 같아요!!!!",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100006731.1.16",
          "form": "저도 조카가 있는데 너무너무 귀여워요ㅠㅠ",
          "original_form": "저도 조카가 있는데 너무너무 귀여워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100006731.1.17",
          "form": "ㅎㅎ 아직 어린 조카인가봐요",
          "original_form": "ㅎㅎ 아직 어린 조카인가봐요",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100006731.1.18",
          "form": "네 이제 맘마 아빠 해요",
          "original_form": "네 이제 맘마 아빠 해요",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100006731.1.19",
          "form": "ㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱㄱㅋㄱㅋ",
          "original_form": "ㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱㄱㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100006731.1.20",
          "form": "으아 두살?",
          "original_form": "으아 두살?",
          "speaker_id": "1",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100006731.1.21",
          "form": "젤 귀여울때네요",
          "original_form": "젤 귀여울때네요",
          "speaker_id": "1",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100006731.1.22",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100006731.1.23",
          "form": "역시 부모님은 다르네요 바로 맞추시는ㅋㅋㅋㅋ",
          "original_form": "역시 부모님은 다르네요 바로 맞추시는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100006731.1.24",
          "form": "한번은 회사 직원들끼리 점심에 피크닉(?)을 나갔는데",
          "original_form": "한번은 회사 직원들끼리 점심에 피크닉(?)을 나갔는데",
          "speaker_id": "2",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006731.1.25",
          "form": "유치원생들이 소풍을 왔더라구요",
          "original_form": "유치원생들이 소풍을 왔더라구요",
          "speaker_id": "2",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006731.1.26",
          "form": "한 아이 나이를 추측해봤는데 자녀가 있는 대리님만 맞추셨어요ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "한 아이 나이를 추측해봤는데 자녀가 있는 대리님만 맞추셨어요ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006731.1.27",
          "form": "ㅋㅋㅋㅋ 키워봐야 아는",
          "original_form": "ㅋㅋㅋㅋ 키워봐야 아는",
          "speaker_id": "1",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006731.1.28",
          "form": "피크닉 아가들도 너무 귀엽죠?",
          "original_form": "피크닉 아가들도 너무 귀엽죠?",
          "speaker_id": "1",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006731.1.29",
          "form": "네 진짜 너무 귀여웠어요",
          "original_form": "네 진짜 너무 귀여웠어요",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006731.1.30",
          "form": "몇살이냐고 물어보니까 꼬물대면서 손가락으로 알려주더라구요 ㅋㅋ",
