      "utterance": [
        {
          "id": "MDRW2100015138.1.1",
          "form": "오히려 맥주를 못드시네요?",
          "original_form": "오히려 맥주를 못드시네요?",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015138.1.2",
          "form": "전 술 못마시는데",
          "original_form": "전 술 못마시는데",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015138.1.3",
          "form": "네...ㅋㅋㅋㅋ",
          "original_form": "네...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015138.1.4",
          "form": "그나마 맥주만 쬐금 마셔요",
          "original_form": "그나마 맥주만 쬐금 마셔요",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015138.1.5",
          "form": "저도 술 못마시는데",
          "original_form": "저도 술 못마시는데",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015138.1.6",
          "form": "회식같은때 소주2병정도만 마셔요",
          "original_form": "회식같은때 소주2병정도만 마셔요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015138.1.7",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015138.1.8",
          "form": "두병이요?",
          "original_form": "두병이요?",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015138.1.9",
          "form": "두잔도 아니고?",
          "original_form": "두잔도 아니고?",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015138.1.10",
          "form": "술도 정신력 아잉교ㅋㅋ",
          "original_form": "술도 정신력 아잉교ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015138.1.11",
          "form": "정신력도",
          "original_form": "정신력도",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015138.1.12",
          "form": "술이 몸에서 받아야..",
          "original_form": "술이 몸에서 받아야..",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015138.1.13",
          "form": "ㅋㅋ저는 정신력이에요ㅋㅋ",
          "original_form": "ㅋㅋ저는 정신력이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015138.1.14",
          "form": "굳이 그렇게까지 드셔야해요?ㅋㅋㅋ",
          "original_form": "굳이 그렇게까지 드셔야해요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015138.1.15",
          "form": "ㅋㅋ이젠 그렇게까지\r\n안먹어도 되네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋ이젠 그렇게까지\r\n안먹어도 되네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015138.1.16",
          "form": "술마심 귀에서\r\n심장소리가 들려와여ㅋㅋㅋ",
          "original_form": "술마심 귀에서\r\n심장소리가 들려와여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015138.1.17",
          "form": "술을 권하는곳이었군여?",
          "original_form": "술을 권하는곳이었군여?",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015138.1.18",
          "form": "아니라고는 하지만 \r\n안마실수 없는 분위기? ㅋㅋㅈ",
          "original_form": "아니라고는 하지만 \r\n안마실수 없는 분위기? ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015138.1.19",
          "form": "그런거 진짜 시러요",
          "original_form": "그런거 진짜 시러요",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015138.1.20",
          "form": "시대가 어떤시댄데",
          "original_form": "시대가 어떤시댄데",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015138.1.21",
          "form": "ㅋㅋ갑자기 파도타기하고ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ갑자기 파도타기하고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.22",
          "form": "술을 강요합니까! 네?",
          "original_form": "술을 강요합니까! 네?",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.23",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.24",
          "form": "그러게요ㅋㅋㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.25",
          "form": "파도타기는 야구장에서",
          "original_form": "파도타기는 야구장에서",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.26",
          "form": "하시죠",
          "original_form": "하시죠",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.27",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.28",
          "form": "바다에서 하고 싶은데염",
          "original_form": "바다에서 하고 싶은데염",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015138.1.30",
          "form": "바다도 좋구여 ㅋㅋㅋ",
          "original_form": "바다도 좋구여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        }
      ]
