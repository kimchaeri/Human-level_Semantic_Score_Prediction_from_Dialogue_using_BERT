      "utterance": [
        {
          "id": "MDRW2100026290.1.1",
          "form": "그 팀장님 손은 황금손이래요?",
          "original_form": "그 팀장님 손은 황금손이래요?",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026290.1.2",
          "form": "왜 언니만 부려먹어요?",
          "original_form": "왜 언니만 부려먹어요?",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026290.1.3",
          "form": "내가 챙겨주고 싶었던 마음이 후회가 되더라고",
          "original_form": "내가 챙겨주고 싶었던 마음이 후회가 되더라고",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026290.1.4",
          "form": "아니 먹고 남을수도 있고 치우기 싫을수도 있는데 나한테 어떻게 처리하냐고 묻지도 않고",
          "original_form": "아니 먹고 남을수도 있고 치우기 싫을수도 있는데 나한테 어떻게 처리하냐고 묻지도 않고",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026290.1.5",
          "form": "아니 그분은 왜 귀남이래요?",
          "original_form": "아니 그분은 왜 귀남이래요?",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026290.1.6",
          "form": "그냥 그대로 넣어둔게 싫은거야",
          "original_form": "그냥 그대로 넣어둔게 싫은거야",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026290.1.7",
          "form": "일도 무임승차 일벌리기 선수에",
          "original_form": "일도 무임승차 일벌리기 선수에",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026290.1.8",
          "form": "묻기 귀찮았겠죠.",
          "original_form": "묻기 귀찮았겠죠.",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026290.1.9",
          "form": "그래서 내가 외근도 힘들게 하고 왔는데 탕비실 보니까 진짜 한숨이 나오더라고",
          "original_form": "그래서 내가 외근도 힘들게 하고 왔는데 탕비실 보니까 진짜 한숨이 나오더라고",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026290.1.10",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026290.1.11",
          "form": "탕비실은 왜요?",
          "original_form": "탕비실은 왜요?",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026290.1.12",
          "form": "탕비실을 보니까 먹은티도 다 내고 설거지 하는 사람도 없고",
          "original_form": "탕비실을 보니까 먹은티도 다 내고 설거지 하는 사람도 없고",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026290.1.13",
          "form": "손님오면 유리컵에 커피내가는데 그것도 내 업무라고 생각하는지",
          "original_form": "손님오면 유리컵에 커피내가는데 그것도 내 업무라고 생각하는지",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026290.1.14",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026290.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026290.1.16",
          "form": "설거지하는 사람 하나없어",
          "original_form": "설거지하는 사람 하나없어",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026290.1.17",
          "form": "아니 언니가 혼자 3인분 일하네요",
          "original_form": "아니 언니가 혼자 3인분 일하네요",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026290.1.18",
          "form": "그게 당연한거예요?",
          "original_form": "그게 당연한거예요?",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026290.1.19",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026290.1.20",
          "form": "웃긴게 뭐냐면",
          "original_form": "웃긴게 뭐냐면",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026290.1.21",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026290.1.22",
          "form": "그렇게 싱크대에 다 넣어놓고",
          "original_form": "그렇게 싱크대에 다 넣어놓고",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026290.1.23",
          "form": "거기서 설거지는 안하는데 손은 씻어",
          "original_form": "거기서 설거지는 안하는데 손은 씻어",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026290.1.24",
          "form": "손은 화장실이",
          "original_form": "손은 화장실이",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026290.1.25",
          "form": "있는데 왜요?",
          "original_form": "있는데 왜요?",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026290.1.26",
          "form": "화장실에서 안씻어",
          "original_form": "화장실에서 안씻어",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026290.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026290.1.28",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026290.1.29",
          "form": "찌린내나서?",
          "original_form": "찌린내나서?",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026290.1.30",
          "form": "그건 모르겠어 나는 거기서 씻거든 비누가 없는거도 아닌데",
          "original_form": "그건 모르겠어 나는 거기서 씻거든 비누가 없는거도 아닌데",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026290.1.31",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026290.1.32",
          "form": "같은 화장실을 쓰는데 왜그럴까?",
          "original_form": "같은 화장실을 쓰는데 왜그럴까?",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026290.1.33",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026290.1.34",
          "form": "본인손만귀하다",
          "original_form": "본인손만귀하다",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026290.1.35",
          "form": "이런건가",
          "original_form": "이런건가",
          "speaker_id": "1",
          "time": "20210901 21:27"
        }
      ]
    }
