      "utterance": [
        {
          "id": "MDRW2100036168.1.1",
          "form": "사실 제 언니들 중에",
          "original_form": "사실 제 언니들 중에",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036168.1.2",
          "form": "한 명이 미국으로 이민을 갔는데",
          "original_form": "한 명이 미국으로 이민을 갔는데",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036168.1.3",
          "form": "코로나 때문에 거의 2년째 못보고 있어요ㅠㅠ",
          "original_form": "코로나 때문에 거의 2년째 못보고 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036168.1.4",
          "form": "생이별이에요...",
          "original_form": "생이별이에요...",
          "speaker_id": "2",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100036168.1.5",
          "form": "헉....ㅠㅠㅠㅜ 진짜 이게 무슨일이예요ㅠㅠ",
          "original_form": "헉....ㅠㅠㅠㅜ 진짜 이게 무슨일이예요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036168.1.6",
          "form": "줌이나 화상통화밖에 못하겠네요",
          "original_form": "줌이나 화상통화밖에 못하겠네요",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036168.1.7",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036168.1.8",
          "form": "그래서 언니네 보러 갈 겸",
          "original_form": "그래서 언니네 보러 갈 겸",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036168.1.9",
          "form": "미국 너무 가고싶은데",
          "original_form": "미국 너무 가고싶은데",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036168.1.10",
          "form": "이렇게 더 길어질 줄 몰랐어요ㅠ",
          "original_form": "이렇게 더 길어질 줄 몰랐어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100036168.1.11",
          "form": "미국 가시면 언니분도 있을거구 잘다니실수있을텐데...",
          "original_form": "미국 가시면 언니분도 있을거구 잘다니실수있을텐데...",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100036168.1.12",
          "form": "너무 아쉽네요",
          "original_form": "너무 아쉽네요",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036168.1.13",
          "form": "그래도 코로나끝나면 외국에 아는분들만 만나셔도 너무 좋은 여행이겠어요",
          "original_form": "그래도 코로나끝나면 외국에 아는분들만 만나셔도 너무 좋은 여행이겠어요",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036168.1.14",
          "form": "맞아요... 따흐흑",
          "original_form": "맞아요... 따흐흑",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036168.1.15",
          "form": "우와 그렇게 생각하면",
          "original_form": "우와 그렇게 생각하면",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036168.1.16",
          "form": "진짜 그렇네요 ㅎㅎㅎ",
          "original_form": "진짜 그렇네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036168.1.17",
          "form": "갑자기 마음이 따수워져요",
          "original_form": "갑자기 마음이 따수워져요",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036168.1.18",
          "form": "언젠지 몰라서 그렇지 끝나긴 할꺼니까요!",
          "original_form": "언젠지 몰라서 그렇지 끝나긴 할꺼니까요!",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100036168.1.19",
          "form": "외국에 아는분들 많아서 부러워요",
          "original_form": "외국에 아는분들 많아서 부러워요",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100036168.1.20",
          "form": "아 정말 부러워하는 친구들 많더라구요...!! 헤헤",
          "original_form": "아 정말 부러워하는 친구들 많더라구요...!! 헤헤",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036168.1.21",
          "form": "좋은거라고 생각하려구요....",
          "original_form": "좋은거라고 생각하려구요....",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036168.1.22",
          "form": "당장 언니를 못봐서 아쉽지만!!!",
          "original_form": "당장 언니를 못봐서 아쉽지만!!!",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036168.1.23",
          "form": "미국 가게 되면 시카고부터 가게될 것 같은데",
          "original_form": "미국 가게 되면 시카고부터 가게될 것 같은데",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036168.1.24",
          "form": "화려한 시카고가 참 궁금해요 ㅎㅎ",
          "original_form": "화려한 시카고가 참 궁금해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036168.1.25",
          "form": "헉 시카고! 제가 좋아하는 뮤지컬 시카고의 배경이된곳 구경가고싶어요!!",
          "original_form": "헉 시카고! 제가 좋아하는 뮤지컬 시카고의 배경이된곳 구경가고싶어요!!",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036168.1.26",
          "form": "부러울만하죠!",
          "original_form": "부러울만하죠!",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036168.1.27",
          "form": "오오 저도 뮤지컬 시카고 좋아하는데!!!",
          "original_form": "오오 저도 뮤지컬 시카고 좋아하는데!!!",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036168.1.28",
          "form": "저도 진짜 시카고에서 하는 뮤지컬을 보고싶어요",
          "original_form": "저도 진짜 시카고에서 하는 뮤지컬을 보고싶어요",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036168.1.29",
          "form": "총기는 좀 무섭지만....",
          "original_form": "총기는 좀 무섭지만....",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036168.1.30",
          "form": "여행 간 분위기 제대로 만끽할 수 있을거같아요 ㅎㅎㅎ",
          "original_form": "여행 간 분위기 제대로 만끽할 수 있을거같아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036168.1.31",
          "form": "너무 재밌겠네요",
          "original_form": "너무 재밌겠네요",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036168.1.32",
          "form": "지금까지 제가 들은것만해도",
          "original_form": "지금까지 제가 들은것만해도",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036168.1.33",
          "form": "미국,대만,독일인데 여기만다녀와도 일년훌쩍 가겠어요",
          "original_form": "미국,대만,독일인데 여기만다녀와도 일년훌쩍 가겠어요",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036168.1.34",
          "form": "그렇게 말씀해주시니 감사합니다 ㅎㅎ",
          "original_form": "그렇게 말씀해주시니 감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036168.1.35",
          "form": "얼른 비행기 타고 해외로 나가고 싶네요~!",
          "original_form": "얼른 비행기 타고 해외로 나가고 싶네요~!",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036168.1.36",
          "form": "늦어도 내년에는...갈수있겠죠!",
          "original_form": "늦어도 내년에는...갈수있겠죠!",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036168.1.37",
          "form": "그러게요 코로나가 좀 잠잠해져야",
          "original_form": "그러게요 코로나가 좀 잠잠해져야",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036168.1.38",
          "form": "여행을 다닐텐데...",
          "original_form": "여행을 다닐텐데...",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036168.1.39",
          "form": "여행은 고사하고 요즘은 친구들이랑 4명 이상도 모이질 못하니까요...ㅠ",
          "original_form": "여행은 고사하고 요즘은 친구들이랑 4명 이상도 모이질 못하니까요...ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:08"
        }
      ]
