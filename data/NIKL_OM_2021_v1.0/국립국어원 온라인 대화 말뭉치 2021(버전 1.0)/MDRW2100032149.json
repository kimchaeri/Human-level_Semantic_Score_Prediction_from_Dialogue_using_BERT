      "utterance": [
        {
          "id": "MDRW2100032149.1.1",
          "form": "학교 어때??",
          "original_form": "학교 어때??",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032149.1.2",
          "form": "원격수업하나??",
          "original_form": "원격수업하나??",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032149.1.3",
          "form": "굳굳",
          "original_form": "굳굳",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032149.1.4",
          "form": "이제 다음주부터 전면등교!!",
          "original_form": "이제 다음주부터 전면등교!!",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032149.1.5",
          "form": "난 1년동안 계속 전면등교였음...",
          "original_form": "난 1년동안 계속 전면등교였음...",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032149.1.6",
          "form": "올해부턴 야자도 다 하고",
          "original_form": "올해부턴 야자도 다 하고",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032149.1.7",
          "form": "헐... 힘들었겠다",
          "original_form": "헐... 힘들었겠다",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032149.1.8",
          "form": "온클이랑 등교 번갈아가면서 하는 건 좀 힘든게 왔다갔다하면서 책을 가지고 갔다가 다시 가지고 왔다가 해야하니까... 좀 힘들었는데 그래도 전면등교보다 더 하리...",
          "original_form": "온클이랑 등교 번갈아가면서 하는 건 좀 힘든게 왔다갔다하면서 책을 가지고 갔다가 다시 가지고 왔다가 해야하니까... 좀 힘들었는데 그래도 전면등교보다 더 하리...",
          "speaker_id": "2",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100032149.1.9",
          "form": "나는 온클 하면 공부를 아예 안 하게 돼서...",
          "original_form": "나는 온클 하면 공부를 아예 안 하게 돼서...",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100032149.1.10",
          "form": "차라리 학교 가는게 나은듯",
          "original_form": "차라리 학교 가는게 나은듯",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032149.1.11",
          "form": "가도 트위터만 하지만...",
          "original_form": "가도 트위터만 하지만...",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032149.1.12",
          "form": "아 인정!!!!",
          "original_form": "아 인정!!!!",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032149.1.13",
          "form": "온클하면 공부를 진~짜 안 하게 됨",
          "original_form": "온클하면 공부를 진~짜 안 하게 됨",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032149.1.14",
          "form": "등교하면 그래도 수업은 듣는데...",
          "original_form": "등교하면 그래도 수업은 듣는데...",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032149.1.15",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032149.1.16",
          "form": "온클 틀고 영화나 보고...",
          "original_form": "온클 틀고 영화나 보고...",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032149.1.17",
          "form": "나만 그런가...?",
          "original_form": "나만 그런가...?",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032149.1.18",
          "form": "나 컨텐츠 수업이면 과제만 내고 그냥 누워서 트위터만 하게 됨 ㅋㅋㅋ",
          "original_form": "나 컨텐츠 수업이면 과제만 내고 그냥 누워서 트위터만 하게 됨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032149.1.19",
          "form": "실시간이면 모를까",
          "original_form": "실시간이면 모를까",
          "speaker_id": "2",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032149.1.20",
          "form": "우린 실시간이 거의 없었어",
          "original_form": "우린 실시간이 거의 없었어",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032149.1.21",
          "form": "있어도 카메라 켜서 출첵만 하고 과제 내기고",
          "original_form": "있어도 카메라 켜서 출첵만 하고 과제 내기고",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032149.1.22",
          "form": "학교에 동아리는 있음?",
          "original_form": "학교에 동아리는 있음?",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032149.1.23",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032149.1.24",
          "form": "동아리 있어",
          "original_form": "동아리 있어",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032149.1.25",
          "form": "뭐해?",
          "original_form": "뭐해?",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032149.1.26",
          "form": "자율동아리도 하나??",
          "original_form": "자율동아리도 하나??",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032149.1.27",
          "form": "나 도서부! 자율동아리는 우리 학년부터는 그 머냐 아 갑자기 기억이 안 남 세특같은 거 쓰는 거... 거기에 안 들어가서 하는 애들이 거의 없어",
          "original_form": "나 도서부! 자율동아리는 우리 학년부터는 그 머냐 아 갑자기 기억이 안 남 세특같은 거 쓰는 거... 거기에 안 들어가서 하는 애들이 거의 없어",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032149.1.28",
          "form": "우리도 동아리 이름만 기재돼긴 함",
          "original_form": "우리도 동아리 이름만 기재돼긴 함",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032149.1.29",
          "form": "나 1 2학년때는 글쓰기부였고",
          "original_form": "나 1 2학년때는 글쓰기부였고",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032149.1.30",
          "form": "지금은 활동 없이 맨날 자습이다",
          "original_form": "지금은 활동 없이 맨날 자습이다",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032149.1.31",
          "form": "맞어 3학년 되면 동아리는 이름만 있고 다 자습한다고 들었어...",
          "original_form": "맞어 3학년 되면 동아리는 이름만 있고 다 자습한다고 들었어...",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032149.1.32",
          "form": "아예 동아리 시간이 없어??",
          "original_form": "아예 동아리 시간이 없어??",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032149.1.33",
          "form": "동아리 딱 한 번 했어",
          "original_form": "동아리 딱 한 번 했어",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032149.1.34",
          "form": "그리고 짧게 보고서 써서 내고",
          "original_form": "그리고 짧게 보고서 써서 내고",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032149.1.35",
          "form": "세특 신경쓰는 애들은 좀 길게 쓰고",
          "original_form": "세특 신경쓰는 애들은 좀 길게 쓰고",
          "speaker_id": "1",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032149.1.36",
          "form": "나는 걍 10분동안 대충 써서 냄",
          "original_form": "나는 걍 10분동안 대충 써서 냄",
          "speaker_id": "1",
          "time": "20210905 13:38"
        }
      ]
    }
