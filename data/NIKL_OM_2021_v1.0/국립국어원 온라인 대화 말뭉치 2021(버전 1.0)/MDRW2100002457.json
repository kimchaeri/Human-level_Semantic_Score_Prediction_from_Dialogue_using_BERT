      "utterance": [
        {
          "id": "MDRW2100002457.1.1",
          "form": "좋아하는 스포츠가 있으신가요?",
          "original_form": "좋아하는 스포츠가 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002457.1.2",
          "form": "최근엔 양궁이요",
          "original_form": "최근엔 양궁이요",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002457.1.3",
          "form": "빠르고 빨리끝나고",
          "original_form": "빠르고 빨리끝나고",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002457.1.4",
          "form": "취향저격",
          "original_form": "취향저격",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002457.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋk승질머리에 딱이라는 양궁",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋk승질머리에 딱이라는 양궁",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002457.1.6",
          "form": "맞아요 쏘자마자 점수 알수있고",
          "original_form": "맞아요 쏘자마자 점수 알수있고",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002457.1.7",
          "form": "진짜 제일재밌더라구요.",
          "original_form": "진짜 제일재밌더라구요.",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002457.1.8",
          "form": "심판이 개입할일도 없구요",
          "original_form": "심판이 개입할일도 없구요",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002457.1.9",
          "form": "뛰라노님은 뭐 좋아하세요?",
          "original_form": "뛰라노님은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002457.1.10",
          "form": "저도 이번 올림픽때 양궁 정말 재밌게 봤어요",
          "original_form": "저도 이번 올림픽때 양궁 정말 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002457.1.11",
          "form": "배구도 재밌었고요!",
          "original_form": "배구도 재밌었고요!",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002457.1.12",
          "form": "배구는 진짜 인정",
          "original_form": "배구는 진짜 인정",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002457.1.13",
          "form": "감동적인 역전잊을 수가 없어요",
          "original_form": "감동적인 역전잊을 수가 없어요",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002457.1.14",
          "form": "타격감도 엄청나고 선수들의 열정이 느껴져서 너무 좋았어요",
          "original_form": "타격감도 엄청나고 선수들의 열정이 느껴져서 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002457.1.15",
          "form": "실제 경기 관람도 가보고싶을정도로",
          "original_form": "실제 경기 관람도 가보고싶을정도로",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002457.1.16",
          "form": "진짜 코로나로 지친 일상에 활력소가된거같아요",
          "original_form": "진짜 코로나로 지친 일상에 활력소가된거같아요",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002457.1.17",
          "form": "얼마만에 느껴보는 열정과 열기였는지",
          "original_form": "얼마만에 느껴보는 열정과 열기였는지",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002457.1.18",
          "form": "맞아요 많은 도움이 됐어요 덕분에 즐거운 시간을 보냈어요 저도",
          "original_form": "맞아요 많은 도움이 됐어요 덕분에 즐거운 시간을 보냈어요 저도",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002457.1.19",
          "form": "실제로 스포츠 경기 관람하러 가보신적 있나요?",
          "original_form": "실제로 스포츠 경기 관람하러 가보신적 있나요?",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002457.1.20",
          "form": "기회가 없어서.. 야구는 본 적 있어요!",
          "original_form": "기회가 없어서.. 야구는 본 적 있어요!",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002457.1.21",
          "form": "저는 아주 어릴때 축구 보러 간게 전부예요..!",
          "original_form": "저는 아주 어릴때 축구 보러 간게 전부예요..!",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002457.1.22",
          "form": "야구 좋아하시나요?",
          "original_form": "야구 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002457.1.23",
          "form": "아빠가 좋아하셔서 자주 따라갔어요",
          "original_form": "아빠가 좋아하셔서 자주 따라갔어요",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002457.1.24",
          "form": "아빠가 야구팬이거든요",
          "original_form": "아빠가 야구팬이거든요",
          "speaker_id": "2",
          "time": "20210820 12:40"
        }
      ]
    }
