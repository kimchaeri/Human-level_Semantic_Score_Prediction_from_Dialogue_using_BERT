      "utterance": [
        {
          "id": "MDRW2100029633.1.1",
          "form": "화분키우기같은 게임인데..",
          "original_form": "화분키우기같은 게임인데..",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.2",
          "form": "남자 요정들?",
          "original_form": "남자 요정들?",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.3",
          "form": "열매도 열리나요",
          "original_form": "열매도 열리나요",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.4",
          "form": "나오는 게임이 있었던거같아요",
          "original_form": "나오는 게임이 있었던거같아요",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.5",
          "form": "춤도 추나요",
          "original_form": "춤도 추나요",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.6",
          "form": "그런거같네요",
          "original_form": "그런거같네요",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.7",
          "form": "취향별로 모으고",
          "original_form": "취향별로 모으고",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.8",
          "form": "오 남자요정",
          "original_form": "오 남자요정",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.9",
          "form": "근육있나요",
          "original_form": "근육있나요",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029633.1.10",
          "form": "취향따라 다르지않을까요",
          "original_form": "취향따라 다르지않을까요",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.11",
          "form": "프린세스메이커도 지금생각해보면",
          "original_form": "프린세스메이커도 지금생각해보면",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.12",
          "form": "여리여리 미소년이",
          "original_form": "여리여리 미소년이",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.13",
          "form": "많았던거같아요",
          "original_form": "많았던거같아요",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.14",
          "form": "프린세스메이커 좀 이상했죠",
          "original_form": "프린세스메이커 좀 이상했죠",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.15",
          "form": "맞아요 아빠가",
          "original_form": "맞아요 아빠가",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.16",
          "form": "딸을 키운다니",
          "original_form": "딸을 키운다니",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.17",
          "form": "미소년은 좋은데",
          "original_form": "미소년은 좋은데",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.18",
          "form": "이상한쪽으로 ㅋㅋㅋㅋ",
          "original_form": "이상한쪽으로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.19",
          "form": "그래서 그냥 아케이드나",
          "original_form": "그래서 그냥 아케이드나",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.20",
          "form": "퍼즐쪽이",
          "original_form": "퍼즐쪽이",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.21",
          "form": "취향이네요",
          "original_form": "취향이네요",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.22",
          "form": "네 저도 퍼즐좋아요",
          "original_form": "네 저도 퍼즐좋아요",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029633.1.23",
          "form": "그 중에서도 저는 피쉬돔이라는 게임을 좋아해요",
          "original_form": "그 중에서도 저는 피쉬돔이라는 게임을 좋아해요",
          "speaker_id": "1",
          "time": "20210903 11:07"
        }
      ]
