      "utterance": [
        {
          "id": "MDRW2100037463.1.1",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037463.1.2",
          "form": "이번주 토요일 모임잇는데",
          "original_form": "이번주 토요일 모임잇는데",
          "speaker_id": "1",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037463.1.3",
          "form": "수~금 비온다는데",
          "original_form": "수~금 비온다는데",
          "speaker_id": "2",
          "time": "20210901 00:24"
        },
        {
          "id": "MDRW2100037463.1.4",
          "form": "엉 근데 토요일은 맑음",
          "original_form": "엉 근데 토요일은 맑음",
          "speaker_id": "1",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037463.1.5",
          "form": "금요일에 다시 봐야한다",
          "original_form": "금요일에 다시 봐야한다",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037463.1.6",
          "form": "응 안그래도 금요일 날씨보고 토요일 모임할지 최종결정",
          "original_form": "응 안그래도 금요일 날씨보고 토요일 모임할지 최종결정",
          "speaker_id": "1",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037463.1.7",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "2",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037463.1.8",
          "form": "토요일에도 더워서 야외에서 만나도 안추울듯",
          "original_form": "토요일에도 더워서 야외에서 만나도 안추울듯",
          "speaker_id": "1",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037463.1.9",
          "form": "야외에서 볼려고",
          "original_form": "야외에서 볼려고",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037463.1.10",
          "form": "응 좀 그늘지고 한곳에 있으면 더워도 참을수 잇을듯하다",
          "original_form": "응 좀 그늘지고 한곳에 있으면 더워도 참을수 잇을듯하다",
          "speaker_id": "1",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037463.1.11",
          "form": "그래 선크링도 잘 바르고",
          "original_form": "그래 선크링도 잘 바르고",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037463.1.12",
          "form": "응 양산도 가져가서 쓰고 있을려고 ㅋㅋ",
          "original_form": "응 양산도 가져가서 쓰고 있을려고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037463.1.13",
          "form": "모자 쓰면 괜찮을텐데",
          "original_form": "모자 쓰면 괜찮을텐데",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037463.1.14",
          "form": "모자도 쓰고 이중으로 자외선 차단",
          "original_form": "모자도 쓰고 이중으로 자외선 차단",
          "speaker_id": "1",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037463.1.15",
          "form": "대단하네",
          "original_form": "대단하네",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037463.1.16",
          "form": "갑자기 날씨가 너무 추워짐",
          "original_form": "갑자기 날씨가 너무 추워짐",
