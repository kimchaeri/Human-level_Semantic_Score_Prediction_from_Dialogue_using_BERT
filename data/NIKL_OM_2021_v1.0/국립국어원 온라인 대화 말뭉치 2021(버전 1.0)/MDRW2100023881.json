      "utterance": [
        {
          "id": "MDRW2100023881.1.1",
          "form": "열라면 순두부찌개 아세용?",
          "original_form": "열라면 순두부찌개 아세용?",
          "speaker_id": "1",
          "time": "20210830 19:39"
        },
        {
          "id": "MDRW2100023881.1.2",
          "form": "아뇨 그거 맛있나요?",
          "original_form": "아뇨 그거 맛있나요?",
          "speaker_id": "2",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023881.1.3",
          "form": "네네! 맛있어요ㅋㅋㅋ",
          "original_form": "네네! 맛있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023881.1.4",
          "form": "담에 도전해봐야겠네요",
          "original_form": "담에 도전해봐야겠네요",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023881.1.5",
          "form": "순두부니 순한가요?",
          "original_form": "순두부니 순한가요?",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023881.1.6",
          "form": "네 엄청 순해요ㅋㅋ ㅡ",
          "original_form": "네 엄청 순해요ㅋㅋ ㅡ",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023881.1.7",
          "form": "고소하구요",
          "original_form": "고소하구요",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023881.1.8",
          "form": "열라면은 매웠던걸로 기억하는데",
          "original_form": "열라면은 매웠던걸로 기억하는데",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023881.1.9",
          "form": "덜매운가보네요",
          "original_form": "덜매운가보네요",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023881.1.10",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023881.1.11",
          "form": "라면중에서도 매운편이라",
          "original_form": "라면중에서도 매운편이라",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023881.1.12",
          "form": "순두부 넣으면 일반 라면같아져요",
          "original_form": "순두부 넣으면 일반 라면같아져요",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023881.1.13",
          "form": "세상에  순두부와 조합이 제법괜찮겠네요",
          "original_form": "세상에  순두부와 조합이 제법괜찮겠네요",
          "speaker_id": "2",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023881.1.14",
          "form": "가장 좋아하시는 음식 있으세요?",
          "original_form": "가장 좋아하시는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023881.1.15",
          "form": "제 별명이 name2에요 \"name2\"....라는 뜻이죠ㅎㅎㅎㅎ",
          "original_form": "제 별명이 &name2&에요 \"&name2&\"....라는 뜻이죠ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023881.1.16",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:54"
        },
        {
          "id": "MDRW2100023881.1.17",
          "form": "가리는거 없으신거 넘 좋아요!",
          "original_form": "가리는거 없으신거 넘 좋아요!",
          "speaker_id": "1",
          "time": "20210830 19:54"
        },
        {
          "id": "MDRW2100023881.1.18",
          "form": "전 편식이 심해서ㅠㅠ",
          "original_form": "전 편식이 심해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 19:54"
        },
        {
          "id": "MDRW2100023881.1.19",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ얼마나 좋아요\r\n내가 좋아하는것과 싫어하는것에 구분이 뚜렷하신거잖아요 ㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ얼마나 좋아요\r\n내가 좋아하는것과 싫어하는것에 구분이 뚜렷하신거잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:56"
        },
        {
          "id": "MDRW2100023881.1.20",
          "form": "엇 아니에용ㅎㅎㅎ 먹는거보면 복들어온다 칭찬 많이 들으셨을것같아요",
          "original_form": "엇 아니에용ㅎㅎㅎ 먹는거보면 복들어온다 칭찬 많이 들으셨을것같아요",
          "speaker_id": "1",
          "time": "20210830 19:56"
        },
        {
          "id": "MDRW2100023881.1.21",
          "form": "어른들한테 예쁨받으실 스타일!",
          "original_form": "어른들한테 예쁨받으실 스타일!",
