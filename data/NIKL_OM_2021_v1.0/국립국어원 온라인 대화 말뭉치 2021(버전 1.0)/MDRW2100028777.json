      "utterance": [
        {
          "id": "MDRW2100028777.1.1",
          "form": "저보다 영 하시지 않습니까",
          "original_form": "저보다 영 하시지 않습니까",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028777.1.2",
          "form": "그정도면 날아다닙니다",
          "original_form": "그정도면 날아다닙니다",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028777.1.3",
          "form": "저는 옥수수차를 좋아했서요",
          "original_form": "저는 옥수수차를 좋아했서요",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028777.1.4",
          "form": "영ㅋㅋㅋ",
          "original_form": "영ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028777.1.5",
          "form": "옥수수차도 맛있죠ㅎㅎ",
          "original_form": "옥수수차도 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028777.1.6",
          "form": "영어 실력이 영아이네요",
          "original_form": "영어 실력이 영아이네요",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028777.1.7",
          "form": "제가 영어가 약하드라구요",
          "original_form": "제가 영어가 약하드라구요",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028777.1.8",
          "form": "그리고 손에 쥐어지는 합격 목걸이",
          "original_form": "그리고 손에 쥐어지는 합격 목걸이",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028777.1.9",
          "form": "아 이거 짤 첨부해야 하는데ㅜㅇㅜ",
          "original_form": "아 이거 짤 첨부해야 하는데ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028777.1.10",
          "form": "아 짤 가지고싶다",
          "original_form": "아 짤 가지고싶다",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028777.1.11",
          "form": "특히 제리짤",
          "original_form": "특히 제리짤",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028777.1.12",
          "form": "오 졸귀",
          "original_form": "오 졸귀",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028777.1.13",
          "form": "아깝네요",
          "original_form": "아깝네요",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028777.1.14",
          "form": "블라인드에서",
          "original_form": "블라인드에서",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028777.1.15",
          "form": "가끔 줍줍했는데",
          "original_form": "가끔 줍줍했는데",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028777.1.16",
          "form": "주시면 잘 받아쓸건데 ㅠ",
          "original_form": "주시면 잘 받아쓸건데 ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028777.1.17",
          "form": "젤리먹고싶네요",
          "original_form": "젤리먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028777.1.18",
          "form": "ㅜㅇㅜ 합격목걸이도 텍스트가 아닌 짤이어야 하는데",
          "original_form": "ㅜㅇㅜ 합격목걸이도 텍스트가 아닌 짤이어야 하는데",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028777.1.19",
          "form": "아니ㅋㅋ",
          "original_form": "아니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028777.1.20",
          "form": "식음료라 ㅠ",
          "original_form": "식음료라 ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028777.1.21",
          "form": "무슨 젤리 좋아하나요 하리보?",
          "original_form": "무슨 젤리 좋아하나요 하리보?",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028777.1.22",
          "form": "하리보 그",
          "original_form": "하리보 그",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028777.1.23",
          "form": "안에 쨈같은거 든거 있져?",
          "original_form": "안에 쨈같은거 든거 있져?",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028777.1.24",
          "form": "흰색 바탕에",
          "original_form": "흰색 바탕에",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028777.1.25",
          "form": "네네 그거 맛있죠ㅋ",
          "original_form": "네네 그거 맛있죠ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028777.1.26",
          "form": "전 그거 좋아하구요",
          "original_form": "전 그거 좋아하구요",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028777.1.27",
          "form": "그리고 마이구미도 아이셔 젤리?",
          "original_form": "그리고 마이구미도 아이셔 젤리?",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028777.1.28",
          "form": "마이구미 맛있죠!",
          "original_form": "마이구미 맛있죠!",
          "speaker_id": "1",
          "time": "20210902 22:02"
        }
      ]
    }
