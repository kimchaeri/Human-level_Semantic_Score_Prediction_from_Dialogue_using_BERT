          "time": "20210906 01:20"
        },
        {
          "id": "MDRW2100032842.1.3",
          "form": "데려다줄까???",
          "original_form": "데려다줄까???",
          "speaker_id": "2",
          "time": "20210906 01:20"
        },
        {
          "id": "MDRW2100032842.1.4",
          "form": "맨날 가는데 매일 데려다주려고??ㅋㅋㅋㅋ",
          "original_form": "맨날 가는데 매일 데려다주려고??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 01:20"
        },
        {
          "id": "MDRW2100032842.1.5",
          "form": "아니 버스타고 갈거야",
          "original_form": "아니 버스타고 갈거야",
          "speaker_id": "1",
          "time": "20210906 01:20"
        },
        {
          "id": "MDRW2100032842.1.6",
          "form": "신림까지 한번에 가는거 있어",
          "original_form": "신림까지 한번에 가는거 있어",
          "speaker_id": "2",
          "time": "20210906 01:21"
        },
        {
          "id": "MDRW2100032842.1.7",
          "form": "넹 그거 타고 갈거여요",
          "original_form": "넹 그거 타고 갈거여요",
          "speaker_id": "1",
          "time": "20210906 01:23"
        },
        {
          "id": "MDRW2100032842.1.8",
          "form": "아침먹고 갈거야????",
          "original_form": "아침먹고 갈거야????",
          "speaker_id": "2",
          "time": "20210906 01:23"
        },
        {
          "id": "MDRW2100032842.1.9",
          "form": "아니 난 잠을 더 자고싶어...",
          "original_form": "아니 난 잠을 더 자고싶어...",
          "speaker_id": "1",
          "time": "20210906 01:23"
        },
        {
          "id": "MDRW2100032842.1.10",
          "form": "몇시에 끝나???",
          "original_form": "몇시에 끝나???",
