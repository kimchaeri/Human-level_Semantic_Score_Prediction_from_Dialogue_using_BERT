      "utterance": [
        {
          "id": "MDRW2100005199.1.1",
          "form": "요즘 눈이 너무 뻑뻑한 것 같아",
          "original_form": "요즘 눈이 너무 뻑뻑한 것 같아",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005199.1.2",
          "form": "핸드폰 너무 많이 봐서 그렇지",
          "original_form": "핸드폰 너무 많이 봐서 그렇지",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005199.1.3",
          "form": "그냥 있어도 눈물나고 시리고 그러네",
          "original_form": "그냥 있어도 눈물나고 시리고 그러네",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005199.1.4",
          "form": "안구건조인가보다",
          "original_form": "안구건조인가보다",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005199.1.5",
          "form": "아침에 일어나도 엄청 뻑뻑해",
          "original_form": "아침에 일어나도 엄청 뻑뻑해",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005199.1.6",
          "form": "인공눈물 수시로 넣어봐",
          "original_form": "인공눈물 수시로 넣어봐",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005199.1.7",
          "form": "안구건조는 완치가 없겠지?",
          "original_form": "안구건조는 완치가 없겠지?",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005199.1.8",
          "form": "없지.. 한번 생기면 꾸준히 관리하는 수밖에",
          "original_form": "없지.. 한번 생기면 꾸준히 관리하는 수밖에",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005199.1.9",
          "form": "눈 안에 돌이 굴러다니는 느낌이야",
          "original_form": "눈 안에 돌이 굴러다니는 느낌이야",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005199.1.10",
          "form": "많이 불편하겠네.. 심하면 병원에 가보던지",
          "original_form": "많이 불편하겠네.. 심하면 병원에 가보던지",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005199.1.11",
          "form": "좀 더 보고.. 인공눈물 넣으면서 버텨보다가",
          "original_form": "좀 더 보고.. 인공눈물 넣으면서 버텨보다가",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005199.1.12",
          "form": "일부러 버티지마~ 증상 보이면 얼른 가는 게 나아",
          "original_form": "일부러 버티지마~ 증상 보이면 얼른 가는 게 나아",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005199.1.13",
          "form": "가도 딱히 처방해주는 게 없을 것 같은데",
          "original_form": "가도 딱히 처방해주는 게 없을 것 같은데",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005199.1.14",
          "form": "머리 한 번 자르러 가야되는데",
          "original_form": "머리 한 번 자르러 가야되는데",
