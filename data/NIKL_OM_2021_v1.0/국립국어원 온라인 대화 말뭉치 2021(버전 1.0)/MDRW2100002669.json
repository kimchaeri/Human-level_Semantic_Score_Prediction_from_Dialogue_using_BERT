      "utterance": [
        {
          "id": "MDRW2100002669.1.1",
          "form": "점심 식사는 하셨나용?",
          "original_form": "점심 식사는 하셨나용?",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002669.1.2",
          "form": "아뇽.....ㅎ.ㅎ..",
          "original_form": "아뇽.....ㅎ.ㅎ..",
          "speaker_id": "2",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002669.1.3",
          "form": "집가서 순두부찌개",
          "original_form": "집가서 순두부찌개",
          "speaker_id": "2",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002669.1.4",
          "form": "해머글라구염",
          "original_form": "해머글라구염",
          "speaker_id": "2",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002669.1.5",
          "form": "우왕 순두부찌개",
          "original_form": "우왕 순두부찌개",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002669.1.6",
          "form": "밀키트에요? 아님 직접? ㅋㅋ",
          "original_form": "밀키트에요? 아님 직접? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002669.1.7",
          "form": "직접이면 망하죠 ^^...",
          "original_form": "직접이면 망하죠 ^^...",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002669.1.8",
          "form": "순찌 양념사왔으여 ㅋㅋㅋ",
          "original_form": "순찌 양념사왔으여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002669.1.9",
          "form": "ㅋㅋ순찌 은근 쉬울거 같은데\r\n맨날 망하는 메뉴라ㅜㅜ\r\n저는 더 이상 안해먹어여",
          "original_form": "ㅋㅋ순찌 은근 쉬울거 같은데\r\n맨날 망하는 메뉴라ㅜㅜ\r\n저는 더 이상 안해먹어여",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002669.1.10",
          "form": "양념으로하면 그럴싸하나염?",
          "original_form": "양념으로하면 그럴싸하나염?",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002669.1.11",
          "form": "넹 괜춘더라구요ㅠㅋㅋㅋ",
          "original_form": "넹 괜춘더라구요ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002669.1.12",
          "form": "간단해얌ㅋ그리고",
          "original_form": "간단해얌ㅋ그리고",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002669.1.13",
          "form": "제일중요한거 간단 !",
          "original_form": "제일중요한거 간단 !",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002669.1.14",
          "form": "ㅋㅋㅋ간단하고 맛있으면 끝이져",
          "original_form": "ㅋㅋㅋ간단하고 맛있으면 끝이져",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002669.1.15",
          "form": "불금인데 저녁은 특식 드시나여?",
          "original_form": "불금인데 저녁은 특식 드시나여?",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002669.1.16",
          "form": "걍 엄니가사온...",
          "original_form": "걍 엄니가사온...",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002669.1.17",
          "form": "소시지나 구워먹어야댐...",
          "original_form": "소시지나 구워먹어야댐...",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002669.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002669.1.19",
          "form": "먹을게별ㄹ로 없네염",
          "original_form": "먹을게별ㄹ로 없네염",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002669.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002669.1.21",
          "form": "소시지 맛있겠네염ㅋㅋㅋㅋ",
          "original_form": "소시지 맛있겠네염ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002669.1.22",
          "form": "아님 불금인데 치키니 드세요ㅋㅋ",
          "original_form": "아님 불금인데 치키니 드세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002669.1.23",
          "form": "수욜에",
          "original_form": "수욜에",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002669.1.24",
          "form": "이미 쿠폰으로",
          "original_form": "이미 쿠폰으로",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002669.1.25",
          "form": "치퀸먹었슴돳 ㅋㅋ",
          "original_form": "치퀸먹었슴돳 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002669.1.26",
          "form": "와 벌써 그 치키니 드셨군영ㅋㅋ",
          "original_form": "와 벌써 그 치키니 드셨군영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002669.1.27",
          "form": "뿌링클이었나여?ㅋㅋ",
          "original_form": "뿌링클이었나여?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002669.1.28",
