      "utterance": [
        {
          "id": "MDRW2100002501.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002501.1.2",
          "form": "너무 좋아해요!",
          "original_form": "너무 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002501.1.3",
          "form": "저는 백패킹도 자주다녓어요",
          "original_form": "저는 백패킹도 자주다녓어요",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002501.1.4",
          "form": "오 어디로가보셨나요?",
          "original_form": "오 어디로가보셨나요?",
          "speaker_id": "1",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002501.1.5",
          "form": "여주에 노지나",
          "original_form": "여주에 노지나",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002501.1.6",
          "form": "산같은곳도 가고",
          "original_form": "산같은곳도 가고",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002501.1.7",
          "form": "지금은 애기키우느라 못간지 좀 됫네요ㅠ",
          "original_form": "지금은 애기키우느라 못간지 좀 됫네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002501.1.8",
          "form": "와 애기키우시는구나 대박이네요ㅠㅠ",
          "original_form": "와 애기키우시는구나 대박이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002501.1.9",
          "form": "힘드시겠어요ㅠ",
          "original_form": "힘드시겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002501.1.10",
          "form": "그러게요ㅠ 놀던때가 좋앗는데요",
          "original_form": "그러게요ㅠ 놀던때가 좋앗는데요",
          "speaker_id": "2",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002501.1.11",
          "form": "여행자주다니세요?",
          "original_form": "여행자주다니세요?",
          "speaker_id": "2",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002501.1.12",
          "form": "저도 요즘은코로나땜에",
          "original_form": "저도 요즘은코로나땜에",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002501.1.13",
          "form": "못가네요ㅠㅠ",
          "original_form": "못가네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002501.1.14",
          "form": "가고싶어도",
          "original_form": "가고싶어도",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002501.1.15",
          "form": "그러게요ㅠ 코로나",
          "original_form": "그러게요ㅠ 코로나",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002501.1.16",
          "form": "저는 괌 가고싶더라고요ㅠㅠ",
          "original_form": "저는 괌 가고싶더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002501.1.17",
          "form": "괌 좋죠ㅜㅜ 가보고싶어요",
          "original_form": "괌 좋죠ㅜㅜ 가보고싶어요",
          "speaker_id": "2",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002501.1.18",
          "form": "저도요ㅠㅠ 글고 싱가폴도요",
          "original_form": "저도요ㅠㅠ 글고 싱가폴도요",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002501.1.19",
          "form": "진짜로",
          "original_form": "진짜로",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002501.1.20",
          "form": "저는 스위스요!!",
          "original_form": "저는 스위스요!!",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002501.1.21",
          "form": "오 스위스좋네요 저도유럽쪽 가보고싶어요!",
          "original_form": "오 스위스좋네요 저도유럽쪽 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002501.1.22",
          "form": "버킷리스트에요ㅎ",
          "original_form": "버킷리스트에요ㅎ",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002501.1.23",
          "form": "꼭 이루실거예요!",
          "original_form": "꼭 이루실거예요!",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002501.1.24",
          "form": "저는 신혼여행으로 동유럽 다녀왓는데 북유럽쪽도 가보고싶어요",
          "original_form": "저는 신혼여행으로 동유럽 다녀왓는데 북유럽쪽도 가보고싶어요",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002501.1.25",
          "form": "오 유렵으로신혼여행너무좋네요",
          "original_form": "오 유렵으로신혼여행너무좋네요",
          "speaker_id": "1",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002501.1.26",
          "form": "그냥 지금은 국내라도 ㅇㅓ디든 자유롭게 가고싶네요!!!!!",
          "original_form": "그냥 지금은 국내라도 ㅇㅓ디든 자유롭게 가고싶네요!!!!!",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002501.1.27",
          "form": "얼마간다녀오셨나요??!",
          "original_form": "얼마간다녀오셨나요??!",
          "speaker_id": "1",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002501.1.28",
          "form": "2주다녀왓엇어요",
          "original_form": "2주다녀왓엇어요",
          "speaker_id": "2",
          "time": "20210820 13:38"
        }
      ]
    }
  ]
