        },
        {
          "id": "MDRW2100007358.1.3",
          "form": "재밌습니다",
          "original_form": "재밌습니다",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007358.1.4",
          "form": "솔리테어구나",
          "original_form": "솔리테어구나",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007358.1.5",
          "form": "이거 재밌어요",
          "original_form": "이거 재밌어요",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007358.1.6",
          "form": "카드게임이라고 했었지?",
          "original_form": "카드게임이라고 했었지?",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007358.1.7",
          "form": "게임 이름을 잘 안보고 다녀서",
          "original_form": "게임 이름을 잘 안보고 다녀서",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007358.1.8",
          "form": "윈도우 10에도 있을까?",
          "original_form": "윈도우 10에도 있을까?",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007358.1.9",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007358.1.10",
          "form": "영어로 쳐야 나와요 근데",
          "original_form": "영어로 쳐야 나와요 근데",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007358.1.11",
          "form": "마이크로 소프트 솔리테어 어쩌구",
          "original_form": "마이크로 소프트 솔리테어 어쩌구",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007358.1.12",
          "form": "뭐 따로 설치해야해?",
          "original_form": "뭐 따로 설치해야해?",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007358.1.13",
          "form": "ㄴㄴ 설치할거없어요",
          "original_form": "ㄴㄴ 설치할거없어요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007358.1.14",
          "form": "근데 전 폰으로",
          "original_form": "근데 전 폰으로",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007358.1.15",
          "form": "솔리테어 덱다웃이라고",
          "original_form": "솔리테어 덱다웃이라고",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007358.1.16",
          "form": "어플받은게 훨씬 귀여워서 좋더라고요",
          "original_form": "어플받은게 훨씬 귀여워서 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007358.1.17",
          "form": "그리고ㅠ이게 컴퓨터보다 쉬움...",
          "original_form": "그리고ㅠ이게 컴퓨터보다 쉬움...",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007358.1.18",
          "form": "앗앗 나 이거 지금 검색해봤는데",
          "original_form": "앗앗 나 이거 지금 검색해봤는데",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007358.1.19",
          "form": "특급은 도저히 못하겠더라고요 ...",
          "original_form": "특급은 도저히 못하겠더라고요 ...",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007358.1.20",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007358.1.21",
          "form": "뭔지 알아!",
          "original_form": "뭔지 알아!",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007358.1.22",
          "form": "한때 몇시간 씩 했었어!",
          "original_form": "한때 몇시간 씩 했었어!",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007358.1.23",
          "form": "이거구나 ㅋㅋㅋ",
          "original_form": "이거구나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007358.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ넹넹",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ넹넹",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.25",
          "form": "컴으로 하는거 너무 자비없고 어려움여...",
          "original_form": "컴으로 하는거 너무 자비없고 어려움여...",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.26",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.27",
          "form": "어플이 귀엽다니 어플로 해야징",
          "original_form": "어플이 귀엽다니 어플로 해야징",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.28",
          "form": "모바일 솔리테어로 실력쌓고",
          "original_form": "모바일 솔리테어로 실력쌓고",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.29",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.30",
          "form": "다시 컴으로ㅠ해야지",
          "original_form": "다시 컴으로ㅠ해야지",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.31",
          "form": "솔리테어 덱다웃 까세요",
          "original_form": "솔리테어 덱다웃 까세요",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.32",
          "form": "고양이 스킨있는데",
          "original_form": "고양이 스킨있는데",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.33",
          "form": "귀엽습니다",
          "original_form": "귀엽습니다",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.34",
          "form": "객기넘치게 컴퓨터한테 어려움으로 싸움걸었다가",
          "original_form": "객기넘치게 컴퓨터한테 어려움으로 싸움걸었다가",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007358.1.35",
          "form": "허망한 거지꼴으로",
          "original_form": "허망한 거지꼴으로",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007358.1.36",
          "form": "되돌아나왔어",
          "original_form": "되돌아나왔어",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007358.1.37",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007358.1.38",
          "form": "컴퓨터 난이도ㅠ너무 높음 ㅠㅠㅠㅠㅠ",
          "original_form": "컴퓨터 난이도ㅠ너무 높음 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007358.1.39",
          "form": "또르르...",
          "original_form": "또르르...",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007358.1.40",
          "form": "알파고는 사람을 봐주지 않더라구",
          "original_form": "알파고는 사람을 봐주지 않더라구",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007358.1.41",
          "form": "응...",
          "original_form": "응...",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007358.1.42",
          "form": "매정하더라구...",
          "original_form": "매정하더라구...",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007358.1.43",
          "form": "맞아요 ..ㅎㅎ...",
          "original_form": "맞아요 ..ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007358.1.44",
          "form": "근데 무슨 난이도를 그랜드마스터인가",
          "original_form": "근데 무슨 난이도를 그랜드마스터인가",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007358.1.45",
          "form": "엄청 높게까지 나눠놔서",
          "original_form": "엄청 높게까지 나눠놔서",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007358.1.46",
          "form": "두렵더라고요",
          "original_form": "두렵더라고요",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007358.1.47",
          "form": "그정도로 단계를 세분화할수있단말야...??",
          "original_form": "그정도로 단계를 세분화할수있단말야...??",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007358.1.48",
          "form": "헉 세상에 그렇게 게임 고수가 있단 말이야?",
          "original_form": "헉 세상에 그렇게 게임 고수가 있단 말이야?",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007358.1.49",
          "form": "난 중급같은 난이도에도 고전하는데",
          "original_form": "난 중급같은 난이도에도 고전하는데",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007358.1.50",
          "form": "그런거 보면 쪼꼼 슬퍼져...",
          "original_form": "그런거 보면 쪼꼼 슬퍼져...",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007358.1.51",
          "form": "그니까요...",
          "original_form": "그니까요...",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007358.1.52",
          "form": "전 요행으로 힌트쓰고 해서",
          "original_form": "전 요행으로 힌트쓰고 해서",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007358.1.53",
          "form": "상급까진깼는데",
          "original_form": "상급까진깼는데",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007358.1.54",
          "form": "특급은 힌트써도 못깨겠더라도요 ㅋㅋㅋㅋㅋ",
          "original_form": "특급은 힌트써도 못깨겠더라도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007358.1.55",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 이런 게임류도 프로인 사람이 있는 걸까?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 이런 게임류도 프로인 사람이 있는 걸까?",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007358.1.56",
          "form": "난... 못하겠던데...",
          "original_form": "난... 못하겠던데...",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007358.1.57",
          "form": "신기해",
          "original_form": "신기해",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007358.1.58",
          "form": "솔리테어 프로는 본적없지만...",
          "original_form": "솔리테어 프로는 본적없지만...",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007358.1.59",
          "form": "난... 솔리테어 고수... ㅋ...초급 솔리테어따윈 시.시.해.",
          "original_form": "난... 솔리테어 고수... ㅋ...초급 솔리테어따윈 시.시.해.",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007358.1.60",
          "form": "이런 사람들이",
          "original_form": "이런 사람들이",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007358.1.61",
          "form": "그런 단계를 하는게 아닐까요...",
          "original_form": "그런 단계를 하는게 아닐까요...",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007358.1.62",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
