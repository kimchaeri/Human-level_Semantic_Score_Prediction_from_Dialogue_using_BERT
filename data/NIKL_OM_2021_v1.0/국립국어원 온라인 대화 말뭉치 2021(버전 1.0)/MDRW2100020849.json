      "utterance": [
        {
          "id": "MDRW2100020849.1.1",
          "form": "드라마 좋아하세요?",
          "original_form": "드라마 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020849.1.2",
          "form": "요즘엔 잘 안보지만 좋아해요!",
          "original_form": "요즘엔 잘 안보지만 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020849.1.3",
          "form": "마지막으로 본게 뭐에여",
          "original_form": "마지막으로 본게 뭐에여",
          "speaker_id": "2",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020849.1.4",
          "form": "보이스4요!",
          "original_form": "보이스4요!",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020849.1.5",
          "form": "오 그거 재밌나요",
          "original_form": "오 그거 재밌나요",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020849.1.6",
          "form": "꽤 재밌었어요!",
          "original_form": "꽤 재밌었어요!",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020849.1.7",
          "form": "앞에 시즌은 안봤는데 앞이 더 재밌다더라구요",
          "original_form": "앞에 시즌은 안봤는데 앞이 더 재밌다더라구요",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020849.1.8",
          "form": "앞시즌이랑 내용 안 이어지나요?",
          "original_form": "앞시즌이랑 내용 안 이어지나요?",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020849.1.9",
          "form": "안이어진다고 했는데 이어지는거 같더라구요",
          "original_form": "안이어진다고 했는데 이어지는거 같더라구요",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020849.1.10",
          "form": "ㅋㅋㅋ 앞시즌 안봐도 이해될 정도인가봐요",
          "original_form": "ㅋㅋㅋ 앞시즌 안봐도 이해될 정도인가봐요",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020849.1.11",
          "form": "네ㅋㅋ 세부적인건 따로고 큰 틀이 이어지는거 같았어요",
          "original_form": "네ㅋㅋ 세부적인건 따로고 큰 틀이 이어지는거 같았어요",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100020849.1.12",
          "form": "오호 담에 한번 정주행 해봐야겠네요",
          "original_form": "오호 담에 한번 정주행 해봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100020849.1.13",
          "form": "오ㅋㅋ 시간떼우는건 영상이 최고긴하죠ㅋㅋ",
          "original_form": "오ㅋㅋ 시간떼우는건 영상이 최고긴하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020849.1.14",
          "form": "맨날 드라마 추천 많이 받고ㅋㅋㅋㅋ 쌓아두고만 있어요",
          "original_form": "맨날 드라마 추천 많이 받고ㅋㅋㅋㅋ 쌓아두고만 있어요",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100020849.1.15",
          "form": "ㅋㅋㅋ볼 게 너무 많아요ㅠㅜㅋㅋ",
          "original_form": "ㅋㅋㅋ볼 게 너무 많아요ㅠㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020849.1.16",
          "form": "그니까요 볼건 많거 체력은 없어서 ㅋㅋㅋㅋ",
          "original_form": "그니까요 볼건 많거 체력은 없어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020849.1.17",
          "form": "저도요ㅋㅋㅋ 드라마는 볼 화가 많다보니 더 그러네요",
          "original_form": "저도요ㅋㅋㅋ 드라마는 볼 화가 많다보니 더 그러네요",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020849.1.18",
          "form": "맞아요 길기도 길고ㅋㅋㅋ 한번씩 엄마랑 드라마 정주행하려면",
          "original_form": "맞아요 길기도 길고ㅋㅋㅋ 한번씩 엄마랑 드라마 정주행하려면",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020849.1.19",
          "form": "엄마가 답답하다고 그냥 젤 마지막편 보자고 하시더라구요ㅋㅋㅋㅋ",
          "original_form": "엄마가 답답하다고 그냥 젤 마지막편 보자고 하시더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020849.1.20",
          "form": "아ㅋㅋㅋ 저도 가끔 그럴때 있어요",
          "original_form": "아ㅋㅋㅋ 저도 가끔 그럴때 있어요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020849.1.21",
          "form": "생각보다 안봐도 알긴 하겠더라구요",
          "original_form": "생각보다 안봐도 알긴 하겠더라구요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020849.1.22",
          "form": "맞아요 뭐 계속 갈등이 생겻다 풀렷다 반복하는 거니깤",
          "original_form": "맞아요 뭐 계속 갈등이 생겻다 풀렷다 반복하는 거니깤",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020849.1.23",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:47"
        }
      ]
