      "utterance": [
        {
          "id": "MDRW2100017419.1.1",
          "form": "베이킹 하세용?",
          "original_form": "베이킹 하세용?",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017419.1.2",
          "form": "베이킹 잘은 못하고 흉내정도만해요!",
          "original_form": "베이킹 잘은 못하고 흉내정도만해요!",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017419.1.3",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017419.1.4",
          "form": "저요 ...좀 ? 귀찮아서 많인 못하구요",
          "original_form": "저요 ...좀 ? 귀찮아서 많인 못하구요",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017419.1.5",
          "form": "베이킹저두관심있어요",
          "original_form": "베이킹저두관심있어요",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017419.1.6",
          "form": "뭐해보셨어용",
          "original_form": "뭐해보셨어용",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017419.1.7",
          "form": "아 잘하시는거같은데요!",
          "original_form": "아 잘하시는거같은데요!",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017419.1.8",
          "form": "잘은 아니에요 젤 쉬운 스콘 이런거 햇어요 ㅋㅋㅋㅋ",
          "original_form": "잘은 아니에요 젤 쉬운 스콘 이런거 햇어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017419.1.9",
          "form": "저 제빵 자격증반 한번 들어보고 집에선 파운드만 해봤어요 쿠키랑",
          "original_form": "저 제빵 자격증반 한번 들어보고 집에선 파운드만 해봤어요 쿠키랑",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017419.1.10",
          "form": "앗 자격증반까지",
          "original_form": "앗 자격증반까지",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017419.1.11",
          "form": "스콘 맛잇게하기 진짜 어렵대요",
          "original_form": "스콘 맛잇게하기 진짜 어렵대요",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017419.1.12",
          "form": "ㅋㅋㅋㅋ 저도 진짜 걍 흉내만..",
          "original_form": "ㅋㅋㅋㅋ 저도 진짜 걍 흉내만..",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017419.1.13",
          "form": "들으신 팁 잇나요,?",
          "original_form": "들으신 팁 잇나요,?",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017419.1.14",
          "form": "하지만 여러명이서 해서... ㅋㅋ 혼자 집에선 아무것도...",
          "original_form": "하지만 여러명이서 해서... ㅋㅋ 혼자 집에선 아무것도...",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017419.1.15",
          "form": "제빵은 집에서 어렵고 요즘 무반죽 치아바타같은건 좋은거같아요",
          "original_form": "제빵은 집에서 어렵고 요즘 무반죽 치아바타같은건 좋은거같아요",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017419.1.16",
          "form": "시간만 오래 걸리는거 빼곤요 추천합니다",
          "original_form": "시간만 오래 걸리는거 빼곤요 추천합니다",
          "speaker_id": "2",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017419.1.17",
          "form": "오 그런거도있구나 찾아봐야겠어요",
          "original_form": "오 그런거도있구나 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017419.1.18",
          "form": "근데 베이킹하다보면 설탕 진짜많이 들어가더라구욬ㅋㅋㅋ",
          "original_form": "근데 베이킹하다보면 설탕 진짜많이 들어가더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017419.1.19",
          "form": "네 시간 오래걸리는데 맛은 파는거랑 비슷해요",
          "original_form": "네 시간 오래걸리는데 맛은 파는거랑 비슷해요",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017419.1.20",
          "form": "설탕과 버터... ㅋㅋ",
          "original_form": "설탕과 버터... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017419.1.21",
          "form": "줄이면 또 맛이 없어요ㅜ",
          "original_form": "줄이면 또 맛이 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017419.1.22",
          "form": "ㄹㅇㄹㅇ ... 버터 ㅋㅋㅋㅋ",
          "original_form": "ㄹㅇㄹㅇ ... 버터 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017419.1.23",
          "form": "그죠 ㅠㅠㅠ",
          "original_form": "그죠 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:23"
        }
      ]
