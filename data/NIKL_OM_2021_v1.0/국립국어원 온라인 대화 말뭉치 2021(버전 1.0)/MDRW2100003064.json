      "utterance": [
        {
          "id": "MDRW2100003064.1.1",
          "form": "빙어축제는 많잖아",
          "original_form": "빙어축제는 많잖아",
          "speaker_id": "2",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003064.1.2",
          "form": "엉 많아 양평 빙어축제",
          "original_form": "엉 많아 양평 빙어축제",
          "speaker_id": "1",
          "time": "20210820 18:13"
        },
        {
          "id": "MDRW2100003064.1.3",
          "form": "아니 안가봤어",
          "original_form": "아니 안가봤어",
          "speaker_id": "2",
          "time": "20210820 18:14"
        },
        {
          "id": "MDRW2100003064.1.4",
          "form": "여기는 겨울에 빙어랑 썰매도 탈수 있어",
          "original_form": "여기는 겨울에 빙어랑 썰매도 탈수 있어",
          "speaker_id": "1",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003064.1.5",
          "form": "다들 그렇지 않나",
          "original_form": "다들 그렇지 않나",
          "speaker_id": "2",
          "time": "20210820 18:15"
        },
        {
          "id": "MDRW2100003064.1.6",
          "form": "송어축제부터 4계절 축제가 있어",
          "original_form": "송어축제부터 4계절 축제가 있어",
          "speaker_id": "1",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003064.1.7",
          "form": "꼭 겨울에 안가도 되겠다",
          "original_form": "꼭 겨울에 안가도 되겠다",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100003064.1.8",
          "form": "응 홈페이지에 체험들을 소개해서 예약 받고 있어",
          "original_form": "응 홈페이지에 체험들을 소개해서 예약 받고 있어",
          "speaker_id": "1",
          "time": "20210820 18:17"
        },
        {
          "id": "MDRW2100003064.1.9",
          "form": "체계적이네",
          "original_form": "체계적이네",
          "speaker_id": "2",
          "time": "20210820 18:19"
        },
        {
          "id": "MDRW2100003064.1.10",
          "form": "마을운영위원회에서 하나보네 가격은 괜찮아",
          "original_form": "마을운영위원회에서 하나보네 가격은 괜찮아",
          "speaker_id": "1",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003064.1.11",
          "form": "착한가격이야?",
          "original_form": "착한가격이야?",
          "speaker_id": "2",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003064.1.12",
          "form": "피크닉공간 포함 체험 패키지 9만원대",
          "original_form": "피크닉공간 포함 체험 패키지 9만원대",
          "speaker_id": "1",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003064.1.13",
          "form": "1인당이야?",
          "original_form": "1인당이야?",
          "speaker_id": "2",
          "time": "20210820 18:21"
        },
        {
          "id": "MDRW2100003064.1.14",
          "form": "그건 안나와있네",
          "original_form": "그건 안나와있네",
          "speaker_id": "1",
          "time": "20210820 18:22"
