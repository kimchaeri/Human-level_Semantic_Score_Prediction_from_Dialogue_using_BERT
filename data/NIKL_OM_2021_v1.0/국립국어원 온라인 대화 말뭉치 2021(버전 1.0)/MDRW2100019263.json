      "utterance": [
        {
          "id": "MDRW2100019263.1.1",
          "form": "결혼 하실껀가요 !",
          "original_form": "결혼 하실껀가요 !",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019263.1.2",
          "form": "저는 음 좋은 사람 있으면",
          "original_form": "저는 음 좋은 사람 있으면",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019263.1.3",
          "form": "할꺼에요 ㅎㅎ",
          "original_form": "할꺼에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019263.1.4",
          "form": "저는 하고 싶어요 ㅋㅋㅋ",
          "original_form": "저는 하고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019263.1.5",
          "form": "맞죠 ㅋㅋ 근데 결혼상대는 딱 그런 생각이 든다던데",
          "original_form": "맞죠 ㅋㅋ 근데 결혼상대는 딱 그런 생각이 든다던데",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019263.1.6",
          "form": "이사람이랑 결혼 하겠구나 ~ 이런 생각 ㅋㅋ",
          "original_form": "이사람이랑 결혼 하겠구나 ~ 이런 생각 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019263.1.7",
          "form": "아그래요? 신기해요  티비에서도 그런 소리 하더라구요 ㅋㅋㅋ",
          "original_form": "아그래요? 신기해요  티비에서도 그런 소리 하더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019263.1.8",
          "form": "맞죠 ㅋㅋ 막 8-9년 연애해도",
          "original_form": "맞죠 ㅋㅋ 막 8-9년 연애해도",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019263.1.9",
          "form": "헤어지고 막 1년만나고 결혼하던데",
          "original_form": "헤어지고 막 1년만나고 결혼하던데",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019263.1.10",
          "form": "와 맞아요",
          "original_form": "와 맞아요",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019263.1.11",
          "form": "그런거보면 인연은 있나봐요 ㅋㅋ",
          "original_form": "그런거보면 인연은 있나봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019263.1.12",
          "form": "누구랑 할지 진짜 모르는거같아요",
          "original_form": "누구랑 할지 진짜 모르는거같아요",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019263.1.13",
          "form": "연예인들만 해도 그러더라구여 ㅋㅋㅋ",
          "original_form": "연예인들만 해도 그러더라구여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019263.1.14",
          "form": "오히려 오래 만나는게 안좋을까요 ,, ?",
          "original_form": "오히려 오래 만나는게 안좋을까요 ,, ?",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019263.1.15",
          "form": "글쎄요 그렇지만 가정이룰 생각이 있다면 일단 만나봐야하지 않을까요 ??",
          "original_form": "글쎄요 그렇지만 가정이룰 생각이 있다면 일단 만나봐야하지 않을까요 ??",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019263.1.16",
          "form": "맞죠 ! 한 2년은 만나야할거같은데 ㅋㅋ",
          "original_form": "맞죠 ! 한 2년은 만나야할거같은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019263.1.17",
          "form": "맞아요 ㅋㅋㅋ 6개월사귀고 결혼하는 사람들도 많더라구요",
          "original_form": "맞아요 ㅋㅋㅋ 6개월사귀고 결혼하는 사람들도 많더라구요",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019263.1.18",
          "form": "맞아요 ㅋㅋ 진짜 신기해요",
          "original_form": "맞아요 ㅋㅋ 진짜 신기해요",
          "speaker_id": "2",
