      "utterance": [
        {
          "id": "MDRW2100034344.1.1",
          "form": "밤에 뭐 보고 주무세요?",
          "original_form": "밤에 뭐 보고 주무세요?",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100034344.1.2",
          "form": "자기전에 전 유튜브요",
          "original_form": "자기전에 전 유튜브요",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100034344.1.3",
          "form": "저는 웹튜이요ㅋㅋ",
          "original_form": "저는 웹튜이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034344.1.4",
          "form": "웹툰이요",
          "original_form": "웹툰이요",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034344.1.5",
          "form": "아 전 카카오페이지",
          "original_form": "아 전 카카오페이지",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034344.1.6",
          "form": "소설요",
          "original_form": "소설요",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034344.1.7",
          "form": "웹툰도 봐요",
          "original_form": "웹툰도 봐요",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034344.1.8",
          "form": "요새 카카오 웹툰 퀴즈도 풀고요 ㅋㅋ",
          "original_form": "요새 카카오 웹툰 퀴즈도 풀고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100034344.1.9",
          "form": "퀴즈는 뭐에요??",
          "original_form": "퀴즈는 뭐에요??",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100034344.1.10",
          "form": "카카오 웹툰 주식주는 퀴즈요",
          "original_form": "카카오 웹툰 주식주는 퀴즈요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034344.1.11",
          "form": "새로 오픈기념요",
          "original_form": "새로 오픈기념요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034344.1.12",
          "form": "선전 안보셨어요?",
          "original_form": "선전 안보셨어요?",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034344.1.13",
          "form": "헉 그런게 있군요",
          "original_form": "헉 그런게 있군요",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034344.1.14",
          "form": "네 그런 이벤트 있는건 몰랐어요ㅋㅋ",
          "original_form": "네 그런 이벤트 있는건 몰랐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034344.1.15",
          "form": "네 저번부터 웹툰보며 했러요 ㅎㅎ",
          "original_form": "네 저번부터 웹툰보며 했러요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034344.1.16",
          "form": "어서 해보세요",
          "original_form": "어서 해보세요",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034344.1.17",
          "form": "웹툰 뭐 보세요?",
          "original_form": "웹툰 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034344.1.18",
          "form": "흐억 얼른 찾아봐야겠네요",
          "original_form": "흐억 얼른 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100034344.1.19",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100034344.1.20",
          "form": "감사합니당",
          "original_form": "감사합니당",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100034344.1.21",
          "form": "유일무이로맨스요!",
          "original_form": "유일무이로맨스요!",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100034344.1.22",
          "form": "아실까요?",
          "original_form": "아실까요?",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100034344.1.23",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100034344.1.24",
          "form": "오오 보시나용?",
          "original_form": "오오 보시나용?",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100034344.1.25",
          "form": "아뇨 보더가 ㅋㅋ",
          "original_form": "아뇨 보더가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100034344.1.26",
          "form": "하차요",
          "original_form": "하차요",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100034344.1.27",
          "form": "제가 뭘 끝까지 안봐요 ㅜㅜ",
          "original_form": "제가 뭘 끝까지 안봐요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100034344.1.28",
          "form": "전 사실 육아물 좋아해요 ㅎㅎ",
          "original_form": "전 사실 육아물 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100034344.1.29",
          "form": "근데 다들 좋아 하던데요",
          "original_form": "근데 다들 좋아 하던데요",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100034344.1.30",
          "form": "저만 그럽니다 ㅎㅎ",
          "original_form": "저만 그럽니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100034344.1.31",
          "form": "주위에 읽던데요",
          "original_form": "주위에 읽던데요",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100034344.1.32",
          "form": "무협지 최고 악당의 귀한딸입니다 ㅋㅋ",
          "original_form": "무협지 최고 악당의 귀한딸입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100034344.1.33",
          "form": "요런거 봐요 ㅋㅋ",
          "original_form": "요런거 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100034344.1.34",
          "form": "아항ㅋㅋ 그러시군요",
          "original_form": "아항ㅋㅋ 그러시군요",
          "speaker_id": "1",
          "time": "20210828 00:44"
        }
      ]
    }
