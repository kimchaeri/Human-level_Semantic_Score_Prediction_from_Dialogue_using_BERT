      "utterance": [
        {
          "id": "MDRW2100028211.1.1",
          "form": "너네 강아지 잘 있니?",
          "original_form": "너네 강아지 잘 있니?",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028211.1.2",
          "form": "name3 수술했어",
          "original_form": "&name3& 수술했어",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028211.1.3",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028211.1.4",
          "form": "피부에 여드름같이 피지가 차서",
          "original_form": "피부에 여드름같이 피지가 차서",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028211.1.5",
          "form": "그거 뗐어",
          "original_form": "그거 뗐어",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028211.1.6",
          "form": "얼마 들었어?",
          "original_form": "얼마 들었어?",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028211.1.7",
          "form": "그냥 부분마취하고 떼낸거라서 4만원대였어",
          "original_form": "그냥 부분마취하고 떼낸거라서 4만원대였어",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100028211.1.8",
          "form": "내가 검색해서 괜찮은 동물병원 알아냈어",
          "original_form": "내가 검색해서 괜찮은 동물병원 알아냈어",
          "speaker_id": "2",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100028211.1.9",
          "form": "너의 강아지가 건강해져서 다행이다",
          "original_form": "너의 강아지가 건강해져서 다행이다",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028211.1.10",
          "form": "지금은 어때?",
          "original_form": "지금은 어때?",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028211.1.11",
          "form": "ㅋㅋㅋㅋㅋ 아직 다 떼어낸건 아니지만 우리 강아지는 괜찮아",
          "original_form": "ㅋㅋㅋㅋㅋ 아직 다 떼어낸건 아니지만 우리 강아지는 괜찮아",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028211.1.12",
          "form": "너희 강아지 이름이 name3구나",
          "original_form": "너희 강아지 이름이 &name3&구나",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028211.1.13",
          "form": "name3랑 산책할래?",
          "original_form": "&name3&랑 산책할래?",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028211.1.14",
          "form": "그래 너희 강아지 만나고 싶다",
          "original_form": "그래 너희 강아지 만나고 싶다",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028211.1.15",
          "form": "주변에 동물 키우는 친구 없니?",
          "original_form": "주변에 동물 키우는 친구 없니?",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028211.1.16",
          "form": "고양이 키우는 친구도 있어",
          "original_form": "고양이 키우는 친구도 있어",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028211.1.17",
          "form": "나는 금붕어를 키울 생각이야",
          "original_form": "나는 금붕어를 키울 생각이야",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028211.1.18",
          "form": "금붕어 말고 베타라는 물고기를 키워보는거 어때?",
          "original_form": "금붕어 말고 베타라는 물고기를 키워보는거 어때?",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028211.1.19",
          "form": "어떤 금붕어니?",
          "original_form": "어떤 금붕어니?",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028211.1.20",
          "form": "비늘이 아주 예쁜데 키우기가 조금 까다롭대",
          "original_form": "비늘이 아주 예쁜데 키우기가 조금 까다롭대",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028211.1.21",
          "form": "그렇구나 금붕어 키우고 싶다",
          "original_form": "그렇구나 금붕어 키우고 싶다",
          "speaker_id": "1",
          "time": "20210902 18:48"
        }
