      "utterance": [
        {
          "id": "MDRW2100018935.1.1",
          "form": "요새 마트에도 필고 많아요",
          "original_form": "요새 마트에도 필고 많아요",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018935.1.2",
          "form": "수집품 사는거 없으세요?",
          "original_form": "수집품 사는거 없으세요?",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018935.1.3",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018935.1.4",
          "form": "브랜드가 있나요?",
          "original_form": "브랜드가 있나요?",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018935.1.5",
          "form": "전 잔 모았었어요",
          "original_form": "전 잔 모았었어요",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018935.1.6",
          "form": "리멘트",
          "original_form": "리멘트",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018935.1.7",
          "form": "아 잔요",
          "original_form": "아 잔요",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018935.1.8",
          "form": "첨들어봐요",
          "original_form": "첨들어봐요",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018935.1.9",
          "form": "장식장이 넘 이뿌겠다",
          "original_form": "장식장이 넘 이뿌겠다",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018935.1.10",
          "form": "ㅎㅎ 너무 많아져서",
          "original_form": "ㅎㅎ 너무 많아져서",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018935.1.11",
          "form": "전시하고 그러셨어요",
          "original_form": "전시하고 그러셨어요",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
