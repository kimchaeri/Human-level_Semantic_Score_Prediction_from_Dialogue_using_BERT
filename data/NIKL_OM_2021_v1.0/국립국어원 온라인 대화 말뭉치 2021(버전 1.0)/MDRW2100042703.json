      "utterance": [
        {
          "id": "MDRW2100042703.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 02:10"
        },
        {
          "id": "MDRW2100042703.1.2",
          "form": "디제이하면서 더 그런듯",
          "original_form": "디제이하면서 더 그런듯",
          "speaker_id": "1",
          "time": "20210907 02:10"
        },
        {
          "id": "MDRW2100042703.1.3",
          "form": "말을 예쁘게하네",
          "original_form": "말을 예쁘게하네",
          "speaker_id": "1",
          "time": "20210907 02:11"
        },
        {
          "id": "MDRW2100042703.1.4",
          "form": "아 영현이?",
          "original_form": "아 영현이?",
          "speaker_id": "2",
          "time": "20210907 02:11"
        },
        {
          "id": "MDRW2100042703.1.5",
          "form": "자라고 자장가도 불러주는군",
          "original_form": "자라고 자장가도 불러주는군",
          "speaker_id": "1",
          "time": "20210907 02:11"
        },
        {
          "id": "MDRW2100042703.1.6",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 02:11"
        },
        {
          "id": "MDRW2100042703.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 예전부터느꼈지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 예전부터느꼈지만",
          "speaker_id": "1",
          "time": "20210907 02:11"
        },
        {
          "id": "MDRW2100042703.1.8",
          "form": "영현이가 참 말을 예쁘게하네",
          "original_form": "영현이가 참 말을 예쁘게하네",
          "speaker_id": "1",
          "time": "20210907 02:12"
        },
        {
          "id": "MDRW2100042703.1.9",
          "form": "디제이하면서 더 그런거 같다",
          "original_form": "디제이하면서 더 그런거 같다",
          "speaker_id": "1",
          "time": "20210907 02:12"
        },
        {
          "id": "MDRW2100042703.1.10",
          "form": "가 그사이에 한말",
          "original_form": "가 그사이에 한말",
          "speaker_id": "1",
          "time": "20210907 02:12"
        },
        {
          "id": "MDRW2100042703.1.11",
          "form": "막곡 잔잔한거치곤",
          "original_form": "막곡 잔잔한거치곤",
          "speaker_id": "1",
          "time": "20210907 02:12"
        },
        {
          "id": "MDRW2100042703.1.12",
          "form": "멜로디는 취향이군여",
          "original_form": "멜로디는 취향이군여",
          "speaker_id": "1",
          "time": "20210907 02:12"
        },
        {
          "id": "MDRW2100042703.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 천성적으로 성격이 순한건 아닌데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 천성적으로 성격이 순한건 아닌데",
          "speaker_id": "2",
          "time": "20210907 02:12"
        },
        {
          "id": "MDRW2100042703.1.14",
          "form": "장조라서",
          "original_form": "장조라서",
          "speaker_id": "1",
          "time": "20210907 02:12"
        },
        {
          "id": "MDRW2100042703.1.15",
          "form": "본인이 노력함+작사 맡음 에",
          "original_form": "본인이 노력함+작사 맡음 에",
          "speaker_id": "2",
          "time": "20210907 02:12"
        },
        {
          "id": "MDRW2100042703.1.16",
          "form": "디제이까지 더해져서ㅇㅇㅇ",
          "original_form": "디제이까지 더해져서ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 02:13"
        },
        {
          "id": "MDRW2100042703.1.17",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 02:13"
        },
        {
          "id": "MDRW2100042703.1.18",
          "form": "자장가인데 단조면",
          "original_form": "자장가인데 단조면",
          "speaker_id": "2",
          "time": "20210907 02:13"
        },
        {
          "id": "MDRW2100042703.1.19",
          "form": "조금 슬퍼졌을듯...",
          "original_form": "조금 슬퍼졌을듯...",
          "speaker_id": "2",
          "time": "20210907 02:14"
        },
        {
          "id": "MDRW2100042703.1.20",
          "form": "장조로 만들어줘서 고맙다...",
          "original_form": "장조로 만들어줘서 고맙다...",
          "speaker_id": "2",
          "time": "20210907 02:14"
        },
        {
          "id": "MDRW2100042703.1.21",
          "form": "제가그래서",
          "original_form": "제가그래서",
          "speaker_id": "1",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042703.1.22",
          "form": "엄마가섬그늘에",
          "original_form": "엄마가섬그늘에",
          "speaker_id": "1",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042703.1.23",
          "form": "굴따러가면을안좋아해",
          "original_form": "굴따러가면을안좋아해",
          "speaker_id": "1",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042703.1.24",
          "form": "스산해",
          "original_form": "스산해",
          "speaker_id": "1",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042703.1.25",
          "form": "엄마가애놔두고어딜가",
          "original_form": "엄마가애놔두고어딜가",
          "speaker_id": "1",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042703.1.26",
          "form": "무섭잖아",
          "original_form": "무섭잖아",
          "speaker_id": "1",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042703.1.27",
          "form": "섬집아기 ㄹㅇ",
          "original_form": "섬집아기 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042703.1.28",
          "form": "실제로 예전에",
          "original_form": "실제로 예전에",
          "speaker_id": "2",
          "time": "20210907 02:18"
        },
        {
          "id": "MDRW2100042703.1.29",
          "form": "그리고 이선희가그거불러주는데",
          "original_form": "그리고 이선희가그거불러주는데",
          "speaker_id": "1",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.30",
          "form": "뭐더라 그.. 미스테리극장같은 단막극식 공포물",
          "original_form": "뭐더라 그.. 미스테리극장같은 단막극식 공포물",
          "speaker_id": "2",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.31",
          "form": "이선희같이가느다란 목소리는",
          "original_form": "이선희같이가느다란 목소리는",
          "speaker_id": "1",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.32",
          "form": "그런데에서 공포사연 소개하면서",
          "original_form": "그런데에서 공포사연 소개하면서",
          "speaker_id": "2",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.33",
          "form": "무서워무섭다고",
          "original_form": "무서워무섭다고",
          "speaker_id": "1",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.34",
          "form": "스산해",
          "original_form": "스산해",
          "speaker_id": "1",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.35",
          "form": "브금으로 섬집아기 깔아놓은거 보고",
          "original_form": "브금으로 섬집아기 깔아놓은거 보고",
          "speaker_id": "2",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.36",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.37",
          "form": "약간 트라우마 됨...",
          "original_form": "약간 트라우마 됨...",
          "speaker_id": "2",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.38",
          "form": "히익",
          "original_form": "히익",
          "speaker_id": "1",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.39",
          "form": "브금도 아니고... 그 노래 부르면서 귀신 나오는 장면이었어..",
          "original_form": "브금도 아니고... 그 노래 부르면서 귀신 나오는 장면이었어..",
          "speaker_id": "2",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.40",
          "form": "으아아",
          "original_form": "으아아",
          "speaker_id": "1",
          "time": "20210907 02:19"
        },
        {
          "id": "MDRW2100042703.1.41",
          "form": "자장가를",
          "original_form": "자장가를",
          "speaker_id": "1",
