      "utterance": [
        {
          "id": "MDRW2100040489.1.1",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠ 죄송합니당..ㅠㅠㅠㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠ 죄송합니당..ㅠㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100040489.1.2",
          "form": "저 연어사케동인가요?",
          "original_form": "저 연어사케동인가요?",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100040489.1.3",
          "form": "밥위에 연어 올라가있는거 그거 먹었어요!",
          "original_form": "밥위에 연어 올라가있는거 그거 먹었어요!",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100040489.1.4",
          "form": "헐 우와",
          "original_form": "헐 우와",
          "speaker_id": "1",
          "time": "20210904 21:57"
        },
        {
          "id": "MDRW2100040489.1.5",
          "form": "나가서 드셨군요!!!",
          "original_form": "나가서 드셨군요!!!",
          "speaker_id": "1",
          "time": "20210904 21:57"
        },
        {
          "id": "MDRW2100040489.1.6",
          "form": "맛난거 드셨네요~~~!!!!",
          "original_form": "맛난거 드셨네요~~~!!!!",
          "speaker_id": "1",
          "time": "20210904 21:57"
        },
        {
          "id": "MDRW2100040489.1.7",
          "form": "그 안에 와사비도 있고 맞죠?!",
          "original_form": "그 안에 와사비도 있고 맞죠?!",
          "speaker_id": "1",
          "time": "20210904 21:57"
        },
        {
          "id": "MDRW2100040489.1.8",
          "form": "배달 시켜서 먹었어요ㅋㅋㅋ 와사비 있어요!",
          "original_form": "배달 시켜서 먹었어요ㅋㅋㅋ 와사비 있어요!",
          "speaker_id": "2",
          "time": "20210904 21:58"
        },
        {
          "id": "MDRW2100040489.1.9",
          "form": "덕분에 먹다가 눈물 뺐어요 ㅎㅎ",
          "original_form": "덕분에 먹다가 눈물 뺐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:58"
        },
        {
          "id": "MDRW2100040489.1.10",
          "form": "회 좋아하세요?",
          "original_form": "회 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 21:58"
        },
        {
          "id": "MDRW2100040489.1.11",
          "form": "그럼요 그럼요!!!",
          "original_form": "그럼요 그럼요!!!",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040489.1.12",
          "form": "없어서 못먹는데",
          "original_form": "없어서 못먹는데",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040489.1.13",
          "form": "그럼 초밥은요!",
          "original_form": "그럼 초밥은요!",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040489.1.14",
          "form": "당연히 좋아합니다..ㅠㅠ",
          "original_form": "당연히 좋아합니다..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040489.1.15",
          "form": "오마카세 같은 데 가고싶은데",
          "original_form": "오마카세 같은 데 가고싶은데",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040489.1.16",
          "form": "비싸서 못가죠~~~",
          "original_form": "비싸서 못가죠~~~",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040489.1.17",
          "form": "꺄아아아 저도 회 초밥 다 좋아요",
          "original_form": "꺄아아아 저도 회 초밥 다 좋아요",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100040489.1.18",
          "form": "오마카세는 저는 꿈에서나 갈 수 있을 것 같아요",
          "original_form": "오마카세는 저는 꿈에서나 갈 수 있을 것 같아요",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100040489.1.19",
          "form": "살아생전엔 불가능할듯요 ㅎ",
          "original_form": "살아생전엔 불가능할듯요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100040489.1.20",
          "form": "어떻게 한 번애",
          "original_form": "어떻게 한 번애",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040489.1.21",
          "form": "이십만원 어치를 먹는지...",
          "original_form": "이십만원 어치를 먹는지...",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040489.1.22",
          "form": "그래도 취업하면 갈 수 있지 않을까요ㅠ",
          "original_form": "그래도 취업하면 갈 수 있지 않을까요ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040489.1.23",
          "form": "저는 오마카세는 그 한끼에 그만큼 써도 그냥 껌 사듯 할 수 있을 때 그때 갈래요",
          "original_form": "저는 오마카세는 그 한끼에 그만큼 써도 그냥 껌 사듯 할 수 있을 때 그때 갈래요",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040489.1.24",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100040489.1.25",
          "form": "로또 맞으면 가야지",
          "original_form": "로또 맞으면 가야지",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100040489.1.26",
          "form": "맞아요 그리고 그만큼",
          "original_form": "맞아요 그리고 그만큼",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040489.1.27",
          "form": "맛있지도 않을거옝6",
          "original_form": "맛있지도 않을거옝6",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040489.1.28",
          "form": "거예요 분위기랑 자릿세지...",
          "original_form": "거예요 분위기랑 자릿세지...",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040489.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040489.1.30",
          "form": "사실 별거 아니죠",
          "original_form": "사실 별거 아니죠",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040489.1.31",
          "form": "그냥 삼겹살 꾸워 먹고 싶다 갑자기 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 삼겹살 꾸워 먹고 싶다 갑자기 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100040489.1.32",
          "form": "삼겹살 좋아하세요?",
          "original_form": "삼겹살 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100040489.1.33",
          "form": "네네 좋아해요!!!!",
          "original_form": "네네 좋아해요!!!!",
          "speaker_id": "1",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040489.1.34",
          "form": "저는 육류는",
          "original_form": "저는 육류는",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040489.1.35",
          "form": "양고기나",
          "original_form": "양고기나",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040489.1.36",
          "form": "막창 대창 곱창 이런거만 아니면 잘먹어요 ㅎㅎㅎ",
          "original_form": "막창 대창 곱창 이런거만 아니면 잘먹어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040489.1.37",
          "form": "저도 고기 좋아요 ㅋㅋㅋㅋ 양고기 드셔보셨어요?",
          "original_form": "저도 고기 좋아요 ㅋㅋㅋㅋ 양고기 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100040489.1.38",
