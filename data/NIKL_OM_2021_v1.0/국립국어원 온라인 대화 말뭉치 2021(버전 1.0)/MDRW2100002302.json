      "utterance": [
        {
          "id": "MDRW2100002302.1.1",
          "form": "오늘은 머해 name1? 점검스케쥴같은거없어?",
          "original_form": "오늘은 머해 &name1&? 점검스케쥴같은거없어?",
          "speaker_id": "2",
          "time": "20210820 09:27"
        },
        {
          "id": "MDRW2100002302.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100002302.1.3",
          "form": "어제 나 반차쓰는동안 다 해놨더라고",
          "original_form": "어제 나 반차쓰는동안 다 해놨더라고",
          "speaker_id": "1",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100002302.1.4",
          "form": "오...좋은 동료들이로다",
          "original_form": "오...좋은 동료들이로다",
          "speaker_id": "2",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100002302.1.5",
          "form": "나는 오늘 코로나19 특별방역점검가야대...",
          "original_form": "나는 오늘 코로나19 특별방역점검가야대...",
          "speaker_id": "2",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100002302.1.6",
          "form": "가지마",
          "original_form": "가지마",
          "speaker_id": "1",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100002302.1.7",
          "form": "나랑 대화해",
          "original_form": "나랑 대화해",
          "speaker_id": "1",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100002302.1.8",
          "form": "오후에 갈거야",
          "original_form": "오후에 갈거야",
          "speaker_id": "2",
          "time": "20210820 09:35"
        },
        {
          "id": "MDRW2100002302.1.9",
          "form": "힝구ㅠㅠ",
          "original_form": "힝구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 09:35"
        },
        {
          "id": "MDRW2100002302.1.10",
          "form": "근데 4단계 연장되서 계속 나가야될거같애...",
          "original_form": "근데 4단계 연장되서 계속 나가야될거같애...",
          "speaker_id": "2",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002302.1.11",
          "form": "대전 4단계야",
          "original_form": "대전 4단계야",
          "speaker_id": "1",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002302.1.12",
          "form": "심각하네",
          "original_form": "심각하네",
          "speaker_id": "1",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002302.1.13",
          "form": "공무원 갈아넣은 케이방역",
          "original_form": "공무원 갈아넣은 케이방역",
          "speaker_id": "1",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002302.1.14",
          "form": "발표는 시장이하고",
          "original_form": "발표는 시장이하고",
          "speaker_id": "2",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002302.1.15",
          "form": "욕은 내가 먹네",
          "original_form": "욕은 내가 먹네",
          "speaker_id": "2",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100002302.1.16",
          "form": "왜ㅋㅋㅋ",
          "original_form": "왜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:43"
        },
        {
          "id": "MDRW2100002302.1.17",
          "form": "만만한게 공무원자나 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "만만한게 공무원자나 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002302.1.18",
          "form": "힘을 내",
          "original_form": "힘을 내",
          "speaker_id": "1",
          "time": "20210820 09:51"
        },
        {
          "id": "MDRW2100002302.1.19",
          "form": "어젯밤에는 다른 사람들도 말걸더라",
          "original_form": "어젯밤에는 다른 사람들도 말걸더라",
          "speaker_id": "1",
          "time": "20210820 09:52"
        },
        {
          "id": "MDRW2100002302.1.20",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "2",
          "time": "20210820 09:55"
        },
        {
          "id": "MDRW2100002302.1.21",
          "form": "지금 4갠가 하고있어",
          "original_form": "지금 4갠가 하고있어",
          "speaker_id": "2",
          "time": "20210820 09:55"
        },
        {
          "id": "MDRW2100002302.1.22",
          "form": "제출한거 하나 빠꾸먹음 ㅜ",
          "original_form": "제출한거 하나 빠꾸먹음 ㅜ",
          "speaker_id": "2",
          "time": "20210820 09:55"
        },
        {
          "id": "MDRW2100002302.1.23",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210820 09:56"
        },
        {
          "id": "MDRW2100002302.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:56"
        },
        {
          "id": "MDRW2100002302.1.25",
          "form": "그거 나중에 승인될지도 몰라",
          "original_form": "그거 나중에 승인될지도 몰라",
