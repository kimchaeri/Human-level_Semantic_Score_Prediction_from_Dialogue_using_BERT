      "utterance": [
        {
          "id": "MDRW2100013049.1.1",
          "form": "내일은",
          "original_form": "내일은",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013049.1.2",
          "form": "일찍마칠듯",
          "original_form": "일찍마칠듯",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013049.1.3",
          "form": "내일 일찍마칠것같은ㅋ",
          "original_form": "내일 일찍마칠것같은ㅋ",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013049.1.4",
          "form": "흠..학원 바로 가라고 해야겠다~",
          "original_form": "흠..학원 바로 가라고 해야겠다~",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013049.1.5",
          "form": "학원도 요즘 뭘하는지;;",
          "original_form": "학원도 요즘 뭘하는지;;",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013049.1.6",
          "form": "ㅋㅋ 피아노학원,",
          "original_form": "ㅋㅋ 피아노학원,",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013049.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013049.1.8",
          "form": "응~바이올린도 언제하는지도 모림..",
          "original_form": "응~바이올린도 언제하는지도 모림..",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013049.1.9",
          "form": "조율이 안되서 못했다 캐삿코..",
          "original_form": "조율이 안되서 못했다 캐삿코..",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100013049.1.10",
          "form": "맘에 안든다이;;",
          "original_form": "맘에 안든다이;;",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100013049.1.11",
          "form": "아하ㅜㅜ저번부터 하더니ㅜㅜ",
          "original_form": "아하ㅜㅜ저번부터 하더니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013049.1.12",
          "form": "바이올린은 끊음안되나?",
          "original_form": "바이올린은 끊음안되나?",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013049.1.13",
          "form": "지금 끊기엔 아깝나?ㅜ",
          "original_form": "지금 끊기엔 아깝나?ㅜ",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013049.1.14",
          "form": "안할라했는데 또 하고싶데ㅡㅡ",
          "original_form": "안할라했는데 또 하고싶데ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013049.1.15",
          "form": "진짜 승질이 난다..",
          "original_form": "진짜 승질이 난다..",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013049.1.16",
          "form": "두개는 좀 힘든거 같다~",
          "original_form": "두개는 좀 힘든거 같다~",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013049.1.17",
          "form": "아하ㅜㅜ그렇네잉 ㅜㅜ",
          "original_form": "아하ㅜㅜ그렇네잉 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100013049.1.18",
          "form": "첫째한테도 교육비 꽤나가제? 50나가나?",
          "original_form": "첫째한테도 교육비 꽤나가제? 50나가나?",
          "speaker_id": "1",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100013049.1.19",
          "form": "응 딱 50인데..그 빨간펜이랑,도요새아님 괜찮은디 패드하고 펜 값이ㅡㅡ",
          "original_form": "응 딱 50인데..그 빨간펜이랑,도요새아님 괜찮은디 패드하고 펜 값이ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100013049.1.20",
          "form": "그거 아까바가 둘찌도 빨간펜 해야됨 ㅋㅋㅋㅋ",
          "original_form": "그거 아까바가 둘찌도 빨간펜 해야됨 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100013049.1.21",
          "form": "이젠 캐도 웅진 끝나가 그나마 낫다~",
          "original_form": "이젠 캐도 웅진 끝나가 그나마 낫다~",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.22",
          "form": "둘찌는 언제하려고",
          "original_form": "둘찌는 언제하려고",
          "speaker_id": "1",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.23",
          "form": "둘찌는 7살 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "둘찌는 7살 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.24",
          "form": "웅진끝낫구나",
          "original_form": "웅진끝낫구나",
          "speaker_id": "1",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.25",
          "form": "다행이다ㅋㅋ",
          "original_form": "다행이다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.26",
          "form": "아 아직 시간있네ㅋㅋ",
          "original_form": "아 아직 시간있네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.27",
          "form": "미리하는건 뭐 의미가.크게 없는듯 ㅋㅋㅋ",
          "original_form": "미리하는건 뭐 의미가.크게 없는듯 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.28",
          "form": "그래 아까워서ㅋㅋ",
          "original_form": "그래 아까워서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.29",
          "form": "맞다 나둡ㆍ니",
          "original_form": "맞다 나둡ㆍ니",
          "speaker_id": "1",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013049.1.30",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.31",
          "form": "보니",
          "original_form": "보니",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.32",
          "form": "6살쯤 천천히 해도 되긋다 ㅋㅋㅋㅋ",
          "original_form": "6살쯤 천천히 해도 되긋다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.33",
          "form": "패드가 정말 아깝다;;",
          "original_form": "패드가 정말 아깝다;;",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.34",
          "form": "6살후반부터",
          "original_form": "6살후반부터",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.35",
          "form": "해도되겟더라",
          "original_form": "해도되겟더라",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.36",
          "form": "웅진꺼도 하나 있는데 천지 쓸데없네..",
          "original_form": "웅진꺼도 하나 있는데 천지 쓸데없네..",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.37",
          "form": "응응 맞아",
          "original_form": "응응 맞아",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.38",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.39",
          "form": "2학년되믄 30갈꺼야??",
          "original_form": "2학년되믄 30갈꺼야??",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013049.1.40",
          "form": "나도 둘째는",
          "original_form": "나도 둘째는",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013049.1.41",
          "form": "6살후반이나 7살초에",
          "original_form": "6살후반이나 7살초에",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013049.1.42",
          "form": "한글가르쳐주려고",
          "original_form": "한글가르쳐주려고",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013049.1.43",
          "form": "첫째때보니 7살 중반부터는 늦더라",
          "original_form": "첫째때보니 7살 중반부터는 늦더라",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013049.1.44",
          "form": "너무",
          "original_form": "너무",
          "speaker_id": "1",
          "time": "20210825 11:52"
        }
      ]
