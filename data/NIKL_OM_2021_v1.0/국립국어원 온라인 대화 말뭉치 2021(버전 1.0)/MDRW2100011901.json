      "utterance": [
        {
          "id": "MDRW2100011901.1.1",
          "form": "그러게 내가 결혼 말렷잖아~~",
          "original_form": "그러게 내가 결혼 말렷잖아~~",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011901.1.2",
          "form": "이정도 말밖에 못할거같은데..ㅡㅋㅋ ㅜ",
          "original_form": "이정도 말밖에 못할거같은데..ㅡㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011901.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011901.1.4",
          "form": "그러다보면 또 싸우게돼겠죠",
          "original_form": "그러다보면 또 싸우게돼겠죠",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011901.1.5",
          "form": "하소연하면서 가정을 유지하려는게 이해가 안가요",
          "original_form": "하소연하면서 가정을 유지하려는게 이해가 안가요",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011901.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011901.1.7",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011901.1.8",
          "form": "결혼을 하면",
          "original_form": "결혼을 하면",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011901.1.9",
          "form": "남편이랑 하나가 되는건지",
          "original_form": "남편이랑 하나가 되는건지",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011901.1.10",
          "form": "가족이 되는거니까",
          "original_form": "가족이 되는거니까",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011901.1.11",
          "form": "남이라는 걸 인식 못하는건진 모르겠지만요",
          "original_form": "남이라는 걸 인식 못하는건진 모르겠지만요",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011901.1.12",
          "form": "전 결혼 안할거라고 꾸준히 말했는데",
          "original_form": "전 결혼 안할거라고 꾸준히 말했는데",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011901.1.13",
          "form": "그런애가 먼저 가더라",
          "original_form": "그런애가 먼저 가더라",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011901.1.14",
          "form": "우리 결혼하고 애 낳아서 같이 여행하면 재밌겠다",
          "original_form": "우리 결혼하고 애 낳아서 같이 여행하면 재밌겠다",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011901.1.15",
          "form": "이런 친구들이 있어서 착찹합니다",
          "original_form": "이런 친구들이 있어서 착찹합니다",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011901.1.16",
          "form": "계속 그 친구랑 기싸움하는 느낌이에요ㅋㅋㅋㅋ",
          "original_form": "계속 그 친구랑 기싸움하는 느낌이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011901.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 그거 뭔지 알아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 그거 뭔지 알아요",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011901.1.18",
          "form": "\"그런애가 먼저 가더라\" 저도 500번 들었어요",
          "original_form": "\"그런애가 먼저 가더라\" 저도 500번 들었어요",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011901.1.19",
          "form": "어쩌란건지..",
          "original_form": "어쩌란건지..",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100011901.1.20",
          "form": "ㅈㅣㄴ짜 그리고 결혼하면 한몸되는거같아서 진짜 기괴해요 뭐만 하면 기승전남편 기승전애기",
          "original_form": "ㅈㅣㄴ짜 그리고 결혼하면 한몸되는거같아서 진짜 기괴해요 뭐만 하면 기승전남편 기승전애기",
          "speaker_id": "1",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011901.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100011901.1.22",
          "form": "세상이 이런식으로 돌아가서",
          "original_form": "세상이 이런식으로 돌아가서",
          "speaker_id": "2",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100011901.1.23",
          "form": "다들 연애도 하고 결혼도 하나봐요",
          "original_form": "다들 연애도 하고 결혼도 하나봐요",
          "speaker_id": "2",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100011901.1.24",
          "form": "근데 궁금하기도 하네요",
          "original_form": "근데 궁금하기도 하네요",
          "speaker_id": "2",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100011901.1.25",
          "form": "그 친구들은 제가 결혼 안한다는 걸",
          "original_form": "그 친구들은 제가 결혼 안한다는 걸",
          "speaker_id": "2",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011901.1.26",
          "form": "그냥 하는 말로 생각하는 걸까요?",
          "original_form": "그냥 하는 말로 생각하는 걸까요?",
          "speaker_id": "2",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011901.1.27",
          "form": "결혼을 당연히 해야 하는 세계관에서 살고 있는게 아닐까요?",
          "original_form": "결혼을 당연히 해야 하는 세계관에서 살고 있는게 아닐까요?",
          "speaker_id": "1",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100011901.1.28",
          "form": "다른 세계에서 살고있어서 말이 안통하는걸로 추정중입니다 ㅋㅋㅋㅋ",
          "original_form": "다른 세계에서 살고있어서 말이 안통하는걸로 추정중입니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100011901.1.29",
          "form": "다른 세계 맞는듯해요",
          "original_form": "다른 세계 맞는듯해요",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100011901.1.30",
          "form": "근데 저도 좀 지는성격이 아니라그런지",
          "original_form": "근데 저도 좀 지는성격이 아니라그런지",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100011901.1.31",
          "form": "근데 나 결혼 안할건데 꼭 말하게 되더라구요",
          "original_form": "근데 나 결혼 안할건데 꼭 말하게 되더라구요",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100011901.1.32",
          "form": "이래서 친구인가봐요",
          "original_form": "이래서 친구인가봐요",
          "speaker_id": "2",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100011901.1.33",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100011901.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 강대강",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 강대강",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100011901.1.35",
          "form": "자기표현 확실히 하시니까 결혼관 달라도 친구하실 수 있으실거같아요",
          "original_form": "자기표현 확실히 하시니까 결혼관 달라도 친구하실 수 있으실거같아요",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100011901.1.36",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100011901.1.37",
          "form": "근데 신기하네요 애낳는건 ㄹㅇ 결혼을 넘어서서 .. 엄청 큰일인데",
          "original_form": "근데 신기하네요 애낳는건 ㄹㅇ 결혼을 넘어서서 .. 엄청 큰일인데",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100011901.1.38",
          "form": "그걸 꿈꾼다는게..",
          "original_form": "그걸 꿈꾼다는게..",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100011901.1.39",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100011901.1.40",
          "form": "저도 좀 궁금해서",
          "original_form": "저도 좀 궁금해서",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100011901.1.41",
          "form": "왜 애를 낳고싶어?",
          "original_form": "왜 애를 낳고싶어?",
