          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044136.1.7",
          "form": "그러길 바래라",
          "original_form": "그러길 바래라",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044136.1.8",
          "form": "웅웅 ㅋㅋ 나도 좀 본받아야겠다 ㅋ",
          "original_form": "웅웅 ㅋㅋ 나도 좀 본받아야겠다 ㅋ",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044136.1.9",
          "form": "그래 너도 운동해라",
          "original_form": "그래 너도 운동해라",
          "speaker_id": "2",
          "time": "20210907 21:42"
