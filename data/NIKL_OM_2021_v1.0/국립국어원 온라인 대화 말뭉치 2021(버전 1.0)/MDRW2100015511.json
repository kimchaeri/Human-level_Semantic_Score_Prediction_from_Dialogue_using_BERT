      "utterance": [
        {
          "id": "MDRW2100015511.1.1",
          "form": "무슨 날씨가 좋아?",
          "original_form": "무슨 날씨가 좋아?",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015511.1.2",
          "form": "무슨 날씨 좋아해",
          "original_form": "무슨 날씨 좋아해",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015511.1.3",
          "form": "선선한 날씨! 딱 뛰어놀기 좋잖아",
          "original_form": "선선한 날씨! 딱 뛰어놀기 좋잖아",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015511.1.4",
          "form": "어 그치ㅋㅋ",
          "original_form": "어 그치ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015511.1.5",
          "form": "그럼 계절은",
          "original_form": "그럼 계절은",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015511.1.6",
          "form": "요즘 비 많이 오던데",
          "original_form": "요즘 비 많이 오던데",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015511.1.7",
          "form": "나는 가을좋아해",
          "original_form": "나는 가을좋아해",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015511.1.8",
          "form": "비 그쳤는데ㅋㅋ",
          "original_form": "비 그쳤는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015511.1.9",
          "form": "난 봄이 좋아ㅎㅎ",
          "original_form": "난 봄이 좋아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015511.1.10",
          "form": "봄하니까 벚꽃보고싶다",
          "original_form": "봄하니까 벚꽃보고싶다",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015511.1.11",
          "form": "오늘은 별이 잘보인다",
          "original_form": "오늘은 별이 잘보인다",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015511.1.12",
          "form": "이제 선선해지니까 하늘이 맑네",
          "original_form": "이제 선선해지니까 하늘이 맑네",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015511.1.13",
          "form": "점점 날씨가 선선해지구 있어 ㅎ",
          "original_form": "점점 날씨가 선선해지구 있어 ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015511.1.14",
          "form": "밤하늘에 별을따서 너에게 주고싶어",
          "original_form": "밤하늘에 별을따서 너에게 주고싶어",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015511.1.15",
          "form": "그러고보니 요즘 미세먼지도 없는 것 같아",
          "original_form": "그러고보니 요즘 미세먼지도 없는 것 같아",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015511.1.16",
          "form": "그치?",
          "original_form": "그치?",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015511.1.17",
          "form": "맞아 공기가 맑아!",
          "original_form": "맞아 공기가 맑아!",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015511.1.18",
          "form": "예전에는 진짜 하늘이 회색빛이었는데",
          "original_form": "예전에는 진짜 하늘이 회색빛이었는데",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015511.1.19",
          "form": "요즘은 맑아",
          "original_form": "요즘은 맑아",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015511.1.20",
          "form": "미세먼지 때문에 넘 힘들어씀 ㅠㅜ",
          "original_form": "미세먼지 때문에 넘 힘들어씀 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015511.1.21",
          "form": "그니깡ㅠㅠ",
          "original_form": "그니깡ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:29"
        }
      ]
    }
