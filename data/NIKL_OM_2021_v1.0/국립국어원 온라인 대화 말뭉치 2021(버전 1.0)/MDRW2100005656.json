      "utterance": [
        {
          "id": "MDRW2100005656.1.1",
          "form": "여행 다니는 거 좋아하시나요?",
          "original_form": "여행 다니는 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005656.1.2",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005656.1.3",
          "form": "국내좋아하시나요",
          "original_form": "국내좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005656.1.4",
          "form": "해외좋아하시나요",
          "original_form": "해외좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005656.1.5",
          "form": "저는 국내보단 해외가 좋은것같아요!",
          "original_form": "저는 국내보단 해외가 좋은것같아요!",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005656.1.6",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005656.1.7",
          "form": "더 여행하는느낌나서좋아요",
          "original_form": "더 여행하는느낌나서좋아요",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005656.1.8",
          "form": "해외가보신곳있나요",
          "original_form": "해외가보신곳있나요",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005656.1.9",
          "form": "저는 터키랑 일본, 홍콩, 대만 이렇게 다녀왔어요",
          "original_form": "저는 터키랑 일본, 홍콩, 대만 이렇게 다녀왔어요",
          "speaker_id": "1",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005656.1.10",
          "form": "오 많이다녀오셧네요",
          "original_form": "오 많이다녀오셧네요",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005656.1.11",
          "form": "저도 일본 가봤어요",
          "original_form": "저도 일본 가봤어요",
          "speaker_id": "2",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005656.1.12",
          "form": "일본 좋죠!",
          "original_form": "일본 좋죠!",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005656.1.13",
          "form": "부모님덕에 어렸을때 그래도 많이 다녀왔네요 ㅎㅎ",
          "original_form": "부모님덕에 어렸을때 그래도 많이 다녀왔네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005656.1.14",
          "form": "부럼네요ㅠ 저는 20대돼서 처음나가봤거든요",
          "original_form": "부럼네요ㅠ 저는 20대돼서 처음나가봤거든요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005656.1.15",
          "form": "코로나끝나면가고싶은곳있나요",
          "original_form": "코로나끝나면가고싶은곳있나요",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005656.1.16",
          "form": "저는 태국 가보고 싶어요!",
          "original_form": "저는 태국 가보고 싶어요!",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005656.1.17",
          "form": "관광보다는 힐링 먹방으로 ㅋㅋ",
          "original_form": "관광보다는 힐링 먹방으로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005656.1.18",
          "form": "힐링좋죠ㅠㅠ",
          "original_form": "힐링좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005656.1.19",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005656.1.20",
          "form": "코로나만 끝나면 ㅎㅎ",
          "original_form": "코로나만 끝나면 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100005656.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100005656.1.22",
          "form": "코로나끝나면",
          "original_form": "코로나끝나면",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100005656.1.23",
          "form": "동남아가고싶어요",
          "original_form": "동남아가고싶어요",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100005656.1.24",
          "form": "마스크 벗고 여행하는 날이 오겠죠..? ㅋㅋ",
          "original_form": "마스크 벗고 여행하는 날이 오겠죠..? ㅋㅋ",
