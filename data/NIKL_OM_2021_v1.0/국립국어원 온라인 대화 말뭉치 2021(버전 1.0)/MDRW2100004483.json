      "utterance": [
        {
          "id": "MDRW2100004483.1.1",
          "form": "다이어트 해보신적 있나요?",
          "original_form": "다이어트 해보신적 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004483.1.2",
          "form": "네네 내일부터 하려구요! 홈트로",
          "original_form": "네네 내일부터 하려구요! 홈트로",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004483.1.3",
          "form": "오ㅎ 플랭크랑 스쿼트 이런거 하나요?",
          "original_form": "오ㅎ 플랭크랑 스쿼트 이런거 하나요?",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004483.1.4",
          "form": "스쿼트 하려고용!",
          "original_form": "스쿼트 하려고용!",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004483.1.5",
          "form": "스쿼트 좋대요 ㅎ 전 게을러서 누가 옆에서 시켜야하지 아님 못하겠어요",
          "original_form": "스쿼트 좋대요 ㅎ 전 게을러서 누가 옆에서 시켜야하지 아님 못하겠어요",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004483.1.6",
          "form": "저도 엄청 게을러요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 엄청 게을러요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004483.1.7",
          "form": "최후의 보루로 운동을 하는거여요",
          "original_form": "최후의 보루로 운동을 하는거여요",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004483.1.8",
          "form": "건강을 위해서 해야되긴해요",
          "original_form": "건강을 위해서 해야되긴해요",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004483.1.9",
          "form": "근육량을 늘려야되는데 쉽지않네요",
          "original_form": "근육량을 늘려야되는데 쉽지않네요",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004483.1.10",
          "form": "그러니까요.. 굶으면 살은 빠지는데 다 근육이 빠지는거라..",
          "original_form": "그러니까요.. 굶으면 살은 빠지는데 다 근육이 빠지는거라..",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004483.1.11",
          "form": "건강히 슬림탄탄한 몸 갖고싶어요",
          "original_form": "건강히 슬림탄탄한 몸 갖고싶어요",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004483.1.12",
          "form": "스쿼트라도 집에서 해야겠어요ㅎ",
          "original_form": "스쿼트라도 집에서 해야겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004483.1.13",
          "form": "좋죠 자기관리 하면 뿌듯하잖아요",
          "original_form": "좋죠 자기관리 하면 뿌듯하잖아요",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004483.1.14",
          "form": "맞아요 건강에도 좋고",
          "original_form": "맞아요 건강에도 좋고",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004483.1.15",
          "form": "요즘은 코성형이 하고싶더라구요ㅠㅜ",
          "original_form": "요즘은 코성형이 하고싶더라구요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004483.1.16",
          "form": "성형은 무섭던데",
          "original_form": "성형은 무섭던데",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004483.1.17",
          "form": "코가 맘에 안드세요?",
          "original_form": "코가 맘에 안드세요?",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004483.1.18",
          "form": "네 약간ㅠㅠ 그런 느낌이졍ㅠㅠ",
          "original_form": "네 약간ㅠㅠ 그런 느낌이졍ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004483.1.19",
          "form": "성형은 신중히 하는게 좋은거같아요",
          "original_form": "성형은 신중히 하는게 좋은거같아요",
          "speaker_id": "1",
          "time": "20210821 17:39"
        }
      ]
