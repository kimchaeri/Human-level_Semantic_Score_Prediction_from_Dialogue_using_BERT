          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100031780.1.7",
          "form": "양조위 얘기만 해 ㅋㅋㅋ",
          "original_form": "양조위 얘기만 해 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100031780.1.8",
          "form": "아잌ㅋㅋㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100031780.1.9",
          "form": "찐이야",
          "original_form": "찐이야",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100031780.1.10",
          "form": "저도 양조위 보러 갔는걸요..",
          "original_form": "저도 양조위 보러 갔는걸요..",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100031780.1.11",
          "form": "아앗",
