      "utterance": [
        {
          "id": "MDRW2100033601.1.1",
          "form": "요즘 즐겨듣는 음악 있어요?",
          "original_form": "요즘 즐겨듣는 음악 있어요?",
          "speaker_id": "1",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100033601.1.2",
          "form": "저 요새 알콜프리 즐겨들어요",
          "original_form": "저 요새 알콜프리 즐겨들어요",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100033601.1.3",
          "form": "듣는 노래 있으세요?",
          "original_form": "듣는 노래 있으세요?",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100033601.1.4",
          "form": "어 저도 알콜프리 좋아해요ㅎ",
          "original_form": "어 저도 알콜프리 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100033601.1.5",
          "form": "약간 휴양지에 온 듯한 나른한 느낌이 좋더라고요",
          "original_form": "약간 휴양지에 온 듯한 나른한 느낌이 좋더라고요",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100033601.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100033601.1.7",
          "form": "알콜프리 너뮤 제 취향이에요",
          "original_form": "알콜프리 너뮤 제 취향이에요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100033601.1.8",
          "form": "그리고 스테이시 asap도 즐겨 들어요",
          "original_form": "그리고 스테이시 asap도 즐겨 들어요",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100033601.1.9",
          "form": "오 저도 완전 좋아해요",
          "original_form": "오 저도 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033601.1.10",
          "form": "꾹꾹이춤ㅋㅋㅋㅋㅋ",
          "original_form": "꾹꾹이춤ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033601.1.11",
          "form": "에이쎕 내 빵조가리..",
          "original_form": "에이쎕 내 빵조가리..",
          "speaker_id": "1",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033601.1.12",
          "form": "ㅋㅋ노래 약간 뭐라그래야하지 이런 표현이 어울리나 모르겠는데 노래의 느낌을 표현하라면 블럭같아요",
          "original_form": "ㅋㅋ노래 약간 뭐라그래야하지 이런 표현이 어울리나 모르겠는데 노래의 느낌을 표현하라면 블럭같아요",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033601.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 정말요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 정말요?",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033601.1.14",
          "form": "너무 중독성있어요",
          "original_form": "너무 중독성있어요",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033601.1.15",
          "form": "넥스트레벨두요",
          "original_form": "넥스트레벨두요",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033601.1.16",
          "form": "저도 그거 좋아해요",
          "original_form": "저도 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033601.1.17",
          "form": "혹시..",
          "original_form": "혹시..",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033601.1.18",
          "form": "마피아도...?",
          "original_form": "마피아도...?",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033601.1.19",
          "form": "그것도 좋아해요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그것도 좋아해요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033601.1.20",
          "form": "약간 다 비슷한 계열 같기두",
          "original_form": "약간 다 비슷한 계열 같기두",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033601.1.21",
          "form": "알콜프리 빼구요",
          "original_form": "알콜프리 빼구요",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033601.1.22",
          "form": "그럼 설마 던 던 댄스도..?",
          "original_form": "그럼 설마 던 던 댄스도..?",
          "speaker_id": "1",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033601.1.23",
          "form": "좋아히나요",
          "original_form": "좋아히나요",
          "speaker_id": "1",
          "time": "20210824 16:56"
        },
