      "utterance": [
        {
          "id": "MDRW2100007907.1.1",
          "form": "name2님은 하시는 게임이 있으신가요?",
          "original_form": "&name2&님은 하시는 게임이 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007907.1.2",
          "form": "name2가 아닙니다 name2입니다...",
          "original_form": "&name2&가 아닙니다 &name2&입니다...",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007907.1.3",
          "form": "name2요",
          "original_form": "&name2&요",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007907.1.4",
          "form": "죄송해요 name2님은 어떤 게임을 하세요?",
          "original_form": "죄송해요 &name2&님은 어떤 게임을 하세요?",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007907.1.5",
          "form": "돈 벌어서 살 거예요 카마로를...",
          "original_form": "돈 벌어서 살 거예요 카마로를...",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007907.1.6",
          "form": "저는 1년 정도 로오히를 했어요.",
          "original_form": "저는 1년 정도 로오히를 했어요.",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007907.1.7",
          "form": "그래요 서울 집 보단 카마로가 싸죠.. 요즘세상 그게 더 빠를거에요 흑흑..",
          "original_form": "그래요 서울 집 보단 카마로가 싸죠.. 요즘세상 그게 더 빠를거에요 흑흑..",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007907.1.8",
          "form": "로드 오브 히어로즈 요.",
          "original_form": "로드 오브 히어로즈 요.",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007907.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 겜 얘기하다 왜 급 현실 얘기하세요 마음 아프게...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 겜 얘기하다 왜 급 현실 얘기하세요 마음 아프게...",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007907.1.10",
          "form": "로오히는 재미있나요?",
          "original_form": "로오히는 재미있나요?",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007907.1.11",
          "form": "플레이 자체는 개노잼인데",
          "original_form": "플레이 자체는 개노잼인데",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007907.1.12",
          "form": "캐릭터 보는 맛으로 합니다...",
          "original_form": "캐릭터 보는 맛으로 합니다...",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007907.1.13",
          "form": "페그오랑 비슷하네요",
          "original_form": "페그오랑 비슷하네요",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007907.1.14",
          "form": "뽑기 게임이 대체로 그렇죠 머...",
          "original_form": "뽑기 게임이 대체로 그렇죠 머...",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007907.1.15",
          "form": "돈내고 캐릭터를 사고 지루하게 키우고.",
          "original_form": "돈내고 캐릭터를 사고 지루하게 키우고.",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007907.1.16",
          "form": "마저요... 지루해요...",
          "original_form": "마저요... 지루해요...",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007907.1.17",
          "form": "너무 졸려요..",
          "original_form": "너무 졸려요..",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007907.1.18",
          "form": "하다보면 고생 하는 느낌",
          "original_form": "하다보면 고생 하는 느낌",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007907.1.19",
          "form": "저도 최근엔 자동전투 돌려놓고 딴짓 했어요.",
          "original_form": "저도 최근엔 자동전투 돌려놓고 딴짓 했어요.",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007907.1.20",
          "form": "이젠 너무 질려서",
          "original_form": "이젠 너무 질려서",
          "speaker_id": "2",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007907.1.21",
          "form": "그나마도 끊었습니다.",
          "original_form": "그나마도 끊었습니다.",
          "speaker_id": "2",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007907.1.22",
          "form": "자동전투 좋네요",
          "original_form": "자동전투 좋네요",
          "speaker_id": "1",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007907.1.23",
          "form": "금 롷 7일 째",
          "original_form": "금 롷 7일 째",
          "speaker_id": "2",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007907.1.24",
          "form": "담배끊듯 얘기하시네요",
          "original_form": "담배끊듯 얘기하시네요",
          "speaker_id": "1",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007907.1.25",
          "form": "ㅋㅋㅋ 웹서핑하다보면 자꾸 소식이 들려서요...",
          "original_form": "ㅋㅋㅋ 웹서핑하다보면 자꾸 소식이 들려서요...",
          "speaker_id": "2",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007907.1.26",
          "form": "신규 정보 들을 때마다 접속하고 싶어서 손 떨려요.",
          "original_form": "신규 정보 들을 때마다 접속하고 싶어서 손 떨려요.",
          "speaker_id": "2",
          "time": "20210822 11:50"
        },
        {
          "id": "MDRW2100007907.1.27",
          "form": "게임 이콜 도박",
          "original_form": "게임 이콜 도박",
          "speaker_id": "1",
