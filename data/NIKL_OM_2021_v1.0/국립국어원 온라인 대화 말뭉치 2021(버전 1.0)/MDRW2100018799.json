      "utterance": [
        {
          "id": "MDRW2100018799.1.1",
          "form": "점심먹음?",
          "original_form": "점심먹음?",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018799.1.2",
          "form": "ㄴㄴ 아침을 조금 늦게 먹어서 아직 배가 안 고파",
          "original_form": "ㄴㄴ 아침을 조금 늦게 먹어서 아직 배가 안 고파",
          "speaker_id": "1",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018799.1.3",
          "form": "너는 먹었어?",
          "original_form": "너는 먹었어?",
          "speaker_id": "1",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018799.1.4",
          "form": "응 육전비빔밥 괜찮았어ㅎ",
          "original_form": "응 육전비빔밥 괜찮았어ㅎ",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018799.1.5",
          "form": "몇시 먹게?",
          "original_form": "몇시 먹게?",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018799.1.6",
          "form": "와 육전비빔밥 맛있겠다 비빔밥 육전하고 많이 팔더라 역앞에도 저거 파는곳 맛집이라고 블로그에 올라와있던데",
          "original_form": "와 육전비빔밥 맛있겠다 비빔밥 육전하고 많이 팔더라 역앞에도 저거 파는곳 맛집이라고 블로그에 올라와있던데",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018799.1.7",
          "form": "아 비빔밥 아니고 냉면인가...?;;",
          "original_form": "아 비빔밥 아니고 냉면인가...?;;",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018799.1.8",
          "form": "난 안먹을수도 있고 먹을수도 있고 배고플때?",
          "original_form": "난 안먹을수도 있고 먹을수도 있고 배고플때?",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018799.1.9",
          "form": "ㅎㅎㅎ오늘은 하루 2끼 먹겠네ㅎ",
          "original_form": "ㅎㅎㅎ오늘은 하루 2끼 먹겠네ㅎ",
          "speaker_id": "2",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100018799.1.10",
          "form": "이제 좀 있음 배고플거 같아 ㅎㅎ",
          "original_form": "이제 좀 있음 배고플거 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018799.1.11",
          "form": "점심 먹을 메뉴는 생각중?",
          "original_form": "점심 먹을 메뉴는 생각중?",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018799.1.12",
          "form": "샌드위치 먹고싶은데 나가기가 싫네 집에있는걸로 먹어야겠어",
          "original_form": "샌드위치 먹고싶은데 나가기가 싫네 집에있는걸로 먹어야겠어",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018799.1.13",
          "form": "고구마를 먹을까",
          "original_form": "고구마를 먹을까",
          "speaker_id": "1",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018799.1.14",
          "form": "고구마 밤고구마 생각난다ㅎ\r\n맛있는 고구마야?",
          "original_form": "고구마 밤고구마 생각난다ㅎ\r\n맛있는 고구마야?",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018799.1.15",
          "form": "요즘 고구마는 거의 밤고구마더라",
          "original_form": "요즘 고구마는 거의 밤고구마더라",
          "speaker_id": "1",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018799.1.16",
          "form": "나 카레에 밥 먹는다~~",
          "original_form": "나 카레에 밥 먹는다~~",
          "speaker_id": "1",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018799.1.17",
          "form": "카레 맛있겠다",
          "original_form": "카레 맛있겠다",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018799.1.18",
          "form": "닭살 카레?",
          "original_form": "닭살 카레?",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018799.1.19",
          "form": "ㅇㅇ전에 만든거 냉장고에 넣어놨었어",
          "original_form": "ㅇㅇ전에 만든거 냉장고에 넣어놨었어",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018799.1.20",
          "form": "당근양파많이들어가서맛있당",
          "original_form": "당근양파많이들어가서맛있당",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018799.1.21",
          "form": "당근 많이많이",
          "original_form": "당근 많이많이",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018799.1.22",
          "form": "너가 만듦?",
          "original_form": "너가 만듦?",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018799.1.23",
          "form": "아니 엄마가 만들었엉!",
          "original_form": "아니 엄마가 만들었엉!",
          "speaker_id": "1",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018799.1.24",
          "form": "엄마 솜씨 음식맛 좋아",
          "original_form": "엄마 솜씨 음식맛 좋아",
          "speaker_id": "2",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018799.1.25",
          "form": "ㅇㅇ카레는 근데 내가 해도 똑같을거 같아 ㅋㅋ",
          "original_form": "ㅇㅇ카레는 근데 내가 해도 똑같을거 같아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:07"
        }
      ]
    }
