      "utterance": [
        {
          "id": "MDRW2100032508.1.1",
          "form": "잘 걸으면",
          "original_form": "잘 걸으면",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032508.1.2",
          "form": "그 고관절이 아프대",
          "original_form": "그 고관절이 아프대",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032508.1.3",
          "form": "고관절..?",
          "original_form": "고관절..?",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032508.1.4",
          "form": "그게어디야",
          "original_form": "그게어디야",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032508.1.5",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032508.1.6",
          "form": "골반에서",
          "original_form": "골반에서",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.7",
          "form": "허벅지 내려가는",
          "original_form": "허벅지 내려가는",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.8",
          "form": "그 부분",
          "original_form": "그 부분",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.9",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.10",
          "form": "맞아 나 걸을때",
          "original_form": "맞아 나 걸을때",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.11",
          "form": "허벅지근육도 골고루써야하는데",
          "original_form": "허벅지근육도 골고루써야하는데",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.12",
          "form": "종아리근육만 씀",
          "original_form": "종아리근육만 씀",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.13",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.14",
          "form": "그래서 걷고나면",
          "original_form": "그래서 걷고나면",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.15",
          "form": "그래서 나 피티 처음할때",
          "original_form": "그래서 나 피티 처음할때",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032508.1.16",
          "form": "다리 더 피곤하고 뭉침",
          "original_form": "다리 더 피곤하고 뭉침",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032508.1.17",
          "form": "걷는것 부터 배웠음",
          "original_form": "걷는것 부터 배웠음",
          "speaker_id": "2",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032508.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032508.1.19",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032508.1.20",
          "form": "확실히 다르구나",
          "original_form": "확실히 다르구나",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032508.1.21",
          "form": "어떤식으로 가르텨줘?",
          "original_form": "어떤식으로 가르텨줘?",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032508.1.22",
          "form": "복근에 힘주고 걸으래",
          "original_form": "복근에 힘주고 걸으래",
          "speaker_id": "2",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032508.1.23",
          "form": "오ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032508.1.24",
          "form": "복근하고 엉덩이 근육이 연결되어 있다고 그런식으로",
          "original_form": "복근하고 엉덩이 근육이 연결되어 있다고 그런식으로",
          "speaker_id": "2",
          "time": "20210905 19:51"
        },
        {
          "id": "MDRW2100032508.1.25",
          "form": "말했었어",
          "original_form": "말했었어",
          "speaker_id": "2",
          "time": "20210905 19:51"
        },
        {
          "id": "MDRW2100032508.1.26",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210905 19:51"
        },
        {
          "id": "MDRW2100032508.1.27",
          "form": "신기하다",
