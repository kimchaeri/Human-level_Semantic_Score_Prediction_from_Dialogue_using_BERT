      "utterance": [
        {
          "id": "MDRW2100012142.1.1",
          "form": "피자도 좋아하시나요?",
          "original_form": "피자도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 19:34"
        },
        {
          "id": "MDRW2100012142.1.2",
          "form": "앗! 그러면 혹시 그거 드셔보셨어요?",
          "original_form": "앗! 그러면 혹시 그거 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210824 19:34"
        },
        {
          "id": "MDRW2100012142.1.3",
          "form": "미친피자요!",
          "original_form": "미친피자요!",
          "speaker_id": "2",
          "time": "20210824 19:34"
        },
        {
          "id": "MDRW2100012142.1.4",
          "form": "피자 좋아해요 어렸을땐 야채가 많아서",
          "original_form": "피자 좋아해요 어렸을땐 야채가 많아서",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.5",
          "form": "미친피자요? 첨들어요!",
          "original_form": "미친피자요? 첨들어요!",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.6",
          "form": "별로안좋아했는데 지금은 조아해요",
          "original_form": "별로안좋아했는데 지금은 조아해요",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.7",
          "form": "브랜드이름이 그거에요?",
          "original_form": "브랜드이름이 그거에요?",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.8",
          "form": "이게 신생브랜드인거같은데",
          "original_form": "이게 신생브랜드인거같은데",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.9",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.10",
          "form": "토핑이 듬뿍올라와있어요!",
          "original_form": "토핑이 듬뿍올라와있어요!",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.11",
          "form": "아하 여기 지방이라 아직 안생겼나봐여",
          "original_form": "아하 여기 지방이라 아직 안생겼나봐여",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.12",
          "form": "어떤 토핑피자 젤좋아하세요?",
          "original_form": "어떤 토핑피자 젤좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012142.1.13",
          "form": "저는 쉬림프요!!! 그리고 콘치즈피자도 조아해요",
          "original_form": "저는 쉬림프요!!! 그리고 콘치즈피자도 조아해요",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012142.1.14",
          "form": "어떤피자 자주 드세요?",
          "original_form": "어떤피자 자주 드세요?",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012142.1.15",
          "form": "저도 새우랑 콘 좋아해요ㅜ",
          "original_form": "저도 새우랑 콘 좋아해요ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012142.1.16",
          "form": "저도 무조건 새우올라간거 시켜요",
          "original_form": "저도 무조건 새우올라간거 시켜요",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012142.1.17",
          "form": "유로코피자아세요?",
          "original_form": "유로코피자아세요?",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012142.1.18",
          "form": "새우 최고예요!",
          "original_form": "새우 최고예요!",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012142.1.19",
          "form": "아니요 처음들어봤어요!",
          "original_form": "아니요 처음들어봤어요!",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012142.1.20",
          "form": "이게 밑에지방 체인점인가보네요",
          "original_form": "이게 밑에지방 체인점인가보네요",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.21",
          "form": "여기도 토핑듬뿍에 가격저렴하고 맛있거든요 일주일에 한번은 시켜먹어요ㅜ",
          "original_form": "여기도 토핑듬뿍에 가격저렴하고 맛있거든요 일주일에 한번은 시켜먹어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.22",
          "form": "앜ㅋㅋㅋ 헐 너무좋아요",
          "original_form": "앜ㅋㅋㅋ 헐 너무좋아요",
          "speaker_id": "2",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.23",
          "form": "저옛날에 피자넘좋아해서 미피잘나갈적에..알바도했었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저옛날에 피자넘좋아해서 미피잘나갈적에..알바도했었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.24",
          "form": "토핑 듬뿍주는곳 최고",
          "original_form": "토핑 듬뿍주는곳 최고",
          "speaker_id": "2",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.25",
          "form": "미피하니까 생각나네요 고구마 크러스트!",
          "original_form": "미피하니까 생각나네요 고구마 크러스트!",
          "speaker_id": "2",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.26",
          "form": "하프앤하프엄청먹었었는데 맛있었죠..",
          "original_form": "하프앤하프엄청먹었었는데 맛있었죠..",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.27",
          "form": "안힘드셨어요?",
          "original_form": "안힘드셨어요?",
          "speaker_id": "2",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.28",
          "form": "네 엣지 골드",
          "original_form": "네 엣지 골드",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012142.1.29",
          "form": "그땐 엄청 바빳을꺼같은데",
          "original_form": "그땐 엄청 바빳을꺼같은데",
          "speaker_id": "2",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100012142.1.30",
          "form": "힘들긴했는데 친구들이랑 같이했어서",
          "original_form": "힘들긴했는데 친구들이랑 같이했어서",
          "speaker_id": "1",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100012142.1.31",
          "form": "재밌게 오래잘했어요 ㅎㅎ",
          "original_form": "재밌게 오래잘했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:39"
        }
      ]
    }
