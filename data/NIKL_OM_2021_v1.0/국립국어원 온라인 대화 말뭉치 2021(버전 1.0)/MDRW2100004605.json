      "utterance": [
        {
          "id": "MDRW2100004605.1.1",
          "form": "아니여 저는 안 기르는데",
          "original_form": "아니여 저는 안 기르는데",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004605.1.2",
          "form": "가까운 곳 사는 친척이 길러서 자주 보고 있어여",
          "original_form": "가까운 곳 사는 친척이 길러서 자주 보고 있어여",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004605.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004605.1.4",
          "form": "저는 햄지 길러요!",
          "original_form": "저는 햄지 길러요!",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004605.1.5",
          "form": "드워프 햄스터인데 작고 귀여워요",
          "original_form": "드워프 햄스터인데 작고 귀여워요",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004605.1.6",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004605.1.7",
          "form": "이름이 먼가여?",
          "original_form": "이름이 먼가여?",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004605.1.8",
          "form": "name3에요 귀엽죠ㅎㅎ",
          "original_form": "&name3&에요 귀엽죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004605.1.9",
          "form": "밥도 잘먹어요",
          "original_form": "밥도 잘먹어요",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004605.1.10",
          "form": "ㅋㅋㅋㅋ되게 직관적인 이름이네요 귀여워요",
          "original_form": "ㅋㅋㅋㅋ되게 직관적인 이름이네요 귀여워요",
          "speaker_id": "2",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004605.1.11",
          "form": "저는 어릴 때 햄스터를 기르는게 꿈이었는데..",
          "original_form": "저는 어릴 때 햄스터를 기르는게 꿈이었는데..",
          "speaker_id": "2",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004605.1.12",
          "form": "털은 부드럽나요",
          "original_form": "털은 부드럽나요",
          "speaker_id": "2",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004605.1.13",
          "form": "엄청  부드러워요 생각하시는 촉감 딱 그대로일거에요",
          "original_form": "엄청  부드러워요 생각하시는 촉감 딱 그대로일거에요",
          "speaker_id": "1",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004605.1.14",
          "form": "펄햄스터라 털도 하얗고 깜찍해요",
          "original_form": "펄햄스터라 털도 하얗고 깜찍해요",
          "speaker_id": "1",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004605.1.15",
          "form": "사진첨부가 안되는게 아쉽네요",
          "original_form": "사진첨부가 안되는게 아쉽네요",
          "speaker_id": "2",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004605.1.16",
          "form": "좋아하는 간식은 뭔가요",
          "original_form": "좋아하는 간식은 뭔가요",
          "speaker_id": "2",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004605.1.17",
          "form": "그러게요ㅠ 좋아하는 간식은 밀웜과 웨하스처럼 생긴 간식이있어요",
          "original_form": "그러게요ㅠ 좋아하는 간식은 밀웜과 웨하스처럼 생긴 간식이있어요",
          "speaker_id": "1",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004605.1.18",
          "form": "이 2개는 자다가도 벌떡 일어나서 먹으러 온답니다",
          "original_form": "이 2개는 자다가도 벌떡 일어나서 먹으러 온답니다",
          "speaker_id": "1",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004605.1.19",
          "form": "생각만해도 귀여워요",
          "original_form": "생각만해도 귀여워요",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004605.1.20",
          "form": "저 제가 키우는 개는 아니지만 자주보는 친척 강아지",
          "original_form": "저 제가 키우는 개는 아니지만 자주보는 친척 강아지",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004605.1.21",
          "form": "종이 뭘지 맞춰보시겠어요",
          "original_form": "종이 뭘지 맞춰보시겠어요",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004605.1.22",
          "form": "음... 포메라니안일거 같아요!",
          "original_form": "음... 포메라니안일거 같아요!",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004605.1.23",
          "form": "땡",
          "original_form": "땡",
          "speaker_id": "2",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004605.1.24",
          "form": "힌트는 참지 않는 강아쥐 입니다.",
          "original_form": "힌트는 참지 않는 강아쥐 입니다.",
          "speaker_id": "2",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004605.1.25",
          "form": "치와와!!!!",
          "original_form": "치와와!!!!",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004605.1.26",
          "form": "좀 더 털이 길어요",
          "original_form": "좀 더 털이 길어요",
          "speaker_id": "2",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004605.1.27",
          "form": "말티즈인가요?",
          "original_form": "말티즈인가요?",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004605.1.28",
          "form": "정답입니다!!!ㅋㅋㅋ",
          "original_form": "정답입니다!!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004605.1.29",
          "form": "우와 말티즈 너무 귀여워서 좋아요 하얗고 부드럽고..",
          "original_form": "우와 말티즈 너무 귀여워서 좋아요 하얗고 부드럽고..",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004605.1.30",
          "form": "착하고 순하니 귀여울거같아요",
          "original_form": "착하고 순하니 귀여울거같아요",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004605.1.31",
          "form": "몇살인가요?",
          "original_form": "몇살인가요?",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004605.1.32",
          "form": "맞아요 말티즈 평소에 좋아하는 견종은 아니었는데",
          "original_form": "맞아요 말티즈 평소에 좋아하는 견종은 아니었는데",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004605.1.33",
          "form": "계속 보니 귀엽더라고요",
          "original_form": "계속 보니 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004605.1.34",
          "form": "나이는 2살이에요!!",
          "original_form": "나이는 2살이에요!!",
          "speaker_id": "2",
          "time": "20210821 17:47"
        }
      ]
    }
