      "utterance": [
        {
          "id": "MDRW2100015753.1.1",
          "form": "최근에 구매하신 물건 있으세요?",
          "original_form": "최근에 구매하신 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100015753.1.2",
          "form": "향수를 구매했어요~",
          "original_form": "향수를 구매했어요~",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100015753.1.3",
          "form": "무슨 향인지 여쭤봐도 될까요? 궁금",
          "original_form": "무슨 향인지 여쭤봐도 될까요? 궁금",
          "speaker_id": "1",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100015753.1.4",
          "form": "불가리 아메시스트요! 아시나요~~?",
          "original_form": "불가리 아메시스트요! 아시나요~~?",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015753.1.5",
          "form": "그... 자몽향? 맞나요?",
          "original_form": "그... 자몽향? 맞나요?",
          "speaker_id": "1",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015753.1.6",
          "form": "아니요 ㅎㅎ 상큼한 계통은 아니에요!",
          "original_form": "아니요 ㅎㅎ 상큼한 계통은 아니에요!",
          "speaker_id": "2",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015753.1.7",
          "form": "저는 크레파스 향이라고 부릅니다...ㅋㅋㅋ",
          "original_form": "저는 크레파스 향이라고 부릅니다...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015753.1.8",
          "form": "평소 어떤 향수 사용하세요?",
          "original_form": "평소 어떤 향수 사용하세요?",
          "speaker_id": "2",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015753.1.9",
          "form": "저는 시트러스계열 좋아해요",
          "original_form": "저는 시트러스계열 좋아해요",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015753.1.10",
          "form": "호~ 추천해주신다면 어떤향수를 추천하시나요",
          "original_form": "호~ 추천해주신다면 어떤향수를 추천하시나요",
          "speaker_id": "2",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015753.1.11",
          "form": "추천할만큼 향수를 잘 아는편은 아니구요ㅋㅋㅋㅋ 저는 조말론 라임바질앤만다린 즐겨쓰고있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "추천할만큼 향수를 잘 아는편은 아니구요ㅋㅋㅋㅋ 저는 조말론 라임바질앤만다린 즐겨쓰고있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015753.1.12",
          "form": "오오 라임과 바질과 만다린이 들어간 향수군요 ㅋㅋㅋㅋㅋ",
          "original_form": "오오 라임과 바질과 만다린이 들어간 향수군요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015753.1.13",
          "form": "이름 상당히 직관적인것 ㅋㅋㅋ",
          "original_form": "이름 상당히 직관적인것 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015753.1.14",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 이름에서 뿜어져나오는 상!!큼!!!",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 이름에서 뿜어져나오는 상!!큼!!!",
          "speaker_id": "1",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015753.1.15",
          "form": "ㅋㅋㅋㅋ 상큼한 향 진짜 좋아하시는구나! 저는... 저도 사실 딱히 취향은 없는데 달달한 쪽을 좋아하는거 같아요",
          "original_form": "ㅋㅋㅋㅋ 상큼한 향 진짜 좋아하시는구나! 저는... 저도 사실 딱히 취향은 없는데 달달한 쪽을 좋아하는거 같아요",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015753.1.16",
          "form": "name1님 이름에서도 느껴지는 상큼함ㅋㅋ",
          "original_form": "&name1&님 이름에서도 느껴지는 상큼함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015753.1.17",
          "form": "최근에 구입한 물건은 뭐가 있으세요?",
          "original_form": "최근에 구입한 물건은 뭐가 있으세요?",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015753.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 최근에....아 폼롤러샀어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 최근에....아 폼롤러샀어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100015753.1.19",
          "form": "아아 ㅋㅋㅋ 저희집에도 몇년전에 구입한 폼롤러가...",
          "original_form": "아아 ㅋㅋㅋ 저희집에도 몇년전에 구입한 폼롤러가...",
          "speaker_id": "2",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015753.1.20",
          "form": "이게 은근히 자리를 많이 차지해서 ㅋㅋㅋ",
          "original_form": "이게 은근히 자리를 많이 차지해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015753.1.21",
          "form": "좀 짧은걸로 살걸 그랬어요ㅜㅜ",
          "original_form": "좀 짧은걸로 살걸 그랬어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015753.1.22",
          "form": "ㅋㅋㅋㅋㅋ저도 긴걸로 샀는데 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저도 긴걸로 샀는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015753.1.23",
          "form": "짧은걸 산 친구가 짧은거 너무 불편하다고 해서요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "짧은걸 산 친구가 짧은거 너무 불편하다고 해서요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015753.1.24",
          "form": "긴게 자리차지는 하지만 쓰기엔 편하대요",
          "original_form": "긴게 자리차지는 하지만 쓰기엔 편하대요",
          "speaker_id": "1",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015753.1.25",
          "form": "ㅋㅋㅋ그렇긴해요 짧으면 감질맛 날것 같아요 왠지 ㅋㅋ",
          "original_form": "ㅋㅋㅋ그렇긴해요 짧으면 감질맛 날것 같아요 왠지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:10"
        },
        {
          "id": "MDRW2100015753.1.26",
          "form": "지금 그 폼롤러는 제 침대옆에 세워져있다는...",
          "original_form": "지금 그 폼롤러는 제 침대옆에 세워져있다는...",
          "speaker_id": "2",
          "time": "20210826 01:10"
        },
        {
          "id": "MDRW2100015753.1.27",
          "form": "아 저 최근에 믹서기 샀는데 믹서기가 너무커요...",
          "original_form": "아 저 최근에 믹서기 샀는데 믹서기가 너무커요...",
          "speaker_id": "2",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100015753.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 요며칠 비명지르면서 잘쓰고잇어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 요며칠 비명지르면서 잘쓰고잇어요",
          "speaker_id": "1",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100015753.1.29",
