        {
          "id": "MDRW2100034447.1.1",
          "form": "프리한19는 순위를 순위 정해주는 사람이 있다",
          "original_form": "프리한19는 순위를 순위 정해주는 사람이 있다",
          "speaker_id": "2",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100034447.1.2",
          "form": "아니 똑같은 프로를 진행자만 바꺼서 하는거?",
          "original_form": "아니 똑같은 프로를 진행자만 바꺼서 하는거?",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100034447.1.3",
          "form": "진행자 3명 똑같은데 아나운서 남자3명",
          "original_form": "진행자 3명 똑같은데 아나운서 남자3명",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100034447.1.4",
          "form": "차달남은 2 명ㅋ",
          "original_form": "차달남은 2 명ㅋ",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100034447.1.5",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100034447.1.6",
          "form": "어떤 순위라고 했지?",
          "original_form": "어떤 순위라고 했지?",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100034447.1.7",
          "form": "역사 미스터리",
          "original_form": "역사 미스터리",
          "speaker_id": "2",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100034447.1.8",
          "form": "ㅋㅋ 그런거 많겠지",
          "original_form": "ㅋㅋ 그런거 많겠지",
          "speaker_id": "1",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100034447.1.9",
          "form": "주제가 그리 재밌지는 안을꺼 같긴하다",
          "original_form": "주제가 그리 재밌지는 안을꺼 같긴하다",
          "speaker_id": "2",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100034447.1.10",
          "form": "연예인 순위 이런게 재밌지",
          "original_form": "연예인 순위 이런게 재밌지",
          "speaker_id": "1",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100034447.1.11",
          "form": "이십세기 힛트쏭 하는데 이거 보까",
          "original_form": "이십세기 힛트쏭 하는데 이거 보까",
          "speaker_id": "2",
          "time": "20210828 11:32"
        },
        {
          "id": "MDRW2100034447.1.12",
          "form": "그래라",
          "original_form": "그래라",
          "speaker_id": "1",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100034447.1.13",
          "form": "북치고장구치고 싱어송라이터 란다",
          "original_form": "북치고장구치고 싱어송라이터 란다",
