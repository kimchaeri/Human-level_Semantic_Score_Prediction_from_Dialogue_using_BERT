        },
        {
          "id": "MDRW2100032934.1.2",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210906 08:50"
        },
        {
          "id": "MDRW2100032934.1.3",
          "form": "저도 지금 작년부터 어떤 운동도 센터에서는 못하고 있어요",
          "original_form": "저도 지금 작년부터 어떤 운동도 센터에서는 못하고 있어요",
          "speaker_id": "2",
          "time": "20210906 08:51"
        },
        {
          "id": "MDRW2100032934.1.4",
          "form": "위험하다고 느껴져서 못가고 있죠",
          "original_form": "위험하다고 느껴져서 못가고 있죠",
          "speaker_id": "2",
          "time": "20210906 08:51"
        },
        {
          "id": "MDRW2100032934.1.5",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210906 08:55"
        },
        {
          "id": "MDRW2100032934.1.6",
          "form": "건강 생각하다가",
          "original_form": "건강 생각하다가",
          "speaker_id": "1",
          "time": "20210906 08:55"
        },
        {
          "id": "MDRW2100032934.1.7",
          "form": "코로나가...",
          "original_form": "코로나가...",
          "speaker_id": "1",
          "time": "20210906 08:56"
        },
        {
          "id": "MDRW2100032934.1.8",
          "form": "그렇게 될 수 있으니",
          "original_form": "그렇게 될 수 있으니",
          "speaker_id": "1",
          "time": "20210906 08:56"
        },
        {
          "id": "MDRW2100032934.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210906 08:59"
        },
        {
          "id": "MDRW2100032934.1.10",
          "form": "그래서 굉장히 좀이 쑤서요",
          "original_form": "그래서 굉장히 좀이 쑤서요",
          "speaker_id": "2",
          "time": "20210906 08:59"
        },
        {
          "id": "MDRW2100032934.1.11",
          "form": "아무것도 안할 순 없어서 요새는 산책해요",
          "original_form": "아무것도 안할 순 없어서 요새는 산책해요",
          "speaker_id": "2",
          "time": "20210906 08:59"
        },
        {
          "id": "MDRW2100032934.1.12",
          "form": "공원에 있는 기구들 장갑끼고 좀 사용하구요",
          "original_form": "공원에 있는 기구들 장갑끼고 좀 사용하구요",
          "speaker_id": "2",
          "time": "20210906 09:00"
        },
        {
          "id": "MDRW2100032934.1.13",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 09:00"
        },
        {
          "id": "MDRW2100032934.1.14",
          "form": "산책 코스는",
          "original_form": "산책 코스는",
          "speaker_id": "1",
          "time": "20210906 09:00"
        },
        {
          "id": "MDRW2100032934.1.15",
          "form": "따로 있으신가요",
          "original_form": "따로 있으신가요",
          "speaker_id": "1",
          "time": "20210906 09:00"
        },
        {
          "id": "MDRW2100032934.1.16",
          "form": "공원은 좀 탁 트인 공간이니까 마스크 쓰고 움직이면 좀 걱정이 덜 돼요",
          "original_form": "공원은 좀 탁 트인 공간이니까 마스크 쓰고 움직이면 좀 걱정이 덜 돼요",
          "speaker_id": "2",
          "time": "20210906 09:00"
        },
        {
          "id": "MDRW2100032934.1.17",
          "form": "네 코스 있어요",
          "original_form": "네 코스 있어요",
          "speaker_id": "2",
          "time": "20210906 09:01"
        },
        {
          "id": "MDRW2100032934.1.18",
          "form": "아 공원쪽이신가요",
          "original_form": "아 공원쪽이신가요",
          "speaker_id": "1",
          "time": "20210906 09:01"
        },
        {
          "id": "MDRW2100032934.1.19",
          "form": "공원이 2개인데",
          "original_form": "공원이 2개인데",
          "speaker_id": "2",
          "time": "20210906 09:01"
        },
        {
          "id": "MDRW2100032934.1.20",
          "form": "하나는 평지 트랙돌듯이 있는 호수공원이구요",
          "original_form": "하나는 평지 트랙돌듯이 있는 호수공원이구요",
          "speaker_id": "2",
          "time": "20210906 09:01"
        },
        {
          "id": "MDRW2100032934.1.21",
          "form": "하나는 작은 언덕공원이라서 꼭 등산하는 기분도 들고 그래요",
          "original_form": "하나는 작은 언덕공원이라서 꼭 등산하는 기분도 들고 그래요",
          "speaker_id": "2",
          "time": "20210906 09:02"
        },
        {
          "id": "MDRW2100032934.1.22",
          "form": "전 언덕공원을 주로 이용하죠",
          "original_form": "전 언덕공원을 주로 이용하죠",
          "speaker_id": "2",
          "time": "20210906 09:02"
        },
        {
          "id": "MDRW2100032934.1.23",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100032934.1.24",
          "form": "그 뭐지",
          "original_form": "그 뭐지",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100032934.1.25",
          "form": "저는 제가 부천-인천쪽 사람인지라",
          "original_form": "저는 제가 부천-인천쪽 사람인지라",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100032934.1.26",
          "form": "호수공원하면",
          "original_form": "호수공원하면",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100032934.1.27",
          "form": "부천에 있는게 먼저 생각나서",
          "original_form": "부천에 있는게 먼저 생각나서",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100032934.1.28",
          "form": "머릿속에서 그려지네요",
          "original_form": "머릿속에서 그려지네요",
          "speaker_id": "1",
          "time": "20210906 09:03"
        },
        {
          "id": "MDRW2100032934.1.29",
          "form": "가운데 저수지가 있고 둘레로 길 만들고 나무 심어놓고",
          "original_form": "가운데 저수지가 있고 둘레로 길 만들고 나무 심어놓고",
          "speaker_id": "2",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100032934.1.30",
          "form": "그런데 거기는 평지라 사람들이 많아서",
          "original_form": "그런데 거기는 평지라 사람들이 많아서",
          "speaker_id": "2",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100032934.1.31",
          "form": "사람 별로 없는 언덕 공원 가죠",
          "original_form": "사람 별로 없는 언덕 공원 가죠",
          "speaker_id": "2",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100032934.1.32",
          "form": "하기야",
          "original_form": "하기야",
          "speaker_id": "1",
          "time": "20210906 09:33"
        },
        {
          "id": "MDRW2100032934.1.33",
          "form": "물 따라(?) 걷는 사람이 많죠",
          "original_form": "물 따라(?) 걷는 사람이 많죠",
          "speaker_id": "1",
          "time": "20210906 09:33"
        },
        {
          "id": "MDRW2100032934.1.34",
          "form": "언덕보다 평평하기도 할테고",
          "original_form": "언덕보다 평평하기도 할테고",
          "speaker_id": "1",
          "time": "20210906 09:33"
        },
        {
          "id": "MDRW2100032934.1.35",
          "form": "그렇죠 물도 보고 봄에는 벚꽃터널이 만들어져서 정말 예쁘거든요",
          "original_form": "그렇죠 물도 보고 봄에는 벚꽃터널이 만들어져서 정말 예쁘거든요",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100032934.1.36",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100032934.1.37",
          "form": "진짜 잘 꾸민 공원인가보네요",
          "original_form": "진짜 잘 꾸민 공원인가보네요",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100032934.1.38",
          "form": "멀리서도 구경 오더라구요",
          "original_form": "멀리서도 구경 오더라구요",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100032934.1.39",
          "form": "근데 그래서  사람이 많아서 전 가기가 좀 꺼려져서 그냥 인적 드문 언덕공원을 가는거죠",
          "original_form": "근데 그래서  사람이 많아서 전 가기가 좀 꺼려져서 그냥 인적 드문 언덕공원을 가는거죠",
          "speaker_id": "2",
