        },
        {
          "id": "MDRW2100024124.1.2",
          "form": "원어민 선생님 국적이 어디에요",
          "original_form": "원어민 선생님 국적이 어디에요",
          "speaker_id": "1",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024124.1.3",
          "form": "미국인이에요",
          "original_form": "미국인이에요",
          "speaker_id": "2",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024124.1.4",
          "form": "bts 보려고 한국왔대오",
          "original_form": "bts 보려고 한국왔대오",
          "speaker_id": "2",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024124.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아진짜 대단하시다....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아진짜 대단하시다....",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.6",
          "form": "너무 이유가 귀여운 ㅋㅋㅋㅋㅋ",
          "original_form": "너무 이유가 귀여운 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.7",
          "form": "그래서 만나보셨대요?",
          "original_form": "그래서 만나보셨대요?",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.8",
          "form": "그래서 한국어 공부 열신히 하더라구요",
          "original_form": "그래서 한국어 공부 열신히 하더라구요",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.9",
          "form": "아직 못만낫겟죠?",
          "original_form": "아직 못만낫겟죠?",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.10",
          "form": "한국어 공부 하는 외국인 보면 진짜 존경스러워요",
          "original_form": "한국어 공부 하는 외국인 보면 진짜 존경스러워요",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.11",
          "form": "방탄이 급 미국을 가고",
          "original_form": "방탄이 급 미국을 가고",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.12",
          "form": "서로 스위치 했네요....",
          "original_form": "서로 스위치 했네요....",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.13",
          "form": "한국에선 콘서트도 못하고잇고",
          "original_form": "한국에선 콘서트도 못하고잇고",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.15",
          "form": "발음이 너무 좋아서",
          "original_form": "발음이 너무 좋아서",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.16",
          "form": "종종 한국어로 말걸어요",
          "original_form": "종종 한국어로 말걸어요",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024124.1.17",
          "form": "ㅋㅋㅋㅋ제가 더 친해지고 싶네요 ^.^",
          "original_form": "ㅋㅋㅋㅋ제가 더 친해지고 싶네요 ^.^",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.18",
          "form": "첨엔 영어로 말걸다가...",
          "original_form": "첨엔 영어로 말걸다가...",
          "speaker_id": "2",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.19",
          "form": "할수잇는 말이 없길래..^^...",
          "original_form": "할수잇는 말이 없길래..^^...",
          "speaker_id": "2",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.20",
          "form": "저도 서울에서 외국인 친구 몇 번 사겼었는데",
          "original_form": "저도 서울에서 외국인 친구 몇 번 사겼었는데",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.21",
          "form": "저는 영어 쓰고 그 친구는 한국어로 대화하고 그런 적 있었어옄ㅋㅋ",
          "original_form": "저는 영어 쓰고 그 친구는 한국어로 대화하고 그런 적 있었어옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.23",
          "form": "그래서 갑자기 서로 빵 터지고",
          "original_form": "그래서 갑자기 서로 빵 터지고",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.24",
          "form": "오 그래도 도움되네욬ㅋㅋㅋㅋ",
          "original_form": "오 그래도 도움되네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.25",
          "form": "우리 지금 뭐하는거냐고 ㅋㅋㅋ",
          "original_form": "우리 지금 뭐하는거냐고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.26",
          "form": "역시 술이 문제야",
          "original_form": "역시 술이 문제야",
          "speaker_id": "1",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.27",
          "form": "회화배우러 학원가서는 한국어만 가르쳐주고 왓는데",
          "original_form": "회화배우러 학원가서는 한국어만 가르쳐주고 왓는데",
          "speaker_id": "2",
          "time": "20210830 22:24"
        },
        {
          "id": "MDRW2100024124.1.28",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100024124.1.29",
          "form": "앗 ㅋㅋㅋ 강습비 받으셔야겠는데요",
