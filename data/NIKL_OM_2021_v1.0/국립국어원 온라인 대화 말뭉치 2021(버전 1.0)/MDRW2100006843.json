        {
          "id": "MDRW2100006843.1.1",
          "form": "네 눈 칼슘제 비타민 먹고 있조",
          "original_form": "네 눈 칼슘제 비타민 먹고 있조",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006843.1.2",
          "form": "눈은 루테인이요?",
          "original_form": "눈은 루테인이요?",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100006843.1.3",
          "form": "네 루테인 지아잔틴요",
          "original_form": "네 루테인 지아잔틴요",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006843.1.4",
          "form": "저도 어재 부모님 루테인 샀어요",
          "original_form": "저도 어재 부모님 루테인 샀어요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006843.1.5",
          "form": "효도 하셨네요",
          "original_form": "효도 하셨네요",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006843.1.6",
          "form": "전 부모님 껀 홍삼 준비 했죠",
          "original_form": "전 부모님 껀 홍삼 준비 했죠",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006843.1.7",
          "form": "정관장이요?",
          "original_form": "정관장이요?",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006843.1.8",
          "form": "네 홍삼농축액요",
          "original_form": "네 홍삼농축액요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006843.1.9",
          "form": "나이드니 영양제가 꼭 필요하죠",
          "original_form": "나이드니 영양제가 꼭 필요하죠",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006843.1.10",
          "form": "네 저도 꼭 챙겨먹게되네요",
          "original_form": "네 저도 꼭 챙겨먹게되네요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006843.1.11",
          "form": "건강에 도움이 되죠",
          "original_form": "건강에 도움이 되죠",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006843.1.12",
          "form": "유산균운 안드시나요",
          "original_form": "유산균운 안드시나요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006843.1.13",
          "form": "네 안 먹어요",
          "original_form": "네 안 먹어요",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006843.1.14",
          "form": "그러시군요 저는 유산균 오메가3 비타민 챙겨먹어요",
          "original_form": "그러시군요 저는 유산균 오메가3 비타민 챙겨먹어요",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006843.1.15",
          "form": "유산균도 좋죠",
          "original_form": "유산균도 좋죠",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006843.1.16",
          "form": "챙겨 먹어야할 영양제가 넘 많아요",
          "original_form": "챙겨 먹어야할 영양제가 넘 많아요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006843.1.17",
          "form": "머리는 자주 하시나요ㅡ",
