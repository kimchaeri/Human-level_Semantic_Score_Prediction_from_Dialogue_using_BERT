      "utterance": [
        {
          "id": "MDRW2100024851.1.1",
          "form": "최근 구매한 물건 있으신가요?",
          "original_form": "최근 구매한 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 19:42"
        },
        {
          "id": "MDRW2100024851.1.2",
          "form": "저 이번에 일회용안대 샀는데 너무 좋아요!!!",
          "original_form": "저 이번에 일회용안대 샀는데 너무 좋아요!!!",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100024851.1.3",
          "form": "오..그 찜질 되는 안대인가요?",
          "original_form": "오..그 찜질 되는 안대인가요?",
          "speaker_id": "1",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100024851.1.4",
          "form": "ㅓㄹ 맞아요",
          "original_form": "ㅓㄹ 맞아요",
          "speaker_id": "2",
          "time": "20210901 08:39"
        },
        {
          "id": "MDRW2100024851.1.5",
          "form": "바로아시네여",
          "original_form": "바로아시네여",
          "speaker_id": "2",
          "time": "20210901 08:39"
        },
        {
          "id": "MDRW2100024851.1.6",
          "form": "ㅋㅋㅋ저도 쓰고 있거든요",
          "original_form": "ㅋㅋㅋ저도 쓰고 있거든요",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100024851.1.7",
          "form": "안구 건조증 있는데 좋더라구요",
          "original_form": "안구 건조증 있는데 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 13:20"
        },
        {
          "id": "MDRW2100024851.1.8",
          "form": "오 그거 건조증에도 좋군요!",
          "original_form": "오 그거 건조증에도 좋군요!",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100024851.1.9",
          "form": "눈의 피로가 괜히 풀리는게 아니었네요",
          "original_form": "눈의 피로가 괜히 풀리는게 아니었네요",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100024851.1.10",
          "form": "ㅋㅋㅋ잠도 더 잘 오지 않던가요?",
          "original_form": "ㅋㅋㅋ잠도 더 잘 오지 않던가요?",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100024851.1.11",
          "form": "저는 그거 하고 나면 잠 잘 오더라구요",
          "original_form": "저는 그거 하고 나면 잠 잘 오더라구요",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100024851.1.12",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ 이제 향이 다양해서 더 좋아요 히히",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ 이제 향이 다양해서 더 좋아요 히히",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100024851.1.13",
          "form": "문제는 일어나기도 힘들다는거 ㅎ",
          "original_form": "문제는 일어나기도 힘들다는거 ㅎ",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100024851.1.14",
          "form": "ㅋㅋㅋㅋㅋ그렇긴 하죠",
          "original_form": "ㅋㅋㅋㅋㅋ그렇긴 하죠",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100024851.1.15",
          "form": "저는 최근에 열 패치..? 열 내리는 패치 샀어요",
          "original_form": "저는 최근에 열 패치..? 열 내리는 패치 샀어요",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100024851.1.16",
          "form": "우와 그런게 있군요",
          "original_form": "우와 그런게 있군요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100024851.1.17",
          "form": "어디 아프셨나유",
          "original_form": "어디 아프셨나유",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100024851.1.18",
          "form": "백신 2차 맞으면 열 날 수 있다길래",
          "original_form": "백신 2차 맞으면 열 날 수 있다길래",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100024851.1.19",
          "form": "혹시 몰라서 준비했어요!",
          "original_form": "혹시 몰라서 준비했어요!",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100024851.1.20",
          "form": "아하 그러네요! 저도 맞을때 준비해야할듭...",
          "original_form": "아하 그러네요! 저도 맞을때 준비해야할듭...",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100024851.1.21",
          "form": "2차땐 진짜 아프대요ㅜㅜ",
          "original_form": "2차땐 진짜 아프대요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100024851.1.22",
          "form": "그러게요..그래서 겁 먹고 있어요..",
          "original_form": "그러게요..그래서 겁 먹고 있어요..",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100024851.1.23",
          "form": "저 1차때도 무서워서 타이레놀 2개 샀거든요..",
          "original_form": "저 1차때도 무서워서 타이레놀 2개 샀거든요..",
          "speaker_id": "1",
          "time": "20210902 22:16"
        }
      ]
    }
