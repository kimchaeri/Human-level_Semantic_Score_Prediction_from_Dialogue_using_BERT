          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017818.1.4",
          "form": "무늬 있는 셔츠야?",
          "original_form": "무늬 있는 셔츠야?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017818.1.5",
          "form": "아니 그냥 셔츠!",
          "original_form": "아니 그냥 셔츠!",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017818.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017818.1.7",
          "form": "어떤 색?",
          "original_form": "어떤 색?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017818.1.8",
          "form": "민트색!",
          "original_form": "민트색!",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017818.1.9",
          "form": "오 대박!",
          "original_form": "오 대박!",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017818.1.10",
          "form": "긴팔 셔츠야?",
          "original_form": "긴팔 셔츠야?",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017818.1.11",
          "form": "반팔!",
          "original_form": "반팔!",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017818.1.12",
          "form": "더 추워지기 전에 자주 입어야겠다",
          "original_form": "더 추워지기 전에 자주 입어야겠다",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017818.1.13",
          "form": "추워서 이제 못입어",
          "original_form": "추워서 이제 못입어",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017818.1.14",
          "form": "헿",
          "original_form": "헿",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017818.1.15",
          "form": "저번에 생일 선물로 준 옷 사이즈 괜찮았어?",
          "original_form": "저번에 생일 선물로 준 옷 사이즈 괜찮았어?",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017818.1.16",
          "form": "엉엉!!",
          "original_form": "엉엉!!",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017818.1.17",
          "form": "그 뒤로 따로 물어본 적이 없었네",
          "original_form": "그 뒤로 따로 물어본 적이 없었네",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017818.1.18",
          "form": "오 다행이다~",
          "original_form": "오 다행이다~",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
