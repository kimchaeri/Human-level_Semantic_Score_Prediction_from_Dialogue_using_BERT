      "utterance": [
        {
          "id": "MDRW2100034354.1.1",
          "form": "백신 예약하셨어요?",
          "original_form": "백신 예약하셨어요?",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100034354.1.2",
          "form": "전 다맞았어요 ㅎㅎ",
          "original_form": "전 다맞았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034354.1.3",
          "form": "6월에",
          "original_form": "6월에",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034354.1.4",
          "form": "안맞으셨죠?",
          "original_form": "안맞으셨죠?",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034354.1.5",
          "form": "오 빨리맞으셨네요?",
          "original_form": "오 빨리맞으셨네요?",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034354.1.6",
          "form": "네 잔여백신 예약해서 했어요",
          "original_form": "네 잔여백신 예약해서 했어요",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034354.1.7",
          "form": "저는 다음달에 예약해놨어요",
          "original_form": "저는 다음달에 예약해놨어요",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034354.1.8",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034354.1.9",
          "form": "좀 떨리시죠?",
          "original_form": "좀 떨리시죠?",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034354.1.10",
          "form": "백신종류 머맞으셨어요?",
          "original_form": "백신종류 머맞으셨어요?",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034354.1.11",
          "form": "저도 맞기전에 엄청 떨렸어요",
          "original_form": "저도 맞기전에 엄청 떨렸어요",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034354.1.12",
          "form": "전 얀센요",
          "original_form": "전 얀센요",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034354.1.13",
          "form": "근데 2차가 없어서 불안해요",
          "original_form": "근데 2차가 없어서 불안해요",
          "speaker_id": "1",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100034354.1.14",
          "form": "오~그럼 1차만맞으면되니",
          "original_form": "오~그럼 1차만맞으면되니",
          "speaker_id": "2",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100034354.1.15",
          "form": "편하셨겠어요",
          "original_form": "편하셨겠어요",
          "speaker_id": "2",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100034354.1.16",
          "form": "편한데",
          "original_form": "편한데",
          "speaker_id": "1",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100034354.1.17",
          "form": "요백신 말많더 라구요",
          "original_form": "요백신 말많더 라구요",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100034354.1.18",
          "form": "효과가 덜하다고 ㅜㅜ",
          "original_form": "효과가 덜하다고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100034354.1.19",
          "form": "화이자 세요?",
          "original_form": "화이자 세요?",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100034354.1.20",
          "form": "그렇긴해요 물백신이라고",
          "original_form": "그렇긴해요 물백신이라고",
          "speaker_id": "2",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100034354.1.21",
          "form": "저는 아직 머맞을지 몰라요",
          "original_form": "저는 아직 머맞을지 몰라요",
          "speaker_id": "2",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100034354.1.22",
          "form": "ㅜㅜ슬퍼요 또 한번 놔주지",
          "original_form": "ㅜㅜ슬퍼요 또 한번 놔주지",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100034354.1.23",
          "form": "아마 화이자 모더나겠죠",
          "original_form": "아마 화이자 모더나겠죠",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100034354.1.24",
          "form": "모더나 아님 화이자라는데",
          "original_form": "모더나 아님 화이자라는데",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100034354.1.25",
          "form": "저희집 화이자 다 괜찮더라구요",
          "original_form": "저희집 화이자 다 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100034354.1.26",
          "form": "아들 남편 맞았어요",
          "original_form": "아들 남편 맞았어요",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100034354.1.27",
          "form": "주변에서는 다들 모더나 원하더라구요",
          "original_form": "주변에서는 다들 모더나 원하더라구요",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100034354.1.28",
          "form": "아들까지 다 빨리 끝내셨네요",
          "original_form": "아들까지 다 빨리 끝내셨네요",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100034354.1.29",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100034354.1.30",
          "form": "모더나 엄청 아프다던데",
          "original_form": "모더나 엄청 아프다던데",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100034354.1.31",
          "form": "친구 죽다 살아났다고",
          "original_form": "친구 죽다 살아났다고",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100034354.1.32",
          "form": "비추래요 ㅜㅜ",
          "original_form": "비추래요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100034354.1.33",
          "form": "헉..그리아픈가요?",
          "original_form": "헉..그리아픈가요?",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100034354.1.34",
          "form": "회사언니는 병원관계자알고있어서",
          "original_form": "회사언니는 병원관계자알고있어서",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100034354.1.35",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100034354.1.36",
          "form": "잔여있다고 연락받고 가서 맞고오더라구요",
          "original_form": "잔여있다고 연락받고 가서 맞고오더라구요",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100034354.1.37",
          "form": "맞아요 그렇게 맞죠",
          "original_form": "맞아요 그렇게 맞죠",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100034354.1.38",
          "form": "전 잔여신청해놔서 순서 되서 맞았어요",
          "original_form": "전 잔여신청해놔서 순서 되서 맞았어요",
          "speaker_id": "1",
          "time": "20210828 13:15"
        }
      ]
    }
