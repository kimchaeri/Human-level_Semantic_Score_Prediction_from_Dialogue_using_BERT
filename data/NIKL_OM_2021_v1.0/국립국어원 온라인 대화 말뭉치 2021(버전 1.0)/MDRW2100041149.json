      "utterance": [
        {
          "id": "MDRW2100041149.1.1",
          "form": "저녁드셔야죠!",
          "original_form": "저녁드셔야죠!",
          "speaker_id": "1",
          "time": "20210905 18:07"
        },
        {
          "id": "MDRW2100041149.1.2",
          "form": "마라탕 먹엇어요 밀키트",
          "original_form": "마라탕 먹엇어요 밀키트",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100041149.1.3",
          "form": "밀키트 좋게 나오나요???",
          "original_form": "밀키트 좋게 나오나요???",
          "speaker_id": "1",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100041149.1.4",
          "form": "구성 갠차는데",
          "original_form": "구성 갠차는데",
          "speaker_id": "2",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100041149.1.5",
          "form": "국물맛이 식당이 나으네요",
          "original_form": "국물맛이 식당이 나으네요",
          "speaker_id": "2",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100041149.1.6",
          "form": "아 전 소스만 사서 해먹어봤는데 라면이 더 맛있었어요",
          "original_form": "아 전 소스만 사서 해먹어봤는데 라면이 더 맛있었어요",
          "speaker_id": "1",
          "time": "20210905 18:47"
        },
        {
          "id": "MDRW2100041149.1.7",
          "form": "소스 어떤거요?",
          "original_form": "소스 어떤거요?",
          "speaker_id": "2",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100041149.1.8",
          "form": "이금기마라탕소스 이거요",
          "original_form": "이금기마라탕소스 이거요",
          "speaker_id": "1",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100041149.1.9",
          "form": "물에 그냥 넣고 끓이면 대요?",
          "original_form": "물에 그냥 넣고 끓이면 대요?",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100041149.1.10",
          "form": "네네 야채몽땅 넣고 사실 고기넣음 다 맛나죠!!",
          "original_form": "네네 야채몽땅 넣고 사실 고기넣음 다 맛나죠!!",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100041149.1.11",
          "form": "우와 ...저도사볼래용",
          "original_form": "우와 ...저도사볼래용",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100041149.1.12",
          "form": "고기 어떤거사야하죠",
          "original_form": "고기 어떤거사야하죠",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100041149.1.13",
          "form": "차돌박이가 잘어울려요",
          "original_form": "차돌박이가 잘어울려요",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100041149.1.14",
          "form": "오! 그렇겟다",
          "original_form": "오! 그렇겟다",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100041149.1.15",
          "form": "도전해야겟네요",
          "original_form": "도전해야겟네요",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100041149.1.16",
          "form": "그리고 사골육수에 끓이면 사먹는거랑 같대요!",
          "original_form": "그리고 사골육수에 끓이면 사먹는거랑 같대요!",
          "speaker_id": "1",
          "time": "20210906 21:28"
        }
