      "utterance": [
        {
          "id": "MDRW2100013255.1.1",
          "form": "안녕하세요! 동호회나 봉사활동 하시나요??",
          "original_form": "안녕하세요! 동호회나 봉사활동 하시나요??",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013255.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013255.1.3",
          "form": "하시나요?",
          "original_form": "하시나요?",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013255.1.4",
          "form": "저도 아직은 안해요! 테니스동호회 들어가고 싶은데 실력이 부족해서 기회를 노리고 있습니다 ㅎㅎ",
          "original_form": "저도 아직은 안해요! 테니스동호회 들어가고 싶은데 실력이 부족해서 기회를 노리고 있습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013255.1.5",
          "form": "도서관 봉사활동은 꾸준히 하고 있어요!",
          "original_form": "도서관 봉사활동은 꾸준히 하고 있어요!",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013255.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013255.1.7",
          "form": "활동적이시네요",
          "original_form": "활동적이시네요",
          "speaker_id": "2",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013255.1.8",
          "form": "도서관 봉활은 그냥하시는건가요?",
          "original_form": "도서관 봉활은 그냥하시는건가요?",
          "speaker_id": "2",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013255.1.9",
          "form": "네 이전에 봉사활동경험이 없어서 어떤건지 궁금하더라고요ㅋㅋㅋㅋ",
          "original_form": "네 이전에 봉사활동경험이 없어서 어떤건지 궁금하더라고요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013255.1.10",
          "form": "그래서 그냥 취미로 하고있어요",
          "original_form": "그래서 그냥 취미로 하고있어요",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013255.1.11",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013255.1.12",
          "form": "쉽지않은데 대단하시네요",
          "original_form": "쉽지않은데 대단하시네요",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013255.1.13",
          "form": "감사합니다 담달에는 학교가야해서 이번주 주말까지만 하기로했어요!",
          "original_form": "감사합니다 담달에는 학교가야해서 이번주 주말까지만 하기로했어요!",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013255.1.14",
          "form": "name2은 하고 싶은 동호회 있으신가요?",
          "original_form": "&name2&은 하고 싶은 동호회 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013255.1.15",
          "form": "학생이시군요?",
          "original_form": "학생이시군요?",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100013255.1.16",
          "form": "역시 젊어서 체력이좋네요ㅋㅋㅋㅋ",
          "original_form": "역시 젊어서 체력이좋네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100013255.1.17",
          "form": "전 등산이요",
          "original_form": "전 등산이요",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100013255.1.18",
          "form": "넵 학생이요",
          "original_form": "넵 학생이요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013255.1.19",
          "form": "좀 name2이야말로 체력이 좋으신가봐요",
          "original_form": "좀 &name2&이야말로 체력이 좋으신가봐요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013255.1.20",
          "form": "등산이라니!",
          "original_form": "등산이라니!",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013255.1.21",
          "form": "전 엄두도 못네요 ㅜ 넘 힘들것같아요",
          "original_form": "전 엄두도 못네요 ㅜ 넘 힘들것같아요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013255.1.22",
          "form": "ㅋㅋㅋㅋ 저 사실",
          "original_form": "ㅋㅋㅋㅋ 저 사실",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013255.1.23",
          "form": "한번도안가봤어요",
          "original_form": "한번도안가봤어요",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013255.1.24",
          "form": "이제 가보고싶네요ㅋㅋㅋ",
          "original_form": "이제 가보고싶네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013255.1.25",
          "form": "저두 한번도ㅋㅋㅋ 올해 목표 중 하나예요 ㅋㅋㅋ 이룰지는 모루겠지만",
          "original_form": "저두 한번도ㅋㅋㅋ 올해 목표 중 하나예요 ㅋㅋㅋ 이룰지는 모루겠지만",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013255.1.26",
          "form": "사실코로나때문에",
          "original_form": "사실코로나때문에",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013255.1.27",
          "form": "모이기가 부담스럽죠",
          "original_form": "모이기가 부담스럽죠",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013255.1.28",
          "form": "그건 마자요ㅜ 코로나때문에 의지가 있어도 모이는게 쉽지 않으니까....",
          "original_form": "그건 마자요ㅜ 코로나때문에 의지가 있어도 모이는게 쉽지 않으니까....",
          "speaker_id": "1",
          "time": "20210825 15:49"
        }
      ]
