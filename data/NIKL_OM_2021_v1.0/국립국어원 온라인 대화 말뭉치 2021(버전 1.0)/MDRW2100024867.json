      "utterance": [
        {
          "id": "MDRW2100024867.1.1",
          "form": "차돌풍 차돌 맛있었는데ㅜ",
          "original_form": "차돌풍 차돌 맛있었는데ㅜ",
          "speaker_id": "1",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024867.1.2",
          "form": "비빔국수도 맛있었는데",
          "original_form": "비빔국수도 맛있었는데",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100024867.1.3",
          "form": "안되겠다 오늘 야식은 족발이다",
          "original_form": "안되겠다 오늘 야식은 족발이다",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100024867.1.4",
          "form": "급 야식",
          "original_form": "급 야식",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100024867.1.5",
          "form": "전 아이스크림ㅋ",
          "original_form": "전 아이스크림ㅋ",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100024867.1.6",
          "form": "뭐 또 비싼거 드심미까",
          "original_form": "뭐 또 비싼거 드심미까",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100024867.1.7",
          "form": "하겐다즈아이스크림...?",
          "original_form": "하겐다즈아이스크림...?",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100024867.1.8",
          "form": "ㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100024867.1.9",
          "form": "나는 빠삐꼬나 먹고있는데ㅡㅡ",
          "original_form": "나는 빠삐꼬나 먹고있는데ㅡㅡ",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100024867.1.10",
          "form": "ㅋㅋㄱ세일하더라구요",
          "original_form": "ㅋㅋㄱ세일하더라구요",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100024867.1.11",
          "form": "그래도 비싸긴하지만",
          "original_form": "그래도 비싸긴하지만",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100024867.1.12",
          "form": "월급이 10일에 들어와서",
          "original_form": "월급이 10일에 들어와서",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100024867.1.13",
          "form": "족발은 무산됐습니다",
          "original_form": "족발은 무산됐습니다",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100024867.1.14",
          "form": "컵라면 사가는중",
          "original_form": "컵라면 사가는중",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100024867.1.15",
          "form": "슬프군여...ㅜㅜ",
          "original_form": "슬프군여...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100024867.1.16",
          "form": "10일에 족발ㄱㄱ",
          "original_form": "10일에 족발ㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100024867.1.17",
          "form": "불족발이 엄청 땡겨서 어제 불닭 사먹었어요",
          "original_form": "불족발이 엄청 땡겨서 어제 불닭 사먹었어요",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100024867.1.18",
          "form": "뭔가 더 서럽다",
          "original_form": "뭔가 더 서럽다",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100024867.1.19",
          "form": "ㅋㅋㅋㅋㅋ불족발 요즘 유행인가요",
          "original_form": "ㅋㅋㅋㅋㅋ불족발 요즘 유행인가요",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100024867.1.20",
          "form": "남치니도 불족발 시켰대요",
          "original_form": "남치니도 불족발 시켰대요",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100024867.1.21",
          "form": "주머니에 두조각만 넣어오라 그래요",
          "original_form": "주머니에 두조각만 넣어오라 그래요",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100024867.1.22",
          "form": "후 서럽",
          "original_form": "후 서럽",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100024867.1.23",
          "form": "ㅋㅋㅋㅋㅋ인터넷에 파나봐요",
          "original_form": "ㅋㅋㅋㅋㅋ인터넷에 파나봐요",
          "speaker_id": "1",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100024867.1.24",
          "form": "요즘 식비 절약해야돼서",
          "original_form": "요즘 식비 절약해야돼서",
          "speaker_id": "2",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100024867.1.25",
          "form": "라면 하루에 두끼 먹습니다",
          "original_form": "라면 하루에 두끼 먹습니다",
          "speaker_id": "2",
          "time": "20210902 12:38"
        }
      ]
    }
