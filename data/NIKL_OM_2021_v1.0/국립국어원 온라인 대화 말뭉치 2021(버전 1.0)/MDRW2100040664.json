      "utterance": [
        {
          "id": "MDRW2100040664.1.1",
          "form": "혹시 한 직장에서 십이년 일하셨나요?",
          "original_form": "혹시 한 직장에서 십이년 일하셨나요?",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100040664.1.2",
          "form": "name2님은 다시 일을 준비하시게 되어도 너무 스트레스 받지마시고",
          "original_form": "&name2&님은 다시 일을 준비하시게 되어도 너무 스트레스 받지마시고",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100040664.1.3",
          "form": "실례가 안된다면...",
          "original_form": "실례가 안된다면...",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100040664.1.4",
          "form": "지점을 옮긴거니 큰 의미로는 한 곳일듯요",
          "original_form": "지점을 옮긴거니 큰 의미로는 한 곳일듯요",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100040664.1.5",
          "form": "대단하시네여",
          "original_form": "대단하시네여",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100040664.1.6",
          "form": "별다른 재주가 없어서 ㅎㅎ",
          "original_form": "별다른 재주가 없어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100040664.1.7",
          "form": "진짜 한곳에서 길게 일하시는 분들 대단해요",
          "original_form": "진짜 한곳에서 길게 일하시는 분들 대단해요",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100040664.1.8",
          "form": "지겨워요",
          "original_form": "지겨워요",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100040664.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100040664.1.10",
          "form": "저는 직업자체가 변동성이 커서",
          "original_form": "저는 직업자체가 변동성이 커서",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100040664.1.11",
          "form": "옮겨도 사실은 옮긴게 아니에요",
          "original_form": "옮겨도 사실은 옮긴게 아니에요",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100040664.1.12",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100040664.1.13",
          "form": "건너건너 아는 사람이기 때문에",
          "original_form": "건너건너 아는 사람이기 때문에",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100040664.1.14",
          "form": "아 그래서 그러시군요",
          "original_form": "아 그래서 그러시군요",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100040664.1.15",
          "form": "그게 갑갑할거 같긴 하네요",
          "original_form": "그게 갑갑할거 같긴 하네요",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100040664.1.16",
          "form": "네 ㅎㅎ 꼬리표가 계속 따라다닐수도 있고요",
          "original_form": "네 ㅎㅎ 꼬리표가 계속 따라다닐수도 있고요",
          "speaker_id": "1",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100040664.1.17",
          "form": "그래서 공무원이어도 힘들다고 하는 사람 얘기는 들었어요",
          "original_form": "그래서 공무원이어도 힘들다고 하는 사람 얘기는 들었어요",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040664.1.18",
          "form": "거기도 옮기긴 해도 변동이 크지 않으니까",
          "original_form": "거기도 옮기긴 해도 변동이 크지 않으니까",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040664.1.19",
          "form": "쉬운게 없는거 같아요",
          "original_form": "쉬운게 없는거 같아요",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100040664.1.20",
          "form": "그쵸 관공서가면 무대뽀인 사람들과 이야기하느라 기빨릴듯요",
          "original_form": "그쵸 관공서가면 무대뽀인 사람들과 이야기하느라 기빨릴듯요",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100040664.1.21",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100040664.1.22",
          "form": "클라이언트 상대하는것도 힘든데",
          "original_form": "클라이언트 상대하는것도 힘든데",
          "speaker_id": "2",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100040664.1.23",
          "form": "조금만 갑이면 갑질 하는 분들이 계시긴 해여",
