      "utterance": [
        {
          "id": "MDRW2100009508.1.1",
          "form": "몰아서보심 재밌겠어요ㅋㅋㄱ",
          "original_form": "몰아서보심 재밌겠어요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009508.1.2",
          "form": "전 그걸 못기다려서ㅋㅋㄱ웹툰도 매주 꼭꼭 읽고",
          "original_form": "전 그걸 못기다려서ㅋㅋㄱ웹툰도 매주 꼭꼭 읽고",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009508.1.3",
          "form": "기다리는걸 못해요ㅋㅋ",
          "original_form": "기다리는걸 못해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009508.1.4",
          "form": "저도ㅎ 그래서 본방사수하긴 했는데",
          "original_form": "저도ㅎ 그래서 본방사수하긴 했는데",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009508.1.5",
          "form": "펜트하우스 끝나고 하는것도 재밌을거 같긴한데 너무늦은시간이라ㅠ 챙겨보진 몬할듯해여ㅡ",
          "original_form": "펜트하우스 끝나고 하는것도 재밌을거 같긴한데 너무늦은시간이라ㅠ 챙겨보진 몬할듯해여ㅡ",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009508.1.6",
          "form": "펜트하우스는 보던거라 봤는데ㅋ",
          "original_form": "펜트하우스는 보던거라 봤는데ㅋ",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009508.1.7",
          "form": "시즌3는 너무 꼬니까 스트레스를 받아서 ㅎㅎ",
          "original_form": "시즌3는 너무 꼬니까 스트레스를 받아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009508.1.8",
          "form": "맞아요ㅡ거기다가 1회라",
          "original_form": "맞아요ㅡ거기다가 1회라",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009508.1.9",
          "form": "더 실망",
          "original_form": "더 실망",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009508.1.10",
          "form": "인기많아지니 그런듯",
          "original_form": "인기많아지니 그런듯",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009508.1.11",
          "form": "그런말도있잖아요 사람들이 추측하면 일부러",
          "original_form": "그런말도있잖아요 사람들이 추측하면 일부러",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009508.1.12",
          "form": "그렇게안한다고ㅋㅋ",
          "original_form": "그렇게안한다고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009508.1.13",
          "form": "ㅋㅋ 오 지금 재방할건가봐요",
          "original_form": "ㅋㅋ 오 지금 재방할건가봐요",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009508.1.14",
          "form": "펜하",
          "original_form": "펜하",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009508.1.15",
          "form": "와 몇화에여?",
          "original_form": "와 몇화에여?",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009508.1.16",
          "form": "이번주껀가",
          "original_form": "이번주껀가",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009508.1.17",
          "form": "어제한거일듯요",
          "original_form": "어제한거일듯요",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009508.1.18",
          "form": "아 이번주꺼는 재밌었어요",
          "original_form": "아 이번주꺼는 재밌었어요",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009508.1.19",
          "form": "대충 얘기들었으니 함 봐볼게요ㅋ 내용대충파악될듯",
          "original_form": "대충 얘기들었으니 함 봐볼게요ㅋ 내용대충파악될듯",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009508.1.20",
          "form": "전 징그러운장면 잘 못보는데 ㅎ19세라그런지 좀 글터라구요ㅠ",
          "original_form": "전 징그러운장면 잘 못보는데 ㅎ19세라그런지 좀 글터라구요ㅠ",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009508.1.21",
          "form": "전 그런거 좋아해요ㅋ 잔인한거",
          "original_form": "전 그런거 좋아해요ㅋ 잔인한거",
          "speaker_id": "2",
