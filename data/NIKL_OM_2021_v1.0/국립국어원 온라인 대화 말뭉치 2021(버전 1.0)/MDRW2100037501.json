      "utterance": [
        {
          "id": "MDRW2100037501.1.1",
          "form": "저는 키링이요",
          "original_form": "저는 키링이요",
          "speaker_id": "2",
          "time": "20210901 01:46"
        },
        {
          "id": "MDRW2100037501.1.2",
          "form": "오 무슨키링인대요",
          "original_form": "오 무슨키링인대요",
          "speaker_id": "1",
          "time": "20210901 01:46"
        },
        {
          "id": "MDRW2100037501.1.3",
          "form": "홀맨  아시나요? 홀맨 키링을 받았어요",
          "original_form": "홀맨  아시나요? 홀맨 키링을 받았어요",
          "speaker_id": "2",
          "time": "20210901 01:46"
        },
        {
          "id": "MDRW2100037501.1.4",
          "form": "오 어떤디자인인데요?",
          "original_form": "오 어떤디자인인데요?",
          "speaker_id": "1",
          "time": "20210901 01:47"
        },
        {
          "id": "MDRW2100037501.1.5",
          "form": "홀맨이라고 쓰여진 스트랩 달린거 하나랑",
          "original_form": "홀맨이라고 쓰여진 스트랩 달린거 하나랑",
          "speaker_id": "2",
          "time": "20210901 01:48"
        },
        {
          "id": "MDRW2100037501.1.6",
          "form": "홀맨인형 달린거 하나",
          "original_form": "홀맨인형 달린거 하나",
          "speaker_id": "2",
          "time": "20210901 01:49"
        },
        {
          "id": "MDRW2100037501.1.7",
          "form": "요렇게 들어있어요",
          "original_form": "요렇게 들어있어요",
          "speaker_id": "2",
          "time": "20210901 01:49"
        },
        {
          "id": "MDRW2100037501.1.8",
          "form": "그거 붙히는게아닐까요??",
          "original_form": "그거 붙히는게아닐까요??",
          "speaker_id": "1",
          "time": "20210901 01:49"
        },
        {
          "id": "MDRW2100037501.1.9",
          "form": "따로 써도 괜찮을거 같네요",
          "original_form": "따로 써도 괜찮을거 같네요",
          "speaker_id": "2",
          "time": "20210901 01:52"
        },
        {
          "id": "MDRW2100037501.1.10",
          "form": "자세히 보니 원래 하나로 쓰는건 맞는거 같아요",
          "original_form": "자세히 보니 원래 하나로 쓰는건 맞는거 같아요",
          "speaker_id": "2",
          "time": "20210901 01:52"
        },
        {
          "id": "MDRW2100037501.1.11",
          "form": "아직 포장된채로 두고 열어보지는 않았었거든요ㅋㅋㄱㅋㅋ",
          "original_form": "아직 포장된채로 두고 열어보지는 않았었거든요ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:52"
        },
        {
          "id": "MDRW2100037501.1.12",
          "form": "몇개받앗어요?",
          "original_form": "몇개받앗어요?",
          "speaker_id": "1",
          "time": "20210901 01:55"
        },
        {
          "id": "MDRW2100037501.1.13",
          "form": "3개받았어요",
          "original_form": "3개받았어요",
          "speaker_id": "2",
          "time": "20210901 02:05"
        },
        {
          "id": "MDRW2100037501.1.14",
          "form": "오 모양이 다른가요?",
          "original_form": "오 모양이 다른가요?",
          "speaker_id": "1",
          "time": "20210901 02:05"
        },
        {
          "id": "MDRW2100037501.1.15",
          "form": "운이 안 좋은가봐요",
          "original_form": "운이 안 좋은가봐요",
          "speaker_id": "2",
          "time": "20210901 02:08"
        },
        {
          "id": "MDRW2100037501.1.16",
          "form": "다  같은거에요ㅜㅜ",
          "original_form": "다  같은거에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 02:08"
        },
        {
          "id": "MDRW2100037501.1.17",
          "form": "오.. 무슨색이고 어떻게생겻조?",
          "original_form": "오.. 무슨색이고 어떻게생겻조?",
          "speaker_id": "1",
          "time": "20210901 02:10"
        },
        {
          "id": "MDRW2100037501.1.18",
          "form": "홀맨은 앉아있고",
          "original_form": "홀맨은 앉아있고",
          "speaker_id": "2",
          "time": "20210901 02:11"
        },
        {
          "id": "MDRW2100037501.1.19",
          "form": "스트랩은 검은색이요",
          "original_form": "스트랩은 검은색이요",
          "speaker_id": "2",
          "time": "20210901 02:11"
        },
        {
          "id": "MDRW2100037501.1.20",
          "form": "오.... 그렇구나.. 귀엽겟네요",
          "original_form": "오.... 그렇구나.. 귀엽겟네요",
          "speaker_id": "1",
          "time": "20210901 02:11"
        }
