      "utterance": [
        {
          "id": "MDRW2100036547.1.1",
          "form": "아토차역 가야겠네",
          "original_form": "아토차역 가야겠네",
          "speaker_id": "2",
          "time": "20210831 00:24"
        },
        {
          "id": "MDRW2100036547.1.2",
          "form": "소피아왕비 에술센터",
          "original_form": "소피아왕비 에술센터",
          "speaker_id": "1",
          "time": "20210831 00:24"
        },
        {
          "id": "MDRW2100036547.1.3",
          "form": "또 있어",
          "original_form": "또 있어",
          "speaker_id": "2",
          "time": "20210831 00:24"
        },
        {
          "id": "MDRW2100036547.1.4",
          "form": "티센 보르네미스사 미술광 등 미술관과 박물관이 모여있데",
          "original_form": "티센 보르네미스사 미술광 등 미술관과 박물관이 모여있데",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100036547.1.5",
          "form": "많네",
          "original_form": "많네",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100036547.1.6",
          "form": "그러게 소피아 왕비 예술 센터는 피카소 그림이 있데",
          "original_form": "그러게 소피아 왕비 예술 센터는 피카소 그림이 있데",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100036547.1.7",
          "form": "오 피카소 그림이",
          "original_form": "오 피카소 그림이",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100036547.1.8",
          "form": "응 피카소가 스페인 출신인거 알지",
          "original_form": "응 피카소가 스페인 출신인거 알지",
          "speaker_id": "1",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100036547.1.9",
          "form": "엉 알지",
          "original_form": "엉 알지",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100036547.1.10",
          "form": "아버지가 미술교사여서 자연스럽게 그림을 접하기 시작했데",
          "original_form": "아버지가 미술교사여서 자연스럽게 그림을 접하기 시작했데",
          "speaker_id": "1",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100036547.1.11",
          "form": "아 아버지가 미술교사였구나",
          "original_form": "아 아버지가 미술교사였구나",
          "speaker_id": "2",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100036547.1.12",
          "form": "엉 바르셀로나로 이사하면서 그림을 제대로 배우기 시작했다네 그러다가 파리로 가지",
          "original_form": "엉 바르셀로나로 이사하면서 그림을 제대로 배우기 시작했다네 그러다가 파리로 가지",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100036547.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100036547.1.14",
          "form": "그래서 피카소 그림이 프랑스와 스폐인 곳곳에 있는듯해",
          "original_form": "그래서 피카소 그림이 프랑스와 스폐인 곳곳에 있는듯해",
          "speaker_id": "1",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100036547.1.15",
