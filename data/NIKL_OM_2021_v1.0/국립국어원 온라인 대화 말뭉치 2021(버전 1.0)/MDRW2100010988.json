      "utterance": [
        {
          "id": "MDRW2100010988.1.1",
          "form": "장마라니 일단 내일까지는 계속 올 거 같아.",
          "original_form": "장마라니 일단 내일까지는 계속 올 거 같아.",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.2",
          "form": "비오는데 이사라니 힘들겠구나",
          "original_form": "비오는데 이사라니 힘들겠구나",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.3",
          "form": "그러게ㅠ 내가 이사하는낳",
          "original_form": "그러게ㅠ 내가 이사하는낳",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.4",
          "form": "일단 확실히 내일꺄진 비가온대.",
          "original_form": "일단 확실히 내일꺄진 비가온대.",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.5",
          "form": "비가 안오길 빌어주라",
          "original_form": "비가 안오길 빌어주라",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.6",
          "form": "비는중.",
          "original_form": "비는중.",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.7",
          "form": "토요일에 이사를 할 것 같아",
          "original_form": "토요일에 이사를 할 것 같아",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.8",
          "form": "오늘 태풍이 오고있다던데",
          "original_form": "오늘 태풍이 오고있다던데",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.9",
          "form": "어머. 그래도 다행이다.",
          "original_form": "어머. 그래도 다행이다.",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.10",
          "form": "오늘이나 내일 이사가 아니라서.",
          "original_form": "오늘이나 내일 이사가 아니라서.",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.11",
          "form": "태풍에 이사면 정말 최악일거야.",
          "original_form": "태풍에 이사면 정말 최악일거야.",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010988.1.12",
          "form": "그치? 이사날을 잘 잡았어",
          "original_form": "그치? 이사날을 잘 잡았어",
          "speaker_id": "1",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010988.1.13",
          "form": "맞아. 적당히 선선했으면 좋겠다.",
          "original_form": "맞아. 적당히 선선했으면 좋겠다.",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010988.1.14",
          "form": "맞아 그래도 더위는 한층 가신것 같아",
          "original_form": "맞아 그래도 더위는 한층 가신것 같아",
          "speaker_id": "1",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010988.1.15",
          "form": "가을이 최고야",
          "original_form": "가을이 최고야",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010988.1.16",
          "form": "가을날씨 너무 좋지",
          "original_form": "가을날씨 너무 좋지",
          "speaker_id": "1",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010988.1.17",
          "form": "그래도 우산 챙기는 거 잊지마. 우산은 항상 필수품이니까.",
          "original_form": "그래도 우산 챙기는 거 잊지마. 우산은 항상 필수품이니까.",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010988.1.18",
          "form": "고마워. ",
          "original_form": "고마워. ",
          "speaker_id": "1",
          "time": "20210823 23:34"
        }
      ]
