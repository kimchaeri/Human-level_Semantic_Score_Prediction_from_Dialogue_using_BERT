      "utterance": [
        {
          "id": "MDRW2100007327.1.1",
          "form": "내일 휴일이신가요",
          "original_form": "내일 휴일이신가요",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007327.1.2",
          "form": "졸업하시고",
          "original_form": "졸업하시고",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007327.1.3",
          "form": "앗 아뇨 저는 지금 주말알바생이라",
          "original_form": "앗 아뇨 저는 지금 주말알바생이라",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007327.1.4",
          "form": "흐흑흡ㅎ",
          "original_form": "흐흑흡ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007327.1.5",
          "form": "취업하셨나요?!",
          "original_form": "취업하셨나요?!",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007327.1.6",
          "form": "바쁘시겠군요ㅠㅠ",
          "original_form": "바쁘시겠군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007327.1.7",
          "form": "저는 프리랜서입니다",
          "original_form": "저는 프리랜서입니다",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007327.1.8",
          "form": "허어어",
          "original_form": "허어어",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007327.1.9",
          "form": "어떤 일 하세요?!",
          "original_form": "어떤 일 하세요?!",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007327.1.10",
          "form": "IT쪽이신가요?!",
          "original_form": "IT쪽이신가요?!",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007327.1.11",
          "form": "글쓰는 쪽이에요...",
          "original_form": "글쓰는 쪽이에요...",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007327.1.12",
          "form": "웹소보신다고 해서 들어와봤어요",
          "original_form": "웹소보신다고 해서 들어와봤어요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007327.1.13",
          "form": "약간 직군이 겹치는..!",
          "original_form": "약간 직군이 겹치는..!",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007327.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007327.1.15",
          "form": "ㅎ..혹시 웹소 작가님?!?!?!?!?!",
          "original_form": "ㅎ..혹시 웹소 작가님?!?!?!?!?!",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007327.1.16",
          "form": "아니면 마케팅 쪽으로 글쓰시는 분이신가?!!!!!!!!!!!!!!!",
          "original_form": "아니면 마케팅 쪽으로 글쓰시는 분이신가?!!!!!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007327.1.17",
          "form": "허헙 글쓰신다니",
          "original_form": "허헙 글쓰신다니",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007327.1.18",
          "form": "백지 공포 어케 이겨내세요?!",
          "original_form": "백지 공포 어케 이겨내세요?!",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007327.1.19",
          "form": "아뇨ㅎㅎ 웹소는 보는 걸 더 좋아합니다!",
          "original_form": "아뇨ㅎㅎ 웹소는 보는 걸 더 좋아합니다!",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007327.1.20",
          "form": "그냥 꾸준히 써요",
          "original_form": "그냥 꾸준히 써요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007327.1.21",
          "form": "쓰다 보면 분량은 늘릴 수 있습니다",
          "original_form": "쓰다 보면 분량은 늘릴 수 있습니다",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007327.1.22",
          "form": "넘 멋져요",
          "original_form": "넘 멋져요",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007327.1.23",
          "form": "꾸준히가 진짜 어려운 거잖아요",
          "original_form": "꾸준히가 진짜 어려운 거잖아요",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007327.1.24",
          "form": "멋있진 않고 그냥 엉덩이 붙이고 휴대폰을 참고...",
          "original_form": "멋있진 않고 그냥 엉덩이 붙이고 휴대폰을 참고...",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007327.1.25",
          "form": "어케..그러죠?",
          "original_form": "어케..그러죠?",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007327.1.26",
          "form": "수험생때도 못한..",
          "original_form": "수험생때도 못한..",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007327.1.27",
          "form": "글을 원래 잘쓰셨나요",
          "original_form": "글을 원래 잘쓰셨나요",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007327.1.28",
          "form": "진짜 젤 힘든 일인 것 같아요ㅋㅋㅋㅋ",
          "original_form": "진짜 젤 힘든 일인 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007327.1.29",
          "form": "프리랜서로 활동하실 정도면 ㄹㅇ 잘쓰시는건데",
          "original_form": "프리랜서로 활동하실 정도면 ㄹㅇ 잘쓰시는건데",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007327.1.30",
          "form": "이게 프리랜서가 더 힘들잖아요",
          "original_form": "이게 프리랜서가 더 힘들잖아요",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007327.1.31",
          "form": "잘쓰진 않는데 그냥 어떻게 그렇게 됐어요ㅠㅠ",
          "original_form": "잘쓰진 않는데 그냥 어떻게 그렇게 됐어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007327.1.32",
          "form": "안정적인 삶으로 가고 싶었지만ㅠㅠ흑흑",
          "original_form": "안정적인 삶으로 가고 싶었지만ㅠㅠ흑흑",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007327.1.33",
          "form": "로또같은 (?)",
          "original_form": "로또같은 (?)",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007327.1.34",
          "form": "다들 한방을 원하죠ㅋㅋ",
          "original_form": "다들 한방을 원하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.35",
          "form": "저는 사실 그 웹소를 웹툰화하는 회사에 넘 들어가고 싶어서",
          "original_form": "저는 사실 그 웹소를 웹툰화하는 회사에 넘 들어가고 싶어서",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.36",
          "form": "취준중이긴 한데",
          "original_form": "취준중이긴 한데",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.37",
          "form": "아 그러시구나!!",
          "original_form": "아 그러시구나!!",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.38",
          "form": "엄청 재미있을 것 같아요",
          "original_form": "엄청 재미있을 것 같아요",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.39",
          "form": "덕업일치",
          "original_form": "덕업일치",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.40",
          "form": "프리랜서로 활동하시는 분들이 더 많더라고요",
          "original_form": "프리랜서로 활동하시는 분들이 더 많더라고요",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.41",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.42",
          "form": "근데 그만큼 박봉이라곸ㅋㅋㅋ그래섴ㅋㅋㅋ",
          "original_form": "근데 그만큼 박봉이라곸ㅋㅋㅋ그래섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007327.1.43",
          "form": "못벌거..! 좋아하는거라도 하겠다..! 이런 느낌인것같아요",
          "original_form": "못벌거..! 좋아하는거라도 하겠다..! 이런 느낌인것같아요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007327.1.44",
          "form": "알바도 글쓰는 알바로 꾸준히 해오셨던 건가요?",
          "original_form": "알바도 글쓰는 알바로 꾸준히 해오셨던 건가요?",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007327.1.45",
          "form": "좋은 건 어쩔 수 없죠",
          "original_form": "좋은 건 어쩔 수 없죠",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007327.1.46",
          "form": "밥벌이로 글을 쓴다는 것 자체가 너무 멋있어요",
          "original_form": "밥벌이로 글을 쓴다는 것 자체가 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007327.1.47",
          "form": "누구나 쓸 수는 있는데 잘하는 사람은 적잖아요",
          "original_form": "누구나 쓸 수는 있는데 잘하는 사람은 적잖아요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007327.1.48",
          "form": "글쓴다...기보다도 뭐더라 방송 원영상 음성 타이핑하고 그런 것도 하고",
          "original_form": "글쓴다...기보다도 뭐더라 방송 원영상 음성 타이핑하고 그런 것도 하고",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007327.1.49",
          "form": "키보드와 함께 하게 됐어요ㅋㄱ",
          "original_form": "키보드와 함께 하게 됐어요ㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007327.1.50",
          "form": "잘하는 건 아니지만 그냥 킵고잉온이다 생각만 합니다",
          "original_form": "잘하는 건 아니지만 그냥 킵고잉온이다 생각만 합니다",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007327.1.51",
          "form": "하다보면 길이 보인대요",
          "original_form": "하다보면 길이 보인대요",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007327.1.52",
          "form": "저애게도 하는말이지만",
          "original_form": "저애게도 하는말이지만",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007327.1.53",
          "form": "휴 힘냅시다",
          "original_form": "휴 힘냅시다",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007327.1.54",
          "form": "오 음성타이핑",
          "original_form": "오 음성타이핑",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007327.1.55",
          "form": "넘 신기하네요",
          "original_form": "넘 신기하네요",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007327.1.56",
          "form": "네 로또같은 한방!! 누리실 수 있기를!!!",
          "original_form": "네 로또같은 한방!! 누리실 수 있기를!!!",
          "speaker_id": "2",
          "time": "20210822 01:33"
        }
      ]
