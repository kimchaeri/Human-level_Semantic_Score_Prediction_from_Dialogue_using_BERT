      "utterance": [
        {
          "id": "MDRW2100039718.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039718.1.2",
          "form": "아뇽ㅋㅋ",
          "original_form": "아뇽ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039718.1.3",
          "form": "좋아하지는 않는데\r\n건강을 생각해서 홈트합니다",
          "original_form": "좋아하지는 않는데\r\n건강을 생각해서 홈트합니다",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039718.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039718.1.5",
          "form": "저도 런닝하다 요즘은 홈트만ㅎㅎ",
          "original_form": "저도 런닝하다 요즘은 홈트만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039718.1.6",
          "form": "누구꺼보세요.?",
          "original_form": "누구꺼보세요.?",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039718.1.7",
          "form": "오 홈트하시는군영",
          "original_form": "오 홈트하시는군영",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039718.1.8",
          "form": "저는 땅끄부부 봅니당",
          "original_form": "저는 땅끄부부 봅니당",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100039718.1.9",
          "form": "진짜 땅끄 많이 보네요",
          "original_form": "진짜 땅끄 많이 보네요",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039718.1.10",
          "form": "전 빅씨스라고ㅎㅎ 그분꺼 봐요",
          "original_form": "전 빅씨스라고ㅎㅎ 그분꺼 봐요",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039718.1.11",
          "form": "오늘도 하셨나요?",
          "original_form": "오늘도 하셨나요?",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039718.1.12",
          "form": "네! ㅋㅋ",
          "original_form": "네! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039718.1.13",
          "form": "동작 반복이 없어서 좋아요",
          "original_form": "동작 반복이 없어서 좋아요",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039718.1.14",
          "form": "와우 매일 시간 정해두고 하세요?",
          "original_form": "와우 매일 시간 정해두고 하세요?",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039718.1.15",
          "form": "백신맞고 일주일동안 못했어요ㅜ",
          "original_form": "백신맞고 일주일동안 못했어요ㅜ",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039718.1.16",
          "form": "아 백신 맞으셨군여ㅜㅜ",
          "original_form": "아 백신 맞으셨군여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039718.1.17",
          "form": "많이 아프셨나봐요",
          "original_form": "많이 아프셨나봐요",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039718.1.18",
          "form": "아뇨 아픈건 하루였는데 혹시 몰라서ㅜ",
          "original_form": "아뇨 아픈건 하루였는데 혹시 몰라서ㅜ",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039718.1.19",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039718.1.20",
          "form": "아뇽 ㅜㅜ 전 아직이요",
          "original_form": "아뇽 ㅜㅜ 전 아직이요",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100039718.1.21",
          "form": "아ㅜㅋㅋ 겁먹지마세요! 별로 안아파요!",
          "original_form": "아ㅜㅋㅋ 겁먹지마세요! 별로 안아파요!",
          "speaker_id": "1",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039718.1.22",
          "form": "주변에서 많이 아팠던 분들이\r\n많기도 하고 돌아가신분도\r\n계셔서 걱정이 많네여ㅠ",
          "original_form": "주변에서 많이 아팠던 분들이\r\n많기도 하고 돌아가신분도\r\n계셔서 걱정이 많네여ㅠ",
          "speaker_id": "2",
          "time": "20210903 21:05"
        }
      ]
