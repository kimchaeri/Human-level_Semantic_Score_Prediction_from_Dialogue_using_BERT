      "utterance": [
        {
          "id": "MDRW2100025624.1.1",
          "form": "싱어게인 나왔던 이승윤님 좋아해요",
          "original_form": "싱어게인 나왔던 이승윤님 좋아해요",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100025624.1.2",
          "form": "앨범 내줬으면 좋겠네요",
          "original_form": "앨범 내줬으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100025624.1.3",
          "form": "오 지금 찾아보니 9월에 콘서트 하시나봐요 그분",
          "original_form": "오 지금 찾아보니 9월에 콘서트 하시나봐요 그분",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025624.1.4",
          "form": "네 생중계 해준다고 하네요ㅎ",
          "original_form": "네 생중계 해준다고 하네요ㅎ",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025624.1.5",
          "form": "좋아하는 가수 있으세요?",
          "original_form": "좋아하는 가수 있으세요?",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025624.1.6",
          "form": "헐 대박이네요 집에서 즐기는 콘서트 짱",
          "original_form": "헐 대박이네요 집에서 즐기는 콘서트 짱",
          "speaker_id": "1",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025624.1.7",
          "form": "요새 이런 집콘 많더라구요ㅋㅋㅋ",
          "original_form": "요새 이런 집콘 많더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025624.1.8",
          "form": "옛날에는 지오디 좋아해서 콘서트 보러 자주  갔는데 요즘은 시들해요",
          "original_form": "옛날에는 지오디 좋아해서 콘서트 보러 자주  갔는데 요즘은 시들해요",
          "speaker_id": "1",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100025624.1.9",
          "form": "뮤지컬도 한번 봤는데 신나더라구요",
          "original_form": "뮤지컬도 한번 봤는데 신나더라구요",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025624.1.10",
          "form": "근데 비싸서 잘 못가는",
          "original_form": "근데 비싸서 잘 못가는",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025624.1.11",
          "form": "ㅋㅋㅋ저 요번에 네이버에서 송출해주는걸로 몇편 봤어요",
          "original_form": "ㅋㅋㅋ저 요번에 네이버에서 송출해주는걸로 몇편 봤어요",
          "speaker_id": "2",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100025624.1.12",
          "form": "아니 뮤지컬도 송출해주나요?",
          "original_form": "아니 뮤지컬도 송출해주나요?",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025624.1.13",
          "form": "신세계네요 ㄷㄷ",
          "original_form": "신세계네요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100025624.1.14",
          "form": "코로나 심했을때 해줬어요",
          "original_form": "코로나 심했을때 해줬어요",
          "speaker_id": "2",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025624.1.15",
          "form": "예전거를 보여주는거 같던데요",
          "original_form": "예전거를 보여주는거 같던데요",
          "speaker_id": "2",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100025624.1.16",
          "form": "오호 그래도 좋네요 코로나 때문에 다 송출 하네요 ㅜㅜㅋ",
          "original_form": "오호 그래도 좋네요 코로나 때문에 다 송출 하네요 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210901 15:02"
        },
        {
          "id": "MDRW2100025624.1.17",
          "form": "빨리 코로나 끝나서 직접 보러 가고 싶네요",
          "original_form": "빨리 코로나 끝나서 직접 보러 가고 싶네요",
          "speaker_id": "1",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100025624.1.18",
          "form": "뭐 보셨어요? 가서 보면 더 재밌다고 하더라구요",
          "original_form": "뭐 보셨어요? 가서 보면 더 재밌다고 하더라구요",
          "speaker_id": "2",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100025624.1.19",
          "form": "올슉업? 이거 봤는데 엄청 옛날이라",
          "original_form": "올슉업? 이거 봤는데 엄청 옛날이라",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100025624.1.20",
          "form": "신났던 기억만 ㅎㅎ",
          "original_form": "신났던 기억만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100025624.1.21",
          "form": "코로나 끝나면 직접 보고 싶네요",
          "original_form": "코로나 끝나면 직접 보고 싶네요",
          "speaker_id": "2",
          "time": "20210901 15:22"
        }
      ]
