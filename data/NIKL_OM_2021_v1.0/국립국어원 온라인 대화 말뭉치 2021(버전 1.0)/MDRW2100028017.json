      "utterance": [
        {
          "id": "MDRW2100028017.1.1",
          "form": "메타버스 재밌을것같다",
          "original_form": "메타버스 재밌을것같다",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100028017.1.2",
          "form": "메타버스가 요즘 핫해",
          "original_form": "메타버스가 요즘 핫해",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100028017.1.3",
          "form": "나 메타버스 게임해보고싶어",
          "original_form": "나 메타버스 게임해보고싶어",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100028017.1.4",
          "form": "미국에있는로블록스란곳이 융셩하대",
          "original_form": "미국에있는로블록스란곳이 융셩하대",
          "speaker_id": "1",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028017.1.5",
          "form": "유명",
          "original_form": "유명",
          "speaker_id": "1",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028017.1.6",
          "form": "나 얼마전에 구글 루이비통 뭐 이런 명품브랜드들이 메타버스로 뭐 한다는 거 봤어",
          "original_form": "나 얼마전에 구글 루이비통 뭐 이런 명품브랜드들이 메타버스로 뭐 한다는 거 봤어",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100028017.1.7",
          "form": "게임뿐 아니라 여러가지 하나본데 어려워",
          "original_form": "게임뿐 아니라 여러가지 하나본데 어려워",
          "speaker_id": "1",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028017.1.8",
          "form": "무슨 가상 가방인데 그게 엄청 비싸게 팔렸더라고",
          "original_form": "무슨 가상 가방인데 그게 엄청 비싸게 팔렸더라고",
          "speaker_id": "2",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028017.1.9",
          "form": "신기하다\r\n건대에선 메타버스로 비대면축제도 했대",
          "original_form": "신기하다\r\n건대에선 메타버스로 비대면축제도 했대",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028017.1.10",
          "form": "아니 메타 버스로 비대면 축제를 어떻게 해",
          "original_form": "아니 메타 버스로 비대면 축제를 어떻게 해",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028017.1.11",
          "form": "잘모르는데 배워보고싶어",
          "original_form": "잘모르는데 배워보고싶어",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028017.1.12",
          "form": "신기하네 그럼 그 축제 공간은 누가 만드나..... 학생들이?",
          "original_form": "신기하네 그럼 그 축제 공간은 누가 만드나..... 학생들이?",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028017.1.13",
          "form": "어 대단하지 우리도 경험해보자",
          "original_form": "어 대단하지 우리도 경험해보자",
          "speaker_id": "1",
