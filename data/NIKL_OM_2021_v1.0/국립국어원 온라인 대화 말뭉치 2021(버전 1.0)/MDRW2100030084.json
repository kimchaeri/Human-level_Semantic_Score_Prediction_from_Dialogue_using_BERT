      "utterance": [
        {
          "id": "MDRW2100030084.1.1",
          "form": "갑자기 인정?",
          "original_form": "갑자기 인정?",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100030084.1.2",
          "form": "제가 좀 자상하거든요",
          "original_form": "제가 좀 자상하거든요",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030084.1.3",
          "form": "그래서 그 매력에 빠진거 같아요",
          "original_form": "그래서 그 매력에 빠진거 같아요",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030084.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030084.1.5",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030084.1.6",
          "form": "여전히 자상한가요?",
          "original_form": "여전히 자상한가요?",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030084.1.7",
          "form": "여전히 재밌구?",
          "original_form": "여전히 재밌구?",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030084.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030084.1.9",
          "form": "아 요즘은 좀 노잼이라고",
          "original_form": "아 요즘은 좀 노잼이라고",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100030084.1.10",
          "form": "대놓고 말하던데",
          "original_form": "대놓고 말하던데",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100030084.1.11",
          "form": "감이 죽었나봅니다",
          "original_form": "감이 죽었나봅니다",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100030084.1.12",
          "form": "헤어지자는 시그널 아니신지",
          "original_form": "헤어지자는 시그널 아니신지",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100030084.1.13",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030084.1.14",
          "form": "잘만나는 커플한테",
          "original_form": "잘만나는 커플한테",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030084.1.15",
          "form": "그게 무슨소리에요",
          "original_form": "그게 무슨소리에요",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030084.1.16",
          "form": "결투신청인가여?",
          "original_form": "결투신청인가여?",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030084.1.17",
          "form": "동상이몽이 아니길",
          "original_form": "동상이몽이 아니길",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030084.1.18",
          "form": "두 분 사랑 영원하길 빕니다 ㅋㅋㅋ",
          "original_form": "두 분 사랑 영원하길 빕니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030084.1.19",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030084.1.20",
          "form": "이제 결투의 시간이 온거에요?",
          "original_form": "이제 결투의 시간이 온거에요?",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030084.1.21",
          "form": "ㅋㅋㅋ아니에여 name2님ㅋㅋ",
          "original_form": "ㅋㅋㅋ아니에여 &name2&님ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030084.1.22",
          "form": "여자친구분은\r\n나이가어떻거 되나여",
          "original_form": "여자친구분은\r\n나이가어떻거 되나여",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030084.1.23",
          "form": "네살차이요",
          "original_form": "네살차이요",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100030084.1.24",
          "form": "그래도 궁합은 꼭 보시구염",
          "original_form": "그래도 궁합은 꼭 보시구염",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100030084.1.25",
          "form": "봤어요",
          "original_form": "봤어요",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100030084.1.26",
          "form": "이미",
          "original_form": "이미",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100030084.1.27",
          "form": "궁합 믿으세요?",
          "original_form": "궁합 믿으세요?",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100030084.1.28",
          "form": "아 나도 볼 걸",
          "original_form": "아 나도 볼 걸",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100030084.1.29",
          "form": "안봤어요?",
          "original_form": "안봤어요?",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100030084.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100030084.1.31",
          "form": "보라고 안하셨잖아요",
          "original_form": "보라고 안하셨잖아요",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100030084.1.32",
          "form": "결혼후에는요?",
          "original_form": "결혼후에는요?",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100030084.1.33",
          "form": "이제라도 볼까봐요",
          "original_form": "이제라도 볼까봐요",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100030084.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100030084.1.35",
          "form": "한번 봐요",
          "original_form": "한번 봐요",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100030084.1.36",
          "form": "결혼전에 보는것도 좋은데",
          "original_form": "결혼전에 보는것도 좋은데",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100030084.1.37",
          "form": "안좋으면 기분이 그렇다던데",
          "original_form": "안좋으면 기분이 그렇다던데",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100030084.1.38",
          "form": "쨌든 저희 궁합은 좋대요 ㅎㅎ",
          "original_form": "쨌든 저희 궁합은 좋대요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:18"
        }
      ]
    }
