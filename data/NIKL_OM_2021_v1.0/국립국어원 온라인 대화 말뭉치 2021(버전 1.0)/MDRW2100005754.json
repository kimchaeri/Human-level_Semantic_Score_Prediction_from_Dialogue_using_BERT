      "utterance": [
        {
          "id": "MDRW2100005754.1.1",
          "form": "최근에 구매하신 물건 있으신가요?",
          "original_form": "최근에 구매하신 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 10:57"
        },
        {
          "id": "MDRW2100005754.1.2",
          "form": "저 소불고기요!",
          "original_form": "저 소불고기요!",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100005754.1.3",
          "form": "근데 배송이 안와요 ㅎㅎ",
          "original_form": "근데 배송이 안와요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100005754.1.4",
          "form": "오 소불고기 맛있겠네요 빨리 와야할텐데ㅎㅎ",
          "original_form": "오 소불고기 맛있겠네요 빨리 와야할텐데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100005754.1.5",
          "form": "전 운동화 사려고 인터넷 조금씩 뒤져보는 중이에요ㅎㅎ",
          "original_form": "전 운동화 사려고 인터넷 조금씩 뒤져보는 중이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005754.1.6",
          "form": "쿠폰대란때 산거라...",
          "original_form": "쿠폰대란때 산거라...",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005754.1.7",
          "form": "배송해준다고 하고서는...",
          "original_form": "배송해준다고 하고서는...",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005754.1.8",
          "form": "앗.. 좀 늦어지려나요.. 빨리 와야할텐데..",
          "original_form": "앗.. 좀 늦어지려나요.. 빨리 와야할텐데..",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100005754.1.9",
          "form": "4일에 도착이었는데..",
          "original_form": "4일에 도착이었는데..",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100005754.1.10",
          "form": "아이고.. 배송지연 연락 안오셨나요..??",
          "original_form": "아이고.. 배송지연 연락 안오셨나요..??",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100005754.1.11",
          "form": "이게 만원사면 만원 할인",
          "original_form": "이게 만원사면 만원 할인",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100005754.1.12",
          "form": "쿠폰 잘못 뿌린 ㅋㅋ",
          "original_form": "쿠폰 잘못 뿌린 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100005754.1.13",
          "form": "어엇ㅋㅋㅋ 진짜 그렇게 뿌렸나요?? 웃기네욬ㅋㅋㅋㅋ",
          "original_form": "어엇ㅋㅋㅋ 진짜 그렇게 뿌렸나요?? 웃기네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100005754.1.14",
          "form": "네 네이버에 신세계푸드 쿠폰",
          "original_form": "네 네이버에 신세계푸드 쿠폰",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100005754.1.15",
          "form": "치시면 나옵니다ㅋㅋ",
          "original_form": "치시면 나옵니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100005754.1.16",
          "form": "검색해봤는데 아이고ㅋㅋㅋ 득템하신분들 개꿀이셨겠네요ㅋㅋㅋ",
          "original_form": "검색해봤는데 아이고ㅋㅋㅋ 득템하신분들 개꿀이셨겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100005754.1.17",
          "form": "네 근데 배송이 안옵니다 ㅋㅋㅋ",
          "original_form": "네 근데 배송이 안옵니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100005754.1.18",
          "form": "희소식 듣고 사람 엄청 몰렸었나보네요ㅋㅋㅋ",
          "original_form": "희소식 듣고 사람 엄청 몰렸었나보네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100005754.1.19",
