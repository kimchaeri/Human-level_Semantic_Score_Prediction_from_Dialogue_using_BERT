      "utterance": [
        {
          "id": "MDRW2100024682.1.1",
          "form": "ㅋㅋㅋㅋㅋ나도 일본 갔다가 기념품으로 먹거리만 사왔던 기억이 나",
          "original_form": "ㅋㅋㅋㅋㅋ나도 일본 갔다가 기념품으로 먹거리만 사왔던 기억이 나",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100024682.1.2",
          "form": "나는 오키나와도 너무 가보고싶어",
          "original_form": "나는 오키나와도 너무 가보고싶어",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100024682.1.3",
          "form": "먹는건 후회라도 없으니까",
          "original_form": "먹는건 후회라도 없으니까",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100024682.1.4",
          "form": "선물하기도 편하고",
          "original_form": "선물하기도 편하고",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100024682.1.5",
          "form": "헉 맞아 오키나와의 화창한 날씨와 분위기를 누려보고싶어",
          "original_form": "헉 맞아 오키나와의 화창한 날씨와 분위기를 누려보고싶어",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100024682.1.6",
          "form": "맞아 먹는 거 선물이 최고야",
          "original_form": "맞아 먹는 거 선물이 최고야",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100024682.1.7",
          "form": "오키나와에 츄라우미 수족관",
          "original_form": "오키나와에 츄라우미 수족관",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024682.1.8",
          "form": "가보고싶어",
          "original_form": "가보고싶어",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024682.1.9",
          "form": "오 거기는 뭐야 엄청 큰 곳이야?",
          "original_form": "오 거기는 뭐야 엄청 큰 곳이야?",
          "speaker_id": "2",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024682.1.10",
          "form": "응 세계 3대 수족관중",
          "original_form": "응 세계 3대 수족관중",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024682.1.11",
          "form": "하나야",
          "original_form": "하나야",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024682.1.12",
          "form": "와 으리으리하겠네",
          "original_form": "와 으리으리하겠네",
          "speaker_id": "2",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100024682.1.13",
          "form": "다음에 같이 여행가면 그 수족관 가보자",
          "original_form": "다음에 같이 여행가면 그 수족관 가보자",
          "speaker_id": "2",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100024682.1.14",
          "form": "오키나와 숙소도 좋아보이더라",
          "original_form": "오키나와 숙소도 좋아보이더라",
          "speaker_id": "1",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100024682.1.15",
          "form": "같이가면 너무재밌겠다",
          "original_form": "같이가면 너무재밌겠다",
          "speaker_id": "1",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100024682.1.16",
          "form": "너 면허 국제면허되니",
          "original_form": "너 면허 국제면허되니",
          "speaker_id": "1",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100024682.1.17",
          "form": "모르겠어ㅋㅋㅋㅋ여행가서 차 끌어볼 생각은 안 해봤는데",
          "original_form": "모르겠어ㅋㅋㅋㅋ여행가서 차 끌어볼 생각은 안 해봤는데",
          "speaker_id": "2",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100024682.1.18",
          "form": "나 좀 태워줘",
          "original_form": "나 좀 태워줘",
          "speaker_id": "1",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100024682.1.19",
          "form": "국제면허인지 확인도 안 해봄",
          "original_form": "국제면허인지 확인도 안 해봄",
          "speaker_id": "2",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100024682.1.20",
          "form": "히치하이킹",
          "original_form": "히치하이킹",
          "speaker_id": "1",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100024682.1.21",
          "form": "ㅋㅋㅋㅋ외지에서 사고나면 어떡해",
          "original_form": "ㅋㅋㅋㅋ외지에서 사고나면 어떡해",
          "speaker_id": "2",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100024682.1.22",
          "form": "보험들면 괜찮지않을까",
          "original_form": "보험들면 괜찮지않을까",
          "speaker_id": "1",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100024682.1.23",
          "form": "여행지에 차운전하면 이동반경이 넓어지잖아",
          "original_form": "여행지에 차운전하면 이동반경이 넓어지잖아",
          "speaker_id": "1",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100024682.1.24",
          "form": "맞아 확실히 기동성이 느는 건 있어",
          "original_form": "맞아 확실히 기동성이 느는 건 있어",
          "speaker_id": "2",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100024682.1.25",
          "form": "사막 같이 허허벌판 사고 안 나는 지역이면",
          "original_form": "사막 같이 허허벌판 사고 안 나는 지역이면",
          "speaker_id": "2",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100024682.1.26",
          "form": "태워줄게 ㅋㅋㅋㅋ거긴 모래밖에 없잖어",
          "original_form": "태워줄게 ㅋㅋㅋㅋ거긴 모래밖에 없잖어",
          "speaker_id": "2",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100024682.1.27",
          "form": "더 무섭지않냐",
          "original_form": "더 무섭지않냐",
          "speaker_id": "1",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024682.1.28",
          "form": "아무것도 없는 곳에 사고나면",
          "original_form": "아무것도 없는 곳에 사고나면",
          "speaker_id": "1",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024682.1.29",
          "form": "구조가 힘들던데",
          "original_form": "구조가 힘들던데",
          "speaker_id": "1",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024682.1.30",
          "form": "아 그러네 거긴 모래폭풍에 휩쓸리면 죽잖아",
          "original_form": "아 그러네 거긴 모래폭풍에 휩쓸리면 죽잖아",
          "speaker_id": "2",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024682.1.31",
          "form": "여행자보험들고",
          "original_form": "여행자보험들고",
          "speaker_id": "1",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024682.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ여행 절망편",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ여행 절망편",
          "speaker_id": "2",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100024682.1.33",
          "form": "여행갔었냐",
          "original_form": "여행갔었냐",
          "speaker_id": "1",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024682.1.34",
          "form": "응 보험은 들었었을걸?",
          "original_form": "응 보험은 들었었을걸?",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024682.1.35",
          "form": "보험하루에 천원단위던데 보통",
          "original_form": "보험하루에 천원단위던데 보통",
          "speaker_id": "1",
          "time": "20210831 17:16"
        }
      ]
