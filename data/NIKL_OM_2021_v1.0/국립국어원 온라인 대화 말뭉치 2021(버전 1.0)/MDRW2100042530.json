      "utterance": [
        {
          "id": "MDRW2100042530.1.1",
          "form": "뮤지컬 가봤어요?",
          "original_form": "뮤지컬 가봤어요?",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042530.1.2",
          "form": "음 국내에선 두번가봤어요!",
          "original_form": "음 국내에선 두번가봤어요!",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042530.1.3",
          "form": "가보셨나요?",
          "original_form": "가보셨나요?",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042530.1.4",
          "form": "와 뭐가보셨어요",
          "original_form": "와 뭐가보셨어요",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042530.1.5",
          "form": "전 뮤지컬은 안가봤어요",
          "original_form": "전 뮤지컬은 안가봤어요",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042530.1.6",
          "form": "음 저 오페라유령이랑 위키드요ㅎㅎ",
          "original_form": "음 저 오페라유령이랑 위키드요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042530.1.7",
          "form": "영화는좋아하세요?",
          "original_form": "영화는좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 11:10"
        },
        {
          "id": "MDRW2100042530.1.8",
          "form": "와 오페라유령 ㅠㅠㅠ멋졌겠다",
          "original_form": "와 오페라유령 ㅠㅠㅠ멋졌겠다",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042530.1.9",
          "form": "네네 영화 조아해요",
          "original_form": "네네 영화 조아해요",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042530.1.10",
          "form": "어제밤에 에밀리인 파리 봤어요",
          "original_form": "어제밤에 에밀리인 파리 봤어요",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042530.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042530.1.12",
          "form": "저그거봐요",
          "original_form": "저그거봐요",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042530.1.13",
          "form": "여주이뻐요",
          "original_form": "여주이뻐요",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042530.1.14",
          "form": "그쵸 저도 작년에 재택근뮤 할땨 자주 틀어놨는데",
          "original_form": "그쵸 저도 작년에 재택근뮤 할땨 자주 틀어놨는데",
          "speaker_id": "2",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042530.1.15",
          "form": "어제 다시 봐도 재밌더라고요 로망의 삶",
          "original_form": "어제 다시 봐도 재밌더라고요 로망의 삶",
          "speaker_id": "2",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042530.1.16",
          "form": "ㅎㅎㅎ맞아요",
          "original_form": "ㅎㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042530.1.17",
          "form": "배경도이뻐요",
          "original_form": "배경도이뻐요",
          "speaker_id": "1",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042530.1.18",
          "form": "네네 파리 너무 가고싶다 ...",
          "original_form": "네네 파리 너무 가고싶다 ...",
          "speaker_id": "2",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100042530.1.19",
          "form": "그르게요ㅜㅜ",
          "original_form": "그르게요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042530.1.20",
          "form": "어디 제알 가고파요?",
          "original_form": "어디 제알 가고파요?",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042530.1.21",
          "form": "영화어떤장르좋아하세요",
          "original_form": "영화어떤장르좋아하세요",
          "speaker_id": "1",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042530.1.22",
          "form": "저는 로맨틱 코미디 젤 조아하죠",
          "original_form": "저는 로맨틱 코미디 젤 조아하죠",
          "speaker_id": "2",
          "time": "20210907 12:18"
        }
      ]
