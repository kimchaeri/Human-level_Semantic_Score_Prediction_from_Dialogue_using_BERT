      "utterance": [
        {
          "id": "MDRW2100002119.1.1",
          "form": "공연 보러 가는 거 좋아해?",
          "original_form": "공연 보러 가는 거 좋아해?",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002119.1.2",
          "form": "ㅇㅇ 좋아하는데 코로나라 못감",
          "original_form": "ㅇㅇ 좋아하는데 코로나라 못감",
          "speaker_id": "2",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002119.1.3",
          "form": "맞아 코로나 ㅠㅠ 무섭지",
          "original_form": "맞아 코로나 ㅠㅠ 무섭지",
          "speaker_id": "1",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100002119.1.4",
          "form": "무슨공연좋아하는데",
          "original_form": "무슨공연좋아하는데",
          "speaker_id": "2",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002119.1.5",
          "form": "원래 밴드 공연같은거 가는거 좋아했는데",
          "original_form": "원래 밴드 공연같은거 가는거 좋아했는데",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002119.1.6",
          "form": "오 밴드 좋지 누구",
          "original_form": "오 밴드 좋지 누구",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002119.1.7",
          "form": "크라잉넛 이런 아저씨들 좋아했지",
          "original_form": "크라잉넛 이런 아저씨들 좋아했지",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002119.1.8",
          "form": "최근엔 유튜브로 공연하더라 ㅠㅠ",
          "original_form": "최근엔 유튜브로 공연하더라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002119.1.9",
          "form": "아 크라잉넛세대는 아닌것같은데 어찌 잘알았네",
          "original_form": "아 크라잉넛세대는 아닌것같은데 어찌 잘알았네",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002119.1.10",
          "form": "현장의 감동이 더큰데 아쉽긴해ㅜ",
          "original_form": "현장의 감동이 더큰데 아쉽긴해ㅜ",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002119.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그세대야 ㅠㅠ 맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그세대야 ㅠㅠ 맞아",
          "speaker_id": "1",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002119.1.12",
          "form": "너는 누구 좋아했어?",
          "original_form": "너는 누구 좋아했어?",
          "speaker_id": "1",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002119.1.13",
          "form": "나 자우림 ㅋㅋㅋㅋ크라잉넛이랑 같은세댄데 갓윤아",
          "original_form": "나 자우림 ㅋㅋㅋㅋ크라잉넛이랑 같은세댄데 갓윤아",
          "speaker_id": "2",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002119.1.14",
          "form": "자우림ㅋㅋㅋㅋ 아직도 스물다섯스물하나 이런 노래들 한번씩 듣는다 나두",
          "original_form": "자우림ㅋㅋㅋㅋ 아직도 스물다섯스물하나 이런 노래들 한번씩 듣는다 나두",
          "speaker_id": "1",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002119.1.15",
          "form": "멋있어너무ㅜㅜ",
          "original_form": "멋있어너무ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002119.1.16",
          "form": "슴달슴하나는 완전 최신이지ㅜ 난 콘서트 얼마전에 갔다왔거든",
          "original_form": "슴달슴하나는 완전 최신이지ㅜ 난 콘서트 얼마전에 갔다왔거든",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002119.1.17",
          "form": "장난아니더라",
          "original_form": "장난아니더라",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002119.1.18",
          "form": "진심 대박 눈물남",
          "original_form": "진심 대박 눈물남",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002119.1.19",
          "form": "아 최근에 콘서트 했구나",
          "original_form": "아 최근에 콘서트 했구나",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002119.1.20",
          "form": "대박이다 완전부럽다 ㅠㅜ",
          "original_form": "대박이다 완전부럽다 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002119.1.21",
          "form": "응 좀 지났긴했는데 작년에취소됐던거 올해 조용하게했었음",
          "original_form": "응 좀 지났긴했는데 작년에취소됐던거 올해 조용하게했었음",
          "speaker_id": "2",
          "time": "20210820 00:16"
        },
        {
          "id": "MDRW2100002119.1.22",
          "form": "진짜 라이브 장난아니더라..",
          "original_form": "진짜 라이브 장난아니더라..",
