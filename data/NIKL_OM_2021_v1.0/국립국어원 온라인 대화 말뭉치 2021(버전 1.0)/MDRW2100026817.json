        {
          "id": "MDRW2100026817.1.1",
          "form": "영화 좋아하시나요?",
          "original_form": "영화 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026817.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026817.1.3",
          "form": "최근에 뭐보셨나요",
          "original_form": "최근에 뭐보셨나요",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026817.1.4",
          "form": "극장도 가시나요?",
          "original_form": "극장도 가시나요?",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026817.1.5",
          "form": "저는 넷플릭스로 영화봤어요",
          "original_form": "저는 넷플릭스로 영화봤어요",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026817.1.6",
          "form": "최근 본것은 호텔뭄바이입니다",
          "original_form": "최근 본것은 호텔뭄바이입니다",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026817.1.7",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026817.1.8",
          "form": "극장에서 공포영화 보는거 좋아해요",
          "original_form": "극장에서 공포영화 보는거 좋아해요",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026817.1.9",
          "form": "음향이 좋더라구요",
          "original_form": "음향이 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026817.1.10",
          "form": "저도 넷플릭스 이용중이에요",
          "original_form": "저도 넷플릭스 이용중이에요",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026817.1.11",
          "form": "저는 넷플릭스 기생중이에요",
          "original_form": "저는 넷플릭스 기생중이에요",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026817.1.12",
          "form": "공포영화는 싫어합니다",
          "original_form": "공포영화는 싫어합니다",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026817.1.13",
          "form": "저는 개미 역할 하고있어요",
          "original_form": "저는 개미 역할 하고있어요",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026817.1.14",
          "form": "많이들 봐줘서 기뻐요",
          "original_form": "많이들 봐줘서 기뻐요",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026817.1.15",
          "form": "액션좋아하시나요",
          "original_form": "액션좋아하시나요",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026817.1.16",
          "form": "물주시군요. 영화계에서 환영받으실겁니다",
          "original_form": "물주시군요. 영화계에서 환영받으실겁니다",
          "speaker_id": "2",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026817.1.17",
          "form": "전 스릴러 공포좋아해요",
          "original_form": "전 스릴러 공포좋아해요",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026817.1.18",
