      "utterance": [
        {
          "id": "MDRW2100038509.1.1",
          "form": "감자전 맛나게 만드는 방법 안다",
          "original_form": "감자전 맛나게 만드는 방법 안다",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038509.1.2",
          "form": "뭔데",
          "original_form": "뭔데",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038509.1.3",
          "form": "맛있겠네 집에 치즈 많다",
          "original_form": "맛있겠네 집에 치즈 많다",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038509.1.4",
          "form": "웅웅ㅋ ㅋ 냉동실 치즈 좀 써버려야겠네",
          "original_form": "웅웅ㅋ ㅋ 냉동실 치즈 좀 써버려야겠네",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038509.1.5",
          "form": "그래 많이 만들어라",
          "original_form": "그래 많이 만들어라",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038509.1.6",
          "form": "웅웅 넘 맛나겠다 치즈 감자전..",
          "original_form": "웅웅 넘 맛나겠다 치즈 감자전..",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038509.1.7",
          "form": "근데 살은 질 듯",
          "original_form": "근데 살은 질 듯",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038509.1.8",
          "form": "살찌는건,, 어쩔수 없다 ㅋ",
          "original_form": "살찌는건,, 어쩔수 없다 ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038509.1.9",
          "form": "그래 그래도 적당히 먹어",
          "original_form": "그래 그래도 적당히 먹어",
          "speaker_id": "2",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100038509.1.10",
          "form": "치즈 넘 맛있옹",
          "original_form": "치즈 넘 맛있옹",
          "speaker_id": "1",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100038509.1.11",
          "form": "치즈 많이 넣어라 많다",
          "original_form": "치즈 많이 넣어라 많다",
          "speaker_id": "2",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100038509.1.12",
          "form": "웅웅 넘 고소하겠당",
          "original_form": "웅웅 넘 고소하겠당",
          "speaker_id": "1",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100038509.1.13",
          "form": "기름은 덜 써라",
          "original_form": "기름은 덜 써라",
          "speaker_id": "2",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100038509.1.14",
          "form": "기름을 많이 넣어야 맛난데~",
          "original_form": "기름을 많이 넣어야 맛난데~",
          "speaker_id": "1",
          "time": "20210902 14:56"
        },
        {
