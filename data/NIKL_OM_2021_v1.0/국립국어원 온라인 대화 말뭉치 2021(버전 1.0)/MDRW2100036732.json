      "utterance": [
        {
          "id": "MDRW2100036732.1.1",
          "form": "ㅋㅋㅋ아 언니 ㅋㅋ 내가 실수했나보다 ㅋㅋ",
          "original_form": "ㅋㅋㅋ아 언니 ㅋㅋ 내가 실수했나보다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:40"
        },
        {
          "id": "MDRW2100036732.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 그럴수도 있지\nㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 그럴수도 있지\nㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036732.1.3",
          "form": "이거 저거 한다고 막 눌러졌나부다 ㅋ",
          "original_form": "이거 저거 한다고 막 눌러졌나부다 ㅋ",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036732.1.4",
          "form": "청소해야되는데 ㅋㅋㅋ 일단 내 청소하고 올게 ㅋ",
          "original_form": "청소해야되는데 ㅋㅋㅋ 일단 내 청소하고 올게 ㅋ",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036732.1.5",
          "form": "ㅋㅋㅋㄱ 그랴 대충하고 와",
          "original_form": "ㅋㅋㅋㄱ 그랴 대충하고 와",
          "speaker_id": "1",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100036732.1.6",
          "form": "얍 대충하고 와야되겠당 ㅋㅋ",
          "original_form": "얍 대충하고 와야되겠당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100036732.1.7",
          "form": "옹 ㅋㅋㅋㅋㄱ",
          "original_form": "옹 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036732.1.8",
          "form": "청소하고 왔고 ㅋㅋ 이제 밥먹어야되는데 뭘먹을지..고민이구먼 ㅋㅋㅋ",
          "original_form": "청소하고 왔고 ㅋㅋ 이제 밥먹어야되는데 뭘먹을지..고민이구먼 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100036732.1.9",
          "form": "언니는 열심히 일하고있겠구만",
          "original_form": "언니는 열심히 일하고있겠구만",
          "speaker_id": "2",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100036732.1.10",
          "form": "나도 밥 먹고 왔다",
          "original_form": "나도 밥 먹고 왔다",
          "speaker_id": "1",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100036732.1.11",
          "form": "청국장 순두부",
          "original_form": "청국장 순두부",
          "speaker_id": "1",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100036732.1.12",
          "form": "밥 먹었나??",
          "original_form": "밥 먹었나??",
          "speaker_id": "1",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100036732.1.13",
          "form": "오오 나도 밥 먹어야징 ㅋㅋ 뭘 먹을지ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ푸핫 ㅋㅋ",
          "original_form": "오오 나도 밥 먹어야징 ㅋㅋ 뭘 먹을지ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ푸핫 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036732.1.14",
          "form": "ㅋㅋㅋㄱ 아직 안먹고 뭐했노??",
          "original_form": "ㅋㅋㅋㄱ 아직 안먹고 뭐했노??",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036732.1.15",
          "form": "우리가 너무 괴롭혔나 ㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "우리가 너무 괴롭혔나 ㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036732.1.16",
          "form": "일단 통장은 만들었으니",
          "original_form": "일단 통장은 만들었으니",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036732.1.17",
          "form": "돈넣는건 내일 해야지 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "돈넣는건 내일 해야지 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036732.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ친구가 또 뭘 묻길래 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ친구가 또 뭘 묻길래 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036732.1.19",
          "form": "그거 한다고 ㅋㅋ",
          "original_form": "그거 한다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036732.1.20",
          "form": "그려 돈은 천천히 넣으면된다. ㅋㅋ",
          "original_form": "그려 돈은 천천히 넣으면된다. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036732.1.21",
          "form": "한 번 해보면 쉽다 할걸 ㅋㅋ",
          "original_form": "한 번 해보면 쉽다 할걸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100036732.1.22",
          "form": "엉 ㅎㅎ 통장 만들기가 어렵네 ㅎㅎ",
          "original_form": "엉 ㅎㅎ 통장 만들기가 어렵네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036732.1.23",
          "form": "저번에도 통장만들다가 포기했다 ㅋㅋ",
          "original_form": "저번에도 통장만들다가 포기했다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036732.1.24",
          "form": "웅웅 그 통장으로 계속 해라 언니 ㅋㅋ",
          "original_form": "웅웅 그 통장으로 계속 해라 언니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036732.1.25",
          "form": "다른통장은 일단 패스하자 ㅋㅋ",
          "original_form": "다른통장은 일단 패스하자 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036732.1.26",
          "form": "옹 알았으 ㅎㅎ 담에 주식하는것도 좀 가르쳐도이 ㅎㅎ",
          "original_form": "옹 알았으 ㅎㅎ 담에 주식하는것도 좀 가르쳐도이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036732.1.27",
          "form": "이걸로 우리원투",
          "original_form": "이걸로 우리원투",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036732.1.28",
          "form": "과자값이라도 벌면 좋지",
          "original_form": "과자값이라도 벌면 좋지",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036732.1.29",
          "form": "내커피도 ㅋㅋㅋㅋㅋ",
          "original_form": "내커피도 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036732.1.30",
          "form": "근데 언니 나도 주식은...흠...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ마이너스다 ㅋㅋㅋㅋㅋㅋ",
