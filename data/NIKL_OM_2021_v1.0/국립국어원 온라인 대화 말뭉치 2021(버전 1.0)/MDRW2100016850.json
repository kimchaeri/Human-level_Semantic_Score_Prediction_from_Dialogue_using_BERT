      "utterance": [
        {
          "id": "MDRW2100016850.1.1",
          "form": "좋아하는 음식 있어요?",
          "original_form": "좋아하는 음식 있어요?",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016850.1.2",
          "form": "가리는 음식 없어요. 육해공 다 좋아해요",
          "original_form": "가리는 음식 없어요. 육해공 다 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016850.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016850.1.4",
          "form": "오 되게 잘드시는구나",
          "original_form": "오 되게 잘드시는구나",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016850.1.5",
          "form": "저는 떡튀순이요",
          "original_form": "저는 떡튀순이요",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016850.1.6",
          "form": "너무맛있어요ㅎㅎ",
          "original_form": "너무맛있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016850.1.7",
          "form": "ㅎㅎ순대도 좋아하시나요",
          "original_form": "ㅎㅎ순대도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016850.1.8",
          "form": "네!! 순대 너무좋아하죠",
          "original_form": "네!! 순대 너무좋아하죠",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016850.1.9",
          "form": "제가 당면류를 좋아해서",
          "original_form": "제가 당면류를 좋아해서",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016850.1.10",
          "form": "아 그래서ㅎㅎ",
          "original_form": "아 그래서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016850.1.11",
          "form": "특히 맛있게느껴지는거같아요ㅎㅎ",
          "original_form": "특히 맛있게느껴지는거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016850.1.12",
          "form": "간 같은 부속물도 좋아하세요?",
          "original_form": "간 같은 부속물도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016850.1.13",
          "form": "네 그래서 곱창류도 환장해요ㅎㅎ",
          "original_form": "네 그래서 곱창류도 환장해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016850.1.14",
          "form": "곱창 좋아하세요?",
          "original_form": "곱창 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016850.1.15",
          "form": "없어서 못 먹어요",
          "original_form": "없어서 못 먹어요",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016850.1.16",
          "form": "저두요ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016850.1.17",
          "form": "특히 소! 곱막대창 다 좋아요",
          "original_form": "특히 소! 곱막대창 다 좋아요",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016850.1.18",
          "form": "소곱창 너무 맛있지않아요??",
          "original_form": "소곱창 너무 맛있지않아요??",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016850.1.19",
          "form": "진짜ㅜㅜ",
          "original_form": "진짜ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016850.1.20",
          "form": "간 천엽 같이 주면 더 좋고ㅜ",
          "original_form": "간 천엽 같이 주면 더 좋고ㅜ",
