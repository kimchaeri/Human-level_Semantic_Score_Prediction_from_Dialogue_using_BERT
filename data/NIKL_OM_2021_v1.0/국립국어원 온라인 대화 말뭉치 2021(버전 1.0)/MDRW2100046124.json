      "utterance": [
        {
          "id": "MDRW2100046124.1.1",
          "form": "저런 인형도 팔린다니 충격이에요",
          "original_form": "저런 인형도 팔린다니 충격이에요",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046124.1.2",
          "form": "고양이를 모르는 사람이 그린것같이요 ㅋㅋ",
          "original_form": "고양이를 모르는 사람이 그린것같이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046124.1.3",
          "form": "그냥 점점 보편에서 멀어지는 기분ㅋㅋㅠㅠ",
          "original_form": "그냥 점점 보편에서 멀어지는 기분ㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046124.1.4",
          "form": "네... ㅋㅋㅋ",
          "original_form": "네... ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.5",
          "form": "마녀배달부 키키에 나오는 고양이는",
          "original_form": "마녀배달부 키키에 나오는 고양이는",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.6",
          "form": "너무 귀여운데ㅜㅜ",
          "original_form": "너무 귀여운데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.7",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.8",
          "form": "꼬리도",
          "original_form": "꼬리도",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.9",
          "form": "그런 고양이 인형 만들지ㅜㅜ",
          "original_form": "그런 고양이 인형 만들지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.10",
          "form": "동그랗게말려있는데",
          "original_form": "동그랗게말려있는데",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.11",
          "form": "저런고양이가 어디있는지참",
          "original_form": "저런고양이가 어디있는지참",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.12",
          "form": "검은색에 빨간 리본도 너무 이쁘고요ㅜㅜ",
          "original_form": "검은색에 빨간 리본도 너무 이쁘고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.13",
          "form": "키키에니오는냥이는귀엽져 ㅠㅜ",
          "original_form": "키키에니오는냥이는귀엽져 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.14",
          "form": "근데 춘식이는 이름부터 좀 그런 컨셉인 거 같아서",
          "original_form": "근데 춘식이는 이름부터 좀 그런 컨셉인 거 같아서",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046124.1.15",
          "form": "이상해요",
          "original_form": "이상해요",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046124.1.16",
          "form": "뭔가 유쾌하려고 했다가",
          "original_form": "뭔가 유쾌하려고 했다가",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046124.1.17",
          "form": "유사동물인듯 ㅋㅋ",
          "original_form": "유사동물인듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046124.1.18",
          "form": "실패한 느낌",
          "original_form": "실패한 느낌",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046124.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046124.1.20",
          "form": "그냥 뭉텅이느낌",
          "original_form": "그냥 뭉텅이느낌",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046124.1.21",
          "form": "유사 동물 상품...",
          "original_form": "유사 동물 상품...",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046124.1.22",
          "form": "근데 임티도 추가되면",
          "original_form": "근데 임티도 추가되면",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046124.1.23",
          "form": "너무 싫을 거 같아요",
