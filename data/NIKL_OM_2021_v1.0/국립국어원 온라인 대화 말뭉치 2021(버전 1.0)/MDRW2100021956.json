      "utterance": [
        {
          "id": "MDRW2100021956.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021956.1.2",
          "form": "조던 보신적있나용",
          "original_form": "조던 보신적있나용",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021956.1.3",
          "form": "저는 그 종류는 관심이 없어서 잘 몰라요 ㅎㅎㅎ",
          "original_form": "저는 그 종류는 관심이 없어서 잘 몰라요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021956.1.4",
          "form": "어떤 패션 좋아하세요?",
          "original_form": "어떤 패션 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100021956.1.5",
          "form": "편하게 입는것 좋아하고 신발도 편한거 신어요! 제가 운동하다가 무릎을 다쳐서 좀 몸에 좋은거 편한거 선호하는편이에요!",
          "original_form": "편하게 입는것 좋아하고 신발도 편한거 신어요! 제가 운동하다가 무릎을 다쳐서 좀 몸에 좋은거 편한거 선호하는편이에요!",
          "speaker_id": "1",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100021956.1.6",
          "form": "나이키 운동화중에선 p6000제일 좋아해요 ㅎㅎ",
          "original_form": "나이키 운동화중에선 p6000제일 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100021956.1.7",
          "form": "아 정말요? ㅠㅠㅠㅠ 더 안아프시면 좋게쎠요오ㅠㅠㅠ",
          "original_form": "아 정말요? ㅠㅠㅠㅠ 더 안아프시면 좋게쎠요오ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100021956.1.8",
          "form": "이전엔 에어도 많이 신었는데 그게 편하지는 않더라구여!",
          "original_form": "이전엔 에어도 많이 신었는데 그게 편하지는 않더라구여!",
          "speaker_id": "1",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100021956.1.9",
          "form": "그래서 뮬 이런거 유행할때도 안신어봤어요ㅜㅜ",
          "original_form": "그래서 뮬 이런거 유행할때도 안신어봤어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100021956.1.10",
          "form": "완전 무릎에도 안좋고 발목에도 안좋은 스타일 ㅜㅜ",
          "original_form": "완전 무릎에도 안좋고 발목에도 안좋은 스타일 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021956.1.11",
          "form": "아아ㅠㅠㅠ 맞아요",
          "original_form": "아아ㅠㅠㅠ 맞아요",
          "speaker_id": "2",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021956.1.12",
          "form": "저도 뮬은 몸에 안 좋대서",
          "original_form": "저도 뮬은 몸에 안 좋대서",
          "speaker_id": "2",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021956.1.13",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021956.1.14",
          "form": "건강해야죠! 스타일보다는!",
          "original_form": "건강해야죠! 스타일보다는!",
          "speaker_id": "2",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021956.1.15",
          "form": "머리에도 관심있으시나요?",
          "original_form": "머리에도 관심있으시나요?",
          "speaker_id": "2",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021956.1.16",
          "form": "그쵸 ㅎㅎ그래서 바지 통 넓은거 유행해서 좋아요!",
          "original_form": "그쵸 ㅎㅎ그래서 바지 통 넓은거 유행해서 좋아요!",
          "speaker_id": "1",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100021956.1.17",
          "form": "머리 자주 하세여??",
          "original_form": "머리 자주 하세여??",
          "speaker_id": "1",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100021956.1.18",
          "form": "아뇨아뇨 돈 아깝더라구요..코로나라ㅠㅠ",
          "original_form": "아뇨아뇨 돈 아깝더라구요..코로나라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100021956.1.19",
          "form": "저는 예전엔 미용실 자주 갔었는데 코로나 이후론 그냥 최대한 버티다가? 가는것 같아요 ㅋㅋ",
          "original_form": "저는 예전엔 미용실 자주 갔었는데 코로나 이후론 그냥 최대한 버티다가? 가는것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100021956.1.20",
          "form": "똑같네요 ㅋㅋ",
          "original_form": "똑같네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100021956.1.21",
          "form": "그래도 요즘엔 마스크하고 하지만 코로나 초반엔 마스크 벗고 했거든요",
          "original_form": "그래도 요즘엔 마스크하고 하지만 코로나 초반엔 마스크 벗고 했거든요",
          "speaker_id": "1",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100021956.1.22",
          "form": "그래서 좀 걱정도 되더라구요",
          "original_form": "그래서 좀 걱정도 되더라구요",
          "speaker_id": "1",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100021956.1.23",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100021956.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100021956.1.25",
          "form": "탈색하신적있나요?",
          "original_form": "탈색하신적있나요?",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100021956.1.26",
          "form": "아니요 없어요~~",
          "original_form": "아니요 없어요~~",
          "speaker_id": "1",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100021956.1.27",
          "form": "분홍빛나게 염색한적 있는데 탈색해야 오래간다더라구요",
          "original_form": "분홍빛나게 염색한적 있는데 탈색해야 오래간다더라구요",
          "speaker_id": "1",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100021956.1.28",
          "form": "근데 어우 두피상할까봐 저는 안해봤어요",
          "original_form": "근데 어우 두피상할까봐 저는 안해봤어요",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100021956.1.29",
          "form": "탈색해보셨어요?",
          "original_form": "탈색해보셨어요?",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100021956.1.30",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100021956.1.31",
          "form": "하고 다시 길으려니",
          "original_form": "하고 다시 길으려니",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100021956.1.32",
          "form": "2년넘게걸려서 후회중입니다ㅠ",
          "original_form": "2년넘게걸려서 후회중입니다ㅠ",
