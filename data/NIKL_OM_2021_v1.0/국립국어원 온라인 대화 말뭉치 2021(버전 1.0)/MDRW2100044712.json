      "utterance": [
        {
          "id": "MDRW2100044712.1.1",
          "form": "직장다니세요?",
          "original_form": "직장다니세요?",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044712.1.2",
          "form": "네 다니고있어요ㅜ",
          "original_form": "네 다니고있어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044712.1.3",
          "form": "출근하기싫네요ㅜ",
          "original_form": "출근하기싫네요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044712.1.4",
          "form": "아...ㅜ재택은 안하세요?",
          "original_form": "아...ㅜ재택은 안하세요?",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044712.1.5",
          "form": "재택이 어려운직종이라 그냥 쭈욱다니고있습니다",
          "original_form": "재택이 어려운직종이라 그냥 쭈욱다니고있습니다",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044712.1.6",
          "form": "재택하세요?",
          "original_form": "재택하세요?",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044712.1.7",
          "form": "가끔이요",
          "original_form": "가끔이요",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044712.1.8",
          "form": "음 어떠세요?",
          "original_form": "음 어떠세요?",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044712.1.9",
          "form": "관리부라 쭉하기 힘들고 주1회정도? ㅋㅋ",
          "original_form": "관리부라 쭉하기 힘들고 주1회정도? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044712.1.10",
          "form": "장단점이 있죠?",
          "original_form": "장단점이 있죠?",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044712.1.11",
          "form": "네네 우선 출퇴근시간엔 너무 좋은데 집중은 회사가 넘사벽이죠",
          "original_form": "네네 우선 출퇴근시간엔 너무 좋은데 집중은 회사가 넘사벽이죠",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044712.1.12",
          "form": "ㅋㅋ그렇겠져",
          "original_form": "ㅋㅋ그렇겠져",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044712.1.13",
          "form": "예전에 집에서 공부하겠다고 도서관 나오면",
          "original_form": "예전에 집에서 공부하겠다고 도서관 나오면",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044712.1.14",
          "form": "집에서 공부 안했었는데",
          "original_form": "집에서 공부 안했었는데",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044712.1.15",
          "form": "저같은 사람은 재택안하는것이 나을듯요ㅋㅋ",
          "original_form": "저같은 사람은 재택안하는것이 나을듯요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044712.1.16",
          "form": "ㅋㅋ 저도 반성....",
          "original_form": "ㅋㅋ 저도 반성....",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044712.1.17",
          "form": "재택하기전날 최대한 일하고 나와요",
          "original_form": "재택하기전날 최대한 일하고 나와요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044712.1.18",
          "form": "ㅋㅋ어머나",
          "original_form": "ㅋㅋ어머나",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044712.1.19",
          "form": "재택할 마음의 준비인가요",
          "original_form": "재택할 마음의 준비인가요",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044712.1.20",
          "form": "그렇죠 그리고 작은 양심..",
          "original_form": "그렇죠 그리고 작은 양심..",
          "speaker_id": "1",
          "time": "20210907 23:38"
        }
      ]
