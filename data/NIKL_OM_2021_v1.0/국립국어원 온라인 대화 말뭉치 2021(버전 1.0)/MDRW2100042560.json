          "original_form": "ㅇㅇ......",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.2",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.3",
          "form": "아냐",
          "original_form": "아냐",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.4",
          "form": "앙스타는 2도 내고",
          "original_form": "앙스타는 2도 내고",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.5",
          "form": "이젠 사실",
          "original_form": "이젠 사실",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.6",
          "form": "잘먹고 잘사는데",
          "original_form": "잘먹고 잘사는데",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.7",
          "form": "많이 무덤덤해졋어",
          "original_form": "많이 무덤덤해졋어",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.8",
          "form": "왜 럽프듀가 망해야하지?",
          "original_form": "왜 럽프듀가 망해야하지?",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.9",
          "form": "이런 생각도",
          "original_form": "이런 생각도",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.10",
          "form": "햇엇지.....",
          "original_form": "햇엇지.....",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042560.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.12",
          "form": "글킨 해",
          "original_form": "글킨 해",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.13",
          "form": "나름 한섭챙겨준다고",
          "original_form": "나름 한섭챙겨준다고",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.14",
          "form": "한복 낸건데",
          "original_form": "한복 낸건데",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.15",
          "form": "아니 솔직히 망하려면",
          "original_form": "아니 솔직히 망하려면",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.16",
          "form": "눈치가 없었을뿐...",
          "original_form": "눈치가 없었을뿐...",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.17",
          "form": "앙별이 먼저 망해야지",
          "original_form": "앙별이 먼저 망해야지",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.18",
          "form": "왜 걔넨 안망하죠....?",
          "original_form": "왜 걔넨 안망하죠....?",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.19",
          "form": "강가에서 낚시하고잇으면",
          "original_form": "강가에서 낚시하고잇으면",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.20",
          "form": "원수의 시체 떠내려온다매.....",
          "original_form": "원수의 시체 떠내려온다매.....",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042560.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서 한스타 망햇자너",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서 한스타 망햇자너",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.22",
          "form": "그치만 일스타는",
          "original_form": "그치만 일스타는",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.23",
          "form": "승승장구 하잔아....",
          "original_form": "승승장구 하잔아....",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.24",
          "form": "자꾸 실트에 걔네 올라와.....",
          "original_form": "자꾸 실트에 걔네 올라와.....",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.25",
          "form": "대체재가",
          "original_form": "대체재가",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.26",
          "form": "없어서 그런듯",
          "original_form": "없어서 그런듯",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.27",
          "form": "비록 한때 그 아성을 따라갈만큼은",
          "original_form": "비록 한때 그 아성을 따라갈만큼은",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.28",
          "form": "아니지만",
          "original_form": "아니지만",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.29",
          "form": "ㅇㅇ그래서 사람들이",
          "original_form": "ㅇㅇ그래서 사람들이",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.30",
          "form": "로오히같은거나",
          "original_form": "로오히같은거나",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042560.1.31",
          "form": "주워먹는듯",
          "original_form": "주워먹는듯",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.32",
          "form": "로오히도",
          "original_form": "로오히도",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.33",
          "form": "사실 겜적으로도 굉장히 노잼겜이었는데",
          "original_form": "사실 겜적으로도 굉장히 노잼겜이었는데",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.34",
          "form": "전성기에 비하면 많이 죽었지",
          "original_form": "전성기에 비하면 많이 죽었지",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.35",
          "form": "난 그게 죽은건지",
          "original_form": "난 그게 죽은건지",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.36",
          "form": "모바일은 초기 플로우를 유지하는 게임이 드문듯",
          "original_form": "모바일은 초기 플로우를 유지하는 게임이 드문듯",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.37",
          "form": "내가 결국 모조리 블락에 성공한건지",
          "original_form": "내가 결국 모조리 블락에 성공한건지",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.38",
          "form": "잘 구분이 안가서",
          "original_form": "잘 구분이 안가서",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아냐 나 겜 커뮤하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아냐 나 겜 커뮤하는데",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.40",
          "form": "초반엔 진짜",
          "original_form": "초반엔 진짜",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042560.1.41",
          "form": "한페이지에 절반은 로오히인데다",
          "original_form": "한페이지에 절반은 로오히인데다",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.42",
          "form": "누가 불호글 올리면",
          "original_form": "누가 불호글 올리면",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.43",
          "form": "근데 얘는",
          "original_form": "근데 얘는",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.44",
          "form": "걍 조용히 접거나 숨만 쉬는 사람 비중이",
          "original_form": "걍 조용히 접거나 숨만 쉬는 사람 비중이",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.45",
          "form": "높아진듯",
          "original_form": "높아진듯",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.46",
          "form": "그래도 얘만한 겜 없다 그랬거든",
          "original_form": "그래도 얘만한 겜 없다 그랬거든",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.47",
          "form": "그렇게 터져댈땐 안접더니",
          "original_form": "그렇게 터져댈땐 안접더니",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.48",
          "form": "ㅇㅇ 근데 이젠 글 빈도도 확 줄고",
          "original_form": "ㅇㅇ 근데 이젠 글 빈도도 확 줄고",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.49",
          "form": "분위기도 좀 바뀜",
          "original_form": "분위기도 좀 바뀜",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042560.1.50",
          "form": "애초에 게임이 노잼이었어",
          "original_form": "애초에 게임이 노잼이었어",
          "speaker_id": "2",
          "time": "20210906 22:57"
        }
      ]
    }
