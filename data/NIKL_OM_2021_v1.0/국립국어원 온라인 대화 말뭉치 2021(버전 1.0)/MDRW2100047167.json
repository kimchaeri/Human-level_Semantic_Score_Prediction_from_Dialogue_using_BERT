      "utterance": [
        {
          "id": "MDRW2100047167.1.1",
          "form": "아쉽죠?",
          "original_form": "아쉽죠?",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100047167.1.2",
          "form": "저는 투어로 이탈리아 스위스가봤어요",
          "original_form": "저는 투어로 이탈리아 스위스가봤어요",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100047167.1.3",
          "form": "네네ㅜㅠ 미국이랑 유럽은 가봐야하는데요!!",
          "original_form": "네네ㅜㅠ 미국이랑 유럽은 가봐야하는데요!!",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100047167.1.4",
          "form": "이탈리아 커피 맛있죠?",
          "original_form": "이탈리아 커피 맛있죠?",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100047167.1.5",
          "form": "맞아요! 에스프레소먹었는데 맛있었어요",
          "original_form": "맞아요! 에스프레소먹었는데 맛있었어요",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100047167.1.6",
          "form": "왜 스벅이 거기서 맥을 못축이는지 알겠더라고요",
          "original_form": "왜 스벅이 거기서 맥을 못축이는지 알겠더라고요",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100047167.1.7",
          "form": "진짜 한국 커피는 쓰레기라고 ㅋㅋ",
          "original_form": "진짜 한국 커피는 쓰레기라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100047167.1.8",
          "form": "맞아여ㅋㅋ 근데 한국이 살긴편해요",
          "original_form": "맞아여ㅋㅋ 근데 한국이 살긴편해요",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100047167.1.9",
          "form": "물도 다사먹어야하고",
          "original_form": "물도 다사먹어야하고",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100047167.1.10",
          "form": "로컬음식들은 짠경향이 있었어요",
          "original_form": "로컬음식들은 짠경향이 있었어요",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100047167.1.11",
          "form": "전 캐나다 좀 길게 있다왔는데 ㅋㅋ 한국이 좋아요ㅋㅋ한국인은 한국이 살기엔 최고죠",
          "original_form": "전 캐나다 좀 길게 있다왔는데 ㅋㅋ 한국이 좋아요ㅋㅋ한국인은 한국이 살기엔 최고죠",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100047167.1.12",
          "form": "어디가 제일 좋으셨어요??",
          "original_form": "어디가 제일 좋으셨어요??",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100047167.1.13",
          "form": "오 워킹가셨었어요?",
          "original_form": "오 워킹가셨었어요?",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100047167.1.14",
          "form": "저는ㅋㅋ글쎄요 대만?",
          "original_form": "저는ㅋㅋ글쎄요 대만?",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100047167.1.15",
          "form": "이탈리아랑스위스도",
          "original_form": "이탈리아랑스위스도",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100047167.1.16",
          "form": "러시아도",
          "original_form": "러시아도",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100047167.1.17",
          "form": "필리핀도 일본도...다 다른매력이ㅠ",
          "original_form": "필리핀도 일본도...다 다른매력이ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100047167.1.18",
          "form": "되게 여러 곳 가셨네요!  넵 워킹ㅎㅎ",
          "original_form": "되게 여러 곳 가셨네요!  넵 워킹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100047167.1.19",
          "form": "다 좋군요ㅋㅋ",
          "original_form": "다 좋군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100047167.1.20",
          "form": "코로나전에 러시아 찾아보고있었어요!",
          "original_form": "코로나전에 러시아 찾아보고있었어요!",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100047167.1.21",
          "form": "러시아 어느 쪽으로 가신거예요?",
          "original_form": "러시아 어느 쪽으로 가신거예요?",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100047167.1.22",
          "form": "와 저 워킹신청했었는데 떨어져서ㅠㅠ",
          "original_form": "와 저 워킹신청했었는데 떨어져서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100047167.1.23",
          "form": "블라디보스톡이요!",
          "original_form": "블라디보스톡이요!",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100047167.1.24",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100047167.1.25",
          "form": "ㅋㅋ근데 영어 못하고 그래요ㅜㅋㅋ",
          "original_form": "ㅋㅋ근데 영어 못하고 그래요ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100047167.1.26",
          "form": "거긴 물가 좀 싸죠???",
          "original_form": "거긴 물가 좀 싸죠???",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100047167.1.27",
          "form": "음식 맛있어요?",
          "original_form": "음식 맛있어요?",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100047167.1.28",
          "form": "몇년있으셨어요?",
          "original_form": "몇년있으셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100047167.1.29",
          "form": "쪼오금요",
          "original_form": "쪼오금요",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100047167.1.30",
          "form": "곰새우ㅠㅠ 새로운맛에 눈뜨고왔어요",
          "original_form": "곰새우ㅠㅠ 새로운맛에 눈뜨고왔어요",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100047167.1.31",
          "form": "늦게 가서 일찍 왔어요ㅋㅋ",
          "original_form": "늦게 가서 일찍 왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100047167.1.32",
          "form": "곰새우 적어놔야겠네요!",
          "original_form": "곰새우 적어놔야겠네요!",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100047167.1.33",
          "form": "젊을때 가면 진짜 좋은게 워킹같아요!",
          "original_form": "젊을때 가면 진짜 좋은게 워킹같아요!",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100047167.1.34",
          "form": "너무맛있어서 한국에서 가끔 시켜먹오요ㅋㅋ",
          "original_form": "너무맛있어서 한국에서 가끔 시켜먹오요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100047167.1.35",
          "form": "ㅠㅠ 전이제나이가...",
          "original_form": "ㅠㅠ 전이제나이가...",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100047167.1.36",
          "form": "안돼여",
          "original_form": "안돼여",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100047167.1.37",
          "form": "아...ㅜ 저도 막차에 막차로 가서...ㅜ 그러니까 체력이 안되더라고요ㅜㅠ",
          "original_form": "아...ㅜ 저도 막차에 막차로 가서...ㅜ 그러니까 체력이 안되더라고요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100047167.1.38",
          "form": "호주 한번 더 가볼까했는데 나이 늘려준다더니 없던일이 되구ㅜ",
          "original_form": "호주 한번 더 가볼까했는데 나이 늘려준다더니 없던일이 되구ㅜ",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100047167.1.39",
          "form": "아ㅠㅠ 저랑 나이대가 비슷하신가봐요ㅋㅋ",
          "original_form": "아ㅠㅠ 저랑 나이대가 비슷하신가봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:45"
        }
      ]
    }
