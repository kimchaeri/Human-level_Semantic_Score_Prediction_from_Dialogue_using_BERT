      "utterance": [
        {
          "id": "MDRW2100037791.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "2",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037791.1.2",
          "form": "아니요 학생이신가요?",
          "original_form": "아니요 학생이신가요?",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100037791.1.3",
          "form": "아 저두요 ㅎ 졸업하신지 오래되셨나요?",
          "original_form": "아 저두요 ㅎ 졸업하신지 오래되셨나요?",
          "speaker_id": "2",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100037791.1.4",
          "form": "한 오년되었어요",
          "original_form": "한 오년되었어요",
          "speaker_id": "1",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100037791.1.5",
          "form": "동아리활동 하셨어요?",
          "original_form": "동아리활동 하셨어요?",
          "speaker_id": "1",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100037791.1.6",
          "form": "아뇨 ㅠ 님은요?",
          "original_form": "아뇨 ㅠ 님은요?",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100037791.1.7",
          "form": "저는 새내기때 아주 잠깐했어요",
          "original_form": "저는 새내기때 아주 잠깐했어요",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037791.1.8",
          "form": "어떤 동아리요?",
          "original_form": "어떤 동아리요?",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037791.1.9",
          "form": "배드민턴이요",
          "original_form": "배드민턴이요",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037791.1.10",
          "form": "오 저도 배드민턴 동아리나 동호회 들어가보고싶어용 호기심 !",
          "original_form": "오 저도 배드민턴 동아리나 동호회 들어가보고싶어용 호기심 !",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100037791.1.11",
          "form": "그죠 근데 술동아리라서",
          "original_form": "그죠 근데 술동아리라서",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100037791.1.12",
          "form": "저는 술을 안좋아해서 나왔어요",
          "original_form": "저는 술을 안좋아해서 나왔어요",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100037791.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100037791.1.14",
          "form": "보통 이름만 그렇게해놓도",
          "original_form": "보통 이름만 그렇게해놓도",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100037791.1.15",
          "form": "친목도모가 주목적이죠 ㅋㅋ",
          "original_form": "친목도모가 주목적이죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100037791.1.16",
          "form": "저는 배드민턴이 목적이였는데ㅠㅠ",
          "original_form": "저는 배드민턴이 목적이였는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100037791.1.17",
          "form": "잘찾아보면 아닌데도 있겠지만요",
          "original_form": "잘찾아보면 아닌데도 있겠지만요",
