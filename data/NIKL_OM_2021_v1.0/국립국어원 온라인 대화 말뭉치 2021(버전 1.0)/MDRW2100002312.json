      "utterance": [
        {
          "id": "MDRW2100002312.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210820 09:12"
        },
        {
          "id": "MDRW2100002312.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002312.1.3",
          "form": "애인 있으세요? 전 없는데 흑흑",
          "original_form": "애인 있으세요? 전 없는데 흑흑",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002312.1.4",
          "form": "ㅎㅎ 저는 신혼이에요",
          "original_form": "ㅎㅎ 저는 신혼이에요",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002312.1.5",
          "form": "어머 너무좋으시겠어요",
          "original_form": "어머 너무좋으시겠어요",
          "speaker_id": "1",
          "time": "20210820 09:18"
        },
        {
          "id": "MDRW2100002312.1.6",
          "form": "결혼한지 얼마나되셨어요?",
          "original_form": "결혼한지 얼마나되셨어요?",
          "speaker_id": "1",
          "time": "20210820 09:18"
        },
        {
          "id": "MDRW2100002312.1.7",
          "form": "2년이요 ㅎ 연애한지 얼마나 되셨어용?",
          "original_form": "2년이요 ㅎ 연애한지 얼마나 되셨어용?",
          "speaker_id": "2",
          "time": "20210820 09:19"
        },
        {
          "id": "MDRW2100002312.1.8",
          "form": "올해 초에 헤어지고 없네요 ㅎㅎ ㅜ",
          "original_form": "올해 초에 헤어지고 없네요 ㅎㅎ ㅜ",
          "speaker_id": "1",
          "time": "20210820 09:19"
        },
        {
          "id": "MDRW2100002312.1.9",
          "form": "남편분은 어떻게만나셨어요?",
          "original_form": "남편분은 어떻게만나셨어요?",
          "speaker_id": "1",
          "time": "20210820 09:19"
        },
        {
          "id": "MDRW2100002312.1.10",
          "form": "전 결혼정보회사 생각중이라..",
          "original_form": "전 결혼정보회사 생각중이라..",
          "speaker_id": "1",
          "time": "20210820 09:19"
        },
        {
          "id": "MDRW2100002312.1.11",
          "form": "그러셨구나 ㅜ 저는 고용지원센터어 갔다가 만났어요",
          "original_form": "그러셨구나 ㅜ 저는 고용지원센터어 갔다가 만났어요",
          "speaker_id": "2",
          "time": "20210820 09:20"
        },
        {
          "id": "MDRW2100002312.1.12",
          "form": "연애는 얼마나하셨어요?",
          "original_form": "연애는 얼마나하셨어요?",
          "speaker_id": "1",
          "time": "20210820 09:20"
        },
        {
          "id": "MDRW2100002312.1.13",
          "form": "오년 넘게요",
          "original_form": "오년 넘게요",
          "speaker_id": "2",
          "time": "20210820 09:21"
        },
        {
          "id": "MDRW2100002312.1.14",
          "form": "엄청오래만나셨네요! 거의 도합 7년..",
          "original_form": "엄청오래만나셨네요! 거의 도합 7년..",
          "speaker_id": "1",
          "time": "20210820 09:22"
        },
        {
          "id": "MDRW2100002312.1.15",
          "form": "넵 ㅎㅎ 그러네요",
          "original_form": "넵 ㅎㅎ 그러네요",
          "speaker_id": "2",
          "time": "20210820 09:24"
        },
        {
          "id": "MDRW2100002312.1.16",
          "form": "남편분 보시면 아직도 설레고 그러시나요? ㅎㅎ",
          "original_form": "남편분 보시면 아직도 설레고 그러시나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:27"
        },
        {
          "id": "MDRW2100002312.1.17",
          "form": "음.. 그건아닌것같아요 ㅋ 근데 신기한건",
          "original_form": "음.. 그건아닌것같아요 ㅋ 근데 신기한건",
          "speaker_id": "2",
          "time": "20210820 09:28"
        },
        {
          "id": "MDRW2100002312.1.18",
          "form": "연애할때도 엄청 좋아했다고 생각했는데 결혼하니 더 좋아졌어요",
          "original_form": "연애할때도 엄청 좋아했다고 생각했는데 결혼하니 더 좋아졌어요",
          "speaker_id": "2",
          "time": "20210820 09:28"
        },
        {
          "id": "MDRW2100002312.1.19",
          "form": "헉 그럴수도 있군요.. 오래 연애안해봐서 그런 감정을 모르겠어요 신기하다!",
          "original_form": "헉 그럴수도 있군요.. 오래 연애안해봐서 그런 감정을 모르겠어요 신기하다!",
          "speaker_id": "1",
          "time": "20210820 09:29"
        }
      ]
