        },
        {
          "id": "MDRW2100027319.1.2",
          "form": "마시고 싶네요",
          "original_form": "마시고 싶네요",
          "speaker_id": "2",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027319.1.3",
          "form": "눈감고 마시면 콜라 사이다 구별이 어럽대요",
          "original_form": "눈감고 마시면 콜라 사이다 구별이 어럽대요",
          "speaker_id": "2",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027319.1.4",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "1",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027319.1.5",
          "form": "해봐야겠네요",
          "original_form": "해봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027319.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027319.1.7",
          "form": "전 밀키스 암바사도",
          "original_form": "전 밀키스 암바사도",
          "speaker_id": "2",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027319.1.8",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.9",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "2",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.10",
          "form": "저도요!ㅇ",
          "original_form": "저도요!ㅇ",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.11",
          "form": "사이다는 천연사이다 좋아해요",
          "original_form": "사이다는 천연사이다 좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.12",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.13",
          "form": "사이다에 막걸리",
          "original_form": "사이다에 막걸리",
          "speaker_id": "2",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.14",
          "form": "칠성보다는 천연ㅋ",
          "original_form": "칠성보다는 천연ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.15",
          "form": "존맛이에요",
          "original_form": "존맛이에요",
          "speaker_id": "2",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.16",
          "form": "1통2반으로ㅋㅋ",
          "original_form": "1통2반으로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.17",
          "form": "천연사이다 짱이에요",
          "original_form": "천연사이다 짱이에요",
          "speaker_id": "2",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.18",
          "form": "2통1반아닌가요",
          "original_form": "2통1반아닌가요",
          "speaker_id": "2",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.19",
          "form": "부드럽고",
          "original_form": "부드럽고",
          "speaker_id": "1",
          "time": "20210902 11:28"
        },
        {
          "id": "MDRW2100027319.1.20",
