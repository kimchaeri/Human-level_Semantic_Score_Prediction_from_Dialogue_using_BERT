      "utterance": [
        {
          "id": "MDRW2100000871.1.1",
          "form": "애들 오늘 숙제 다했지?",
          "original_form": "애들 오늘 숙제 다했지?",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000871.1.2",
          "form": "웅 다했지",
          "original_form": "웅 다했지",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000871.1.3",
          "form": "용선생 한국사도 다 읽고?",
          "original_form": "용선생 한국사도 다 읽고?",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000871.1.4",
          "form": "웅 읽긴 했는데 잘 이해하는진 모르겠어 ㅎㅎ",
          "original_form": "웅 읽긴 했는데 잘 이해하는진 모르겠어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000871.1.5",
          "form": "ㅎㅎ 잘 이해하고 있겠지",
          "original_form": "ㅎㅎ 잘 이해하고 있겠지",
          "speaker_id": "2",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000871.1.6",
          "form": "걱정은 좀 되는데 잘하겠지 뭐",
          "original_form": "걱정은 좀 되는데 잘하겠지 뭐",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000871.1.7",
          "form": "방학동안 힘들텐데 잘 참고 했네",
          "original_form": "방학동안 힘들텐데 잘 참고 했네",
          "speaker_id": "2",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000871.1.8",
          "form": "기특하징",
          "original_form": "기특하징",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000871.1.9",
          "form": "응 정말 기특하다",
          "original_form": "응 정말 기특하다",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000871.1.10",
          "form": "맛난거라도 사줘야겠네",
          "original_form": "맛난거라도 사줘야겠네",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000871.1.11",
          "form": "ㅎㅎ 기대하겠어",
          "original_form": "ㅎㅎ 기대하겠어",
          "speaker_id": "1",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000871.1.12",
          "form": "색시가 왜 기대함 애들 사줄건데 ㅋㅋ",
          "original_form": "색시가 왜 기대함 애들 사줄건데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000871.1.13",
          "form": "한집에서 먹는데 나도 포함이지 뭘 ㅎㅎ",
          "original_form": "한집에서 먹는데 나도 포함이지 뭘 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000871.1.14",
          "form": "ㅎㅎㅎ 완전 웃김",
          "original_form": "ㅎㅎㅎ 완전 웃김",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000871.1.15",
          "form": "뭐 사줄라공",
          "original_form": "뭐 사줄라공",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000871.1.16",
          "form": "글쎄 고생한 아들들을 위해 피자 쏠까나",
          "original_form": "글쎄 고생한 아들들을 위해 피자 쏠까나",
          "speaker_id": "2",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000871.1.17",
          "form": "오우 애들 좋아하겠다",
          "original_form": "오우 애들 좋아하겠다",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000871.1.18",
          "form": "언제 사줄건지 예약 잡아야겠어 ㅋㅋ",
          "original_form": "언제 사줄건지 예약 잡아야겠어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000871.1.19",
          "form": "ㅎㅎ당장 내일이라도 사줄테니 기대햐",
          "original_form": "ㅎㅎ당장 내일이라도 사줄테니 기대햐",
          "speaker_id": "2",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000871.1.20",
          "form": "굿 너무 좋앙",
          "original_form": "굿 너무 좋앙",
          "speaker_id": "1",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000871.1.21",
          "form": "아침부터 4~5시간씩 숙제하는게 쉽지는 않을껴",
          "original_form": "아침부터 4~5시간씩 숙제하는게 쉽지는 않을껴",
          "speaker_id": "2",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000871.1.22",
          "form": "힘들지 방학내내 ㅠ",
          "original_form": "힘들지 방학내내 ㅠ",
          "speaker_id": "1",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000871.1.23",
          "form": "보상이 있어야 또 열심히 하지",
          "original_form": "보상이 있어야 또 열심히 하지",
          "speaker_id": "2",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000871.1.24",
          "form": "옳은 말씀",
          "original_form": "옳은 말씀",
          "speaker_id": "1",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000871.1.25",
          "form": "열심히만 하라고 해 보상은 팡팡 해줄테니 ㅎㅎ",
          "original_form": "열심히만 하라고 해 보상은 팡팡 해줄테니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000871.1.26",
          "form": "그대로 전해주겠어",
          "original_form": "그대로 전해주겠어",
          "speaker_id": "1",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000871.1.27",
          "form": "오키",
          "original_form": "오키",
          "speaker_id": "2",
          "time": "20210819 20:26"
        }
      ]
    }
