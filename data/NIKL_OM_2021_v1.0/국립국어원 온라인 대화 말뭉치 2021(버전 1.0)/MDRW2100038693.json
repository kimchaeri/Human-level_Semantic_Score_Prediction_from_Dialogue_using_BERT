      "utterance": [
        {
          "id": "MDRW2100038693.1.1",
          "form": "네 유산균이랑 종비요 ㅋㅋ",
          "original_form": "네 유산균이랑 종비요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100038693.1.2",
          "form": "어디거드세용",
          "original_form": "어디거드세용",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100038693.1.3",
          "form": "저 종비는 얼라이브꺼랑 유산균은 아이허브에서 샀는데 갑자기 생각이 안나네요 ㅋㅋ",
          "original_form": "저 종비는 얼라이브꺼랑 유산균은 아이허브에서 샀는데 갑자기 생각이 안나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100038693.1.4",
          "form": "아항 ㅋㅋㅋㅋ 얼라이브 알 엄청 큰거요",
          "original_form": "아항 ㅋㅋㅋㅋ 얼라이브 알 엄청 큰거요",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100038693.1.5",
          "form": "네 맞아요 ㅋㅋㅋ 첨에 먹을땐 넘 커서 삼키기 힘들었어요 ㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋㅋ 첨에 먹을땐 넘 커서 삼키기 힘들었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100038693.1.6",
          "form": "효과가 잇는지 사실 전 몰겟어요ㅋㅋ",
          "original_form": "효과가 잇는지 사실 전 몰겟어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038693.1.7",
          "form": "ㅋㅋㅋ 사실 저도 그래요 ㅋㅋ 그냥 먹으라니까 먹는 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 사실 저도 그래요 ㅋㅋ 그냥 먹으라니까 먹는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038693.1.8",
          "form": "ㅋㅋㅋㅋ 그나마 효과느끼는거 유산균",
          "original_form": "ㅋㅋㅋㅋ 그나마 효과느끼는거 유산균",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100038693.1.9",
          "form": "아 유산균은 정말.. 안 먹다 처음 먹을때 진짜 확 느껴요 ㅋㅋ",
          "original_form": "아 유산균은 정말.. 안 먹다 처음 먹을때 진짜 확 느껴요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100038693.1.10",
          "form": "ㅋㅋㅋㅋ 마자요 안챙기다 먹으면 또 느끼고..",
          "original_form": "ㅋㅋㅋㅋ 마자요 안챙기다 먹으면 또 느끼고..",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038693.1.11",
          "form": "그쵸 꾸준히 먹는 게 중요한 거 같아요!",
          "original_form": "그쵸 꾸준히 먹는 게 중요한 거 같아요!",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038693.1.12",
          "form": "식습관 안좋으면 더 필요한거같아용",
          "original_form": "식습관 안좋으면 더 필요한거같아용",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038693.1.13",
          "form": "정말요 ㅎㅎ 나이들수록 느끼네요",
          "original_form": "정말요 ㅎㅎ 나이들수록 느끼네요",
