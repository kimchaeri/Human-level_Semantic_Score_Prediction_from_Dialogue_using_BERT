          "form": "ㅋㅋ네",
          "original_form": "ㅋㅋ네",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013088.1.9",
          "form": "저도 한식좋아해요",
          "original_form": "저도 한식좋아해요",
          "speaker_id": "1",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013088.1.10",
          "form": "고기필수 ㅋㅋ",
          "original_form": "고기필수 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013088.1.11",
          "form": "구워먹는거요?",
          "original_form": "구워먹는거요?",
          "speaker_id": "2",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013088.1.12",
          "form": "아님 불고가같은거요?",
          "original_form": "아님 불고가같은거요?",
          "speaker_id": "2",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100013088.1.13",
