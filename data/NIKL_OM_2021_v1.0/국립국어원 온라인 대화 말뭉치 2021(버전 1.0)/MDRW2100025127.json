      "utterance": [
        {
          "id": "MDRW2100025127.1.1",
          "form": "요즘 날씨가 왕덥네여ㅋㅋㅋㅋㅋㅋ",
          "original_form": "요즘 날씨가 왕덥네여ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100025127.1.2",
          "form": "습해요 완전!!!!!!!",
          "original_form": "습해요 완전!!!!!!!",
          "speaker_id": "2",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025127.1.3",
          "form": "지금은 비가 퍼붓네요ㅠㅠ",
          "original_form": "지금은 비가 퍼붓네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025127.1.4",
          "form": "다시하늘이맑아졌어요ㅋㅋㅋ",
          "original_form": "다시하늘이맑아졌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100025127.1.5",
          "form": "언제까지비가올런지",
          "original_form": "언제까지비가올런지",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100025127.1.6",
          "form": "아 님이시군요",
          "original_form": "아 님이시군요",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025127.1.7",
          "form": "예",
          "original_form": "예",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025127.1.8",
          "form": "그러쳐",
          "original_form": "그러쳐",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025127.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025127.1.10",
          "form": "날씨가 너무 기분나빠요",
          "original_form": "날씨가 너무 기분나빠요",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025127.1.11",
          "form": "끕끕하고 찐득한날씨에여",
          "original_form": "끕끕하고 찐득한날씨에여",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025127.1.12",
          "form": "이번주는계속비가온데요",
          "original_form": "이번주는계속비가온데요",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025127.1.13",
          "form": "지겹다요 지겨워",
          "original_form": "지겹다요 지겨워",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025127.1.14",
          "form": "아까는 뭐 구멍 뚫린줄 알았습니다",
          "original_form": "아까는 뭐 구멍 뚫린줄 알았습니다",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025127.1.15",
          "form": "ㅋㅋㅋ 너무다행이에요 밖에나갔다왔는데",
          "original_form": "ㅋㅋㅋ 너무다행이에요 밖에나갔다왔는데",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025127.1.16",
          "form": "비가너무많이와서걱정했거든요",
          "original_form": "비가너무많이와서걱정했거든요",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025127.1.17",
          "form": "그렇구나 고생했네요",
          "original_form": "그렇구나 고생했네요",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025127.1.18",
          "form": "저는 나가려는데 누가 우산 훔쳐가서 짜증이났네요",
          "original_form": "저는 나가려는데 누가 우산 훔쳐가서 짜증이났네요",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025127.1.19",
          "form": "뭔가 비가계속오니까",
          "original_form": "뭔가 비가계속오니까",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025127.1.20",
          "form": "곧여름이올것같은기분이에요",
          "original_form": "곧여름이올것같은기분이에요",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025127.1.21",
          "form": "누가우산을훔쳐갔어요??ㅋㅋㅋ",
          "original_form": "누가우산을훔쳐갔어요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025127.1.22",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025127.1.23",
          "form": "아직 여름이 안간거같은데",
          "original_form": "아직 여름이 안간거같은데",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025127.1.24",
          "form": "네 누가 훔쳐갔어요 말도안하고",
          "original_form": "네 누가 훔쳐갔어요 말도안하고",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025127.1.25",
          "form": "범인을찾았나요",
          "original_form": "범인을찾았나요",
          "speaker_id": "1",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025127.1.26",
          "form": "네 찾았어요 전화를 했죠",
          "original_form": "네 찾았어요 전화를 했죠",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025127.1.27",
          "form": "도둑이라고",
          "original_form": "도둑이라고",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025127.1.28",
          "form": "도둑이져말도안하고가져가면",
          "original_form": "도둑이져말도안하고가져가면",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025127.1.29",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025127.1.30",
          "form": "다행이네요 범인을바로잡아가지고",
          "original_form": "다행이네요 범인을바로잡아가지고",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025127.1.31",
          "form": "낼은 비가올까요",
          "original_form": "낼은 비가올까요",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025127.1.32",
          "form": "다행이죠",
          "original_form": "다행이죠",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025127.1.33",
          "form": "낼도 비가온다고하긴했는데 모르겠네용",
          "original_form": "낼도 비가온다고하긴했는데 모르겠네용",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025127.1.34",
          "form": "비를 다 마셔줄래요?",
          "original_form": "비를 다 마셔줄래요?",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025127.1.35",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025127.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 환경오염이심해서 안대여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 환경오염이심해서 안대여",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025127.1.37",
          "form": "환경오염이 심하니까",
          "original_form": "환경오염이 심하니까",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025127.1.38",
          "form": "비가 겁나 오나봐요",
          "original_form": "비가 겁나 오나봐요",
          "speaker_id": "2",
          "time": "20210901 11:14"
        }
      ]
