      "utterance": [
        {
          "id": "MDRW2100011409.1.1",
          "form": "나이대가 저랑 비슷하시면 싸이월드 아시겠어요",
          "original_form": "나이대가 저랑 비슷하시면 싸이월드 아시겠어요",
          "speaker_id": "1",
          "time": "20210824 12:17"
        },
        {
          "id": "MDRW2100011409.1.2",
          "form": "네~고등학교때 핫했던 것 같아요",
          "original_form": "네~고등학교때 핫했던 것 같아요",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011409.1.3",
          "form": "저는 대학교때 싸이 열심히 했어요",
          "original_form": "저는 대학교때 싸이 열심히 했어요",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011409.1.4",
          "form": "다시 복구되면 하지는 않을것 같아요",
          "original_form": "다시 복구되면 하지는 않을것 같아요",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011409.1.5",
          "form": "아~복구되면 옛날 사진볼수 있어 좋을것 같아요",
          "original_form": "아~복구되면 옛날 사진볼수 있어 좋을것 같아요",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100011409.1.6",
          "form": "네 그런데 싸이 일기장 썼던거 생각하면 오글오글",
          "original_form": "네 그런데 싸이 일기장 썼던거 생각하면 오글오글",
          "speaker_id": "1",
          "time": "20210825 11:30"
        },
        {
          "id": "MDRW2100011409.1.7",
          "form": "그러게요~근데 뭐라고 썼을지 궁금해요 ㅎ",
          "original_form": "그러게요~근데 뭐라고 썼을지 궁금해요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100011409.1.8",
          "form": "연애의 슬픔?ㅎㅎ",
          "original_form": "연애의 슬픔?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:10"
        },
        {
          "id": "MDRW2100011409.1.9",
          "form": "ㅋㅋ 그런것도 있을 수도 있겠어욯ㅎ",
          "original_form": "ㅋㅋ 그런것도 있을 수도 있겠어욯ㅎ",
          "speaker_id": "2",
          "time": "20210826 09:49"
        },
        {
          "id": "MDRW2100011409.1.10",
          "form": "연애인들도 싸이에 올린거로 계속 놀림 받가도하고 ㅎㅎ",
          "original_form": "연애인들도 싸이에 올린거로 계속 놀림 받가도하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100011409.1.11",
          "form": "예전에 싸이월드 홈피 꾸미는 것도 하셨었어요?",
          "original_form": "예전에 싸이월드 홈피 꾸미는 것도 하셨었어요?",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100011409.1.12",
          "form": "많이는 안했어요 노래정도?",
          "original_form": "많이는 안했어요 노래정도?",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100011409.1.13",
          "form": "싸이가 부활한다면 전보단 더 할지도모르겠어요",
          "original_form": "싸이가 부활한다면 전보단 더 할지도모르겠어요",
          "speaker_id": "1",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100011409.1.14",
          "form": "아~정말요?ㅎㅎ 옛날 감성이 살아날지 모르겠네욯ㅎ",
          "original_form": "아~정말요?ㅎㅎ 옛날 감성이 살아날지 모르겠네욯ㅎ",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100011409.1.15",
          "form": "인스타 등이 워낙 자리잡아서 차라리 갬성으로 다가가면 좀 나으려나요 ㅎㅎ",
          "original_form": "인스타 등이 워낙 자리잡아서 차라리 갬성으로 다가가면 좀 나으려나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100011409.1.16",
          "form": "그렇겠네요~ㅎㅎ 요즘 세대들은 어떻게 받아들일지 궁금하네요",
          "original_form": "그렇겠네요~ㅎㅎ 요즘 세대들은 어떻게 받아들일지 궁금하네요",
          "speaker_id": "2",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100011409.1.17",
          "form": "워낙 빠른걸 원하는 풍토라 저도 궁금하네요",
          "original_form": "워낙 빠른걸 원하는 풍토라 저도 궁금하네요",
          "speaker_id": "1",
          "time": "20210826 13:30"
        }
      ]
