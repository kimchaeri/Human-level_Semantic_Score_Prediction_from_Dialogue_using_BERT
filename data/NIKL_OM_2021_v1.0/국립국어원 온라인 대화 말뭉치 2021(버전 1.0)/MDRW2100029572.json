      "utterance": [
        {
          "id": "MDRW2100029572.1.1",
          "form": "커피 좋아하세요?^^",
          "original_form": "커피 좋아하세요?^^",
          "speaker_id": "1",
          "time": "20210903 10:28"
        },
        {
          "id": "MDRW2100029572.1.2",
          "form": "커피 싫어하진 않아요",
          "original_form": "커피 싫어하진 않아요",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100029572.1.3",
          "form": "저는 향 때문에 커피를 좋아하는데",
          "original_form": "저는 향 때문에 커피를 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 12:55"
        },
        {
          "id": "MDRW2100029572.1.4",
          "form": "카페인때문에 거의 마시지를 못한답니다",
          "original_form": "카페인때문에 거의 마시지를 못한답니다",
          "speaker_id": "1",
          "time": "20210903 12:55"
        },
        {
          "id": "MDRW2100029572.1.5",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "2",
          "time": "20210903 12:55"
        },
        {
          "id": "MDRW2100029572.1.6",
          "form": "저는 향도 좋아하긴 하는데",
          "original_form": "저는 향도 좋아하긴 하는데",
          "speaker_id": "2",
          "time": "20210903 12:55"
        },
        {
          "id": "MDRW2100029572.1.7",
          "form": "주로 카페인 섭취가 목적인 경우가 많아서...",
          "original_form": "주로 카페인 섭취가 목적인 경우가 많아서...",
          "speaker_id": "2",
          "time": "20210903 12:55"
        },
        {
          "id": "MDRW2100029572.1.8",
          "form": "카페인 먹으면 잠을 못자요",
          "original_form": "카페인 먹으면 잠을 못자요",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100029572.1.9",
          "form": "그래서 어쩌다가 오전 중에 한잔 정도 마시고 있어요",
          "original_form": "그래서 어쩌다가 오전 중에 한잔 정도 마시고 있어요",
          "speaker_id": "1",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100029572.1.10",
          "form": "아아...",
          "original_form": "아아...",
          "speaker_id": "2",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100029572.1.11",
          "form": "기호랑 체질이 안 맞는 안타까운 상황이네요...",
          "original_form": "기호랑 체질이 안 맞는 안타까운 상황이네요...",
          "speaker_id": "2",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100029572.1.12",
          "form": "네.. 참 아쉬운 상황이죠...",
          "original_form": "네.. 참 아쉬운 상황이죠...",
          "speaker_id": "1",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100029572.1.13",
          "form": "그럼 name2님은 커피맛과 향보다는",
          "original_form": "그럼 &name2&님은 커피맛과 향보다는",
          "speaker_id": "1",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100029572.1.14",
          "form": "카페인 때문에 마시는 건가요?",
          "original_form": "카페인 때문에 마시는 건가요?",
          "speaker_id": "1",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100029572.1.15",
          "form": "그...렇다보니",
          "original_form": "그...렇다보니",
          "speaker_id": "2",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100029572.1.16",
          "form": "몬스터를 많이 마셔요...",
          "original_form": "몬스터를 많이 마셔요...",
          "speaker_id": "2",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100029572.1.17",
          "form": "그러네요.. 전 카페인에 약하니까 가능한 카페인 없는 음료 마셔서 카페인 음료 종류를 잘 몰라요",
          "original_form": "그러네요.. 전 카페인에 약하니까 가능한 카페인 없는 음료 마셔서 카페인 음료 종류를 잘 몰라요",
          "speaker_id": "1",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029572.1.18",
          "form": "그럼 그냥 차나 주스 종류도 별로인가요?",
          "original_form": "그럼 그냥 차나 주스 종류도 별로인가요?",
          "speaker_id": "1",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029572.1.19",
          "form": "별로는 아니고",
          "original_form": "별로는 아니고",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029572.1.20",
          "form": "전반적으로 싫어하는 음료가 없는 편입니다",
          "original_form": "전반적으로 싫어하는 음료가 없는 편입니다",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029572.1.21",
          "form": "아 지코 빼고...",
          "original_form": "아 지코 빼고...",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029572.1.22",
          "form": "지코는 뭐죠? 특이한 맛인가요?",
          "original_form": "지코는 뭐죠? 특이한 맛인가요?",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029572.1.23",
          "form": "그... 코코넛 음료 인데",
          "original_form": "그... 코코넛 음료 인데",
          "speaker_id": "2",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100029572.1.24",
          "form": "악명이 좀 있죠...",
          "original_form": "악명이 좀 있죠...",
          "speaker_id": "2",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100029572.1.25",
          "form": "아.. 코코넛... 그렇군ㅇ요",
          "original_form": "아.. 코코넛... 그렇군ㅇ요",
