        },
        {
          "id": "MDRW2100041407.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041407.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041407.1.4",
          "form": "네네 치킨이랑 통닭요 님은 ?",
          "original_form": "네네 치킨이랑 통닭요 님은 ?",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041407.1.5",
          "form": "저는 찜닭이요",
          "original_form": "저는 찜닭이요",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041407.1.6",
          "form": "어떤 브랜드 치킨이랑 통닭을 드셨나요?",
          "original_form": "어떤 브랜드 치킨이랑 통닭을 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041407.1.7",
          "form": "오 닭드셨네요",
          "original_form": "오 닭드셨네요",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041407.1.8",
          "form": "어머 통닭이랑",
          "original_form": "어머 통닭이랑",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041407.1.9",
          "form": "피자 먹었어요 ㅋㅋㅋㅋ",
          "original_form": "피자 먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041407.1.10",
          "form": "어떤브랜드 피자 드셨나요?",
          "original_form": "어떤브랜드 피자 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041407.1.11",
          "form": "도미노요 !",
          "original_form": "도미노요 !",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041407.1.12",
          "form": "좋아하는 브랜드있나요?",
          "original_form": "좋아하는 브랜드있나요?",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041407.1.13",
          "form": "저는 반올림이요",
          "original_form": "저는 반올림이요",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041407.1.14",
          "form": "도미노 어떤 피자 드셨나요?",
          "original_form": "도미노 어떤 피자 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041407.1.15",
          "form": "오늘은 기프티콘이 포테이토있어서 그거용",
          "original_form": "오늘은 기프티콘이 포테이토있어서 그거용",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100041407.1.16",
          "form": "올만애 먹으니 맛나더라구요",
          "original_form": "올만애 먹으니 맛나더라구요",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100041407.1.17",
          "form": "그럼 도미노 어떤 피자 좋아하시나요?",
          "original_form": "그럼 도미노 어떤 피자 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100041407.1.18",
          "form": "원래는 그 새우들어간거 블랙타이거인가 그거좋아해용",
          "original_form": "원래는 그 새우들어간거 블랙타이거인가 그거좋아해용",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100041407.1.19",
          "form": "반올림 맛나나요? 안먹어봣어요!",
          "original_form": "반올림 맛나나요? 안먹어봣어요!",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100041407.1.20",
          "form": "네 자극적이어서 맛있어요!!",
          "original_form": "네 자극적이어서 맛있어요!!",
          "speaker_id": "1",
          "time": "20210905 23:14"
        }
      ]
