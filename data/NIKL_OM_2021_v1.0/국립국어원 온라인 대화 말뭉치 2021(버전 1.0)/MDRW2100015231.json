        {
          "id": "MDRW2100015231.1.1",
          "form": "스포츠 좋아하새요?",
          "original_form": "스포츠 좋아하새요?",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100015231.1.2",
          "form": "네 님은요?",
          "original_form": "네 님은요?",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100015231.1.3",
          "form": "좋아하는 편이에요",
          "original_form": "좋아하는 편이에요",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100015231.1.4",
          "form": "올림픽 보셧나요?",
          "original_form": "올림픽 보셧나요?",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100015231.1.5",
          "form": "네 봤어요!",
          "original_form": "네 봤어요!",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100015231.1.6",
          "form": "특히 배구!?",
          "original_form": "특히 배구!?",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100015231.1.7",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100015231.1.8",
          "form": "헐 저도 배구봤어요",
          "original_form": "헐 저도 배구봤어요",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100015231.1.9",
          "form": "너무 멋있죠",
          "original_form": "너무 멋있죠",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100015231.1.10",
          "form": "맞아요 특히 김연경ㅜ",
          "original_form": "맞아요 특히 김연경ㅜ",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100015231.1.11",
          "form": "ㅠㅠ 진짜 너무 멋있어요",
          "original_form": "ㅠㅠ 진짜 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100015231.1.12",
          "form": "식빵언니..",
          "original_form": "식빵언니..",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100015231.1.13",
          "form": "직관가보셨나요?",
          "original_form": "직관가보셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100015231.1.14",
          "form": "아니요 ㅠㅠ 가보고싶네요",
          "original_form": "아니요 ㅠㅠ 가보고싶네요",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100015231.1.15",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100015231.1.16",
          "form": "저도요 가보고싶네요",
          "original_form": "저도요 가보고싶네요",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100015231.1.17",
          "form": "연경언니는 보러갈수도 없고 ㅠㅠ 아쉽네요",
          "original_form": "연경언니는 보러갈수도 없고 ㅠㅠ 아쉽네요",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100015231.1.18",
          "form": "올림픽 양궁 보셨나요?",
          "original_form": "올림픽 양궁 보셨나요?",
