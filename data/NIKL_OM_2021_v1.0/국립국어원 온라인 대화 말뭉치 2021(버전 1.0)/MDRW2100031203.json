      "utterance": [
        {
          "id": "MDRW2100031203.1.1",
          "form": "저희 고등학교 지학 선생님이 아주 유명한분이셔서",
          "original_form": "저희 고등학교 지학 선생님이 아주 유명한분이셔서",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031203.1.2",
          "form": "아 진짜요",
          "original_form": "아 진짜요",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031203.1.3",
          "form": "게다가 저희 담임이셔서 ㅋㅋㅋㄱ",
          "original_form": "게다가 저희 담임이셔서 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031203.1.4",
          "form": "수능 출제위원이셨어요",
          "original_form": "수능 출제위원이셨어요",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031203.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031203.1.6",
          "form": "그럼 수능 전에 사라지시잖아요",
          "original_form": "그럼 수능 전에 사라지시잖아요",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031203.1.7",
          "form": "교과서대신 본인이 만든 교재로",
          "original_form": "교과서대신 본인이 만든 교재로",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031203.1.8",
          "form": "수업을 하셨던",
          "original_form": "수업을 하셨던",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031203.1.9",
          "form": "사라지죠",
          "original_form": "사라지죠",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031203.1.10",
          "form": "캬 멋지시다",
          "original_form": "캬 멋지시다",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031203.1.11",
          "form": "애들데리고 같이 별보러가자고 하고",
          "original_form": "애들데리고 같이 별보러가자고 하고",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031203.1.12",
          "form": "고등학교는 그런 전문성이 멋져요",
          "original_form": "고등학교는 그런 전문성이 멋져요",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031203.1.13",
          "form": "별사진 찍으러가자고 하고",
          "original_form": "별사진 찍으러가자고 하고",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031203.1.14",
          "form": "와 찍으러가셧나요",
          "original_form": "와 찍으러가셧나요",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031203.1.15",
          "form": "네 갔었어요",
          "original_form": "네 갔었어요",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031203.1.16",
          "form": "공부하기 시러서 ㅋㅋㅋㅋㅋ",
          "original_form": "공부하기 시러서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031203.1.17",
          "form": "더 큰 경험하고 오셨네요ㅋㅋㅋㅋ",
          "original_form": "더 큰 경험하고 오셨네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031203.1.18",
          "form": "그때가 그립네요 ㅋㅋㅋㅋ",
          "original_form": "그때가 그립네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031203.1.19",
          "form": "순수했던? 그때 ㅋㅋㅋㅋ",
          "original_form": "순수했던? 그때 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031203.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031203.1.21",
          "form": "최근에 별보러 가신적 잇나요",
          "original_form": "최근에 별보러 가신적 잇나요",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031203.1.22",
          "form": "가고싶은데 여친이 그런 취미가 없고",
          "original_form": "가고싶은데 여친이 그런 취미가 없고",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031203.1.23",
          "form": "요즘은 별보러 많이 가시던데",
          "original_form": "요즘은 별보러 많이 가시던데",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031203.1.24",
          "form": "아 진짜여",
          "original_form": "아 진짜여",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031203.1.25",
          "form": "잠이 많아서",
          "original_form": "잠이 많아서",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031203.1.26",
          "form": "아마 가다가 잘껄요?",
          "original_form": "아마 가다가 잘껄요?",
          "speaker_id": "2",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031203.1.27",
          "form": "아ㅋㅋㅋ 안되겠네요",
          "original_form": "아ㅋㅋㅋ 안되겠네요",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031203.1.28",
          "form": "저도 별보러 가는 데이트도 약간 로망인데",
          "original_form": "저도 별보러 가는 데이트도 약간 로망인데",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031203.1.29",
          "form": "코로나 전 1월에 일출도 간신히 보러간...",
          "original_form": "코로나 전 1월에 일출도 간신히 보러간...",
          "speaker_id": "2",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031203.1.30",
          "form": "남친이 열시반에 자는 사람이라 안되겠어요",
          "original_form": "남친이 열시반에 자는 사람이라 안되겠어요",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031203.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031203.1.32",
          "form": "저는 잠이 많이 없는데",
          "original_form": "저는 잠이 많이 없는데",
          "speaker_id": "2",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031203.1.33",
          "form": "여친은 잠이 진짜 많아서",
          "original_form": "여친은 잠이 진짜 많아서",
          "speaker_id": "2",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031203.1.34",
          "form": "제로망이 여친하고 해외여행가서",
          "original_form": "제로망이 여친하고 해외여행가서",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.35",
          "form": "오로라보는거였는데",
          "original_form": "오로라보는거였는데",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.36",
          "form": "저두 잠 별로 없고 남친은 무슨 열시간 넘게 자더라구오",
          "original_form": "저두 잠 별로 없고 남친은 무슨 열시간 넘게 자더라구오",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.37",
          "form": "어?",
          "original_form": "어?",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.38",
          "form": "오로라!! 너무 좋다",
          "original_form": "오로라!! 너무 좋다",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.39",
          "form": "제여친이 그래요",
          "original_form": "제여친이 그래요",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.40",
          "form": "겁나 자요",
          "original_form": "겁나 자요",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.41",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.42",
          "form": "제가 나무늘보라고 놀림",
          "original_form": "제가 나무늘보라고 놀림",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.43",
          "form": "저는 잠자는 숲속의 왕자라고 놀리는데",
          "original_form": "저는 잠자는 숲속의 왕자라고 놀리는데",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031203.1.44",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031203.1.45",
          "form": "작명봐",
          "original_form": "작명봐",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031203.1.46",
          "form": "왕자....",
          "original_form": "왕자....",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031203.1.47",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031203.1.48",
          "form": "저는 나무늘보",
          "original_form": "저는 나무늘보",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031203.1.49",
          "form": "name1님 왕자",
          "original_form": "&name1&님 왕자",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031203.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031203.1.51",
          "form": "귀여우니까 코알라라고 해줘야겠네요",
          "original_form": "귀여우니까 코알라라고 해줘야겠네요",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031203.1.52",
          "form": "5년차와 2주일의 차이 아니겠습니까",
          "original_form": "5년차와 2주일의 차이 아니겠습니까",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031203.1.53",
          "form": "갭이 크네요",
          "original_form": "갭이 크네요",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031203.1.54",
          "form": "여친이 깨서 이따올게요 ㅎㅎㅎ",
          "original_form": "여친이 깨서 이따올게요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031203.1.55",
          "form": "나무늘보 기상",
          "original_form": "나무늘보 기상",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031203.1.56",
          "form": "넹넹 즐데~~",
          "original_form": "넹넹 즐데~~",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031203.1.57",
          "form": "데이트 끝",
          "original_form": "데이트 끝",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031203.1.58",
          "form": "운전해서 집에오니 지금이네요",
          "original_form": "운전해서 집에오니 지금이네요",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031203.1.59",
          "form": "몸은 괜찮으세요?",
          "original_form": "몸은 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031203.1.60",
          "form": "넹 저는 팔말고는 아직 아픈게 없네요 체온도 정상이고ㅋㅋ",
          "original_form": "넹 저는 팔말고는 아직 아픈게 없네요 체온도 정상이고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100031203.1.61",
          "form": "근데 24시간 이후에 증상 나타나신 분도 있길래 계속 지켜는 봐야죠",
          "original_form": "근데 24시간 이후에 증상 나타나신 분도 있길래 계속 지켜는 봐야죠",
          "speaker_id": "1",
          "time": "20210905 07:06"
        },
        {
          "id": "MDRW2100031203.1.62",
          "form": "오 그럼 괜찮으신거 아니에요?",
          "original_form": "오 그럼 괜찮으신거 아니에요?",
          "speaker_id": "2",
          "time": "20210905 08:23"
        },
        {
          "id": "MDRW2100031203.1.63",
          "form": "거의 하루 다되가는데",
          "original_form": "거의 하루 다되가는데",
          "speaker_id": "2",
          "time": "20210905 08:24"
        },
        {
          "id": "MDRW2100031203.1.64",
          "form": "근데 하루, 이틀 지나서 아프기시작한 사람들도 있기느하더라구요!",
          "original_form": "근데 하루, 이틀 지나서 아프기시작한 사람들도 있기느하더라구요!",
          "speaker_id": "1",
          "time": "20210905 08:26"
        },
        {
