      "utterance": [
        {
          "id": "MDRW2100011421.1.1",
          "form": "name3언니는 그병원에 취직안하는가??",
          "original_form": "&name3&언니는 그병원에 취직안하는가??",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011421.1.2",
          "form": "취직 할꺼 같아요 ㅋㅋㅋㅋ",
          "original_form": "취직 할꺼 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011421.1.3",
          "form": "요즘 저렇게 일해주는 사람 암만 찾아도 없음 ㅋㅋㅋㅋ",
          "original_form": "요즘 저렇게 일해주는 사람 암만 찾아도 없음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011421.1.4",
          "form": "우짜든동 취직 시킬꺼 같아요^^",
          "original_form": "우짜든동 취직 시킬꺼 같아요^^",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011421.1.5",
          "form": "그래\r\nㅋㅋㅋㅋㅋㅋㄱ 언니정도면",
          "original_form": "그래\r\nㅋㅋㅋㅋㅋㅋㄱ 언니정도면",
          "speaker_id": "2",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011421.1.6",
          "form": "제발 오라고 할듯 ㅎㅎ",
          "original_form": "제발 오라고 할듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011421.1.7",
          "form": "언니 나가고 다른사람 오면 속터질듯요 ㅋㅋㅋ",
          "original_form": "언니 나가고 다른사람 오면 속터질듯요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011421.1.8",
          "form": "실습 갈때 젤 피해야되는 인물 ㅋㅋㅋㅋㅋ",
          "original_form": "실습 갈때 젤 피해야되는 인물 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011421.1.9",
          "form": "같이하면 뭘해도 티 안남 ㅋㅋ",
          "original_form": "같이하면 뭘해도 티 안남 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011421.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그러이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그러이",
          "speaker_id": "2",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011421.1.11",
          "form": "나도 처음에 일할때",
          "original_form": "나도 처음에 일할때",
          "speaker_id": "2",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011421.1.12",
          "form": "내앞에 선배가 매일 빨간고무장갑끼고",
          "original_form": "내앞에 선배가 매일 빨간고무장갑끼고",
          "speaker_id": "2",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011421.1.13",
          "form": "남자화장실 변기 닦고",
          "original_form": "남자화장실 변기 닦고",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011421.1.14",
          "form": "밥먹으러가면 수저 놔주는거는 기본이고 어른들 신발까지 정리해줘가지고",
          "original_form": "밥먹으러가면 수저 놔주는거는 기본이고 어른들 신발까지 정리해줘가지고",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011421.1.15",
          "form": "정말 힘들었음",
          "original_form": "정말 힘들었음",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011421.1.16",
          "form": "아..맞아요~그게 내 밑으로 들어오는 사람이냐",
          "original_form": "아..맞아요~그게 내 밑으로 들어오는 사람이냐",
          "speaker_id": "1",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011421.1.17",
          "form": "동기냐에 따라 다름 ㅋㅋㅋㅋ",
          "original_form": "동기냐에 따라 다름 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011421.1.18",
          "form": "고무장갑 ㅋㅋㅋㅋㅋ",
          "original_form": "고무장갑 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011421.1.19",
          "form": "숟가락 던졌음 던졌지 ㅋㅋㅋㅋㅋ",
          "original_form": "숟가락 던졌음 던졌지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011421.1.20",
          "form": "그래 근무화 정리해주는거는",
          "original_form": "그래 근무화 정리해주는거는",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011421.1.21",
          "form": "좀 그렇지않나....",
          "original_form": "좀 그렇지않나....",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011421.1.22",
          "form": "변기라니....",
          "original_form": "변기라니....",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011421.1.23",
          "form": "그건..쫌..",
          "original_form": "그건..쫌..",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100011421.1.24",
          "form": "그런사람들이 집에서 아무것도 안함 ㅋㅋㅋㅋ",
          "original_form": "그런사람들이 집에서 아무것도 안함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100011421.1.25",
          "form": "내 가족도 아니고..신발을??",
          "original_form": "내 가족도 아니고..신발을??",
          "speaker_id": "1",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100011421.1.26",
          "form": "그러니",
          "original_form": "그러니",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100011421.1.27",
          "form": "여튼 어른들은 정말 그 언니를 좋아했지만",
          "original_form": "여튼 어른들은 정말 그 언니를 좋아했지만",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100011421.1.28",
          "form": "젊은이들은 굉장히 피곤했음",
          "original_form": "젊은이들은 굉장히 피곤했음",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100011421.1.29",
          "form": "그뿐만이 아님",
          "original_form": "그뿐만이 아님",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100011421.1.30",
          "form": "맞아요~그런사람 한번왔다가면 피곤함 ㅋㅋㅋ",
          "original_form": "맞아요~그런사람 한번왔다가면 피곤함 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100011421.1.31",
          "form": "식당 정해서 밥먹었는데",
          "original_form": "식당 정해서 밥먹었는데",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.32",
          "form": "그냥 앉아서 기다리면 되는데",
          "original_form": "그냥 앉아서 기다리면 되는데",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.33",
          "form": "또 뭘하던데요??ㅋㅋ",
          "original_form": "또 뭘하던데요??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.34",
          "form": "꼭 주방에 가서 반찬 쟁반에 담고 날라줌",
          "original_form": "꼭 주방에 가서 반찬 쟁반에 담고 날라줌",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.35",
          "form": "미침....",
          "original_form": "미침....",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.36",
          "form": "아...근데..",
          "original_form": "아...근데..",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.37",
          "form": "아줌마가 내오니까 무슨 주방 보조 부르듯이",
          "original_form": "아줌마가 내오니까 무슨 주방 보조 부르듯이",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.38",
          "form": "내가 상사라면 늠 부담스러운데..",
          "original_form": "내가 상사라면 늠 부담스러운데..",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.39",
          "form": "일로와보라고",
          "original_form": "일로와보라고",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.40",
          "form": "좋을듯요 ㅋㅋㅋ",
          "original_form": "좋을듯요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.41",
          "form": "왜 부르나했는데 쟁반 나르라고...",
          "original_form": "왜 부르나했는데 쟁반 나르라고...",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.42",
          "form": "그사람이 하니깐..",
          "original_form": "그사람이 하니깐..",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011421.1.43",
          "form": "진짜 처음 질을 잘들여야됨..",
          "original_form": "진짜 처음 질을 잘들여야됨..",
          "speaker_id": "1",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011421.1.44",
          "form": "어 그언니가 했으니까 나도 하라고",
          "original_form": "어 그언니가 했으니까 나도 하라고",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011421.1.45",
          "form": "짱나서 하다 치움",
          "original_form": "짱나서 하다 치움",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011421.1.46",
          "form": "뭐지,,내돈주고 밥묵는데ㅡㅡ",
          "original_form": "뭐지,,내돈주고 밥묵는데ㅡㅡ",
          "speaker_id": "1",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011421.1.47",
          "form": "22살 반항기 심한 나이에",
          "original_form": "22살 반항기 심한 나이에",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011421.1.48",
          "form": "쟁반 나르라캐가 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "쟁반 나르라캐가 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011421.1.49",
          "form": "ㅋㅋㅋㅋㅋ그쵸~쟁반 던져뿌져 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그쵸~쟁반 던져뿌져 ㅋㅋㅋㅋ",
