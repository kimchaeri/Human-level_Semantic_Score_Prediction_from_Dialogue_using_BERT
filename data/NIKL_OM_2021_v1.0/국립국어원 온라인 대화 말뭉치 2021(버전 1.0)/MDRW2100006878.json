        {
          "id": "MDRW2100006878.1.1",
          "form": "튀김 싫어하는 사람이 있을까요",
          "original_form": "튀김 싫어하는 사람이 있을까요",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006878.1.2",
          "form": "아마 없겠지요?ㅎㅎ 통닭이 제일이지요",
          "original_form": "아마 없겠지요?ㅎㅎ 통닭이 제일이지요",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006878.1.3",
          "form": "집에서 튀기긴번거롭죠",
          "original_form": "집에서 튀기긴번거롭죠",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006878.1.4",
          "form": "추석엔 튀김하는 사람들 많아요",
          "original_form": "추석엔 튀김하는 사람들 많아요",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006878.1.5",
          "form": "제주도는 추석에 하진않아요",
          "original_form": "제주도는 추석에 하진않아요",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006878.1.6",
          "form": "그래요? 부산은 튀김과 전이 필수거든요",
          "original_form": "그래요? 부산은 튀김과 전이 필수거든요",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006878.1.7",
          "form": "어떤 튀김해요",
          "original_form": "어떤 튀김해요",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006878.1.8",
          "form": "오징어랑 고구마는 기본이에요",
          "original_form": "오징어랑 고구마는 기본이에요",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006878.1.9",
          "form": "쥐포도 튀기죠",
          "original_form": "쥐포도 튀기죠",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006878.1.10",
          "form": "네 간편해서 좋아요 바로 튀겨져서요",
          "original_form": "네 간편해서 좋아요 바로 튀겨져서요",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006878.1.11",
          "form": "저는 음.. 새우튀김",
          "original_form": "저는 음.. 새우튀김",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006878.1.12",
          "form": "그건 진짜 없어서 못먹죠",
          "original_form": "그건 진짜 없어서 못먹죠",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006878.1.13",
          "form": "계란이나 순대 튀겨도좋죠",
          "original_form": "계란이나 순대 튀겨도좋죠",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006878.1.14",
          "form": "빙어 튀김 드셔보셨나요",
          "original_form": "빙어 튀김 드셔보셨나요",
          "speaker_id": "1",
