        },
        {
          "id": "MDRW2100010301.1.3",
          "form": "날이 많이 추워졌네요",
          "original_form": "날이 많이 추워졌네요",
          "speaker_id": "1",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100010301.1.4",
          "form": "그렇죠 많이 추울거 같아요",
          "original_form": "그렇죠 많이 추울거 같아요",
          "speaker_id": "2",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010301.1.5",
          "form": "날씨가",
          "original_form": "날씨가",
          "speaker_id": "1",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100010301.1.6",
          "form": "십도나 내려갔어요ㅎㅎㅎ",
          "original_form": "십도나 내려갔어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:45"
        },
        {
          "id": "MDRW2100010301.1.7",
          "form": "그런지 많이 쌀쌀합니다",
          "original_form": "그런지 많이 쌀쌀합니다",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010301.1.8",
          "form": "비가 오다가 그쳤어요",
          "original_form": "비가 오다가 그쳤어요",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010301.1.9",
          "form": "거기는 비가 오나요?",
          "original_form": "거기는 비가 오나요?",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010301.1.10",
          "form": "지금 비 와요",
          "original_form": "지금 비 와요",
          "speaker_id": "2",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010301.1.11",
          "form": "여기는 그쳤네요",
          "original_form": "여기는 그쳤네요",
          "speaker_id": "1",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010301.1.12",
          "form": "비 오락가락 하네요",
          "original_form": "비 오락가락 하네요",
          "speaker_id": "2",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010301.1.13",
          "form": "파전에 막걸리가",
          "original_form": "파전에 막걸리가",
          "speaker_id": "1",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100010301.1.14",
          "form": "땡기네요",
          "original_form": "땡기네요",
          "speaker_id": "1",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100010301.1.15",
          "form": "비 오는 날에는 파전 이죠",
          "original_form": "비 오는 날에는 파전 이죠",
          "speaker_id": "2",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010301.1.16",
          "form": "술 잘드시나요?",
