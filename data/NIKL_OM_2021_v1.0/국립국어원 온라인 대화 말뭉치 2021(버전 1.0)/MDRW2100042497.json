      "utterance": [
        {
          "id": "MDRW2100042497.1.1",
          "form": "무슨 얘기 하고 있었지",
          "original_form": "무슨 얘기 하고 있었지",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042497.1.2",
          "form": "낙곱새?",
          "original_form": "낙곱새?",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042497.1.3",
          "form": "맞아 곱창얘기였다",
          "original_form": "맞아 곱창얘기였다",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042497.1.4",
          "form": "아무튼 양념 소스 만드실때는 시판 고추장 쓰시는게 나은거같아요",
          "original_form": "아무튼 양념 소스 만드실때는 시판 고추장 쓰시는게 나은거같아요",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042497.1.5",
          "form": "조막만한거 하나 사두지 뭐",
          "original_form": "조막만한거 하나 사두지 뭐",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042497.1.6",
          "form": "어쨌든 그거 인기있어서 저희 근처에도 생겼으니까",
          "original_form": "어쨌든 그거 인기있어서 저희 근처에도 생겼으니까",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042497.1.7",
          "form": "같이 먹어요 다음에",
          "original_form": "같이 먹어요 다음에",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042497.1.8",
          "form": "거기 근처면 가야하잖아",
          "original_form": "거기 근처면 가야하잖아",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042497.1.9",
          "form": "아 ㅎ 저희 근처가 고향 근처란 얘기에요",
          "original_form": "아 ㅎ 저희 근처가 고향 근처란 얘기에요",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042497.1.10",
          "form": "그래 위치도 알아놔",
          "original_form": "그래 위치도 알아놔",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042497.1.11",
          "form": "아빠랑 같이 가게",
          "original_form": "아빠랑 같이 가게",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042497.1.12",
          "form": "근데 거기 맛있을지모르겠네요 안먹어봐서",
          "original_form": "근데 거기 맛있을지모르겠네요 안먹어봐서",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042497.1.13",
          "form": "원래 새로운 맛집은 개척하는거다",
          "original_form": "원래 새로운 맛집은 개척하는거다",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042497.1.14",
          "form": "그건 그래요 ㅋ 코로나때문에 다 망하고 새로 생긴것들만 잔뜩이라",
          "original_form": "그건 그래요 ㅋ 코로나때문에 다 망하고 새로 생긴것들만 잔뜩이라",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042497.1.15",
          "form": "고르는게 힘드네요",
          "original_form": "고르는게 힘드네요",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042497.1.16",
          "form": "엄마 친구도 장사했는데",
          "original_form": "엄마 친구도 장사했는데",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042497.1.17",
          "form": "힘들어서 가게 내놨대",
          "original_form": "힘들어서 가게 내놨대",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042497.1.18",
          "form": "ㅠㅠ 안타깝네요",
          "original_form": "ㅠㅠ 안타깝네요",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042497.1.19",
          "form": "고모도 식당하시는데 얘기 못들었네",
          "original_form": "고모도 식당하시는데 얘기 못들었네",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042497.1.20",
          "form": "잘 하구 계셔서 그런게 아닐까요 그랬음 좋겠다",
          "original_form": "잘 하구 계셔서 그런게 아닐까요 그랬음 좋겠다",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042497.1.21",
          "form": "맞아 많이 팔아드려야하는데 멀어서",
          "original_form": "맞아 많이 팔아드려야하는데 멀어서",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042497.1.22",
          "form": "담에 올라가서 많이 해드려요 ㅈ ㅓ 많이 먹을수 있으니까 ㅎㅎ",
          "original_form": "담에 올라가서 많이 해드려요 ㅈ ㅓ 많이 먹을수 있으니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042497.1.23",
          "form": "우리 딸 살찐다",
          "original_form": "우리 딸 살찐다",
          "speaker_id": "2",
          "time": "20210906 22:39"
        }
      ]
    }
  ]
