      "utterance": [
        {
          "id": "MDRW2100029831.1.1",
          "form": "좋은 하루 보내",
          "original_form": "좋은 하루 보내",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029831.1.2",
          "form": "너라고 한거 죄송..... 십대인데 학교 안간대서 나보다 어린줄(;;;",
          "original_form": "너라고 한거 죄송..... 십대인데 학교 안간대서 나보다 어린줄(;;;",
          "speaker_id": "1",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100029831.1.3",
          "form": "ㅋㅋㅋ 아냐아냐",
          "original_form": "ㅋㅋㅋ 아냐아냐",
          "speaker_id": "2",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029831.1.4",
          "form": "요즘 많이 덥지",
          "original_form": "요즘 많이 덥지",
          "speaker_id": "2",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029831.1.5",
          "form": "응... 그래도 비오고나니까 좀 선선해진거같애",
          "original_form": "응... 그래도 비오고나니까 좀 선선해진거같애",
          "speaker_id": "1",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100029831.1.6",
          "form": "학교 에어컨은 틀어줘?",
          "original_form": "학교 에어컨은 틀어줘?",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029831.1.7",
          "form": "틀어줘",
          "original_form": "틀어줘",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029831.1.8",
          "form": "오히려 너무 추울지경ㄷㄷ 한여름에 가디건입고갔어",
          "original_form": "오히려 너무 추울지경ㄷㄷ 한여름에 가디건입고갔어",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029831.1.9",
          "form": "헉... ㄷㄷ",
          "original_form": "헉... ㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029831.1.10",
          "form": "환기 해? 코로나 때문에",
          "original_form": "환기 해? 코로나 때문에",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029831.1.11",
          "form": "환기하는데 애들이 덥다고 막 창문닫아ㅡㅡ",
          "original_form": "환기하는데 애들이 덥다고 막 창문닫아ㅡㅡ",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029831.1.12",
          "form": "난 얼어죽겠는데",
          "original_form": "난 얼어죽겠는데",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029831.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029831.1.14",
          "form": "코로나 핑계대면서 슬쩍 열어",
          "original_form": "코로나 핑계대면서 슬쩍 열어",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029831.1.15",
          "form": "무슨계절좋아해?",
          "original_form": "무슨계절좋아해?",
          "speaker_id": "1",
          "time": "20210903 15:58"
        },
        {
          "id": "MDRW2100029831.1.16",
          "form": "그래야겠다ㅜ",
          "original_form": "그래야겠다ㅜ",
          "speaker_id": "1",
          "time": "20210903 15:58"
        },
        {
          "id": "MDRW2100029831.1.17",
          "form": "나는 가을!",
          "original_form": "나는 가을!",
          "speaker_id": "2",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100029831.1.18",
          "form": "적당히 시원해서",
          "original_form": "적당히 시원해서",
          "speaker_id": "2",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100029831.1.19",
          "form": "지금 가을인데 딱좋겠네ㅋㅋ",
          "original_form": "지금 가을인데 딱좋겠네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029831.1.20",
          "form": "아직은 조금 덥고",
          "original_form": "아직은 조금 덥고",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029831.1.21",
          "form": "한 9월 중순쯤 되면 괜찮을듯",
          "original_form": "한 9월 중순쯤 되면 괜찮을듯",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029831.1.22",
          "form": "밖에 햇빛 쨍쨍해",
          "original_form": "밖에 햇빛 쨍쨍해",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029831.1.23",
          "form": "아 진짜? 여긴 되게 선선하던데",
          "original_form": "아 진짜? 여긴 되게 선선하던데",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029831.1.24",
          "form": "사람들 다 긴바지 긴팔입고다님",
          "original_form": "사람들 다 긴바지 긴팔입고다님",
