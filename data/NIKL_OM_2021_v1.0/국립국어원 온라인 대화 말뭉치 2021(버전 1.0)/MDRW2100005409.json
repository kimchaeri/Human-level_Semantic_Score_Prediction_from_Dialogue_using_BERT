      "utterance": [
        {
          "id": "MDRW2100005409.1.1",
          "form": "수강신청 했어?",
          "original_form": "수강신청 했어?",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005409.1.2",
          "form": "했지~ 나 15학점 듣는다ㅎㅎ",
          "original_form": "했지~ 나 15학점 듣는다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005409.1.3",
          "form": "그정도면 많이 듣는건가?",
          "original_form": "그정도면 많이 듣는건가?",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005409.1.4",
          "form": "내가 시간표가 짜여져 나와서 몇학점이 어떤지를 모르겠네ㅠㅠ",
          "original_form": "내가 시간표가 짜여져 나와서 몇학점이 어떤지를 모르겠네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005409.1.5",
          "form": "적게 듣는 거야!",
          "original_form": "적게 듣는 거야!",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005409.1.6",
          "form": "보통 18학점 많이 듣더라구",
          "original_form": "보통 18학점 많이 듣더라구",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005409.1.7",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005409.1.8",
          "form": "엥 그러면 졸업은 어떡행",
          "original_form": "엥 그러면 졸업은 어떡행",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005409.1.9",
          "form": "계절학기 들었나?",
          "original_form": "계절학기 들었나?",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005409.1.10",
          "form": "응 계절학기ㅋㅋㅋㅋ",
          "original_form": "응 계절학기ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005409.1.11",
          "form": "졸업학점 거의 다 채워서 괜찮아!",
          "original_form": "졸업학점 거의 다 채워서 괜찮아!",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005409.1.12",
          "form": "우와 여유롭겠다 부러워!",
          "original_form": "우와 여유롭겠다 부러워!",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005409.1.13",
          "form": "공강 있어?",
          "original_form": "공강 있어?",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005409.1.14",
          "form": "아니 공강은 없는데 비대면이라서 공강인 날도 생길 것 같아!",
          "original_form": "아니 공강은 없는데 비대면이라서 공강인 날도 생길 것 같아!",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005409.1.15",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005409.1.16",
          "form": "우와 그렇구나",
          "original_form": "우와 그렇구나",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005409.1.17",
          "form": "나는 학점이 많은 학과라 공강 없어ㅠㅠ",
          "original_form": "나는 학점이 많은 학과라 공강 없어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005409.1.18",
          "form": "근데 11월 부터 대면이야!",
          "original_form": "근데 11월 부터 대면이야!",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005409.1.19",
          "form": "글쿠나 나는 아직 정확한 공지는 안 떠서..",
          "original_form": "글쿠나 나는 아직 정확한 공지는 안 떠서..",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005409.1.20",
          "form": "어떻게 될지는 모르겠다..",
          "original_form": "어떻게 될지는 모르겠다..",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005409.1.21",
          "form": "비대면 좋지 않아?",
          "original_form": "비대면 좋지 않아?",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005409.1.22",
          "form": "나는 그때 일할 수 있어서 좋더라구 ㅎㅎ",
          "original_form": "나는 그때 일할 수 있어서 좋더라구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005409.1.23",
          "form": "나도 좋지ㅋㅋㅋㅋㄱ 근데 정확한 일정을 알려줬음 좋겠어",
          "original_form": "나도 좋지ㅋㅋㅋㅋㄱ 근데 정확한 일정을 알려줬음 좋겠어",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005409.1.24",
          "form": "스케줄 변동 때문에 스트레스 받는 건 싫어서ㅠ",
          "original_form": "스케줄 변동 때문에 스트레스 받는 건 싫어서ㅠ",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005409.1.25",
          "form": "맞아 너무 갑작스럽게 공지하면 좀 당황스러워",
          "original_form": "맞아 너무 갑작스럽게 공지하면 좀 당황스러워",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005409.1.26",
          "form": "완전 맞는 말인 것 같아",
          "original_form": "완전 맞는 말인 것 같아",
          "speaker_id": "1",
          "time": "20210821 22:26"
        }
      ]
    }
  ]
