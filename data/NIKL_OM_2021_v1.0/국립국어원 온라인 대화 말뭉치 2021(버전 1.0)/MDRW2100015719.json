      "utterance": [
        {
          "id": "MDRW2100015719.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015719.1.2",
          "form": "name2님은 넷플릭스나 왓챠 보시나요?",
          "original_form": "&name2&님은 넷플릭스나 왓챠 보시나요?",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015719.1.3",
          "form": "네 저는 넷플릭스랑 티빙 봐요~",
          "original_form": "네 저는 넷플릭스랑 티빙 봐요~",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015719.1.4",
          "form": "오 저 넷플릭스랑 왓챠랑 고민중인데 혹시 넷플릭스 가장 좋았던 점이 뭐예요?",
          "original_form": "오 저 넷플릭스랑 왓챠랑 고민중인데 혹시 넷플릭스 가장 좋았던 점이 뭐예요?",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015719.1.5",
          "form": "다양한 컨텐츠랑 오리지널 시리즈, 오프라인 저장이요",
          "original_form": "다양한 컨텐츠랑 오리지널 시리즈, 오프라인 저장이요",
          "speaker_id": "2",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015719.1.6",
          "form": "넷플릭스 쓰는 친구들이 더 많기도 해요",
          "original_form": "넷플릭스 쓰는 친구들이 더 많기도 해요",
          "speaker_id": "2",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015719.1.7",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210826 00:43"
        },
        {
          "id": "MDRW2100015719.1.8",
          "form": "주변에 다 넷플릭스로",
          "original_form": "주변에 다 넷플릭스로",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015719.1.9",
          "form": "하라고 그러더라구요",
          "original_form": "하라고 그러더라구요",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015719.1.10",
          "form": "혹시 추천 영화 있으신가요??",
          "original_form": "혹시 추천 영화 있으신가요??",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015719.1.11",
          "form": "넷플릭스 아니더라도",
          "original_form": "넷플릭스 아니더라도",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015719.1.12",
          "form": "인생 영화로 꼽는 영황ㅅ",
          "original_form": "인생 영화로 꼽는 영황ㅅ",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015719.1.13",
          "form": "화요",
          "original_form": "화요",
          "speaker_id": "1",
          "time": "20210826 00:44"
        },
        {
          "id": "MDRW2100015719.1.14",
          "form": "저는 블록버스터 좋아해서 마블 시리즈 재미있게 봤어요.",
          "original_form": "저는 블록버스터 좋아해서 마블 시리즈 재미있게 봤어요.",
          "speaker_id": "2",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015719.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015719.1.16",
          "form": "저는 최근에 업 다시 봤는데 너무 귀여웠어요",
          "original_form": "저는 최근에 업 다시 봤는데 너무 귀여웠어요",
          "speaker_id": "1",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015719.1.17",
          "form": "그리고 로맨스 영화를 많이 봐서",
          "original_form": "그리고 로맨스 영화를 많이 봐서",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100015719.1.18",
          "form": "타이타닉 추천받아서 미루고 미루다가",
          "original_form": "타이타닉 추천받아서 미루고 미루다가",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100015719.1.19",
          "form": "결국 봤는데",
          "original_form": "결국 봤는데",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100015719.1.20",
          "form": "재미는 있더라구요",
          "original_form": "재미는 있더라구요",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100015719.1.21",
          "form": "타이타닉 보고 엄청 울었는데ㅠㅠ",
          "original_form": "타이타닉 보고 엄청 울었는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015719.1.22",
          "form": "업도 재미있게 봤어요!",
          "original_form": "업도 재미있게 봤어요!",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015719.1.23",
          "form": "그래서 업 보고 나서",
          "original_form": "그래서 업 보고 나서",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015719.1.24",
          "form": "디즈니 애니메이션 같은 영화들에 꽂혀서",
          "original_form": "디즈니 애니메이션 같은 영화들에 꽂혀서",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015719.1.25",
          "form": "보다가",
          "original_form": "보다가",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015719.1.26",
          "form": "그러고 있어요 ㅎㅎ",
          "original_form": "그러고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015719.1.27",
          "form": "저도 디즈니 픽사 애니메이션 좋아해요~",
          "original_form": "저도 디즈니 픽사 애니메이션 좋아해요~",
          "speaker_id": "2",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015719.1.28",
          "form": "표정이 익살맞은 게 참 귀여워요",
          "original_form": "표정이 익살맞은 게 참 귀여워요",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015719.1.29",
          "form": "키링으로 갖고 싶은",
          "original_form": "키링으로 갖고 싶은",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015719.1.30",
          "form": "느낌",
          "original_form": "느낌",
          "speaker_id": "1",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015719.1.31",
          "form": "맞아요! 혹시 주토피아 보셨어요?",
          "original_form": "맞아요! 혹시 주토피아 보셨어요?",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015719.1.32",
          "form": "닉과 주디 나오는 거요",
          "original_form": "닉과 주디 나오는 거요",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015719.1.33",
          "form": "봤어요 ㅎㅎㅎ",
          "original_form": "봤어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015719.1.34",
          "form": "주토피아 킬포인트는 그 손바닥 하드 먹고 다니는",
          "original_form": "주토피아 킬포인트는 그 손바닥 하드 먹고 다니는",
          "speaker_id": "1",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015719.1.35",
          "form": "조그만 주황색 쥐돌이들입니다...ㅠㅠㅠ",
          "original_form": "조그만 주황색 쥐돌이들입니다...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015719.1.36",
          "form": "맞아요ㅠㅠㅠ 너무 귀여웠어요",
          "original_form": "맞아요ㅠㅠㅠ 너무 귀여웠어요",
          "speaker_id": "2",
          "time": "20210826 01:06"
        }
      ]
