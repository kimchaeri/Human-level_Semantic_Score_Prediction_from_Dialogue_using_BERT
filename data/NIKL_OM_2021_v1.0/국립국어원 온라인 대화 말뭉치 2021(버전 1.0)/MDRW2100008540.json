      "utterance": [
        {
          "id": "MDRW2100008540.1.1",
          "form": "갈아만든배 아시나요",
          "original_form": "갈아만든배 아시나요",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008540.1.2",
          "form": "숙취음료로최고죠",
          "original_form": "숙취음료로최고죠",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008540.1.3",
          "form": "어떤음료 좋아하시나요",
          "original_form": "어떤음료 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008540.1.4",
          "form": "저는봉봉좋아해요",
          "original_form": "저는봉봉좋아해요",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008540.1.5",
          "form": "봉봉은 무슨맛인가요?",
          "original_form": "봉봉은 무슨맛인가요?",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008540.1.6",
          "form": "포도맛이예요",
          "original_form": "포도맛이예요",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008540.1.7",
          "form": "포도알도있어요",
          "original_form": "포도알도있어요",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008540.1.8",
          "form": "식감이 좋겠네요",
          "original_form": "식감이 좋겠네요",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008540.1.9",
          "form": "부드러워요",
          "original_form": "부드러워요",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008540.1.10",
          "form": "코코팜도 알갱이가 있어 맛있어요",
          "original_form": "코코팜도 알갱이가 있어 맛있어요",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008540.1.11",
          "form": "맞아요 봉봉도 드셔보세요",
          "original_form": "맞아요 봉봉도 드셔보세요",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008540.1.12",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008540.1.13",
          "form": "봉봉도 먹어보고싶네요",
          "original_form": "봉봉도 먹어보고싶네요",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008540.1.14",
          "form": "포도좋아하심 좋아할거예요",
          "original_form": "포도좋아하심 좋아할거예요",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008540.1.15",
          "form": "봉봉복숭아도있대요",
          "original_form": "봉봉복숭아도있대요",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008540.1.16",
          "form": "포도 복숭아 모두 좋아해요. 봉봉 맛별로 먹어야겠어요.",
          "original_form": "포도 복숭아 모두 좋아해요. 봉봉 맛별로 먹어야겠어요.",
          "speaker_id": "2",
          "time": "20210822 16:40"
