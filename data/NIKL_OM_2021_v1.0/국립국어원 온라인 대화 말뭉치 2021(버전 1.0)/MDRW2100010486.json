      "utterance": [
        {
          "id": "MDRW2100010486.1.1",
          "form": "닭가슴살 다 먹었던데 좀 시켜줘요",
          "original_form": "닭가슴살 다 먹었던데 좀 시켜줘요",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010486.1.2",
          "form": "지금 먹는걸로?",
          "original_form": "지금 먹는걸로?",
          "speaker_id": "1",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010486.1.3",
          "form": "ㅇㅇ 트레이더스",
          "original_form": "ㅇㅇ 트레이더스",
          "speaker_id": "2",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010486.1.4",
          "form": "그게 안짜고 맛있긴 하더라",
          "original_form": "그게 안짜고 맛있긴 하더라",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010486.1.5",
          "form": "트레이더스 닭가슴살이 운동하는 사람들 사이에선 유명해",
          "original_form": "트레이더스 닭가슴살이 운동하는 사람들 사이에선 유명해",
          "speaker_id": "2",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010486.1.6",
          "form": "ㅎㅎ 닭가슴살 보통은 퍼석하고 맛없는데",
          "original_form": "ㅎㅎ 닭가슴살 보통은 퍼석하고 맛없는데",
          "speaker_id": "1",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010486.1.7",
          "form": "이건 좀 덜 질리더라고",
          "original_form": "이건 좀 덜 질리더라고",
          "speaker_id": "2",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010486.1.8",
          "form": "이번 주말에 트레이더스 가야겠네",
          "original_form": "이번 주말에 트레이더스 가야겠네",
          "speaker_id": "1",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010486.1.9",
          "form": "배송 안하고?",
          "original_form": "배송 안하고?",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010486.1.10",
          "form": "직접 가는 게 더 싸더라고",
          "original_form": "직접 가는 게 더 싸더라고",
          "speaker_id": "1",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010486.1.11",
          "form": "아 배송비 있나?",
          "original_form": "아 배송비 있나?",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010486.1.12",
          "form": "트레이더스는 10만원 이상 사야 배송비 없어",
          "original_form": "트레이더스는 10만원 이상 사야 배송비 없어",
          "speaker_id": "1",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010486.1.13",
          "form": "아.. 그럼 가야겠네 ㅎㅎ",
          "original_form": "아.. 그럼 가야겠네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010486.1.14",
          "form": "가까우니까 뭐",
          "original_form": "가까우니까 뭐",
          "speaker_id": "1",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010486.1.15",
          "form": "많이 사서 쟁여두자",
          "original_form": "많이 사서 쟁여두자",
          "speaker_id": "2",
