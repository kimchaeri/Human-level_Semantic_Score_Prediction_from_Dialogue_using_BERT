      "utterance": [
        {
          "id": "MDRW2100019689.1.1",
          "form": "고추장 만들어야겠다",
          "original_form": "고추장 만들어야겠다",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.2",
          "form": "고추장을 만든다고?",
          "original_form": "고추장을 만든다고?",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.4",
          "form": "어떻게? 고추장은 사먹는거지",
          "original_form": "어떻게? 고추장은 사먹는거지",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.5",
          "form": "만들 수 있다",
          "original_form": "만들 수 있다",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.6",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.7",
          "form": "유튜브에 레시피 있다",
          "original_form": "유튜브에 레시피 있다",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.8",
          "form": "아하 ㅋㅋㅋ 그럼 만들기 쉽겠네",
          "original_form": "아하 ㅋㅋㅋ 그럼 만들기 쉽겠네",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.9",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019689.1.10",
          "form": "재료가 많이 필요할것 같은데",
          "original_form": "재료가 많이 필요할것 같은데",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019689.1.11",
          "form": "다 있다",
          "original_form": "다 있다",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019689.1.12",
          "form": "신기하구만 ㅎㅎ 구경해야지",
          "original_form": "신기하구만 ㅎㅎ 구경해야지",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019689.1.13",
          "form": "그래 좀이따 만들게",
          "original_form": "그래 좀이따 만들게",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019689.1.14",
          "form": "만들때 말해용",
          "original_form": "만들때 말해용",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019689.1.15",
          "form": "컵누들 얘기하니까 국수가 먹고싶다..",
          "original_form": "컵누들 얘기하니까 국수가 먹고싶다..",
