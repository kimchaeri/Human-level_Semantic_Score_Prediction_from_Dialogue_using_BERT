      "utterance": [
        {
          "id": "MDRW2100000647.1.1",
          "form": "혹시 핫한 플립 구매하셨나요?ㅎㅎ",
          "original_form": "혹시 핫한 플립 구매하셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000647.1.2",
          "form": "삼성  플립3",
          "original_form": "삼성  플립3",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000647.1.3",
          "form": "앗 아니요! 구매하셨어요?",
          "original_form": "앗 아니요! 구매하셨어요?",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000647.1.4",
          "form": "전 플립1 썼었어요 ㅎㅎㅎ",
          "original_form": "전 플립1 썼었어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000647.1.5",
          "form": "아뇨 너무비싸서 못사구요 워치4만 주문했어요",
          "original_form": "아뇨 너무비싸서 못사구요 워치4만 주문했어요",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000647.1.6",
          "form": "오 저두 워치 탐나더라구요 ㅠㅠ",
          "original_form": "오 저두 워치 탐나더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000647.1.7",
          "form": "워치 전부터 사고싶었는데 액티브2  살까하다가 가격차이가 얼마안나더라구요 그래서 워치4로 예약했어요",
          "original_form": "워치 전부터 사고싶었는데 액티브2  살까하다가 가격차이가 얼마안나더라구요 그래서 워치4로 예약했어요",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000647.1.8",
          "form": "좋네요~! 신상이 좋죠! ㅎㅎ",
          "original_form": "좋네요~! 신상이 좋죠! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000647.1.9",
          "form": "인바디 기능 되는거죠!",
          "original_form": "인바디 기능 되는거죠!",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000647.1.10",
          "form": "네 인바디된데요 기대되요",
          "original_form": "네 인바디된데요 기대되요",
          "speaker_id": "2",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000647.1.11",
          "form": "좋네용~! 전 삼성폰 잠시 쓰다가 아이폰으로 돌아갔어요 ㅋㅋ",
          "original_form": "좋네용~! 전 삼성폰 잠시 쓰다가 아이폰으로 돌아갔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000647.1.12",
          "form": "저번달에 충동구매로 ㅠㅋㅋ",
          "original_form": "저번달에 충동구매로 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000647.1.13",
          "form": "오~ 저는 아이폰쓰다 갤로 왔는데요 ㅎ",
          "original_form": "오~ 저는 아이폰쓰다 갤로 왔는데요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000647.1.14",
          "form": "옷 그렇군요 ㅋㅋ 삼성폰이 기능좋은건 짱인것같아요~",
          "original_form": "옷 그렇군요 ㅋㅋ 삼성폰이 기능좋은건 짱인것같아요~",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000647.1.15",
          "form": "요새는 디자인도 넘 이쁘게 나오고~",
          "original_form": "요새는 디자인도 넘 이쁘게 나오고~",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000647.1.16",
          "form": "아이폰이 기능상 좀더 가볍게 넘어가기도하고 좋긴해요",
          "original_form": "아이폰이 기능상 좀더 가볍게 넘어가기도하고 좋긴해요",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000647.1.17",
          "form": "오 맞아용 ㅠ좀더 가벼운 느낌~ㅎㅎ 그게 전 넘익숙하네용",
          "original_form": "오 맞아용 ㅠ좀더 가벼운 느낌~ㅎㅎ 그게 전 넘익숙하네용",
