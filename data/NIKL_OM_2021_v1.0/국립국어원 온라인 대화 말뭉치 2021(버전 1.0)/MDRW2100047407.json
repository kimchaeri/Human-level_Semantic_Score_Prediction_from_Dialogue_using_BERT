      "utterance": [
        {
          "id": "MDRW2100047407.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047407.1.2",
          "form": "네 name1님은요?",
          "original_form": "네 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047407.1.3",
          "form": "저두용 전시회 가본적은있나요?",
          "original_form": "저두용 전시회 가본적은있나요?",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100047407.1.4",
          "form": "네 ㅋㅋ 예전에는",
          "original_form": "네 ㅋㅋ 예전에는",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047407.1.5",
          "form": "자주 갔어요",
          "original_form": "자주 갔어요",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047407.1.6",
          "form": "우와 전시회는 아직 안가봤어여",
          "original_form": "우와 전시회는 아직 안가봤어여",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047407.1.7",
          "form": "함 가봐야겠어요 ㅎㅎ",
          "original_form": "함 가봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047407.1.8",
          "form": "만족스러유ㅓ요",
          "original_form": "만족스러유ㅓ요",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100047407.1.9",
          "form": "요즘 한국영화도 많이 개봉했더라구요",
          "original_form": "요즘 한국영화도 많이 개봉했더라구요",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100047407.1.10",
          "form": "ㅋㅋㅋ맞아요 ㅎ",
          "original_form": "ㅋㅋㅋ맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100047407.1.11",
          "form": "보신 거 있나요",
          "original_form": "보신 거 있나요",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100047407.1.12",
          "form": "아뇨 아직 못봤네요 ㅠ",
          "original_form": "아뇨 아직 못봤네요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100047407.1.13",
          "form": "저도요..",
          "original_form": "저도요..",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100047407.1.14",
          "form": "ㅋㅋ 사실 요새 안본지 됐어요",
          "original_form": "ㅋㅋ 사실 요새 안본지 됐어요",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100047407.1.15",
          "form": "콘서트도 넘 가고파요 ㅠㅠ",
          "original_form": "콘서트도 넘 가고파요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100047407.1.16",
          "form": "저두요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047407.1.17",
          "form": "어디 가고싶으세요",
          "original_form": "어디 가고싶으세요",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047407.1.18",
          "form": "박효신콘서트용 ㅠㅠ",
          "original_form": "박효신콘서트용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047407.1.19",
          "form": "아 귀 힐링일 거 같아요ㅋㅋㅋㅋ",
          "original_form": "아 귀 힐링일 거 같아요ㅋㅋㅋㅋ",
