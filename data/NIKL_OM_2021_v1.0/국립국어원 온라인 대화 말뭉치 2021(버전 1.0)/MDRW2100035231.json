      "utterance": [
        {
          "id": "MDRW2100035231.1.1",
          "form": "카페 자주 가시나요?",
          "original_form": "카페 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100035231.1.2",
          "form": "가끔 가는편이에요!",
          "original_form": "가끔 가는편이에요!",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100035231.1.3",
          "form": "자주 가시나요?",
          "original_form": "자주 가시나요?",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100035231.1.4",
          "form": "저는 자주 가는 편이에요!",
          "original_form": "저는 자주 가는 편이에요!",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100035231.1.5",
          "form": "어디 자주 가세요?",
          "original_form": "어디 자주 가세요?",
          "speaker_id": "1",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100035231.1.6",
          "form": "동네에서는 개인 카페나 이디야 가고, 좀 번화가면 폴바셋이나 투썸이요!",
          "original_form": "동네에서는 개인 카페나 이디야 가고, 좀 번화가면 폴바셋이나 투썸이요!",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100035231.1.7",
          "form": "오오 저는 투썸 케이크 좋아해요!",
          "original_form": "오오 저는 투썸 케이크 좋아해요!",
          "speaker_id": "1",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100035231.1.8",
          "form": "어떤 케이크 좋아하시나요??",
          "original_form": "어떤 케이크 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100035231.1.9",
          "form": "요즘 티라미스에 빠졌어요!!!",
          "original_form": "요즘 티라미스에 빠졌어요!!!",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100035231.1.10",
          "form": "케이크 좋아하세요?",
          "original_form": "케이크 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100035231.1.11",
          "form": "완전 좋아해요! 저는 요즘 레드벨벳이요!",
          "original_form": "완전 좋아해요! 저는 요즘 레드벨벳이요!",
          "speaker_id": "2",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035231.1.12",
          "form": "그것도 맛있죠!!",
          "original_form": "그것도 맛있죠!!",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100035231.1.13",
          "form": "레드벨벳은 카페마다 맛이 다른것도 매력이에요",
          "original_form": "레드벨벳은 카페마다 맛이 다른것도 매력이에요",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100035231.1.14",
          "form": "맞아요! 크림치즈 진하기도 다 다르고요",
          "original_form": "맞아요! 크림치즈 진하기도 다 다르고요",
          "speaker_id": "2",
          "time": "20210829 14:54"
        },
        {
          "id": "MDRW2100035231.1.15",
          "form": "맞아용~ 그 맛에 케이크 탐방 한다니까요",
          "original_form": "맞아용~ 그 맛에 케이크 탐방 한다니까요",
          "speaker_id": "1",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100035231.1.16",
          "form": "요새 개인카페 케이크도 맛나구요ㅠ",
          "original_form": "요새 개인카페 케이크도 맛나구요ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100035231.1.17",
