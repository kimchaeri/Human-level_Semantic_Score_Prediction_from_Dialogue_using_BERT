        },
        {
          "id": "MDRW2100016348.1.4",
          "form": "가끔이요",
          "original_form": "가끔이요",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016348.1.5",
          "form": "최근 잼나게 읽은거 있으세요?",
          "original_form": "최근 잼나게 읽은거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016348.1.6",
          "form": "엘리어트 파동이론이라는 책을 읽고있어요",
          "original_form": "엘리어트 파동이론이라는 책을 읽고있어요",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016348.1.7",
          "form": "우와 경제학 책인가요?",
          "original_form": "우와 경제학 책인가요?",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016348.1.8",
          "form": "네 주식 책이에요",
          "original_form": "네 주식 책이에요",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016348.1.9",
          "form": "오 베스트셀러인가요?",
          "original_form": "오 베스트셀러인가요?",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
