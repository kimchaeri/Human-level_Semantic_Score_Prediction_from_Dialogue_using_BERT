      "utterance": [
        {
          "id": "MDRW2100033475.1.1",
          "form": "냘뵈요 ㅎㅎ",
          "original_form": "냘뵈요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100033475.1.2",
          "form": "자러가게요 ㅎㅎ",
          "original_form": "자러가게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100033475.1.3",
          "form": "네네 쉬세용",
          "original_form": "네네 쉬세용",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100033475.1.4",
          "form": "굿밤되세요 ㅎㅎ즐거웠어요",
          "original_form": "굿밤되세요 ㅎㅎ즐거웠어요",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100033475.1.5",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100033475.1.6",
          "form": "점심 메뉴는 머였나요?ㅎㅎ",
          "original_form": "점심 메뉴는 머였나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100033475.1.7",
          "form": "궁금해지네요",
          "original_form": "궁금해지네요",
          "speaker_id": "1",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100033475.1.8",
          "form": "아 지금 먹으려고요",
          "original_form": "아 지금 먹으려고요",
          "speaker_id": "2",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100033475.1.9",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100033475.1.10",
          "form": "전 혼자라 라면요 ㅡㅡ",
          "original_form": "전 혼자라 라면요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100033475.1.11",
          "form": "저는 카레요 ㅎ",
          "original_form": "저는 카레요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100033475.1.12",
          "form": "카레에 김싸서 먹었어요 ㅎ",
          "original_form": "카레에 김싸서 먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100033475.1.13",
          "form": "오 딱 환상조합이네요",
          "original_form": "오 딱 환상조합이네요",
          "speaker_id": "2",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100033475.1.14",
          "form": "카레좀 해먹어야 겠어요",
          "original_form": "카레좀 해먹어야 겠어요",
          "speaker_id": "2",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100033475.1.15",
          "form": "네 맛나요 ㅎㅎ",
          "original_form": "네 맛나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100033475.1.16",
          "form": "쩝쪼롬하니 입맛 땡기네요",
          "original_form": "쩝쪼롬하니 입맛 땡기네요",
          "speaker_id": "2",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100033475.1.17",
          "form": "저도 오랫만에 먹었네요",
          "original_form": "저도 오랫만에 먹었네요",
          "speaker_id": "1",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100033475.1.18",
          "form": "음식궁합을 잘 아시네요",
          "original_form": "음식궁합을 잘 아시네요",
          "speaker_id": "2",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100033475.1.19",
          "form": "애가 맵다고 안먹어서 맨날 짜장만 하다가 ㅎㅎ",
          "original_form": "애가 맵다고 안먹어서 맨날 짜장만 하다가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100033475.1.20",
          "form": "네 고기감자 듬뿍 넣어 저녁에 해봐야 겠러요",
          "original_form": "네 고기감자 듬뿍 넣어 저녁에 해봐야 겠러요",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100033475.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100033475.1.22",
          "form": "똑같네요 ㅡㅡ저도 항시 그랬어요",
          "original_form": "똑같네요 ㅡㅡ저도 항시 그랬어요",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100033475.1.23",
          "form": "애들이 향기도 싫어하죠?",
          "original_form": "애들이 향기도 싫어하죠?",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100033475.1.24",
          "form": "네 약간 화한게 싫은가봐요",
          "original_form": "네 약간 화한게 싫은가봐요",
          "speaker_id": "1",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100033475.1.25",
          "form": "그렇죠 엄마는 먹고 싶은것도 맘대로 못먹어요",
          "original_form": "그렇죠 엄마는 먹고 싶은것도 맘대로 못먹어요",
          "speaker_id": "2",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100033475.1.26",
          "form": "근데 어제 유치원선생님 말씀으론 카레떡볶이는 잘먹는데요 ㅋㅋ",
          "original_form": "근데 어제 유치원선생님 말씀으론 카레떡볶이는 잘먹는데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100033475.1.27",
          "form": "어서 커야 할텐데 그래야 고추가루 팍팍먹죠",
          "original_form": "어서 커야 할텐데 그래야 고추가루 팍팍먹죠",
          "speaker_id": "2",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100033475.1.28",
          "form": "ㅋㅋ역시 떡볶이는 아이들이 좋아하는군요",
          "original_form": "ㅋㅋ역시 떡볶이는 아이들이 좋아하는군요",
          "speaker_id": "2",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100033475.1.29",
          "form": "결국은 내요리가 맛이 없었던건지....ㅋㅋ",
          "original_form": "결국은 내요리가 맛이 없었던건지....ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100033475.1.30",
          "form": "에이 설마요 애들이 나가면 또 잘먹어요",
          "original_form": "에이 설마요 애들이 나가면 또 잘먹어요",
          "speaker_id": "2",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100033475.1.31",
          "form": "점심에 배고파 먹었을거예요",
          "original_form": "점심에 배고파 먹었을거예요",
          "speaker_id": "2",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100033475.1.32",
          "form": "뭐든 실컷놀면 잘먹죠",
          "original_form": "뭐든 실컷놀면 잘먹죠",
          "speaker_id": "2",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100033475.1.33",
          "form": "레시피 물어보세요 ㅋㅋ",
          "original_form": "레시피 물어보세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100033475.1.34",
          "form": "그래야할까봐요",
          "original_form": "그래야할까봐요",
          "speaker_id": "1",
          "time": "20210824 12:57"
        }
      ]
    }
