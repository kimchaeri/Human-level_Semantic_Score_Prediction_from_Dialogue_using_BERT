        {
          "id": "MDRW2100045547.1.1",
          "form": "여행 많이 다니셨나요?",
          "original_form": "여행 많이 다니셨나요?",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045547.1.2",
          "form": "적당히요? ㅋㅋ",
          "original_form": "적당히요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045547.1.3",
          "form": "해외어디가 좋으셨나요",
          "original_form": "해외어디가 좋으셨나요",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045547.1.4",
          "form": "저는 사이판요",
          "original_form": "저는 사이판요",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045547.1.5",
          "form": "물 색이 너무 이쁘더라구요",
          "original_form": "물 색이 너무 이쁘더라구요",
