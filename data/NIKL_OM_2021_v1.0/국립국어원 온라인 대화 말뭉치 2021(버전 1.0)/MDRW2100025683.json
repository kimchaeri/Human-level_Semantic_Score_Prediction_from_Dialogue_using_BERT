        },
        {
          "id": "MDRW2100025683.1.3",
          "form": "name3이가 자꾸 김치찌개 먹재",
          "original_form": "&name3&이가 자꾸 김치찌개 먹재",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025683.1.4",
          "form": "나 요즘에 마라탕 먹고싶어",
          "original_form": "나 요즘에 마라탕 먹고싶어",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025683.1.5",
          "form": "김찌 좋지",
          "original_form": "김찌 좋지",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025683.1.6",
          "form": "나는 좋은데 동방사람들한테 물어봐",
          "original_form": "나는 좋은데 동방사람들한테 물어봐",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025683.1.7",
          "form": "어차피 이name4은 이name3 말 듣게 돼있음",
          "original_form": "어차피 이&name4&은 이&name3& 말 듣게 돼있음",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025683.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025683.1.9",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025683.1.10",
          "form": "전에 누구였더라... 누가 우리 코딩 동아리 아니라 김찌 동아리 아니냐고 했잖아",
          "original_form": "전에 누구였더라... 누가 우리 코딩 동아리 아니라 김찌 동아리 아니냐고 했잖아",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025683.1.11",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025683.1.12",
          "form": "그거 name5이었을걸ㅋㅋㅋ",
          "original_form": "그거 &name5&이었을걸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025683.1.13",
          "form": "근데 나는 김찌 좋아...",
          "original_form": "근데 나는 김찌 좋아...",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025683.1.14",
          "form": "계란후라이에 김찌 최고",
          "original_form": "계란후라이에 김찌 최고",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025683.1.15",
          "form": "name5인 우리동아리도 아닌데",
          "original_form": "&name5&인 우리동아리도 아닌데",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025683.1.16",
          "form": "name3이가 끌고 다니니...",
          "original_form": "&name3&이가 끌고 다니니...",
          "speaker_id": "2",
          "time": "20210901 16:02"
        },
        {
          "id": "MDRW2100025683.1.17",
          "form": "우리 고딩때도 name3이가 계속 김찌김찌 그랬자녀",
          "original_form": "우리 고딩때도 &name3&이가 계속 김찌김찌 그랬자녀",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025683.1.18",
          "form": "하긴 매번 밥먹었냐고 물어보면 김찌라고 하니까",
          "original_form": "하긴 매번 밥먹었냐고 물어보면 김찌라고 하니까",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025683.1.19",
          "form": "애들 대화시즌이라 식단할때도 우리 계속 김찌 먹었잖아",
          "original_form": "애들 대화시즌이라 식단할때도 우리 계속 김찌 먹었잖아",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025683.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
