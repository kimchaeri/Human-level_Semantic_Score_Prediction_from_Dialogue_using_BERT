      "utterance": [
        {
          "id": "MDRW2100010545.1.1",
          "form": "오늘 저녁은 머ㅓ머겅",
          "original_form": "오늘 저녁은 머ㅓ머겅",
          "speaker_id": "1",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010545.1.2",
          "form": "콩나물낙지비빔밥",
          "original_form": "콩나물낙지비빔밥",
          "speaker_id": "2",
          "time": "20210823 19:38"
        },
        {
          "id": "MDRW2100010545.1.3",
          "form": "오 짱맛있겠다 살짝 매운",
          "original_form": "오 짱맛있겠다 살짝 매운",
          "speaker_id": "1",
          "time": "20210824 08:23"
        },
        {
          "id": "MDRW2100010545.1.4",
          "form": "콩낙비는 진리지",
          "original_form": "콩낙비는 진리지",
          "speaker_id": "2",
          "time": "20210824 09:08"
        },
        {
          "id": "MDRW2100010545.1.5",
          "form": "아침은 빵에 딸기잼발라서 라떼랑 먹고왔다",
          "original_form": "아침은 빵에 딸기잼발라서 라떼랑 먹고왔다",
          "speaker_id": "2",
          "time": "20210824 09:08"
        },
        {
          "id": "MDRW2100010545.1.6",
          "form": "벌써 집 가고싶다",
          "original_form": "벌써 집 가고싶다",
          "speaker_id": "2",
          "time": "20210824 09:08"
        },
        {
          "id": "MDRW2100010545.1.7",
          "form": "인정........",
          "original_form": "인정........",
          "speaker_id": "1",
          "time": "20210824 09:08"
        },
        {
          "id": "MDRW2100010545.1.8",
          "form": "회사가자마자 탕비실 털어야지....",
          "original_form": "회사가자마자 탕비실 털어야지....",
          "speaker_id": "1",
          "time": "20210824 09:08"
        },
        {
          "id": "MDRW2100010545.1.9",
          "form": "카누 주머니에 세개만 넣어와",
          "original_form": "카누 주머니에 세개만 넣어와",
          "speaker_id": "2",
          "time": "20210824 09:09"
        },
        {
          "id": "MDRW2100010545.1.10",
          "form": "더 훔치면 걸려",
          "original_form": "더 훔치면 걸려",
          "speaker_id": "2",
          "time": "20210824 09:09"
        },
        {
          "id": "MDRW2100010545.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:09"
        },
        {
          "id": "MDRW2100010545.1.12",
          "form": "우린 그 머신?이라ㅠ",
          "original_form": "우린 그 머신?이라ㅠ",
          "speaker_id": "1",
          "time": "20210824 09:09"
        },
        {
          "id": "MDRW2100010545.1.13",
          "form": "캡슐이면 두개",
          "original_form": "캡슐이면 두개",
          "speaker_id": "2",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100010545.1.14",
          "form": "주머니에 하나씩 넣어와",
          "original_form": "주머니에 하나씩 넣어와",
          "speaker_id": "2",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100010545.1.15",
          "form": "평소엔 로투스 쳐다도 안보는데",
          "original_form": "평소엔 로투스 쳐다도 안보는데",
          "speaker_id": "2",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100010545.1.16",
          "form": "탕비실 로투스는 가끔 겁나 맛있어",
          "original_form": "탕비실 로투스는 가끔 겁나 맛있어",
          "speaker_id": "2",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100010545.1.17",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㅋㄱ맞아",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㅋㄱ맞아",
          "speaker_id": "1",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100010545.1.18",
          "form": "회사에 있는 간식을은 왤케 맛있는지",
          "original_form": "회사에 있는 간식을은 왤케 맛있는지",
          "speaker_id": "1",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100010545.1.19",
          "form": "간식을 먹으면서 때우는 시간이 맛있는게 아닐까",
          "original_form": "간식을 먹으면서 때우는 시간이 맛있는게 아닐까",
          "speaker_id": "2",
          "time": "20210824 09:25"
        },
        {
          "id": "MDRW2100010545.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ인정",
          "original_form": "ㅋㅋㅋㅋㅋㅋ인정",
          "speaker_id": "1",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100010545.1.21",
          "form": "오늘은 과자 털어먹었어",
          "original_form": "오늘은 과자 털어먹었어",
          "speaker_id": "1",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100010545.1.22",
          "form": "난 예전에 학원 보조알바할때",
          "original_form": "난 예전에 학원 보조알바할때",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100010545.1.23",
          "form": "캡슐머신이었는데 커피 진짜 뭔가에 홀린것처럼 먹음",
          "original_form": "캡슐머신이었는데 커피 진짜 뭔가에 홀린것처럼 먹음",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100010545.1.24",
          "form": "왠지 안먹으면 손해보는기분이어서",
          "original_form": "왠지 안먹으면 손해보는기분이어서",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100010545.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ뭔느낌인지 알지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ뭔느낌인지 알지",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100010545.1.26",
          "form": "나도 탕비실 열심히 털잖아 완전",
          "original_form": "나도 탕비실 열심히 털잖아 완전",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100010545.1.27",
          "form": "점심메뉴 고르는게 젤 힘들어",
          "original_form": "점심메뉴 고르는게 젤 힘들어",
          "speaker_id": "1",
          "time": "20210824 23:19"
        }
      ]
    }
