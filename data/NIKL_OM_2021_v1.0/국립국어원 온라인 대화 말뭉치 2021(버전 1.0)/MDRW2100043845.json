      "utterance": [
        {
          "id": "MDRW2100043845.1.1",
          "form": "네ㅋㅋ백종원님 만만세",
          "original_form": "네ㅋㅋ백종원님 만만세",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043845.1.2",
          "form": "ㅋㅋㅋㅋㅋ 그니깐요",
          "original_form": "ㅋㅋㅋㅋㅋ 그니깐요",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043845.1.3",
          "form": "한동안 김치볶음밥 따라서 만들어 먹고그랬는데",
          "original_form": "한동안 김치볶음밥 따라서 만들어 먹고그랬는데",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043845.1.4",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043845.1.5",
          "form": "요새 챙겨보시는거 잇으신가요?",
          "original_form": "요새 챙겨보시는거 잇으신가요?",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043845.1.6",
          "form": "뭐보시나여",
          "original_form": "뭐보시나여",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043845.1.7",
          "form": "저~유튜브는 일주어터님꺼랑 복희? 그분꺼잘봐요",
          "original_form": "저~유튜브는 일주어터님꺼랑 복희? 그분꺼잘봐요",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043845.1.8",
          "form": "저는 환승연애!",
          "original_form": "저는 환승연애!",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043845.1.9",
          "form": "이게...무슨내용인가요?",
          "original_form": "이게...무슨내용인가요?",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043845.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043845.1.11",
          "form": "일반인들이 같이 살면서 썸타는 프로에요",
          "original_form": "일반인들이 같이 살면서 썸타는 프로에요",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043845.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043845.1.13",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043845.1.14",
          "form": "그런거군요",
          "original_form": "그런거군요",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043845.1.15",
          "form": "유튜브에서만 볼수있나요?",
          "original_form": "유튜브에서만 볼수있나요?",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043845.1.16",
          "form": "티빙에만잇어요",
          "original_form": "티빙에만잇어요",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043845.1.17",
          "form": "티빙단독!",
          "original_form": "티빙단독!",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043845.1.18",
          "form": "아 티빙..",
          "original_form": "아 티빙..",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043845.1.19",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043845.1.20",
          "form": "티빙은 구독을 안해서",
          "original_form": "티빙은 구독을 안해서",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043845.1.21",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043845.1.22",
          "form": "다른프로그램",
          "original_form": "다른프로그램",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043845.1.23",
          "form": "일반인들 나오는거는 잘 못보겠더라고요 뭔가",
          "original_form": "일반인들 나오는거는 잘 못보겠더라고요 뭔가",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043845.1.24",
          "form": "챙겨보시는거잇으신가용",
          "original_form": "챙겨보시는거잇으신가용",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043845.1.25",
          "form": "맞아요 이거 호불호많이갈려요ㅋㅋㅋ",
          "original_form": "맞아요 이거 호불호많이갈려요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043845.1.26",
          "form": "저는~ 식스센스랑 심야괴담회요 ㅎ",
          "original_form": "저는~ 식스센스랑 심야괴담회요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:16"
        }
      ]
    }
