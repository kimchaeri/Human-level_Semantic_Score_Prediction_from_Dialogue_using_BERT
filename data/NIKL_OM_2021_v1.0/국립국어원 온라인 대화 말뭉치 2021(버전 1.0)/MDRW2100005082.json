      "utterance": [
        {
          "id": "MDRW2100005082.1.1",
          "form": "나 오늘 짜장면 먹음",
          "original_form": "나 오늘 짜장면 먹음",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005082.1.2",
          "form": "물짜장 먹었어?",
          "original_form": "물짜장 먹었어?",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005082.1.3",
          "form": "나 오늘 짜장면 먹음",
          "original_form": "나 오늘 짜장면 먹음",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005082.1.4",
          "form": "아니 물짜장 맛없어",
          "original_form": "아니 물짜장 맛없어",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005082.1.5",
          "form": "물짜장 뭔 맛이야",
          "original_form": "물짜장 뭔 맛이야",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005082.1.6",
          "form": "너무 오래전에 먹어봐서 기억이 안나",
          "original_form": "너무 오래전에 먹어봐서 기억이 안나",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005082.1.7",
          "form": "나는 오늘 고속도로 휴게소에서 엄청 사먹었어",
          "original_form": "나는 오늘 고속도로 휴게소에서 엄청 사먹었어",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005082.1.8",
          "form": "뭐 먹었는데",
          "original_form": "뭐 먹었는데",
          "speaker_id": "1",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005082.1.9",
          "form": "음  .. 순두부찌개 꽈배기 찹쌀도넛",
          "original_form": "음  .. 순두부찌개 꽈배기 찹쌀도넛",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005082.1.10",
          "form": "갈비만두",
          "original_form": "갈비만두",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005082.1.11",
          "form": "폴바셋에서 컵빙수도 먹었다",
          "original_form": "폴바셋에서 컵빙수도 먹었다",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005082.1.12",
          "form": "음 많이 먹었넼ㅋㅋㅋㅋㅋ",
          "original_form": "음 많이 먹었넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005082.1.13",
          "form": "갈비만두 맛있음?",
          "original_form": "갈비만두 맛있음?",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005082.1.14",
          "form": "갈비만두 맛있지",
          "original_form": "갈비만두 맛있지",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005082.1.15",
          "form": "만두는 다 맛있어",
          "original_form": "만두는 다 맛있어",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005082.1.16",
          "form": "폴바셋에서는 그 우유? 초콜릿? 아이스크림이 맛있자너",
          "original_form": "폴바셋에서는 그 우유? 초콜릿? 아이스크림이 맛있자너",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005082.1.17",
          "form": "밀크아이스크림이 맛있어",
          "original_form": "밀크아이스크림이 맛있어",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005082.1.18",
          "form": "빙수에도 올려주더라고",
          "original_form": "빙수에도 올려주더라고",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005082.1.19",
          "form": "나 까눌레 먹고 싶음",
          "original_form": "나 까눌레 먹고 싶음",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005082.1.20",
          "form": "나는 밀푀유",
          "original_form": "나는 밀푀유",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005082.1.21",
          "form": "전주에 맛있는 카페 많지않나?",
          "original_form": "전주에 맛있는 카페 많지않나?",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005082.1.22",
          "form": "까눌레는 생각보다 별로 안팔더라고",
          "original_form": "까눌레는 생각보다 별로 안팔더라고",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005082.1.23",
