      "utterance": [
        {
          "id": "MDRW2100016626.1.1",
          "form": "좋아하는 음식이 어떤거셔요",
          "original_form": "좋아하는 음식이 어떤거셔요",
          "speaker_id": "2",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016626.1.2",
          "form": "고기 해산물 안 가립니다ㅎ",
          "original_form": "고기 해산물 안 가립니다ㅎ",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016626.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016626.1.4",
          "form": "아 저도 다 좋아해요.",
          "original_form": "아 저도 다 좋아해요.",
          "speaker_id": "2",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100016626.1.5",
          "form": "간식같은걸 끊어야하는데 달달한 간식의 유혹을 뿌리칠수가 없네요",
          "original_form": "간식같은걸 끊어야하는데 달달한 간식의 유혹을 뿌리칠수가 없네요",
          "speaker_id": "2",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100016626.1.6",
          "form": "ㅎㅎ 계속 손이 가는 마력이 있죠",
          "original_form": "ㅎㅎ 계속 손이 가는 마력이 있죠",
          "speaker_id": "1",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100016626.1.7",
          "form": "무슨 간식 좋아하시나요",
          "original_form": "무슨 간식 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100016626.1.8",
          "form": "과자중에는 꿀꽈배기 새우깡이요",
          "original_form": "과자중에는 꿀꽈배기 새우깡이요",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100016626.1.9",
          "form": "옛날사람같네요 흑",
          "original_form": "옛날사람같네요 흑",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100016626.1.10",
          "form": "ㅎㅎ사실 전 맛동산 좋아해요",
          "original_form": "ㅎㅎ사실 전 맛동산 좋아해요",
          "speaker_id": "1",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100016626.1.11",
          "form": "근데 가격이 엄청 비싸더라고요ㅜ",
          "original_form": "근데 가격이 엄청 비싸더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100016626.1.12",
          "form": "일반 과자 두세배ㅜ;;",
          "original_form": "일반 과자 두세배ㅜ;;",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100016626.1.13",
          "form": "맞아요. 양도 적어지고",
          "original_form": "맞아요. 양도 적어지고",
          "speaker_id": "2",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100016626.1.14",
          "form": "얘기하고보니 과자가 당기네요",
          "original_form": "얘기하고보니 과자가 당기네요",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100016626.1.15",
          "form": "집에는 꼬깔콘만 있는데",
          "original_form": "집에는 꼬깔콘만 있는데",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100016626.1.16",
          "form": "이마저도 스테디과자네요ㅎ",
          "original_form": "이마저도 스테디과자네요ㅎ",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100016626.1.17",
          "form": "아 꼬깔콘도 좋네요",
          "original_form": "아 꼬깔콘도 좋네요",
          "speaker_id": "2",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100016626.1.18",
          "form": "우리 완전 취향이 옛날사람ㅎㅎ",
          "original_form": "우리 완전 취향이 옛날사람ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100016626.1.19",
          "form": "요즘 새로나온 과자에는 뭐가있을까요.",
          "original_form": "요즘 새로나온 과자에는 뭐가있을까요.",
          "speaker_id": "2",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100016626.1.20",
          "form": "허니버터칩도 이미 지난 거고;",
          "original_form": "허니버터칩도 이미 지난 거고;",
          "speaker_id": "1",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100016626.1.21",
          "form": "꼬북칩?",
          "original_form": "꼬북칩?",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100016626.1.22",
          "form": "근데 기대정도는 아니더라고요",
          "original_form": "근데 기대정도는 아니더라고요",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100016626.1.23",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100016626.1.24",
          "form": "아 먹어봤는데 제취향은 아니예요 ㅎ",
          "original_form": "아 먹어봤는데 제취향은 아니예요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100016626.1.25",
          "form": "ㅎㅎ 오래 입증된 과자를 따라오기엔 아직 역부족이죠",
          "original_form": "ㅎㅎ 오래 입증된 과자를 따라오기엔 아직 역부족이죠",
          "speaker_id": "1",
          "time": "20210827 10:35"
        }
      ]
    }
  ]
