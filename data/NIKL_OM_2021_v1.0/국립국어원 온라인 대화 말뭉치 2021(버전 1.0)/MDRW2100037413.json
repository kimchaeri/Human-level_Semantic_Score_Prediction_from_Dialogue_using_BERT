      "utterance": [
        {
          "id": "MDRW2100037413.1.1",
          "form": "당연히 군만두지",
          "original_form": "당연히 군만두지",
          "speaker_id": "2",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037413.1.2",
          "form": "너는 설마 찐만두야m",
          "original_form": "너는 설마 찐만두야m",
          "speaker_id": "2",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037413.1.3",
          "form": "아니 나도 군만두야",
          "original_form": "아니 나도 군만두야",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037413.1.4",
          "form": "김치만두는 찐만두도ㅠ괜찮아",
          "original_form": "김치만두는 찐만두도ㅠ괜찮아",
          "speaker_id": "2",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037413.1.5",
          "form": "근데 에어프라이기만두도 좋아",
          "original_form": "근데 에어프라이기만두도 좋아",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037413.1.6",
          "form": "아 다행이다.",
          "original_form": "아 다행이다.",
          "speaker_id": "2",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037413.1.7",
          "form": "아 그지 김치만두는\r\n다 맛있어",
          "original_form": "아 그지 김치만두는\r\n다 맛있어",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.8",
          "form": "칠곡에 도화짬뽕 만두가 기가막혀",
          "original_form": "칠곡에 도화짬뽕 만두가 기가막혀",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.9",
          "form": "그럼 물만두는 어때?",
          "original_form": "그럼 물만두는 어때?",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.10",
          "form": "세승에 그런건 빨리알려주렴",
          "original_form": "세승에 그런건 빨리알려주렴",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.11",
          "form": "물만두는 별로야. 만둣국안에 만두만 좋아",
          "original_form": "물만두는 별로야. 만둣국안에 만두만 좋아",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.12",
          "form": "나는 물만두도 좋아해",
          "original_form": "나는 물만두도 좋아해",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.13",
          "form": "도화짬뽈 만두는 만두세상 만두랑 비슷해",
          "original_form": "도화짬뽈 만두는 만두세상 만두랑 비슷해",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.14",
          "form": "근데 더 바삭하고",
          "original_form": "근데 더 바삭하고",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.15",
          "form": "후루룩 주워먹기 참 좋더라",
          "original_form": "후루룩 주워먹기 참 좋더라",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.16",
          "form": "속이 꽉 찼어",
          "original_form": "속이 꽉 찼어",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.17",
          "form": "나는 둘 다 안먹어봤어",
          "original_form": "나는 둘 다 안먹어봤어",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.18",
          "form": "다음에 만두투어도 가자",
          "original_form": "다음에 만두투어도 가자",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.19",
          "form": "정말 주방장이 요리를 제대로 배우셨나봐",
          "original_form": "정말 주방장이 요리를 제대로 배우셨나봐",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.20",
          "form": "좋아.",
          "original_form": "좋아.",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.21",
          "form": "만두전골이 땡기는 날씨가 다가오네",
          "original_form": "만두전골이 땡기는 날씨가 다가오네",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.22",
          "form": "나릉 거기로 데려가주렴",
          "original_form": "나릉 거기로 데려가주렴",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ역시 배운사람이 최고야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ역시 배운사람이 최고야",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037413.1.24",
          "form": "만두전골 먹고싶댜",
          "original_form": "만두전골 먹고싶댜",
          "speaker_id": "2",
          "time": "20210831 23:27"
        }
      ]
    }
  ]
