          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006273.1.11",
          "form": "멋지시네요",
          "original_form": "멋지시네요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006273.1.12",
          "form": "오 님도 신청하셨어요?",
          "original_form": "오 님도 신청하셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:23"
