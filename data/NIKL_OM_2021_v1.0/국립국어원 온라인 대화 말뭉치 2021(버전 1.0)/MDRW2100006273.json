          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006273.1.24",
          "form": "얼마 버셨어요?",
          "original_form": "얼마 버셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006273.1.25",
          "form": "멋있나요?",
          "original_form": "멋있나요?",
