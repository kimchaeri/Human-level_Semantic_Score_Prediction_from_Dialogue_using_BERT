          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006273.1.15",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006273.1.16",
          "form": "일 하셨어요?",
          "original_form": "일 하셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
