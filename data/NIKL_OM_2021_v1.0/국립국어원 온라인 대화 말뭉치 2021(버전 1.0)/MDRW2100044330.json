      "utterance": [
        {
          "id": "MDRW2100044330.1.1",
          "form": "작가 너무한거 아니가",
          "original_form": "작가 너무한거 아니가",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044330.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ심햇재",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ심햇재",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044330.1.3",
          "form": "그 아버지 친자식은 맨 윗오빠뿐이래",
          "original_form": "그 아버지 친자식은 맨 윗오빠뿐이래",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044330.1.4",
          "form": "내가 써도 그거보단 개연성있다 ㅋㅋㅋㅋㅋ",
          "original_form": "내가 써도 그거보단 개연성있다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044330.1.5",
          "form": "오빠 있었음?",
          "original_form": "오빠 있었음?",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044330.1.6",
          "form": "근데 그 오빠 일찍 쥬금 ㅜ",
          "original_form": "근데 그 오빠 일찍 쥬금 ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044330.1.7",
          "form": "와...아버지....ㅠㅠ",
          "original_form": "와...아버지....ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044330.1.8",
          "form": "세상마상이네 진짜 ㅋㅋㅋ",
          "original_form": "세상마상이네 진짜 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044330.1.9",
          "form": "ㅇㅇ그 드라마초반에 엄마 바람펴서",
          "original_form": "ㅇㅇ그 드라마초반에 엄마 바람펴서",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044330.1.10",
          "form": "엄마 팜므파탈 쩔었엇네",
          "original_form": "엄마 팜므파탈 쩔었엇네",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044330.1.11",
          "form": "스스로 쥬금",
          "original_form": "스스로 쥬금",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044330.1.12",
          "form": "?.....",
          "original_form": "?.....",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044330.1.13",
          "form": "띠용스러운 전개네 ㅋㅋㅋ",
          "original_form": "띠용스러운 전개네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044330.1.14",
          "form": "첨에 타살인줄알고 막 범인찾기로",
          "original_form": "첨에 타살인줄알고 막 범인찾기로",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044330.1.15",
          "form": "드라마시작햇는데 걍 바람펴서 죄책감에 쥬것나?그럴걸..",
          "original_form": "드라마시작햇는데 걍 바람펴서 죄책감에 쥬것나?그럴걸..",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044330.1.16",
          "form": "ㄹㅇ 막장드라마 ㅋㅋㅋㅋ",
          "original_form": "ㄹㅇ 막장드라마 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044330.1.17",
          "form": "순옥킴 뺨치네",
          "original_form": "순옥킴 뺨치네",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044330.1.18",
          "form": "그러니깤ㅋㅋㅋㅋㅋ",
          "original_form": "그러니깤ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044330.1.19",
          "form": "마지막에 다 배다른남매에서",
          "original_form": "마지막에 다 배다른남매에서",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044330.1.20",
          "form": "놀랍ㅋㅋㅋㅋ",
          "original_form": "놀랍ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044330.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ어머니들이 챙겨보는 이유잇다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어머니들이 챙겨보는 이유잇다",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044330.1.22",
          "form": "너무 흥미진진해섴ㅋㅋ",
          "original_form": "너무 흥미진진해섴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044330.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그러니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그러니까",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044330.1.24",
          "form": "상상도 못한 전개 ㄴㅇㄱ",
          "original_form": "상상도 못한 전개 ㄴㅇㄱ",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044330.1.25",
          "form": "뇨즘 드라마 보는거잇나",
          "original_form": "뇨즘 드라마 보는거잇나",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044330.1.26",
          "form": "챙겨보는 건 없구",
          "original_form": "챙겨보는 건 없구",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044330.1.27",
          "form": "어제 우수무당 가두심?",
          "original_form": "어제 우수무당 가두심?",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044330.1.28",
