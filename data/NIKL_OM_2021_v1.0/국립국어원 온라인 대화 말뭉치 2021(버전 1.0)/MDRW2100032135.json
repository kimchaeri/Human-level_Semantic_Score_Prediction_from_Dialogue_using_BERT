      "utterance": [
        {
          "id": "MDRW2100032135.1.1",
          "form": "점심 때인데 밥은 드셨나요??",
          "original_form": "점심 때인데 밥은 드셨나요??",
          "speaker_id": "1",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100032135.1.2",
          "form": "앗 아뇨... 대청소중이에요ㅋㅋㅋ",
          "original_form": "앗 아뇨... 대청소중이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100032135.1.3",
          "form": "으앜ㅋㅋㅋㅋㅋㅋ 언제 끝나나요?",
          "original_form": "으앜ㅋㅋㅋㅋㅋㅋ 언제 끝나나요?",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032135.1.4",
          "form": "빨리 밥 드셔야죠 !!",
          "original_form": "빨리 밥 드셔야죠 !!",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032135.1.5",
          "form": "지금..막바지입니다..",
          "original_form": "지금..막바지입니다..",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100032135.1.6",
          "form": "족발이 넘 먹구싶어요",
          "original_form": "족발이 넘 먹구싶어요",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100032135.1.7",
          "form": "대청소한 name2님을 위해 셀프 족잘 선물 어때요",
          "original_form": "대청소한 &name2&님을 위해 셀프 족잘 선물 어때요",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100032135.1.8",
          "form": "그럴까요 이따 저녁에 족발 먹어야겠어요",
          "original_form": "그럴까요 이따 저녁에 족발 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100032135.1.9",
          "form": "점심 드셨나요!!",
          "original_form": "점심 드셨나요!!",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100032135.1.10",
          "form": "김볶밥 !!",
          "original_form": "김볶밥 !!",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100032135.1.11",
          "form": "여기에 양파와 스팸을 넣은!!",
          "original_form": "여기에 양파와 스팸을 넣은!!",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100032135.1.12",
          "form": "어제 밤부터 엄청 먹고싶더라구오",
          "original_form": "어제 밤부터 엄청 먹고싶더라구오",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100032135.1.13",
          "form": "아침에 일어나자 마자 쓱싹해먹었죠",
          "original_form": "아침에 일어나자 마자 쓱싹해먹었죠",
          "speaker_id": "1",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100032135.1.14",
          "form": "name2님은 저녁 족발이시면 점심은 김볶밥 어떠세요",
          "original_form": "&name2&님은 저녁 족발이시면 점심은 김볶밥 어떠세요",
          "speaker_id": "1",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100032135.1.15",
          "form": "헉 김볶밥ㅠㅡㅠ",
          "original_form": "헉 김볶밥ㅠㅡㅠ",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032135.1.16",
          "form": "요리 잘하시죠..그쵸...!!",
          "original_form": "요리 잘하시죠..그쵸...!!",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032135.1.17",
          "form": "점심 좀 눌러붙은 김볶밥..만들기 도전...ㅎ",
          "original_form": "점심 좀 눌러붙은 김볶밥..만들기 도전...ㅎ",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032135.1.18",
          "form": "간이 실패서 삼삼...했어요...",
          "original_form": "간이 실패서 삼삼...했어요...",
          "speaker_id": "1",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100032135.1.19",
          "form": "요리 잘하고 싶은 사람입니다",
          "original_form": "요리 잘하고 싶은 사람입니다",
          "speaker_id": "1",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100032135.1.20",
          "form": "ㅋㅋㅋㅋ 아니 그런데 욾님 왜 벌써 김볶밥이 눌러붙은거죠",
          "original_form": "ㅋㅋㅋㅋ 아니 그런데 욾님 왜 벌써 김볶밥이 눌러붙은거죠",
          "speaker_id": "1",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100032135.1.21",
          "form": "요알못은 볶음밥을 눌어붙게 만듭니다..",
          "original_form": "요알못은 볶음밥을 눌어붙게 만듭니다..",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032135.1.22",
          "form": "흑흑..",
          "original_form": "흑흑..",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032135.1.23",
          "form": "누가 해주는 음식이 최고에요",
          "original_form": "누가 해주는 음식이 최고에요",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032135.1.24",
          "form": "아 맞아요 ㅋㅋㅋㅋ 그래서 저도 배달음식 러버가 되었죠..",
          "original_form": "아 맞아요 ㅋㅋㅋㅋ 그래서 저도 배달음식 러버가 되었죠..",
          "speaker_id": "1",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032135.1.25",
          "form": "청소는 지금 끝나셨나요??",
          "original_form": "청소는 지금 끝나셨나요??",
          "speaker_id": "1",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032135.1.26",
          "form": "name1님의 최애 음식은 뭔가요",
          "original_form": "&name1&님의 최애 음식은 뭔가요",
          "speaker_id": "2",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032135.1.27",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032135.1.28",
          "form": "청소 끝~",
          "original_form": "청소 끝~",
          "speaker_id": "2",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032135.1.29",
          "form": "아 요즘 맛있는 음식이 너무 많아서 가릴 수가 없어요 ㅠㅠ",
          "original_form": "아 요즘 맛있는 음식이 너무 많아서 가릴 수가 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032135.1.30",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032135.1.31",
          "form": "냉면 최고입니다..냉면 냉면 냉면 냉면이 세계를 지배한다",
          "original_form": "냉면 최고입니다..냉면 냉면 냉면 냉면이 세계를 지배한다",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032135.1.32",
          "form": "냉면 믿으세요 냉면 믿으셔야해요",
          "original_form": "냉면 믿으세요 냉면 믿으셔야해요",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032135.1.33",
          "form": "냉면 좋지만 이제 날이 추워져서 ~~",
          "original_form": "냉면 좋지만 이제 날이 추워져서 ~~",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032135.1.34",
          "form": "냉면은 여름에 많이 먹었죠",
          "original_form": "냉면은 여름에 많이 먹었죠",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032135.1.35",
          "form": "...어떻게 그런..말씀을..",
          "original_form": "...어떻게 그런..말씀을..",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032135.1.36",
          "form": "냉면은..사시사철...",
          "original_form": "냉면은..사시사철...",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032135.1.37",
          "form": "속상해요..",
          "original_form": "속상해요..",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032135.1.38",
          "form": "겨울에 냉면은 좀 힘들죠 ! 머리도 띵해지고 !!",
          "original_form": "겨울에 냉면은 좀 힘들죠 ! 머리도 띵해지고 !!",
          "speaker_id": "1",
