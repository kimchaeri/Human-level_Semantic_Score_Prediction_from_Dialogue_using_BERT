      "utterance": [
        {
          "id": "MDRW2100045245.1.1",
          "form": "엇 아니용 찌개로 끝납니당",
          "original_form": "엇 아니용 찌개로 끝납니당",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045245.1.2",
          "form": "정답!!! 김치찌개!!!!",
          "original_form": "정답!!! 김치찌개!!!!",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045245.1.3",
          "form": "오 맞아여 저녁으로 김치지개 먹었어요",
          "original_form": "오 맞아여 저녁으로 김치지개 먹었어요",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045245.1.4",
          "form": "쌀밥이랑",
          "original_form": "쌀밥이랑",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045245.1.5",
          "form": "아싸 정답 ㅎㅎㅎㅎㅎ",
          "original_form": "아싸 정답 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045245.1.6",
          "form": "김치찌개 맛있죠",
          "original_form": "김치찌개 맛있죠",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045245.1.7",
          "form": "이번엔 제 저녁메뉴 맞춰보시겠어요?",
          "original_form": "이번엔 제 저녁메뉴 맞춰보시겠어요?",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045245.1.8",
          "form": "음 요리해서 드셨나용?",
          "original_form": "음 요리해서 드셨나용?",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045245.1.9",
          "form": "네! 직접 만들어 먹었답니다",
          "original_form": "네! 직접 만들어 먹었답니다",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045245.1.10",
          "form": "국물이 있겠죠? 아무래도?",
          "original_form": "국물이 있겠죠? 아무래도?",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045245.1.11",
          "form": "아니요 국물 없는 요리였습니다ㅎㅎ",
          "original_form": "아니요 국물 없는 요리였습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045245.1.12",
          "form": "흠... 그럼 매운 요리인가요?",
          "original_form": "흠... 그럼 매운 요리인가요?",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045245.1.13",
          "form": "아니요 전혀 안 매운 요리예요!",
          "original_form": "아니요 전혀 안 매운 요리예요!",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045245.1.14",
          "form": "고기가 들어가겠죠??",
          "original_form": "고기가 들어가겠죠??",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045245.1.15",
          "form": "아뇨! 고기가 안 들어가는 요리입니다",
          "original_form": "아뇨! 고기가 안 들어가는 요리입니다",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045245.1.16",
          "form": "힌트좀 주세여ㅠㅠ 모르겠어요.. 카레..?",
          "original_form": "힌트좀 주세여ㅠㅠ 모르겠어요.. 카레..?",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045245.1.17",
          "form": "ㅋㅋㅋㅋ카레처럼 노란 색도 들어가요",
          "original_form": "ㅋㅋㅋㅋ카레처럼 노란 색도 들어가요",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045245.1.18",
          "form": "야채로만 만들어요",
          "original_form": "야채로만 만들어요",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045245.1.19",
          "form": "저 요리를 못해가지구ㅠ 제가 모르는 요리는 아닐까요 ㅋㅋㅋ큐ㅠ 뭐죠..",
          "original_form": "저 요리를 못해가지구ㅠ 제가 모르는 요리는 아닐까요 ㅋㅋㅋ큐ㅠ 뭐죠..",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045245.1.20",
          "form": "카레 비슷한 노란 음식이라니",
          "original_form": "카레 비슷한 노란 음식이라니",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045245.1.21",
          "form": "힌트! 토마토가 들어가요!",
          "original_form": "힌트! 토마토가 들어가요!",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045245.1.22",
          "form": "힌트! 노란 것도 들어가고 토마토가 들어가는 요리입니다",
          "original_form": "힌트! 노란 것도 들어가고 토마토가 들어가는 요리입니다",
          "speaker_id": "1",
          "time": "20210908 01:36"
        }
      ]
