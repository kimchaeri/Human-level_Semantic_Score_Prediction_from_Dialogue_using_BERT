        },
        {
          "id": "MDRW2100016079.1.2",
          "form": "집안일 뭐 하고왔어요",
          "original_form": "집안일 뭐 하고왔어요",
          "speaker_id": "1",
          "time": "20210826 09:17"
        },
        {
          "id": "MDRW2100016079.1.3",
          "form": "설거지는 아까하셨다 그러셨고",
          "original_form": "설거지는 아까하셨다 그러셨고",
          "speaker_id": "1",
          "time": "20210826 09:17"
        },
        {
          "id": "MDRW2100016079.1.4",
          "form": "설거지하고 식탁닦고",
          "original_form": "설거지하고 식탁닦고",
          "speaker_id": "2",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016079.1.5",
          "form": "행주빨공",
          "original_form": "행주빨공",
          "speaker_id": "2",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016079.1.6",
          "form": "말리고",
          "original_form": "말리고",
          "speaker_id": "2",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016079.1.7",
          "form": "나왔어요",
          "original_form": "나왔어요",
          "speaker_id": "2",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016079.1.8",
          "form": "행주 삶았어요?",
          "original_form": "행주 삶았어요?",
          "speaker_id": "1",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016079.1.9",
          "form": "삶기까지는",
          "original_form": "삶기까지는",
          "speaker_id": "2",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016079.1.10",
          "form": "조금",
          "original_form": "조금",
          "speaker_id": "2",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016079.1.11",
          "form": "그거까진 안하셨구나ㅋㅋ",
          "original_form": "그거까진 안하셨구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016079.1.12",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016079.1.13",
          "form": "동생이랑은 사이가 좋은가요",
          "original_form": "동생이랑은 사이가 좋은가요",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016079.1.14",
          "form": "그냥 보통요",
          "original_form": "그냥 보통요",
          "speaker_id": "2",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016079.1.15",
          "form": "좋았다가 나빴다가 하죠",
          "original_form": "좋았다가 나빴다가 하죠",
          "speaker_id": "2",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016079.1.16",
          "form": "다들 그런가봐요",
          "original_form": "다들 그런가봐요",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016079.1.17",
          "form": "저도 그렇거든요",
          "original_form": "저도 그렇거든요",
          "speaker_id": "1",
          "time": "20210826 09:26"
        },
        {
          "id": "MDRW2100016079.1.18",
          "form": "청소는 분담해서 하시나요",
          "original_form": "청소는 분담해서 하시나요",
          "speaker_id": "1",
          "time": "20210826 09:30"
        },
        {
          "id": "MDRW2100016079.1.19",
          "form": "2번씩 오네요 우앗",
          "original_form": "2번씩 오네요 우앗",
          "speaker_id": "2",
          "time": "20210826 10:06"
        },
        {
          "id": "MDRW2100016079.1.20",
          "form": "답변늦어서 죄송해요",
          "original_form": "답변늦어서 죄송해요",
          "speaker_id": "2",
          "time": "20210826 10:06"
        },
        {
          "id": "MDRW2100016079.1.21",
          "form": "저희집은 분담해서 하는 편인데",
          "original_form": "저희집은 분담해서 하는 편인데",
          "speaker_id": "2",
          "time": "20210826 10:06"
        },
        {
          "id": "MDRW2100016079.1.22",
          "form": "엄마가 가사노동을 극도로 싫어하셔서",
          "original_form": "엄마가 가사노동을 극도로 싫어하셔서",
          "speaker_id": "2",
          "time": "20210826 10:07"
        },
        {
          "id": "MDRW2100016079.1.23",
          "form": "좀.  그래요",
          "original_form": "좀.  그래요",
          "speaker_id": "2",
          "time": "20210826 10:07"
        },
        {
          "id": "MDRW2100016079.1.24",
          "form": "괜찮아요ㅎ",
          "original_form": "괜찮아요ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:07"
        },
        {
          "id": "MDRW2100016079.1.25",
          "form": "그럼 주로 name2이 하시냐오",
          "original_form": "그럼 주로 &name2&이 하시냐오",
          "speaker_id": "1",
          "time": "20210826 10:07"
        },
        {
          "id": "MDRW2100016079.1.26",
          "form": "아빠는 분리수거",
          "original_form": "아빠는 분리수거",
          "speaker_id": "2",
          "time": "20210826 10:08"
        },
        {
          "id": "MDRW2100016079.1.27",
          "form": "저는 주로 설거지하는것같아요",
          "original_form": "저는 주로 설거지하는것같아요",
          "speaker_id": "2",
          "time": "20210826 10:08"
        },
        {
          "id": "MDRW2100016079.1.28",
          "form": "분리수거 너무 귀찮은데 그걸 아버님이 하시는구나",
          "original_form": "분리수거 너무 귀찮은데 그걸 아버님이 하시는구나",
          "speaker_id": "1",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016079.1.29",
          "form": "엄마가 물 걸 레 를 하세요",
          "original_form": "엄마가 물 걸 레 를 하세요",
          "speaker_id": "2",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016079.1.30",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016079.1.31",
          "form": "어 바닥닦기..? 라고 하죠",
          "original_form": "어 바닥닦기..? 라고 하죠",
          "speaker_id": "1",
          "time": "20210826 10:09"
        },
        {
          "id": "MDRW2100016079.1.32",
          "form": "음 역할분담이 잘 되어있으시구나",
          "original_form": "음 역할분담이 잘 되어있으시구나",
