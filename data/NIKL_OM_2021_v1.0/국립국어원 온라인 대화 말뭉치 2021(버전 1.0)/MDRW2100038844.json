      "utterance": [
        {
          "id": "MDRW2100038844.1.1",
          "form": "이엘이 세남매중에서 첫째",
          "original_form": "이엘이 세남매중에서 첫째",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038844.1.2",
          "form": "둘째,셋째는 누구예요?",
          "original_form": "둘째,셋째는 누구예요?",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038844.1.3",
          "form": "이민기 김지원이 동생들이고",
          "original_form": "이민기 김지원이 동생들이고",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038844.1.4",
          "form": "아 남매들이네요. 난 둘이 연인관계인줄 알았네요",
          "original_form": "아 남매들이네요. 난 둘이 연인관계인줄 알았네요",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038844.1.5",
          "form": "김석윤 피디가 연출이네\r\n 로스쿨 분이 부시게 올드미스다이어리 연출",
          "original_form": "김석윤 피디가 연출이네\r\n 로스쿨 분이 부시게 올드미스다이어리 연출",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100038844.1.6",
          "form": "올드미스다이어리 추억 돋네요",
          "original_form": "올드미스다이어리 추억 돋네요",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100038844.1.7",
          "form": "그러게 나두 남주여주인줄 알았음",
          "original_form": "그러게 나두 남주여주인줄 알았음",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100038844.1.8",
          "form": "그럼 멜로는 없나보네요",
          "original_form": "그럼 멜로는 없나보네요",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100038844.1.9",
          "form": "손석구는 최근에 넷플릭스에도 출연했네",
          "original_form": "손석구는 최근에 넷플릭스에도 출연했네",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100038844.1.10",
          "form": "진짜 넷플릭스 드라마도 많던데",
          "original_form": "진짜 넷플릭스 드라마도 많던데",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100038844.1.11",
          "form": "글쎄 우리나라에는 멜로가 빠질수 없는데 ㅋㅋ",
          "original_form": "글쎄 우리나라에는 멜로가 빠질수 없는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100038844.1.12",
          "form": "그니깐요 보면 또 있겠죠?",
          "original_form": "그니깐요 보면 또 있겠죠?",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100038844.1.13",
          "form": "응 손석구랑 있을듯",
          "original_form": "응 손석구랑 있을듯",
          "speaker_id": "2",
          "time": "20210902 22:09"
        }
      ]
