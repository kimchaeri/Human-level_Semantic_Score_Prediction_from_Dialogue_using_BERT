      "utterance": [
        {
          "id": "MDRW2100040208.1.1",
          "form": "우와 참외조아하시나요",
          "original_form": "우와 참외조아하시나요",
          "speaker_id": "2",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100040208.1.2",
          "form": "저는 안좋아하는데 엄마는 좋아하세용",
          "original_form": "저는 안좋아하는데 엄마는 좋아하세용",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100040208.1.3",
          "form": "저는 수박좋아하는데 이제 여름 끝나서 슬퍼요",
          "original_form": "저는 수박좋아하는데 이제 여름 끝나서 슬퍼요",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100040208.1.4",
          "form": "멜론은요",
          "original_form": "멜론은요",
          "speaker_id": "2",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100040208.1.5",
          "form": "멜론은 좋아하는데 이것도 씨 가까운부분먹으면",
          "original_form": "멜론은 좋아하는데 이것도 씨 가까운부분먹으면",
          "speaker_id": "1",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100040208.1.6",
          "form": "목아파요ㅠ 식도 좁아지는느낌",
          "original_form": "목아파요ㅠ 식도 좁아지는느낌",
          "speaker_id": "1",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100040208.1.7",
          "form": "허걱 알러지?",
          "original_form": "허걱 알러지?",
          "speaker_id": "2",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100040208.1.8",
          "form": "네 근데 검색해보니까 한국인중에 이 알러지 되게 많은데 그냥 다들 참고 먹는대요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 근데 검색해보니까 한국인중에 이 알러지 되게 많은데 그냥 다들 참고 먹는대요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100040208.1.9",
          "form": "헉 글쿠나 모르고먹을듯",
          "original_form": "헉 글쿠나 모르고먹을듯",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100040208.1.10",
          "form": "그초 그냥 아 이게 멜론 맛이구나 ㅋㅋ달콤한 맛 뒤에 까끌한게 멜론이구나",
          "original_form": "그초 그냥 아 이게 멜론 맛이구나 ㅋㅋ달콤한 맛 뒤에 까끌한게 멜론이구나",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100040208.1.11",
          "form": "하구 먹는대요",
          "original_form": "하구 먹는대요",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100040208.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100040208.1.13",
          "form": "바보들",
          "original_form": "바보들",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100040208.1.14",
          "form": "멜론 먹고시펑ㅎ",
          "original_form": "멜론 먹고시펑ㅎ",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100040208.1.15",
          "form": "name2님은 멜론먹고 목아픈적없나요",
          "original_form": "&name2&님은 멜론먹고 목아픈적없나요",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100040208.1.16",
          "form": "네 업서요",
          "original_form": "네 업서요",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100040208.1.17",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100040208.1.18",
          "form": "부러워용 멜론수저시군요",
          "original_form": "부러워용 멜론수저시군요",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100040208.1.19",
          "form": "멜론빙수 조아해요",
