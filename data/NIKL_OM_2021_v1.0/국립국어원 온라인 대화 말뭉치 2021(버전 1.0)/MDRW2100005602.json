      "utterance": [
        {
          "id": "MDRW2100005602.1.1",
          "form": "넷플릭스 슬기로운의사생활 다봤어??",
          "original_form": "넷플릭스 슬기로운의사생활 다봤어??",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005602.1.2",
          "form": "응 다봐써ㅠㅠ",
          "original_form": "응 다봐써ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005602.1.3",
          "form": "다음주언제와",
          "original_form": "다음주언제와",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005602.1.4",
          "form": "몇화까지나온거야",
          "original_form": "몇화까지나온거야",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005602.1.5",
          "form": "하... ",
          "original_form": "하... ",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005602.1.6",
          "form": "얼마나남았어",
          "original_form": "얼마나남았어",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005602.1.7",
          "form": "얼마나남았더라",
          "original_form": "얼마나남았더라",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005602.1.8",
          "form": "3회남았다",
          "original_form": "3회남았다",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005602.1.9",
          "form": "그래도 거의 다 끝났넹",
          "original_form": "그래도 거의 다 끝났넹",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005602.1.10",
          "form": "요즘 노래 뭐들어",
          "original_form": "요즘 노래 뭐들어",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005602.1.11",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005602.1.12",
          "form": "노래잘안들어",
          "original_form": "노래잘안들어",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005602.1.13",
          "form": "오빠는뭐들어",
          "original_form": "오빠는뭐들어",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005602.1.14",
          "form": "나는 악동뮤지션 노래 많이들었어 이번에 나온거",
          "original_form": "나는 악동뮤지션 노래 많이들었어 이번에 나온거",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005602.1.15",
          "form": "이번이라하긴 벌써 시간이 많이 지났군",
          "original_form": "이번이라하긴 벌써 시간이 많이 지났군",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005602.1.16",
          "form": "유튜브는 뭐봐",
          "original_form": "유튜브는 뭐봐",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005602.1.17",
          "form": "얼마전에무한도전도보구",
          "original_form": "얼마전에무한도전도보구",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005602.1.18",
          "form": "그 네이버멤버십",
          "original_form": "그 네이버멤버십",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005602.1.19",
          "form": "티빙으로",
          "original_form": "티빙으로",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005602.1.20",
          "form": "꽃할배유럽편이랑",
          "original_form": "꽃할배유럽편이랑",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005602.1.21",
          "form": "식스센스보고있어",
          "original_form": "식스센스보고있어",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005602.1.22",
          "form": "나 영화보고싶은거있어",
          "original_form": "나 영화보고싶은거있어",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005602.1.23",
          "form": "뭐??",
          "original_form": "뭐??",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005602.1.24",
          "form": "요즘 하나 영화가 볼만한게",
          "original_form": "요즘 하나 영화가 볼만한게",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005602.1.25",
          "form": "아 아까 유튜브에 예랑가랑 만우절날 찍은거 알고리즘에 떴는데",
          "original_form": "아 아까 유튜브에 예랑가랑 만우절날 찍은거 알고리즘에 떴는데",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005602.1.26",
          "form": "둘이 역할바꿔서 하더라 ㅋㅋㅋㅋ",
          "original_form": "둘이 역할바꿔서 하더라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005602.1.27",
          "form": "너는 이미 봤을듯",
          "original_form": "너는 이미 봤을듯",
          "speaker_id": "1",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005602.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005602.1.29",
          "form": "난이미봤지",
          "original_form": "난이미봤지",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005602.1.30",
          "form": "모디가슈인지",
          "original_form": "모디가슈인지",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005602.1.31",
