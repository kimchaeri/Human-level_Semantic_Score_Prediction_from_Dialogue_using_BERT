        {
          "id": "MDRW2100047369.1.1",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100047369.1.2",
          "form": "아무래도 배달안시키면",
          "original_form": "아무래도 배달안시키면",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100047369.1.3",
          "form": "일회용품도 없으시겠어요",
          "original_form": "일회용품도 없으시겠어요",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100047369.1.4",
          "form": "요리하면 쩔수없이 나오는데",
          "original_form": "요리하면 쩔수없이 나오는데",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100047369.1.5",
          "form": "일회용품은 별로없어요",
          "original_form": "일회용품은 별로없어요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100047369.1.6",
          "form": "그러겠어요",
          "original_form": "그러겠어요",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047369.1.7",
          "form": "저는 엄청나와요ㅠ",
          "original_form": "저는 엄청나와요ㅠ",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047369.1.8",
          "form": "분리수거힘드시겠다",
          "original_form": "분리수거힘드시겠다",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047369.1.9",
          "form": "정말귀찮죠ㅋㅋㅋ",
          "original_form": "정말귀찮죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047369.1.10",
          "form": "먹는 쓰레가 젤많은거 같아요 청결해야하니까용ㅜ",
          "original_form": "먹는 쓰레가 젤많은거 같아요 청결해야하니까용ㅜ",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100047369.1.11",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100047369.1.12",
          "form": "하나하나오니까",
          "original_form": "하나하나오니까",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100047369.1.13",
          "form": "까서버리기 진짜귀찮ㅇㅏ요",
          "original_form": "까서버리기 진짜귀찮ㅇㅏ요",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100047369.1.14",
          "form": "ㅋㅋㅋ 이럴땐 기름진텃밭이 부럽네요 캐오면됨",
          "original_form": "ㅋㅋㅋ 이럴땐 기름진텃밭이 부럽네요 캐오면됨",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100047369.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100047369.1.16",
          "form": "상추따고 깻잎따고요?",
          "original_form": "상추따고 깻잎따고요?",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100047369.1.17",
          "form": "너무좋죠 ㅋㅋ 가지랑 토마토랑 피망두요ㅎㅎ",
          "original_form": "너무좋죠 ㅋㅋ 가지랑 토마토랑 피망두요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100047369.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100047369.1.19",
          "form": "파프리카도요",
          "original_form": "파프리카도요",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100047369.1.20",
          "form": "제가좋아하거든요",
          "original_form": "제가좋아하거든요",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100047369.1.21",
          "form": "아 너무좋죠 ㅋㅋ 신선하게 먹을수있죠",
          "original_form": "아 너무좋죠 ㅋㅋ 신선하게 먹을수있죠",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100047369.1.22",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
