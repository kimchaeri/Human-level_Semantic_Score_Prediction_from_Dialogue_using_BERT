      "utterance": [
        {
          "id": "MDRW2100037542.1.1",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 09:49"
        },
        {
          "id": "MDRW2100037542.1.2",
          "form": "안먹었어요 name1님은 드셨나요?",
          "original_form": "안먹었어요 &name1&님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 09:50"
        },
        {
          "id": "MDRW2100037542.1.3",
          "form": "저두요ㅎ",
          "original_form": "저두요ㅎ",
          "speaker_id": "1",
          "time": "20210901 09:50"
        },
        {
          "id": "MDRW2100037542.1.4",
          "form": "안먹는 이유가 있으세요?",
          "original_form": "안먹는 이유가 있으세요?",
          "speaker_id": "1",
          "time": "20210901 09:50"
        },
        {
          "id": "MDRW2100037542.1.5",
          "form": "저는 그냥 아침에 뭘 먹으면 별로더라고여",
          "original_form": "저는 그냥 아침에 뭘 먹으면 별로더라고여",
          "speaker_id": "2",
          "time": "20210901 09:51"
        },
        {
          "id": "MDRW2100037542.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 09:51"
        },
        {
          "id": "MDRW2100037542.1.7",
          "form": "저는 아침 먹는 시간 대신에",
          "original_form": "저는 아침 먹는 시간 대신에",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100037542.1.8",
          "form": "잠을 선택했어요ㅋㅋㅋㅋㅋㅋㅠ",
          "original_form": "잠을 선택했어요ㅋㅋㅋㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100037542.1.9",
          "form": "그러무배 안고프세요?",
          "original_form": "그러무배 안고프세요?",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100037542.1.10",
          "form": "아직까지는 괜찮네요ㅋㅋㅋㅋ",
          "original_form": "아직까지는 괜찮네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100037542.1.11",
          "form": "전 배고파요ㅠㅠㅠㅠㅠ",
          "original_form": "전 배고파요ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:02"
        },
        {
          "id": "MDRW2100037542.1.12",
          "form": "그러면 초콜렛같은 거라도 드세요",
          "original_form": "그러면 초콜렛같은 거라도 드세요",
          "speaker_id": "2",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100037542.1.13",
          "form": "그러고싶은데..",
          "original_form": "그러고싶은데..",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100037542.1.14",
          "form": "없네요 ....",
          "original_form": "없네요 ....",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100037542.1.15",
          "form": "그렇군요ㅠㅠ\r\n그럼 좋아하는 음식 있으신가요?",
          "original_form": "그렇군요ㅠㅠ\r\n그럼 좋아하는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100037542.1.16",
          "form": "저 다 좋지만",
          "original_form": "저 다 좋지만",
          "speaker_id": "1",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100037542.1.17",
          "form": "지금은 떡볶이가 먹고싶네요 ㅎ",
          "original_form": "지금은 떡볶이가 먹고싶네요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100037542.1.18",
          "form": "어떤 브랜드 떡볶이 좋아하시나요?",
          "original_form": "어떤 브랜드 떡볶이 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100037542.1.19",
          "form": "저 고루거루 다 좋아해요",
          "original_form": "저 고루거루 다 좋아해요",
          "speaker_id": "1",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100037542.1.20",
          "form": "딱 좋아하는 곳 있나요?",
          "original_form": "딱 좋아하는 곳 있나요?",
          "speaker_id": "1",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100037542.1.21",
          "form": "저는 엽떡이요",
          "original_form": "저는 엽떡이요",
          "speaker_id": "2",
          "time": "20210901 11:24"
        }
      ]
