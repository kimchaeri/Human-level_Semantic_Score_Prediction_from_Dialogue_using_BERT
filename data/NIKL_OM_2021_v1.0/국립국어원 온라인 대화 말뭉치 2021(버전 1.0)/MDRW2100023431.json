      "utterance": [
        {
          "id": "MDRW2100023431.1.1",
          "form": "특히좋아하는 배달 메뉴있으세요.?",
          "original_form": "특히좋아하는 배달 메뉴있으세요.?",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023431.1.2",
          "form": "치킨을 자주먹어요",
          "original_form": "치킨을 자주먹어요",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023431.1.3",
          "form": "피자도 좋구요",
          "original_form": "피자도 좋구요",
          "speaker_id": "2",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023431.1.4",
          "form": "맛있지요...ㅜㅜㅜㅜ",
          "original_form": "맛있지요...ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023431.1.5",
          "form": "새벽에얘기하니배고파지네요ㅎㅎ",
          "original_form": "새벽에얘기하니배고파지네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023431.1.6",
          "form": "초밥이나 떡볶이 이런것도 가끔 시켜요",
          "original_form": "초밥이나 떡볶이 이런것도 가끔 시켜요",
          "speaker_id": "2",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023431.1.7",
          "form": "근데 떡볶이는 너무 비싼거같아요",
          "original_form": "근데 떡볶이는 너무 비싼거같아요",
          "speaker_id": "2",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023431.1.8",
          "form": "어느순간 고급화가 된 느낌",
          "original_form": "어느순간 고급화가 된 느낌",
          "speaker_id": "2",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023431.1.9",
          "form": "네 말도안돼요 너무  큰통에 15000원 그냥 넘어버리니까 ...",
          "original_form": "네 말도안돼요 너무  큰통에 15000원 그냥 넘어버리니까 ...",
          "speaker_id": "1",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023431.1.10",
          "form": "저는 그래서 주로 신전시켜먹어요..ㅎㅎ",
          "original_form": "저는 그래서 주로 신전시켜먹어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023431.1.11",
          "form": "신전떡볶이는 머가 좀 다릅니까?",
          "original_form": "신전떡볶이는 머가 좀 다릅니까?",
          "speaker_id": "2",
          "time": "20210830 02:23"
        },
        {
          "id": "MDRW2100023431.1.12",
          "form": "그냥 가격대가 무난해요\r\n3000원대엿나..그럴거예요! \r\n그래서 다른 사이드랑 이것저것 시키면 돼요!  그게 좋아요 !!",
          "original_form": "그냥 가격대가 무난해요\r\n3000원대엿나..그럴거예요! \r\n그래서 다른 사이드랑 이것저것 시키면 돼요!  그게 좋아요 !!",
          "speaker_id": "1",
          "time": "20210830 02:25"
        },
        {
          "id": "MDRW2100023431.1.13",
          "form": "다음에 신전한번 시켜볼게요",
          "original_form": "다음에 신전한번 시켜볼게요",
          "speaker_id": "2",
          "time": "20210830 02:26"
        },
        {
          "id": "MDRW2100023431.1.14",
          "form": "name1이 추천해주셨으니",
