      "utterance": [
        {
          "id": "MDRW2100046865.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211108 00:40"
        },
        {
          "id": "MDRW2100046865.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20211108 00:41"
        },
        {
          "id": "MDRW2100046865.1.3",
          "form": "코로나 좀 풀리면 여행 어디 가고 싶으세요?",
          "original_form": "코로나 좀 풀리면 여행 어디 가고 싶으세요?",
          "speaker_id": "2",
          "time": "20211108 00:41"
        },
        {
          "id": "MDRW2100046865.1.4",
          "form": "통영 이요",
          "original_form": "통영 이요",
          "speaker_id": "1",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.5",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.6",
          "form": "국내 대답은 처음이에요 ㅋㅋㅋ",
          "original_form": "국내 대답은 처음이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.7",
          "form": "통영은 왜요?",
          "original_form": "통영은 왜요?",
          "speaker_id": "2",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.8",
          "form": "이제 곧 굴철 이지 않습니까",
          "original_form": "이제 곧 굴철 이지 않습니까",
          "speaker_id": "1",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.10",
          "form": "맛집여행 하고 싶습니다",
          "original_form": "맛집여행 하고 싶습니다",
          "speaker_id": "1",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.11",
          "form": "그정도로",
          "original_form": "그정도로",
          "speaker_id": "2",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.12",
          "form": "굴을 좋아하시는 거예요?",
          "original_form": "굴을 좋아하시는 거예요?",
          "speaker_id": "2",
          "time": "20211108 00:42"
        },
        {
          "id": "MDRW2100046865.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211108 00:43"
        },
        {
          "id": "MDRW2100046865.1.14",
          "form": "굴 외에도 통영의 매력이 또 있을까요?",
          "original_form": "굴 외에도 통영의 매력이 또 있을까요?",
          "speaker_id": "2",
          "time": "20211108 00:43"
        },
        {
          "id": "MDRW2100046865.1.15",
          "form": "근처가 바다라서",
          "original_form": "근처가 바다라서",
          "speaker_id": "1",
          "time": "20211108 00:43"
        },
        {
          "id": "MDRW2100046865.1.16",
          "form": "바닷가 구경도 할수 있지않을까요",
          "original_form": "바닷가 구경도 할수 있지않을까요",
          "speaker_id": "1",
          "time": "20211108 00:43"
        },
        {
          "id": "MDRW2100046865.1.17",
          "form": "쌀쌀해진 굴철에",
          "original_form": "쌀쌀해진 굴철에",
          "speaker_id": "2",
          "time": "20211108 00:44"
        },
        {
          "id": "MDRW2100046865.1.18",
          "form": "바닷가에 들아가시지는 않을 거죠? ㅎㅎ",
          "original_form": "바닷가에 들아가시지는 않을 거죠? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 00:44"
        },
        {
          "id": "MDRW2100046865.1.19",
          "form": "추어서 들어가기는 힘들어요",
          "original_form": "추어서 들어가기는 힘들어요",
          "speaker_id": "1",
          "time": "20211108 00:44"
        },
        {
          "id": "MDRW2100046865.1.20",
          "form": "(크크)",
          "original_form": "(크크)",
          "speaker_id": "2",
          "time": "20211108 00:45"
        },
        {
          "id": "MDRW2100046865.1.21",
          "form": "얼른 코로나 걱정 없이 좋아하시는 굴 마음껏 누리러 여행 가실 수 있길 바랄게요",
          "original_form": "얼른 코로나 걱정 없이 좋아하시는 굴 마음껏 누리러 여행 가실 수 있길 바랄게요",
          "speaker_id": "2",
          "time": "20211108 00:45"
        }
      ]
