      "utterance": [
        {
          "id": "MDRW2100020186.1.1",
          "form": "아뇨 지금은 백수입니다 잠시 쉬고 있어요",
          "original_form": "아뇨 지금은 백수입니다 잠시 쉬고 있어요",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020186.1.2",
          "form": "덕분에 굴러다녀요ㅋㅋㅋ",
          "original_form": "덕분에 굴러다녀요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020186.1.3",
          "form": "오 부럽다..",
          "original_form": "오 부럽다..",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020186.1.4",
          "form": "그럼 전에.일하던 곳은 어떤곳이였나요??",
          "original_form": "그럼 전에.일하던 곳은 어떤곳이였나요??",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020186.1.5",
          "form": "그냥 평범한 사무직이에요ㅋㅋㅋ",
          "original_form": "그냥 평범한 사무직이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020186.1.6",
          "form": "name1은 무슨일 하세요?",
          "original_form": "&name1&은 무슨일 하세요?",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020186.1.7",
          "form": "저도 평범한 사무직입니다 ㅋㅋ",
          "original_form": "저도 평범한 사무직입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020186.1.8",
          "form": "직장얘긴 재미없죠..ㅋㅋㅋ 현생도 힘든데 쿠ㅜㅜ",
          "original_form": "직장얘긴 재미없죠..ㅋㅋㅋ 현생도 힘든데 쿠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020186.1.9",
          "form": "그러게요..회사에서 즐거운 일 없으셨나요??",
          "original_form": "그러게요..회사에서 즐거운 일 없으셨나요??",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100020186.1.10",
          "form": "팀원들이 저랑 성향이 안맞아서 힘들었어요ㅋㅋ",
          "original_form": "팀원들이 저랑 성향이 안맞아서 힘들었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100020186.1.11",
          "form": "그래서 별로 할 얘기가 없네요 ㅠ",
          "original_form": "그래서 별로 할 얘기가 없네요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100020186.1.12",
          "form": "앗 그렇구낭...회사복지중 가장괜찮았던건 없었나요",
          "original_form": "앗 그렇구낭...회사복지중 가장괜찮았던건 없었나요",
          "speaker_id": "1",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100020186.1.13",
          "form": "저는 집순이고 팀원분들은 인싸셔가지구 ㅋㅋㅋ",
          "original_form": "저는 집순이고 팀원분들은 인싸셔가지구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100020186.1.14",
          "form": "음.. 딱히 없었어요 ㅋㅋㅋ",
          "original_form": "음.. 딱히 없었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100020186.1.15",
          "form": "name1네는 있으신가봐요?",
          "original_form": "&name1&네는 있으신가봐요?",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100020186.1.16",
          "form": "앗그럼 버티기 힘들었겟네요ㅠㅠ",
          "original_form": "앗그럼 버티기 힘들었겟네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100020186.1.17",
          "form": "음..저희는 간식..?",
          "original_form": "음..저희는 간식..?",
          "speaker_id": "1",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100020186.1.18",
          "form": "그나마 간식비 안나가는게 복지인것같네용ㅋㅋㅋㅋ",
          "original_form": "그나마 간식비 안나가는게 복지인것같네용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100020186.1.19",
          "form": "군것질 좋아하세요?",
          "original_form": "군것질 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100020186.1.20",
          "form": "커피도 안맞고 군것질도 별로 안해서 ㅜㅋㅋ",
          "original_form": "커피도 안맞고 군것질도 별로 안해서 ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100020186.1.21",
          "form": "말하고 보니까 되게 사회부적응자 같네요",
          "original_form": "말하고 보니까 되게 사회부적응자 같네요",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100020186.1.22",
          "form": "커피 마시면 심장이 빨리 뛰어서 불편해요",
          "original_form": "커피 마시면 심장이 빨리 뛰어서 불편해요",
          "speaker_id": "2",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100020186.1.23",
          "form": "아녜요ㅎㅎ 그럼 뭐좋아하시나요??",
          "original_form": "아녜요ㅎㅎ 그럼 뭐좋아하시나요??",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100020186.1.24",
          "form": "월급 받으면 적금 넣고 부모님 용돈 드리는게 즐거웠어요",
          "original_form": "월급 받으면 적금 넣고 부모님 용돈 드리는게 즐거웠어요",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100020186.1.25",
          "form": "앗 돈 좋죠 ㅋㅋㅋㅋ",
          "original_form": "앗 돈 좋죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100020186.1.26",
          "form": "부모님한테 용돈은 언제부터 드렸어요???",
          "original_form": "부모님한테 용돈은 언제부터 드렸어요???",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100020186.1.27",
          "form": "저희는 생활비 내라고 하시더라구요",
          "original_form": "저희는 생활비 내라고 하시더라구요",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100020186.1.28",
          "form": "고3 수능마치고 알바 시작하자마자 부터요!",
          "original_form": "고3 수능마치고 알바 시작하자마자 부터요!",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100020186.1.29",
          "form": "저는 생활비 대신 용돈으로..ㅎㅎㅎ",
          "original_form": "저는 생활비 대신 용돈으로..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100020186.1.30",
          "form": "헉 되게 일찍부터 드렸네요!!",
          "original_form": "헉 되게 일찍부터 드렸네요!!",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100020186.1.31",
          "form": "부모님이 좋아하셨겠어요",
          "original_form": "부모님이 좋아하셨겠어요",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100020186.1.32",
          "form": "주지말고 모으라고 하셨었는데 저는 그냥 주는게 기분이 좋아서 드렸었어요",
          "original_form": "주지말고 모으라고 하셨었는데 저는 그냥 주는게 기분이 좋아서 드렸었어요",
