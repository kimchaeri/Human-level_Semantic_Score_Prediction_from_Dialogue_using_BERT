      "utterance": [
        {
          "id": "MDRW2100042047.1.1",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100042047.1.2",
          "form": "없어서 못먹죠!!! ㅋㅋ",
          "original_form": "없어서 못먹죠!!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042047.1.3",
          "form": "오 무슨빵 좋아하세요",
          "original_form": "오 무슨빵 좋아하세요",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042047.1.4",
          "form": "저 모든 빵!!! 단거 담백한거ㅋㅋ 가리지않아요",
          "original_form": "저 모든 빵!!! 단거 담백한거ㅋㅋ 가리지않아요",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042047.1.5",
          "form": "초코파이만 안먹어요ㅋㅋ",
          "original_form": "초코파이만 안먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042047.1.6",
          "form": "헉 초코파이 ?? 안드시는 이유가 있나요??",
          "original_form": "헉 초코파이 ?? 안드시는 이유가 있나요??",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042047.1.7",
          "form": "그 머시멜로우가 비호감ㅋㅋ 나머진 다 좋아요!",
          "original_form": "그 머시멜로우가 비호감ㅋㅋ 나머진 다 좋아요!",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100042047.1.8",
          "form": "아아 마시멜로우 ㅋㅋㅋㅋㅋ",
          "original_form": "아아 마시멜로우 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100042047.1.9",
          "form": "너무 달긴 하죠 ㅠㅠ",
          "original_form": "너무 달긴 하죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100042047.1.10",
          "form": "다른 머시멜로우는 좋은데 초코파이는 식감이ㅋㅋ",
          "original_form": "다른 머시멜로우는 좋은데 초코파이는 식감이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100042047.1.11",
          "form": "아아 !! 어릴때부터 안드셧나요??",
          "original_form": "아아 !! 어릴때부터 안드셧나요??",
          "speaker_id": "1",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100042047.1.12",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042047.1.13",
          "form": "가리는거 있으세요?",
          "original_form": "가리는거 있으세요?",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042047.1.14",
          "form": "저는 맛살을 못먹어욬ㅋㅋㅋ",
          "original_form": "저는 맛살을 못먹어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042047.1.15",
          "form": "어릴때 체한적이있어서",
          "original_form": "어릴때 체한적이있어서",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042047.1.16",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042047.1.17",
          "form": "아ㅜㅠ 그럼 김밥도요???",
          "original_form": "아ㅜㅠ 그럼 김밥도요???",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042047.1.18",
          "form": "김밥 빼서 먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "김밥 빼서 먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042047.1.19",
          "form": "그냥 먹긴하는데 단독으로는 못먹겠어요 ㅋㅋ",
          "original_form": "그냥 먹긴하는데 단독으로는 못먹겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042047.1.20",
          "form": "아 트라우마 있음 그럴거같아요",
          "original_form": "아 트라우마 있음 그럴거같아요",
