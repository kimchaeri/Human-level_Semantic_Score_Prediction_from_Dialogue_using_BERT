      "utterance": [
        {
          "id": "MDRW2100020163.1.1",
          "form": "아까 영화보니까 이탈리아 가고싶다",
          "original_form": "아까 영화보니까 이탈리아 가고싶다",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100020163.1.2",
          "form": "난 유럽 경치 예쁜것 같아",
          "original_form": "난 유럽 경치 예쁜것 같아",
          "speaker_id": "2",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100020163.1.3",
          "form": "유럽 진짜 예뻐",
          "original_form": "유럽 진짜 예뻐",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100020163.1.4",
          "form": "나도 가보고 싶다",
          "original_form": "나도 가보고 싶다",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100020163.1.5",
          "form": "진짜 명화에서 본 그 느낌이야 딱",
          "original_form": "진짜 명화에서 본 그 느낌이야 딱",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100020163.1.6",
          "form": "그러니까 되게 말로 표현 할 수 없는 그게 있는거 같아",
          "original_form": "그러니까 되게 말로 표현 할 수 없는 그게 있는거 같아",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100020163.1.7",
          "form": "맞아 말로 다 표현이 안되지",
          "original_form": "맞아 말로 다 표현이 안되지",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100020163.1.8",
          "form": "난 한 번도 못 가봐서 ㅠㅜ",
          "original_form": "난 한 번도 못 가봐서 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100020163.1.9",
          "form": "코로나 끝나면 가봐",
          "original_form": "코로나 끝나면 가봐",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100020163.1.10",
          "form": "코로나 언제 끝나 ㅠ",
          "original_form": "코로나 언제 끝나 ㅠ",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100020163.1.11",
          "form": "그러니까 말이야",
          "original_form": "그러니까 말이야",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100020163.1.12",
          "form": "끝나야 갈텐데",
          "original_form": "끝나야 갈텐데",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020163.1.13",
          "form": "근데 요즘 유럽 유관중 경기하지 않아?",
          "original_form": "근데 요즘 유럽 유관중 경기하지 않아?",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020163.1.14",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020163.1.15",
          "form": "거기는 좀 괜찮아진건가?",
          "original_form": "거기는 좀 괜찮아진건가?",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020163.1.16",
          "form": "요즘 친구들이랑 여행 가고 싶어",
          "original_form": "요즘 친구들이랑 여행 가고 싶어",
