      "utterance": [
        {
          "id": "MDRW2100024319.1.1",
          "form": "낼 저녁은 뭐 먹을지 아직 못정했어요 ㅜㅜ",
          "original_form": "낼 저녁은 뭐 먹을지 아직 못정했어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024319.1.2",
          "form": "엑.. 물어보고 여셔도 됐을테데요..",
          "original_form": "엑.. 물어보고 여셔도 됐을테데요..",
          "speaker_id": "2",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024319.1.3",
          "form": "앗 벌써 저녁까지..!",
          "original_form": "앗 벌써 저녁까지..!",
          "speaker_id": "2",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100024319.1.4",
          "form": "미리 재료 다듬거나 해동시켜놔야돼서 ㅋㅋㅋㅋ",
          "original_form": "미리 재료 다듬거나 해동시켜놔야돼서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100024319.1.5",
          "form": "전 냉장에서 자연스레 해동되는 게 좋아서 미리 생각해놔요",
          "original_form": "전 냉장에서 자연스레 해동되는 게 좋아서 미리 생각해놔요",
          "speaker_id": "1",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100024319.1.6",
          "form": "우왕..ㅋㅋㅋ 요리잘하시는분 너무 멋있어요",
          "original_form": "우왕..ㅋㅋㅋ 요리잘하시는분 너무 멋있어요",
          "speaker_id": "2",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100024319.1.7",
          "form": "멘트부터가 멋짐이 여기까지 오네요",
          "original_form": "멘트부터가 멋짐이 여기까지 오네요",
          "speaker_id": "2",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100024319.1.8",
          "form": "앗 칭찬 너무 감사해요 ㅎㅎㅎ",
          "original_form": "앗 칭찬 너무 감사해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024319.1.9",
          "form": "어제 사놓은 바게트 어떻게 처리해야될 지 모르겠어서 비슷한 메뉴로 갈 거 같아요~",
          "original_form": "어제 사놓은 바게트 어떻게 처리해야될 지 모르겠어서 비슷한 메뉴로 갈 거 같아요~",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024319.1.10",
          "form": "에그인더헬을 만들어 먹거나 연어대신 익힌새우로 올리거나 하는 등등으로요 ㅎㅎ",
          "original_form": "에그인더헬을 만들어 먹거나 연어대신 익힌새우로 올리거나 하는 등등으로요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024319.1.11",
          "form": "오오ㅋㅋㅋ집에 새우 있음 끝이래서 냉동 새우 사서 있어보이려했는데 장인은 도구탓을 안한다는걸",
          "original_form": "오오ㅋㅋㅋ집에 새우 있음 끝이래서 냉동 새우 사서 있어보이려했는데 장인은 도구탓을 안한다는걸",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024319.1.12",
          "form": "이렇게 느낌당ㅋㅋ",
          "original_form": "이렇게 느낌당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024319.1.13",
          "form": "와인하고 먹고픈 메누네요",
          "original_form": "와인하고 먹고픈 메누네요",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024319.1.14",
          "form": "오 ㅋㅋ 집에 레드와인 있는데",
          "original_form": "오 ㅋㅋ 집에 레드와인 있는데",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024319.1.15",
          "form": "내일 한 잔 각이네요!",
          "original_form": "내일 한 잔 각이네요!",
          "speaker_id": "1",
          "time": "20210831 00:49"
        },
        {
          "id": "MDRW2100024319.1.16",
          "form": "크으으 최고시다",
          "original_form": "크으으 최고시다",
          "speaker_id": "2",
          "time": "20210831 00:50"
        },
        {
          "id": "MDRW2100024319.1.17",
          "form": "진짜 후기 들려주셔야해요",
          "original_form": "진짜 후기 들려주셔야해요",
          "speaker_id": "2",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024319.1.18",
          "form": "제 배에서 소리나게 하셨기때문이에요",
          "original_form": "제 배에서 소리나게 하셨기때문이에요",
          "speaker_id": "2",
          "time": "20210831 00:51"
        },
        {
          "id": "MDRW2100024319.1.19",
          "form": "그래도 제 위장은 즐거웠어용",
          "original_form": "그래도 제 위장은 즐거웠어용",
          "speaker_id": "2",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024319.1.20",
          "form": "상상으료 뚝딱 했슴닷",
          "original_form": "상상으료 뚝딱 했슴닷",
          "speaker_id": "2",
          "time": "20210831 00:57"
        },
        {
          "id": "MDRW2100024319.1.21",
          "form": "앗 ㅋㅋㅋ 님도 맛있는 요리 먹게 되기를",
          "original_form": "앗 ㅋㅋㅋ 님도 맛있는 요리 먹게 되기를",
          "speaker_id": "1",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100024319.1.22",
          "form": "진심으로 축복할게요!",
          "original_form": "진심으로 축복할게요!",
          "speaker_id": "1",
          "time": "20210831 00:58"
        }
      ]
    }
