      "utterance": [
        {
          "id": "MDRW2100002536.1.1",
          "form": "name2님은 학교 어느 지역에서 다니셨나요??",
          "original_form": "&name2&님은 학교 어느 지역에서 다니셨나요??",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002536.1.2",
          "form": "경상북도쪽이요!",
          "original_form": "경상북도쪽이요!",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002536.1.3",
          "form": "저는 태어날때부터 쭉 모든 학교를 다 경북에서 다녔어요!",
          "original_form": "저는 태어날때부터 쭉 모든 학교를 다 경북에서 다녔어요!",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002536.1.4",
          "form": "그럼 경상도 사투리 쓰시겠네요 ㅋㅋ",
          "original_form": "그럼 경상도 사투리 쓰시겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002536.1.5",
          "form": "학교에서 학생회나 동아리 같은거 해보셨나요??",
          "original_form": "학교에서 학생회나 동아리 같은거 해보셨나요??",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002536.1.6",
          "form": "동아리같은 경우는",
          "original_form": "동아리같은 경우는",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002536.1.7",
          "form": "이제 보건동아리? 그런쪽으로 해봤고",
          "original_form": "이제 보건동아리? 그런쪽으로 해봤고",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002536.1.8",
          "form": "학생회는 안해봤네요",
          "original_form": "학생회는 안해봤네요",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002536.1.9",
          "form": "ㅋㅋ다른 동아리 같은건 안하셨나요",
          "original_form": "ㅋㅋ다른 동아리 같은건 안하셨나요",
          "speaker_id": "1",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002536.1.10",
          "form": "저는 게임동아리랑 음악쪽 동아리 해봤어요 ㅋㅋ",
          "original_form": "저는 게임동아리랑 음악쪽 동아리 해봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002536.1.11",
          "form": "축구를 좋아해서",
          "original_form": "축구를 좋아해서",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002536.1.12",
          "form": "축구쪽 동아리랑",
          "original_form": "축구쪽 동아리랑",
          "speaker_id": "2",
          "time": "20210820 13:24"
        },
        {
          "id": "MDRW2100002536.1.13",
          "form": "일본어에 관심이 있어서 일본어 동아리도 해봤어요!",
          "original_form": "일본어에 관심이 있어서 일본어 동아리도 해봤어요!",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002536.1.14",
          "form": "오 저는 일본어 수업 들어본적있어요",
          "original_form": "오 저는 일본어 수업 들어본적있어요",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002536.1.15",
          "form": "그 수업 끝나자마자 일본으로 여행가기두 했고 ㅎㅎ",
          "original_form": "그 수업 끝나자마자 일본으로 여행가기두 했고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002536.1.16",
          "form": "근데 수업은 기본만 알려주다보니까 여행갈때는 별로 도움이 안됐어요 ㅋㅋ",
          "original_form": "근데 수업은 기본만 알려주다보니까 여행갈때는 별로 도움이 안됐어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002536.1.17",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002536.1.18",
          "form": "회화에 사실 큰 도움이 될순없긴하죠 ㅠㅠ",
          "original_form": "회화에 사실 큰 도움이 될순없긴하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002536.1.19",
          "form": "맞아요 결국 일본 가서 길잃고 겨우겨우 찾아갔어요",
          "original_form": "맞아요 결국 일본 가서 길잃고 겨우겨우 찾아갔어요",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002536.1.20",
          "form": "아이구.. 다행이네요",
          "original_form": "아이구.. 다행이네요",
          "speaker_id": "2",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002536.1.21",
          "form": "그뒤로 해외여행가는게 무서워졌어요 ㅋㅋ",
          "original_form": "그뒤로 해외여행가는게 무서워졌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002536.1.22",
          "form": "해외여행은 친구나 지인이랑 가는게 좋죠",
          "original_form": "해외여행은 친구나 지인이랑 가는게 좋죠",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002536.1.23",
          "form": "혼자가는건 좀 위험해보이기도 해요",
          "original_form": "혼자가는건 좀 위험해보이기도 해요",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002536.1.24",
          "form": "치안이 안좋은곳이 많아서 잘보면..",
          "original_form": "치안이 안좋은곳이 많아서 잘보면..",
          "speaker_id": "2",
          "time": "20210820 13:35"
        }
      ]
    }
