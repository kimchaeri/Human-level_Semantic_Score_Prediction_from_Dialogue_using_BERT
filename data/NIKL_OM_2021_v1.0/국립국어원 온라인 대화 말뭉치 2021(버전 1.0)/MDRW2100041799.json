      "utterance": [
        {
          "id": "MDRW2100041799.1.1",
          "form": "딱 요즘 날씨를 좋아해요",
          "original_form": "딱 요즘 날씨를 좋아해요",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100041799.1.2",
          "form": "적당히 선선하고 하늘이 맑고 공긴 깨끗한",
          "original_form": "적당히 선선하고 하늘이 맑고 공긴 깨끗한",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100041799.1.3",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041799.1.4",
          "form": "저도 단풍이 있는 가을 좋아해요",
          "original_form": "저도 단풍이 있는 가을 좋아해요",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041799.1.5",
          "form": "초가을을 주말동안 잠깐 느꼈는데",
          "original_form": "초가을을 주말동안 잠깐 느꼈는데",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041799.1.6",
          "form": "오늘 다시 흐리고 비오기 시작하네요",
          "original_form": "오늘 다시 흐리고 비오기 시작하네요",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041799.1.7",
          "form": "헉, 오늘 비온댔나요?",
          "original_form": "헉, 오늘 비온댔나요?",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041799.1.8",
          "form": "우산 작은거밖에 없는데, 많이는 안왔으면 좋겠네요ㅠ",
          "original_form": "우산 작은거밖에 없는데, 많이는 안왔으면 좋겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041799.1.9",
          "form": "여기는 비 와요 내일까지 온대요",
          "original_form": "여기는 비 와요 내일까지 온대요",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041799.1.10",
          "form": "지금 바람도 꽤 불어요",
          "original_form": "지금 바람도 꽤 불어요",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041799.1.11",
          "form": "여기도 지금 비가 꽤 오네요.",
          "original_form": "여기도 지금 비가 꽤 오네요.",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041799.1.12",
          "form": "좀 추울정도에요 ㅠㅠ",
          "original_form": "좀 추울정도에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100041799.1.13",
          "form": "기온이 꽤 떨어졌어요",
          "original_form": "기온이 꽤 떨어졌어요",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041799.1.14",
          "form": "진짜 감기 조심해야해요",
          "original_form": "진짜 감기 조심해야해요",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041799.1.15",
          "form": "하... 10시쯤 퇴근하는데 그때는 비가 좀 그일까요?",
          "original_form": "하... 10시쯤 퇴근하는데 그때는 비가 좀 그일까요?",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100041799.1.16",
          "form": "동네마다 다를 거 같아요",
          "original_form": "동네마다 다를 거 같아요",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041799.1.17",
          "form": "저희 동네는 내일까지 온대요",
          "original_form": "저희 동네는 내일까지 온대요",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041799.1.18",
          "form": "아이고....",
          "original_form": "아이고....",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100041799.1.19",
          "form": "지금 꽤 쏟아지는데 우산 큰거 가져올걸",
          "original_form": "지금 꽤 쏟아지는데 우산 큰거 가져올걸",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100041799.1.20",
          "form": "이따 퇴근때는 또 잦아질수도 있으니까요",
          "original_form": "이따 퇴근때는 또 잦아질수도 있으니까요",
          "speaker_id": "1",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041799.1.21",
          "form": "그러길 바랄 뿐입니다.",
          "original_form": "그러길 바랄 뿐입니다.",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100041799.1.22",
          "form": "저저번주엔 퇴근하다 신발에 물들어가서....ㅂㄷㅂㄷ",
          "original_form": "저저번주엔 퇴근하다 신발에 물들어가서....ㅂㄷㅂㄷ",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100041799.1.23",
          "form": "저저번주부터 꽤 비가 왔었죠",
          "original_form": "저저번주부터 꽤 비가 왔었죠",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100041799.1.24",
          "form": "가을장마요",
          "original_form": "가을장마요",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100041799.1.25",
          "form": "이젠 비도 그만 올때가 된거 같은데",
          "original_form": "이젠 비도 그만 올때가 된거 같은데",
          "speaker_id": "1",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100041799.1.26",
          "form": "가을비가 많이 오면 너무 금방 추워지는데",
          "original_form": "가을비가 많이 오면 너무 금방 추워지는데",
          "speaker_id": "2",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100041799.1.27",
          "form": "외투를 슬슬 꺼내야겠어요",
          "original_form": "외투를 슬슬 꺼내야겠어요",
          "speaker_id": "2",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100041799.1.28",
          "form": "이제 옷정리 해야할 듯해요",
          "original_form": "이제 옷정리 해야할 듯해요",
          "speaker_id": "1",
          "time": "20210906 20:04"
        }
      ]
