      "utterance": [
        {
          "id": "MDRW2100043450.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043450.1.2",
          "form": "비가 많이 오네요",
          "original_form": "비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043450.1.3",
          "form": "안녕하세요 ~",
          "original_form": "안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043450.1.4",
          "form": "그러니깐요",
          "original_form": "그러니깐요",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043450.1.5",
          "form": "비가 꽤 긴 시간동안 내리네요",
          "original_form": "비가 꽤 긴 시간동안 내리네요",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043450.1.6",
          "form": "이제 그만 왔으면..",
          "original_form": "이제 그만 왔으면..",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043450.1.7",
          "form": "네 비가 길게 오니",
          "original_form": "네 비가 길게 오니",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043450.1.8",
          "form": "좀 지치네요ㅜ",
          "original_form": "좀 지치네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043450.1.9",
          "form": "그쵸ㅜㅠ",
          "original_form": "그쵸ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043450.1.10",
          "form": "진짜 사람이 축축 쳐져요",
          "original_form": "진짜 사람이 축축 쳐져요",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043450.1.11",
          "form": "맑은날이 거의없는기분",
          "original_form": "맑은날이 거의없는기분",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043450.1.12",
          "form": "네 컨디션이 처지죠ㅜ",
          "original_form": "네 컨디션이 처지죠ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043450.1.13",
          "form": "해 보고 싶어요",
          "original_form": "해 보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043450.1.14",
          "form": "안그래도 일 많아서 피곤한데 더 쳐지는",
          "original_form": "안그래도 일 많아서 피곤한데 더 쳐지는",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043450.1.15",
          "form": "그쵸 맑은 하늘이 그립네요",
          "original_form": "그쵸 맑은 하늘이 그립네요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043450.1.16",
          "form": "가을이라 또 달라보일텐데",
          "original_form": "가을이라 또 달라보일텐데",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043450.1.17",
          "form": "네 맑은 하늘이 보고 싶어요",
          "original_form": "네 맑은 하늘이 보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043450.1.18",
          "form": "지난 주말 하늘 진짜 예쁘던데",
          "original_form": "지난 주말 하늘 진짜 예쁘던데",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043450.1.19",
          "form": "주말에 일하느라 하늘 볼새도없었던거 같네여ㅠ",
          "original_form": "주말에 일하느라 하늘 볼새도없었던거 같네여ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043450.1.20",
          "form": "그래도 낼 까지 비온다고 하니 믿어보려고요",
          "original_form": "그래도 낼 까지 비온다고 하니 믿어보려고요",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043450.1.21",
          "form": "아 저는 백신 접종하고 실내에서 하늘 봤어요ㅜ",
          "original_form": "아 저는 백신 접종하고 실내에서 하늘 봤어요ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043450.1.22",
          "form": "아! 접종하셨구나",
          "original_form": "아! 접종하셨구나",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043450.1.23",
          "form": "괜찮으시죠?!",
          "original_form": "괜찮으시죠?!",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043450.1.24",
          "form": "네 지난주말에요",
          "original_form": "네 지난주말에요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043450.1.25",
          "form": "네네 이젠 괜찮아요ㅎㅎ",
          "original_form": "네네 이젠 괜찮아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043450.1.26",
          "form": "전...진작 완료했어서 ㅎ",
          "original_form": "전...진작 완료했어서 ㅎ",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043450.1.27",
          "form": "화이자 맞으셨나여?",
          "original_form": "화이자 맞으셨나여?",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043450.1.28",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043450.1.29",
          "form": "네 화이자 2차접종",
          "original_form": "네 화이자 2차접종",
          "speaker_id": "1",
          "time": "20210907 19:38"
        }
      ]
    }
  ]
