      "utterance": [
        {
          "id": "MDRW2100002020.1.1",
          "form": "얼마 전에 해서",
          "original_form": "얼마 전에 해서",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.2",
          "form": "한참 뒤에 하겠지",
          "original_form": "한참 뒤에 하겠지",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.3",
          "form": "그른가",
          "original_form": "그른가",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.4",
          "form": "오랜만에 고급스럽게",
          "original_form": "오랜만에 고급스럽게",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.5",
          "form": "연주가 듣고싶당",
          "original_form": "연주가 듣고싶당",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.6",
          "form": "언제 하는지 연락해 봐라",
          "original_form": "언제 하는지 연락해 봐라",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.7",
          "form": "ㅋㅋ 엄마한테 물어보라고 해야겠다",
          "original_form": "ㅋㅋ 엄마한테 물어보라고 해야겠다",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.8",
          "form": "저번에 못가서 넘 아쉽 ㅠㅠ",
          "original_form": "저번에 못가서 넘 아쉽 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.9",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.10",
          "form": "다음엔 다 같이 가자",
          "original_form": "다음엔 다 같이 가자",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.11",
          "form": "알았옹",
          "original_form": "알았옹",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002020.1.12",
          "form": "선물도 사가야겠다 먹는걸로",
          "original_form": "선물도 사가야겠다 먹는걸로",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002020.1.13",
          "form": "꽃은 넘 많이 받더라",
          "original_form": "꽃은 넘 많이 받더라",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002020.1.14",
          "form": "그래 꽃은 넘 많이 받으니까",
          "original_form": "그래 꽃은 넘 많이 받으니까",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002020.1.15",
          "form": "먹는거를 주는 게 좋지",
          "original_form": "먹는거를 주는 게 좋지",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002020.1.16",
          "form": "웅 ㅎㅎ",
          "original_form": "웅 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002020.1.17",
          "form": "공연 빨리 하면 좋겠네",
          "original_form": "공연 빨리 하면 좋겠네",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002020.1.18",
          "form": "가을에하면 딱인데",
          "original_form": "가을에하면 딱인데",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002020.1.19",
          "form": "왠지 할것같다",
          "original_form": "왠지 할것같다",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002020.1.20",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002020.1.21",
          "form": "우리 가족 다 같이 보러 가야지",
          "original_form": "우리 가족 다 같이 보러 가야지",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002020.1.22",
          "form": "옹옹~ 가면 친척도 다 보니까 좋다 ㅎ",
          "original_form": "옹옹~ 가면 친척도 다 보니까 좋다 ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002020.1.23",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210819 22:52"
        }
      ]
