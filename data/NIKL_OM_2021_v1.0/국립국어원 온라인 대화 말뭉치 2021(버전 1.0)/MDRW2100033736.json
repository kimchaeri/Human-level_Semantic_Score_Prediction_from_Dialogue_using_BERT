      "utterance": [
        {
          "id": "MDRW2100033736.1.1",
          "form": "나 이번에 다이어트 해!",
          "original_form": "나 이번에 다이어트 해!",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033736.1.2",
          "form": "아정말? 그거 매년 얘기한 거 같은데",
          "original_form": "아정말? 그거 매년 얘기한 거 같은데",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.3",
          "form": "이번엔 며칠동안 할거야?",
          "original_form": "이번엔 며칠동안 할거야?",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.4",
          "form": "이번엔 한달 목표야!",
          "original_form": "이번엔 한달 목표야!",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.5",
          "form": "운동할거야?",
          "original_form": "운동할거야?",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.6",
          "form": "대단한걸",
          "original_form": "대단한걸",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.7",
          "form": "아니 홈트를 할꺼야",
          "original_form": "아니 홈트를 할꺼야",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.8",
          "form": "한달 다이어트는 정말 힘든 거 같아",
          "original_form": "한달 다이어트는 정말 힘든 거 같아",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.9",
          "form": "홈트라고 들어봤어?",
          "original_form": "홈트라고 들어봤어?",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.10",
          "form": "땅끄부부는 어때?",
          "original_form": "땅끄부부는 어때?",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.11",
          "form": "아주 힘들더라구.",
          "original_form": "아주 힘들더라구.",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.12",
          "form": "아 그사람들은 초보자에게 너무 가혹해",
          "original_form": "아 그사람들은 초보자에게 너무 가혹해",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.13",
          "form": "홈트 내가 알려줬단ㅅ아 너에게.",
          "original_form": "홈트 내가 알려줬단ㅅ아 너에게.",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033736.1.14",
          "form": "그리고 식단도 병행할꺼야",
          "original_form": "그리고 식단도 병행할꺼야",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033736.1.15",
          "form": "아무래도 너는 뇌건강을 위해 노력할 필요가 있어.",
          "original_form": "아무래도 너는 뇌건강을 위해 노력할 필요가 있어.",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033736.1.16",
          "form": "미안미안 착각했네",
          "original_form": "미안미안 착각했네",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033736.1.17",
          "form": "그러면 야식은 이제 끊는거야?",
          "original_form": "그러면 야식은 이제 끊는거야?",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033736.1.18",
          "form": "호두라도 챙겨먹어야겠어",
          "original_form": "호두라도 챙겨먹어야겠어",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033736.1.19",
          "form": "응응 이제 배달의민족을 지울꺼야",
          "original_form": "응응 이제 배달의민족을 지울꺼야",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033736.1.20",
          "form": "술을 박스째 사먹는 것도 그만두는게 좋아",
          "original_form": "술을 박스째 사먹는 것도 그만두는게 좋아",
          "speaker_id": "2",
          "time": "20210824 23:54"
        }
      ]
