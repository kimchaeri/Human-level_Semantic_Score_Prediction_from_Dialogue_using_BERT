      "utterance": [
        {
          "id": "MDRW2100033993.1.1",
          "form": "내일도 출근하시나용",
          "original_form": "내일도 출근하시나용",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100033993.1.2",
          "form": "네 출근하죠 ㅜㅜㅠ",
          "original_form": "네 출근하죠 ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033993.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033993.1.4",
          "form": "저두용 .. 몇시출근이세요?",
          "original_form": "저두용 .. 몇시출근이세요?",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100033993.1.5",
          "form": "전 9시 출근이여",
          "original_form": "전 9시 출근이여",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100033993.1.6",
          "form": "name1님두여?",
          "original_form": "&name1&님두여?",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100033993.1.7",
          "form": "오 저두요 ㅠㅠ",
          "original_form": "오 저두요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100033993.1.8",
          "form": "역시 같군요ㅋㅋㅋㅋㅋ",
          "original_form": "역시 같군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100033993.1.9",
          "form": "벌써 11시라니요 .. 몇시취침하시나요",
          "original_form": "벌써 11시라니요 .. 몇시취침하시나요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100033993.1.10",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100033993.1.11",
          "form": "저 맨날 2시에 자고",
          "original_form": "저 맨날 2시에 자고",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100033993.1.12",
          "form": "아침ㅇ0 후회해요",
          "original_form": "아침ㅇ0 후회해요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100033993.1.13",
          "form": "와ㅋㅋㅋ그래도 체력대단 2시까지 못버티겠던데요 ㅠㅠ",
          "original_form": "와ㅋㅋㅋ그래도 체력대단 2시까지 못버티겠던데요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100033993.1.14",
          "form": "ㅋㅋ ㅋㅋㅋ 잠자고 싶어도 잘 안와요",
          "original_form": "ㅋㅋ ㅋㅋㅋ 잠자고 싶어도 잘 안와요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100033993.1.15",
          "form": "그래서 좀 괴롭기도",
          "original_form": "그래서 좀 괴롭기도",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100033993.1.16",
          "form": "밤잠이 없으시구낭 ㅠㅠ",
          "original_form": "밤잠이 없으시구낭 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100033993.1.17",
          "form": "네 ㅠ 그래서 아침에",
          "original_form": "네 ㅠ 그래서 아침에",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100033993.1.18",
          "form": "피곤해죽어요",
          "original_form": "피곤해죽어요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100033993.1.19",
          "form": "ㅠㅠㅠㅠㅠ 진짜 괴로워요그거 ㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ 진짜 괴로워요그거 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100033993.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 08:25"
        },
        {
          "id": "MDRW2100033993.1.21",
          "form": "오늘 아침도 죽어나네요",
