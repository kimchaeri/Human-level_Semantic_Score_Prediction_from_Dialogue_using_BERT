      "utterance": [
        {
          "id": "MDRW2100017099.1.1",
          "form": "저녁식사 메뉴 정하셨나요",
          "original_form": "저녁식사 메뉴 정하셨나요",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017099.1.2",
          "form": "아 ㅋㅋㅋ 감사해요",
          "original_form": "아 ㅋㅋㅋ 감사해요",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017099.1.3",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017099.1.4",
          "form": "name1은 저녁 정하셨나요",
          "original_form": "&name1&은 저녁 정하셨나요",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017099.1.5",
          "form": "전 아마 순대볶음 먹을거같은데",
          "original_form": "전 아마 순대볶음 먹을거같은데",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017099.1.6",
          "form": "순대볶음좋아하시나요",
          "original_form": "순대볶음좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017099.1.7",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017099.1.8",
          "form": "순대 안먹은지 꽤 됐네요 근데",
          "original_form": "순대 안먹은지 꽤 됐네요 근데",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017099.1.9",
          "form": "순대먹을 때 내장도 드세여?",
          "original_form": "순대먹을 때 내장도 드세여?",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017099.1.10",
          "form": "코로나 이후부터는 외식이 줄다보니...",
          "original_form": "코로나 이후부터는 외식이 줄다보니...",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017099.1.11",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017099.1.12",
          "form": "예전엔 먹었는데",
          "original_form": "예전엔 먹었는데",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017099.1.13",
          "form": "지금은 안 먹어요",
          "original_form": "지금은 안 먹어요",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017099.1.14",
          "form": "name1님은 드세요?",
          "original_form": "&name1&님은 드세요?",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017099.1.15",
          "form": "저는 허파빼고 다 먹어요",
          "original_form": "저는 허파빼고 다 먹어요",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017099.1.16",
          "form": "약간 허파의 식감이 싫어가지고",
          "original_form": "약간 허파의 식감이 싫어가지고",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017099.1.17",
          "form": "오 잘 드시네요",
          "original_form": "오 잘 드시네요",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017099.1.18",
          "form": "아 맞아요 ㅋㅋㅋ 식감이 독특하죠",
          "original_form": "아 맞아요 ㅋㅋㅋ 식감이 독특하죠",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017099.1.19",
          "form": "무슨 음식 좋아하세요?",
          "original_form": "무슨 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017099.1.20",
          "form": "약간 물컹한게 싫더라구요",
          "original_form": "약간 물컹한게 싫더라구요",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017099.1.21",
          "form": "저 김밥 좋아해요 김밥좋아하시나요",
          "original_form": "저 김밥 좋아해요 김밥좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017099.1.22",
          "form": "오 김밥 자주 먹죠",
          "original_form": "오 김밥 자주 먹죠",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017099.1.23",
          "form": "참치김밥 젤루 좋아해요",
          "original_form": "참치김밥 젤루 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017099.1.24",
          "form": "집 앞에 김밥천국 있어서",
          "original_form": "집 앞에 김밥천국 있어서",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017099.1.25",
          "form": "참치 그것은 갓벽..",
          "original_form": "참치 그것은 갓벽..",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017099.1.26",
          "form": "지난달에는 거의 맨날 먹었어요",
          "original_form": "지난달에는 거의 맨날 먹었어요",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017099.1.27",
          "form": "일하면서 먹으면 좋더라구요",
          "original_form": "일하면서 먹으면 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017099.1.28",
          "form": "그정도로 좋아하시다니",
          "original_form": "그정도로 좋아하시다니",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017099.1.29",
          "form": "또 좋아하는 분식있으세요",
          "original_form": "또 좋아하는 분식있으세요",
          "speaker_id": "1",
