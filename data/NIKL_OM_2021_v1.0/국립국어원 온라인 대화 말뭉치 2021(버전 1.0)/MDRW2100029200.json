      "utterance": [
        {
          "id": "MDRW2100029200.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029200.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029200.1.3",
          "form": "햄버거 어디 브랜드를 제일 선호하시나요",
          "original_form": "햄버거 어디 브랜드를 제일 선호하시나요",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029200.1.4",
          "form": "저는 햄버거는 버거킹이 맛있어요!",
          "original_form": "저는 햄버거는 버거킹이 맛있어요!",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029200.1.5",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029200.1.6",
          "form": "저는 버거킹 롯데리아 맥날 중엔",
          "original_form": "저는 버거킹 롯데리아 맥날 중엔",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029200.1.7",
          "form": "버거킹이고 그 위에",
          "original_form": "버거킹이고 그 위에",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029200.1.8",
          "form": "하이엔드 느낌으로다가",
          "original_form": "하이엔드 느낌으로다가",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029200.1.9",
          "form": "쉑쉑 좋아핮니다",
          "original_form": "쉑쉑 좋아핮니다",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029200.1.10",
          "form": "ㅋㅋㅋ 오우 평가가 세부적이시네요",
          "original_form": "ㅋㅋㅋ 오우 평가가 세부적이시네요",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029200.1.11",
          "form": "저도 쉑쉑 버거는 맛있더라고요",
          "original_form": "저도 쉑쉑 버거는 맛있더라고요",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029200.1.12",
          "form": "감자튀김은 맥도날드가 기본이라 맛있다고 생각해요 ㅋㅋ",
          "original_form": "감자튀김은 맥도날드가 기본이라 맛있다고 생각해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029200.1.13",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029200.1.14",
          "form": "밈터 감튀 첨 나왓을때",
          "original_form": "밈터 감튀 첨 나왓을때",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029200.1.15",
          "form": "신세계얐는데 다시 맥날로 돌아가더군요",
          "original_form": "신세계얐는데 다시 맥날로 돌아가더군요",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029200.1.16",
          "form": "선호도가요 ㅋㅋ",
          "original_form": "선호도가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029200.1.17",
          "form": "맞아요 맘스터치 감튀도 맛있는데 기본이 제일이더라고요ㅎㅎ",
          "original_form": "맞아요 맘스터치 감튀도 맛있는데 기본이 제일이더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029200.1.18",
          "form": "무슨 버거 좋아하세요??",
          "original_form": "무슨 버거 좋아하세요??",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029200.1.19",
          "form": "맘터는 화이트 갈릭",
          "original_form": "맘터는 화이트 갈릭",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029200.1.20",
          "form": "맥날은 1955",
          "original_form": "맥날은 1955",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029200.1.21",
          "form": "롯데리아는 새우버거요 ㅋㅋㅋㅋ",
          "original_form": "롯데리아는 새우버거요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029200.1.22",
          "form": "맞아요 셋다 맛있는데 먹고 싶네요ㅋㅋㅋㅋ 저는 맘터에서는 할라피뇨버거 자주 먹고 맥날은 불고기버거 먹고 롯데리아는 새우버거 먹어요 ㅎㄹ",
          "original_form": "맞아요 셋다 맛있는데 먹고 싶네요ㅋㅋㅋㅋ 저는 맘터에서는 할라피뇨버거 자주 먹고 맥날은 불고기버거 먹고 롯데리아는 새우버거 먹어요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029200.1.23",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029200.1.24",
          "form": "롯데리아에서 새우버거랑 양념감자 조합 좋아합니다 ㅋㅋ",
          "original_form": "롯데리아에서 새우버거랑 양념감자 조합 좋아합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029200.1.25",
          "form": "떡볶이는 어디꺼...??",
          "original_form": "떡볶이는 어디꺼...??",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029200.1.26",
          "form": "맛있는 조합이네요 먹고 싶어요 ㅋㅋㅋ",
          "original_form": "맛있는 조합이네요 먹고 싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029200.1.27",
          "form": "저는 안가리는데 신전 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 안가리는데 신전 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029200.1.28",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "1",
