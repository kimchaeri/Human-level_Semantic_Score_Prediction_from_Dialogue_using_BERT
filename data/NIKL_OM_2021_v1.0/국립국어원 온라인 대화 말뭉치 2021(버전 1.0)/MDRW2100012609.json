      "utterance": [
        {
          "id": "MDRW2100012609.1.1",
          "form": "ㅋㅋ진짜 저도 슬픈부분에서는 엄청 울고ㅋㅋㅋ",
          "original_form": "ㅋㅋ진짜 저도 슬픈부분에서는 엄청 울고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012609.1.2",
          "form": "너무 재밌었어요! 눈도 정화되고요!ㅋㅋㅋ",
          "original_form": "너무 재밌었어요! 눈도 정화되고요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012609.1.3",
          "form": "맞어요 공유랑 케미가 너무 대박이었어요!",
          "original_form": "맞어요 공유랑 케미가 너무 대박이었어요!",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012609.1.4",
          "form": "김고은 배우도 너무 예쁘고",
          "original_form": "김고은 배우도 너무 예쁘고",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100012609.1.5",
          "form": "노래도 너무 좋지 않나요? ㅋㅋ",
          "original_form": "노래도 너무 좋지 않나요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012609.1.6",
          "form": "드라마 끝나고나서도 계속 ost 들었어요",
          "original_form": "드라마 끝나고나서도 계속 ost 들었어요",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100012609.1.7",
          "form": "맞아요!!!! 전 특히 에일리 노래요 ㅠㅠ !!!",
          "original_form": "맞아요!!!! 전 특히 에일리 노래요 ㅠㅠ !!!",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100012609.1.8",
          "form": "들을 때마다 울컥하는..",
          "original_form": "들을 때마다 울컥하는..",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100012609.1.9",
          "form": "네네!!! 에일리노래!!!",
          "original_form": "네네!!! 에일리노래!!!",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012609.1.10",
          "form": "첫눈처럼 너에게 가겠다!!!",
          "original_form": "첫눈처럼 너에게 가겠다!!!",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012609.1.11",
          "form": "크으...진짜 언제들어도 명곡...",
          "original_form": "크으...진짜 언제들어도 명곡...",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012609.1.12",
          "form": "맞아요!!!!!!!!!",
          "original_form": "맞아요!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012609.1.13",
          "form": "그리고 도꺄비의 또 다른 포인트는 역시 육성재..",
          "original_form": "그리고 도꺄비의 또 다른 포인트는 역시 육성재..",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012609.1.14",
          "form": "너무 웃겼어요 ㅠ",
          "original_form": "너무 웃겼어요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012609.1.15",
          "form": "ㅋㅋㅋㅋ육성재의 재발견ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ육성재의 재발견ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012609.1.16",
          "form": "근데 막판에 육성재한테 신이 빙의되었던건가요??",
          "original_form": "근데 막판에 육성재한테 신이 빙의되었던건가요??",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012609.1.17",
          "form": "네네 맞아요!!!!!!! 자주 들어갔다 나왔다 했던 걸로 알고 있어요!!!",
          "original_form": "네네 맞아요!!!!!!! 자주 들어갔다 나왔다 했던 걸로 알고 있어요!!!",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012609.1.18",
          "form": "그 막 답지않게 날카로울 땐 신이었던 걸로 알아요",
          "original_form": "그 막 답지않게 날카로울 땐 신이었던 걸로 알아요",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012609.1.19",
          "form": "오오...신기...",
          "original_form": "오오...신기...",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012609.1.20",
          "form": "하여간 너무 재밌었어요 도깨비ㅋㅋ",
          "original_form": "하여간 너무 재밌었어요 도깨비ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012609.1.21",
          "form": "맞아요 ㅠ ㅠ 전 결말은 좀 슬펐어요",
          "original_form": "맞아요 ㅠ ㅠ 전 결말은 좀 슬펐어요",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012609.1.22",
          "form": "결국 공유가 혼자 살아갈 거 같아서 ㅠㅠ",
          "original_form": "결국 공유가 혼자 살아갈 거 같아서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012609.1.23",
          "form": "마지막에 환생한 김고은이 공유 찾은거 아니었나요??",
          "original_form": "마지막에 환생한 김고은이 공유 찾은거 아니었나요??",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100012609.1.24",
          "form": "앗 맞긴한데 ㅠ 그 환생이 3번만 가능해서 ㅠㅠ",
          "original_form": "앗 맞긴한데 ㅠ 그 환생이 3번만 가능해서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 12:52"
        },
        {
          "id": "MDRW2100012609.1.25",
          "form": "나머지 생은 공유가 혼자 사는 거라고 하더라고요!",
          "original_form": "나머지 생은 공유가 혼자 사는 거라고 하더라고요!",
          "speaker_id": "1",
          "time": "20210825 12:52"
        },
        {
          "id": "MDRW2100012609.1.26",
          "form": "아아ㅠ 외롭겠어요 진짜",
          "original_form": "아아ㅠ 외롭겠어요 진짜",
          "speaker_id": "2",
          "time": "20210825 12:53"
        }
      ]
    }
