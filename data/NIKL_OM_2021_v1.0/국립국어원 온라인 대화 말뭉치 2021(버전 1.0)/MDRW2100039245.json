      "utterance": [
        {
          "id": "MDRW2100039245.1.1",
          "form": "맛집 자주가세요?",
          "original_form": "맛집 자주가세요?",
          "speaker_id": "1",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100039245.1.2",
          "form": "아니요 사무실에서 점심을 밖에 나가서 항상 사 먹는 편이라",
          "original_form": "아니요 사무실에서 점심을 밖에 나가서 항상 사 먹는 편이라",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039245.1.3",
          "form": "근처에 있는 식당을 가요",
          "original_form": "근처에 있는 식당을 가요",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039245.1.4",
          "form": "저녁에는 코로나 때문에 애들이랑 외식을 못하고 있구요",
          "original_form": "저녁에는 코로나 때문에 애들이랑 외식을 못하고 있구요",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039245.1.5",
          "form": "근처식당에  맛있는곳은 있나요?",
          "original_form": "근처식당에  맛있는곳은 있나요?",
          "speaker_id": "1",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039245.1.6",
          "form": "보통 돈까스 짬뽕 짬뽕국밥 제육볶음 제육볶음 칼국수 순두부찌개",
          "original_form": "보통 돈까스 짬뽕 짬뽕국밥 제육볶음 제육볶음 칼국수 순두부찌개",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039245.1.7",
          "form": "돌아가면서 먹는 거 같애요",
          "original_form": "돌아가면서 먹는 거 같애요",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039245.1.8",
          "form": "짬뽕도 좋네요!",
          "original_form": "짬뽕도 좋네요!",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100039245.1.9",
          "form": "추워져서 그런가봐요ㅎㅎ",
          "original_form": "추워져서 그런가봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039245.1.10",
          "form": "뭐 따끈한 국물이 좋더라구요",
          "original_form": "뭐 따끈한 국물이 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039245.1.11",
          "form": "저녁은 뭐 드세요??",
          "original_form": "저녁은 뭐 드세요??",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039245.1.12",
          "form": "이제 일이 끝나서 고민해봐야할것 같아요",
          "original_form": "이제 일이 끝나서 고민해봐야할것 같아요",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039245.1.13",
          "form": "추천해주실게 있나요?",
          "original_form": "추천해주실게 있나요?",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039245.1.14",
          "form": "전미역국에 생선 먹고 있어요",
          "original_form": "전미역국에 생선 먹고 있어요",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039245.1.15",
          "form": "불금이라 맛있는 거 드시나요??",
          "original_form": "불금이라 맛있는 거 드시나요??",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039245.1.16",
          "form": "맛난거 먹고 싶네요ㅎㅎㅎ",
          "original_form": "맛난거 먹고 싶네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039245.1.17",
          "form": "일주일 동안 일한다고 수고하셨는데",
          "original_form": "일주일 동안 일한다고 수고하셨는데",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039245.1.18",
          "form": "맛있는 거 드세요",
          "original_form": "맛있는 거 드세요",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100039245.1.19",
          "form": "네 ㅎㅎㅅ감사해요",
          "original_form": "네 ㅎㅎㅅ감사해요",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039245.1.20",
          "form": "전어회로 골랐어요ㅎㅎ",
          "original_form": "전어회로 골랐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039245.1.21",
          "form": "전어좋아하세요?",
          "original_form": "전어좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039245.1.22",
          "form": "찾아서 먹을만큼 좋아하는건 아니고",
          "original_form": "찾아서 먹을만큼 좋아하는건 아니고",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039245.1.23",
          "form": "있으면 먹는 편이에요",
          "original_form": "있으면 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039245.1.24",
          "form": "식사 맛있게 하세요",
          "original_form": "식사 맛있게 하세요",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039245.1.25",
          "form": "네 감사합니다~~~~^^",
          "original_form": "네 감사합니다~~~~^^",
          "speaker_id": "1",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039245.1.26",
          "form": "미역국에 생선이면 정말 맛있었겠네요ㅎㅎ",
          "original_form": "미역국에 생선이면 정말 맛있었겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:43"
        }
      ]
