        {
          "id": "MDRW2100004718.1.19",
          "form": "ㅎㅎ 듀오는.... 너무 이르죠ㅎ",
          "original_form": "ㅎㅎ 듀오는.... 너무 이르죠ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004718.1.20",
