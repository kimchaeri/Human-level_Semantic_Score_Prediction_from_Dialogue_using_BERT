      "utterance": [
        {
          "id": "MDRW2100009227.1.1",
          "form": "재택근무 할만 하신가요?",
          "original_form": "재택근무 할만 하신가요?",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009227.1.2",
          "form": "첨엔 좋다고했었는데",
          "original_form": "첨엔 좋다고했었는데",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009227.1.3",
          "form": "별로네요ㅋㅋ",
          "original_form": "별로네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009227.1.4",
          "form": "저도요 ㅠ 애가 없는 재택근무면 좋겠네요",
          "original_form": "저도요 ㅠ 애가 없는 재택근무면 좋겠네요",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009227.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009227.1.6",
          "form": "출근이 해방",
          "original_form": "출근이 해방",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009227.1.7",
          "form": "애랑 같이 재택근무 하니 엉망진창이예요",
          "original_form": "애랑 같이 재택근무 하니 엉망진창이예요",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009227.1.8",
          "form": "암것도 안되요",
          "original_form": "암것도 안되요",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009227.1.9",
          "form": "그냥 육아",
          "original_form": "그냥 육아",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009227.1.10",
          "form": "이번에 줌 연수 듣는데 제가 발표할 차례라 마이크 켰는데 애가 똥 닦아 달라고 했어요 ㅠ ㅜ",
          "original_form": "이번에 줌 연수 듣는데 제가 발표할 차례라 마이크 켰는데 애가 똥 닦아 달라고 했어요 ㅠ ㅜ",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009227.1.11",
          "form": "ㅋㅋ똥",
          "original_form": "ㅋㅋ똥",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009227.1.12",
          "form": "절모한 타이밍",
          "original_form": "절모한 타이밍",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009227.1.13",
          "form": "선생님이 얼릉 똥 닦아주고 오라고 하셨어요 ㅠ 그 뒤로 과일도 깎고 애 옷도 갈아입히고 힘든 재택이었어요",
          "original_form": "선생님이 얼릉 똥 닦아주고 오라고 하셨어요 ㅠ 그 뒤로 과일도 깎고 애 옷도 갈아입히고 힘든 재택이었어요",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009227.1.14",
          "form": "ㅎㅎ 그냥 근무가 나은",
          "original_form": "ㅎㅎ 그냥 근무가 나은",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009227.1.15",
          "form": "맞아요 저만의 시간도 있고요",
          "original_form": "맞아요 저만의 시간도 있고요",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009227.1.16",
          "form": "커피도 마시고",
          "original_form": "커피도 마시고",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009227.1.17",
          "form": "맞아요 어른 사람이랑 얼굴보고 대화도 하고 ㅋㅋ",
          "original_form": "맞아요 어른 사람이랑 얼굴보고 대화도 하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:36"
        }
      ]
