      "utterance": [
        {
          "id": "MDRW2100046718.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211104 15:14"
        },
        {
          "id": "MDRW2100046718.1.2",
          "form": "안녕하세요! 날이 많이 쌀쌀하네요~",
          "original_form": "안녕하세요! 날이 많이 쌀쌀하네요~",
          "speaker_id": "2",
          "time": "20211104 15:15"
        },
        {
          "id": "MDRW2100046718.1.3",
          "form": "네 저희지역은 비가 아침부터 엄청 오더라고요",
          "original_form": "네 저희지역은 비가 아침부터 엄청 오더라고요",
          "speaker_id": "1",
          "time": "20211104 15:15"
        },
        {
          "id": "MDRW2100046718.1.4",
          "form": "그랬군요 여긴 날이 하루종일 흐리기만 하네요~ 꼭 눈 오기 전같이요 ㅋㅋ",
          "original_form": "그랬군요 여긴 날이 하루종일 흐리기만 하네요~ 꼭 눈 오기 전같이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 15:16"
        },
        {
          "id": "MDRW2100046718.1.5",
          "form": "점심은 머드셨어요?",
          "original_form": "점심은 머드셨어요?",
          "speaker_id": "1",
          "time": "20211104 15:16"
        },
        {
          "id": "MDRW2100046718.1.6",
          "form": "저는 분식세트 먹었습니다! ",
          "original_form": "저는 분식세트 먹었습니다! ",
          "speaker_id": "2",
          "time": "20211104 15:16"
        },
        {
          "id": "MDRW2100046718.1.7",
          "form": "맛있는거 드셨나요?",
          "original_form": "맛있는거 드셨나요?",
          "speaker_id": "2",
          "time": "20211104 15:16"
        },
        {
          "id": "MDRW2100046718.1.8",
          "form": "전 백반세트요",
          "original_form": "전 백반세트요",
          "speaker_id": "1",
          "time": "20211104 15:17"
        },
        {
          "id": "MDRW2100046718.1.9",
          "form": "오 반찬은 맛있으셨나요",
          "original_form": "오 반찬은 맛있으셨나요",
          "speaker_id": "2",
          "time": "20211104 15:17"
        },
        {
          "id": "MDRW2100046718.1.10",
          "form": "네 7가지는 됐던거 같아요",
          "original_form": "네 7가지는 됐던거 같아요",
          "speaker_id": "1",
          "time": "20211104 15:17"
        },
        {
          "id": "MDRW2100046718.1.11",
          "form": "와 엄청 많은데요?? 한식뷔페 느낌이네요",
          "original_form": "와 엄청 많은데요?? 한식뷔페 느낌이네요",
          "speaker_id": "2",
          "time": "20211104 15:18"
        },
        {
          "id": "MDRW2100046718.1.12",
          "form": "네 7천원이요",
          "original_form": "네 7천원이요",
          "speaker_id": "1",
          "time": "20211104 15:19"
        },
        {
          "id": "MDRW2100046718.1.13",
          "form": "가성비도 좋네요! 좋아하는 반찬이 있으면 맨날 먹어도 괜찮을것같네요",
          "original_form": "가성비도 좋네요! 좋아하는 반찬이 있으면 맨날 먹어도 괜찮을것같네요",
          "speaker_id": "2",
          "time": "20211104 15:21"
        },
        {
          "id": "MDRW2100046718.1.14",
          "form": "자주 이용해요",
          "original_form": "자주 이용해요",
          "speaker_id": "1",
          "time": "20211104 15:21"
        },
        {
          "id": "MDRW2100046718.1.15",
          "form": "저도 근처에 그런 곳 있으면 좋겠어요 ㅠㅠ ",
          "original_form": "저도 근처에 그런 곳 있으면 좋겠어요 ㅠㅠ ",
          "speaker_id": "2",
          "time": "20211104 15:21"
        },
        {
          "id": "MDRW2100046718.1.16",
          "form": "맨날 메뉴 고민하는것도 일이니깐요 ㅎ",
          "original_form": "맨날 메뉴 고민하는것도 일이니깐요 ㅎ",
          "speaker_id": "2",
          "time": "20211104 15:22"
        }
      ]
