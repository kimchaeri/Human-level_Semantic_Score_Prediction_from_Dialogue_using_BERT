      "utterance": [
        {
          "id": "MDRW2100009715.1.1",
          "form": "name2이님도 싸이월드 세대시죠?",
          "original_form": "&name2&이님도 싸이월드 세대시죠?",
          "speaker_id": "1",
          "time": "20210823 08:05"
        },
        {
          "id": "MDRW2100009715.1.2",
          "form": "네 ㅋㅋ 저 싸이세대에요",
          "original_form": "네 ㅋㅋ 저 싸이세대에요",
          "speaker_id": "2",
          "time": "20210823 08:27"
        },
        {
          "id": "MDRW2100009715.1.3",
          "form": "이번에 싸이 계정 복구하셨어요?",
          "original_form": "이번에 싸이 계정 복구하셨어요?",
          "speaker_id": "1",
          "time": "20210823 08:28"
        },
        {
          "id": "MDRW2100009715.1.4",
          "form": "복구신청 해놓고 까먹었어요",
          "original_form": "복구신청 해놓고 까먹었어요",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100009715.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:53"
        },
        {
          "id": "MDRW2100009715.1.6",
          "form": "거기 흑역사 사진 겁나 많은데",
          "original_form": "거기 흑역사 사진 겁나 많은데",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100009715.1.7",
          "form": "군대때 사진도 있고...",
          "original_form": "군대때 사진도 있고...",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100009715.1.8",
          "form": "여는 순간 이불킥 예상합니다ㅋㅋㅋㅋ",
          "original_form": "여는 순간 이불킥 예상합니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100009715.1.9",
          "form": "진짜 옛날에 사겼던 여친하고 찍은사진도 있을텐데...",
          "original_form": "진짜 옛날에 사겼던 여친하고 찍은사진도 있을텐데...",
          "speaker_id": "2",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100009715.1.10",
          "form": "저도 아무래도 있을거 같아서",
          "original_form": "저도 아무래도 있을거 같아서",
          "speaker_id": "1",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100009715.1.11",
          "form": "가정의 평화를 위해\r\n영원히 묻어둘까봐여ㅋㅋㅋ",
          "original_form": "가정의 평화를 위해\r\n영원히 묻어둘까봐여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:01"
        },
        {
          "id": "MDRW2100009715.1.12",
          "form": "여자여자했던 그때",
          "original_form": "여자여자했던 그때",
          "speaker_id": "2",
          "time": "20210823 21:01"
        },
        {
          "id": "MDRW2100009715.1.13",
          "form": "그 사진들 다 버리시게여?ㅋㅋㅋ",
          "original_form": "그 사진들 다 버리시게여?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100009715.1.14",
          "form": "지금도 입다물고 있음\r\n여자여자해욧! 왜그러세여ㅋㅋㅋ",
          "original_form": "지금도 입다물고 있음\r\n여자여자해욧! 왜그러세여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100009715.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100009715.1.16",
          "form": "너무 발끈하시는데요",
          "original_form": "너무 발끈하시는데요",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100009715.1.17",
          "form": "ㅠㅠ 다시는 못 돌아갈\r\n그때가 너무 그립네요ㅋㅋ",
          "original_form": "ㅠㅠ 다시는 못 돌아갈\r\n그때가 너무 그립네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100009715.1.18",
          "form": "리즈시절 사진들 다 거기있겠죠",
          "original_form": "리즈시절 사진들 다 거기있겠죠",
          "speaker_id": "1",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100009715.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100009715.1.20",
          "form": "보고싶지 않은 사진까지 모두다",
          "original_form": "보고싶지 않은 사진까지 모두다",
          "speaker_id": "2",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100009715.1.21",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100009715.1.22",
          "form": "저도 그때가 그립네요",
          "original_form": "저도 그때가 그립네요",
          "speaker_id": "2",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100009715.1.23",
          "form": "근데 생각해보면 시절인연이라고",
          "original_form": "근데 생각해보면 시절인연이라고",
          "speaker_id": "1",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100009715.1.24",
          "form": "싸이시절 친했던 나의 일촌들은\r\n다 어디간걸까여 ㅋㅋㅋ",
          "original_form": "싸이시절 친했던 나의 일촌들은\r\n다 어디간걸까여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100009715.1.25",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100009715.1.26",
          "form": "그 일촌들 다 어디간건지...",
          "original_form": "그 일촌들 다 어디간건지...",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100009715.1.27",
          "form": "다들 어디서 뭐하고 살까요\r\n우리의 일촌들ㅋㅋㅋㅋㅋ",
          "original_form": "다들 어디서 뭐하고 살까요\r\n우리의 일촌들ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100009715.1.28",
          "form": "그때 일촌평좀 써달라고 그렇게 얘기했었는데 ㅋㅋㅋ",
          "original_form": "그때 일촌평좀 써달라고 그렇게 얘기했었는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100009715.1.29",
          "form": "기억나세요?",
          "original_form": "기억나세요?",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100009715.1.30",
          "form": "ㅋㅋ저는 안그랬었...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ저는 안그랬었...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100009715.1.31",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100009715.1.32",
          "form": "나만그랬구나",
          "original_form": "나만그랬구나",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100009715.1.33",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100009715.1.34",
          "form": "다 추억이네요",
          "original_form": "다 추억이네요",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100009715.1.35",
          "form": "저는 일촌도 가려서 받았었어요ㅋㅋ",
          "original_form": "저는 일촌도 가려서 받았었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100009715.1.36",
          "form": "측근이 아님 일촌 놉!",
          "original_form": "측근이 아님 일촌 놉!",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100009715.1.37",
          "form": "ㅋㅋㅋㅋ 아주 폐쇄적이군요",
          "original_form": "ㅋㅋㅋㅋ 아주 폐쇄적이군요",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100009715.1.38",
          "form": "전 기억이 안나요 일촌이 몇명이나되는지도",
          "original_form": "전 기억이 안나요 일촌이 몇명이나되는지도",
          "speaker_id": "2",
          "time": "20210823 23:40"
        }
      ]
    }
