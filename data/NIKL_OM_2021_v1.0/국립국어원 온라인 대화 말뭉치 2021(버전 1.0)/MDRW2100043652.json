      "utterance": [
        {
          "id": "MDRW2100043652.1.1",
          "form": "비가 계속 오네요",
          "original_form": "비가 계속 오네요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043652.1.2",
          "form": "아직도 비가내리나요~~ 여기도 아까 뉴스보니까",
          "original_form": "아직도 비가내리나요~~ 여기도 아까 뉴스보니까",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043652.1.3",
          "form": "밤에도 비올거라고 하긴하더라구요ㅠㅠ",
          "original_form": "밤에도 비올거라고 하긴하더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043652.1.4",
          "form": "습한것만빼면",
          "original_form": "습한것만빼면",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043652.1.5",
          "form": "밤에오는건 아주찬성인데요 ㅋㅋ아침에만 안왔으면",
          "original_form": "밤에오는건 아주찬성인데요 ㅋㅋ아침에만 안왔으면",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043652.1.6",
          "form": "네 비가 계속 와요",
          "original_form": "네 비가 계속 와요",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043652.1.7",
          "form": "저도 밤비는 좋은데",
          "original_form": "저도 밤비는 좋은데",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043652.1.8",
          "form": "출근길 비는 싫어요ㅜ",
          "original_form": "출근길 비는 싫어요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043652.1.9",
          "form": "그쵸 저두요ㅠㅠ 아침에 비가쪼끔오는데 모르고 등원시키러 나갔다가",
          "original_form": "그쵸 저두요ㅠㅠ 아침에 비가쪼끔오는데 모르고 등원시키러 나갔다가",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043652.1.10",
          "form": "비가점점 많이내려서",
          "original_form": "비가점점 많이내려서",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043652.1.11",
          "form": "에고 고생하셨겠다ㅠ",
          "original_form": "에고 고생하셨겠다ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043652.1.12",
          "form": "다행히 아는언니가 애등원시키고  지나가다 저에게 우산토스해줘서 쓰고 갔네요 ㅋㅋㅋㅋ",
          "original_form": "다행히 아는언니가 애등원시키고  지나가다 저에게 우산토스해줘서 쓰고 갔네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043652.1.13",
          "form": "오늘 내내 비가 갑자기 확쏟아졌다 바로 멈췄다 또 확 쏟아졌다 말다 하더라구요",
          "original_form": "오늘 내내 비가 갑자기 확쏟아졌다 바로 멈췄다 또 확 쏟아졌다 말다 하더라구요",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043652.1.14",
          "form": "우산펼시간에도 맞을정도로요",
          "original_form": "우산펼시간에도 맞을정도로요",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043652.1.15",
          "form": "아이고 갑자기 비가 쏟아졌구나",
          "original_form": "아이고 갑자기 비가 쏟아졌구나",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043652.1.16",
          "form": "그래서 주변분들이",
          "original_form": "그래서 주변분들이",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043652.1.17",
          "form": "무슨 날씨가 이렇게 땡땡맞냐고 욕하시더라구요 ㅎㅎㅎ",
          "original_form": "무슨 날씨가 이렇게 땡땡맞냐고 욕하시더라구요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043652.1.18",
          "form": "아아 그럴만하겠어요",
          "original_form": "아아 그럴만하겠어요",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043652.1.19",
          "form": "이번 여름에도 막 갑자기 비 쏟아지고 그런 날들이 많았잖아요",
          "original_form": "이번 여름에도 막 갑자기 비 쏟아지고 그런 날들이 많았잖아요",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043652.1.20",
          "form": "그쵸 올해유독 심한거 같아요",
          "original_form": "그쵸 올해유독 심한거 같아요",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043652.1.21",
          "form": "그래도 내일부턴 드뎌 비소식이없어요",
          "original_form": "그래도 내일부턴 드뎌 비소식이없어요",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043652.1.22",
          "form": "아 그런가요?",
          "original_form": "아 그런가요?",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043652.1.23",
          "form": "맑은 날 그립네요",
          "original_form": "맑은 날 그립네요",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043652.1.24",
          "form": "근데 빨래말릴만큼",
          "original_form": "근데 빨래말릴만큼",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043652.1.25",
          "form": "햇빛쨍쨍 날씨는 아닌거같아요!",
          "original_form": "햇빛쨍쨍 날씨는 아닌거같아요!",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043652.1.26",
          "form": "어제 조금 쌀쌀했다고 애들 바람막이 입고나왔던데",
          "original_form": "어제 조금 쌀쌀했다고 애들 바람막이 입고나왔던데",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043652.1.27",
          "form": "오늘은 또 덥더라구요;",
          "original_form": "오늘은 또 덥더라구요;",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043652.1.28",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043652.1.29",
          "form": "저흰 비 와서 쌀쌀했어요",
          "original_form": "저흰 비 와서 쌀쌀했어요",
          "speaker_id": "1",
          "time": "20210907 21:21"
        }
      ]
    }
