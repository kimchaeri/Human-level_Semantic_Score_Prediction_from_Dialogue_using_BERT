      "utterance": [
        {
          "id": "MDRW2100013829.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013829.1.2",
          "form": "네~ 오늘도 했네요!",
          "original_form": "네~ 오늘도 했네요!",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013829.1.3",
          "form": "매일 하나요",
          "original_form": "매일 하나요",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013829.1.4",
          "form": "2일에 1번씩 해요!",
          "original_form": "2일에 1번씩 해요!",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013829.1.5",
          "form": "자주 하네뇨",
          "original_form": "자주 하네뇨",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013829.1.6",
          "form": "네 좀 자주하는 편이긴하죠!",
          "original_form": "네 좀 자주하는 편이긴하죠!",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013829.1.7",
          "form": "먼지가 좀 자주 쌓이는거같아서 ㅠㅠ",
          "original_form": "먼지가 좀 자주 쌓이는거같아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013829.1.8",
          "form": "그렇져",
          "original_form": "그렇져",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013829.1.9",
          "form": "그래서 그런지 좀 자주하는 편이에요!",
          "original_form": "그래서 그런지 좀 자주하는 편이에요!",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013829.1.10",
          "form": "전 화장실청소요",
          "original_form": "전 화장실청소요",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013829.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013829.1.12",
          "form": "저는 주로 안방청소 설거지 빨래하고있어요!",
          "original_form": "저는 주로 안방청소 설거지 빨래하고있어요!",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013829.1.13",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013829.1.14",
          "form": "저도 락스냄새땜에",
          "original_form": "저도 락스냄새땜에",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013829.1.15",
          "form": "화장실청소는 안하구있어요 ㅠㅠ",
          "original_form": "화장실청소는 안하구있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013829.1.16",
