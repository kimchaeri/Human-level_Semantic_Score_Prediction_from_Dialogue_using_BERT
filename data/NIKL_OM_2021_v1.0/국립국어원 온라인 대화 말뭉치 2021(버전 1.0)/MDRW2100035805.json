      "utterance": [
        {
          "id": "MDRW2100035805.1.1",
          "form": "나 오늘 결혼식 갔다 왔어",
          "original_form": "나 오늘 결혼식 갔다 왔어",
          "speaker_id": "2",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.2",
          "form": "누구 결혼식",
          "original_form": "누구 결혼식",
          "speaker_id": "1",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.3",
          "form": "말해도 모를걸",
          "original_form": "말해도 모를걸",
          "speaker_id": "2",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.4",
          "form": "직장 동료인가 보네",
          "original_form": "직장 동료인가 보네",
          "speaker_id": "1",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.5",
          "form": "그렇게 친한 사이는 아니야",
          "original_form": "그렇게 친한 사이는 아니야",
          "speaker_id": "2",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.6",
          "form": "결혼식 보고 오면 결혼이 하고 싶어진다던데",
          "original_form": "결혼식 보고 오면 결혼이 하고 싶어진다던데",
          "speaker_id": "1",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.7",
          "form": "나는 그렇지는 않더라",
          "original_form": "나는 그렇지는 않더라",
          "speaker_id": "2",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.8",
          "form": "이유가 있는 건가",
          "original_form": "이유가 있는 건가",
          "speaker_id": "1",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.9",
          "form": "준비 과정을 봤는데 그렇게 고생했는데 이렇게 하루에 끝나니 좀 허탈해",
          "original_form": "준비 과정을 봤는데 그렇게 고생했는데 이렇게 하루에 끝나니 좀 허탈해",
          "speaker_id": "2",
          "time": "20210830 03:06"
        },
        {
          "id": "MDRW2100035805.1.10",
          "form": "그래도 결혼식장에서 무척 이뻤을 거 아니야",
          "original_form": "그래도 결혼식장에서 무척 이뻤을 거 아니야",
          "speaker_id": "1",
          "time": "20210830 03:07"
        },
        {
          "id": "MDRW2100035805.1.11",
          "form": "그러긴 하지만 힘들어 보이기도 했어",
          "original_form": "그러긴 하지만 힘들어 보이기도 했어",
          "speaker_id": "2",
          "time": "20210830 03:07"
        },
        {
          "id": "MDRW2100035805.1.12",
          "form": "결혼에 대한 로망은 별로 없구나",
          "original_form": "결혼에 대한 로망은 별로 없구나",
          "speaker_id": "1",
          "time": "20210830 03:07"
        },
        {
          "id": "MDRW2100035805.1.13",
          "form": "아직은 너무 먼 얘기 같다",
          "original_form": "아직은 너무 먼 얘기 같다",
          "speaker_id": "2",
          "time": "20210830 03:07"
        }
      ]
