      "utterance": [
        {
          "id": "MDRW2100040864.1.1",
          "form": "안녕하세요 아침 드셨나요?",
          "original_form": "안녕하세요 아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 10:23"
        },
        {
          "id": "MDRW2100040864.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100040864.1.3",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100040864.1.4",
          "form": "아니요 방금 일어나서 아직입니다ㅎㅎ",
          "original_form": "아니요 방금 일어나서 아직입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100040864.1.5",
          "form": "아침 뭐드셨나요?",
          "original_form": "아침 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100040864.1.6",
          "form": "저는 늙은호박전이요",
          "original_form": "저는 늙은호박전이요",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100040864.1.7",
          "form": "집에 호박이 생겨서 어제 잔뜩 전부쳐 놓았어요",
          "original_form": "집에 호박이 생겨서 어제 잔뜩 전부쳐 놓았어요",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100040864.1.8",
          "form": "호박전 안먹은지 오래됐네요!",
          "original_form": "호박전 안먹은지 오래됐네요!",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100040864.1.9",
          "form": "호박전 좋아하시나요??",
          "original_form": "호박전 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100040864.1.10",
          "form": "그냥 소소인데",
          "original_form": "그냥 소소인데",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100040864.1.11",
          "form": "호박이 생겨서 선택의 여지가 없었어요",
          "original_form": "호박이 생겨서 선택의 여지가 없었어요",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100040864.1.12",
          "form": "늙은호박은 죽이나 전 정도로 먹어야할 듯해서",
          "original_form": "늙은호박은 죽이나 전 정도로 먹어야할 듯해서",
          "speaker_id": "2",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100040864.1.13",
          "form": "호박전 얘기하니까 저도 오랜만에 먹고싶네요ㅎㅎ",
          "original_form": "호박전 얘기하니까 저도 오랜만에 먹고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100040864.1.14",
          "form": "가장 자주 먹는 음식 있으신가요?",
          "original_form": "가장 자주 먹는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040864.1.15",
          "form": "음 최근에는 양배추로 코울슬로 만들어 매일 아침에 먹었어요",
          "original_form": "음 최근에는 양배추로 코울슬로 만들어 매일 아침에 먹었어요",
          "speaker_id": "2",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100040864.1.16",
          "form": "우와 뭔가 건강한 음식으로 잘드시네요!",
          "original_form": "우와 뭔가 건강한 음식으로 잘드시네요!",
          "speaker_id": "1",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100040864.1.17",
          "form": "ㅎㅎ저것 말고 고칼로리나 자극적인 음식도 자주 먹어요",
          "original_form": "ㅎㅎ저것 말고 고칼로리나 자극적인 음식도 자주 먹어요",
          "speaker_id": "2",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100040864.1.18",
          "form": "요리도 직접 하시는걸 보니 요리에 관심이 많으신가요?",
          "original_form": "요리도 직접 하시는걸 보니 요리에 관심이 많으신가요?",
          "speaker_id": "1",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100040864.1.19",
          "form": "아 요리 자주하진 않고 돕는 정도예요",
          "original_form": "아 요리 자주하진 않고 돕는 정도예요",
          "speaker_id": "2",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100040864.1.20",
          "form": "아 배달음식도 드시나요??",
          "original_form": "아 배달음식도 드시나요??",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100040864.1.21",
          "form": "네네 배달음식도 자주 먹어요",
          "original_form": "네네 배달음식도 자주 먹어요",
          "speaker_id": "2",
          "time": "20210905 10:40"
        }
      ]
