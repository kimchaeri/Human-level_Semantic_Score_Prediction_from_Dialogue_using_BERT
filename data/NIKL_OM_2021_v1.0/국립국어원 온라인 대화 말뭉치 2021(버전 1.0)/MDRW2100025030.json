      "utterance": [
        {
          "id": "MDRW2100025030.1.1",
          "form": "가끔 보러 가는 편이에요",
          "original_form": "가끔 보러 가는 편이에요",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025030.1.2",
          "form": "특별히 좋았던 공연 있나요?",
          "original_form": "특별히 좋았던 공연 있나요?",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025030.1.3",
          "form": "저는 많이는 못가봤는데 레베카 좋았어요",
          "original_form": "저는 많이는 못가봤는데 레베카 좋았어요",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025030.1.4",
          "form": "코로나 이후로 많이 못가서",
          "original_form": "코로나 이후로 많이 못가서",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025030.1.5",
          "form": "콘서트나 영화는 많이 봤었어요 코로나 전에는",
          "original_form": "콘서트나 영화는 많이 봤었어요 코로나 전에는",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025030.1.6",
          "form": "레베카 유명하던데 아직 못 봤어요 궁금하네요",
          "original_form": "레베카 유명하던데 아직 못 봤어요 궁금하네요",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025030.1.7",
          "form": "옥주현이 하는 역할이 레베카가 아니라면서요?ㅎㅎ",
          "original_form": "옥주현이 하는 역할이 레베카가 아니라면서요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025030.1.8",
          "form": "맞아요ㅎ 레베카는 나오지도 않아요",
          "original_form": "맞아요ㅎ 레베카는 나오지도 않아요",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025030.1.9",
          "form": "레베카는 옥주현이 모시던 죽은 사모님(?) 이에요",
          "original_form": "레베카는 옥주현이 모시던 죽은 사모님(?) 이에요",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025030.1.10",
          "form": "저는 옥주현 무대는 못봤고 다른분꺼봤는데 장난아니더라고요",
          "original_form": "저는 옥주현 무대는 못봤고 다른분꺼봤는데 장난아니더라고요",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025030.1.11",
          "form": "뭔가 사연이 있나보네요ㅋㅋ 언젠가 기회가 된다면 꼭..",
          "original_form": "뭔가 사연이 있나보네요ㅋㅋ 언젠가 기회가 된다면 꼭..",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025030.1.12",
          "form": "몇 달 전에 미나리 보러 극장 갔었는데 사람이 한 4~5명 정도 있었던 거 같아요",
          "original_form": "몇 달 전에 미나리 보러 극장 갔었는데 사람이 한 4~5명 정도 있었던 거 같아요",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025030.1.13",
          "form": "기회가 된다면 봐보세용",
          "original_form": "기회가 된다면 봐보세용",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025030.1.14",
          "form": "오 미나리 아직 못봤는데 재밌나요?",
          "original_form": "오 미나리 아직 못봤는데 재밌나요?",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025030.1.15",
          "form": "영화관도 진짜 안간지 오래된거같아요",
          "original_form": "영화관도 진짜 안간지 오래된거같아요",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100025030.1.16",
          "form": "볼만하더라구요ㅋㅋ 나중에 시간 나면 봐보셔도 좋을 영화였어요",
          "original_form": "볼만하더라구요ㅋㅋ 나중에 시간 나면 봐보셔도 좋을 영화였어요",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100025030.1.17",
          "form": "그럴게요 ㅎ",
          "original_form": "그럴게요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025030.1.18",
          "form": "오랜만에 이번주 일욜에 극장가보려고요",
          "original_form": "오랜만에 이번주 일욜에 극장가보려고요",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025030.1.19",
          "form": "영화관",
          "original_form": "영화관",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025030.1.20",
          "form": "뭐 보실 예정이에요?",
          "original_form": "뭐 보실 예정이에요?",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025030.1.21",
          "form": "아직 정확히 정한건 아닌데 예매순위 1위인거 볼거같아요",
          "original_form": "아직 정확히 정한건 아닌데 예매순위 1위인거 볼거같아요",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025030.1.22",
          "form": "샹치...? 맞나",
          "original_form": "샹치...? 맞나",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025030.1.23",
          "form": "마블영화라는데 볼만한게 그거밖에 없어보이는",
          "original_form": "마블영화라는데 볼만한게 그거밖에 없어보이는",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025030.1.24",
          "form": "요새 뭐 하는지를 잘 모르겠네요 ㅎㅎ 마블이면 그래도 볼만하겠죠",
          "original_form": "요새 뭐 하는지를 잘 모르겠네요 ㅎㅎ 마블이면 그래도 볼만하겠죠",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025030.1.25",
          "form": "저도요 진짜 모르겠더라고요",
          "original_form": "저도요 진짜 모르겠더라고요",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025030.1.26",
          "form": "황정민 영화도 나왔더라고요 ㅎ",
          "original_form": "황정민 영화도 나왔더라고요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025030.1.27",
          "form": "아 홍보하러 나왔던 거 같은데 내용은 기억이 안 나네요 ㅎㅎ",
          "original_form": "아 홍보하러 나왔던 거 같은데 내용은 기억이 안 나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:55"
        }
      ]
    }
