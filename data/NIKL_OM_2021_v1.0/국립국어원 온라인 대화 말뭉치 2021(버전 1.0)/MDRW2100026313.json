      "utterance": [
        {
          "id": "MDRW2100026313.1.1",
          "form": "수요일엔 뭐가 재밋을까요",
          "original_form": "수요일엔 뭐가 재밋을까요",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026313.1.2",
          "form": "잘모르겠는데 수요일에 뭐하나요?",
          "original_form": "잘모르겠는데 수요일에 뭐하나요?",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026313.1.3",
          "form": "저도 몰라서 방황중이에요 ㅜㅜ",
          "original_form": "저도 몰라서 방황중이에요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026313.1.4",
          "form": "혹시 티비보다 잠에 드셨나요",
          "original_form": "혹시 티비보다 잠에 드셨나요",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026313.1.5",
          "form": "ㅋㅋ네 잠들었어요 유투브도보시나요",
          "original_form": "ㅋㅋ네 잠들었어요 유투브도보시나요",
          "speaker_id": "2",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100026313.1.6",
          "form": "네 저는 고양이 여행 먹방 다봐요 ㅋㅋ",
          "original_form": "네 저는 고양이 여행 먹방 다봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:56"
        },
        {
          "id": "MDRW2100026313.1.7",
          "form": "뭐 보시나요.",
          "original_form": "뭐 보시나요.",
          "speaker_id": "1",
          "time": "20210902 08:56"
        },
        {
          "id": "MDRW2100026313.1.8",
          "form": "저도 강아지, 여행이나 가끔 요리도보고 가수 무대영상도 보고요 주로 음악도 많이들어요",
          "original_form": "저도 강아지, 여행이나 가끔 요리도보고 가수 무대영상도 보고요 주로 음악도 많이들어요",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100026313.1.9",
          "form": "오 여행 어디 재밌는곳 추천방송 있나요",
          "original_form": "오 여행 어디 재밌는곳 추천방송 있나요",
          "speaker_id": "1",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100026313.1.10",
          "form": "한번본거라 저도 잘모르는데 여행 유투버가 꽤 많더라구요",
          "original_form": "한번본거라 저도 잘모르는데 여행 유투버가 꽤 많더라구요",
          "speaker_id": "2",
          "time": "20210902 09:10"
        },
        {
          "id": "MDRW2100026313.1.11",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100026313.1.12",
          "form": "빠니보틀 추천합니다",
          "original_form": "빠니보틀 추천합니다",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100026313.1.13",
          "form": "보신적있나요",
          "original_form": "보신적있나요",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100026313.1.14",
          "form": "아니요 유명한가요?",
          "original_form": "아니요 유명한가요?",
          "speaker_id": "2",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100026313.1.15",
          "form": "네 코로나인데 또 외국 가셨던데요 ㅋㅋ",
          "original_form": "네 코로나인데 또 외국 가셨던데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100026313.1.16",
          "form": "인도 기차편 너무 잼써요",
          "original_form": "인도 기차편 너무 잼써요",
          "speaker_id": "1",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100026313.1.17",
          "form": "그렇군요 봐야겠네요",
          "original_form": "그렇군요 봐야겠네요",
          "speaker_id": "2",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100026313.1.18",
          "form": "저도 외국 가고싶어요",
          "original_form": "저도 외국 가고싶어요",
          "speaker_id": "1",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100026313.1.19",
          "form": "저도요 외국가면 평범한 집 담벼락도 신기할거같고 재밌을것같아요ㅋ",
          "original_form": "저도요 외국가면 평범한 집 담벼락도 신기할거같고 재밌을것같아요ㅋ",
          "speaker_id": "2",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100026313.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100026313.1.21",
          "form": "색달라서 재밌을듯 ㅎㅎ",
          "original_form": "색달라서 재밌을듯 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:27"
        }
      ]
