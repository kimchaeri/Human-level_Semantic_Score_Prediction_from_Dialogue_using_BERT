      "utterance": [
        {
          "id": "MDRW2100016217.1.1",
          "form": "점심",
          "original_form": "점심",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016217.1.2",
          "form": "맛나게 드세요",
          "original_form": "맛나게 드세요",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016217.1.3",
          "form": "저도 배고파요",
          "original_form": "저도 배고파요",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016217.1.4",
          "form": "전 월남쌈 먹고싶어요",
          "original_form": "전 월남쌈 먹고싶어요",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016217.1.5",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016217.1.6",
          "form": "함냠",
          "original_form": "함냠",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016217.1.7",
          "form": "근데 요즘 시국에 월남쌈 가능항지 모르겠어요",
          "original_form": "근데 요즘 시국에 월남쌈 가능항지 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016217.1.8",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016217.1.9",
          "form": "월남쌈 손으로 안싸드세요?",
          "original_form": "월남쌈 손으로 안싸드세요?",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016217.1.10",
          "form": "젓가락을 하는데요",
          "original_form": "젓가락을 하는데요",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.11",
          "form": "손은 잘 닦으면 되죠",
          "original_form": "손은 잘 닦으면 되죠",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.12",
          "form": "찝찝하잖아요",
          "original_form": "찝찝하잖아요",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.13",
          "form": "아 예",
          "original_form": "아 예",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.14",
          "form": "쌀국수 좋아하세요?",
          "original_form": "쌀국수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.15",
          "form": "월남쌈은 손으로 싸드셔야해요",
          "original_form": "월남쌈은 손으로 싸드셔야해요",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.16",
          "form": "헐 저랑 통하셨네요",
          "original_form": "헐 저랑 통하셨네요",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.17",
          "form": "전 젓가락으로 빱가능하거든요",
          "original_form": "전 젓가락으로 빱가능하거든요",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.18",
          "form": "졔가 물어보려고 했는데",
          "original_form": "졔가 물어보려고 했는데",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.19",
          "form": "네 저 완전 좋아해요",
          "original_form": "네 저 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016217.1.20",
          "form": "고수도 넣어먹어요",
          "original_form": "고수도 넣어먹어요",
          "speaker_id": "2",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016217.1.21",
          "form": "자꾸 운명의 상대인척 하시네요",
          "original_form": "자꾸 운명의 상대인척 하시네요",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016217.1.22",
          "form": "님음요",
          "original_form": "님음요",
          "speaker_id": "2",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016217.1.23",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016217.1.24",
          "form": "아네 죄송함다",
          "original_form": "아네 죄송함다",
          "speaker_id": "2",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016217.1.25",
          "form": "자제좀요^^",
          "original_form": "자제좀요^^",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016217.1.26",
          "form": "아 네",
