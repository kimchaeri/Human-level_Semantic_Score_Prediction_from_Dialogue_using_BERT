        },
        {
          "id": "MDRW2100015491.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ마자",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ마자",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015491.1.4",
          "form": "근데 꽤 예전이자나",
          "original_form": "근데 꽤 예전이자나",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015491.1.5",
          "form": "아직도 연락하는거 신기해",
          "original_form": "아직도 연락하는거 신기해",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015491.1.6",
          "form": "음... 한 번 사람 사귈때... 깊게 사귀는 편이라 긍가",
          "original_form": "음... 한 번 사람 사귈때... 깊게 사귀는 편이라 긍가",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015491.1.7",
          "form": "근데 내가 좀 예전에",
          "original_form": "근데 내가 좀 예전에",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015491.1.8",
          "form": "주변 언니들한테",
          "original_form": "주변 언니들한테",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015491.1.9",
          "form": "귀염많이받던 타입인것두 이따",
          "original_form": "귀염많이받던 타입인것두 이따",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015491.1.10",
          "form": "이열ㅋㅋㅋㅋㅋ 대단타",
          "original_form": "이열ㅋㅋㅋㅋㅋ 대단타",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015491.1.11",
          "form": "너 광주 살았던때면",
          "original_form": "너 광주 살았던때면",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015491.1.12",
          "form": "초딩 5학년? 그쯤아냐?",
          "original_form": "초딩 5학년? 그쯤아냐?",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015491.1.13",
          "form": "난 초딩 친구들도차업는데",
          "original_form": "난 초딩 친구들도차업는데",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015491.1.14",
          "form": "넌 언니들이랑 아직 연락을 한다니까 신기해 ㅋㅋㅋ",
          "original_form": "넌 언니들이랑 아직 연락을 한다니까 신기해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015491.1.15",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ초5 잼 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ초5 잼 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015491.1.16",
          "form": "근데 이 언닌 초딩때 안 언니 아냐",
          "original_form": "근데 이 언닌 초딩때 안 언니 아냐",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015491.1.17",
          "form": "중1때 안 언니임",
          "original_form": "중1때 안 언니임",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015491.1.18",
          "form": "초등학교때 나랑 같은 초등학교였던 다른 언니랑",
          "original_form": "초등학교때 나랑 같은 초등학교였던 다른 언니랑",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015491.1.19",
          "form": "아는 사인데",
          "original_form": "아는 사인데",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015491.1.20",
          "form": "그 같은 초등학교였던 언니랑은 오히려 연락안하고",
          "original_form": "그 같은 초등학교였던 언니랑은 오히려 연락안하고",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015491.1.21",
          "form": "이언니랑만 연락함ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이언니랑만 연락함ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015491.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015491.1.23",
