      "utterance": [
        {
          "id": "MDRW2100041262.1.1",
          "form": "학교때는 뭐 공부하셨어요?",
          "original_form": "학교때는 뭐 공부하셨어요?",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041262.1.2",
          "form": "해양경영 공부했습니당ㅋㄲㅋ \r\n재미드럽게없었어여..",
          "original_form": "해양경영 공부했습니당ㅋㄲㅋ \r\n재미드럽게없었어여..",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041262.1.3",
          "form": "어떤거 공부하셨어요!",
          "original_form": "어떤거 공부하셨어요!",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041262.1.4",
          "form": "우와!!!",
          "original_form": "우와!!!",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041262.1.5",
          "form": "전 국제경영ㅋㅋ",
          "original_form": "전 국제경영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041262.1.6",
          "form": "전공 살리셨나요?",
          "original_form": "전공 살리셨나요?",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041262.1.7",
          "form": "오ㅋㅋㅋㅋ경영라인이시군요ㅋㅋㅋ\r\n아뇨전혀..ㅋㅋㅋㅋ\r\n전공살리셧나용",
          "original_form": "오ㅋㅋㅋㅋ경영라인이시군요ㅋㅋㅋ\r\n아뇨전혀..ㅋㅋㅋㅋ\r\n전공살리셧나용",
          "speaker_id": "2",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100041262.1.8",
          "form": "ㅋㅋ 전 그냥 작은 회사라 살리긴 한거같은데ㅋㅋ",
          "original_form": "ㅋㅋ 전 그냥 작은 회사라 살리긴 한거같은데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100041262.1.9",
          "form": "썩ㅋㅋ 전공 학점도ㅋㅋ 웃음만ㅋㅋ",
          "original_form": "썩ㅋㅋ 전공 학점도ㅋㅋ 웃음만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100041262.1.10",
          "form": "하하하전공학점...;-;  할말없군요저도ㅋㅋㅋ\r\n나름살리셧다니괜찮네용",
          "original_form": "하하하전공학점...;-;  할말없군요저도ㅋㅋㅋ\r\n나름살리셧다니괜찮네용",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100041262.1.11",
          "form": "ㅋㅋ 남녀공학 나오셨죠?",
          "original_form": "ㅋㅋ 남녀공학 나오셨죠?",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041262.1.12",
          "form": "네네ㅋㅋㅋ! 남녀공학나오셨나용?",
          "original_form": "네네ㅋㅋㅋ! 남녀공학나오셨나용?",
          "speaker_id": "2",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100041262.1.13",
          "form": "아녀ㅋㅋ 여대ㅋㅋ",
          "original_form": "아녀ㅋㅋ 여대ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100041262.1.14",
          "form": "동기군대송별회 그런거 부러웠어요ㅋㅋ",
          "original_form": "동기군대송별회 그런거 부러웠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100041262.1.15",
          "form": "앗ㅋㅋ그런것이 부러우셨을수도있겠네요ㅋㄲ 여대면 미팅해보셨나요ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋ그런것이 부러우셨을수도있겠네요ㅋㄲ 여대면 미팅해보셨나요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041262.1.16",
          "form": "그거시 빈익빈부익부라 하는 친구들은 하루에 몇탕ㅋㅋ",
          "original_form": "그거시 빈익빈부익부라 하는 친구들은 하루에 몇탕ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041262.1.17",
          "form": "와 하루에 몇탕이라니ㅋㅋㅋ",
          "original_form": "와 하루에 몇탕이라니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100041262.1.18",
          "form": "신기해요헙",
          "original_form": "신기해요헙",
          "speaker_id": "2",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100041262.1.19",
          "form": "안하는 친구들은 전혀ㅋㅋ",
