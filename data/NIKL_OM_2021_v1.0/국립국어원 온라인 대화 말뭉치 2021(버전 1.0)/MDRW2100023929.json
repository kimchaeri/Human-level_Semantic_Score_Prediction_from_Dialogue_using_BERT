      "utterance": [
        {
          "id": "MDRW2100023929.1.1",
          "form": "name1님은 중고거래 해보셨나요?",
          "original_form": "&name1&님은 중고거래 해보셨나요?",
          "speaker_id": "2",
          "time": "20210831 06:25"
        },
        {
          "id": "MDRW2100023929.1.2",
          "form": "넵!!",
          "original_form": "넵!!",
          "speaker_id": "1",
          "time": "20210831 06:49"
        },
        {
          "id": "MDRW2100023929.1.3",
          "form": "당근마켓 가끔이용해요 ㅎㅎ",
          "original_form": "당근마켓 가끔이용해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 06:49"
        },
        {
          "id": "MDRW2100023929.1.4",
          "form": "name2님은요~?",
          "original_form": "&name2&님은요~?",
          "speaker_id": "1",
          "time": "20210831 06:49"
        },
        {
          "id": "MDRW2100023929.1.5",
          "form": "저도 당근이요 ㅎㅎ 그것만 해요",
          "original_form": "저도 당근이요 ㅎㅎ 그것만 해요",
          "speaker_id": "2",
          "time": "20210831 07:10"
        },
        {
          "id": "MDRW2100023929.1.6",
          "form": "많이 파셨나요?",
          "original_form": "많이 파셨나요?",
          "speaker_id": "2",
          "time": "20210831 07:10"
        },
        {
          "id": "MDRW2100023929.1.7",
          "form": "음 그냥 안쓰는 물건은 거의 잘 파는 것 같아요!",
          "original_form": "음 그냥 안쓰는 물건은 거의 잘 파는 것 같아요!",
          "speaker_id": "1",
          "time": "20210831 07:11"
        },
        {
          "id": "MDRW2100023929.1.8",
          "form": "엄청 싸게 내놓거든요ㅎㅎ",
          "original_form": "엄청 싸게 내놓거든요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 07:11"
        },
        {
          "id": "MDRW2100023929.1.9",
          "form": "뭐 팔아보셨어요?",
          "original_form": "뭐 팔아보셨어요?",
          "speaker_id": "1",
          "time": "20210831 07:11"
        },
        {
          "id": "MDRW2100023929.1.10",
          "form": "저 카세트 테잎이요",
          "original_form": "저 카세트 테잎이요",
          "speaker_id": "2",
          "time": "20210831 07:15"
        },
        {
          "id": "MDRW2100023929.1.11",
          "form": "오 구매는 자주 하세요?",
          "original_form": "오 구매는 자주 하세요?",
          "speaker_id": "1",
          "time": "20210831 07:17"
        },
        {
          "id": "MDRW2100023929.1.12",
          "form": "저는 잠바하나 사본게 끝이에요ㅎㅎ",
          "original_form": "저는 잠바하나 사본게 끝이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 07:17"
        },
        {
          "id": "MDRW2100023929.1.13",
          "form": "구매 거의 안해요 팔려고 당근 시작했어요 ㅎㅎ",
          "original_form": "구매 거의 안해요 팔려고 당근 시작했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:18"
        },
        {
          "id": "MDRW2100023929.1.14",
          "form": "앟ㅎㅎ 당근으로 이것저것 팔면 돈이 꽤 되는 것 같아요!",
          "original_form": "앟ㅎㅎ 당근으로 이것저것 팔면 돈이 꽤 되는 것 같아요!",
          "speaker_id": "1",
          "time": "20210831 07:19"
        },
        {
          "id": "MDRW2100023929.1.15",
          "form": "근데 현금으로 받을때가 많아서인지",
          "original_form": "근데 현금으로 받을때가 많아서인지",
          "speaker_id": "1",
          "time": "20210831 07:19"
        },
        {
          "id": "MDRW2100023929.1.16",
          "form": "금방 쓰게돼요ㅋㅋ",
          "original_form": "금방 쓰게돼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 07:19"
        },
        {
          "id": "MDRW2100023929.1.17",
          "form": "ㅎㅎ 팔고 받은 돈 더해서 더 사죠",
          "original_form": "ㅎㅎ 팔고 받은 돈 더해서 더 사죠",
          "speaker_id": "2",
          "time": "20210831 07:24"
        },
        {
          "id": "MDRW2100023929.1.18",
          "form": "첫거래때 그랬네요 ㅋㅋ",
          "original_form": "첫거래때 그랬네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 07:24"
        },
        {
          "id": "MDRW2100023929.1.19",
          "form": "5천원 팔고 만원이상 사먹고 집에 들어간 경험이 ㅎㅎ",
          "original_form": "5천원 팔고 만원이상 사먹고 집에 들어간 경험이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:24"
        },
        {
          "id": "MDRW2100023929.1.20",
          "form": "ㅋㅋㅋㅋ뭔가 꽁돈생긴 느낌이라",
          "original_form": "ㅋㅋㅋㅋ뭔가 꽁돈생긴 느낌이라",
          "speaker_id": "1",
          "time": "20210831 07:33"
        },
        {
          "id": "MDRW2100023929.1.21",
          "form": "현금으로 받으면 안돼요ㅋㅋ",
          "original_form": "현금으로 받으면 안돼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 07:33"
        },
        {
          "id": "MDRW2100023929.1.22",
          "form": "ㅋㅋㅋ 계좌거래선호하시는군요",
          "original_form": "ㅋㅋㅋ 계좌거래선호하시는군요",
          "speaker_id": "2",
          "time": "20210831 07:34"
        },
        {
          "id": "MDRW2100023929.1.23",
          "form": "전 팔아도 저렴하게 팔아서 돈이 별루 안돼요",
          "original_form": "전 팔아도 저렴하게 팔아서 돈이 별루 안돼요",
          "speaker_id": "2",
          "time": "20210831 07:35"
        },
        {
          "id": "MDRW2100023929.1.24",
          "form": "ㅋㅋㅋ 사실 당근하는것보다",
          "original_form": "ㅋㅋㅋ 사실 당근하는것보다",
          "speaker_id": "1",
          "time": "20210831 07:36"
        },
        {
          "id": "MDRW2100023929.1.25",
          "form": "지금 하고 있는 이 심심이가",
          "original_form": "지금 하고 있는 이 심심이가",
          "speaker_id": "1",
          "time": "20210831 07:36"
        },
        {
          "id": "MDRW2100023929.1.26",
          "form": "더 돈 많이 벌 것 같아요ㅋㅋㅋ",
          "original_form": "더 돈 많이 벌 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 07:36"
        },
        {
          "id": "MDRW2100023929.1.27",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 08:20"
        },
        {
          "id": "MDRW2100023929.1.28",
          "form": "당근은 물건 처분하려고 시작한 거라서요",
          "original_form": "당근은 물건 처분하려고 시작한 거라서요",
          "speaker_id": "2",
          "time": "20210831 08:20"
        },
        {
          "id": "MDRW2100023929.1.29",
          "form": "그냥 버리기 그런것들 얼마라도 받으면 좋겠다 싶어서 시작했죠",
          "original_form": "그냥 버리기 그런것들 얼마라도 받으면 좋겠다 싶어서 시작했죠",
          "speaker_id": "2",
          "time": "20210831 08:21"
        }
      ]
    }
