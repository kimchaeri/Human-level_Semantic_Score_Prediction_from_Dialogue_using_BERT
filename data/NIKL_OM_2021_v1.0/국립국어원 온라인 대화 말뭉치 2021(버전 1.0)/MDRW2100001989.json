      "utterance": [
        {
          "id": "MDRW2100001989.1.1",
          "form": "동호회있어요?",
          "original_form": "동호회있어요?",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001989.1.2",
          "form": "따로 하는 동호회는 없고 같이 겜하는 모임은 있어요ㅋㅋ",
          "original_form": "따로 하는 동호회는 없고 같이 겜하는 모임은 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001989.1.3",
          "form": "무슨 게임이요?",
          "original_form": "무슨 게임이요?",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001989.1.4",
          "form": "쿠키런킹덤이요!",
          "original_form": "쿠키런킹덤이요!",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001989.1.5",
          "form": "따로 모임 하시는 거 있으세요??",
          "original_form": "따로 모임 하시는 거 있으세요??",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001989.1.6",
          "form": "저는 없아요",
          "original_form": "저는 없아요",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001989.1.7",
          "form": "동호회도요??",
          "original_form": "동호회도요??",
          "speaker_id": "2",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001989.1.8",
          "form": "취미가 없어서 ㅎㅎ",
          "original_form": "취미가 없어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001989.1.9",
          "form": "하고싶은 모임같은 건 없으세요?ㅎ",
          "original_form": "하고싶은 모임같은 건 없으세요?ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001989.1.10",
          "form": "저도 게임 좋을 거 갘아요",
          "original_form": "저도 게임 좋을 거 갘아요",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001989.1.11",
          "form": "그럼 게임 클랜 가입해보시는건 어때요?",
          "original_form": "그럼 게임 클랜 가입해보시는건 어때요?",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001989.1.12",
          "form": "최근게임은 안해서",
          "original_form": "최근게임은 안해서",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001989.1.13",
          "form": "봉사 활동도 안 하세요?",
          "original_form": "봉사 활동도 안 하세요?",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100001989.1.14",
          "form": "요새 코로나 때문에 모임하긴 힘들긴하죠ㅎㅎ",
          "original_form": "요새 코로나 때문에 모임하긴 힘들긴하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100001989.1.15",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
