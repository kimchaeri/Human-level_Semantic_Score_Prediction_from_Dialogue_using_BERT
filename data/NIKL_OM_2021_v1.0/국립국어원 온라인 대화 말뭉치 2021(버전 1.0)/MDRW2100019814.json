      "utterance": [
        {
          "id": "MDRW2100019814.1.1",
          "form": "오늘 날씨 어땠어",
          "original_form": "오늘 날씨 어땠어",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019814.1.2",
          "form": "벌초 직접 하신거야? 더우셨겠는데",
          "original_form": "벌초 직접 하신거야? 더우셨겠는데",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019814.1.3",
          "form": "흐리더라고 강릉은 비도 좀 오고",
          "original_form": "흐리더라고 강릉은 비도 좀 오고",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019814.1.4",
          "form": "그랬구나 비왔나보네",
          "original_form": "그랬구나 비왔나보네",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019814.1.5",
          "form": "인천도 조금 왔어",
          "original_form": "인천도 조금 왔어",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019814.1.6",
          "form": "다행히 덥진 않았는데 비가 부슬부슬 왔어",
          "original_form": "다행히 덥진 않았는데 비가 부슬부슬 왔어",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019814.1.7",
          "form": "날씨 맞추기도 어렵더라고",
          "original_form": "날씨 맞추기도 어렵더라고",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019814.1.8",
          "form": "맞아 그런거 어려워",
          "original_form": "맞아 그런거 어려워",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019814.1.9",
          "form": "도련님이랑 셋이서 했어",
          "original_form": "도련님이랑 셋이서 했어",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019814.1.10",
          "form": "문자와서 일욜에 모여서 벌초한다는거야",
          "original_form": "문자와서 일욜에 모여서 벌초한다는거야",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019814.1.11",
          "form": "아 그래도 그런건 하시나보네",
          "original_form": "아 그래도 그런건 하시나보네",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019814.1.12",
          "form": "종친회에서",
          "original_form": "종친회에서",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019814.1.13",
          "form": "그래서 그날가면 강릉 큰집 얼굴봐야하니까 보기싫어서",
          "original_form": "그래서 그날가면 강릉 큰집 얼굴봐야하니까 보기싫어서",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019814.1.14",
          "form": "날씨가 흐렸지만 그래도 잘하고 왔다",
          "original_form": "날씨가 흐렸지만 그래도 잘하고 왔다",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019814.1.15",
          "form": "미리 갔다온거야 급하게 우린 9월쯤 할줄알았지..",
          "original_form": "미리 갔다온거야 급하게 우린 9월쯤 할줄알았지..",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019814.1.16",
          "form": "그래도 선선해서 할만하더라고",
          "original_form": "그래도 선선해서 할만하더라고",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019814.1.17",
          "form": "되게 빨리하긴 했네",
          "original_form": "되게 빨리하긴 했네",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019814.1.18",
          "form": "저녁늦어지니까 춥던데 바닷가쪽은",
          "original_form": "저녁늦어지니까 춥던데 바닷가쪽은",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019814.1.19",
          "form": "근데 빨리하는게 낫긴해 이번주부터 벌초하러 가는 차때문에 엄청 막힐거야",
          "original_form": "근데 빨리하는게 낫긴해 이번주부터 벌초하러 가는 차때문에 엄청 막힐거야",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019814.1.20",
          "form": "벌초하는거 보니 가을이네",
          "original_form": "벌초하는거 보니 가을이네",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019814.1.21",
          "form": "그러니까 이제 9월부터 많이들 벌초가겠지",
          "original_form": "그러니까 이제 9월부터 많이들 벌초가겠지",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019814.1.22",
          "form": "우리도 9월에가려고했는데 좀 빨리갔네 ㅎㅎ",
          "original_form": "우리도 9월에가려고했는데 좀 빨리갔네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019814.1.23",
          "form": "단풍도 들고 예뻐지겠구만",
          "original_form": "단풍도 들고 예뻐지겠구만",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019814.1.24",
          "form": "강릉 구경 잘하고 왔겄어",
          "original_form": "강릉 구경 잘하고 왔겄어",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019814.1.25",
          "form": "딘풍은 강원도지",
          "original_form": "딘풍은 강원도지",
          "speaker_id": "1",
          "time": "20210827 22:14"
        }
      ]
    }
  ]
