          "speaker_id": "2",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045590.1.6",
          "form": "나 아까 마셔봤거든",
          "original_form": "나 아까 마셔봤거든",
          "speaker_id": "1",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045590.1.7",
          "form": "헐 그것도 라떼야?",
          "original_form": "헐 그것도 라떼야?",
          "speaker_id": "2",
          "time": "20210908 03:55"
