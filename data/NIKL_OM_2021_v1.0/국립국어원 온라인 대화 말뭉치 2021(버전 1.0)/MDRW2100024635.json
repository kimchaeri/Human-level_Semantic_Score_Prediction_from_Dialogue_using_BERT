          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024635.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024635.1.7",
          "form": "2월까지 존버할거면",
          "original_form": "2월까지 존버할거면",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024635.1.8",
          "form": "걍 라이덴을 뽑아....",
          "original_form": "걍 라이덴을 뽑아....",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024635.1.9",
          "form": "시러.........",
          "original_form": "시러.........",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024635.1.10",
          "form": "일단 2.1이 10월에 끝나고",
          "original_form": "일단 2.1이 10월에 끝나고",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024635.1.11",
          "form": "치마",
          "original_form": "치마",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.12",
          "form": "2.2가 11월말쯤 끝나고",
          "original_form": "2.2가 11월말쯤 끝나고",
          "speaker_id": "1",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.14",
          "form": "라이덴의 치마의 멵ㆍㄱ이",
          "original_form": "라이덴의 치마의 멵ㆍㄱ이",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.15",
          "form": "면적이",
          "original_form": "면적이",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.16",
          "form": "2.3은 1월초에 열릴거같은데",
          "original_form": "2.3은 1월초에 열릴거같은데",
          "speaker_id": "1",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.17",
          "form": "한뼘만 넓었어도",
          "original_form": "한뼘만 넓었어도",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.18",
          "form": "돌렷을텐데",
          "original_form": "돌렷을텐데",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.19",
          "form": "솔직히 지금 길이 ㄹㅇ",
          "original_form": "솔직히 지금 길이 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.20",
          "form": "빤스만 겨우 가린수준이잔아",
          "original_form": "빤스만 겨우 가린수준이잔아",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.21",
          "form": "부담스러워...",
          "original_form": "부담스러워...",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024635.1.22",
          "form": "글킨 해 ㅎ",
          "original_form": "글킨 해 ㅎ",
          "speaker_id": "1",
          "time": "20210831 16:24"
        },
        {
          "id": "MDRW2100024635.1.23",
          "form": "솔직히 일러에서도",
          "original_form": "솔직히 일러에서도",
          "speaker_id": "2",
          "time": "20210831 16:24"
        },
        {
          "id": "MDRW2100024635.1.24",
          "form": "무릎 꿇고 앉아있는데",
          "original_form": "무릎 꿇고 앉아있는데",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.25",
          "form": "제발 무릎에 뭐라도",
          "original_form": "제발 무릎에 뭐라도",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.26",
          "form": "덮어주고싶어....",
          "original_form": "덮어주고싶어....",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.27",
          "form": "ㅇㅇ 글킨 해",
          "original_form": "ㅇㅇ 글킨 해",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.28",
          "form": "내가 이제 상의는 포기했거든",
          "original_form": "내가 이제 상의는 포기했거든",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.29",
          "form": "하의라도 좀",
          "original_form": "하의라도 좀",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.30",
          "form": "바지 바라지도 않으니까",
          "original_form": "바지 바라지도 않으니까",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.31",
          "form": "길게라도 입어 얘들아...",
          "original_form": "길게라도 입어 얘들아...",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.32",
          "form": "아니 진짜",
          "original_form": "아니 진짜",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.33",
          "form": "그게 좋아?",
          "original_form": "그게 좋아?",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.34",
          "form": "그게 예뻐?",
          "original_form": "그게 예뻐?",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024635.1.35",
          "form": "이해가 안가",
          "original_form": "이해가 안가",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024635.1.36",
          "form": "솔직히 다 안바라고",
          "original_form": "솔직히 다 안바라고",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024635.1.37",
          "form": "미적감각까지 무시하고",
          "original_form": "미적감각까지 무시하고",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024635.1.38",
          "form": "y존 파는짓만",
          "original_form": "y존 파는짓만",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024635.1.39",
          "form": "안하면 좋겠어.....",
          "original_form": "안하면 좋겠어.....",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024635.1.40",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024635.1.41",
          "form": "걍 예쁜 수준에라도 맞춰봐......",
          "original_form": "걍 예쁜 수준에라도 맞춰봐......",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024635.1.42",
          "form": "차라리",
          "original_form": "차라리",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024635.1.43",
          "form": "ㅇㅇ 예쁘게라도 그리던지",
          "original_form": "ㅇㅇ 예쁘게라도 그리던지",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.44",
          "form": "나도 겜하지만",
          "original_form": "나도 겜하지만",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.45",
          "form": "ㄹㅇ 방구석 오타쿠나 좋아할거같은 디자인이잖아...",
          "original_form": "ㄹㅇ 방구석 오타쿠나 좋아할거같은 디자인이잖아...",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.46",
          "form": "ㄹㅇ.......",
          "original_form": "ㄹㅇ.......",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.47",
          "form": "기모노면서 치파오처럼 다리양쪽 파진건",
          "original_form": "기모노면서 치파오처럼 다리양쪽 파진건",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.48",
          "form": "넘어갈수있어",
          "original_form": "넘어갈수있어",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.49",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.50",
          "form": "근데 그 길이는 좀",
          "original_form": "근데 그 길이는 좀",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.51",
          "form": "앞을 왜자르는데",
          "original_form": "앞을 왜자르는데",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.52",
          "form": "너무한거 아니냐고...",
          "original_form": "너무한거 아니냐고...",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024635.1.53",
          "form": "옆을 팠으면 앞을 내비두라고",
          "original_form": "옆을 팠으면 앞을 내비두라고",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.54",
          "form": "유저 성비도 5대5던데",
          "original_form": "유저 성비도 5대5던데",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.55",
          "form": "뭐가 불만이야",
          "original_form": "뭐가 불만이야",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.56",
          "form": "ㅇㅇ그니까",
          "original_form": "ㅇㅇ그니까",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.57",
          "form": "그리고 신이잖아",
          "original_form": "그리고 신이잖아",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.58",
          "form": "위엄을 갖춘옷을",
          "original_form": "위엄을 갖춘옷을",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.59",
          "form": "입히라고",
          "original_form": "입히라고",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.60",
          "form": "이미 벤티 신 의상에서",
          "original_form": "이미 벤티 신 의상에서",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.61",
          "form": "쥬니히토에 열 세겹 다 입히는게",
          "original_form": "쥬니히토에 열 세겹 다 입히는게",
          "speaker_id": "2",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.62",
          "form": "그른거같긴 하지만...",
          "original_form": "그른거같긴 하지만...",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100024635.1.63",
          "form": "차라리 나았음",
          "original_form": "차라리 나았음",
