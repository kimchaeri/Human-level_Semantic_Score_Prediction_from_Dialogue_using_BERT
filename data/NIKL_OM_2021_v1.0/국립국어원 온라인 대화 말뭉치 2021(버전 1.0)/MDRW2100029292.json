      "utterance": [
        {
          "id": "MDRW2100029292.1.1",
          "form": "지금도 가능합니다",
          "original_form": "지금도 가능합니다",
          "speaker_id": "2",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029292.1.2",
          "form": "ㅎㅎ 이번 생은 글렀어요ㅎㅎ",
          "original_form": "ㅎㅎ 이번 생은 글렀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029292.1.3",
          "form": "충분하십니다",
          "original_form": "충분하십니다",
          "speaker_id": "2",
          "time": "20210903 01:07"
        },
        {
          "id": "MDRW2100029292.1.4",
          "form": "서울만 고집하시지 않다면",
          "original_form": "서울만 고집하시지 않다면",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029292.1.5",
          "form": "다 가지신 분이 뭘 알아욧!!",
          "original_form": "다 가지신 분이 뭘 알아욧!!",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029292.1.6",
          "form": "대전쪽에 정출연 다 모여있어요",
          "original_form": "대전쪽에 정출연 다 모여있어요",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029292.1.7",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029292.1.8",
          "form": "전 affiliation라",
          "original_form": "전 &affiliation&라",
          "speaker_id": "2",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029292.1.9",
          "form": "언론 아닌 쪽 이직해도 홍보팀이나 암튼 다 그런쪽일 거라ㅜ",
          "original_form": "언론 아닌 쪽 이직해도 홍보팀이나 암튼 다 그런쪽일 거라ㅜ",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.10",
          "form": "금융공기업이 좋을것같습니다",
          "original_form": "금융공기업이 좋을것같습니다",
          "speaker_id": "2",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.11",
          "form": "홍보팀 진짜 개꿀",
          "original_form": "홍보팀 진짜 개꿀",
          "speaker_id": "2",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.12",
          "form": "오오 affiliation쪽이군요!",
          "original_form": "오오 &affiliation&쪽이군요!",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.13",
          "form": "우리 홍보팀 유튭 제작하고",
          "original_form": "우리 홍보팀 유튭 제작하고",
          "speaker_id": "2",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.14",
          "form": "인형 제작해서 나눳수고",
          "original_form": "인형 제작해서 나눳수고",
          "speaker_id": "2",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.15",
          "form": "ㅎㅎ 전 아예 언론과 저어언혀 상관없는 곳 갈거예요ㅜ",
          "original_form": "ㅎㅎ 전 아예 언론과 저어언혀 상관없는 곳 갈거예요ㅜ",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.16",
          "form": "다음 생애에",
          "original_form": "다음 생애에",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.17",
          "form": "아닉ㅋㅋ 거긴 뭐하는 데죠ㅎㅎ",
          "original_form": "아닉ㅋㅋ 거긴 뭐하는 데죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:09"
        },
        {
          "id": "MDRW2100029292.1.18",
          "form": "전기를 연구하는곳입니다",
          "original_form": "전기를 연구하는곳입니다",
          "speaker_id": "2",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029292.1.19",
          "form": "오오 역시",
          "original_form": "오오 역시",
          "speaker_id": "1",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029292.1.20",
          "form": "공대 다 죽었으면",
          "original_form": "공대 다 죽었으면",
          "speaker_id": "1",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029292.1.21",
          "form": "ㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029292.1.22",
          "form": "설명은 다 들은걸로 할게요ㅎ",
          "original_form": "설명은 다 들은걸로 할게요ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029292.1.23",
          "form": "집에 전기 잘들어오죠?",
          "original_form": "집에 전기 잘들어오죠?",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029292.1.24",
          "form": "어차피 들어도 모르니 ㅎㅎ",
          "original_form": "어차피 들어도 모르니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029292.1.25",
          "form": "저 촛불 켜고 삽니다",
          "original_form": "저 촛불 켜고 삽니다",
          "speaker_id": "1",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029292.1.26",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029292.1.27",
          "form": "아늑하겠네요",
          "original_form": "아늑하겠네요",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029292.1.28",
          "form": "ㅎㅎ 매일매일이 캠프파이어죠ㅎ",
          "original_form": "ㅎㅎ 매일매일이 캠프파이어죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029292.1.29",
          "form": "일이 적성에 잘 맞으시겠군요!!",
          "original_form": "일이 적성에 잘 맞으시겠군요!!",
          "speaker_id": "1",
