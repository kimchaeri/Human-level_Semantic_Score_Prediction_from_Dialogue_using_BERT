      "utterance": [
        {
          "id": "MDRW2100044804.1.1",
          "form": "그때는",
          "original_form": "그때는",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044804.1.2",
          "form": "그냥 대화만 나눠도 넘재밌죠",
          "original_form": "그냥 대화만 나눠도 넘재밌죠",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044804.1.3",
          "form": "마음맞는 친구들 많이만나서",
          "original_form": "마음맞는 친구들 많이만나서",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044804.1.4",
          "form": "아직까지도 잘지내요 ㅎㅎ",
          "original_form": "아직까지도 잘지내요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044804.1.5",
          "form": "맘맞는 친구 만나서 다행이에요",
          "original_form": "맘맞는 친구 만나서 다행이에요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044804.1.6",
          "form": "저도 한명있어요!",
          "original_form": "저도 한명있어요!",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044804.1.7",
          "form": "자주만나지는 못해요ㅜ",
          "original_form": "자주만나지는 못해요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044804.1.8",
          "form": "타지예요?",
          "original_form": "타지예요?",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044804.1.9",
          "form": "네 엄청 멀어요 기차로 두시간이에요!",
          "original_form": "네 엄청 멀어요 기차로 두시간이에요!",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044804.1.10",
          "form": "그래도 자주통화해요",
          "original_form": "그래도 자주통화해요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044804.1.11",
          "form": "님은 자주만나세요?",
          "original_form": "님은 자주만나세요?",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044804.1.12",
          "form": "고등학교친구가 그룹인가요?",
          "original_form": "고등학교친구가 그룹인가요?",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044804.1.13",
          "form": "아뇨 ㅠ 저도 타지온지 7년정도되서 가끔씩 봐요",
          "original_form": "아뇨 ㅠ 저도 타지온지 7년정도되서 가끔씩 봐요",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044804.1.14",
          "form": "그쳐 거리가 멀어지면 진짜 자주못봐요",
          "original_form": "그쳐 거리가 멀어지면 진짜 자주못봐요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044804.1.15",
          "form": "요즘같은시기에는 특히 더그래요",
          "original_form": "요즘같은시기에는 특히 더그래요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044804.1.16",
          "form": "맞아요 요즘은 뭐 2인밖에 안되니깐요 ㅠㅠ",
          "original_form": "맞아요 요즘은 뭐 2인밖에 안되니깐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044804.1.17",
          "form": "저  고등학교때 친구는 교사라서ㅋㅋ",
          "original_form": "저  고등학교때 친구는 교사라서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044804.1.18",
          "form": "정말 그 지역을 떠나지도 못하더라구요ㅋㅋ",
          "original_form": "정말 그 지역을 떠나지도 못하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044804.1.19",
          "form": "와 교사 멋지네요",
          "original_form": "와 교사 멋지네요",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044804.1.20",
          "form": "그쳐 요즘 힘든시기이긴한데 어린이들이 이쁘다고 하더라고요ㅎㅎ",
          "original_form": "그쳐 요즘 힘든시기이긴한데 어린이들이 이쁘다고 하더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044804.1.21",
          "form": "님은 좋아하세요? 어린아이들",
          "original_form": "님은 좋아하세요? 어린아이들",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044804.1.22",
          "form": "ㅎㅎ너무 귀엽고 제가 선생님이라면 좋아하겠지만",
          "original_form": "ㅎㅎ너무 귀엽고 제가 선생님이라면 좋아하겠지만",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044804.1.23",
          "form": "감당하기 어렵다고 생각해요ㅋㅋ",
          "original_form": "감당하기 어렵다고 생각해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:06"
        }
      ]
