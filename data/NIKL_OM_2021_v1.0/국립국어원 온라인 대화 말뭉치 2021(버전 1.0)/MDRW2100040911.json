      "utterance": [
        {
          "id": "MDRW2100040911.1.1",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100040911.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100040911.1.3",
          "form": "name1님은 드셨나요?",
          "original_form": "&name1&님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210905 12:12"
        },
        {
          "id": "MDRW2100040911.1.4",
          "form": "저도 아직인데",
          "original_form": "저도 아직인데",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100040911.1.5",
          "form": "짜파게티 먹고싶어요!!",
          "original_form": "짜파게티 먹고싶어요!!",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100040911.1.6",
          "form": "불닭에짜파게티 섞어먹는것도 좋아하거든요 ㅠ",
          "original_form": "불닭에짜파게티 섞어먹는것도 좋아하거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100040911.1.7",
          "form": "짜파게티 좋아하세요??!!",
          "original_form": "짜파게티 좋아하세요??!!",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100040911.1.8",
          "form": "네 특히 저도 불닭게티 정말 좋아해요",
          "original_form": "네 특히 저도 불닭게티 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100040911.1.9",
          "form": "너무 맛있겠네요",
          "original_form": "너무 맛있겠네요",
          "speaker_id": "1",
          "time": "20210905 12:24"
        },
        {
          "id": "MDRW2100040911.1.10",
          "form": "어서 집가서 먹고싶어요 ㅠ",
          "original_form": "어서 집가서 먹고싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 12:24"
        },
        {
          "id": "MDRW2100040911.1.11",
          "form": "점심 뭐 드실건가요??",
          "original_form": "점심 뭐 드실건가요??",
          "speaker_id": "1",
          "time": "20210905 12:24"
        },
        {
          "id": "MDRW2100040911.1.12",
          "form": "아직은 모르겠어요 저도 불닭게티 먹고싶네요ㅠㅠ",
          "original_form": "아직은 모르겠어요 저도 불닭게티 먹고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 12:26"
        },
        {
          "id": "MDRW2100040911.1.13",
          "form": "말하니까 더 먹고 싶은 것 같아요",
          "original_form": "말하니까 더 먹고 싶은 것 같아요",
          "speaker_id": "1",
          "time": "20210905 12:32"
        },
        {
          "id": "MDRW2100040911.1.14",
          "form": "ㅋㅋㅋ삼겹살도 먹고싶네요",
          "original_form": "ㅋㅋㅋ삼겹살도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210905 12:32"
        },
        {
          "id": "MDRW2100040911.1.15",
          "form": "새우튀김 좋아하시나요?",
          "original_form": "새우튀김 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 12:33"
        },
        {
          "id": "MDRW2100040911.1.16",
          "form": "네 마라님도 새우튀김 좋아하시요",
          "original_form": "네 마라님도 새우튀김 좋아하시요",
          "speaker_id": "2",
          "time": "20210905 12:34"
        },
        {
          "id": "MDRW2100040911.1.17",
          "form": "나요?",
          "original_form": "나요?",
          "speaker_id": "2",
          "time": "20210905 12:34"
        },
        {
          "id": "MDRW2100040911.1.18",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100040911.1.19",
          "form": "ㄱㅇ래서 방금 쇼핑하고 왔어요 ㅠㅠ",
          "original_form": "ㄱㅇ래서 방금 쇼핑하고 왔어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100040911.1.20",
          "form": "에어프라이어에 돌려먹으려구요",
          "original_form": "에어프라이어에 돌려먹으려구요",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100040911.1.21",
          "form": "새우튀김을 사신 건가요?",
          "original_form": "새우튀김을 사신 건가요?",
          "speaker_id": "2",
          "time": "20210905 19:14"
        },
        {
          "id": "MDRW2100040911.1.22",
          "form": "네 마트에서 사왔어요",
          "original_form": "네 마트에서 사왔어요",
          "speaker_id": "1",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100040911.1.23",
          "form": "너무 기대중이에요 떡볶이랑 같이먹으려구요 ㅎㅎ",
          "original_form": "너무 기대중이에요 떡볶이랑 같이먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100040911.1.24",
          "form": "떡볶이는 직접 만들어 먹으실 예정이신가요?",
          "original_form": "떡볶이는 직접 만들어 먹으실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210905 19:26"
        },
        {
          "id": "MDRW2100040911.1.25",
          "form": "아니요 만들어져있는 인스턴트로 샀어요!",
          "original_form": "아니요 만들어져있는 인스턴트로 샀어요!",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100040911.1.26",
          "form": "너무 간편하고 좋아요 ㅎㅎ",
          "original_form": "너무 간편하고 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100040911.1.27",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
