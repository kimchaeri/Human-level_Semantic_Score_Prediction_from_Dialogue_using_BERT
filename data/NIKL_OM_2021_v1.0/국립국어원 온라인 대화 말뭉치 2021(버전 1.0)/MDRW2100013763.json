      "utterance": [
        {
          "id": "MDRW2100013763.1.1",
          "form": "안냐세여",
          "original_form": "안냐세여",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013763.1.2",
          "form": "여행 좋아핫0여",
          "original_form": "여행 좋아핫0여",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013763.1.3",
          "form": "저는 여행 별루여",
          "original_form": "저는 여행 별루여",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013763.1.4",
          "form": "가도 호캉스.같은게 좋지",
          "original_form": "가도 호캉스.같은게 좋지",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013763.1.5",
          "form": "놀러다니는건 별루",
          "original_form": "놀러다니는건 별루",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013763.1.6",
          "form": "가고싶다",
          "original_form": "가고싶다",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.7",
          "form": "여행",
          "original_form": "여행",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.8",
          "form": "가본게",
          "original_form": "가본게",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.9",
          "form": "국내여행 좋아핫여",
          "original_form": "국내여행 좋아핫여",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.10",
          "form": "언젠지",
          "original_form": "언젠지",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.11",
          "form": "국내 외",
          "original_form": "국내 외",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.12",
          "form": "모두다여",
          "original_form": "모두다여",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.13",
          "form": "저는 뱅기가 무서워서",
          "original_form": "저는 뱅기가 무서워서",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.14",
          "form": "해외여행은 별루",
          "original_form": "해외여행은 별루",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.15",
          "form": "국내여행이 좋아여",
          "original_form": "국내여행이 좋아여",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.16",
          "form": "저는 부산 가고 싶어여",
          "original_form": "저는 부산 가고 싶어여",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.17",
          "form": "바다가 보고 싶네요",
          "original_form": "바다가 보고 싶네요",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013763.1.18",
          "form": "울딸이 재주 보내준다고 하고 튀렀어여",
          "original_form": "울딸이 재주 보내준다고 하고 튀렀어여",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013763.1.19",
          "form": "바다좋죠",
          "original_form": "바다좋죠",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013763.1.20",
          "form": "밤자다",
          "original_form": "밤자다",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013763.1.21",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013763.1.22",
          "form": "바다 좋아여",
          "original_form": "바다 좋아여",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013763.1.23",
          "form": "바다 놀러가고 싶어요",
          "original_form": "바다 놀러가고 싶어요",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013763.1.24",
          "form": "제주도도",
          "original_form": "제주도도",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013763.1.25",
          "form": "좋아여",
          "original_form": "좋아여",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013763.1.26",
          "form": "수학여행때 가봣엉",
          "original_form": "수학여행때 가봣엉",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013763.1.27",
          "form": "왜 반말?",
          "original_form": "왜 반말?",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013763.1.28",
          "form": "기분 나쁘게",
          "original_form": "기분 나쁘게",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013763.1.29",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013763.1.30",
          "form": "에여",
          "original_form": "에여",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013763.1.31",
          "form": "산 좋아하세여",
          "original_form": "산 좋아하세여",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013763.1.32",
          "form": "암벽타고시포여",
          "original_form": "암벽타고시포여",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013763.1.33",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013763.1.34",
          "form": "와일드한게 돟아여",
          "original_form": "와일드한게 돟아여",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013763.1.35",
          "form": "이번에 올림픽때",
          "original_form": "이번에 올림픽때",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013763.1.36",
          "form": "오 그럼 그런곳으로",
          "original_form": "오 그럼 그런곳으로",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013763.1.37",
          "form": "관악산 추천합니다",
          "original_form": "관악산 추천합니다",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013763.1.38",
          "form": "클라이밍보고",
          "original_form": "클라이밍보고",
          "speaker_id": "2",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013763.1.39",
          "form": "관악산 놀러가보세여",
          "original_form": "관악산 놀러가보세여",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013763.1.40",
          "form": "추턴합니다",
          "original_form": "추턴합니다",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013763.1.41",
          "form": "더하고 싶어 졌러여",
          "original_form": "더하고 싶어 졌러여",
          "speaker_id": "2",
          "time": "20210825 15:57"
        }
      ]
