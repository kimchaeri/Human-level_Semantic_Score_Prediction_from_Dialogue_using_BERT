      "utterance": [
        {
          "id": "MDRW2100014054.1.1",
          "form": "맛있는 음식 뭐 좋아하세요?",
          "original_form": "맛있는 음식 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014054.1.2",
          "form": "고기류요",
          "original_form": "고기류요",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014054.1.3",
          "form": "저도 고기를 좋아해요",
          "original_form": "저도 고기를 좋아해요",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014054.1.4",
          "form": "어떤고기요",
          "original_form": "어떤고기요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014054.1.5",
          "form": "저는 소 닭 돼지 전부 ㅎ",
          "original_form": "저는 소 닭 돼지 전부 ㅎ",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014054.1.6",
          "form": "아 저도요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그냥 고기면 다 좋아하는거 같아요",
          "original_form": "아 저도요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그냥 고기면 다 좋아하는거 같아요",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014054.1.7",
          "form": "오리고기도 잘먹어요",
          "original_form": "오리고기도 잘먹어요",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014054.1.8",
          "form": "맞아요. 오리도 맛있지요.",
          "original_form": "맞아요. 오리도 맛있지요.",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014054.1.9",
          "form": "사실 가리는거  없이 다 잘먹긴해요.",
          "original_form": "사실 가리는거  없이 다 잘먹긴해요.",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014054.1.10",
          "form": "요리는 잘하셔요? 요즘 밀키트가 많이나와서 요리하기 수월해진듯해요.",
          "original_form": "요리는 잘하셔요? 요즘 밀키트가 많이나와서 요리하기 수월해진듯해요.",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014054.1.11",
          "form": "요리는 취미에 없긴 해요 ㅋㅋㅋㅋ 하면 맛은 있어요",
          "original_form": "요리는 취미에 없긴 해요 ㅋㅋㅋㅋ 하면 맛은 있어요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014054.1.12",
          "form": "저도 요리 시도는 여러번 해봤는데 맛이없어서 그냥 포기각이요.",
          "original_form": "저도 요리 시도는 여러번 해봤는데 맛이없어서 그냥 포기각이요.",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014054.1.13",
          "form": "재능이 없는듯요.",
          "original_form": "재능이 없는듯요.",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014054.1.14",
          "form": "무슨요리를 해도 다 실패하네요",
          "original_form": "무슨요리를 해도 다 실패하네요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014054.1.15",
          "form": "ㅠㅠ 저는 그냥 레시피 보고 해요 왜냐면 그게 그나마 성공확률이 높더라고요",
          "original_form": "ㅠㅠ 저는 그냥 레시피 보고 해요 왜냐면 그게 그나마 성공확률이 높더라고요",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014054.1.16",
          "form": "뭔가 손맛으로 하기에는 부족ㅋㅋ",
          "original_form": "뭔가 손맛으로 하기에는 부족ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014054.1.17",
          "form": "레시피봐도 저같은 사람은 늘 실패하더라구요",
          "original_form": "레시피봐도 저같은 사람은 늘 실패하더라구요",
          "speaker_id": "2",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100014054.1.18",
          "form": "요리도 감각이 있어야 하는듯요",
          "original_form": "요리도 감각이 있어야 하는듯요",
          "speaker_id": "2",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100014054.1.19",
          "form": "그러면 평소에 배달 음식 어떤거 드세요?",
          "original_form": "그러면 평소에 배달 음식 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210826 08:11"
        },
        {
          "id": "MDRW2100014054.1.20",
          "form": "저는 다양한 메뉴로 주문해요 ㅎㅎ",
          "original_form": "저는 다양한 메뉴로 주문해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 08:11"
        },
        {
          "id": "MDRW2100014054.1.21",
          "form": "무조건 중국집이죠 ㅎ",
          "original_form": "무조건 중국집이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210826 08:25"
        },
        {
          "id": "MDRW2100014054.1.22",
          "form": "짬뽕파? 짜장면파? 어떤거 더 좋아하세요? 아침부터 배고프네요 ㅋㅋ",
          "original_form": "짬뽕파? 짜장면파? 어떤거 더 좋아하세요? 아침부터 배고프네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:33"
        },
        {
          "id": "MDRW2100014054.1.23",
          "form": "둘다좋아서 늘 선택장애예요.",
          "original_form": "둘다좋아서 늘 선택장애예요.",
          "speaker_id": "2",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100014054.1.24",
          "form": "그래도 하나 꼽자면 짜장",
          "original_form": "그래도 하나 꼽자면 짜장",
          "speaker_id": "2",
          "time": "20210826 09:15"
        }
      ]
    }
