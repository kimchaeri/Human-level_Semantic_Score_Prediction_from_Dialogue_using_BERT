      "utterance": [
        {
          "id": "MDRW2100044638.1.1",
          "form": "눈이 삐꾸인데 일은 하는 내 몸뚱아리",
          "original_form": "눈이 삐꾸인데 일은 하는 내 몸뚱아리",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044638.1.2",
          "form": "뇌야 일을해 척수반사를 하지 말고",
          "original_form": "뇌야 일을해 척수반사를 하지 말고",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044638.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 그치만",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그치만",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044638.1.4",
          "form": "직장인들은 원래 그렇게 일해",
          "original_form": "직장인들은 원래 그렇게 일해",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044638.1.5",
          "form": "일은 머리가 하는게 아니라 손이 하는거지 ㅎ",
          "original_form": "일은 머리가 하는게 아니라 손이 하는거지 ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044638.1.6",
          "form": "ㅋㅋㅋㅋ 그건 그래..",
          "original_form": "ㅋㅋㅋㅋ 그건 그래..",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044638.1.7",
          "form": "생각은 하지 않는다..",
          "original_form": "생각은 하지 않는다..",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044638.1.8",
          "form": "메뉴얼대로만 일한다..",
          "original_form": "메뉴얼대로만 일한다..",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044638.1.9",
          "form": "기계랑 나랑 다른점은 오직 하나인데",
          "original_form": "기계랑 나랑 다른점은 오직 하나인데",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044638.1.10",
          "form": "기계는 정해진 구간만 다니고 나는 뛰어댕기고",
          "original_form": "기계는 정해진 구간만 다니고 나는 뛰어댕기고",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044638.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그치 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그치 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044638.1.12",
          "form": "사람은 변칙적으로 일하지...ㅎ.",
          "original_form": "사람은 변칙적으로 일하지...ㅎ.",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044638.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 효율은 내가 높나..? ㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 효율은 내가 높나..? ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.14",
          "form": "다행..인가..?",
          "original_form": "다행..인가..?",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 그럴지도...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그럴지도...",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.16",
          "form": "기계한테 밀려서직장 잃진 않겠네...",
          "original_form": "기계한테 밀려서직장 잃진 않겠네...",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.17",
          "form": "아직까진..",
          "original_form": "아직까진..",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.18",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.19",
          "form": "아마 우리 은퇴할 때까지는",
          "original_form": "아마 우리 은퇴할 때까지는",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.20",
          "form": "기계보다 사람이 저렴할듯",
          "original_form": "기계보다 사람이 저렴할듯",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.21",
          "form": "넘 슬프다",
          "original_form": "넘 슬프다",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044638.1.22",
          "form": "인건비가 싸서 사용되는",
          "original_form": "인건비가 싸서 사용되는",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.23",
          "form": "노동의가치",
          "original_form": "노동의가치",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.24",
          "form": "어디가 대체 숭고하죠 노동따위",
          "original_form": "어디가 대체 숭고하죠 노동따위",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.25",
          "form": "ㅋㅋㅋㅋ 원래 그런거지...",
          "original_form": "ㅋㅋㅋㅋ 원래 그런거지...",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.26",
          "form": "예전엔 알람시계도 비싸서",
          "original_form": "예전엔 알람시계도 비싸서",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.27",
          "form": "노동으로 돈벌라는 시대는 지났다..",
          "original_form": "노동으로 돈벌라는 시대는 지났다..",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.28",
          "form": "깨워주는 직업이 잇엇다며",
          "original_form": "깨워주는 직업이 잇엇다며",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.29",
          "form": "아 진짜? 하긴",
          "original_form": "아 진짜? 하긴",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.30",
          "form": "그랬겠다",
          "original_form": "그랬겠다",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.31",
          "form": "인간이 일단 싸고 많고",
          "original_form": "인간이 일단 싸고 많고",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044638.1.32",
          "form": "ㅇㅇ 엄청 옛날에",
          "original_form": "ㅇㅇ 엄청 옛날에",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044638.1.33",
          "form": "말을 입력하기 편하지",
          "original_form": "말을 입력하기 편하지",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044638.1.34",
          "form": "산업혁명시기쯤?",
          "original_form": "산업혁명시기쯤?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044638.1.35",
          "form": "ㅇㅇ 그랬을거 같은",
          "original_form": "ㅇㅇ 그랬을거 같은",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044638.1.36",
          "form": "근데 산업혁명도 한 200년전이니까",
          "original_form": "근데 산업혁명도 한 200년전이니까",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044638.1.37",
          "form": "어엄청 옛날도 아닌데",
          "original_form": "어엄청 옛날도 아닌데",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044638.1.38",
          "form": "(총 역사에서 보면?)",
          "original_form": "(총 역사에서 보면?)",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044638.1.39",
          "form": "ㅋㅋㅋㅋ 그정도면 옛날이라구...",
          "original_form": "ㅋㅋㅋㅋ 그정도면 옛날이라구...",
          "speaker_id": "1",
          "time": "20210907 23:23"
        }
      ]
    }
  ]
