      "utterance": [
        {
          "id": "MDRW2100013641.1.1",
          "form": "저는 이마트에서 반려용품사는게 로망이에요",
          "original_form": "저는 이마트에서 반려용품사는게 로망이에요",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013641.1.2",
          "form": "요즘 동물들의 장난감을 쉽게 구매할수있더라고요.",
          "original_form": "요즘 동물들의 장난감을 쉽게 구매할수있더라고요.",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013641.1.3",
          "form": "반려동물용품이요",
          "original_form": "반려동물용품이요",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013641.1.4",
          "form": "티비에서 종종보던 그 장난감을 실제로보니 가슴이 막 두근거렸어요",
          "original_form": "티비에서 종종보던 그 장난감을 실제로보니 가슴이 막 두근거렸어요",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013641.1.5",
          "form": "가격도 별로 안비싸서 반려동물키우는 사람들이 장난감을 자꾸 구매하는 이유를 알것같아요. 정말로 행복할것같아요",
          "original_form": "가격도 별로 안비싸서 반려동물키우는 사람들이 장난감을 자꾸 구매하는 이유를 알것같아요. 정말로 행복할것같아요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013641.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013641.1.7",
          "form": "반려용품을 사면 뭐해요",
          "original_form": "반려용품을 사면 뭐해요",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013641.1.8",
          "form": "투명 반려동물이 있으신지?",
          "original_form": "투명 반려동물이 있으신지?",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013641.1.9",
          "form": "세상이 많이 좋아졌어요",
          "original_form": "세상이 많이 좋아졌어요",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013641.1.10",
          "form": "로망 참 독특하네요",
          "original_form": "로망 참 독특하네요",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013641.1.11",
          "form": "그냥 꿈만 꾸고있어요",
          "original_form": "그냥 꿈만 꾸고있어요",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013641.1.12",
          "form": "독립하면 무조건 키우려고요",
          "original_form": "독립하면 무조건 키우려고요",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013641.1.13",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013641.1.14",
          "form": "손덜가는 동물로 ㅎ",
          "original_form": "손덜가는 동물로 ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013641.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013641.1.16",
          "form": "저는 햄스터",
          "original_form": "저는 햄스터",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013641.1.17",
          "form": "물고기?",
          "original_form": "물고기?",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013641.1.18",
          "form": "귀엽자나요",
          "original_form": "귀엽자나요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013641.1.19",
          "form": "그럼 어항은 누가씻어요?",
          "original_form": "그럼 어항은 누가씻어요?",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013641.1.20",
          "form": "언니가요",
          "original_form": "언니가요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013641.1.21",
          "form": "어떻게 꾸미고싶나요",
          "original_form": "어떻게 꾸미고싶나요",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013641.1.22",
          "form": "언니분 참 고생이많으시겟네요",
          "original_form": "언니분 참 고생이많으시겟네요",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013641.1.23",
          "form": "귀엽고 예쁘게",
          "original_form": "귀엽고 예쁘게",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013641.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013641.1.25",
          "form": "하고싶어할걸요?",
          "original_form": "하고싶어할걸요?",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013641.1.26",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013641.1.27",
          "form": "빨대같은 물고기 키우고싶어요",
          "original_form": "빨대같은 물고기 키우고싶어요",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013641.1.28",
          "form": "해마랑",
          "original_form": "해마랑",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013641.1.29",
          "form": "그 외엔 앵무새나 고양이 등등이요",
          "original_form": "그 외엔 앵무새나 고양이 등등이요",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013641.1.30",
          "form": "다양한 박스를 구비해두고 캣타워도 마련해둬서",
          "original_form": "다양한 박스를 구비해두고 캣타워도 마련해둬서",
          "speaker_id": "2",
          "time": "20210825 15:38"
        }
      ]
    }
