      "utterance": [
        {
          "id": "MDRW2100038888.1.1",
          "form": "운동 하고 계신거 있나요?",
          "original_form": "운동 하고 계신거 있나요?",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038888.1.2",
          "form": "저는 홈트하고 있어요:)",
          "original_form": "저는 홈트하고 있어요:)",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038888.1.3",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038888.1.4",
          "form": "저도 홈트해요",
          "original_form": "저도 홈트해요",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038888.1.5",
          "form": "유튜브 보고 하시나요?",
          "original_form": "유튜브 보고 하시나요?",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038888.1.6",
          "form": "네:) 땅끄부부 봅니다ㅋ",
          "original_form": "네:) 땅끄부부 봅니다ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038888.1.7",
          "form": "유툽 보세요?",
          "original_form": "유툽 보세요?",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038888.1.8",
          "form": "네 저도 유튜브로 보고 해요",
          "original_form": "네 저도 유튜브로 보고 해요",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038888.1.9",
          "form": "빅씨스 봅니다",
          "original_form": "빅씨스 봅니다",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038888.1.10",
          "form": "빅씨스도 함 봐야겠군용",
          "original_form": "빅씨스도 함 봐야겠군용",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038888.1.11",
          "form": "홈트 꾸준히 하세요?",
          "original_form": "홈트 꾸준히 하세요?",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038888.1.12",
          "form": "시작한지 두달 됐어요",
          "original_form": "시작한지 두달 됐어요",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100038888.1.13",
          "form": "아픈날 빼고는 계속 했어요",
          "original_form": "아픈날 빼고는 계속 했어요",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100038888.1.14",
          "form": "와 지속력이 대단하시네요",
          "original_form": "와 지속력이 대단하시네요",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100038888.1.15",
          "form": "저는 혼자 홈트하다보니\r\n지속이 안되더라구여ㅠ",
          "original_form": "저는 혼자 홈트하다보니\r\n지속이 안되더라구여ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100038888.1.16",
          "form": "저도 몇번 고비 있었어요",
          "original_form": "저도 몇번 고비 있었어요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038888.1.17",
          "form": "그거 넘으니 일상이 되더라구요",
          "original_form": "그거 넘으니 일상이 되더라구요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038888.1.18",
          "form": "고비를 어떻게 넘기셨어여?",
          "original_form": "고비를 어떻게 넘기셨어여?",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038888.1.19",
          "form": "홈트 영상이랑 다른 좋아하는 영상 틀어놓고 했어요",
          "original_form": "홈트 영상이랑 다른 좋아하는 영상 틀어놓고 했어요",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100038888.1.20",
          "form": "방탄요? ㅋㅋㅋ",
          "original_form": "방탄요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100038888.1.21",
          "form": "네ㅋㅋㅋㅋㄱㅋ",
          "original_form": "네ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100038888.1.22",
          "form": "방탄은 고비도\r\n이기게 하는군여 ㅋㅋㅋ",
          "original_form": "방탄은 고비도\r\n이기게 하는군여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100038888.1.23",
          "form": "힘들때는 방탄이 최고죠... ㅎ",
          "original_form": "힘들때는 방탄이 최고죠... ㅎ",
