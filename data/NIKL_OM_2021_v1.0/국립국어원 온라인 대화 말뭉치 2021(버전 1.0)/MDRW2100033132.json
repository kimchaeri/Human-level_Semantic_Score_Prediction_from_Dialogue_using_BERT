        },
        {
          "id": "MDRW2100033132.1.2",
          "form": "넹 ㅋㅋㅋ 형제있으신가요?",
          "original_form": "넹 ㅋㅋㅋ 형제있으신가요?",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033132.1.3",
          "form": "누나가 있죠",
          "original_form": "누나가 있죠",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.4",
          "form": "오 누나 .. 누나랑 안 싸우세요?",
          "original_form": "오 누나 .. 누나랑 안 싸우세요?",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.5",
          "form": "누나가 너무 짜증나요",
          "original_form": "누나가 너무 짜증나요",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.6",
          "form": "그렇구나ㅠㅠ",
          "original_form": "그렇구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.7",
          "form": "다들 그런 말 해요",
          "original_form": "다들 그런 말 해요",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.8",
          "form": "근데 저도 많이 짜증나게 하는거같아요",
          "original_form": "근데 저도 많이 짜증나게 하는거같아요",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.9",
          "form": "아 누나한테요?",
          "original_form": "아 누나한테요?",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.11",
          "form": "잘하셔야겠네요",
          "original_form": "잘하셔야겠네요",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033132.1.12",
          "form": "좀더 누나한테 잘 대해줘야겠어요",
          "original_form": "좀더 누나한테 잘 대해줘야겠어요",
          "speaker_id": "2",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100033132.1.13",
          "form": "그래도 가족인데..",
          "original_form": "그래도 가족인데..",
          "speaker_id": "2",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100033132.1.14",
          "form": "오.. 새람님 멋있네요",
          "original_form": "오.. 새람님 멋있네요",
          "speaker_id": "1",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100033132.1.15",
          "form": "가족이 소중하죠 ㅎㅎ",
