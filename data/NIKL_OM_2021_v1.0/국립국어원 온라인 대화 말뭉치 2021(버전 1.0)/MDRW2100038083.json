      "utterance": [
        {
          "id": "MDRW2100038083.1.1",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.2",
          "form": "빨리 가",
          "original_form": "빨리 가",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.3",
          "form": "쇼핑앱 에이블리에서 할인쿠폰을 줬어",
          "original_form": "쇼핑앱 에이블리에서 할인쿠폰을 줬어",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.4",
          "form": "어디를??가는지 알려줘야지",
          "original_form": "어디를??가는지 알려줘야지",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.5",
          "form": "에이블리 깔려있는거야?",
          "original_form": "에이블리 깔려있는거야?",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.6",
          "form": "3000원을 현금처럼 사용할 수 있어",
          "original_form": "3000원을 현금처럼 사용할 수 있어",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.7",
          "form": "아니 그건 무슨어플이야??",
          "original_form": "아니 그건 무슨어플이야??",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.8",
          "form": "에이블리는 배송비도 무료니까",
          "original_form": "에이블리는 배송비도 무료니까",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.9",
          "form": "옷도 팔고 잡화도 팔아",
          "original_form": "옷도 팔고 잡화도 팔아",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.10",
          "form": "먹을거 빼고 다 파는 고 같아!",
          "original_form": "먹을거 빼고 다 파는 고 같아!",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.11",
          "form": "흠 요즘 살게 엄청많은데 좋은정보 고마워",
          "original_form": "흠 요즘 살게 엄청많은데 좋은정보 고마워",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.12",
          "form": "마침 너 이사를 했으니",
          "original_form": "마침 너 이사를 했으니",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.13",
          "form": "너는 그 쿠폰으로 뭘샀니?",
          "original_form": "너는 그 쿠폰으로 뭘샀니?",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.14",
          "form": "필요한 걸 장만해봐",
          "original_form": "필요한 걸 장만해봐",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.15",
          "form": "나는 잠옷을 살까 해.",
          "original_form": "나는 잠옷을 살까 해.",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.16",
          "form": "사실 스티커도 고민중이야",
          "original_form": "사실 스티커도 고민중이야",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038083.1.17",
          "form": "오 잠옷은 얼마정도해?",
          "original_form": "오 잠옷은 얼마정도해?",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038083.1.18",
          "form": "요즘 스티커값이 금값이야",
          "original_form": "요즘 스티커값이 금값이야",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038083.1.19",
          "form": "9900원 이하를 찾고있어",
          "original_form": "9900원 이하를 찾고있어",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038083.1.20",
          "form": "스티커는 정말 싸게 살 수 있겠다",
          "original_form": "스티커는 정말 싸게 살 수 있겠다",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038083.1.21",
          "form": "비싸 스티커.",
          "original_form": "비싸 스티커.",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038083.1.22",
          "form": "잠온만해",
          "original_form": "잠온만해",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038083.1.23",
          "form": "좋은 상품 잘 찾아봐!!",
          "original_form": "좋은 상품 잘 찾아봐!!",
          "speaker_id": "1",
          "time": "20210901 23:02"
        }
