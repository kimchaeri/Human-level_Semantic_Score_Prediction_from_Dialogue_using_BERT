      "utterance": [
        {
          "id": "MDRW2100023638.1.1",
          "form": "응 먹고싶어!ㅋㅋㅋㅋ",
          "original_form": "응 먹고싶어!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100023638.1.2",
          "form": "아구찜은 안좋아해...........ㅋㅋㅋ",
          "original_form": "아구찜은 안좋아해...........ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100023638.1.3",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100023638.1.4",
          "form": "내가 맛있는걸 안먹어봐서 그런가?",
          "original_form": "내가 맛있는걸 안먹어봐서 그런가?",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023638.1.5",
          "form": "아구찜맛은 다 비슷할듯?ㅋㅋ",
          "original_form": "아구찜맛은 다 비슷할듯?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023638.1.6",
          "form": "뽕족은 안먹고싶어?",
          "original_form": "뽕족은 안먹고싶어?",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023638.1.7",
          "form": "뽕족도 먹고싶지 당연히!!",
          "original_form": "뽕족도 먹고싶지 당연히!!",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023638.1.8",
          "form": "거기도 가까워?",
          "original_form": "거기도 가까워?",
          "speaker_id": "2",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023638.1.9",
          "form": "아니 뽕족은 역삼이지ㅋㅋ",
          "original_form": "아니 뽕족은 역삼이지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023638.1.10",
          "form": "진짜 거기가 내가 먹어본 것 중에 제일 맛있었어",
          "original_form": "진짜 거기가 내가 먹어본 것 중에 제일 맛있었어",
          "speaker_id": "2",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023638.1.11",
          "form": "나도 진짜 뽕족이 제일 맛있어",
          "original_form": "나도 진짜 뽕족이 제일 맛있어",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023638.1.12",
          "form": "그래서 우리회사쪽은 크게 기대는 안되는데 먹어보고싶긴해ㅋㅋ",
          "original_form": "그래서 우리회사쪽은 크게 기대는 안되는데 먹어보고싶긴해ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100023638.1.13",
          "form": "그럼 역삼친구한테 부탁해야하나..ㅋㅋㅋㅋ",
          "original_form": "그럼 역삼친구한테 부탁해야하나..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023638.1.14",
          "form": "근데 그거 사들고 지하철 타기 그래가지구",
          "original_form": "근데 그거 사들고 지하철 타기 그래가지구",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023638.1.15",
          "form": "나도 지하철타는데여?ㅇㅅㅇㅋㅋㅋ",
          "original_form": "나도 지하철타는데여?ㅇㅅㅇㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023638.1.16",
          "form": "너한테도 부탁하기 그렇다궄ㅋㅋ",
          "original_form": "너한테도 부탁하기 그렇다궄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100023638.1.17",
          "form": "나도 그래서 접때 말하려다가 사람 너무 많아서 말못했는데",
          "original_form": "나도 그래서 접때 말하려다가 사람 너무 많아서 말못했는데",
          "speaker_id": "1",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100023638.1.18",
          "form": "손님 많대?",
          "original_form": "손님 많대?",
          "speaker_id": "2",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100023638.1.19",
          "form": "아니 쟈철에 사람많아서 낑기니까 포장하기 불편할까봐",
          "original_form": "아니 쟈철에 사람많아서 낑기니까 포장하기 불편할까봐",
