      "utterance": [
        {
          "id": "MDRW2100025251.1.1",
          "form": "요새 넷플릭스",
          "original_form": "요새 넷플릭스",
          "speaker_id": "2",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100025251.1.2",
          "form": "디피도 핫하던데요",
          "original_form": "디피도 핫하던데요",
          "speaker_id": "2",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100025251.1.3",
          "form": "그런가요~",
          "original_form": "그런가요~",
          "speaker_id": "1",
          "time": "20210901 03:01"
        },
        {
          "id": "MDRW2100025251.1.4",
          "form": "군대이야기죠?",
          "original_form": "군대이야기죠?",
          "speaker_id": "1",
          "time": "20210901 03:01"
        },
        {
          "id": "MDRW2100025251.1.5",
          "form": "원래 드라마를 잘 안봐서 ㅎㅎ",
          "original_form": "원래 드라마를 잘 안봐서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 03:01"
        },
        {
          "id": "MDRW2100025251.1.6",
          "form": "그거 보세요?",
          "original_form": "그거 보세요?",
          "speaker_id": "1",
          "time": "20210901 03:01"
        },
        {
          "id": "MDRW2100025251.1.7",
          "form": "유튜브에서 막 이야기해줘서",
          "original_form": "유튜브에서 막 이야기해줘서",
          "speaker_id": "2",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025251.1.8",
          "form": "볼까하고 있었어요ㅋㅋ",
          "original_form": "볼까하고 있었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025251.1.9",
          "form": "저도 제목은 들어봤어요! 시작한지는 얼마안된거 같던데요~~",
          "original_form": "저도 제목은 들어봤어요! 시작한지는 얼마안된거 같던데요~~",
          "speaker_id": "1",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025251.1.10",
          "form": "재밌게본 드라마는 뭐가 있으세요?",
          "original_form": "재밌게본 드라마는 뭐가 있으세요?",
          "speaker_id": "1",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025251.1.11",
          "form": "킹덤이요 ㅎㅎ",
          "original_form": "킹덤이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 03:05"
        },
        {
          "id": "MDRW2100025251.1.12",
          "form": "좀 된거로는 비밀의숲이요ㅋㅋ",
          "original_form": "좀 된거로는 비밀의숲이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:05"
        },
        {
          "id": "MDRW2100025251.1.13",
          "form": "아~ 저는 비슷한 결이라고 많이하던데 저는 시그널 재밌게 봤어요!",
          "original_form": "아~ 저는 비슷한 결이라고 많이하던데 저는 시그널 재밌게 봤어요!",
          "speaker_id": "1",
          "time": "20210901 03:06"
        },
        {
          "id": "MDRW2100025251.1.14",
          "form": "그 무전 그거 맞나요",
          "original_form": "그 무전 그거 맞나요",
          "speaker_id": "2",
          "time": "20210901 03:09"
        },
        {
          "id": "MDRW2100025251.1.15",
          "form": "제 치지직",
          "original_form": "제 치지직",
          "speaker_id": "1",
          "time": "20210901 03:09"
        },
        {
          "id": "MDRW2100025251.1.16",
          "form": "경위님!",
          "original_form": "경위님!",
          "speaker_id": "1",
          "time": "20210901 03:09"
        },
        {
          "id": "MDRW2100025251.1.17",
          "form": "이재한형사님!",
          "original_form": "이재한형사님!",
          "speaker_id": "1",
          "time": "20210901 03:09"
        },
        {
          "id": "MDRW2100025251.1.18",
          "form": "ㅋㅋ그거도 안봤는데",
          "original_form": "ㅋㅋ그거도 안봤는데",
          "speaker_id": "2",
          "time": "20210901 03:09"
        },
        {
          "id": "MDRW2100025251.1.19",
          "form": "코로나 터져서 집콕하는 시기에",
          "original_form": "코로나 터져서 집콕하는 시기에",
          "speaker_id": "2",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025251.1.20",
          "form": "정주행해야겠어요",
          "original_form": "정주행해야겠어요",
          "speaker_id": "2",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025251.1.21",
          "form": "완전 추천이요!!!",
          "original_form": "완전 추천이요!!!",
          "speaker_id": "1",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025251.1.22",
          "form": "조진웅 너무 멋있어요...",
          "original_form": "조진웅 너무 멋있어요...",
          "speaker_id": "1",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025251.1.23",
          "form": "조진웅 볼매죠 ㅎㅎ",
          "original_form": "조진웅 볼매죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 03:10"
        },
        {
          "id": "MDRW2100025251.1.24",
          "form": "그매! 그냥봐도 매력있어요 ㅋㅋㅋㅋ",
          "original_form": "그매! 그냥봐도 매력있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:11"
        },
        {
          "id": "MDRW2100025251.1.25",
          "form": "이상형이 조진웅이시군요",
          "original_form": "이상형이 조진웅이시군요",
          "speaker_id": "2",
          "time": "20210901 03:11"
        },
        {
          "id": "MDRW2100025251.1.26",
          "form": "연예인중에는 꾸준히 호감이라 그런거같아요 ㅎㅎ",
          "original_form": "연예인중에는 꾸준히 호감이라 그런거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 03:12"
        },
        {
          "id": "MDRW2100025251.1.27",
          "form": "대학생때 사진봐도 잘생겼던데요~",
          "original_form": "대학생때 사진봐도 잘생겼던데요~",
          "speaker_id": "1",
          "time": "20210901 03:13"
        },
        {
          "id": "MDRW2100025251.1.28",
          "form": "저는 예전에 주말연속극 같은데에 나왔을때도 체격에 비해  귀엽다고 생각했는데... 넘나 멋진분이셨고 ㅎㅎ",
          "original_form": "저는 예전에 주말연속극 같은데에 나왔을때도 체격에 비해  귀엽다고 생각했는데... 넘나 멋진분이셨고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 03:13"
        },
        {
          "id": "MDRW2100025251.1.29",
          "form": "뿌리깊은나무에서 절정이었어요! 무사 무휼",
          "original_form": "뿌리깊은나무에서 절정이었어요! 무사 무휼",
          "speaker_id": "1",
          "time": "20210901 03:14"
        }
      ]
