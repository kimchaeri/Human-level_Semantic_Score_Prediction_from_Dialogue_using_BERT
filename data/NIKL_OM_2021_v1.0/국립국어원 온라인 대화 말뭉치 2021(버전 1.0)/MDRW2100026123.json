        },
        {
          "id": "MDRW2100026123.1.2",
          "form": "가볍게 라면먹었습니다",
          "original_form": "가볍게 라면먹었습니다",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026123.1.3",
          "form": "전 오댕국이요",
          "original_form": "전 오댕국이요",
          "speaker_id": "1",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026123.1.4",
          "form": "어묵국도 맛있죠~!",
          "original_form": "어묵국도 맛있죠~!",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100026123.1.5",
          "form": "네 맛나노",
          "original_form": "네 맛나노",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026123.1.6",
          "form": "어묵국에 떡볶이도 같이 있으면 정말",
          "original_form": "어묵국에 떡볶이도 같이 있으면 정말",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026123.1.7",
          "form": "최고일텐데 말이죵 ㅎㅎ",
          "original_form": "최고일텐데 말이죵 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026123.1.8",
          "form": "밥이랑 먹어서 분식은 ㅋㅋ",
          "original_form": "밥이랑 먹어서 분식은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026123.1.9",
          "form": "아하 그렇군요 ㅋㅋ",
          "original_form": "아하 그렇군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026123.1.10",
          "form": "간식으로 같이 먹어봐야겠네요",
          "original_form": "간식으로 같이 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026123.1.11",
          "form": "간식으로 먹어도 최고죠!",
          "original_form": "간식으로 먹어도 최고죠!",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026123.1.12",
          "form": "갠적으로 분식자체가 간식개념이긴해서 ㅎㅎ",
          "original_form": "갠적으로 분식자체가 간식개념이긴해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026123.1.13",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210902 07:10"
        },
        {
          "id": "MDRW2100026123.1.14",
          "form": "배고프네요 ㅎㅎ",
          "original_form": "배고프네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100026123.1.15",
          "form": "저도요",
          "original_form": "저도요",
