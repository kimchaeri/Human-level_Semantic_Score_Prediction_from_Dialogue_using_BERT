        },
        {
          "id": "MDRW2100039213.1.2",
          "form": "저도 볶음밥 먹었어요 ㅋㅋ",
          "original_form": "저도 볶음밥 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039213.1.3",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039213.1.4",
          "form": "아뇨 못마셔요 ㅠㅠ",
          "original_form": "아뇨 못마셔요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039213.1.5",
          "form": "커피 자주 드세요?",
          "original_form": "커피 자주 드세요?",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039213.1.6",
          "form": "아 그러시구나 ㅋㅋㅋ 네 저는 커피 너무 좋아해서요 ㅋㅋ",
          "original_form": "아 그러시구나 ㅋㅋㅋ 네 저는 커피 너무 좋아해서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100039213.1.7",
          "form": "그럼 쥬스 종류 좋아하세요?",
          "original_form": "그럼 쥬스 종류 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100039213.1.8",
          "form": "저는 녹차라떼나 초코맛 좋아해요!!",
          "original_form": "저는 녹차라떼나 초코맛 좋아해요!!",
          "speaker_id": "1",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100039213.1.9",
          "form": "커피는 아메리카노 드세요?",
          "original_form": "커피는 아메리카노 드세요?",
          "speaker_id": "1",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100039213.1.10",
          "form": "아니요 ㅋㅋㅋㅋ 커피 믹스나 ㅋㅋㅋㅋ 카페라떼 바닐라라떼 이런거 마셔요 ㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋㅋ 커피 믹스나 ㅋㅋㅋㅋ 카페라떼 바닐라라떼 이런거 마셔요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039213.1.11",
          "form": "녹차라떼 초코맛도 맛있죠 ㅋㅋ",
          "original_form": "녹차라떼 초코맛도 맛있죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039213.1.12",
          "form": "오 바닐라라떼 !!",
          "original_form": "오 바닐라라떼 !!",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039213.1.13",
          "form": "네 저는 그런 달달한 커피를 자주 마셔서 그런지",
          "original_form": "네 저는 그런 달달한 커피를 자주 마셔서 그런지",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039213.1.14",
          "form": "살이 많이 찌네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "살이 많이 찌네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039213.1.15",
          "form": "저도 ㅠㅠ 음료수 당이 많이 들어간데서",
          "original_form": "저도 ㅠㅠ 음료수 당이 많이 들어간데서",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039213.1.16",
          "form": "줄일려구요 ㅠㅠ",
          "original_form": "줄일려구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039213.1.17",
          "form": "네 저도ㅜㅠ맘처럼 쉽지는않지만ㅋㅋ",
          "original_form": "네 저도ㅜㅠ맘처럼 쉽지는않지만ㅋㅋ",
