      "utterance": [
        {
          "id": "MDRW2100043192.1.1",
          "form": "와 근데 새콤달콤만 봐도",
          "original_form": "와 근데 새콤달콤만 봐도",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043192.1.2",
          "form": "가격오른거 확 오더라고요",
          "original_form": "가격오른거 확 오더라고요",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043192.1.3",
          "form": "500원인가요? 700원인가",
          "original_form": "500원인가요? 700원인가",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043192.1.4",
          "form": "이제 칠백원이래요",
          "original_form": "이제 칠백원이래요",
          "speaker_id": "2",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043192.1.5",
          "form": "세상에나..ㅠㅠ",
          "original_form": "세상에나..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043192.1.6",
          "form": "충격이었어요 ㅋㅋㅋㅋ",
          "original_form": "충격이었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043192.1.7",
          "form": "이백원주고 사먹던 시절이 기억납니다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이백원주고 사먹던 시절이 기억납니다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:35"
        },
        {
          "id": "MDRW2100043192.1.8",
          "form": "저랑 비슷한 연령이신가보네여 ㅠㅠ",
          "original_form": "저랑 비슷한 연령이신가보네여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043192.1.9",
          "form": "ㅎㅎ 저희때는 이백원이었져",
          "original_form": "ㅎㅎ 저희때는 이백원이었져",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043192.1.10",
          "form": "아.....니......요?????????????????????ㅋㅋㅋㅋㅋ",
          "original_form": "아.....니......요?????????????????????ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043192.1.11",
          "form": "꽤 오래 이백원이었던거 같은데 삼백원으로 올랐던게 기억나네여",
          "original_form": "꽤 오래 이백원이었던거 같은데 삼백원으로 올랐던게 기억나네여",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043192.1.12",
          "form": "솔직히 요즘 물가가 너무 비싸여ㅠ 족발도 한번 먹을라치면.",
          "original_form": "솔직히 요즘 물가가 너무 비싸여ㅠ 족발도 한번 먹을라치면.",
          "speaker_id": "1",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043192.1.13",
          "form": "ㅋㅋㅋ ㅇ????!!",
          "original_form": "ㅋㅋㅋ ㅇ????!!",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043192.1.14",
          "form": "맞아요 족발도 많이 올랐더라고요",
          "original_form": "맞아요 족발도 많이 올랐더라고요",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043192.1.15",
          "form": "기본 4만 ㅠㅠㅠㅠ",
          "original_form": "기본 4만 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043192.1.16",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043192.1.17",
          "form": "저 저번달인가에 저희집 앞에 족발집 생겨서 시켜먹었거든요",
          "original_form": "저 저번달인가에 저희집 앞에 족발집 생겨서 시켜먹었거든요",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043192.1.18",
          "form": "맨날 치킨먹다가 한번씩  족발땡길때 보면 허걱 놀래요 가격보고 ㅎ",
          "original_form": "맨날 치킨먹다가 한번씩  족발땡길때 보면 허걱 놀래요 가격보고 ㅎ",
          "speaker_id": "1",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043192.1.19",
          "form": "양이나 많나 ㅠㅠㅠ",
