      "utterance": [
        {
          "id": "MDRW2100016232.1.1",
          "form": "그래\r\n다행이당",
          "original_form": "그래\r\n다행이당",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016232.1.2",
          "form": "환절기되니 또 비염이 시작인가보구먼",
          "original_form": "환절기되니 또 비염이 시작인가보구먼",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016232.1.3",
          "form": "응응 ㅋㅋㅋ이제 먹는거 좀 신경써줘야지 ㅋㅋ",
          "original_form": "응응 ㅋㅋㅋ이제 먹는거 좀 신경써줘야지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016232.1.4",
          "form": "학교갔나??",
          "original_form": "학교갔나??",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016232.1.5",
          "form": "응 그런것같아",
          "original_form": "응 그런것같아",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016232.1.6",
          "form": "응 학교는 보냈지 ㅋㅋ",
          "original_form": "응 학교는 보냈지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016232.1.7",
          "form": "선생님께 여쭤보니 ㅋㅋ 병원 다녀와라고 하더라고",
          "original_form": "선생님께 여쭤보니 ㅋㅋ 병원 다녀와라고 하더라고",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016232.1.8",
          "form": "늦어도 되니깐 ㅋㅋ",
          "original_form": "늦어도 되니깐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016232.1.9",
          "form": "그래 잘했다",
          "original_form": "그래 잘했다",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016232.1.10",
          "form": "첫째가 내보고 엄마. 나는 참 착한 선생님들 만난다며 ㅋㅋ",
          "original_form": "첫째가 내보고 엄마. 나는 참 착한 선생님들 만난다며 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016232.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래 선생님 중요하지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래 선생님 중요하지",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016232.1.12",
          "form": "우리name3도 선생님 2학년때도 선생님이었으면 좋겠다고",
          "original_form": "우리&name3&도 선생님 2학년때도 선생님이었으면 좋겠다고",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016232.1.13",
          "form": "다행히 애들이 선생님 복이 많음",
          "original_form": "다행히 애들이 선생님 복이 많음",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016232.1.14",
          "form": "그러니깐 다행이다 ㅋㅋ",
          "original_form": "그러니깐 다행이다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016232.1.15",
          "form": "나도 지금 첫째 선생님 좋거든 ㅋㅋ 2학년때도 담임해주셨으면 좋겠지만 안되겠지..ㅋㅋ",
          "original_form": "나도 지금 첫째 선생님 좋거든 ㅋㅋ 2학년때도 담임해주셨으면 좋겠지만 안되겠지..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016232.1.16",
          "form": "그래 연임했으면 좋겠네 ㅎㅎㅎ",
          "original_form": "그래 연임했으면 좋겠네 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016232.1.17",
          "form": "집안일 좀 했나??",
          "original_form": "집안일 좀 했나??",
          "speaker_id": "2",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016232.1.18",
          "form": "일단 ㅋㅋㅋ 청소랑 설저지했다. 빨래돌리고 ㅋㅋ",
          "original_form": "일단 ㅋㅋㅋ 청소랑 설저지했다. 빨래돌리고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016232.1.19",
          "form": "이제 창틀 닦고 바닥 닦고 ㅋㅋㅋ 수납장 정리 좀 하게 ㅋㅋ",
          "original_form": "이제 창틀 닦고 바닥 닦고 ㅋㅋㅋ 수납장 정리 좀 하게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016232.1.20",
          "form": "치우고 치우고 해도 ㅋㅋ 왜이리 뭐가 많은지",
          "original_form": "치우고 치우고 해도 ㅋㅋ 왜이리 뭐가 많은지",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016232.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ  뭐 많이 하네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ  뭐 많이 하네",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016232.1.22",
          "form": "쉴 시간이 없구만",
          "original_form": "쉴 시간이 없구만",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016232.1.23",
          "form": "수납장 정리라....",
          "original_form": "수납장 정리라....",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016232.1.24",
          "form": "첫째 오기 전까지 다 할 수 있겠나",
          "original_form": "첫째 오기 전까지 다 할 수 있겠나",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016232.1.25",
          "form": "얍얍 ㅋㅋ 대충 조금씩 정리하고 있어서 ㅋㅋㅋ 조금 정리 또 하구 ㅋㅋ 쉬여야지",
          "original_form": "얍얍 ㅋㅋ 대충 조금씩 정리하고 있어서 ㅋㅋㅋ 조금 정리 또 하구 ㅋㅋ 쉬여야지",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016232.1.26",
          "form": "다는 못한다 ㅋㅋㅋㅋ",
          "original_form": "다는 못한다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016232.1.27",
          "form": "그래 대충 하고 쉬어라",
          "original_form": "그래 대충 하고 쉬어라",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100016232.1.28",
          "form": "어제 리틀 제이니 세일을 한다고 해서",
          "original_form": "어제 리틀 제이니 세일을 한다고 해서",
          "speaker_id": "2",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016232.1.29",
          "form": "애들 바지 세 개 사왔음",
          "original_form": "애들 바지 세 개 사왔음",
          "speaker_id": "2",
          "time": "20210826 11:34"
        }
      ]
