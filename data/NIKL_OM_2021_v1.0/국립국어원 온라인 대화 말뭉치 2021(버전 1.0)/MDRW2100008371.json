      "utterance": [
        {
          "id": "MDRW2100008371.1.1",
          "form": "헌혈도 사실 아무나 못하는 것 같아요",
          "original_form": "헌혈도 사실 아무나 못하는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008371.1.2",
          "form": "약간 건강의 증표?",
          "original_form": "약간 건강의 증표?",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008371.1.3",
          "form": "아 그것도 있는거 같아요",
          "original_form": "아 그것도 있는거 같아요",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008371.1.4",
          "form": "으어 단백질이 높아 이러게 된다는 ㅋㅋ",
          "original_form": "으어 단백질이 높아 이러게 된다는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008371.1.5",
          "form": "ㅋㅋㅋ운동은 좋아하세요?",
          "original_form": "ㅋㅋㅋ운동은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008371.1.6",
          "form": "아뇨ㅜ 운동 좋아하는 사람이 되고싶어요ㅜㅜ",
          "original_form": "아뇨ㅜ 운동 좋아하는 사람이 되고싶어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008371.1.7",
          "form": "학교에서 하는 것중에선 재밌었던것 없나요?",
          "original_form": "학교에서 하는 것중에선 재밌었던것 없나요?",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008371.1.8",
          "form": "제일 좋아했던 거나~",
          "original_form": "제일 좋아했던 거나~",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008371.1.9",
          "form": "운동 쪽은 다 잘 못했던거같아요",
          "original_form": "운동 쪽은 다 잘 못했던거같아요",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008371.1.10",
          "form": "요즘최소한 다이어트 땜에 만보걷기는 하려고해요",
          "original_form": "요즘최소한 다이어트 땜에 만보걷기는 하려고해요",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008371.1.11",
          "form": "만보걷기 건강에 좋죠!",
          "original_form": "만보걷기 건강에 좋죠!",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008371.1.12",
          "form": "근데 은근 힘들더라고요 ㅎㅎ",
          "original_form": "근데 은근 힘들더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008371.1.13",
          "form": "시간이 오래걸리더라고요 빨리 걸어야 효과있대여",
          "original_form": "시간이 오래걸리더라고요 빨리 걸어야 효과있대여",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008371.1.14",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008371.1.15",
          "form": "그건 몰랐내요",
          "original_form": "그건 몰랐내요",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008371.1.16",
          "form": "얼마나 빨리걸어야할까요?",
          "original_form": "얼마나 빨리걸어야할까요?",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008371.1.17",
          "form": "정확히는 인터벌로 걸어야한대여",
          "original_form": "정확히는 인터벌로 걸어야한대여",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008371.1.18",
          "form": "아 런데이에서 하는 그런거요?",
          "original_form": "아 런데이에서 하는 그런거요?",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008371.1.19",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008371.1.20",
          "form": "근데 저는 폰하면거 걸으니 속도가 넘느리더라고요 ㅋㅋㅋ",
          "original_form": "근데 저는 폰하면거 걸으니 속도가 넘느리더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008371.1.21",
          "form": "운동 안되게스리 ㅋㅋ",
          "original_form": "운동 안되게스리 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008371.1.22",
          "form": "ㅋㅋㅋㅋㅋ폰의 유혹을 떨치기 어렵죠",
          "original_form": "ㅋㅋㅋㅋㅋ폰의 유혹을 떨치기 어렵죠",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008371.1.23",
          "form": "런데이 깔고하시면 열심히하개 되실수도?",
          "original_form": "런데이 깔고하시면 열심히하개 되실수도?",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008371.1.24",
          "form": "앱 써보셨나요? 어떤가요?",
          "original_form": "앱 써보셨나요? 어떤가요?",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008371.1.25",
          "form": "엄청 좋던데요?",
          "original_form": "엄청 좋던데요?",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008371.1.26",
          "form": "30분 달리는데",
          "original_form": "30분 달리는데",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008371.1.27",
          "form": "그 사이에 계속 말걸어줘요",
          "original_form": "그 사이에 계속 말걸어줘요",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008371.1.28",
          "form": "운동팁알려주고 페이스조절 도와주더라고요",
          "original_form": "운동팁알려주고 페이스조절 도와주더라고요",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008371.1.29",
          "form": "오 유료앱인가요?",
          "original_form": "오 유료앱인가요?",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008371.1.30",
          "form": "아니요 무료요!",
          "original_form": "아니요 무료요!",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008371.1.31",
          "form": "그래서 짱이죠",
          "original_form": "그래서 짱이죠",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008371.1.32",
          "form": "오 저도 다운받아봐야겠어요",
          "original_form": "오 저도 다운받아봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008371.1.33",
          "form": "저는 토스만보기만썼는데 ㅎ",
          "original_form": "저는 토스만보기만썼는데 ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:39"
        }
      ]
