      "utterance": [
        {
          "id": "MDRW2100024023.1.1",
          "form": "스벅 완전 좋아합니다",
          "original_form": "스벅 완전 좋아합니다",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024023.1.2",
          "form": "스벅 좋죠 ㅎㅎ",
          "original_form": "스벅 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100024023.1.3",
          "form": "완전 컸어요 이제ㅎㅎ",
          "original_form": "완전 컸어요 이제ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100024023.1.4",
          "form": "오늘 시간나면 스벅가야겠어요",
          "original_form": "오늘 시간나면 스벅가야겠어요",
          "speaker_id": "1",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100024023.1.5",
          "form": "아침을 안먹었더니 배고프네요ㅜㅜ",
          "original_form": "아침을 안먹었더니 배고프네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100024023.1.6",
          "form": "삼계탕이 땡기네요",
          "original_form": "삼계탕이 땡기네요",
          "speaker_id": "1",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100024023.1.7",
          "form": "오 삼계탕 저도 급 땡기는군요",
          "original_form": "오 삼계탕 저도 급 땡기는군요",
          "speaker_id": "2",
          "time": "20210831 10:06"
        },
        {
          "id": "MDRW2100024023.1.8",
          "form": "그니깐요ㅜㅜ아직 지금은 먹을수있으니",
          "original_form": "그니깐요ㅜㅜ아직 지금은 먹을수있으니",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100024023.1.9",
          "form": "ㅋㅋ짜파게티로...",
          "original_form": "ㅋㅋ짜파게티로...",
          "speaker_id": "2",
          "time": "20210831 16:15"
        },
        {
          "id": "MDRW2100024023.1.10",
          "form": "저는 우선 지금 배스킨왔는데 드뎌 바나나맛을 찾아서",
          "original_form": "저는 우선 지금 배스킨왔는데 드뎌 바나나맛을 찾아서",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024023.1.11",
          "form": "먹을수있게됐어요",
          "original_form": "먹을수있게됐어요",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100024023.1.12",
          "form": "오 맛있나요",
          "original_form": "오 맛있나요",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024023.1.13",
          "form": "샀는데 한입먹고 버스타서",
          "original_form": "샀는데 한입먹고 버스타서",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024023.1.14",
          "form": "버스내리면 먹을려구요",
          "original_form": "버스내리면 먹을려구요",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024023.1.15",
          "form": "ㅜㆍㅜ녹았어요",
          "original_form": "ㅜㆍㅜ녹았어요",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024023.1.16",
          "form": "40분타서 내렸는데",
          "original_form": "40분타서 내렸는데",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024023.1.17",
          "form": "녹았네요 지금 마시면서 먹는중....",
          "original_form": "녹았네요 지금 마시면서 먹는중....",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024023.1.18",
          "form": "아 그거 안타까운데ㅠ",
          "original_form": "아 그거 안타까운데ㅠ",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024023.1.19",
          "form": "배스킨마시다가 옷베렸어요ㅜㅜ",
          "original_form": "배스킨마시다가 옷베렸어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100024023.1.20",
          "form": "배가고프네요...",
          "original_form": "배가고프네요...",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100024023.1.21",
          "form": "삼겹살먹고싶어요",
          "original_form": "삼겹살먹고싶어요",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100024023.1.22",
          "form": "삼겹살 진짜죠 ㅎㅎ",
          "original_form": "삼겹살 진짜죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100024023.1.23",
          "form": "맛있겠다요",
          "original_form": "맛있겠다요",
          "speaker_id": "2",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100024023.1.24",
          "form": "삼겹살먹고 배스킨먹으면 꿀인데",
          "original_form": "삼겹살먹고 배스킨먹으면 꿀인데",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100024023.1.25",
          "form": "ㅋㅋ삼쏘 아닙니까",
          "original_form": "ㅋㅋ삼쏘 아닙니까",
          "speaker_id": "2",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100024023.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100024023.1.27",
          "form": "그니깐요",
          "original_form": "그니깐요",
          "speaker_id": "1",
          "time": "20210901 18:16"
        }
      ]
    }
  ]
