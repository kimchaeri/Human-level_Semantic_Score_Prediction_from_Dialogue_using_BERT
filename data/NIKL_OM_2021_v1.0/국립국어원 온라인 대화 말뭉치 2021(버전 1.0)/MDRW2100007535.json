      "utterance": [
        {
          "id": "MDRW2100007535.1.1",
          "form": "저는 그 야구는 좀 열받았어요",
          "original_form": "저는 그 야구는 좀 열받았어요",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007535.1.2",
          "form": "아 진짜",
          "original_form": "아 진짜",
          "speaker_id": "1",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007535.1.3",
          "form": "욕나왂어뇨",
          "original_form": "욕나왂어뇨",
          "speaker_id": "2",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007535.1.4",
          "form": "패기도없고",
          "original_form": "패기도없고",
          "speaker_id": "1",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007535.1.5",
          "form": "열정도없고",
          "original_form": "열정도없고",
          "speaker_id": "1",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007535.1.6",
          "form": "그러면서 공중파 3사 다 받아먹고",
          "original_form": "그러면서 공중파 3사 다 받아먹고",
          "speaker_id": "1",
          "time": "20210822 03:00"
        },
        {
          "id": "MDRW2100007535.1.7",
          "form": "그니까요 그럴거면 왜 나가",
          "original_form": "그니까요 그럴거면 왜 나가",
          "speaker_id": "2",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007535.1.8",
          "form": "아니 메이저리그도 아니었담서요",
          "original_form": "아니 메이저리그도 아니었담서요",
          "speaker_id": "2",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007535.1.9",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007535.1.10",
          "form": "근데 거의 다져",
          "original_form": "근데 거의 다져",
          "speaker_id": "2",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007535.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나 참",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나 참",
          "speaker_id": "2",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007535.1.12",
          "form": "야구 6개팀 나왔다더라구요",
          "original_form": "야구 6개팀 나왔다더라구요",
          "speaker_id": "1",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007535.1.13",
          "form": "어이없지요",
          "original_form": "어이없지요",
          "speaker_id": "2",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007535.1.14",
          "form": "그중에서 한국은 인기종목에 나름 밀어주는 종목인데도 불구하고",
          "original_form": "그중에서 한국은 인기종목에 나름 밀어주는 종목인데도 불구하고",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007535.1.15",
          "form": "그 곳에서 진다는게",
          "original_form": "그 곳에서 진다는게",
          "speaker_id": "2",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007535.1.16",
          "form": "ㅎ.. 그따구로 말아먹으니",
          "original_form": "ㅎ.. 그따구로 말아먹으니",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007535.1.17",
          "form": "반성해야해요",
          "original_form": "반성해야해요",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007535.1.18",
          "form": "글고 야구 선수들은 팬서비스가 별로더라고요",
          "original_form": "글고 야구 선수들은 팬서비스가 별로더라고요",
          "speaker_id": "2",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007535.1.19",
          "form": "약간..자기들이 톱스타인것처럼",
          "original_form": "약간..자기들이 톱스타인것처럼",
          "speaker_id": "2",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007535.1.20",
          "form": "아 뭔 말인지 알거같아요",
          "original_form": "아 뭔 말인지 알거같아요",
          "speaker_id": "1",
          "time": "20210822 03:02"
        },
        {
          "id": "MDRW2100007535.1.21",
          "form": "이번 올림픽에서 제일 열정없는 스포츠였던거같아요",
          "original_form": "이번 올림픽에서 제일 열정없는 스포츠였던거같아요",
          "speaker_id": "1",
          "time": "20210822 03:03"
        }
      ]
