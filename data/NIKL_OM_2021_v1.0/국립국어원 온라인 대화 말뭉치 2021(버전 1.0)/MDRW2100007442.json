          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007442.1.12",
          "form": "수요웹툰이네요",
          "original_form": "수요웹툰이네요",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007442.1.13",
          "form": "ㅋㅋㅋㅋㅋ아닙니다",
          "original_form": "ㅋㅋㅋㅋㅋ아닙니다",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007442.1.14",
          "form": "다아셔",
          "original_form": "다아셔",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007442.1.15",
          "form": "마도조사는재밋어서봣고",
          "original_form": "마도조사는재밋어서봣고",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007442.1.16",
          "form": "평소엔 소설거의안읽어요ㅎ",
          "original_form": "평소엔 소설거의안읽어요ㅎ",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
