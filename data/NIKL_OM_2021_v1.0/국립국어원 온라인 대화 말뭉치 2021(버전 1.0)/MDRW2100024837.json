      "utterance": [
        {
          "id": "MDRW2100024837.1.1",
          "form": "파르페 좋아하세요?",
          "original_form": "파르페 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024837.1.2",
          "form": "한 번 먹어본 거 같은데 기억이 잘 안 나네요ㅋㅋㅋ 요즘은 잘 안 파는 거 같아요",
          "original_form": "한 번 먹어본 거 같은데 기억이 잘 안 나네요ㅋㅋㅋ 요즘은 잘 안 파는 거 같아요",
          "speaker_id": "2",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024837.1.3",
          "form": "파르페가 인상깊었나봐요ㅋㅋ",
          "original_form": "파르페가 인상깊었나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024837.1.4",
          "form": "닉넴도 name2 시고ㅋㅋ",
          "original_form": "닉넴도 &name2& 시고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024837.1.5",
          "form": "아 제 닉넴은 좋아하는 웹툰 주인공 이름이에요!ㅎㅎ",
          "original_form": "아 제 닉넴은 좋아하는 웹툰 주인공 이름이에요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024837.1.6",
          "form": "아그렇구나ㅋㅋㅋㅋ",
          "original_form": "아그렇구나ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024837.1.7",
          "form": "전 먹을건줄알고ㅋㅋ",
          "original_form": "전 먹을건줄알고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100024837.1.8",
          "form": "워낙 어릴 때 한 번 먹어본 거라 지금은 무슨 맛이었는지 기억이 안 나네요",
          "original_form": "워낙 어릴 때 한 번 먹어본 거라 지금은 무슨 맛이었는지 기억이 안 나네요",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024837.1.9",
          "form": "들어가는 재료가 달달하고 맛있는 거 뿐이라서 당연히 맛있었겠지만요ㅋㅋㅋ",
          "original_form": "들어가는 재료가 달달하고 맛있는 거 뿐이라서 당연히 맛있었겠지만요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024837.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 파르페는 기억이없어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 파르페는 기억이없어요",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100024837.1.11",
          "form": "달달한거 땡기네요",
          "original_form": "달달한거 땡기네요",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100024837.1.12",
          "form": "저도요~ 갑자기 쉐이크 마시고 싶네요",
          "original_form": "저도요~ 갑자기 쉐이크 마시고 싶네요",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024837.1.13",
          "form": "무슨맛 쉐이크 좋아하세요?",
          "original_form": "무슨맛 쉐이크 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024837.1.14",
          "form": "오레오 쉐이크 좋아해요!",
          "original_form": "오레오 쉐이크 좋아해요!",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024837.1.15",
          "form": "역시 오쉐는 진리죠",
          "original_form": "역시 오쉐는 진리죠",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024837.1.16",
          "form": "좋아하는 음료 있으세요?",
          "original_form": "좋아하는 음료 있으세요?",
          "speaker_id": "2",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024837.1.17",
          "form": "전 아아 자주먹어요!!!",
          "original_form": "전 아아 자주먹어요!!!",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024837.1.18",
          "form": "저도 커피는 아아만 마셔요!",
          "original_form": "저도 커피는 아아만 마셔요!",
          "speaker_id": "2",
