          "original_form": "그렇군",
          "speaker_id": "1",
          "time": "20210818 15:58"
        },
        {
          "id": "MDRW2100000332.1.13",
          "form": "5/16 5/17",
          "original_form": "5/16 5/17",
          "speaker_id": "2",
          "time": "20210818 15:59"
        },
        {
          "id": "MDRW2100000332.1.14",
          "form": "하락장에 펌핑 오더라",
          "original_form": "하락장에 펌핑 오더라",
          "speaker_id": "2",
          "time": "20210818 15:59"
        },
        {
          "id": "MDRW2100000332.1.15",
          "form": "오늘은 여기까지",
          "original_form": "오늘은 여기까지",
          "speaker_id": "2",
          "time": "20210818 16:01"
        },
        {
          "id": "MDRW2100000332.1.16",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210818 16:03"
        },
        {
          "id": "MDRW2100000332.1.17",
          "form": "굿모닝",
          "original_form": "굿모닝",
          "speaker_id": "1",
          "time": "20210819 07:17"
        },
        {
          "id": "MDRW2100000332.1.18",
          "form": "요즘 늦게 잤더니 피곤하다",
          "original_form": "요즘 늦게 잤더니 피곤하다",
          "speaker_id": "2",
          "time": "20210819 07:18"
        },
        {
          "id": "MDRW2100000332.1.19",
          "form": "나도 12시에 잤어",
          "original_form": "나도 12시에 잤어",
          "speaker_id": "1",
          "time": "20210819 07:19"
        },
        {
          "id": "MDRW2100000332.1.20",
