      "utterance": [
        {
          "id": "MDRW2100000332.1.1",
          "form": "세럼 왜?",
          "original_form": "세럼 왜?",
          "speaker_id": "1",
          "time": "20210818 15:55"
        },
        {
          "id": "MDRW2100000332.1.2",
          "form": "대 폭락의 서막이야?",
          "original_form": "대 폭락의 서막이야?",
          "speaker_id": "1",
          "time": "20210818 15:56"
        },
        {
          "id": "MDRW2100000332.1.3",
          "form": "ㅇㅇ 퀀텀 후 세럼",
          "original_form": "ㅇㅇ 퀀텀 후 세럼",
          "speaker_id": "2",
          "time": "20210818 15:56"
        },
        {
          "id": "MDRW2100000332.1.4",
          "form": "기우겠지",
          "original_form": "기우겠지",
          "speaker_id": "2",
          "time": "20210818 15:56"
        },
        {
          "id": "MDRW2100000332.1.5",
          "form": "5천에 사서 너무 빨리 팔았다 울자",
          "original_form": "5천에 사서 너무 빨리 팔았다 울자",
          "speaker_id": "2",
          "time": "20210818 15:57"
        },
        {
          "id": "MDRW2100000332.1.6",
          "form": "이거 주제 다 채울려면",
          "original_form": "이거 주제 다 채울려면",
          "speaker_id": "2",
          "time": "20210818 15:57"
        },
        {
          "id": "MDRW2100000332.1.7",
          "form": "무슨말인지 모르겠어",
          "original_form": "무슨말인지 모르겠어",
          "speaker_id": "1",
          "time": "20210818 15:57"
        },
        {
          "id": "MDRW2100000332.1.8",
          "form": "어마어마하게 오래 걸리겠네",
          "original_form": "어마어마하게 오래 걸리겠네",
          "speaker_id": "2",
          "time": "20210818 15:57"
        },
        {
          "id": "MDRW2100000332.1.9",
          "form": "세럼 오르면 떨어지나?",
          "original_form": "세럼 오르면 떨어지나?",
          "speaker_id": "1",
          "time": "20210818 15:57"
        },
        {
          "id": "MDRW2100000332.1.10",
          "form": "전에",
          "original_form": "전에",
          "speaker_id": "2",
          "time": "20210818 15:58"
        },
        {
          "id": "MDRW2100000332.1.11",
          "form": "우연일거야",
          "original_form": "우연일거야",
          "speaker_id": "2",
          "time": "20210818 15:58"
        },
        {
