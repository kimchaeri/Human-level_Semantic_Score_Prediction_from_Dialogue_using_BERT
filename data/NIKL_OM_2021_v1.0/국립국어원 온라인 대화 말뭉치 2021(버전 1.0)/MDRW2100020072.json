      "utterance": [
        {
          "id": "MDRW2100020072.1.1",
          "form": "재밌다던데요?",
          "original_form": "재밌다던데요?",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020072.1.2",
          "form": "저는 블위가 최애였어서ㅜㅜ",
          "original_form": "저는 블위가 최애였어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020072.1.3",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020072.1.4",
          "form": "최애인 이유가 있어요?",
          "original_form": "최애인 이유가 있어요?",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020072.1.5",
          "form": "저는 보긴 보는데 잘 알고 보는건 아니라 잘 몰라요",
          "original_form": "저는 보긴 보는데 잘 알고 보는건 아니라 잘 몰라요",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020072.1.6",
          "form": "아무래도 여성 히어로도 별로 없고",
          "original_form": "아무래도 여성 히어로도 별로 없고",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020072.1.7",
          "form": "다른 남캐에 비해서도 밀리지 않는게 너무 멋있더라고요",
          "original_form": "다른 남캐에 비해서도 밀리지 않는게 너무 멋있더라고요",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020072.1.8",
          "form": "맞아요 여성캐릭터가 거의 없네요",
          "original_form": "맞아요 여성캐릭터가 거의 없네요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020072.1.9",
          "form": "그 빨간염력쓰는 여성 캐릭터 이름이 뭐죠?",
          "original_form": "그 빨간염력쓰는 여성 캐릭터 이름이 뭐죠?",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020072.1.10",
          "form": "스칼렛 위치요!",
          "original_form": "스칼렛 위치요!",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020072.1.11",
          "form": "비전이랑 같이있던",
          "original_form": "비전이랑 같이있던",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020072.1.12",
          "form": "캐릭터 이름이 스칼렛위치에요?",
          "original_form": "캐릭터 이름이 스칼렛위치에요?",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020072.1.13",
          "form": "네네 아마 그랬던 것 같아요!",
          "original_form": "네네 아마 그랬던 것 같아요!",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020072.1.14",
          "form": "ㅋㅋㅋㅋ 척척박사네요 신기해요",
          "original_form": "ㅋㅋㅋㅋ 척척박사네요 신기해요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020072.1.15",
          "form": "스칼렛 요한슨은 그냥 보면서 너무 예뻐서",
          "original_form": "스칼렛 요한슨은 그냥 보면서 너무 예뻐서",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020072.1.16",
          "form": "넋놓고 봤었어요",
          "original_form": "넋놓고 봤었어요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020072.1.17",
          "form": "맞아요 너무 멋있고 예뻐요..",
          "original_form": "맞아요 너무 멋있고 예뻐요..",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020072.1.18",
          "form": "블랙위도우가 지금 상영중이죠?",
          "original_form": "블랙위도우가 지금 상영중이죠?",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020072.1.19",
          "form": "name2은 못가셔서 아쉬우시겠어요...",
          "original_form": "&name2&은 못가셔서 아쉬우시겠어요...",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020072.1.20",
          "form": "아마 상영 끝났을거에요..",
          "original_form": "아마 상영 끝났을거에요..",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020072.1.21",
          "form": "너무 아쉽더라고요ㅜㅜ",
          "original_form": "너무 아쉽더라고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020072.1.22",
          "form": "꼭 극장에서 보고싶었는데",
          "original_form": "꼭 극장에서 보고싶었는데",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020072.1.23",
          "form": "아이고 하필ㅠㅜㅠㅠ 나중이라도 재개봉해주면 좋겠네요ㅠ",
          "original_form": "아이고 하필ㅠㅜㅠㅠ 나중이라도 재개봉해주면 좋겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020072.1.24",
          "form": "ㅠㅠ 꼬옥 재개봉 해주면 되....",
