      "utterance": [
        {
          "id": "MDRW2100031418.1.1",
          "form": "와! 부럽다!",
          "original_form": "와! 부럽다!",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031418.1.2",
          "form": "나는 그거",
          "original_form": "나는 그거",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031418.1.3",
          "form": "포토카드 주문했어!",
          "original_form": "포토카드 주문했어!",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031418.1.4",
          "form": "포토카드 이쁘겠다 언제 와?",
          "original_form": "포토카드 이쁘겠다 언제 와?",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031418.1.5",
          "form": "모르겠어 주말끼어서..",
          "original_form": "모르겠어 주말끼어서..",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031418.1.6",
          "form": "다음주에 친구들한테 주고싶은데 되려나",
          "original_form": "다음주에 친구들한테 주고싶은데 되려나",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031418.1.7",
          "form": "아쉽다 나도 키링시켰는데 주말 끼었어",
          "original_form": "아쉽다 나도 키링시켰는데 주말 끼었어",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031418.1.8",
          "form": "되지않을까?",
          "original_form": "되지않을까?",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100031418.1.9",
          "form": "그럴까? 빨리 받고싶다!",
          "original_form": "그럴까? 빨리 받고싶다!",
          "speaker_id": "2",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100031418.1.10",
          "form": "몇 개나 시켰어?",
          "original_form": "몇 개나 시켰어?",
          "speaker_id": "1",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100031418.1.11",
          "form": "ㅎㅎ..",
          "original_form": "ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100031418.1.12",
          "form": "장수로하면 48장",
          "original_form": "장수로하면 48장",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031418.1.13",
          "form": "헉! 그렇게나 많이?",
          "original_form": "헉! 그렇게나 많이?",
          "speaker_id": "1",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031418.1.14",
          "form": "그치만 그게 세트로 시킬수 있는거구.. 2세트밖에 안시켰어!",
          "original_form": "그치만 그게 세트로 시킬수 있는거구.. 2세트밖에 안시켰어!",
          "speaker_id": "2",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100031418.1.15",
          "form": "그렇구나! 비싸진않았어?",
          "original_form": "그렇구나! 비싸진않았어?",
          "speaker_id": "1",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100031418.1.16",
          "form": "그렇게 비싸진.. 어차피 내거 주문하면서 산거라서",
          "original_form": "그렇게 비싸진.. 어차피 내거 주문하면서 산거라서",
          "speaker_id": "2",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100031418.1.17",
          "form": "정말 넌 멋진 친구야",
          "original_form": "정말 넌 멋진 친구야",
          "speaker_id": "1",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100031418.1.18",
          "form": "그런가? 친구끼리는 선물도 막 주고싶고 그러는거지!ㅎㅎ",
          "original_form": "그런가? 친구끼리는 선물도 막 주고싶고 그러는거지!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100031418.1.19",
          "form": "멋져!",
          "original_form": "멋져!",
          "speaker_id": "1",
