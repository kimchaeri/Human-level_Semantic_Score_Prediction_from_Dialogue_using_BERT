      "utterance": [
        {
          "id": "MDRW2100008800.1.1",
          "form": "다른 대표팀 주장들의 주장이 김연경이라며",
          "original_form": "다른 대표팀 주장들의 주장이 김연경이라며",
          "speaker_id": "2",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008800.1.2",
          "form": "맞아 터키에서 주장까지 할 정도면 진심 대단한듯",
          "original_form": "맞아 터키에서 주장까지 할 정도면 진심 대단한듯",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008800.1.3",
          "form": "타국사람들은주장까지잘못하잖아",
          "original_form": "타국사람들은주장까지잘못하잖아",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008800.1.4",
          "form": "그니까 아주그냥 대박이야",
          "original_form": "그니까 아주그냥 대박이야",
          "speaker_id": "2",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008800.1.5",
          "form": "다음올림픽파린데",
          "original_form": "다음올림픽파린데",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008800.1.6",
          "form": "직관하고싶음",
          "original_form": "직관하고싶음",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008800.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100008800.1.8",
          "form": "24년도면제대로올림픽할수있겠지?",
          "original_form": "24년도면제대로올림픽할수있겠지?",
          "speaker_id": "1",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100008800.1.9",
          "form": "그러겠지 아마...",
          "original_form": "그러겠지 아마...",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100008800.1.10",
          "form": "그때 망원경 들고 가서 보고싶다",
          "original_form": "그때 망원경 들고 가서 보고싶다",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100008800.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "1",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100008800.1.12",
          "form": "김연경선수님없더라도",
          "original_form": "김연경선수님없더라도",
          "speaker_id": "1",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100008800.1.13",
          "form": "배구올림픽은진심직관하고싶음",
          "original_form": "배구올림픽은진심직관하고싶음",
          "speaker_id": "1",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100008800.1.14",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210822 18:50"
        },
        {
          "id": "MDRW2100008800.1.15",
          "form": "브라질이랑 다시 해라 태평소 들고 가서 응원해야 하는데",
          "original_form": "브라질이랑 다시 해라 태평소 들고 가서 응원해야 하는데",
          "speaker_id": "2",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100008800.1.16",
          "form": "브라질다음에다시하면",
          "original_form": "브라질다음에다시하면",
          "speaker_id": "1",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100008800.1.17",
          "form": "제대로했으면",
          "original_form": "제대로했으면",
          "speaker_id": "1",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100008800.1.18",
          "form": "또도핑걸리지말고",
          "original_form": "또도핑걸리지말고",
          "speaker_id": "1",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100008800.1.19",
          "form": "그러니까 그리고 무관중이었는데 브라질 대표팀이 와서 방해했었잖아",
          "original_form": "그러니까 그리고 무관중이었는데 브라질 대표팀이 와서 방해했었잖아",
          "speaker_id": "2",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008800.1.20",
          "form": "막 서브 넣는데 휘파람 부르고",
          "original_form": "막 서브 넣는데 휘파람 부르고",
          "speaker_id": "2",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008800.1.21",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008800.1.22",
          "form": "아니 공평하게 해야지",
          "original_form": "아니 공평하게 해야지",
          "speaker_id": "1",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008800.1.23",
