      "utterance": [
        {
          "id": "MDRW2100029164.1.1",
          "form": "영어에 자주 노출되는 환경만 잘 만들고",
          "original_form": "영어에 자주 노출되는 환경만 잘 만들고",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029164.1.2",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100029164.1.3",
          "form": "막 엄청 열심히 외우는 건 아니더라도",
          "original_form": "막 엄청 열심히 외우는 건 아니더라도",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.4",
          "form": "눈으로 맨낳 슥 보기라도 하면",
          "original_form": "눈으로 맨낳 슥 보기라도 하면",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.5",
          "form": "훨씬 괜찮을텐데",
          "original_form": "훨씬 괜찮을텐데",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.6",
          "form": "뭔가 꼼꼼히 외워야한다는",
          "original_form": "뭔가 꼼꼼히 외워야한다는",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.7",
          "form": "맞암자아",
          "original_form": "맞암자아",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.8",
          "form": "압박감?",
          "original_form": "압박감?",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.9",
          "form": "강박때문에",
          "original_form": "강박때문에",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.10",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.11",
          "form": "같은게 있는듯",
          "original_form": "같은게 있는듯",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.12",
          "form": "완벽하게 하려고해서",
          "original_form": "완벽하게 하려고해서",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029164.1.13",
          "form": "항상 오래가질못함",
          "original_form": "항상 오래가질못함",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.14",
          "form": "맞음맞음맞음",
          "original_form": "맞음맞음맞음",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.15",
          "form": "오래하는게 젤중요한데..",
          "original_form": "오래하는게 젤중요한데..",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.16",
          "form": "항상 첫날",
          "original_form": "항상 첫날",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.17",
          "form": "예문같은거까지",
          "original_form": "예문같은거까지",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.18",
          "form": "다 보려다가",
          "original_form": "다 보려다가",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.19",
          "form": "맞아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.20",
          "form": "작심 3일도 아니고",
          "original_form": "작심 3일도 아니고",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.21",
          "form": "예문에 나오는 단어까지",
          "original_form": "예문에 나오는 단어까지",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.22",
          "form": "1일만에 포기하게됨",
          "original_form": "1일만에 포기하게됨",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.23",
          "form": "다 쳐보고",
          "original_form": "다 쳐보고",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.24",
          "form": "그러다가",
          "original_form": "그러다가",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.25",
          "form": "기 다빠짐",
          "original_form": "기 다빠짐",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.26",
          "form": "ㅇㅈ...ㅡ",
          "original_form": "ㅇㅈ...ㅡ",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.27",
          "form": "비효율을 끝판왕임 ㄹㅇ",
          "original_form": "비효율을 끝판왕임 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.28",
          "form": "내가 주식공부를",
          "original_form": "내가 주식공부를",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029164.1.29",
          "form": "올해초부터 시작햇잖아",
          "original_form": "올해초부터 시작햇잖아",
          "speaker_id": "1",
          "time": "20210903 00:02"
        }
      ]
