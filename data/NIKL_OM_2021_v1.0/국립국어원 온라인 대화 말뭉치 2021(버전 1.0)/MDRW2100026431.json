      "utterance": [
        {
          "id": "MDRW2100026431.1.1",
          "form": "화제성 끌고 올려고ㅜㅜㅜㅜ그럴거면 데뷔도 시켜주지ㅜㅜㅜㅜ",
          "original_form": "화제성 끌고 올려고ㅜㅜㅜㅜ그럴거면 데뷔도 시켜주지ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026431.1.2",
          "form": "걸플하고 있는거 진짜 양심없어",
          "original_form": "걸플하고 있는거 진짜 양심없어",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026431.1.3",
          "form": "ㅠㅠ진짜",
          "original_form": "ㅠㅠ진짜",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026431.1.4",
          "form": "그거너무싫어",
          "original_form": "그거너무싫어",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026431.1.5",
          "form": "우리나라화제성암전이니까",
          "original_form": "우리나라화제성암전이니까",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026431.1.6",
          "form": "다른나라에서 싸우고있던데",
          "original_form": "다른나라에서 싸우고있던데",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026431.1.7",
          "form": "그러라고 중국, 일본 끌고 온건가",
          "original_form": "그러라고 중국, 일본 끌고 온건가",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026431.1.8",
          "form": "ㅇㅇㅇ그런듯",
          "original_form": "ㅇㅇㅇ그런듯",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026431.1.9",
          "form": "걸플도 중국몽 있는거 같더라",
          "original_form": "걸플도 중국몽 있는거 같더라",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026431.1.10",
          "form": "돈쓰라고",
          "original_form": "돈쓰라고",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026431.1.11",
          "form": "투표 머니 다 중국발인거노리고",
          "original_form": "투표 머니 다 중국발인거노리고",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026431.1.12",
          "form": "들어온거같다던데",
          "original_form": "들어온거같다던데",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026431.1.13",
          "form": "진짜 씨제이는 써바 못하게 법으로 막아야함ㅜㅜ",
          "original_form": "진짜 씨제이는 써바 못하게 법으로 막아야함ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026431.1.14",
          "form": "양심 없어",
          "original_form": "양심 없어",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026431.1.15",
          "form": "ㅠㅠ그니까",
          "original_form": "ㅠㅠ그니까",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026431.1.16",
          "form": "프듀생각하면 아직도 화나",
          "original_form": "프듀생각하면 아직도 화나",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026431.1.17",
          "form": "그렇게사기를 쳐놓고",
          "original_form": "그렇게사기를 쳐놓고",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026431.1.18",
          "form": "....프듀는 진짜 욕두문자야",
          "original_form": "....프듀는 진짜 욕두문자야",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026431.1.19",
          "form": "서바를계속만들어",
          "original_form": "서바를계속만들어",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026431.1.20",
          "form": "양심 없어서 그래",
          "original_form": "양심 없어서 그래",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026431.1.21",
          "form": "만두나팔것이지",
          "original_form": "만두나팔것이지",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026431.1.22",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026431.1.23",
          "form": "그러고 기생충아카데미가서",
          "original_form": "그러고 기생충아카데미가서",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026431.1.24",
          "form": "수상소감하고있더라",
          "original_form": "수상소감하고있더라",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026431.1.25",
          "form": "그 책임자",
          "original_form": "그 책임자",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026431.1.26",
          "form": "그러고도잘먹고잘살듯",
          "original_form": "그러고도잘먹고잘살듯",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026431.1.27",
          "form": "젊은 애들 미래 볼모 잡아서 장난쳐놓고",
          "original_form": "젊은 애들 미래 볼모 잡아서 장난쳐놓고",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026431.1.28",
          "form": "예술한다고 그러는거",
          "original_form": "예술한다고 그러는거",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026431.1.29",
          "form": "ㅎ....  ,",
          "original_form": "ㅎ....  ,",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026431.1.30",
          "form": "진짜 별로야",
          "original_form": "진짜 별로야",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026431.1.31",
          "form": "똑같이돌려받아야할텐데",
          "original_form": "똑같이돌려받아야할텐데",
          "speaker_id": "1",
          "time": "20210901 22:51"
        }
      ]
    }
