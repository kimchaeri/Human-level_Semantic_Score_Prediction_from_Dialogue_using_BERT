        },
        {
          "id": "MDRW2100045403.1.2",
          "form": "보다안보다",
          "original_form": "보다안보다",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045403.1.3",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045403.1.4",
          "form": "본방사수 드라마는 아닌듯",
          "original_form": "본방사수 드라마는 아닌듯",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045403.1.5",
          "form": "ㅋㅋ 내용이 특이했지",
          "original_form": "ㅋㅋ 내용이 특이했지",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045403.1.6",
          "form": "응 퇴마사",
          "original_form": "응 퇴마사",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045403.1.7",
          "form": "정나라 스타일이 좋았다",
          "original_form": "정나라 스타일이 좋았다",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045403.1.8",
          "form": "헤어스타일만",
          "original_form": "헤어스타일만",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045403.1.9",
          "form": "ㅋㅋ 너무 작으니 의상협찬에서 안좋은듯",
          "original_form": "ㅋㅋ 너무 작으니 의상협찬에서 안좋은듯",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045403.1.10",
          "form": "응 너무 크게 입고 나와서",
          "original_form": "응 너무 크게 입고 나와서",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045403.1.11",
          "form": "맞다 저번 드라마에서 그래서 말 많았다",
          "original_form": "맞다 저번 드라마에서 그래서 말 많았다",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045403.1.12",
          "form": "엉 백화정 vip",
          "original_form": "엉 백화정 vip",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045403.1.13",
          "form": "그래 남편이 불륜이고",
          "original_form": "그래 남편이 불륜이고",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045403.1.14",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045403.1.15",
          "form": "드라마에 불륜이 없으면 안되는건가",
          "original_form": "드라마에 불륜이 없으면 안되는건가",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045403.1.16",
          "form": "실제로 불륜이 엄청 많으니",
          "original_form": "실제로 불륜이 엄청 많으니",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045403.1.17",
          "form": "로스쿨 이드라마도 괜찬핬는데",
          "original_form": "로스쿨 이드라마도 괜찬핬는데",
