      "utterance": [
        {
          "id": "MDRW2100040795.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 07:31"
        },
        {
          "id": "MDRW2100040795.1.2",
          "form": "운동하는 거 좋아하진 않아요",
          "original_form": "운동하는 거 좋아하진 않아요",
          "speaker_id": "1",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040795.1.3",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040795.1.4",
          "form": "근데 코로나 땜에 움직임이 줄어드니",
          "original_form": "근데 코로나 땜에 움직임이 줄어드니",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040795.1.5",
          "form": "건강이 안좋아지는 게 느껴져서ㅜ",
          "original_form": "건강이 안좋아지는 게 느껴져서ㅜ",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040795.1.6",
          "form": "살기 위해서 운동 해야 될 것 같아요",
          "original_form": "살기 위해서 운동 해야 될 것 같아요",
          "speaker_id": "1",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040795.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040795.1.8",
          "form": "진짜 살기 위해 운동해요",
          "original_form": "진짜 살기 위해 운동해요",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040795.1.9",
          "form": "예전엔 살기 위해 운동한다는 말 실감 못했는데..",
          "original_form": "예전엔 살기 위해 운동한다는 말 실감 못했는데..",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040795.1.10",
          "form": "저도 살쪄서 빨리 운동해야 될 거 같아요",
          "original_form": "저도 살쪄서 빨리 운동해야 될 거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040795.1.11",
          "form": "저는 요즘 다이어트는 둘째치고 건강 챙기려면 운동이 필요하다 싶어요",
          "original_form": "저는 요즘 다이어트는 둘째치고 건강 챙기려면 운동이 필요하다 싶어요",
          "speaker_id": "2",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040795.1.12",
          "form": "운동해서 건강 챙겨야 빨리 안 죽을 거 같아요",
          "original_form": "운동해서 건강 챙겨야 빨리 안 죽을 거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040795.1.13",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040795.1.14",
          "form": "수영이 좋은데",
          "original_form": "수영이 좋은데",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040795.1.15",
          "form": "수영이 안전하고 살도 잘 빠지는 거 같아요",
          "original_form": "수영이 안전하고 살도 잘 빠지는 거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040795.1.16",
          "form": "저희동네 수영장은 국공립시설이라",
          "original_form": "저희동네 수영장은 국공립시설이라",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040795.1.17",
          "form": "코로나 때 다 됐네요",
          "original_form": "코로나 때 다 됐네요",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040795.1.18",
          "form": "코로나 4단계에는 계속 휴관이예요",
          "original_form": "코로나 4단계에는 계속 휴관이예요",
          "speaker_id": "2",
          "time": "20210905 07:49"
        }
