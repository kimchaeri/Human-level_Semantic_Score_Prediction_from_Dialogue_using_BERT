      "utterance": [
        {
          "id": "MDRW2100025425.1.1",
          "form": "안녕하세요! 아침드셨나요?",
          "original_form": "안녕하세요! 아침드셨나요?",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025425.1.2",
          "form": "아니요! 아침은 안먹었어요!  점심시간이 좀 일러서 애매하더라고요",
          "original_form": "아니요! 아침은 안먹었어요!  점심시간이 좀 일러서 애매하더라고요",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025425.1.3",
          "form": "아점으로 드시겠네요! 메뉴정하셨엉：?",
          "original_form": "아점으로 드시겠네요! 메뉴정하셨엉：?",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025425.1.4",
          "form": "저희는 구내식당이라 정해져있어요!!",
          "original_form": "저희는 구내식당이라 정해져있어요!!",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025425.1.5",
          "form": "뭐 드실 예정이세요?",
          "original_form": "뭐 드실 예정이세요?",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025425.1.6",
          "form": "아! 근무중이시군요! 전 오늘 연차라서 집에서 토스트나 해먹을까해요!",
          "original_form": "아! 근무중이시군요! 전 오늘 연차라서 집에서 토스트나 해먹을까해요!",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025425.1.7",
          "form": "와!!!!!토스트",
          "original_form": "와!!!!!토스트",
          "speaker_id": "2",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025425.1.8",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025425.1.9",
          "form": "어떤 토스트 드실건가요",
          "original_form": "어떤 토스트 드실건가요",
          "speaker_id": "2",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025425.1.10",
          "form": "식빵굽고 치즈랑 잼 바를려고요!!! 까망베르",
          "original_form": "식빵굽고 치즈랑 잼 바를려고요!!! 까망베르",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025425.1.11",
          "form": "어떤 토스트 좋아하세요?",
          "original_form": "어떤 토스트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025425.1.12",
          "form": "꺅 존맛이겠네요",
          "original_form": "꺅 존맛이겠네요",
          "speaker_id": "2",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025425.1.13",
          "form": "저~ 토스트면 다 좋아요",
          "original_form": "저~ 토스트면 다 좋아요",
          "speaker_id": "2",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025425.1.14",
          "form": "프렌치토스트도 좋고 햄치즈도좋고",
          "original_form": "프렌치토스트도 좋고 햄치즈도좋고",
          "speaker_id": "2",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025425.1.15",
          "form": "저도 이삭도 좋고 트럭토스트도 좋고 무한흡입가능해요ㅋㅋ",
          "original_form": "저도 이삭도 좋고 트럭토스트도 좋고 무한흡입가능해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025425.1.16",
          "form": "근무중에 간식도 즐기시나요?",
          "original_form": "근무중에 간식도 즐기시나요?",
          "speaker_id": "1",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025425.1.17",
          "form": "헉헉 이삭토스트 최애에요",
          "original_form": "헉헉 이삭토스트 최애에요",
          "speaker_id": "2",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025425.1.18",
          "form": "네네 간식 먹어요 ㅎ 근데 거의 과자 이런종류",
          "original_form": "네네 간식 먹어요 ㅎ 근데 거의 과자 이런종류",
          "speaker_id": "2",
          "time": "20210901 11:22"
        },
        {
          "id": "MDRW2100025425.1.19",
          "form": "ㅋㅋ 저도 너무 먹어서 회사지하에 마카롱집이 있어요",
          "original_form": "ㅋㅋ 저도 너무 먹어서 회사지하에 마카롱집이 있어요",
          "speaker_id": "1",
          "time": "20210901 11:23"
        },
        {
          "id": "MDRW2100025425.1.20",
          "form": "와 좋은회사네요",
          "original_form": "와 좋은회사네요",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025425.1.21",
          "form": "저희 회사는 뒤에는 산이고 앞에는 논..이라ㅠ",
          "original_form": "저희 회사는 뒤에는 산이고 앞에는 논..이라ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025425.1.22",
          "form": "먹고싶어도 못사먹어요ㅠ",
          "original_form": "먹고싶어도 못사먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025425.1.23",
          "form": "ㅜㅠ그래도 구내식당이 있으시니!!! 전 도시락싸다녀요ㅋㅋ 야근할때만 배달음식 먹고요",
          "original_form": "ㅜㅠ그래도 구내식당이 있으시니!!! 전 도시락싸다녀요ㅋㅋ 야근할때만 배달음식 먹고요",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025425.1.24",
          "form": "카페같은거 회사안에 있죠?ㅜ",
          "original_form": "카페같은거 회사안에 있죠?ㅜ",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025425.1.25",
          "form": "없어요 없어ㅠ카페라니 흑흑",
          "original_form": "없어요 없어ㅠ카페라니 흑흑",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025425.1.26",
          "form": "없어요 없어ㅠ카페라니 흑흑",
          "original_form": "없어요 없어ㅠ카페라니 흑흑",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025425.1.27",
          "form": "와 도시락싸시다니 대단하시다",
          "original_form": "와 도시락싸시다니 대단하시다",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025425.1.28",
          "form": "부지런하지 않는이상 힘들지 않아요?",
          "original_form": "부지런하지 않는이상 힘들지 않아요?",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025425.1.29",
          "form": "악 회사에서 먹는 커피는 생명순데여ㅜㅠ 그냥 달걀 아님 과일 아님 아침에 먹던거 그냥 가져가요ㅋㅋ",
          "original_form": "악 회사에서 먹는 커피는 생명순데여ㅜㅠ 그냥 달걀 아님 과일 아님 아침에 먹던거 그냥 가져가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025425.1.30",
          "form": "카누로 버텨봅니다",
          "original_form": "카누로 버텨봅니다",
          "speaker_id": "2",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025425.1.31",
          "form": "그래도 도시락 챙긴다는게 대단해요",
          "original_form": "그래도 도시락 챙긴다는게 대단해요",
          "speaker_id": "2",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025425.1.32",
          "form": "저는 진짜 귀찮아서 빵사올듯요",
          "original_form": "저는 진짜 귀찮아서 빵사올듯요",
          "speaker_id": "2",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025425.1.33",
          "form": "ㅋㅋ저도 빵이랑 라면 냉동볶음밥 애용해요ㅋㅋ",
          "original_form": "ㅋㅋ저도 빵이랑 라면 냉동볶음밥 애용해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025425.1.34",
          "form": "카페있어도 카누를 제일 많이 먹고요!!!",
          "original_form": "카페있어도 카누를 제일 많이 먹고요!!!",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025425.1.35",
          "form": "ㅋㅋ맛있죠 요즘 냉동식품 다양해서 좋은거같아요",
          "original_form": "ㅋㅋ맛있죠 요즘 냉동식품 다양해서 좋은거같아요",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025425.1.36",
          "form": "전자레인지는 있나요?",
          "original_form": "전자레인지는 있나요?",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025425.1.37",
          "form": "카누 가성비가 좋아요ㅋㅋㅋㅋ",
          "original_form": "카누 가성비가 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025425.1.38",
          "form": "네 전자레인지 너무 바빠요ㅎㅎ 전엔 에프도 있었는데 과로로 사망하여ㅜㅠ 컵라면과 냉동밥 주로ㅜㅎㅎ",
          "original_form": "네 전자레인지 너무 바빠요ㅎㅎ 전엔 에프도 있었는데 과로로 사망하여ㅜㅠ 컵라면과 냉동밥 주로ㅜㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100025425.1.39",
          "form": "와 에프 ㅠ아 사망ㅠㅠ 근데 그것도 매일먹으면 물릴거같아요",
          "original_form": "와 에프 ㅠ아 사망ㅠㅠ 근데 그것도 매일먹으면 물릴거같아요",
          "speaker_id": "2",
          "time": "20210901 13:45"
        }
      ]
