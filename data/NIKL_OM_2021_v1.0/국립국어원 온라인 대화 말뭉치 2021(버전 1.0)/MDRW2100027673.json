        {
          "id": "MDRW2100027673.1.1",
          "form": "뉘른베르크 햄버거 기억나니",
          "original_form": "뉘른베르크 햄버거 기억나니",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027673.1.2",
          "form": "수제라 맛있었어",
          "original_form": "수제라 맛있었어",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027673.1.3",
          "form": "포테이토랑 맥주랑",
          "original_form": "포테이토랑 맥주랑",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027673.1.4",
          "form": "라떼도 풍미가 좋더라",
          "original_form": "라떼도 풍미가 좋더라",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027673.1.5",
          "form": "카레집이 저렴했어",
          "original_form": "카레집이 저렴했어",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027673.1.6",
          "form": "유럽보단 동남아가 좋지",
          "original_form": "유럽보단 동남아가 좋지",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027673.1.7",
          "form": "익숙하니까",
          "original_form": "익숙하니까",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027673.1.8",
          "form": "산타마을 봤으면 좋으련만",
          "original_form": "산타마을 봤으면 좋으련만",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027673.1.9",
          "form": "겨울에 독일에 또 가자",
          "original_form": "겨울에 독일에 또 가자",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027673.1.10",
          "form": "다음기회가 있을까",
          "original_form": "다음기회가 있을까",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027673.1.11",
          "form": "쌀국수식당도 기억나",
          "original_form": "쌀국수식당도 기억나",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027673.1.12",
          "form": "뉘른베르크에 맛집많았어",
          "original_form": "뉘른베르크에 맛집많았어",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027673.1.13",
          "form": "쇼핑하기에도 좋았지",
          "original_form": "쇼핑하기에도 좋았지",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027673.1.14",
          "form": "그랬드랬지",
          "original_form": "그랬드랬지",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027673.1.15",
          "form": "나 옷 샀어 스웨터랑 조끼",
          "original_form": "나 옷 샀어 스웨터랑 조끼",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027673.1.16",
          "form": "그땐 추웠으니까",
          "original_form": "그땐 추웠으니까",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027673.1.17",
          "form": "따끈한 옷이 필요했어",
          "original_form": "따끈한 옷이 필요했어",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027673.1.18",
          "form": "기념품이 없어",
