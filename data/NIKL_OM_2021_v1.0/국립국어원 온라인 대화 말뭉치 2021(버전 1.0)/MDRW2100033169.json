      "utterance": [
        {
          "id": "MDRW2100033169.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033169.1.2",
          "form": "좋아하는 편이에용",
          "original_form": "좋아하는 편이에용",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100033169.1.3",
          "form": "님은용?",
          "original_form": "님은용?",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100033169.1.4",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033169.1.5",
          "form": "네 저도 좋아해요",
          "original_form": "네 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033169.1.6",
          "form": "가보고 싶은 곳 있으신가요",
          "original_form": "가보고 싶은 곳 있으신가요",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033169.1.7",
          "form": "저는 코로나 나아지면 캐나다요",
          "original_form": "저는 코로나 나아지면 캐나다요",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100033169.1.8",
          "form": "헉 캐나다",
          "original_form": "헉 캐나다",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033169.1.9",
          "form": "폭포밖에 생각이 안 나네요",
          "original_form": "폭포밖에 생각이 안 나네요",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033169.1.10",
          "form": "폭포랑 단풍이 유명하죠....",
          "original_form": "폭포랑 단풍이 유명하죠....",
          "speaker_id": "2",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100033169.1.11",
          "form": "남은 어디를 여행 가보고 싶으신가용",
          "original_form": "남은 어디를 여행 가보고 싶으신가용",
          "speaker_id": "2",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100033169.1.12",
          "form": "저는 터키요!",
          "original_form": "저는 터키요!",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100033169.1.13",
          "form": "우와 멋지네요",
          "original_form": "우와 멋지네요",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100033169.1.14",
          "form": "추운 나라랑 따뜻한 나라~",
          "original_form": "추운 나라랑 따뜻한 나라~",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100033169.1.15",
          "form": "사실... 먹거리가 궁금해서 가보고 싶어요 ㅋㅋㅋ",
          "original_form": "사실... 먹거리가 궁금해서 가보고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100033169.1.16",
          "form": "맞아요 먹을거 중요하죠",
          "original_form": "맞아요 먹을거 중요하죠",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100033169.1.17",
          "form": "달다구리가 많다고 하더라고요",
          "original_form": "달다구리가 많다고 하더라고요",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100033169.1.18",
          "form": "그럼...디저트같은 음식이 맛있겠네용",
          "original_form": "그럼...디저트같은 음식이 맛있겠네용",
          "speaker_id": "2",
          "time": "20210906 19:04"
        },
        {
          "id": "MDRW2100033169.1.19",
          "form": "터키쉬 딜라이트 아시나용? 터키 거는 되게 맛있대요",
          "original_form": "터키쉬 딜라이트 아시나용? 터키 거는 되게 맛있대요",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100033169.1.20",
          "form": "아는데 먹어본적은 없어서... 터키 맛이 더 궁금해지네요",
          "original_form": "아는데 먹어본적은 없어서... 터키 맛이 더 궁금해지네요",
          "speaker_id": "2",
          "time": "20210906 19:09"
        }
      ]
