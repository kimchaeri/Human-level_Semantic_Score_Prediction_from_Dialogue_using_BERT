      "utterance": [
        {
          "id": "MDRW2100027152.1.1",
          "form": "제대로된 연애를 하고싶어요...",
          "original_form": "제대로된 연애를 하고싶어요...",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100027152.1.2",
          "form": "10년연하남과 천국과지옥을 왔다리갔다리하고있어요",
          "original_form": "10년연하남과 천국과지옥을 왔다리갔다리하고있어요",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100027152.1.3",
          "form": "10년 연하?",
          "original_form": "10년 연하?",
          "speaker_id": "1",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100027152.1.4",
          "form": "열살 어린?",
          "original_form": "열살 어린?",
          "speaker_id": "1",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100027152.1.5",
          "form": "10년동안 연하와?",
          "original_form": "10년동안 연하와?",
          "speaker_id": "1",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100027152.1.6",
          "form": "10년된게아니라 20대초반인 남자애에요ㅋㅋㅋ",
          "original_form": "10년된게아니라 20대초반인 남자애에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027152.1.7",
          "form": "저랑 나이차가 좀 나요",
          "original_form": "저랑 나이차가 좀 나요",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027152.1.8",
          "form": "와 능력자시네요!!!!!",
          "original_form": "와 능력자시네요!!!!!",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027152.1.9",
          "form": "근데 걔랑 천국과지옥을 왔다리갔다리하고있어서....",
          "original_form": "근데 걔랑 천국과지옥을 왔다리갔다리하고있어서....",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027152.1.10",
          "form": "안그래도 고민이 많아요ㅜㅜ",
          "original_form": "안그래도 고민이 많아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027152.1.11",
          "form": "달달하기도하다가",
          "original_form": "달달하기도하다가",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027152.1.12",
          "form": "싸우기도하고",
          "original_form": "싸우기도하고",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027152.1.13",
          "form": "ㅡㅅㅡ",
          "original_form": "ㅡㅅㅡ",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027152.1.14",
          "form": "명언이 있는데",
          "original_form": "명언이 있는데",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027152.1.15",
          "form": "모든건 그대의 이익이 되는 쪽으로",
          "original_form": "모든건 그대의 이익이 되는 쪽으로",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027152.1.16",
          "form": "본인이 행복한 선택 하세요",
          "original_form": "본인이 행복한 선택 하세요",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027152.1.17",
          "form": "어느 방향이든 ㅋㅋ",
          "original_form": "어느 방향이든 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027152.1.18",
          "form": "우와 딱 봐도 좋은얘기같은데...",
          "original_form": "우와 딱 봐도 좋은얘기같은데...",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027152.1.19",
          "form": "어떡할지ㅋㅋㄱㄱ",
          "original_form": "어떡할지ㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027152.1.20",
          "form": "걔랑 관계를 계속 유지 할지 말지....",
          "original_form": "걔랑 관계를 계속 유지 할지 말지....",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027152.1.21",
          "form": "ㅋㅋ뭐 본인이 행복한 방향으로",
          "original_form": "ㅋㅋ뭐 본인이 행복한 방향으로",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027152.1.22",
          "form": "근데 사실 남녀관계가 참 어려운거라서",
          "original_form": "근데 사실 남녀관계가 참 어려운거라서",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027152.1.23",
          "form": "그건 그래요....돌고도는",
          "original_form": "그건 그래요....돌고도는",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027152.1.24",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027152.1.25",
          "form": "유지를 하자나 신경이 계속 쓰이고 연락하는방식이",
          "original_form": "유지를 하자나 신경이 계속 쓰이고 연락하는방식이",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027152.1.26",
          "form": "끊자니 이것두 신경쓰이구",
          "original_form": "끊자니 이것두 신경쓰이구",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027152.1.27",
          "form": "복잡복잡하시군요",
          "original_form": "복잡복잡하시군요",
          "speaker_id": "1",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100027152.1.28",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027152.1.29",
          "form": "걔도 카톡 잘안하면서 간만에 저한테 보낸거 내가바로안보면",
          "original_form": "걔도 카톡 잘안하면서 간만에 저한테 보낸거 내가바로안보면",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027152.1.30",
          "form": "톡안보다고 머라그러고....",
          "original_form": "톡안보다고 머라그러고....",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027152.1.31",
          "form": "답장올때까지 기다렸데요",
          "original_form": "답장올때까지 기다렸데요",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027152.1.32",
          "form": "나도 걔한테 톡보내면 잘안보거나 답장도 잘안하면서....",
          "original_form": "나도 걔한테 톡보내면 잘안보거나 답장도 잘안하면서....",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027152.1.33",
          "form": "ㅋㅋㅋ 아니 연인끼리 왜케 연락을 잘 안해요",
          "original_form": "ㅋㅋㅋ 아니 연인끼리 왜케 연락을 잘 안해요",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027152.1.34",
          "form": "정식으로 사귀고있는게 아니고 좋아하는정도....",
          "original_form": "정식으로 사귀고있는게 아니고 좋아하는정도....",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027152.1.35",
