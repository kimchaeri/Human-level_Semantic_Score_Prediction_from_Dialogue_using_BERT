      "utterance": [
        {
          "id": "MDRW2100039057.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100039057.1.2",
          "form": "저 딱 작년에 졸업했어요!",
          "original_form": "저 딱 작년에 졸업했어요!",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039057.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 08:51"
        },
        {
          "id": "MDRW2100039057.1.4",
          "form": "저는 재학중이에요",
          "original_form": "저는 재학중이에요",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100039057.1.5",
          "form": "지금도 수업 듣고온 ㅜㅜ",
          "original_form": "지금도 수업 듣고온 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 11:50"
        },
        {
          "id": "MDRW2100039057.1.6",
          "form": "헉 지금 대면하나요?",
          "original_form": "헉 지금 대면하나요?",
          "speaker_id": "2",
          "time": "20210903 12:26"
        },
        {
          "id": "MDRW2100039057.1.7",
          "form": "다담주부터 대면할고같아요",
          "original_form": "다담주부터 대면할고같아요",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100039057.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100039057.1.9",
          "form": "실강으로 듣고잇어요",
          "original_form": "실강으로 듣고잇어요",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100039057.1.10",
          "form": "헉스... 학교나가기 귀찮은데말이에욬ㅋㅋ",
          "original_form": "헉스... 학교나가기 귀찮은데말이에욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039057.1.11",
          "form": "그러개요.. 수업도 많이들어서 ㅠㅠ",
          "original_form": "그러개요.. 수업도 많이들어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039057.1.12",
          "form": "학교가는 시간이 잴 아까버요 ㅜ",
          "original_form": "학교가는 시간이 잴 아까버요 ㅜ",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039057.1.13",
          "form": "그쵸ㅜㅜ 길에 버리는 시감이 많아서",
          "original_form": "그쵸ㅜㅜ 길에 버리는 시감이 많아서",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039057.1.14",
          "form": "맞아요.. 밥값도 많이 나오고...",
          "original_form": "맞아요.. 밥값도 많이 나오고...",
          "speaker_id": "1",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100039057.1.15",
          "form": "대면 안하고 나니 알겠도라고요",
          "original_form": "대면 안하고 나니 알겠도라고요",
          "speaker_id": "1",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100039057.1.16",
          "form": "아 밥값 인정합니다...",
          "original_form": "아 밥값 인정합니다...",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100039057.1.17",
          "form": "근데 전 학교시설 이용 못하는것도 쬐끔 아쉽더라구요",
          "original_form": "근데 전 학교시설 이용 못하는것도 쬐끔 아쉽더라구요",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100039057.1.18",
          "form": "학교에서 전자기기 다 충전해야하는뎈ㅋㅋㅋㅋㅋㅋ",
          "original_form": "학교에서 전자기기 다 충전해야하는뎈ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100039057.1.19",
          "form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅌㅋ",
          "original_form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅌㅋ",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100039057.1.20",
          "form": "저도 대면할땐 맨날 다 충전하고왔어요",
          "original_form": "저도 대면할땐 맨날 다 충전하고왔어요",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100039057.1.21",
          "form": "등록금 그렇개라도 뽑아야죠 ㅠ",
          "original_form": "등록금 그렇개라도 뽑아야죠 ㅠ",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100039057.1.22",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 소확횡",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 소확횡",
          "speaker_id": "2",
          "time": "20210905 11:30"
        },
        {
          "id": "MDRW2100039057.1.23",
          "form": "은근 학교가 뽑라먹을게 많은데",
          "original_form": "은근 학교가 뽑라먹을게 많은데",
          "speaker_id": "2",
          "time": "20210905 11:30"
        },
        {
          "id": "MDRW2100039057.1.24",
          "form": "너무 아쉬워요ㅠ",
          "original_form": "너무 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210905 11:30"
        },
        {
          "id": "MDRW2100039057.1.25",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100039057.1.26",
          "form": "요즘은 코로나때문애 또",
          "original_form": "요즘은 코로나때문애 또",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100039057.1.27",
          "form": "학교를 잘안가서 ㅠ",
          "original_form": "학교를 잘안가서 ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100039057.1.28",
          "form": "그쵸ㅜㅜ 빨리 대면하는게 좋은데",
          "original_form": "그쵸ㅜㅜ 빨리 대면하는게 좋은데",
          "speaker_id": "2",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100039057.1.29",
          "form": "학교같은 느낌이 없다더라구요",
          "original_form": "학교같은 느낌이 없다더라구요",
