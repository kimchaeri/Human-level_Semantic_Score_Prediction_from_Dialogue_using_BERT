      "utterance": [
        {
          "id": "MDRW2100014061.1.1",
          "form": "연예인같은 느낌 확 나긴 하죠 그분들이",
          "original_form": "연예인같은 느낌 확 나긴 하죠 그분들이",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014061.1.2",
          "form": "제니도 좋아하세요?",
          "original_form": "제니도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014061.1.3",
          "form": "제니는 예쁘죠. 인형같아요",
          "original_form": "제니는 예쁘죠. 인형같아요",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014061.1.4",
          "form": "저는 블핑에서는 지수가 제일 좋더라구요",
          "original_form": "저는 블핑에서는 지수가 제일 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014061.1.5",
          "form": "멤버중에 누가 제일 좋아요?",
          "original_form": "멤버중에 누가 제일 좋아요?",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014061.1.6",
          "form": "제니",
          "original_form": "제니",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014061.1.7",
          "form": "지수도 좋아요^^",
          "original_form": "지수도 좋아요^^",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100014061.1.8",
          "form": "그럼 배우는 누구 좋아하세요?",
          "original_form": "그럼 배우는 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014061.1.9",
          "form": "얼굴상이라던가",
          "original_form": "얼굴상이라던가",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014061.1.10",
          "form": "정혜인 . 서현진 같은 느낌",
          "original_form": "정혜인 . 서현진 같은 느낌",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014061.1.11",
          "form": "오... 서현진 좋죠",
          "original_form": "오... 서현진 좋죠",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014061.1.12",
          "form": "장나라도 예쁜것 같아요",
          "original_form": "장나라도 예쁜것 같아요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014061.1.13",
          "form": "전 신민아 좋아해요",
          "original_form": "전 신민아 좋아해요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014061.1.14",
          "form": "장나라는 진짜 불로초 드신듯",
          "original_form": "장나라는 진짜 불로초 드신듯",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014061.1.15",
          "form": "그쵸. 뱀파이어 같아요",
          "original_form": "그쵸. 뱀파이어 같아요",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014061.1.16",
          "form": "신민아도 예뻐요. 전지현도",
          "original_form": "신민아도 예뻐요. 전지현도",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014061.1.17",
          "form": "여우상 고양이상 좋아해요 저도",
          "original_form": "여우상 고양이상 좋아해요 저도",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014061.1.18",
          "form": "얼굴형이 예쁜것 같아요",
          "original_form": "얼굴형이 예쁜것 같아요",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014061.1.19",
          "form": "눈이 위로 착 올라간게 매력적이죠",
          "original_form": "눈이 위로 착 올라간게 매력적이죠",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014061.1.20",
          "form": "연예인들은 거의 다 예쁜것 같아요. 김혜수도 그렇고",
          "original_form": "연예인들은 거의 다 예쁜것 같아요. 김혜수도 그렇고",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014061.1.21",
          "form": "진짜 연예인같죠 김혜수도",
          "original_form": "진짜 연예인같죠 김혜수도",
