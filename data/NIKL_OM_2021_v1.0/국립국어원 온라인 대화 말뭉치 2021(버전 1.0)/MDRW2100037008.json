      "utterance": [
        {
          "id": "MDRW2100037008.1.1",
          "form": "저는 다이어트를 위해 스파게트를 먹으려구여",
          "original_form": "저는 다이어트를 위해 스파게트를 먹으려구여",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100037008.1.2",
          "form": "위염있으시면 양배추환드셔보셨어요?",
          "original_form": "위염있으시면 양배추환드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100037008.1.3",
          "form": "스파게티? 다이어트용으로요?",
          "original_form": "스파게티? 다이어트용으로요?",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100037008.1.4",
          "form": "네넹 양배추환 먹고 있어요! 아침에 갈아서도 마시구용",
          "original_form": "네넹 양배추환 먹고 있어요! 아침에 갈아서도 마시구용",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100037008.1.5",
          "form": "오오 어떤제품드세요",
          "original_form": "오오 어떤제품드세요",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100037008.1.6",
          "form": "네! 듀럼밀이 밀가루보다 칼로리도 적고 더 좋다더라구요",
          "original_form": "네! 듀럼밀이 밀가루보다 칼로리도 적고 더 좋다더라구요",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100037008.1.7",
          "form": "외국에서는 스파게티가 다이어트 음식이래요!",
          "original_form": "외국에서는 스파게티가 다이어트 음식이래요!",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100037008.1.8",
          "form": "카베진이용!",
          "original_form": "카베진이용!",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100037008.1.9",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100037008.1.10",
          "form": "오오 몰랐네요 면을 다른걸쓰시능구나",
          "original_form": "오오 몰랐네요 면을 다른걸쓰시능구나",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100037008.1.11",
          "form": "전 지금사먹어야될거같아서ㅜㅜ추천받긴했는데",
          "original_form": "전 지금사먹어야될거같아서ㅜㅜ추천받긴했는데",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100037008.1.12",
          "form": "효과 어때요?",
          "original_form": "효과 어때요?",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100037008.1.13",
          "form": "저는 괜찮았어요!",
          "original_form": "저는 괜찮았어요!",
          "speaker_id": "2",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100037008.1.14",
          "form": "속이 너무 쓰릴때 먹었는데 진정되더라구요",
          "original_form": "속이 너무 쓰릴때 먹었는데 진정되더라구요",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037008.1.15",
          "form": "오 한번구매해봐야겠네요",
          "original_form": "오 한번구매해봐야겠네요",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037008.1.16",
          "form": "다이어트스파게티 재료는뭐넣으시나요",
          "original_form": "다이어트스파게티 재료는뭐넣으시나요",
          "speaker_id": "1",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100037008.1.17",
          "form": "새우요!! 버섯하구용",
          "original_form": "새우요!! 버섯하구용",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100037008.1.18",
          "form": "마늘!",
          "original_form": "마늘!",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100037008.1.19",
          "form": "저것만 들어가도 매일 먹을 수 있을정도로 스파게티가 좋네요ㅋㅋㅋㅋ",
          "original_form": "저것만 들어가도 매일 먹을 수 있을정도로 스파게티가 좋네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100037008.1.20",
          "form": "오오 오일파스타인가여",
          "original_form": "오오 오일파스타인가여",
          "speaker_id": "1",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100037008.1.21",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100037008.1.22",
          "form": "크림 넣어도 토마토도 다 좋아욤ㅋㅋ",
          "original_form": "크림 넣어도 토마토도 다 좋아욤ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100037008.1.23",
          "form": "잘어울리고",
          "original_form": "잘어울리고",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100037008.1.24",
          "form": "맞아요 저도 소스 다좋아해요",
          "original_form": "맞아요 저도 소스 다좋아해요",
          "speaker_id": "1",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100037008.1.25",
          "form": "점심이니까 저정도는 괜찮겠지 하면서요ㅋㅋㅋ",
          "original_form": "점심이니까 저정도는 괜찮겠지 하면서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100037008.1.26",
          "form": "유기농도 많으니까용!",
          "original_form": "유기농도 많으니까용!",
          "speaker_id": "2",
          "time": "20210831 17:22"
        }
      ]
    }
