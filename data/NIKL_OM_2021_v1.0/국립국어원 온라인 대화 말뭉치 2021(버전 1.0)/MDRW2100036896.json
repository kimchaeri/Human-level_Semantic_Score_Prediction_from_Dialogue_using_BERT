      "utterance": [
        {
          "id": "MDRW2100036896.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100036896.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036896.1.3",
          "form": "오늘은 종일 비가 오네요",
          "original_form": "오늘은 종일 비가 오네요",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036896.1.4",
          "form": "여기는 너무 덥고 쨍쨍해요",
          "original_form": "여기는 너무 덥고 쨍쨍해요",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036896.1.5",
          "form": "거기비가 많이오나요",
          "original_form": "거기비가 많이오나요",
          "speaker_id": "1",
          "time": "20210831 14:43"
        },
        {
          "id": "MDRW2100036896.1.6",
          "form": "헐 그렇군요! 네 여기는 아침부터 비가 계속와요",
          "original_form": "헐 그렇군요! 네 여기는 아침부터 비가 계속와요",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036896.1.7",
          "form": "아침엔 방울방울이었는데 지금은 꽤 와여",
          "original_form": "아침엔 방울방울이었는데 지금은 꽤 와여",
          "speaker_id": "2",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100036896.1.8",
          "form": "와아 이렇게 다르군여",
          "original_form": "와아 이렇게 다르군여",
          "speaker_id": "1",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100036896.1.9",
          "form": "여긴 한여름입니다ㅜ",
          "original_form": "여긴 한여름입니다ㅜ",
          "speaker_id": "1",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100036896.1.10",
          "form": "덥지는않으세요?",
          "original_form": "덥지는않으세요?",
          "speaker_id": "1",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100036896.1.11",
          "form": "네 비가오니까 엄청 쌀쌀해요 바람도 많이불고요!",
          "original_form": "네 비가오니까 엄청 쌀쌀해요 바람도 많이불고요!",
          "speaker_id": "2",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100036896.1.12",
          "form": "완전다르네요 여기는 푹푹쪄요ㅜ",
          "original_form": "완전다르네요 여기는 푹푹쪄요ㅜ",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036896.1.13",
          "form": "옷은 어떻게입으셨오요",
          "original_form": "옷은 어떻게입으셨오요",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036896.1.14",
          "form": "저는 오늘 재택근무 중이라서 잠옷이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 오늘 재택근무 중이라서 잠옷이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100036896.1.15",
          "form": "창문열어놨더니 쌀쌀해서 후드걸쳐야할것같아요 ㅋㅋㅋ",
          "original_form": "창문열어놨더니 쌀쌀해서 후드걸쳐야할것같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100036896.1.16",
          "form": "너무신기해요 이렇게나 날씨가 다르다니",
          "original_form": "너무신기해요 이렇게나 날씨가 다르다니",
          "speaker_id": "2",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100036896.1.17",
          "form": "ㅋㅋㅋㅋ아하 저도 그래서 전 나시에 반바지",
          "original_form": "ㅋㅋㅋㅋ아하 저도 그래서 전 나시에 반바지",
          "speaker_id": "1",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100036896.1.18",
          "form": "진짜요 넘신기해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜요 넘신기해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100036896.1.19",
          "form": "ㅋㅋ내일도 비예보없어요? 여긴 내일까지 비온대요!!",
          "original_form": "ㅋㅋ내일도 비예보없어요? 여긴 내일까지 비온대요!!",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100036896.1.20",
          "form": "비가온다는 말이있었는데 오늘안왔거든여ㅋㅋㅋㅋ",
          "original_form": "비가온다는 말이있었는데 오늘안왔거든여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100036896.1.21",
          "form": "다시보니 해로바꼈드라구여",
          "original_form": "다시보니 해로바꼈드라구여",
          "speaker_id": "1",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100036896.1.22",
          "form": "내일도 안올거같아요",
          "original_form": "내일도 안올거같아요",
          "speaker_id": "1",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100036896.1.23",
          "form": "헐 그렇군요!! 여기는 내일 오전까지 비래요 ㅋㅋㅋ",
          "original_form": "헐 그렇군요!! 여기는 내일 오전까지 비래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100036896.1.24",
          "form": "ㅋㅋㅋㅋㅋ마지막 비일려나요",
          "original_form": "ㅋㅋㅋㅋㅋ마지막 비일려나요",
          "speaker_id": "1",
          "time": "20210831 15:07"
        }
      ]
    }
