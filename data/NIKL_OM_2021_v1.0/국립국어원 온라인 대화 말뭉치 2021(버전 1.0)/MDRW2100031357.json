      "utterance": [
        {
          "id": "MDRW2100031357.1.1",
          "form": "투랑갈릴라 심포니 이거도 좋아요",
          "original_form": "투랑갈릴라 심포니 이거도 좋아요",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031357.1.2",
          "form": "첨에 들으면 이상한데 자꾸들으면 괜찮아요 추천드리면 들으실거같아서 얘기해봅니다 ㅎㅎㅎ",
          "original_form": "첨에 들으면 이상한데 자꾸들으면 괜찮아요 추천드리면 들으실거같아서 얘기해봅니다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031357.1.3",
          "form": "새의 카탈로그는 그런데",
          "original_form": "새의 카탈로그는 그런데",
          "speaker_id": "2",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100031357.1.4",
          "form": "리듬?이 새들이 지자귀는 느낌같았어요",
          "original_form": "리듬?이 새들이 지자귀는 느낌같았어요",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031357.1.5",
          "form": "아 오타",
          "original_form": "아 오타",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031357.1.6",
          "form": "클래식 정말 좋아하시나봐요 ㅎㄹ",
          "original_form": "클래식 정말 좋아하시나봐요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031357.1.7",
          "form": "넹 그러실거에요",
          "original_form": "넹 그러실거에요",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031357.1.8",
          "form": "네 ㅎㅎ 메시앙 다른 곡에는",
          "original_form": "네 ㅎㅎ 메시앙 다른 곡에는",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100031357.1.9",
          "form": "새소리가 숨겨진거처럼 찾아보는 재미가 있어요",
          "original_form": "새소리가 숨겨진거처럼 찾아보는 재미가 있어요",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031357.1.10",
          "form": "작곡가가 자기작품에 넣기 좋아하는 소재는",
          "original_form": "작곡가가 자기작품에 넣기 좋아하는 소재는",
          "speaker_id": "2",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100031357.1.11",
          "form": "팬들이 다른 작품에서도 유의깊게 찾고는 하더라구요 ㅎㅎ",
          "original_form": "팬들이 다른 작품에서도 유의깊게 찾고는 하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100031357.1.12",
          "form": "그렇군요 저는 새를 좋아해서",
          "original_form": "그렇군요 저는 새를 좋아해서",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031357.1.13",
          "form": "일일이 채보해서 음악에 반영했다는 스토리가",
          "original_form": "일일이 채보해서 음악에 반영했다는 스토리가",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031357.1.14",
          "form": "인상깊었어요",
          "original_form": "인상깊었어요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031357.1.15",
          "form": "책이나 영화에 나올거같은 이야기네요",
          "original_form": "책이나 영화에 나올거같은 이야기네요",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100031357.1.16",
          "form": "네 저도그렇게 느꼈어요",
          "original_form": "네 저도그렇게 느꼈어요",
          "speaker_id": "1",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031357.1.17",
          "form": "검색했을때도 좋은 이야기도 많이나오고 생전 평판도 좋으셨다는데",
          "original_form": "검색했을때도 좋은 이야기도 많이나오고 생전 평판도 좋으셨다는데",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031357.1.18",
          "form": "멋있는분같아요",
          "original_form": "멋있는분같아요",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031357.1.19",
          "form": "조금 신기한느낌이네요 ㅎㅎ",
          "original_form": "조금 신기한느낌이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031357.1.20",
          "form": "그죠 ㅎㅎ",
          "original_form": "그죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031357.1.21",
          "form": "찾아보면 그분께 배운 우리나라 사람 작곡가가 있더라구요",
          "original_form": "찾아보면 그분께 배운 우리나라 사람 작곡가가 있더라구요",
          "speaker_id": "1",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031357.1.22",
          "form": "신기했어요",
          "original_form": "신기했어요",
          "speaker_id": "1",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031357.1.23",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "2",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031357.1.24",
          "form": "저는 검색했을때",
          "original_form": "저는 검색했을때",
          "speaker_id": "2",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031357.1.25",
          "form": "한국에 알려질때 어떤 피아니스트분이 계기셨다고해서",
          "original_form": "한국에 알려질때 어떤 피아니스트분이 계기셨다고해서",
          "speaker_id": "2",
          "time": "20210904 20:58"
        },
        {
          "id": "MDRW2100031357.1.26",
          "form": "그래서 알고계시는걸까? 생각했는데",
          "original_form": "그래서 알고계시는걸까? 생각했는데",
          "speaker_id": "2",
          "time": "20210904 20:58"
        },
        {
          "id": "MDRW2100031357.1.27",
          "form": "제 생각보다 훨씬 깊게알고 계신거같아요",
          "original_form": "제 생각보다 훨씬 깊게알고 계신거같아요",
          "speaker_id": "2",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100031357.1.28",
          "form": "음악회 보다가 자연스레 알게된거같아요",
          "original_form": "음악회 보다가 자연스레 알게된거같아요",
          "speaker_id": "1",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100031357.1.29",
          "form": "전문가같으세요! ㅎㅎ",
          "original_form": "전문가같으세요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100031357.1.30",
          "form": "잘 기억이 안나요 ㅜㅠ 감사합니다",
          "original_form": "잘 기억이 안나요 ㅜㅠ 감사합니다",
          "speaker_id": "1",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100031357.1.31",
          "form": "꽤 오래전 분이신데 이분한테 배운 한국 작곡가가 있는것도 신기하네요 ㅎ",
          "original_form": "꽤 오래전 분이신데 이분한테 배운 한국 작곡가가 있는것도 신기하네요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 21:01"
        }
      ]
