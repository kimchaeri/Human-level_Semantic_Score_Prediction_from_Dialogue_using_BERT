      "utterance": [
        {
          "id": "MDRW2100004827.1.1",
          "form": "지금 계절 어때?",
          "original_form": "지금 계절 어때?",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004827.1.2",
          "form": "난 너무 싫어",
          "original_form": "난 너무 싫어",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004827.1.3",
          "form": "나도 습하고 꿉꿉한데 춥다가도 더워",
          "original_form": "나도 습하고 꿉꿉한데 춥다가도 더워",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004827.1.4",
          "form": "추운가???",
          "original_form": "추운가???",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004827.1.5",
          "form": "에어컨 너무 많이 틀면 춥긴 하지",
          "original_form": "에어컨 너무 많이 틀면 춥긴 하지",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004827.1.6",
          "form": "선풍기가 너무 센가바 감기걸리는jul",
          "original_form": "선풍기가 너무 센가바 감기걸리는jul",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004827.1.7",
          "form": "나도 선풍기 쐐고싶어",
          "original_form": "나도 선풍기 쐐고싶어",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004827.1.8",
          "form": "근데 그럼 무슨 계절 젤 좋아해?",
          "original_form": "근데 그럼 무슨 계절 젤 좋아해?",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004827.1.9",
          "form": "나난 겨울이 제일 좋아",
          "original_form": "나난 겨울이 제일 좋아",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004827.1.10",
          "form": "방학도 길고 껴입으면 대잖앙",
          "original_form": "방학도 길고 껴입으면 대잖앙",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004827.1.11",
          "form": "나도 겨울이 젤 좋은데",
          "original_form": "나도 겨울이 젤 좋은데",
          "speaker_id": "1",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004827.1.12",
          "form": "이번 겨울에 눈이 너무 안왔어",
          "original_form": "이번 겨울에 눈이 너무 안왔어",
          "speaker_id": "1",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004827.1.13",
          "form": "운전하는 사람은 좋았을 듯",
          "original_form": "운전하는 사람은 좋았을 듯",
          "speaker_id": "1",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004827.1.14",
          "form": "맞아 근데 눈사람은 만들었음 ㅎㅎ",
          "original_form": "맞아 근데 눈사람은 만들었음 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004827.1.15",
          "form": "사람까진 아니고 눈주먹인듯",
          "original_form": "사람까진 아니고 눈주먹인듯",
          "speaker_id": "2",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004827.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004827.1.17",
          "form": "난 눈토끼",
          "original_form": "난 눈토끼",
          "speaker_id": "1",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004827.1.18",
          "form": "과연 토끼일까요?",
          "original_form": "과연 토끼일까요?",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004827.1.19",
          "form": "눈오리도 만들었어",
          "original_form": "눈오리도 만들었어",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004827.1.20",
          "form": "완잔 동물왕국",
          "original_form": "완잔 동물왕국",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004827.1.21",
          "form": "동물왕국치고 작은 편",
          "original_form": "동물왕국치고 작은 편",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004827.1.22",
          "form": "그렇긴 하지 근데 난 가을도 좋아",
          "original_form": "그렇긴 하지 근데 난 가을도 좋아",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004827.1.23",
          "form": "겨울은 롱패딩 입어서 옷을 신겨써서 입어도 안보이잖아",
          "original_form": "겨울은 롱패딩 입어서 옷을 신겨써서 입어도 안보이잖아",
          "speaker_id": "1",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004827.1.24",
          "form": "근데 나가기 긔찮을땐 개꿀이야",
          "original_form": "근데 나가기 긔찮을땐 개꿀이야",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004827.1.25",
          "form": "가을은 생각해보니까 방학이 없어서 좀... 슬픈듯...그렇지않니",
          "original_form": "가을은 생각해보니까 방학이 없어서 좀... 슬픈듯...그렇지않니",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004827.1.26",
          "form": "아 그렇네 근데 추석 있잖아",
          "original_form": "아 그렇네 근데 추석 있잖아",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004827.1.27",
          "form": "시험기간이긴 한데 어차피 공부 안해서 괜찮아",
          "original_form": "시험기간이긴 한데 어차피 공부 안해서 괜찮아",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004827.1.28",
          "form": "추석은 5일인데엥에에에엥",
          "original_form": "추석은 5일인데엥에에에엥",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004827.1.29",
          "form": "그건 그런데 많이 먹잖아",
          "original_form": "그건 그런데 많이 먹잖아",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004827.1.30",
          "form": "위거 무지 커져",
          "original_form": "위거 무지 커져",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004827.1.31",
          "form": "하긴 날씨도 선선하고 좋긴하다 마싯고",
          "original_form": "하긴 날씨도 선선하고 좋긴하다 마싯고",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004827.1.32",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210821 18:53"
        }
      ]
