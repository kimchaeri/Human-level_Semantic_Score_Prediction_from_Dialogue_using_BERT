      "utterance": [
        {
          "id": "MDRW2100043198.1.1",
          "form": "네 좋아해요 떡 좋아해서",
          "original_form": "네 좋아해요 떡 좋아해서",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100043198.1.2",
          "form": "쑥절편이랑 백절편중에 뭐가 좋으세요?",
          "original_form": "쑥절편이랑 백절편중에 뭐가 좋으세요?",
          "speaker_id": "1",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100043198.1.3",
          "form": "큰 차이없지 않아요?",
          "original_form": "큰 차이없지 않아요?",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100043198.1.4",
          "form": "쑥은 살짝 씁슬한 맛? 같은게 나던데요!",
          "original_form": "쑥은 살짝 씁슬한 맛? 같은게 나던데요!",
          "speaker_id": "1",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100043198.1.5",
          "form": "쑥함량이 높다면 쑥절편이 더 좋아요",
          "original_form": "쑥함량이 높다면 쑥절편이 더 좋아요",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043198.1.6",
          "form": "헉 저는 백절편이요 ㅋㅋ 너무 써요",
          "original_form": "헉 저는 백절편이요 ㅋㅋ 너무 써요",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043198.1.7",
          "form": "쑥이 비싸서 대부분 색소일거에요",
          "original_form": "쑥이 비싸서 대부분 색소일거에요",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043198.1.8",
          "form": "그럼 앙금절편도 좋아하세요?",
          "original_form": "그럼 앙금절편도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043198.1.9",
          "form": "절편안에 앙금들어간건가요?",
          "original_form": "절편안에 앙금들어간건가요?",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043198.1.10",
          "form": "네 바람떡이랑 비슷해요!",
          "original_form": "네 바람떡이랑 비슷해요!",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043198.1.11",
          "form": "팥앙금 좋아해서 맛있을거같아요",
          "original_form": "팥앙금 좋아해서 맛있을거같아요",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043198.1.12",
          "form": "맛있어요 유행하기도 했고!",
          "original_form": "맛있어요 유행하기도 했고!",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043198.1.13",
          "form": "너무 달지만 않으면 떡은 다 좋아해요",
          "original_form": "너무 달지만 않으면 떡은 다 좋아해요",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043198.1.14",
          "form": "저는 백설기 엄청 좋아해요",
          "original_form": "저는 백설기 엄청 좋아해요",
