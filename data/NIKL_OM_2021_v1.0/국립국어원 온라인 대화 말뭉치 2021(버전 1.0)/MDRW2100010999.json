      "utterance": [
        {
          "id": "MDRW2100010999.1.1",
          "form": "뭐 사는건 좋아하세요?",
          "original_form": "뭐 사는건 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100010999.1.2",
          "form": "쇼핑 완전좋아해요!",
          "original_form": "쇼핑 완전좋아해요!",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010999.1.3",
          "form": "오 진짜요?!",
          "original_form": "오 진짜요?!",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100010999.1.4",
          "form": "주로 뭐 사세요?",
          "original_form": "주로 뭐 사세요?",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100010999.1.5",
          "form": "신발 쇼핑 자주합니다ㅎㅎ",
          "original_form": "신발 쇼핑 자주합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100010999.1.6",
          "form": "조던 같은 거요?",
          "original_form": "조던 같은 거요?",
          "speaker_id": "1",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100010999.1.7",
          "form": "오 네네 신발에 관심좀 있으신가요",
          "original_form": "오 네네 신발에 관심좀 있으신가요",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100010999.1.8",
          "form": "아주 예전에 스무살 되자마자 버킷리스트라 하나 샀는데 여전히 보관중이에요 ㅋㅋㅋ",
          "original_form": "아주 예전에 스무살 되자마자 버킷리스트라 하나 샀는데 여전히 보관중이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100010999.1.9",
          "form": "근데 또 가치있는 모델은 아니라",
          "original_form": "근데 또 가치있는 모델은 아니라",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100010999.1.10",
          "form": "가끔 래플 참여는 하긴해요",
          "original_form": "가끔 래플 참여는 하긴해요",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100010999.1.11",
          "form": "남자들이 신을때가 진짜 예쁜것같아요",
          "original_form": "남자들이 신을때가 진짜 예쁜것같아요",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100010999.1.12",
          "form": "와 아쉽네요 요즘 신발들이 주식마냥 떡상해서..",
          "original_form": "와 아쉽네요 요즘 신발들이 주식마냥 떡상해서..",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100010999.1.13",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100010999.1.14",
          "form": "스캇 같은건 아무리해도 당첨 안되던데",
          "original_form": "스캇 같은건 아무리해도 당첨 안되던데",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100010999.1.15",
          "form": "되면 진짜 비싸게팔더라구요",
          "original_form": "되면 진짜 비싸게팔더라구요",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100010999.1.16",
          "form": "스캇.. 해외까지 한 10개는 응모한것같은데ㅠㅠ",
          "original_form": "스캇.. 해외까지 한 10개는 응모한것같은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100010999.1.17",
          "form": "주변에 그래도 한 2명이 되긴했는데",
          "original_form": "주변에 그래도 한 2명이 되긴했는데",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100010999.1.18",
          "form": "다들 역시나 팔았어요 ㅋㅋㅋ",
          "original_form": "다들 역시나 팔았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100010999.1.19",
          "form": "래플 챙기는것도 일인것같아요,, 전에는 나매 알림설정했는데",
          "original_form": "래플 챙기는것도 일인것같아요,, 전에는 나매 알림설정했는데",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100010999.1.20",
          "form": "그것도 귀찮아져서..",
          "original_form": "그것도 귀찮아져서..",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100010999.1.21",
          "form": "와 스캇당첨이요?",
          "original_form": "와 스캇당첨이요?",
          "speaker_id": "2",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100010999.1.22",
          "form": "저도 나매 항상 챙겨봐요ㅎㅎ",
          "original_form": "저도 나매 항상 챙겨봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100010999.1.23",
          "form": "나매 정보 많죠 ㅋㅋㅋ 신발 좋아하는 사람이 진짜 많구나 싶었어요",
          "original_form": "나매 정보 많죠 ㅋㅋㅋ 신발 좋아하는 사람이 진짜 많구나 싶었어요",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010999.1.24",
          "form": "전 가끔 남자패션유투브도 보는데",
          "original_form": "전 가끔 남자패션유투브도 보는데",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010999.1.25",
          "form": "거기 나온 스테디로만 입어도 괜찮은것같아요",
          "original_form": "거기 나온 스테디로만 입어도 괜찮은것같아요",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010999.1.26",
          "form": "패션에 관심 없는 사람들이 많아서ㅠㅋㅋㅋㅋ",
          "original_form": "패션에 관심 없는 사람들이 많아서ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010999.1.27",
          "form": "오 저도 가끔봐요 핏더사이즈나 이런사람들ㅎㅎ",
          "original_form": "오 저도 가끔봐요 핏더사이즈나 이런사람들ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100010999.1.28",
          "form": "핏더사이즈 진짜 웃기던뎈ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "핏더사이즈 진짜 웃기던뎈ㅋㅋㅋㅋㅋㅋㅋㅋ",
