      "utterance": [
        {
          "id": "MDRW2100019823.1.1",
          "form": "ㅋㅋ name1님 기절 하실듯",
          "original_form": "ㅋㅋ &name1&님 기절 하실듯",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019823.1.2",
          "form": "사망맛 이라니 너무해요",
          "original_form": "사망맛 이라니 너무해요",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019823.1.3",
          "form": "전 불닭볶음면도 매워서",
          "original_form": "전 불닭볶음면도 매워서",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019823.1.4",
          "form": "순대랑먹어요ㅋ",
          "original_form": "순대랑먹어요ㅋ",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019823.1.5",
          "form": "저희지역에 고추짬뽕 유명한데 있는데",
          "original_form": "저희지역에 고추짬뽕 유명한데 있는데",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019823.1.6",
          "form": "그것도 매운것?",
          "original_form": "그것도 매운것?",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019823.1.7",
          "form": "남자들이 호기롭게 도전하다가 내려놓는분 많어요  ㅋ",
          "original_form": "남자들이 호기롭게 도전하다가 내려놓는분 많어요  ㅋ",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019823.1.8",
          "form": "ㅋㄱ 전 못해요",
          "original_form": "ㅋㄱ 전 못해요",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019823.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019823.1.10",
          "form": "매운건 통각입니다",
          "original_form": "매운건 통각입니다",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019823.1.11",
          "form": "저도 매운거 잘먹는 편인데",
          "original_form": "저도 매운거 잘먹는 편인데",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019823.1.12",
          "form": "여성분이 대체로 잘드심",
          "original_form": "여성분이 대체로 잘드심",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019823.1.13",
          "form": "저도 국물에 밥말고 식도가 타는듯 아파서 ㅋㅋ",
          "original_form": "저도 국물에 밥말고 식도가 타는듯 아파서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019823.1.14",
          "form": "그뒤론 면만 먹어여 ㅋㅋ 국물 먹을게 못됨",
          "original_form": "그뒤론 면만 먹어여 ㅋㅋ 국물 먹을게 못됨",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019823.1.15",
          "form": "헐~~보드카느낌",
          "original_form": "헐~~보드카느낌",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019823.1.16",
          "form": "보트카는 저리가라 ㅜㅜ",
          "original_form": "보트카는 저리가라 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019823.1.17",
          "form": "우와 음식이 아님",
          "original_form": "우와 음식이 아님",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019823.1.18",
          "form": "음음 불닭 20배 매워요",
          "original_form": "음음 불닭 20배 매워요",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019823.1.19",
