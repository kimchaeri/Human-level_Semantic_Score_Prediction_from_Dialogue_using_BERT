        },
        {
          "id": "MDRW2100029170.1.2",
          "form": "지금 딱 반년 약간 넘었는데",
          "original_form": "지금 딱 반년 약간 넘었는데",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029170.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029170.1.4",
          "form": "그때부터 뉴스레터 뭔소린지몰라도",
          "original_form": "그때부터 뉴스레터 뭔소린지몰라도",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029170.1.5",
          "form": "걍 머리에넣고",
          "original_form": "걍 머리에넣고",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029170.1.6",
          "form": "유튜브영상도 이해안가도 걍 듣고",
          "original_form": "유튜브영상도 이해안가도 걍 듣고",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029170.1.7",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029170.1.8",
          "form": "지금은 내가생각해도",
          "original_form": "지금은 내가생각해도",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029170.1.9",
          "form": "아는게 엄청많이늘엇음",
          "original_form": "아는게 엄청많이늘엇음",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029170.1.10",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.11",
          "form": "계속 노출시키는게",
          "original_form": "계속 노출시키는게",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.12",
          "form": "아무생각없이 그냥 뭐라도 하는게",
          "original_form": "아무생각없이 그냥 뭐라도 하는게",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.13",
          "form": "제일 좋은것 같음",
          "original_form": "제일 좋은것 같음",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.14",
          "form": "진짜 중요햐",
          "original_form": "진짜 중요햐",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.15",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.16",
          "form": "솔직히 우리 말 배울때도",
          "original_form": "솔직히 우리 말 배울때도",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.17",
          "form": "주변에서 들으니까",
          "original_form": "주변에서 들으니까",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.18",
          "form": "할 수 있게 된거 생각하면",
          "original_form": "할 수 있게 된거 생각하면",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.19",
          "form": "반복이 진짜 중요한것 같더라",
          "original_form": "반복이 진짜 중요한것 같더라",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029170.1.20",
          "form": "그치..",
          "original_form": "그치..",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029170.1.21",
          "form": "교수님들도 매일 복습하라고 하는",
          "original_form": "교수님들도 매일 복습하라고 하는",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
