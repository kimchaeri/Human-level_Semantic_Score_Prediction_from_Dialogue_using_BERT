      "utterance": [
        {
          "id": "MDRW2100038007.1.1",
          "form": "누구 말을 믿어야 되는데",
          "original_form": "누구 말을 믿어야 되는데",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038007.1.2",
          "form": "근데 그때",
          "original_form": "근데 그때",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038007.1.3",
          "form": "내 상담했을때",
          "original_form": "내 상담했을때",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038007.1.4",
          "form": "리코더 부는 실력을 보면",
          "original_form": "리코더 부는 실력을 보면",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038007.1.5",
          "form": "리코더는 하는데 입에 대고는 안한다고 했다.",
          "original_form": "리코더는 하는데 입에 대고는 안한다고 했다.",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038007.1.6",
          "form": "안한 게 맞다ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "안한 게 맞다ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038007.1.7",
          "form": "그냥 손으로만 한다고 했어",
          "original_form": "그냥 손으로만 한다고 했어",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100038007.1.8",
          "form": "어 근데 손도 잘 안돼ㅋㅋㅋㅋ",
          "original_form": "어 근데 손도 잘 안돼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100038007.1.9",
          "form": "그래서 내가 그때 속으로는 그게 무슨 의미가 있나 싶더라고",
          "original_form": "그래서 내가 그때 속으로는 그게 무슨 의미가 있나 싶더라고",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100038007.1.10",
          "form": "구멍을 제대로 못 막아ㅋㅋㅋ",
          "original_form": "구멍을 제대로 못 막아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100038007.1.11",
          "form": "응응 그랬구나.",
          "original_form": "응응 그랬구나.",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100038007.1.12",
          "form": "아하.....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하.....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100038007.1.13",
          "form": "신랑이 리코더를 도레미파솔라시도 불러주니까",
          "original_form": "신랑이 리코더를 도레미파솔라시도 불러주니까",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100038007.1.14",
          "form": "응응 ㅋㅋㅋ",
          "original_form": "응응 ㅋㅋㅋ",
