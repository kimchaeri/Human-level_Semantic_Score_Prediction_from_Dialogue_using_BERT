      "utterance": [
        {
          "id": "MDRW2100025698.1.1",
          "form": "나 최근에 원두커피 드립머신 샀어",
          "original_form": "나 최근에 원두커피 드립머신 샀어",
          "speaker_id": "2",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025698.1.2",
          "form": "어디꺼?",
          "original_form": "어디꺼?",
          "speaker_id": "1",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025698.1.3",
          "form": "테팔거였어ㅋㅋㅋ",
          "original_form": "테팔거였어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025698.1.4",
          "form": "원두는 어디서 사다먹어?",
          "original_form": "원두는 어디서 사다먹어?",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025698.1.5",
          "form": "원두는 그냥 이마트에ㅔ서 통으로 그라인딩된 원두 사먹어",
          "original_form": "원두는 그냥 이마트에ㅔ서 통으로 그라인딩된 원두 사먹어",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025698.1.6",
          "form": "콜롬비아 수프리뫀ㅋㅋ",
          "original_form": "콜롬비아 수프리뫀ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025698.1.7",
          "form": "아항 그건 맛이어때?",
          "original_form": "아항 그건 맛이어때?",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025698.1.8",
          "form": "약간 산미 있는 정도?? 근데 걍 쏘쏘해서 다음엔 로스터리 카페에서 사보려고",
          "original_form": "약간 산미 있는 정도?? 근데 걍 쏘쏘해서 다음엔 로스터리 카페에서 사보려고",
          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025698.1.9",
          "form": "아 로스터리카페는 그때 갔던 너희동네 거기인가?",
          "original_form": "아 로스터리카페는 그때 갔던 너희동네 거기인가?",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025698.1.10",
          "form": "거기도 괜찮고 지금 고려하는 데는 더 작고 골목에 있는 데ㅋㅋㅋ",
          "original_form": "거기도 괜찮고 지금 고려하는 데는 더 작고 골목에 있는 데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025698.1.11",
          "form": "오 카페가 많구나",
          "original_form": "오 카페가 많구나",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025698.1.12",
          "form": "또 산거 더 있어?",
          "original_form": "또 산거 더 있어?",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025698.1.13",
          "form": "나는 더 없어",
          "original_form": "나는 더 없어",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025698.1.14",
          "form": "글쿠나 ㅋㅋㅋ 쇼핑하면 재밌어",
          "original_form": "글쿠나 ㅋㅋㅋ 쇼핑하면 재밌어",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025698.1.15",
          "form": "ㅋㅋ 맞아 쇼핑 그 돈쓰는 재미가 있지",
          "original_form": "ㅋㅋ 맞아 쇼핑 그 돈쓰는 재미가 있지",
