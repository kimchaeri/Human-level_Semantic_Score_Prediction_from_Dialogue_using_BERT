      "utterance": [
        {
          "id": "MDRW2100045882.1.1",
          "form": "즐겨보는 프로 있으세여",
          "original_form": "즐겨보는 프로 있으세여",
          "speaker_id": "1",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100045882.1.2",
          "form": "저 가끔 웨이브 구독해서",
          "original_form": "저 가끔 웨이브 구독해서",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045882.1.3",
          "form": "꼬꼬무 봐요",
          "original_form": "꼬꼬무 봐요",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100045882.1.4",
          "form": "아 가끔 보는데 재밋는내용 많더라고요",
          "original_form": "아 가끔 보는데 재밋는내용 많더라고요",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100045882.1.5",
          "form": "그쵸 패널이 말빨이 좋더라구요",
          "original_form": "그쵸 패널이 말빨이 좋더라구요",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100045882.1.6",
          "form": "네 구성을 잘한거같아요",
          "original_form": "네 구성을 잘한거같아요",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045882.1.7",
          "form": "주제가 무거운데 컨셉을 잘 잡아셔",
          "original_form": "주제가 무거운데 컨셉을 잘 잡아셔",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045882.1.8",
          "form": "좀 수월하게 보게되더라구요",
          "original_form": "좀 수월하게 보게되더라구요",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045882.1.9",
          "form": "즐겨보는 프로 있으세요?",
          "original_form": "즐겨보는 프로 있으세요?",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045882.1.10",
          "form": "전 유퀴즈 잘봐요",
          "original_form": "전 유퀴즈 잘봐요",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045882.1.11",
          "form": "아 유퀴즈 재밌죠",
          "original_form": "아 유퀴즈 재밌죠",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045882.1.12",
          "form": "돌아다님서 할때가 재밋긴 했어요",
          "original_form": "돌아다님서 할때가 재밋긴 했어요",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045882.1.13",
          "form": "맞아요 그건 아쉬워요",
          "original_form": "맞아요 그건 아쉬워요",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045882.1.14",
          "form": "저 그 삼광빌라 여주인공 배우 나오는 편 재밌게 봤어요",
          "original_form": "저 그 삼광빌라 여주인공 배우 나오는 편 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045882.1.15",
          "form": "다 재미잇더라고요 \r\n유재석이랑 조셉도 웃기고요",
          "original_form": "다 재미잇더라고요 \r\n유재석이랑 조셉도 웃기고요",
          "speaker_id": "1",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045882.1.16",
          "form": "게스트도 잘고르는거같아요 ㅋㅋㅋ",
          "original_form": "게스트도 잘고르는거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045882.1.17",
          "form": "네 일반인들 나와도 재밋더라고요",
          "original_form": "네 일반인들 나와도 재밋더라고요",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045882.1.18",
          "form": "진행을 잘하니까 일반인 나와도 어색한거 없더라고요",
          "original_form": "진행을 잘하니까 일반인 나와도 어색한거 없더라고요",
