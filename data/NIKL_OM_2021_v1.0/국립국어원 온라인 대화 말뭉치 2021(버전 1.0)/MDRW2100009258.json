      "utterance": [
        {
          "id": "MDRW2100009258.1.1",
          "form": "오 그건 넘 좋을 것 같어요",
          "original_form": "오 그건 넘 좋을 것 같어요",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009258.1.2",
          "form": "name1님 학교도 수강신청 힘든가요",
          "original_form": "&name1&님 학교도 수강신청 힘든가요",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009258.1.3",
          "form": "저는 시간표 쭉쭉 짜줘서 ㅋㅋㅋㅋ",
          "original_form": "저는 시간표 쭉쭉 짜줘서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009258.1.4",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009258.1.5",
          "form": "교양만 힘들었고 나머진 수월했엉ㅅ",
          "original_form": "교양만 힘들었고 나머진 수월했엉ㅅ",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009258.1.6",
          "form": "대신 꿀교양을 위한 사투는..",
          "original_form": "대신 꿀교양을 위한 사투는..",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.7",
          "form": "치열했죠",
          "original_form": "치열했죠",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.8",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.9",
          "form": "앞에 대기인원 만명 이럴때",
          "original_form": "앞에 대기인원 만명 이럴때",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.10",
          "form": "마치 백신 예약",
          "original_form": "마치 백신 예약",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.11",
          "form": "omg",
          "original_form": "omg",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.12",
          "form": "장난아니네요",
          "original_form": "장난아니네요",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.13",
          "form": "교양 뭐들으셨는데요?",
          "original_form": "교양 뭐들으셨는데요?",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.14",
          "form": "그리고 학교 서버가 유리서버라",
          "original_form": "그리고 학교 서버가 유리서버라",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.15",
          "form": "건지셨나요!!",
          "original_form": "건지셨나요!!",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.16",
          "form": "교양 기억도 안나요",
          "original_form": "교양 기억도 안나요",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.17",
          "form": "아아 ㅠ...그럼 너무 답답하죠",
          "original_form": "아아 ㅠ...그럼 너무 답답하죠",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.18",
          "form": "점수 잘준다했는데",
          "original_form": "점수 잘준다했는데",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.19",
          "form": "비쁠줚어요",
          "original_form": "비쁠줚어요",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009258.1.20",
          "form": "눈물이 났죠",
          "original_form": "눈물이 났죠",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.21",
          "form": "오우...",
          "original_form": "오우...",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.22",
          "form": "눈물나네요...",
          "original_form": "눈물나네요...",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.23",
          "form": "좀더.. 줘라....",
          "original_form": "좀더.. 줘라....",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.24",
          "form": "나의...나의 죄다..",
          "original_form": "나의...나의 죄다..",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.25",
          "form": "ㄴㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄴㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.26",
          "form": "근데 그 교수님한테 메일 보내면",
          "original_form": "근데 그 교수님한테 메일 보내면",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.27",
          "form": "비로 내린다는 소문이 있어서",
          "original_form": "비로 내린다는 소문이 있어서",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.28",
          "form": "조용히 수긍했어요",
          "original_form": "조용히 수긍했어요",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.29",
          "form": "아 그런 교수님달",
          "original_form": "아 그런 교수님달",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.30",
          "form": "있죠...",
          "original_form": "있죠...",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.32",
          "form": "저도 그래서 항상 얌전히있어요",
          "original_form": "저도 그래서 항상 얌전히있어요",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.33",
          "form": "너무 무서워...",
          "original_form": "너무 무서워...",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.34",
          "form": "그래..이게어디냐.ㅡ",
          "original_form": "그래..이게어디냐.ㅡ",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.35",
          "form": "하면서...",
          "original_form": "하면서...",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.36",
          "form": "교수님들 딸램 아들램이 저희보다",
          "original_form": "교수님들 딸램 아들램이 저희보다",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009258.1.37",
          "form": "나이 많을것같은대",
          "original_form": "나이 많을것같은대",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009258.1.38",
          "form": "고졸애들 괴롭히지 말아주셨으면",
          "original_form": "고졸애들 괴롭히지 말아주셨으면",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009258.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴ",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009258.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009258.1.41",
          "form": "맞죠 고졸",
          "original_form": "맞죠 고졸",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009258.1.42",
          "form": "좀 봐주라!??",
          "original_form": "좀 봐주라!??",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009258.1.43",
          "form": "우리 학점좀 잘받자 ㅠㅠ",
          "original_form": "우리 학점좀 잘받자 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:28"
        }
