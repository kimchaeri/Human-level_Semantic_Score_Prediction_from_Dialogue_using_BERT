        {
          "id": "MDRW2100012891.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100012891.1.2",
          "form": "진짜 가고 싶어요",
          "original_form": "진짜 가고 싶어요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100012891.1.3",
          "form": "언세갈수있을지",
          "original_form": "언세갈수있을지",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100012891.1.4",
          "form": "어디가 제일 기억나세요?",
          "original_form": "어디가 제일 기억나세요?",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100012891.1.5",
          "form": "전 태국이여",
          "original_form": "전 태국이여",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100012891.1.6",
          "form": "태국 안가봤는데",
          "original_form": "태국 안가봤는데",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100012891.1.7",
          "form": "뭐가 좋았어요?",
          "original_form": "뭐가 좋았어요?",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100012891.1.8",
          "form": "환락가요ㅋ",
          "original_form": "환락가요ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100012891.1.9",
          "form": "나중에 태국가면 꼭 기억하고 가 볼께요~",
          "original_form": "나중에 태국가면 꼭 기억하고 가 볼께요~",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100012891.1.10",
          "form": "푸켓도요",
          "original_form": "푸켓도요",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100012891.1.11",
          "form": "오~ 푸켓은 뭐가 유명해요?",
          "original_form": "오~ 푸켓은 뭐가 유명해요?",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100012891.1.12",
          "form": "밤문화요ㅎ",
          "original_form": "밤문화요ㅎ",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100012891.1.13",
          "form": "첨에 당황ㅠ",
          "original_form": "첨에 당황ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100012891.1.14",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100012891.1.15",
          "form": "금새 적응하고 즐기기요",
          "original_form": "금새 적응하고 즐기기요",
          "speaker_id": "1",
          "time": "20210825 21:57"
        }
