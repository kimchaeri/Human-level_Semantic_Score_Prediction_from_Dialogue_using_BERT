      "utterance": [
        {
          "id": "MDRW2100035973.1.1",
          "form": "이거 사고 집안일이 더 편해졌다 하는 거 있으세요?",
          "original_form": "이거 사고 집안일이 더 편해졌다 하는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100035973.1.2",
          "form": "저 무릎이 아파서",
          "original_form": "저 무릎이 아파서",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100035973.1.3",
          "form": "스탠드 청소기 밀대 샀는데",
          "original_form": "스탠드 청소기 밀대 샀는데",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100035973.1.4",
          "form": "왜 진작 안샀을까 싶어요ㅋㅋ",
          "original_form": "왜 진작 안샀을까 싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100035973.1.5",
          "form": "무선으로 사셨나요?",
          "original_form": "무선으로 사셨나요?",
          "speaker_id": "1",
          "time": "20210830 14:14"
        },
        {
          "id": "MDRW2100035973.1.6",
          "form": "아 청소기가 아니라 밀대에요!ㅋㅋㅋ",
          "original_form": "아 청소기가 아니라 밀대에요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100035973.1.7",
          "form": "청소기 밀대!",
          "original_form": "청소기 밀대!",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100035973.1.8",
          "form": "아 합쳐진 그런 건줄 알았어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 합쳐진 그런 건줄 알았어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100035973.1.9",
          "form": "청소기에 같이 붙어있는 거로욬ㅋㅋㅋㅋ",
          "original_form": "청소기에 같이 붙어있는 거로욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100035973.1.10",
          "form": "name1님은 최근에 뭐 사셨어요?",
          "original_form": "&name1&님은 최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100035973.1.11",
          "form": "집안일 관련으로는 수세미 놓는 통이 낡아서 새거 샀어요",
          "original_form": "집안일 관련으로는 수세미 놓는 통이 낡아서 새거 샀어요",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100035973.1.12",
          "form": "전 그런거 보통 다이소에서 구매하는데",
          "original_form": "전 그런거 보통 다이소에서 구매하는데",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100035973.1.13",
          "form": "어디서 사셨어요?",
          "original_form": "어디서 사셨어요?",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100035973.1.14",
          "form": "저도 그냥 다이소에서 샀어요ㅋㅋㅋ",
          "original_form": "저도 그냥 다이소에서 샀어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100035973.1.15",
          "form": "젤 싸고 좋더라구요",
          "original_form": "젤 싸고 좋더라구요",
          "speaker_id": "1",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100035973.1.16",
          "form": "맞아요ㅋㅋㅋ 품질도 요즘은 많이 좋아졌어요",
          "original_form": "맞아요ㅋㅋㅋ 품질도 요즘은 많이 좋아졌어요",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100035973.1.17",
          "form": "수세미 놓은 통같은건 세균번식이 쉽다고해서",
          "original_form": "수세미 놓은 통같은건 세균번식이 쉽다고해서",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100035973.1.18",
          "form": "다이소에서 파는 저렴한걸로 갈아주거든요",
          "original_form": "다이소에서 파는 저렴한걸로 갈아주거든요",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100035973.1.19",
          "form": "네넹 맞아욬ㅋㅋ",
          "original_form": "네넹 맞아욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100035973.1.20",
          "form": "그 브리타 정수기 써보셨어요?",
          "original_form": "그 브리타 정수기 써보셨어요?",
          "speaker_id": "1",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100035973.1.21",
          "form": "엇 아니용!",
          "original_form": "엇 아니용!",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100035973.1.22",
          "form": "얘기만 들어보고 안써봤어용",
          "original_form": "얘기만 들어보고 안써봤어용",
          "speaker_id": "2",
          "time": "20210830 14:22"
        },
        {
          "id": "MDRW2100035973.1.23",
          "form": "좋은가용?",
          "original_form": "좋은가용?",
          "speaker_id": "2",
          "time": "20210830 14:22"
        },
        {
          "id": "MDRW2100035973.1.24",
          "form": "저도 바꿔볼까하고 물은거라 아직이에요 ㅎㅎ",
          "original_form": "저도 바꿔볼까하고 물은거라 아직이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:22"
        },
        {
          "id": "MDRW2100035973.1.25",
          "form": "집에 없는 시간이 길어지니까 정수기도 간단한게 좋지 않울까 해서요",
          "original_form": "집에 없는 시간이 길어지니까 정수기도 간단한게 좋지 않울까 해서요",
          "speaker_id": "1",
          "time": "20210830 14:23"
        },
        {
          "id": "MDRW2100035973.1.26",
          "form": "지금 집에 있는 정수기가 아직 마음에 들어서",
          "original_form": "지금 집에 있는 정수기가 아직 마음에 들어서",
          "speaker_id": "2",
          "time": "20210830 14:23"
        },
        {
          "id": "MDRW2100035973.1.27",
          "form": "바꿀 생각은 안해본것같아요ㅎㅎ",
          "original_form": "바꿀 생각은 안해본것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:23"
        }
      ]
    }
