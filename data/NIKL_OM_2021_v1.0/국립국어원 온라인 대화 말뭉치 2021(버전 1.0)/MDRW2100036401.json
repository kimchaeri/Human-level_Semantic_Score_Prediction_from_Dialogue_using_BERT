        },
        {
          "id": "MDRW2100036401.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100036401.1.4",
          "form": "네 당연히 먹었죠ㅋ",
          "original_form": "네 당연히 먹었죠ㅋ",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100036401.1.5",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100036401.1.6",
          "form": "콩나물국 진미채 콩장 등등",
          "original_form": "콩나물국 진미채 콩장 등등",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100036401.1.7",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036401.1.8",
          "form": "저는 김치찌개 먹었어요",
          "original_form": "저는 김치찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036401.1.9",
          "form": "좋아하는 음식이 있으신가요?",
          "original_form": "좋아하는 음식이 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036401.1.10",
          "form": "전 딱히 가리진 않는데 생선 종류 별로 안 좋아해요",
          "original_form": "전 딱히 가리진 않는데 생선 종류 별로 안 좋아해요",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036401.1.11",
          "form": "구운거나 조린 거 상관없이 다 싫어하시는건가요??",
          "original_form": "구운거나 조린 거 상관없이 다 싫어하시는건가요??",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036401.1.12",
          "form": "제가 고등어 알러지가 있어가지고 좀 가리게 됐어요ㅠ",
          "original_form": "제가 고등어 알러지가 있어가지고 좀 가리게 됐어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036401.1.13",
          "form": "그렇군요ㅠㅠ 그럼 생선육수도 싫어하시나요??",
          "original_form": "그렇군요ㅠㅠ 그럼 생선육수도 싫어하시나요??",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036401.1.14",
          "form": "트라우마가 있어 잘 안 먹는 편이예요",
          "original_form": "트라우마가 있어 잘 안 먹는 편이예요",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036401.1.15",
          "form": "그럼 고기는 좋아하시나요?",
          "original_form": "그럼 고기는 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036401.1.16",
          "form": "고기는 당연히 좋아합니다ㅋ",
          "original_form": "고기는 당연히 좋아합니다ㅋ",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
