      "utterance": [
        {
          "id": "MDRW2100003219.1.1",
          "form": "혹시 어떤음식 좋아하세요?",
          "original_form": "혹시 어떤음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003219.1.2",
          "form": "음 저는 한식이나 양식이요!",
          "original_form": "음 저는 한식이나 양식이요!",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003219.1.3",
          "form": "부대찌개랑 빠네 좋아해요ㅋㅋ",
          "original_form": "부대찌개랑 빠네 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100003219.1.4",
          "form": "매운음식이 좋으세요 적당한 음식이 좋으세요?",
          "original_form": "매운음식이 좋으세요 적당한 음식이 좋으세요?",
          "speaker_id": "1",
          "time": "20210820 20:47"
        },
        {
          "id": "MDRW2100003219.1.5",
          "form": "저 맵찔이라ㅋㅋㅋ매운걸 잘 못 먹어요ㅠㅜ",
          "original_form": "저 맵찔이라ㅋㅋㅋ매운걸 잘 못 먹어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003219.1.6",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "2",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003219.1.7",
          "form": "아뇨 저도 매운거 잘 못먹어요...",
          "original_form": "아뇨 저도 매운거 잘 못먹어요...",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003219.1.8",
          "form": "떡볶이도 매워서 잘 못먹어요",
          "original_form": "떡볶이도 매워서 잘 못먹어요",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003219.1.9",
          "form": "헉 떡볶이도요? 로제떡볶이는 어떠세요?ㅋㅋㅋ",
          "original_form": "헉 떡볶이도요? 로제떡볶이는 어떠세요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003219.1.10",
          "form": "맛은 있던데",
          "original_form": "맛은 있던데",
          "speaker_id": "1",
          "time": "20210820 21:16"
        },
        {
          "id": "MDRW2100003219.1.11",
          "form": "근데 그거는 칼로리 굉장히 높더라구요...",
          "original_form": "근데 그거는 칼로리 굉장히 높더라구요...",
          "speaker_id": "1",
          "time": "20210820 21:16"
        },
        {
          "id": "MDRW2100003219.1.12",
          "form": "그건 그래요ㅋㅋㅋ하지만 맛있게 먹으면 0칼로리예요ㅠㅜ",
          "original_form": "그건 그래요ㅋㅋㅋ하지만 맛있게 먹으면 0칼로리예요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210820 21:26"
        },
        {
          "id": "MDRW2100003219.1.13",
          "form": "그럼 배달음식 잘 안 시켜드세요?",
          "original_form": "그럼 배달음식 잘 안 시켜드세요?",
          "speaker_id": "2",
          "time": "20210820 21:26"
        },
        {
          "id": "MDRW2100003219.1.14",
          "form": "아뇨 되게 자주시켜먹는데",
          "original_form": "아뇨 되게 자주시켜먹는데",
          "speaker_id": "1",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003219.1.15",
          "form": "그래서 그런가 작년에 비해 좀 쪘더라구요..",
          "original_form": "그래서 그런가 작년에 비해 좀 쪘더라구요..",
          "speaker_id": "1",
          "time": "20210820 21:28"
        },
        {
          "id": "MDRW2100003219.1.16",
          "form": "ㅋㅋㅋ배달음식 못 참죠ㅠㅜㅜ저도 요새 배달음식 줄이려고 노력 중이예요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ배달음식 못 참죠ㅠㅜㅜ저도 요새 배달음식 줄이려고 노력 중이예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003219.1.17",
          "form": "뭐 자주 시켜드세요?",
          "original_form": "뭐 자주 시켜드세요?",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003219.1.18",
          "form": "자취하다보니 일주일에 다섯번은 시켜먹는 것 같아요ㅋㅋ",
          "original_form": "자취하다보니 일주일에 다섯번은 시켜먹는 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003219.1.19",
          "form": "돈 되게 많이 나가시겠다",
          "original_form": "돈 되게 많이 나가시겠다",
          "speaker_id": "1",
          "time": "20210820 21:43"
        },
        {
          "id": "MDRW2100003219.1.20",
          "form": "맞아요ㅠㅜ식비 장난 아니예요ㅜ",
