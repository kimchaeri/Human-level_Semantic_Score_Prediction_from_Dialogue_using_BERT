      "utterance": [
        {
          "id": "MDRW2100029541.1.1",
          "form": "몇점이 최고인가요",
          "original_form": "몇점이 최고인가요",
          "speaker_id": "2",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029541.1.2",
          "form": "2천점 넘었지요",
          "original_form": "2천점 넘었지요",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029541.1.3",
          "form": "과거에는.?",
          "original_form": "과거에는.?",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029541.1.4",
          "form": "헐 대박이네요",
          "original_form": "헐 대박이네요",
          "speaker_id": "2",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029541.1.5",
          "form": "지금은 안습입니다",
          "original_form": "지금은 안습입니다",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029541.1.6",
          "form": "전700점대가 최고점",
          "original_form": "전700점대가 최고점",
          "speaker_id": "2",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100029541.1.7",
          "form": "게임실력이 퇴보합니다",
          "original_form": "게임실력이 퇴보합니다",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029541.1.8",
          "form": "나이때문일까요",
          "original_form": "나이때문일까요",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029541.1.9",
          "form": "배신감느껴져요",
          "original_form": "배신감느껴져요",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029541.1.10",
          "form": "그런가요ㄷㄷㄷ",
          "original_form": "그런가요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029541.1.11",
          "form": "튀김키우기 켜모니",
          "original_form": "튀김키우기 켜모니",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029541.1.12",
          "form": "은반지 튀김까지 올렸네요",
          "original_form": "은반지 튀김까지 올렸네요",
          "speaker_id": "2",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029541.1.13",
          "form": "튀김키우기 게임은 아직 초보입니다",
          "original_form": "튀김키우기 게임은 아직 초보입니다",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029541.1.14",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100029541.1.15",
          "form": "드레곤날개도 키우고",
          "original_form": "드레곤날개도 키우고",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029541.1.16",
          "form": "은반지라니.. 못먹을것",
          "original_form": "은반지라니.. 못먹을것",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029541.1.17",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029541.1.18",
          "form": "돈을써야 편해지죠",
          "original_form": "돈을써야 편해지죠",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029541.1.19",
          "form": "얼음도 튀겨요",
          "original_form": "얼음도 튀겨요",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029541.1.20",
          "form": "현질 말씀하시는거죠",
          "original_form": "현질 말씀하시는거죠",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029541.1.21",
          "form": "네 ㅋㅋ 게임은 현질입니다",
          "original_form": "네 ㅋㅋ 게임은 현질입니다",
          "speaker_id": "2",
          "time": "20210903 10:09"
        }
      ]
