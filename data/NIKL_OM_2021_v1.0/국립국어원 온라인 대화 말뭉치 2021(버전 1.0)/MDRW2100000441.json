        },
        {
          "id": "MDRW2100000441.1.3",
          "form": "나 침구세트 구입해야하는데  추천좀해줘",
          "original_form": "나 침구세트 구입해야하는데  추천좀해줘",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000441.1.4",
          "form": "애들 침대 샀는데 뭘사얄지",
          "original_form": "애들 침대 샀는데 뭘사얄지",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000441.1.5",
          "form": "오홀 이사해서",
          "original_form": "오홀 이사해서",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000441.1.6",
          "form": "다바꾸고 좋으다",
          "original_form": "다바꾸고 좋으다",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000441.1.7",
          "form": "오랜만에 이사하니 쇼핑할게 너무 많아",
          "original_form": "오랜만에 이사하니 쇼핑할게 너무 많아",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000441.1.8",
          "form": "아이르 출첵 안했어??",
          "original_form": "아이르 출첵 안했어??",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000441.1.9",
          "form": "그런거두 있어???  아이르??  거긴 어떤 쇼핑몰이야",
          "original_form": "그런거두 있어???  아이르??  거긴 어떤 쇼핑몰이야",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000441.1.10",
          "form": "어플 있는디",
          "original_form": "어플 있는디",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000441.1.11",
          "form": "아님 사이트 들어가봐",
          "original_form": "아님 사이트 들어가봐",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000441.1.12",
          "form": "저렴히딜들 뜨더라",
          "original_form": "저렴히딜들 뜨더라",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000441.1.13",
          "form": "정말??  침구. 커튼 발매트 등등",
          "original_form": "정말??  침구. 커튼 발매트 등등",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000441.1.14",
          "form": "살게 왤케 많은지..  할인정보 있음 알려줘봐",
          "original_form": "살게 왤케 많은지..  할인정보 있음 알려줘봐",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000441.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000441.1.16",
          "form": "거기다있어~~",
          "original_form": "거기다있어~~",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000441.1.17",
          "form": "90퍼할인도 있는것같든디",
          "original_form": "90퍼할인도 있는것같든디",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000441.1.18",
          "form": "할인정보  고마워",
          "original_form": "할인정보  고마워",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000441.1.19",
          "form": "나 식기세척기 샀는데",
          "original_form": "나 식기세척기 샀는데",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000441.1.20",
          "form": "거의 50%가격에 있으니 좋더라",
          "original_form": "거의 50%가격에 있으니 좋더라",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000441.1.21",
          "form": "너두 이사가면 식기세척기 하나 들여놔",
          "original_form": "너두 이사가면 식기세척기 하나 들여놔",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000441.1.22",
          "form": "오홀",
          "original_form": "오홀",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000441.1.23",
          "form": "나도이사가믄 들여야지",
          "original_form": "나도이사가믄 들여야지",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000441.1.24",
          "form": "살거천지징",
          "original_form": "살거천지징",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000441.1.25",
          "form": "여기부터 저기 까지  죄다 구입해야해",
          "original_form": "여기부터 저기 까지  죄다 구입해야해",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000441.1.26",
          "form": "이사하면서 없어진거랑 필요한것들이 한두개가 아냐",
          "original_form": "이사하면서 없어진거랑 필요한것들이 한두개가 아냐",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000441.1.27",
          "form": "글치 어렵다",
          "original_form": "글치 어렵다",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000441.1.28",
          "form": "이사후기 써두지",
          "original_form": "이사후기 써두지",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000441.1.29",
          "form": "블로그에 저장해둬",
          "original_form": "블로그에 저장해둬",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000441.1.30",
          "form": "이사한다고 좋았는데 이사하니 할일두 많구\r\n사야할것도 많구",
          "original_form": "이사한다고 좋았는데 이사하니 할일두 많구\r\n사야할것도 많구",
          "speaker_id": "1",
          "time": "20210819 16:14"
