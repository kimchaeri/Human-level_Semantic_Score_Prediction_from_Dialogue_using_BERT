      "utterance": [
        {
          "id": "MDRW2100003536.1.1",
          "form": "수강 신청 잘했어?",
          "original_form": "수강 신청 잘했어?",
          "speaker_id": "1",
          "time": "20210821 00:39"
        },
        {
          "id": "MDRW2100003536.1.2",
          "form": "나 일단 원하는 건 듣게 됐엏ㅋㅋㅋㅋ",
          "original_form": "나 일단 원하는 건 듣게 됐엏ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 00:40"
        },
        {
          "id": "MDRW2100003536.1.3",
          "form": "진짜 빡쎘다",
          "original_form": "진짜 빡쎘다",
          "speaker_id": "2",
          "time": "20210821 00:40"
        },
        {
          "id": "MDRW2100003536.1.4",
          "form": "오 잘됐네",
          "original_form": "오 잘됐네",
          "speaker_id": "1",
          "time": "20210821 00:40"
        },
        {
          "id": "MDRW2100003536.1.5",
          "form": "수강신청 어떻게 하면 잘 해?",
          "original_form": "수강신청 어떻게 하면 잘 해?",
          "speaker_id": "1",
          "time": "20210821 00:40"
        },
        {
          "id": "MDRW2100003536.1.6",
          "form": "걍 피방 갈 필요도 없고",
          "original_form": "걍 피방 갈 필요도 없고",
          "speaker_id": "2",
          "time": "20210821 00:41"
        },
        {
          "id": "MDRW2100003536.1.7",
          "form": "f5안누르기가 답인듯",
          "original_form": "f5안누르기가 답인듯",
          "speaker_id": "2",
          "time": "20210821 00:42"
        },
        {
          "id": "MDRW2100003536.1.8",
          "form": "새로고침을 하면 안되는구나",
          "original_form": "새로고침을 하면 안되는구나",
          "speaker_id": "1",
          "time": "20210821 00:42"
        },
        {
          "id": "MDRW2100003536.1.9",
          "form": "맞아 그러면 튕기더라구",
          "original_form": "맞아 그러면 튕기더라구",
          "speaker_id": "2",
          "time": "20210821 00:43"
        },
        {
          "id": "MDRW2100003536.1.10",
          "form": "언니는 성공함.?",
          "original_form": "언니는 성공함.?",
          "speaker_id": "2",
          "time": "20210821 00:43"
        },
        {
          "id": "MDRW2100003536.1.11",
          "form": "응응 나도 저번에 튕겼어",
          "original_form": "응응 나도 저번에 튕겼어",
          "speaker_id": "1",
          "time": "20210821 00:43"
        },
        {
          "id": "MDRW2100003536.1.12",
          "form": "나는 교수님은 실패했어",
          "original_form": "나는 교수님은 실패했어",
          "speaker_id": "1",
          "time": "20210821 00:43"
        },
        {
          "id": "MDRW2100003536.1.13",
          "form": "ㅠㅠㅠㅠㅠㅠ 헐 왜",
          "original_form": "ㅠㅠㅠㅠㅠㅠ 헐 왜",
          "speaker_id": "2",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003536.1.14",
          "form": "내가 원하는 시간, 원하는 교수님을 골랐는데",
          "original_form": "내가 원하는 시간, 원하는 교수님을 골랐는데",
          "speaker_id": "1",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003536.1.15",
          "form": "이미 끝났더라고",
          "original_form": "이미 끝났더라고",
          "speaker_id": "1",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003536.1.16",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ 에고 좋은 교수님어서 좋겠당",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ 에고 좋은 교수님어서 좋겠당",
          "speaker_id": "2",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003536.1.17",
          "form": "아 좋은 교수님이면 좋겠다고!!",
          "original_form": "아 좋은 교수님이면 좋겠다고!!",
          "speaker_id": "2",
          "time": "20210821 00:46"
        },
        {
          "id": "MDRW2100003536.1.18",
          "form": "응응 수강신청 실패해서",
          "original_form": "응응 수강신청 실패해서",
          "speaker_id": "1",
          "time": "20210821 00:46"
        },
        {
          "id": "MDRW2100003536.1.19",
          "form": "모르는 교수님이랑 했는데 좋은 사람이었으면 좋겠다",
          "original_form": "모르는 교수님이랑 했는데 좋은 사람이었으면 좋겠다",
          "speaker_id": "1",
          "time": "20210821 00:46"
        },
        {
          "id": "MDRW2100003536.1.20",
          "form": "꼭 그럴거야!!",
          "original_form": "꼭 그럴거야!!",
          "speaker_id": "2",
          "time": "20210821 00:47"
        },
        {
          "id": "MDRW2100003536.1.21",
          "form": "그렇게 말해줘서 고마워",
          "original_form": "그렇게 말해줘서 고마워",
          "speaker_id": "1",
          "time": "20210821 00:47"
        },
        {
          "id": "MDRW2100003536.1.22",
          "form": "2학기도 파이팅하자 우리~~~~",
          "original_form": "2학기도 파이팅하자 우리~~~~",
