      "utterance": [
        {
          "id": "MDRW2100016378.1.1",
          "form": "유튭 보시나요",
          "original_form": "유튭 보시나요",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016378.1.2",
          "form": "네네 유튜브 자주봐요!",
          "original_form": "네네 유튜브 자주봐요!",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016378.1.3",
          "form": "어떤 컨텐츠 주로보세요?",
          "original_form": "어떤 컨텐츠 주로보세요?",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016378.1.4",
          "form": "여행영상이요ㅎㅎ",
          "original_form": "여행영상이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016378.1.5",
          "form": "아 그렇군요! 추천 채널있나요?",
          "original_form": "아 그렇군요! 추천 채널있나요?",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016378.1.6",
          "form": "곽튜브요!",
          "original_form": "곽튜브요!",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100016378.1.7",
          "form": "오오 주로 어디쪽 여행인가요?",
          "original_form": "오오 주로 어디쪽 여행인가요?",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016378.1.8",
          "form": "지금 러시아 가있어요ㅎㅎ",
          "original_form": "지금 러시아 가있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100016378.1.9",
          "form": "오 해외여행이구나 저두 한번봐야겠어요",
          "original_form": "오 해외여행이구나 저두 한번봐야겠어요",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016378.1.10",
          "form": "해외여행 넘 가고싶네여 ㅠㅠ",
          "original_form": "해외여행 넘 가고싶네여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016378.1.11",
          "form": "맞아요 코로나빨리끝나야ㅠ",
          "original_form": "맞아요 코로나빨리끝나야ㅠ",
          "speaker_id": "1",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100016378.1.12",
          "form": "그러게요 ㅠㅠ 좋아하는 노래나 가수 있나요?",
          "original_form": "그러게요 ㅠㅠ 좋아하는 노래나 가수 있나요?",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016378.1.13",
          "form": "저 에스파요!",
          "original_form": "저 에스파요!",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016378.1.14",
          "form": "아 아까 얘기했엇나요!?",
          "original_form": "아 아까 얘기했엇나요!?",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016378.1.15",
          "form": "그럼 배우는 누구 좋아하세요!",
          "original_form": "그럼 배우는 누구 좋아하세요!",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016378.1.16",
          "form": "신하균이요!",
          "original_form": "신하균이요!",
          "speaker_id": "1",
          "time": "20210826 13:53"
        }
      ]
