      "utterance": [
        {
          "id": "MDRW2100017173.1.1",
          "form": "이게 선배 후배 상관없이",
          "original_form": "이게 선배 후배 상관없이",
          "speaker_id": "2",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017173.1.2",
          "form": "유니폼이 다 같잖아요",
          "original_form": "유니폼이 다 같잖아요",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.4",
          "form": "다 처음 만난 사이라",
          "original_form": "다 처음 만난 사이라",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.5",
          "form": "그냥 명찰만 보고",
          "original_form": "그냥 명찰만 보고",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.6",
          "form": "name3씨 이랬는데",
          "original_form": "&name3&씨 이랬는데",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.7",
          "form": "혼났나요",
          "original_form": "혼났나요",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.8",
          "form": "선배님이었던 거에요 ㅋㅋㅋㅋ",
          "original_form": "선배님이었던 거에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.9",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.10",
          "form": "하늘 같은 선배님한테",
          "original_form": "하늘 같은 선배님한테",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.11",
          "form": "감히 씨 라고 하냐고",
          "original_form": "감히 씨 라고 하냐고",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.12",
          "form": "서늘하셨겠어요..",
          "original_form": "서늘하셨겠어요..",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.13",
          "form": "네 좀...",
          "original_form": "네 좀...",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017173.1.14",
          "form": "그 다음부터는 무조건 선배님 했는데",
          "original_form": "그 다음부터는 무조건 선배님 했는데",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017173.1.15",
          "form": "다음에 만난 한국인분들은",
          "original_form": "다음에 만난 한국인분들은",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017173.1.16",
          "form": "그런거 보면 좀 참.. 악습같아요",
          "original_form": "그런거 보면 좀 참.. 악습같아요",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017173.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017173.1.18",
          "form": "부담스럽다고 그냥 씨... 라고 하라고",
          "original_form": "부담스럽다고 그냥 씨... 라고 하라고",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017173.1.19",
          "form": "이게 사람마다 좀 다른 것 같아요",
          "original_form": "이게 사람마다 좀 다른 것 같아요",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017173.1.20",
          "form": "전 이런거 별로 신경안써서",
          "original_form": "전 이런거 별로 신경안써서",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017173.1.21",
          "form": "아..국내 다니면 힘들었겠네",
          "original_form": "아..국내 다니면 힘들었겠네",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017173.1.22",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017173.1.23",
          "form": "그럴거같아요",
          "original_form": "그럴거같아요",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017173.1.24",
          "form": "약간 숨막히네요",
          "original_form": "약간 숨막히네요",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017173.1.25",
          "form": "그런 분위기들",
          "original_form": "그런 분위기들",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017173.1.26",
          "form": "남자들이 더 예민하지않나요?",
          "original_form": "남자들이 더 예민하지않나요?",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017173.1.27",
          "form": "그렇긴해요.. 수직적 문화 워낙 좋아하는 인간들이라ㅋㅋ",
          "original_form": "그렇긴해요.. 수직적 문화 워낙 좋아하는 인간들이라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017173.1.28",
          "form": "남자분들도 서열 심할 거 같아요 ㅎㅎ",
          "original_form": "남자분들도 서열 심할 거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017173.1.29",
          "form": "직장에서도 그럴 거 같아요 ㅎㅎ",
          "original_form": "직장에서도 그럴 거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017173.1.30",
          "form": "더하면 더 했지 덜하지는 않는 것 같아요ㅎ",
          "original_form": "더하면 더 했지 덜하지는 않는 것 같아요ㅎ",
