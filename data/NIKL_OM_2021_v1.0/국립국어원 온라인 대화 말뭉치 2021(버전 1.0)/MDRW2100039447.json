      "utterance": [
        {
          "id": "MDRW2100039447.1.1",
          "form": "밥안먹었어?",
          "original_form": "밥안먹었어?",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039447.1.2",
          "form": "먹었지",
          "original_form": "먹었지",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039447.1.3",
          "form": "저녁은 안먹었지",
          "original_form": "저녁은 안먹었지",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039447.1.4",
          "form": "넌 저녁 뭐먹을거야?",
          "original_form": "넌 저녁 뭐먹을거야?",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039447.1.5",
          "form": "나는 떡볶이 먹을꼬야",
          "original_form": "나는 떡볶이 먹을꼬야",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039447.1.6",
          "form": "나는 엽떡",
          "original_form": "나는 엽떡",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100039447.1.7",
          "form": "에 너도 떡볶이?",
          "original_form": "에 너도 떡볶이?",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100039447.1.8",
          "form": "엽떡에 당면 추가 해야지",
          "original_form": "엽떡에 당면 추가 해야지",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100039447.1.9",
          "form": "엽떡에 당면 추가하면 맛있어?",
          "original_form": "엽떡에 당면 추가하면 맛있어?",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.10",
          "form": "짱이야 난 맨날 그렇게 먹어",
          "original_form": "짱이야 난 맨날 그렇게 먹어",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.11",
          "form": "나도 다음엔 그렇게 먹어야겟다",
          "original_form": "나도 다음엔 그렇게 먹어야겟다",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.12",
          "form": "너도 해봐 그래그래",
          "original_form": "너도 해봐 그래그래",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.13",
          "form": "참치김밥도 준비핬어",
          "original_form": "참치김밥도 준비핬어",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.14",
          "form": "나는 덜매운맛",
          "original_form": "나는 덜매운맛",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.15",
          "form": "너는 맵기 어떤 맛으로 먹니?",
          "original_form": "너는 맵기 어떤 맛으로 먹니?",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.16",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.17",
          "form": "오늘 자꾸 통하네",
          "original_form": "오늘 자꾸 통하네",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.18",
          "form": "나는 오리지널로 목어",
          "original_form": "나는 오리지널로 목어",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.19",
          "form": "대단한걸...",
          "original_form": "대단한걸...",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039447.1.20",
          "form": "다음에 생크림 섞어 먹어보자",
          "original_form": "다음에 생크림 섞어 먹어보자",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039447.1.21",
          "form": "담에 같이 먹자 그래!",
          "original_form": "담에 같이 먹자 그래!",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039447.1.22",
          "form": "맛있대",
          "original_form": "맛있대",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039447.1.23",
          "form": "너는 어떤 음료수 좋아해",
          "original_form": "너는 어떤 음료수 좋아해",
          "speaker_id": "1",
          "time": "20210903 17:48"
        }
      ]
