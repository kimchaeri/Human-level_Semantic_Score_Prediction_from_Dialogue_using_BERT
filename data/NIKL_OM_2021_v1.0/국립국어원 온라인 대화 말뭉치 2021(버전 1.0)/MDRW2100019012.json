      "utterance": [
        {
          "id": "MDRW2100019012.1.1",
          "form": "오늘은 비가 너무 많이 와서 산책은 힘들 것 같아ㅠ",
          "original_form": "오늘은 비가 너무 많이 와서 산책은 힘들 것 같아ㅠ",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019012.1.2",
          "form": "ㅇㅇ 하루 쉬어 ㅎㅎ 비 안올 때 나가면 되지",
          "original_form": "ㅇㅇ 하루 쉬어 ㅎㅎ 비 안올 때 나가면 되지",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019012.1.3",
          "form": "주말엔 비소식 없으니 같이 나가자",
          "original_form": "주말엔 비소식 없으니 같이 나가자",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019012.1.4",
          "form": "그러자~ 산책도 하고 맛있는 것도 사오고 ㅎㅎ",
          "original_form": "그러자~ 산책도 하고 맛있는 것도 사오고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019012.1.5",
          "form": "매일 운동하다 안하면 안하는 날 몸이 더 아픈 기분이야 ㅋ",
          "original_form": "매일 운동하다 안하면 안하는 날 몸이 더 아픈 기분이야 ㅋ",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019012.1.6",
          "form": "며칠이나 했다고 ㅋㅋㅋ",
          "original_form": "며칠이나 했다고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019012.1.7",
          "form": "원래 초보자가 더 티를 내는 법이지 ㅎㅎ",
          "original_form": "원래 초보자가 더 티를 내는 법이지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019012.1.8",
          "form": "며칠 꾸준히 했으니 하루 정도 쉬어줘도 괜찮아",
          "original_form": "며칠 꾸준히 했으니 하루 정도 쉬어줘도 괜찮아",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019012.1.9",
          "form": "ㅇㅇ 그리고 비가 계속 내리니 기분도 축 내려앉아서 ㅎㅎ",
          "original_form": "ㅇㅇ 그리고 비가 계속 내리니 기분도 축 내려앉아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019012.1.10",
          "form": "커피 마셨어?",
          "original_form": "커피 마셨어?",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019012.1.11",
          "form": "아 맞다 커피~ 그걸 안마셔서 기분이 다운됐나? ㅋㅋ",
          "original_form": "아 맞다 커피~ 그걸 안마셔서 기분이 다운됐나? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019012.1.12",
          "form": "얼렁 마셔 ㅎㅎ 기분 좀 올라오게",
          "original_form": "얼렁 마셔 ㅎㅎ 기분 좀 올라오게",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019012.1.13",
          "form": "ㅎㅎ 그래야겠네",
          "original_form": "ㅎㅎ 그래야겠네",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019012.1.14",
          "form": "힘든 한 주가 끝나고 주말만 남으니 너무 좋다 ㅎㅎ",
          "original_form": "힘든 한 주가 끝나고 주말만 남으니 너무 좋다 ㅎㅎ",
