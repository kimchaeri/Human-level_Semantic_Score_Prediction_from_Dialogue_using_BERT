        },
        {
          "id": "MDRW2100026768.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100026768.1.4",
          "form": "저는 결혼했어요",
          "original_form": "저는 결혼했어요",
          "speaker_id": "1",
          "time": "20210902 08:54"
        },
        {
          "id": "MDRW2100026768.1.5",
          "form": "혹시 카페말고 좋은 데이트 장소 있을까요?",
          "original_form": "혹시 카페말고 좋은 데이트 장소 있을까요?",
          "speaker_id": "1",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100026768.1.6",
          "form": "오 근데 요새는 데이트장소가 진짜 웬만한데 가기 어려운거 같아요ㅠ",
          "original_form": "오 근데 요새는 데이트장소가 진짜 웬만한데 가기 어려운거 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100026768.1.7",
          "form": "시국도 시국이라..",
          "original_form": "시국도 시국이라..",
          "speaker_id": "2",
          "time": "20210902 09:10"
        },
        {
          "id": "MDRW2100026768.1.8",
          "form": "ㅋㅋㅋ영화관 식사 카페가 걍 코스같은..ㅎㅎ",
          "original_form": "ㅋㅋㅋ영화관 식사 카페가 걍 코스같은..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:10"
        },
        {
          "id": "MDRW2100026768.1.9",
          "form": "그나마 영화관도 못가네요ㅠ",
          "original_form": "그나마 영화관도 못가네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 09:10"
        },
        {
          "id": "MDRW2100026768.1.10",
          "form": "네 항상 가던곳만 가니까 조금 집겹기도하고",
          "original_form": "네 항상 가던곳만 가니까 조금 집겹기도하고",
          "speaker_id": "1",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100026768.1.11",
          "form": "name2은 어떤 데이트 좋아하세요?",
          "original_form": "&name2&은 어떤 데이트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100026768.1.12",
          "form": "저는 그냥 집에서 있는 것도 좋아요ㅋㅋ",
          "original_form": "저는 그냥 집에서 있는 것도 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100026768.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100026768.1.14",
          "form": "요즘 쿠킹클래스 많던데 그런것도 데이트로 해보고싶네요",
          "original_form": "요즘 쿠킹클래스 많던데 그런것도 데이트로 해보고싶네요",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100026768.1.15",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100026768.1.16",
          "form": "종류도 다양하더래구요",
          "original_form": "종류도 다양하더래구요",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100026768.1.17",
          "form": "그런데 저는 지방에 살고 있어서 그런 쿠킹클래스 찾기가 어려워요 ㅠㅠ",
          "original_form": "그런데 저는 지방에 살고 있어서 그런 쿠킹클래스 찾기가 어려워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100026768.1.18",
          "form": "요새 쿠킹클래스 아니어도",
          "original_form": "요새 쿠킹클래스 아니어도",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100026768.1.19",
          "form": "이것저것 클래스 많더라구요 저도 지방 살아요!",
          "original_form": "이것저것 클래스 많더라구요 저도 지방 살아요!",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100026768.1.20",
          "form": "다른것도 한번 찾아봐야겠네오",
          "original_form": "다른것도 한번 찾아봐야겠네오",
