      "utterance": [
        {
          "id": "MDRW2100042896.1.1",
          "form": "최근 쇼핑하신거 잇으신가요ㅡ",
          "original_form": "최근 쇼핑하신거 잇으신가요ㅡ",
          "speaker_id": "1",
          "time": "20210907 10:59"
        },
        {
          "id": "MDRW2100042896.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 10:59"
        },
        {
          "id": "MDRW2100042896.1.3",
          "form": "저 간식샀어요ㅋㅋ 초컬릿ㅋㅋ",
          "original_form": "저 간식샀어요ㅋㅋ 초컬릿ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042896.1.4",
          "form": "뭐사셨어요?",
          "original_form": "뭐사셨어요?",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042896.1.5",
          "form": "ㅋㅋㅋㅋ귀엽네요 무슨초콜릿인가요",
          "original_form": "ㅋㅋㅋㅋ귀엽네요 무슨초콜릿인가요",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042896.1.6",
          "form": "디저트 정말 좋아하시는군요 ㅋㅋㅋㅋ",
          "original_form": "디저트 정말 좋아하시는군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042896.1.7",
          "form": "저는...머삿드라..",
          "original_form": "저는...머삿드라..",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042896.1.8",
          "form": "아 아이패드 충전케이블 고장나서 새로 사구 썬스틱샀어요",
          "original_form": "아 아이패드 충전케이블 고장나서 새로 사구 썬스틱샀어요",
          "speaker_id": "1",
          "time": "20210907 11:03"
        },
        {
          "id": "MDRW2100042896.1.9",
          "form": "ㅋㅋ 회사사람들과 나눠먹는다 생각하고ㅋㅋ 마구 삽니다",
          "original_form": "ㅋㅋ 회사사람들과 나눠먹는다 생각하고ㅋㅋ 마구 삽니다",
          "speaker_id": "2",
          "time": "20210907 11:04"
        },
        {
          "id": "MDRW2100042896.1.10",
          "form": "그리고 반지하나 찾고있어요",
          "original_form": "그리고 반지하나 찾고있어요",
          "speaker_id": "2",
          "time": "20210907 11:04"
        },
        {
          "id": "MDRW2100042896.1.11",
          "form": "데일리템",
          "original_form": "데일리템",
          "speaker_id": "2",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042896.1.12",
          "form": "반지! 예쁘죠",
          "original_form": "반지! 예쁘죠",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042896.1.13",
          "form": "그 합정 망원쪽에 잇는 샵인데",
          "original_form": "그 합정 망원쪽에 잇는 샵인데",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042896.1.14",
          "form": "네이버에서도 팔거든요? 거기반지 예뻐요",
          "original_form": "네이버에서도 팔거든요? 거기반지 예뻐요",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042896.1.15",
          "form": "이름이...하 머드라 너무 난해해서 맨날 잊어요",
          "original_form": "이름이...하 머드라 너무 난해해서 맨날 잊어요",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042896.1.16",
          "form": "아흘로뜰..? 아홀로뜰?",
          "original_form": "아흘로뜰..? 아홀로뜰?",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042896.1.17",
          "form": "아 진짜 필요한 정보예요!",
          "original_form": "아 진짜 필요한 정보예요!",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042896.1.18",
          "form": "금과 은 다 파는거죠?",
          "original_form": "금과 은 다 파는거죠?",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042896.1.19",
          "form": "금은 없구 은이랑",
          "original_form": "금은 없구 은이랑",
          "speaker_id": "1",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042896.1.20",
          "form": "그 원석반지들인데",
          "original_form": "그 원석반지들인데",
          "speaker_id": "1",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042896.1.21",
          "form": "예쁜거많아요! 한번 둘러보세요",
          "original_form": "예쁜거많아요! 한번 둘러보세요",
          "speaker_id": "1",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042896.1.22",
          "form": "저는 매장가서 하나 겟했답니다 ㅎ",
          "original_form": "저는 매장가서 하나 겟했답니다 ㅎ",
          "speaker_id": "1",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042896.1.23",
          "form": "아 금도 잇었던고같다",
          "original_form": "아 금도 잇었던고같다",
          "speaker_id": "1",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042896.1.24",
          "form": "아 은 부담없이 끼려고요!  저 요즘 손에 주렁주렁 끼고 다닙니다",
          "original_form": "아 은 부담없이 끼려고요!  저 요즘 손에 주렁주렁 끼고 다닙니다",
          "speaker_id": "2",
          "time": "20210907 11:12"
        },
        {
          "id": "MDRW2100042896.1.25",
          "form": "화장안하고 손에 직중",
          "original_form": "화장안하고 손에 직중",
          "speaker_id": "2",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042896.1.26",
          "form": "집중",
          "original_form": "집중",
          "speaker_id": "2",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042896.1.27",
          "form": "ㅋㅋㅋ그져 마스크끼니까",
          "original_form": "ㅋㅋㅋ그져 마스크끼니까",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042896.1.28",
          "form": "저는 선크림만 바루고 다닌지 오래",
          "original_form": "저는 선크림만 바루고 다닌지 오래",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042896.1.29",
          "form": "반지 레이어드해서 끼는거 예뻐요",
          "original_form": "반지 레이어드해서 끼는거 예뻐요",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042896.1.30",
          "form": "네네 맞아요 한손에 세개씩 낍니다ㅋㅋ",
          "original_form": "네네 맞아요 한손에 세개씩 낍니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:14"
        },
        {
          "id": "MDRW2100042896.1.31",
          "form": "ㅋㅋㅋㅋ궁금해요 구경하고싶어요",
          "original_form": "ㅋㅋㅋㅋ궁금해요 구경하고싶어요",
          "speaker_id": "1",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042896.1.32",
          "form": "길가다 예쁜거 있음 사서 끼시는건가요? 아님 좋아하시는 브랜드가 잇나요",
          "original_form": "길가다 예쁜거 있음 사서 끼시는건가요? 아님 좋아하시는 브랜드가 잇나요",
          "speaker_id": "1",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042896.1.33",
          "form": "길에서 주로ㅋㅋ 잘 잃어버려서요ㅜ",
          "original_form": "길에서 주로ㅋㅋ 잘 잃어버려서요ㅜ",
          "speaker_id": "2",
          "time": "20210907 11:22"
        },
        {
          "id": "MDRW2100042896.1.34",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042896.1.35",
          "form": "어쩌다 떨어진대욬ㅋㅋㅋ",
          "original_form": "어쩌다 떨어진대욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042896.1.36",
          "form": "반지를 안빼니까 그쪽 살이 빠져서 훌렁",
          "original_form": "반지를 안빼니까 그쪽 살이 빠져서 훌렁",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042896.1.37",
          "form": "빠져요ㅜㅠ",
          "original_form": "빠져요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 11:33"
        }
      ]
    }
  ]
