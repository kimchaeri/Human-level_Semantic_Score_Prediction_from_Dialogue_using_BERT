      "utterance": [
        {
          "id": "MDRW2100016455.1.1",
          "form": "취업하셨나요",
          "original_form": "취업하셨나요",
          "speaker_id": "1",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016455.1.2",
          "form": "네 근무중입니다",
          "original_form": "네 근무중입니다",
          "speaker_id": "2",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016455.1.3",
          "form": "아 일이 바쁘신가요",
          "original_form": "아 일이 바쁘신가요",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016455.1.4",
          "form": "아녀 바쁘진않아요",
          "original_form": "아녀 바쁘진않아요",
          "speaker_id": "2",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016455.1.5",
          "form": "근무중이세요?",
          "original_form": "근무중이세요?",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016455.1.6",
          "form": "네 월루중ㅋㅋㅋ 거기 오래다니셨어요?",
          "original_form": "네 월루중ㅋㅋㅋ 거기 오래다니셨어요?",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016455.1.7",
          "form": "네 저도 월급루팡이에요",
          "original_form": "네 저도 월급루팡이에요",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016455.1.8",
          "form": "오래됐어요",
          "original_form": "오래됐어요",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016455.1.9",
          "form": "오래된분들 존경스러워요",
          "original_form": "오래된분들 존경스러워요",
          "speaker_id": "1",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100016455.1.10",
          "form": "전계약직ㅠ",
          "original_form": "전계약직ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100016455.1.11",
          "form": "에고ㅜㅜ",
          "original_form": "에고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016455.1.12",
          "form": "스트레스많으시겟어요ㅜㅠ",
          "original_form": "스트레스많으시겟어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016455.1.13",
          "form": "끝나면 이직해야하는데 쉽지않아요",
          "original_form": "끝나면 이직해야하는데 쉽지않아요",
          "speaker_id": "1",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016455.1.14",
          "form": "정규직이 짱이에요",
          "original_form": "정규직이 짱이에요",
          "speaker_id": "1",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016455.1.15",
          "form": "맞아요ㅜㅜ그렇다고들엇어요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ그렇다고들엇어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016455.1.16",
          "form": "지금 일하시는데 가장 스트레스 받는건 뭐에요?",
          "original_form": "지금 일하시는데 가장 스트레스 받는건 뭐에요?",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016455.1.17",
          "form": "근무환경이 좋아서 스트레스 없어요",
          "original_form": "근무환경이 좋아서 스트레스 없어요",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016455.1.18",
          "form": "원래도 스트레스받는성격도 아니공",
          "original_form": "원래도 스트레스받는성격도 아니공",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016455.1.19",
          "form": "오 진짜 부럽네요",
          "original_form": "오 진짜 부럽네요",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016455.1.20",
          "form": "재택도하세요?",
          "original_form": "재택도하세요?",
          "speaker_id": "1",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016455.1.21",
          "form": "저는 재택이랑 거리먼 직종이라",
          "original_form": "저는 재택이랑 거리먼 직종이라",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016455.1.22",
          "form": "꿈같은이야기 ㅎㅎ",
          "original_form": "꿈같은이야기 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016455.1.23",
          "form": "재택중이세요?",
          "original_form": "재택중이세요?",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016455.1.24",
          "form": "아하 전 재택중이에요",
          "original_form": "아하 전 재택중이에요",
          "speaker_id": "1",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100016455.1.25",
          "form": "계약직에 어려운일이 아니라서 쥐꼬리만큼 받고 재택중ㅋㅋ",
          "original_form": "계약직에 어려운일이 아니라서 쥐꼬리만큼 받고 재택중ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100016455.1.26",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100016455.1.27",
          "form": "계약기간 다되가세요ㅜ?",
          "original_form": "계약기간 다되가세요ㅜ?",
          "speaker_id": "2",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100016455.1.28",
          "form": "아뇨 아직 일년 남았어요",
          "original_form": "아뇨 아직 일년 남았어요",
          "speaker_id": "1",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100016455.1.29",
          "form": "그동안 차근차근 준비해야죠",
          "original_form": "그동안 차근차근 준비해야죠",
          "speaker_id": "1",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016455.1.30",
          "form": "오 그럼 준비기간은 있네요 ㅎㅎ",
          "original_form": "오 그럼 준비기간은 있네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016455.1.31",
          "form": "스펙쌓구",
          "original_form": "스펙쌓구",
          "speaker_id": "1",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016455.1.32",
          "form": "분명 좋은곳이 잇을거에요",
          "original_form": "분명 좋은곳이 잇을거에요",
          "speaker_id": "2",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016455.1.33",
          "form": "네네 정년까지 다닐수있는직장으로!",
          "original_form": "네네 정년까지 다닐수있는직장으로!",
          "speaker_id": "1",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016455.1.34",
          "form": "ㅎㅎ화이팅하세요",
          "original_form": "ㅎㅎ화이팅하세요",
          "speaker_id": "2",
          "time": "20210826 13:01"
        }
      ]
    }
