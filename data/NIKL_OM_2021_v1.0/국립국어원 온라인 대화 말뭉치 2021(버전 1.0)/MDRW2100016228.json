      "utterance": [
        {
          "id": "MDRW2100016228.1.1",
          "form": "요즘 하는 운동있으세요?",
          "original_form": "요즘 하는 운동있으세요?",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100016228.1.2",
          "form": "유튜브보며 홈트해요 가끔",
          "original_form": "유튜브보며 홈트해요 가끔",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100016228.1.3",
          "form": "어떤 유튜버 보시나요?",
          "original_form": "어떤 유튜버 보시나요?",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100016228.1.4",
          "form": "땅끄부부랑 스미홈트요",
          "original_form": "땅끄부부랑 스미홈트요",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100016228.1.5",
          "form": "저는 빅씨스랑 자세요정?봐요",
          "original_form": "저는 빅씨스랑 자세요정?봐요",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100016228.1.6",
          "form": "자세요정 이름 재밌네요ㅎ",
          "original_form": "자세요정 이름 재밌네요ㅎ",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100016228.1.7",
          "form": "차분하시더라고요",
          "original_form": "차분하시더라고요",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100016228.1.8",
          "form": "저도 자세가 좋지않은데 도움되겠어요",
          "original_form": "저도 자세가 좋지않은데 도움되겠어요",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100016228.1.9",
          "form": "빅씨스는 반복이 없어서 좋아요",
          "original_form": "빅씨스는 반복이 없어서 좋아요",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100016228.1.10",
          "form": "거북목에 좋다고해서 따라하고있어요",
          "original_form": "거북목에 좋다고해서 따라하고있어요",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100016228.1.11",
          "form": "오 좋네요ㅎㅎ",
          "original_form": "오 좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100016228.1.12",
          "form": "네 똑같은 동작으로 한세트 더하자고하면 거의 안따라하고 종료해서ㅎㅎ",
          "original_form": "네 똑같은 동작으로 한세트 더하자고하면 거의 안따라하고 종료해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100016228.1.13",
          "form": "앗ㅋㅋ 땅끄부부는 두번 세번해요ㅋ",
          "original_form": "앗ㅋㅋ 땅끄부부는 두번 세번해요ㅋ",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100016228.1.14",
          "form": "쉽고 한세트가 짧아서 하게되네요",
          "original_form": "쉽고 한세트가 짧아서 하게되네요",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100016228.1.15",
          "form": "ㅋㅋ저도 땅끄 몇번 해보긴했는데ㅋㅋ",
          "original_form": "ㅋㅋ저도 땅끄 몇번 해보긴했는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100016228.1.16",
          "form": "스미는 제 기준 너무 힘들어요ㅋ",
          "original_form": "스미는 제 기준 너무 힘들어요ㅋ",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100016228.1.17",
          "form": "아 허벅지 마사지 몇번해봤죠... 완전 땡겨요",
          "original_form": "아 허벅지 마사지 몇번해봤죠... 완전 땡겨요",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100016228.1.18",
          "form": "근데 많이 먹어서 살은 안빠져요ㅠ",
          "original_form": "근데 많이 먹어서 살은 안빠져요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100016228.1.19",
          "form": "ㅋ건강해지려고 하죠ㅋㅋ",
          "original_form": "ㅋ건강해지려고 하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:24"
        }
