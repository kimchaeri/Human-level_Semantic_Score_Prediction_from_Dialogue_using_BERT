      "utterance": [
        {
          "id": "MDRW2100025045.1.1",
          "form": "요즘은 간단하게 먹을수있는게 좋더라구요",
          "original_form": "요즘은 간단하게 먹을수있는게 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025045.1.2",
          "form": "오호 요즘 꽂힌 간편음식 있으세요?",
          "original_form": "오호 요즘 꽂힌 간편음식 있으세요?",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025045.1.3",
          "form": "전..밀키트..ㅋㅋㅋ",
          "original_form": "전..밀키트..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025045.1.4",
          "form": "헉 저는 밀키트 아직 한 번도 안 써봤는데",
          "original_form": "헉 저는 밀키트 아직 한 번도 안 써봤는데",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025045.1.5",
          "form": "어떤 거 먹어보셨어요?",
          "original_form": "어떤 거 먹어보셨어요?",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025045.1.6",
          "form": "요리를 잘 못해서요ㅎㅎ",
          "original_form": "요리를 잘 못해서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025045.1.7",
          "form": "애슐리 빠네파스타 맛있어요",
          "original_form": "애슐리 빠네파스타 맛있어요",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025045.1.8",
          "form": "ㅋㅋㅋ저도 잘은 못 하는데 그냥 얼렁뚱땅 만들어요",
          "original_form": "ㅋㅋㅋ저도 잘은 못 하는데 그냥 얼렁뚱땅 만들어요",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025045.1.9",
          "form": "우와 빠네파스타면 빵도 들어있는건가요",
          "original_form": "우와 빠네파스타면 빵도 들어있는건가요",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025045.1.10",
          "form": "네네 빵도 있어요ㅋㅋ",
          "original_form": "네네 빵도 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025045.1.11",
          "form": "올리브유도 있고ㅎㅎ",
          "original_form": "올리브유도 있고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025045.1.12",
          "form": "다 들어있답니당",
          "original_form": "다 들어있답니당",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025045.1.13",
          "form": "와 사진 보고 왔는데 대박 맛있을 것 같아요",
          "original_form": "와 사진 보고 왔는데 대박 맛있을 것 같아요",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025045.1.14",
          "form": "근데 빵이 들어있으면 에어프라이어같은 곳에",
          "original_form": "근데 빵이 들어있으면 에어프라이어같은 곳에",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025045.1.15",
          "form": "한번 더 구워야 하나요..?",
          "original_form": "한번 더 구워야 하나요..?",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025045.1.16",
          "form": "네 맛있어용 ㅎㅎ와와닭갈비라고 춘천닭갈비도 좋아용",
          "original_form": "네 맛있어용 ㅎㅎ와와닭갈비라고 춘천닭갈비도 좋아용",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025045.1.17",
          "form": "에어프라이어하거나 없으면 후라이팬에 해도 돼요",
          "original_form": "에어프라이어하거나 없으면 후라이팬에 해도 돼요",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025045.1.18",
          "form": "오 다행이네요 에어프라이어 산다고 해 놓고 맨날 미루고 있거든요(...)",
          "original_form": "오 다행이네요 에어프라이어 산다고 해 놓고 맨날 미루고 있거든요(...)",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025045.1.19",
          "form": "와와닭갈비도 밀키트군요!",
          "original_form": "와와닭갈비도 밀키트군요!",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025045.1.20",
          "form": "닭갈비도 너무 맛있죠 밥 한그릇 뚝딱이예요",
          "original_form": "닭갈비도 너무 맛있죠 밥 한그릇 뚝딱이예요",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025045.1.21",
          "form": "밀키트는 아니지만ㅋㅋ그냥 굽기만하면 돼서 편해요ㅎㅎ",
          "original_form": "밀키트는 아니지만ㅋㅋ그냥 굽기만하면 돼서 편해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025045.1.22",
          "form": "저도 닭갈비 엄청 좋아해서ㅎㅎ",
          "original_form": "저도 닭갈비 엄청 좋아해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025045.1.23",
          "form": "아아 하나로 그냥 포장해서 나오는 거얐군요ㅋㅋㅋ",
          "original_form": "아아 하나로 그냥 포장해서 나오는 거얐군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025045.1.24",
          "form": "저도 닭갈비 완전 사랑해요",
          "original_form": "저도 닭갈비 완전 사랑해요",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025045.1.25",
          "form": "특히 거기 들어가는 고구마랑 떡이 너무 좋아요",
          "original_form": "특히 거기 들어가는 고구마랑 떡이 너무 좋아요",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025045.1.26",
          "form": "그럼 저거 한번 드셔보세요ㅋㅋ다리살로만 만든걸로요ㅎㅎ",
          "original_form": "그럼 저거 한번 드셔보세요ㅋㅋ다리살로만 만든걸로요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025045.1.27",
          "form": "꺄 감사합니다 장바구니에 넣어놓을거예요ㅋㅋㅋ",
          "original_form": "꺄 감사합니다 장바구니에 넣어놓을거예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025045.1.28",
          "form": "사리를 더 좋아하시나봐욪ㅋㅋ전 양배추 좋아해요ㅋㅋㅈ",
          "original_form": "사리를 더 좋아하시나봐욪ㅋㅋ전 양배추 좋아해요ㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025045.1.29",
          "form": "밤에 먹는 얘기하니 급 출출하네요ㅋㅋ",
          "original_form": "밤에 먹는 얘기하니 급 출출하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:51"
        }
      ]
