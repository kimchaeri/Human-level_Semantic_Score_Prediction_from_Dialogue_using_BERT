      "utterance": [
        {
          "id": "MDRW2100035617.1.1",
          "form": "그리고 전 해바라기도 좋아해요",
          "original_form": "그리고 전 해바라기도 좋아해요",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035617.1.2",
          "form": "아아 그 노래도 좋죠ㅎㅎㅎ",
          "original_form": "아아 그 노래도 좋죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035617.1.3",
          "form": "밝고 신나는 노래",
          "original_form": "밝고 신나는 노래",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035617.1.4",
          "form": "유튜브로는 주로 어떤 컨텐츠 보세용?",
          "original_form": "유튜브로는 주로 어떤 컨텐츠 보세용?",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100035617.1.5",
          "form": "저는 개랑 고양이..포레스텔라...요즘 유행하는 스포츠 스타들 봐요",
          "original_form": "저는 개랑 고양이..포레스텔라...요즘 유행하는 스포츠 스타들 봐요",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100035617.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100035617.1.7",
          "form": "저는 무한도전 오분순삭이랑",
          "original_form": "저는 무한도전 오분순삭이랑",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100035617.1.8",
          "form": "mbti 컨텐츠 동영상들 봐요ㅎㅎㅎ",
          "original_form": "mbti 컨텐츠 동영상들 봐요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100035617.1.9",
          "form": "ㅋㅋㅋ그거 웃기죠",
          "original_form": "ㅋㅋㅋ그거 웃기죠",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100035617.1.10",
          "form": "네네 가볍기보기 좋더라구요",
          "original_form": "네네 가볍기보기 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100035617.1.11",
          "form": "오 엠비티아이 좋아하시나바요",
          "original_form": "오 엠비티아이 좋아하시나바요",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100035617.1.12",
          "form": "과몰입러에요ㅠㅠㅋㅋㅋ",
          "original_form": "과몰입러에요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100035617.1.13",
          "form": "ㅋㅋㅋㅋ그거 재밌죠 저도 좋아합니다",
          "original_form": "ㅋㅋㅋㅋ그거 재밌죠 저도 좋아합니다",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035617.1.14",
          "form": "고양이는 어느분 보세용??",
          "original_form": "고양이는 어느분 보세용??",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035617.1.15",
          "form": "저 똥괭이라고",
          "original_form": "저 똥괭이라고",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035617.1.16",
          "form": "23마리 키우시는분 계세요",
          "original_form": "23마리 키우시는분 계세요",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035617.1.17",
          "form": "헐 첨 들어보는데 이름 넘 귀엽네요ㅋㅋㅋ",
          "original_form": "헐 첨 들어보는데 이름 넘 귀엽네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035617.1.18",
          "form": "바로 유튜브 채널 구독하러 갑니당",
          "original_form": "바로 유튜브 채널 구독하러 갑니당",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035617.1.19",
          "form": "다 구조해서 기르시는거라서",
          "original_form": "다 구조해서 기르시는거라서",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035617.1.20",
          "form": "아하ㅠㅠ 좋은일 하시네요",
          "original_form": "아하ㅠㅠ 좋은일 하시네요",
