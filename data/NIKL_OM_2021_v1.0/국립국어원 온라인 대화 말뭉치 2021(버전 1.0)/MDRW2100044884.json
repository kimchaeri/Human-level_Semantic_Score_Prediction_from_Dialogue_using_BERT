      "utterance": [
        {
          "id": "MDRW2100044884.1.1",
          "form": "이번엔 제 아침메뉴 맞춰보실래요!?!?!",
          "original_form": "이번엔 제 아침메뉴 맞춰보실래요!?!?!",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044884.1.2",
          "form": "넵 좋아요~",
          "original_form": "넵 좋아요~",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044884.1.3",
          "form": "익힌음식인가요?",
          "original_form": "익힌음식인가요?",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044884.1.4",
          "form": "네! 익힌 음식이예요",
          "original_form": "네! 익힌 음식이예요",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044884.1.5",
          "form": "밀가루가 들어가나요?",
          "original_form": "밀가루가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044884.1.6",
          "form": "아니요! 밀가루 전혀 안 들어간답니다 후후",
          "original_form": "아니요! 밀가루 전혀 안 들어간답니다 후후",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044884.1.7",
          "form": "계란이 들어가나요?",
          "original_form": "계란이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044884.1.8",
          "form": "아니요 계란도 안 들어가요!",
          "original_form": "아니요 계란도 안 들어가요!",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044884.1.9",
          "form": "힌트 하나 드릴게요! 너무 황당한 답인 ㄱ것 같아서",
          "original_form": "힌트 하나 드릴게요! 너무 황당한 답인 ㄱ것 같아서",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044884.1.10",
          "form": "넵ㅋㅋㅋ 좋아요",
          "original_form": "넵ㅋㅋㅋ 좋아요",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044884.1.11",
          "form": "단일 식품이예요 재료 딱 하나!",
          "original_form": "단일 식품이예요 재료 딱 하나!",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044884.1.12",
          "form": "익혔는데 계란후라이가 아니라니",
          "original_form": "익혔는데 계란후라이가 아니라니",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044884.1.13",
          "form": "쌀이 들어가나요?",
          "original_form": "쌀이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044884.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ계란후라이 아니예요 아깝지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ계란후라이 아니예요 아깝지만",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044884.1.15",
          "form": "아니요 쌀 전혀 안 들어가요",
          "original_form": "아니요 쌀 전혀 안 들어가요",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044884.1.16",
          "form": "고기는 들어가나요?",
          "original_form": "고기는 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044884.1.17",
          "form": "아뇨! 다른 재료 하나도 안 들어가고 오로지 이 재료만 써요",
          "original_form": "아뇨! 다른 재료 하나도 안 들어가고 오로지 이 재료만 써요",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044884.1.18",
          "form": "우유 들어가나요?",
          "original_form": "우유 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044884.1.19",
          "form": "이 요리에 우유는 안 넣지만 이걸 우유랑 곁들여 먹기도 해요",
          "original_form": "이 요리에 우유는 안 넣지만 이걸 우유랑 곁들여 먹기도 해요",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044884.1.20",
          "form": "양식인가요?",
          "original_form": "양식인가요?",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044884.1.21",
          "form": "전국 공통으로 먹는 거예요ㅋㅋㅋㅋㅋ",
          "original_form": "전국 공통으로 먹는 거예요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044884.1.22",
          "form": "우리나라요?",
          "original_form": "우리나라요?",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044884.1.23",
          "form": "아뇨 전 세계적으로요!",
          "original_form": "아뇨 전 세계적으로요!",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044884.1.24",
          "form": "더 헷갈릴 것 같네요 ㅋㅋㅋㅋㅋ",
          "original_form": "더 헷갈릴 것 같네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:14"
        }
      ]
