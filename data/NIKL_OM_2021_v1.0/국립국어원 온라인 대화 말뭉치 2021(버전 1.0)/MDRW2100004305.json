      "utterance": [
        {
          "id": "MDRW2100004305.1.1",
          "form": "겨울에 가면 길에 귤이 막 굴러다니고 그런대ㅋㅋㅋㅋ",
          "original_form": "겨울에 가면 길에 귤이 막 굴러다니고 그런대ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004305.1.2",
          "form": "난 아쉬웠던게 돌아올때 면세점이있는지 몰랐어",
          "original_form": "난 아쉬웠던게 돌아올때 면세점이있는지 몰랐어",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004305.1.3",
          "form": "아이고 아쉽다...",
          "original_form": "아이고 아쉽다...",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004305.1.4",
          "form": "여행의 끝은 면세점인데..",
          "original_form": "여행의 끝은 면세점인데..",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004305.1.5",
          "form": "ㅋㅋㅋㅋㅋㅜㅠㅜ 기념으로 립스틱이라도살까하다가 지금 시국이시국이라 테스터도안되길래",
          "original_form": "ㅋㅋㅋㅋㅋㅜㅠㅜ 기념으로 립스틱이라도살까하다가 지금 시국이시국이라 테스터도안되길래",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004305.1.6",
          "form": "그냥 카카오프렌즈 갔다왔어.... 놀러갈때 날씨는어땠어??",
          "original_form": "그냥 카카오프렌즈 갔다왔어.... 놀러갈때 날씨는어땠어??",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004305.1.7",
          "form": "나는 제주도 놀러갔을때 여름에 갔는데",
          "original_form": "나는 제주도 놀러갔을때 여름에 갔는데",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004305.1.8",
          "form": "태풍이 왔어",
          "original_form": "태풍이 왔어",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004305.1.9",
          "form": "비행기가 결항되는 바람에 강제 +1박",
          "original_form": "비행기가 결항되는 바람에 강제 +1박",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004305.1.10",
          "form": "헉... 태풍을 뚫고 시장도왔다갔다하고",
          "original_form": "헉... 태풍을 뚫고 시장도왔다갔다하고",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004305.1.11",
          "form": "고생했겠네 ...",
          "original_form": "고생했겠네 ...",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004305.1.12",
          "form": "그래도 잊혀지진않을듯 ㅜㅜ",
          "original_form": "그래도 잊혀지진않을듯 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004305.1.13",
          "form": "또 가고싶다ㅜㅜ",
          "original_form": "또 가고싶다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004305.1.14",
          "form": "얼른 코로나 잠잠해지면 좋겠어",
          "original_form": "얼른 코로나 잠잠해지면 좋겠어",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004305.1.15",
          "form": "너는 어디 가고싶어?",
          "original_form": "너는 어디 가고싶어?",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004305.1.16",
          "form": "난 코로나잠잠해지면 해외가고싶어",
          "original_form": "난 코로나잠잠해지면 해외가고싶어",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004305.1.17",
          "form": "괌....",
          "original_form": "괌....",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004305.1.18",
          "form": "괌 휴양지로는 최고래..",
          "original_form": "괌 휴양지로는 최고래..",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004305.1.19",
          "form": "나는 동남아 가서 과일먹고싶다",
          "original_form": "나는 동남아 가서 과일먹고싶다",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004305.1.20",
          "form": "전부 한국인이라는데 난 오히려그게좋아... 와 과일 ㅜ",
          "original_form": "전부 한국인이라는데 난 오히려그게좋아... 와 과일 ㅜ",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004305.1.21",
          "form": "망고스틴 질릴때까지까먹고...",
          "original_form": "망고스틴 질릴때까지까먹고...",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004305.1.22",
          "form": "망고도 썰어먹고",
          "original_form": "망고도 썰어먹고",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004305.1.23",
          "form": "열대과일 너무 맛있어",
          "original_form": "열대과일 너무 맛있어",
          "speaker_id": "2",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004305.1.24",
          "form": "맞아 예전엔 그 자연의단맛이 싫었는데 한번 맛있다고느끼니까",
          "original_form": "맞아 예전엔 그 자연의단맛이 싫었는데 한번 맛있다고느끼니까",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004305.1.25",
          "form": "이젠 없어서못먹어 ㅜ ㅜ",
          "original_form": "이젠 없어서못먹어 ㅜ ㅜ",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004305.1.26",
          "form": "우리나라 과일도 맛있긴 하지만",
          "original_form": "우리나라 과일도 맛있긴 하지만",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004305.1.27",
          "form": "외국 놀러갔을때 그나라 과일 먹으면",
          "original_form": "외국 놀러갔을때 그나라 과일 먹으면",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004305.1.28",
          "form": "여행 때문에 들뜬 마음까지 더해져서",
          "original_form": "여행 때문에 들뜬 마음까지 더해져서",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004305.1.29",
          "form": "두배로 맛있는것 같아",
          "original_form": "두배로 맛있는것 같아",
          "speaker_id": "2",
          "time": "20210821 16:08"
        }
      ]
