      "utterance": [
        {
          "id": "MDRW2100000964.1.1",
          "form": "영화좋아하나요?",
          "original_form": "영화좋아하나요?",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000964.1.2",
          "form": "네 넷플릭스로 종종 봐요",
          "original_form": "네 넷플릭스로 종종 봐요",
          "speaker_id": "2",
          "time": "20210819 20:57"
        },
        {
          "id": "MDRW2100000964.1.3",
          "form": "어떤거보세요?",
          "original_form": "어떤거보세요?",
          "speaker_id": "1",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000964.1.4",
          "form": "요즘 본건 범죄의 재구성",
          "original_form": "요즘 본건 범죄의 재구성",
          "speaker_id": "2",
          "time": "20210819 20:59"
        },
        {
          "id": "MDRW2100000964.1.5",
          "form": "봤어요",
          "original_form": "봤어요",
          "speaker_id": "2",
          "time": "20210819 20:59"
        },
        {
          "id": "MDRW2100000964.1.6",
          "form": "오오 재미있나요?",
          "original_form": "오오 재미있나요?",
          "speaker_id": "1",
          "time": "20210819 20:59"
        },
        {
          "id": "MDRW2100000964.1.7",
          "form": "네 미치는줄알았습니다",
          "original_form": "네 미치는줄알았습니다",
          "speaker_id": "2",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000964.1.8",
          "form": "무슨내용이예요?",
          "original_form": "무슨내용이예요?",
          "speaker_id": "1",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000964.1.9",
          "form": "여기 한국은행이 털렸습니다",
          "original_form": "여기 한국은행이 털렸습니다",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000964.1.10",
          "form": "범죄자도 있죠",
          "original_form": "범죄자도 있죠",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000964.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000964.1.12",
          "form": "재밌겠네요 내용이",
          "original_form": "재밌겠네요 내용이",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000964.1.13",
          "form": "하지만 영화는 한국은행이 털린 후부터 시작합니다",
          "original_form": "하지만 영화는 한국은행이 털린 후부터 시작합니다",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000964.1.14",
          "form": "그리고 역행해요",
          "original_form": "그리고 역행해요",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000964.1.15",
          "form": "요호 저 그런 장르 좋아요",
          "original_form": "요호 저 그런 장르 좋아요",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000964.1.16",
          "form": "꼭 보세요 멋진 영화입니다",
          "original_form": "꼭 보세요 멋진 영화입니다",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000964.1.17",
          "form": "내일 봐야겠어요!",
          "original_form": "내일 봐야겠어요!",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000964.1.18",
          "form": "저는 최근에 정글크루즈영화보고왔어요!",
          "original_form": "저는 최근에 정글크루즈영화보고왔어요!",
          "speaker_id": "1",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000964.1.19",
          "form": "우와 한번 봐야겠어요 어떤영화인가요",
          "original_form": "우와 한번 봐야겠어요 어떤영화인가요",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000964.1.20",
          "form": "디스니 영화인데 정글에서 크루즈타고 모험하는이야기예요",
          "original_form": "디스니 영화인데 정글에서 크루즈타고 모험하는이야기예요",
          "speaker_id": "1",
          "time": "20210819 21:06"
        }
      ]
