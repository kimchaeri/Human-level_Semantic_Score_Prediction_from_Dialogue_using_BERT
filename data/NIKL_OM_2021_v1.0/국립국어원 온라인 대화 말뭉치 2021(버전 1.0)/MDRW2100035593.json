      "utterance": [
        {
          "id": "MDRW2100035593.1.1",
          "form": "어떤 프로 재밌게 보시나요?",
          "original_form": "어떤 프로 재밌게 보시나요?",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100035593.1.2",
          "form": "저 지금 갯마을차차차 보는중요",
          "original_form": "저 지금 갯마을차차차 보는중요",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100035593.1.3",
          "form": "어제부터 시작했는데 재밌네요 ㅎㅎ",
          "original_form": "어제부터 시작했는데 재밌네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100035593.1.4",
          "form": "어떤 내용인가요??",
          "original_form": "어떤 내용인가요??",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035593.1.5",
          "form": "전에 홍반장이라는 영화 아시나요?",
          "original_form": "전에 홍반장이라는 영화 아시나요?",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035593.1.6",
          "form": "들어본 것 같아여!",
          "original_form": "들어본 것 같아여!",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035593.1.7",
          "form": "그 영화 리메이크한 드라마예요",
          "original_form": "그 영화 리메이크한 드라마예요",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035593.1.8",
          "form": "한 번 봐야겠네요ㅋㅋ 내용이 많이 다른가요?",
          "original_form": "한 번 봐야겠네요ㅋㅋ 내용이 많이 다른가요?",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035593.1.9",
          "form": "아뇨 거의 똑같아요 ㅋㅋ",
          "original_form": "아뇨 거의 똑같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035593.1.10",
          "form": "그럼 드라마를 바로 봐도 되겠네요ㅎㅎ",
          "original_form": "그럼 드라마를 바로 봐도 되겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035593.1.11",
          "form": "네 드라마도 꽤 재밌네요",
          "original_form": "네 드라마도 꽤 재밌네요",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035593.1.12",
          "form": "넷플로는 안 보시나요?",
          "original_form": "넷플로는 안 보시나요?",
          "speaker_id": "1",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035593.1.13",
          "form": "네 보통 본방송을 봐요 ㅋㅋㅋ",
          "original_form": "네 보통 본방송을 봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035593.1.14",
          "form": "저는 넷플을 자주 보게 되더라두요",
          "original_form": "저는 넷플을 자주 보게 되더라두요",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035593.1.15",
          "form": "저는 프리미엄 끊었다 넘 안봐서 해지했어요 ㅎㅎ",
          "original_form": "저는 프리미엄 끊었다 넘 안봐서 해지했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:22"
