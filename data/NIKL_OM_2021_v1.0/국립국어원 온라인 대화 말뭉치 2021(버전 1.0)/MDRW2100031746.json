      "utterance": [
        {
          "id": "MDRW2100031746.1.1",
          "form": "뭐 구매하고싶은 물건 있어요 ?",
          "original_form": "뭐 구매하고싶은 물건 있어요 ?",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031746.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031746.1.3",
          "form": "있습니다",
          "original_form": "있습니다",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031746.1.4",
          "form": "어떤~?",
          "original_form": "어떤~?",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031746.1.5",
          "form": "저 반지요!",
          "original_form": "저 반지요!",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031746.1.6",
          "form": "반지 ㅎㅎ",
          "original_form": "반지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100031746.1.7",
          "form": "금반지? 은반지 ?",
          "original_form": "금반지? 은반지 ?",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031746.1.8",
          "form": "금인지 은인지 모르겠어요ㅋㅋ",
          "original_form": "금인지 은인지 모르겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031746.1.9",
          "form": "명품반지였어요ㅋㅋㅋ",
          "original_form": "명품반지였어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031746.1.10",
          "form": "아 명품반지 ㅎㅎㅎ",
          "original_form": "아 명품반지 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100031746.1.11",
          "form": "비싸요 ?",
          "original_form": "비싸요 ?",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031746.1.12",
          "form": "제 능력으로는 아직좀비싸네요ㅠ",
          "original_form": "제 능력으로는 아직좀비싸네요ㅠ",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031746.1.13",
          "form": "살수있을거에요",
          "original_form": "살수있을거에요",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031746.1.14",
          "form": "네 꼭살겁니다!",
          "original_form": "네 꼭살겁니다!",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031746.1.15",
          "form": "반지를 원래 좋아해요?",
          "original_form": "반지를 원래 좋아해요?",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031746.1.16",
          "form": "네 저 약간 까마귀라서",
          "original_form": "네 저 약간 까마귀라서",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031746.1.17",
          "form": "ㅋㄱㄱㅋㅋㅋ",
          "original_form": "ㅋㄱㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031746.1.18",
          "form": "까마귀 ㅎㅎ 반짝이는 걸 좋아하나봐요?",
          "original_form": "까마귀 ㅎㅎ 반짝이는 걸 좋아하나봐요?",
          "speaker_id": "2",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031746.1.19",
          "form": "악세서리류를 좋아해요ㅎㅎ",
          "original_form": "악세서리류를 좋아해요ㅎㅎ",
          "speaker_id": "1",
