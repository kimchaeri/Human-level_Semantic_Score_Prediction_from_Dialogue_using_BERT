      "utterance": [
        {
          "id": "MDRW2100022219.1.1",
          "form": "다이슨 드라이기요? 비싸지 않나요?",
          "original_form": "다이슨 드라이기요? 비싸지 않나요?",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022219.1.2",
          "form": "아 아니다.. 에어랩이요 거기엔 드라이기도 포함되어있어요",
          "original_form": "아 아니다.. 에어랩이요 거기엔 드라이기도 포함되어있어요",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022219.1.3",
          "form": "에어랩은 뭐죠?",
          "original_form": "에어랩은 뭐죠?",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022219.1.4",
          "form": "드라이기랑 고데기랑 같이 되는?거에요",
          "original_form": "드라이기랑 고데기랑 같이 되는?거에요",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022219.1.5",
          "form": "아하 더 비싸겠네요",
          "original_form": "아하 더 비싸겠네요",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022219.1.6",
          "form": "싸진 않더라구요ㅠㅠ",
          "original_form": "싸진 않더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022219.1.7",
          "form": "다이슨은 다 기본적으로 비싸더라고요",
          "original_form": "다이슨은 다 기본적으로 비싸더라고요",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022219.1.8",
          "form": "네네 한 50만원 넘었던 거 같아요",
          "original_form": "네네 한 50만원 넘었던 거 같아요",
          "speaker_id": "1",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022219.1.9",
          "form": "어머 세상에...",
          "original_form": "어머 세상에...",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022219.1.10",
          "form": "그런데 산 사람들 보면 다들 만족하더라구요",
          "original_form": "그런데 산 사람들 보면 다들 만족하더라구요",
          "speaker_id": "1",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022219.1.11",
          "form": "아... 안좋으면 안될거 같긴 하지만",
          "original_form": "아... 안좋으면 안될거 같긴 하지만",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022219.1.12",
          "form": "그건 그렇네요 ㅋㅋㅋ일반드라이기의 10배 가격이니",
          "original_form": "그건 그렇네요 ㅋㅋㅋ일반드라이기의 10배 가격이니",
          "speaker_id": "1",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022219.1.13",
          "form": "그러게 말이에요 ㅎㅎ",
          "original_form": "그러게 말이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022219.1.14",
          "form": "저는 최근 일본 중고거래에 빠졌어요",
