      "utterance": [
        {
          "id": "MDRW2100036591.1.1",
          "form": "저녁은 모드셨어요?",
          "original_form": "저녁은 모드셨어요?",
          "speaker_id": "2",
          "time": "20210831 01:20"
        },
        {
          "id": "MDRW2100036591.1.2",
          "form": "밥이랑 고등어먹었어요",
          "original_form": "밥이랑 고등어먹었어요",
          "speaker_id": "1",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100036591.1.3",
          "form": "어 저도 오늘 저녁 고등어",
          "original_form": "어 저도 오늘 저녁 고등어",
          "speaker_id": "2",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100036591.1.4",
          "form": "같은 메뉴를 드셨군용 ㅎㅎ",
          "original_form": "같은 메뉴를 드셨군용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100036591.1.5",
          "form": "어머 반갑네요",
          "original_form": "어머 반갑네요",
          "speaker_id": "1",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100036591.1.6",
          "form": "오랜만에 먹으니까 맛있더라구요",
          "original_form": "오랜만에 먹으니까 맛있더라구요",
          "speaker_id": "2",
          "time": "20210831 01:21"
        },
        {
          "id": "MDRW2100036591.1.7",
          "form": "네 저도 오랜만에 먹었어요",
          "original_form": "네 저도 오랜만에 먹었어요",
          "speaker_id": "1",
          "time": "20210831 01:22"
        },
        {
          "id": "MDRW2100036591.1.8",
          "form": "생선 냄새나고 기름 튀어서 잘 안먹었는데 애랑 신랑이 좋아해서 오랜만에 ㅎㅎ",
          "original_form": "생선 냄새나고 기름 튀어서 잘 안먹었는데 애랑 신랑이 좋아해서 오랜만에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 01:22"
        },
        {
          "id": "MDRW2100036591.1.9",
          "form": "맞아요 요리하기에 은근 애로점이 있어요",
          "original_form": "맞아요 요리하기에 은근 애로점이 있어요",
          "speaker_id": "1",
          "time": "20210831 01:23"
        },
        {
          "id": "MDRW2100036591.1.10",
          "form": "그쵸 튀김류나 생선이 집에서 하기는 제일 좀 그래요 삼겹살도 ㅋㅋ",
          "original_form": "그쵸 튀김류나 생선이 집에서 하기는 제일 좀 그래요 삼겹살도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:25"
        },
        {
          "id": "MDRW2100036591.1.11",
          "form": "그니까요 제 친구중에 맨날 아빠가",
          "original_form": "그니까요 제 친구중에 맨날 아빠가",
          "speaker_id": "1",
          "time": "20210831 01:27"
        },
        {
          "id": "MDRW2100036591.1.12",
          "form": "집에서 삼겹살 하라도해서 싫다는 친구 있었어요",
          "original_form": "집에서 삼겹살 하라도해서 싫다는 친구 있었어요",
          "speaker_id": "1",
          "time": "20210831 01:27"
        },
        {
          "id": "MDRW2100036591.1.13",
          "form": "하라고",
          "original_form": "하라고",
          "speaker_id": "1",
          "time": "20210831 01:27"
        },
        {
          "id": "MDRW2100036591.1.14",
          "form": "저희신랑도 집에서 굽는거 진짜 싫어해요ㅠㅠ 바닥이 미끌 ㅠㅠ",
          "original_form": "저희신랑도 집에서 굽는거 진짜 싫어해요ㅠㅠ 바닥이 미끌 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 01:34"
        },
        {
          "id": "MDRW2100036591.1.15",
          "form": "기름이 그렇게 떠다녀서",
          "original_form": "기름이 그렇게 떠다녀서",
          "speaker_id": "1",
          "time": "20210831 01:35"
        },
        {
          "id": "MDRW2100036591.1.16",
          "form": "바닥까지 가는거보면 참 신기해요",
          "original_form": "바닥까지 가는거보면 참 신기해요",
          "speaker_id": "1",
          "time": "20210831 01:35"
        },
        {
          "id": "MDRW2100036591.1.17",
          "form": "발에 묻고 그걸로 다니면 온집이..ㅋㅋ",
          "original_form": "발에 묻고 그걸로 다니면 온집이..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:36"
        }
      ]
