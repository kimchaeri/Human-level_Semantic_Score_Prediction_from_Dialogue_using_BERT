      "utterance": [
        {
          "id": "MDRW2100019243.1.1",
          "form": "올림픽때 여자배구 보셨어요?",
          "original_form": "올림픽때 여자배구 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019243.1.2",
          "form": "제가 보면 경기가 지더라구요ㅠㅠ 보다안보다했네요",
          "original_form": "제가 보면 경기가 지더라구요ㅠㅠ 보다안보다했네요",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019243.1.3",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019243.1.4",
          "form": "올림픽 개최가 경제에 도움이 될까요",
          "original_form": "올림픽 개최가 경제에 도움이 될까요",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019243.1.5",
          "form": "개최국은 도움이 많이되겠죠!",
          "original_form": "개최국은 도움이 많이되겠죠!",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019243.1.6",
          "form": "저는 좀 손해보지 않을까 싶어서",
          "original_form": "저는 좀 손해보지 않을까 싶어서",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019243.1.7",
          "form": "도쿄올림픽만 봐도 그런것 같아서요",
          "original_form": "도쿄올림픽만 봐도 그런것 같아서요",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019243.1.8",
          "form": "올해는 악영향이 많죠~",
          "original_form": "올해는 악영향이 많죠~",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019243.1.9",
          "form": "확진자도 너무많이나오고..",
          "original_form": "확진자도 너무많이나오고..",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019243.1.10",
          "form": "네네 그런것 같아쇼",
          "original_form": "네네 그런것 같아쇼",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019243.1.11",
          "form": "좋아하는 스포츠 있어요?",
          "original_form": "좋아하는 스포츠 있어요?",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019243.1.12",
          "form": "강행한 일본정부도 대단한서같아요 전",
          "original_form": "강행한 일본정부도 대단한서같아요 전",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019243.1.13",
          "form": "농구야구보는거!",
          "original_form": "농구야구보는거!",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019243.1.14",
          "form": "아 농구랑 야구",
          "original_form": "아 농구랑 야구",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019243.1.15",
          "form": "즐기진않지만 한번씩보면 잼있더라구요!",
          "original_form": "즐기진않지만 한번씩보면 잼있더라구요!",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019243.1.16",
          "form": "직관도 가세요?",
          "original_form": "직관도 가세요?",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019243.1.17",
          "form": "전에 서울살땐 야구장 많이갔어요 !",
          "original_form": "전에 서울살땐 야구장 많이갔어요 !",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019243.1.18",
          "form": "농구장은 가본적없네요ㅠ",
          "original_form": "농구장은 가본적없네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019243.1.19",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
