      "utterance": [
        {
          "id": "MDRW2100047564.1.1",
          "form": "씻고 애들재우고 시간되면 오세요",
          "original_form": "씻고 애들재우고 시간되면 오세요",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047564.1.2",
          "form": "무리마시고",
          "original_form": "무리마시고",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047564.1.3",
          "form": "같이 잠들만해요 ㅋㅋ넘 수고하셨어요 하루",
          "original_form": "같이 잠들만해요 ㅋㅋ넘 수고하셨어요 하루",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100047564.1.4",
          "form": "배가고파서인지",
          "original_form": "배가고파서인지",
          "speaker_id": "2",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100047564.1.5",
          "form": "잠이 안와서 탈출성공요 ㅎ",
          "original_form": "잠이 안와서 탈출성공요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100047564.1.6",
          "form": "재우고 멀팔려고 올려볼까싶어요 ㅎ",
          "original_form": "재우고 멀팔려고 올려볼까싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100047564.1.7",
          "form": "뭐 파시게요?",
          "original_form": "뭐 파시게요?",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047564.1.8",
          "form": "아직 안자요?",
          "original_form": "아직 안자요?",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047564.1.9",
          "form": "애들이 자야 사진 찍고 파는데 ㅋㅋ",
          "original_form": "애들이 자야 사진 찍고 파는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047564.1.10",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100047564.1.11",
          "form": "그런걸 애들 재우고 해야해서 ㅎ",
          "original_form": "그런걸 애들 재우고 해야해서 ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100047564.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100047564.1.13",
          "form": "가을겨울옷을 좀 올려볼까해요",
          "original_form": "가을겨울옷을 좀 올려볼까해요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100047564.1.14",
          "form": "애들꺼요",
          "original_form": "애들꺼요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100047564.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100047564.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100047564.1.17",
          "form": "애들옷은 한철용이라 ㅜㅠ",
          "original_form": "애들옷은 한철용이라 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.18",
          "form": "계절을 맞게 미리 팔아야죠",
          "original_form": "계절을 맞게 미리 팔아야죠",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.19",
          "form": "진짜 아까워요",
          "original_form": "진짜 아까워요",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.20",
          "form": "그렇죠 후딱 팔아야죠",
          "original_form": "그렇죠 후딱 팔아야죠",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.21",
          "form": "버리긴 진짜 아까워요",
          "original_form": "버리긴 진짜 아까워요",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.22",
          "form": "그래서 많이 안사는데도 선물받고",
          "original_form": "그래서 많이 안사는데도 선물받고",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.23",
          "form": "이뿌게 찍어서 오늘 수입 올려 보세요",
          "original_form": "이뿌게 찍어서 오늘 수입 올려 보세요",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.24",
          "form": "친구가 물려주고 이러면 옷이 많아져요",
          "original_form": "친구가 물려주고 이러면 옷이 많아져요",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.25",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100047564.1.26",
          "form": "좋은거죠 그때는 물려받아야죠 ㅎㅎ",
          "original_form": "좋은거죠 그때는 물려받아야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047564.1.27",
          "form": "옷도 입히는것만 입히고",
          "original_form": "옷도 입히는것만 입히고",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047564.1.28",
          "form": "책도 좀 팔아야겠네요",
          "original_form": "책도 좀 팔아야겠네요",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047564.1.29",
          "form": "그렇게돼요",
          "original_form": "그렇게돼요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047564.1.30",
          "form": "팔거 많다니 부러워요",
          "original_form": "팔거 많다니 부러워요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047564.1.31",
          "form": "한번읽은건 책장에 꽂아두고 안봐요 ㅎ",
          "original_form": "한번읽은건 책장에 꽂아두고 안봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047564.1.32",
          "form": "저도 막 팔고싶은데 ㅋㅋ",
          "original_form": "저도 막 팔고싶은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100047564.1.33",
          "form": "네 저희도요",
          "original_form": "네 저희도요",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100047564.1.34",
          "form": "다신 안봐요",
          "original_form": "다신 안봐요",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100047564.1.35",
          "form": "찾아보면 많아요 ㅎ",
          "original_form": "찾아보면 많아요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:28"
        }
      ]
    }
  ]
