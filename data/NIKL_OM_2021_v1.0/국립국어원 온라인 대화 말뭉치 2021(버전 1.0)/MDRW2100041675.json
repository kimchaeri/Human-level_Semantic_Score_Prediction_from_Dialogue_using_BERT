      "utterance": [
        {
          "id": "MDRW2100041675.1.1",
          "form": "패션 관심많으세요?",
          "original_form": "패션 관심많으세요?",
          "speaker_id": "2",
          "time": "20210906 14:20"
        },
        {
          "id": "MDRW2100041675.1.2",
          "form": "저는 크게 관심은 없는 편이예요",
          "original_form": "저는 크게 관심은 없는 편이예요",
          "speaker_id": "1",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100041675.1.3",
          "form": "패션 관심 많은 편이세요?",
          "original_form": "패션 관심 많은 편이세요?",
          "speaker_id": "1",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100041675.1.4",
          "form": "음 있지도 없지도 않은 정도요 ㅋㅋㅋㅋㅋ",
          "original_form": "음 있지도 없지도 않은 정도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041675.1.5",
          "form": "과한편은아니고 적당히 ..",
          "original_form": "과한편은아니고 적당히 ..",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041675.1.6",
          "form": "보통 어디서 옷 사세요?",
          "original_form": "보통 어디서 옷 사세요?",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041675.1.7",
          "form": "요즘엔 모아서 보는게 편해서",
          "original_form": "요즘엔 모아서 보는게 편해서",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041675.1.8",
          "form": "브랜디 지그재그 앱 주로사용해용",
          "original_form": "브랜디 지그재그 앱 주로사용해용",
          "speaker_id": "2",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041675.1.9",
          "form": "아하 저도 가끔 보긴 하는데",
          "original_form": "아하 저도 가끔 보긴 하는데",
          "speaker_id": "1",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041675.1.10",
          "form": "비슷한데 가격 천차만별 느낌이라...어렵더라구요",
          "original_form": "비슷한데 가격 천차만별 느낌이라...어렵더라구요",
          "speaker_id": "1",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041675.1.11",
          "form": "맞아용 ㅋㅋ진짜루 ㅠ심사숙고해서 지르죠..",
          "original_form": "맞아용 ㅋㅋ진짜루 ㅠ심사숙고해서 지르죠..",
          "speaker_id": "2",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041675.1.12",
          "form": "전 어려워서 맨날 비슷한 거 사는 것 같아요 ㅋㅋ",
          "original_form": "전 어려워서 맨날 비슷한 거 사는 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041675.1.13",
          "form": "가끔 새로운시도했는데 성공하면 기분좋아용 ㅋㅋ",
          "original_form": "가끔 새로운시도했는데 성공하면 기분좋아용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041675.1.14",
          "form": "ㅋㅋ최근에 뭐 사셨나여",
          "original_form": "ㅋㅋ최근에 뭐 사셨나여",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041675.1.15",
          "form": "편하게입는 원피스샀어용 ㅎㅎ 면으로된",
          "original_form": "편하게입는 원피스샀어용 ㅎㅎ 면으로된",
          "speaker_id": "2",
          "time": "20210906 18:51"
        },
        {
          "id": "MDRW2100041675.1.16",
          "form": "오늘은 쌀쌀하네여 이제 아우터를 꺼내야할 것 같아여ㅠ",
