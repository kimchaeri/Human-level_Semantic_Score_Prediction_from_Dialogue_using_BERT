      "utterance": [
        {
          "id": "MDRW2100018902.1.1",
          "form": "웹툰 많이 보세요?",
          "original_form": "웹툰 많이 보세요?",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018902.1.2",
          "form": "넵 저 웹툰 매일매일봐요 ㅎㅎ",
          "original_form": "넵 저 웹툰 매일매일봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018902.1.3",
          "form": "name1님도 많이보세요??",
          "original_form": "&name1&님도 많이보세요??",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018902.1.4",
          "form": "저도 매일 챙겨봐요! 오늘은 뭐 보셨어요?",
          "original_form": "저도 매일 챙겨봐요! 오늘은 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018902.1.5",
          "form": "어제 새로운 웹툰을 발견해서",
          "original_form": "어제 새로운 웹툰을 발견해서",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018902.1.6",
          "form": "정주행했는데",
          "original_form": "정주행했는데",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018902.1.7",
          "form": "별을 삼킨 너에게",
          "original_form": "별을 삼킨 너에게",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018902.1.8",
          "form": "라는 웹툰을 봤어요 ㅋㅋ",
          "original_form": "라는 웹툰을 봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018902.1.9",
          "form": "오 재밌나요?",
          "original_form": "오 재밌나요?",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018902.1.10",
          "form": "넵 볼만해요!!와 존잼!!!까진 아니긴해요..",
          "original_form": "넵 볼만해요!!와 존잼!!!까진 아니긴해요..",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018902.1.11",
          "form": "최근에 본것중에 재밌었던건 별이삼샵",
          "original_form": "최근에 본것중에 재밌었던건 별이삼샵",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018902.1.12",
          "form": "이거에요",
          "original_form": "이거에요",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018902.1.13",
          "form": "시니혀노작가! 맞나요?",
          "original_form": "시니혀노작가! 맞나요?",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018902.1.14",
          "form": "맞아요!!죽음에관하여 작가중 한분이 그림그리고 계세요",
          "original_form": "맞아요!!죽음에관하여 작가중 한분이 그림그리고 계세요",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018902.1.15",
          "form": "로맨스는 안보시나요?",
          "original_form": "로맨스는 안보시나요?",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018902.1.16",
          "form": "보긴봐요!!추천해주실거 잇으신가요?",
          "original_form": "보긴봐요!!추천해주실거 잇으신가요?",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018902.1.17",
          "form": "곱게키웠더니짐승 아세요? 재밌어요!",
          "original_form": "곱게키웠더니짐승 아세요? 재밌어요!",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018902.1.18",
          "form": "앗 제목만 들어봤는데",
          "original_form": "앗 제목만 들어봤는데",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018902.1.19",
          "form": "재미있나요??",
          "original_form": "재미있나요??",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018902.1.20",
          "form": "재밌어요! 슥슥 읽게돼요~ 보는 로맨스는 뭐세요?",
          "original_form": "재밌어요! 슥슥 읽게돼요~ 보는 로맨스는 뭐세요?",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018902.1.21",
          "form": "음 화요일 웹툰중에",
          "original_form": "음 화요일 웹툰중에",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018902.1.22",
          "form": "플레이 플리 랑 달콤살벌한 부부",
          "original_form": "플레이 플리 랑 달콤살벌한 부부",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018902.1.23",
          "form": "이런거 보고잇어요!!",
          "original_form": "이런거 보고잇어요!!",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018902.1.24",
          "form": "들어본적 있는거같아요! 네이버만보시나요?",
          "original_form": "들어본적 있는거같아요! 네이버만보시나요?",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018902.1.25",
          "form": "다음도 종종 보긴보는데",
          "original_form": "다음도 종종 보긴보는데",
          "speaker_id": "2",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100018902.1.26",
          "form": "한번에 몰아보고 손이 잘 안가긴해요 ㅎㅎ",
          "original_form": "한번에 몰아보고 손이 잘 안가긴해요 ㅎㅎ",
