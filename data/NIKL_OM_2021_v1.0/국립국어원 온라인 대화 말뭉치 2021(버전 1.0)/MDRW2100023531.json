      "utterance": [
        {
          "id": "MDRW2100023531.1.1",
          "form": "안녕하세요~^^ 반려동물 키우시나 봐요",
          "original_form": "안녕하세요~^^ 반려동물 키우시나 봐요",
          "speaker_id": "1",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100023531.1.2",
          "form": "안녕하세용 답장이 늦었네요ㅠㅠ",
          "original_form": "안녕하세용 답장이 늦었네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100023531.1.3",
          "form": "저는 비염때문에 키우지는 못하고",
          "original_form": "저는 비염때문에 키우지는 못하고",
          "speaker_id": "2",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100023531.1.4",
          "form": "대신 동물을 너무 사랑해요!",
          "original_form": "대신 동물을 너무 사랑해요!",
          "speaker_id": "2",
          "time": "20210830 12:48"
        },
        {
          "id": "MDRW2100023531.1.5",
          "form": "저도 비염때문에 못 키워요",
          "original_form": "저도 비염때문에 못 키워요",
          "speaker_id": "1",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023531.1.6",
          "form": "어렸을땐 강아지를 기르긴 했는데",
          "original_form": "어렸을땐 강아지를 기르긴 했는데",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023531.1.7",
          "form": "우와 무슨 종이었어요?",
          "original_form": "우와 무슨 종이었어요?",
          "speaker_id": "2",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023531.1.8",
          "form": "걔 가고나선 더 못기르죠",
          "original_form": "걔 가고나선 더 못기르죠",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023531.1.9",
          "form": "말티즈 특징이 진한 믹스요",
          "original_form": "말티즈 특징이 진한 믹스요",
          "speaker_id": "1",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023531.1.10",
          "form": "아이구ㅠㅠ 제 주위에도 name1같은 분들이 계셔요...",
          "original_form": "아이구ㅠㅠ 제 주위에도 &name1&같은 분들이 계셔요...",
          "speaker_id": "2",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023531.1.11",
          "form": "아아아 저 말티즈를 제일 좋아하는데",
          "original_form": "아아아 저 말티즈를 제일 좋아하는데",
          "speaker_id": "2",
          "time": "20210830 12:51"
        },
        {
          "id": "MDRW2100023531.1.12",
          "form": "믹스라니 넘 특별하고 사랑스러울것같아요",
          "original_form": "믹스라니 넘 특별하고 사랑스러울것같아요",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100023531.1.13",
          "form": "그쵸, 그 기억때문인지 여전히 하얀 말티즈 부류가 제일 예뻐보여요",
          "original_form": "그쵸, 그 기억때문인지 여전히 하얀 말티즈 부류가 제일 예뻐보여요",
          "speaker_id": "1",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023531.1.14",
          "form": "걔가 그리고 얼마나 영특했는지",
          "original_form": "걔가 그리고 얼마나 영특했는지",
          "speaker_id": "1",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023531.1.15",
          "form": "맞아요 말티즈 진짜 똑똑해요",
          "original_form": "맞아요 말티즈 진짜 똑똑해요",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023531.1.16",
          "form": "화도 많지만 애교도 많아요ㅋㅋㅋ",
          "original_form": "화도 많지만 애교도 많아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023531.1.17",
          "form": "우리 애ㅋㅋㅋ는 화는 크게 없었어요",
          "original_form": "우리 애ㅋㅋㅋ는 화는 크게 없었어요",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023531.1.18",
          "form": "우와 순딩이였나봐요ㅠㅠ",
          "original_form": "우와 순딩이였나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023531.1.19",
          "form": "애교도 솔직히 많진 않았다",
          "original_form": "애교도 솔직히 많진 않았다",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023531.1.20",
          "form": "복받으셨네",
          "original_form": "복받으셨네",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023531.1.21",
          "form": "좀 할미견 같았네요.",
          "original_form": "좀 할미견 같았네요.",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023531.1.22",
          "form": "생각해보니 주인 닮았네",
          "original_form": "생각해보니 주인 닮았네",
          "speaker_id": "1",
