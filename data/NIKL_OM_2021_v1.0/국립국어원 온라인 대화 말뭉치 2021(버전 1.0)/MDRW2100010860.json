      "utterance": [
        {
          "id": "MDRW2100010860.1.1",
          "form": "해품달도 안봤다",
          "original_form": "해품달도 안봤다",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010860.1.2",
          "form": "아 맞나?  괜찮긴 한데 주인공 둘이 안 어울려",
          "original_form": "아 맞나?  괜찮긴 한데 주인공 둘이 안 어울려",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010860.1.3",
          "form": "별에서 온 그대도 안보고",
          "original_form": "별에서 온 그대도 안보고",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010860.1.4",
          "form": "별그대도 참 재밌었는데",
          "original_form": "별그대도 참 재밌었는데",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010860.1.5",
          "form": "발리를 떠나서가 소지섭 나오는거  이것도 안보고",
          "original_form": "발리를 떠나서가 소지섭 나오는거  이것도 안보고",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010860.1.6",
          "form": "그거는 나도 안봄ㅋ 근데 재밌단다",
          "original_form": "그거는 나도 안봄ㅋ 근데 재밌단다",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010860.1.7",
          "form": "도깨비도 안보고",
          "original_form": "도깨비도 안보고",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010860.1.8",
          "form": "난 송송커플 나오는거 안 봤다",
          "original_form": "난 송송커플 나오는거 안 봤다",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010860.1.9",
          "form": "응답하라 시리즈도 안보고",
          "original_form": "응답하라 시리즈도 안보고",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010860.1.10",
          "form": "응답은 나도 잘 안 봐지드라",
          "original_form": "응답은 나도 잘 안 봐지드라",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010860.1.11",
          "form": "송송커플 그게 이름이 기억안난다",
          "original_form": "송송커플 그게 이름이 기억안난다",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010860.1.12",
          "form": "그니깐 이름 기억이 안나서ㅋ",
          "original_form": "그니깐 이름 기억이 안나서ㅋ",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010860.1.13",
          "form": "손예진이랑 나와서 현빈이랑 연애하는 드라마 뭐지/ 그것도 안봤다",
          "original_form": "손예진이랑 나와서 현빈이랑 연애하는 드라마 뭐지/ 그것도 안봤다",
          "speaker_id": "2",
