      "utterance": [
        {
          "id": "MDRW2100032537.1.1",
          "form": "삼미집 풀네임이 뭐야?",
          "original_form": "삼미집 풀네임이 뭐야?",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032537.1.2",
          "form": "그냥 삼미집 아니야??",
          "original_form": "그냥 삼미집 아니야??",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032537.1.3",
          "form": "닭갈비 파는데",
          "original_form": "닭갈비 파는데",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032537.1.4",
          "form": "아니 파전이 맛있지",
          "original_form": "아니 파전이 맛있지",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032537.1.5",
          "form": "아 그래??ㅋㅋㅋㅋ몰라 까먹었어 나 청주",
          "original_form": "아 그래??ㅋㅋㅋㅋ몰라 까먹었어 나 청주",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032537.1.6",
          "form": "파전 맛있겠다",
          "original_form": "파전 맛있겠다",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032537.1.7",
          "form": "음 글쿤.. ㅠ 어쩔수없지뭐",
          "original_form": "음 글쿤.. ㅠ 어쩔수없지뭐",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100032537.1.8",
          "form": "너가 생각하는 맛집은 어딘데?",
          "original_form": "너가 생각하는 맛집은 어딘데?",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100032537.1.9",
          "form": "청주??",
          "original_form": "청주??",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100032537.1.10",
          "form": "서울??",
          "original_form": "서울??",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100032537.1.11",
          "form": "어디든?",
          "original_form": "어디든?",
          "speaker_id": "1",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032537.1.12",
          "form": "서울에 내가 최근",
          "original_form": "서울에 내가 최근",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032537.1.13",
          "form": "카츠오도라고 갔거든?",
          "original_form": "카츠오도라고 갔거든?",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032537.1.14",
          "form": "돈카츠 집인데 맛있더라 진짜로",
          "original_form": "돈카츠 집인데 맛있더라 진짜로",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032537.1.15",
          "form": "인생 돈카츠였어",
          "original_form": "인생 돈카츠였어",
          "speaker_id": "2",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100032537.1.16",
          "form": "앗 난 돈까스를 안좋아하는편이랗ㅎㅎ",
          "original_form": "앗 난 돈까스를 안좋아하는편이랗ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100032537.1.17",
          "form": "먹긴하는데",
          "original_form": "먹긴하는데",
          "speaker_id": "1",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100032537.1.18",
          "form": "맛집이라고해서 가보고싶진 않다",
          "original_form": "맛집이라고해서 가보고싶진 않다",
          "speaker_id": "1",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100032537.1.19",
          "form": "뭐 언젠가 끌리면 가봐",
          "original_form": "뭐 언젠가 끌리면 가봐",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100032537.1.20",
          "form": "돈카츠 먹으러 오사카를 왜 갔었을까 하는 맛이었어",
          "original_form": "돈카츠 먹으러 오사카를 왜 갔었을까 하는 맛이었어",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100032537.1.21",
          "form": "으엥",
          "original_form": "으엥",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100032537.1.22",
          "form": "돈카츠먹으러 일본가,,?",
          "original_form": "돈카츠먹으러 일본가,,?",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100032537.1.23",
          "form": "아니 간김에 먹었었지 ㅋㅋㅋㅋㅋ",
          "original_form": "아니 간김에 먹었었지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100032537.1.24",
          "form": "아근데 나일본 시러해",
          "original_form": "아근데 나일본 시러해",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032537.1.25",
          "form": "할튼,, 줄서먹는것도 싫구",
          "original_form": "할튼,, 줄서먹는것도 싫구",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032537.1.26",
          "form": "맛집은 큰의미 없어 나는",
          "original_form": "맛집은 큰의미 없어 나는",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032537.1.27",
          "form": "뭐 정치권과 별개로 그냥 간거지",
          "original_form": "뭐 정치권과 별개로 그냥 간거지",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032537.1.28",
          "form": "그...응 그냥 나한케 맛집이었다고 ㅋㅋㅋㅋ",
          "original_form": "그...응 그냥 나한케 맛집이었다고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032537.1.29",
          "form": "정치?랑 상관없이 싫어햏ㅎㅎㅎ",
          "original_form": "정치?랑 상관없이 싫어햏ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032537.1.30",
