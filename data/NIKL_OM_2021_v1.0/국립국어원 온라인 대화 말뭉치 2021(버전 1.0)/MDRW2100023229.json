      "utterance": [
        {
          "id": "MDRW2100023229.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100023229.1.2",
          "form": "좋아하는 동물이 있으신가요?",
          "original_form": "좋아하는 동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100023229.1.3",
          "form": "저 고양이 좋아해여",
          "original_form": "저 고양이 좋아해여",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023229.1.4",
          "form": "아 근데 알레르기 있어서",
          "original_form": "아 근데 알레르기 있어서",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023229.1.5",
          "form": "가까이 가지 못해요 ㅠ",
          "original_form": "가까이 가지 못해요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023229.1.6",
          "form": "친구도 동생도 다 고양이 키우는데여",
          "original_form": "친구도 동생도 다 고양이 키우는데여",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023229.1.7",
          "form": "아이고ㅠ",
          "original_form": "아이고ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023229.1.8",
          "form": "그래서 밤에 친구랑 사료사서 물이랑",
          "original_form": "그래서 밤에 친구랑 사료사서 물이랑",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023229.1.9",
          "form": "밥주러 다니곤 했어여",
          "original_form": "밥주러 다니곤 했어여",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023229.1.10",
          "form": "멀리서 바라만 볼분~~",
          "original_form": "멀리서 바라만 볼분~~",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023229.1.11",
          "form": "가까이에서 키우는데 못다가간다니 ㅠ",
          "original_form": "가까이에서 키우는데 못다가간다니 ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023229.1.12",
          "form": "바라만 봐도",
          "original_form": "바라만 봐도",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023229.1.13",
          "form": "털알레르기같은거같애여",
          "original_form": "털알레르기같은거같애여",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023229.1.14",
          "form": "고양이들은 님의 마음을 알거예요",
          "original_form": "고양이들은 님의 마음을 알거예요",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023229.1.15",
          "form": "밤에 밥을 챙겨주신다니 대단해요 ...",
          "original_form": "밤에 밥을 챙겨주신다니 대단해요 ...",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023229.1.16",
          "form": "혹시 간식도 챙겨주시나요?",
          "original_form": "혹시 간식도 챙겨주시나요?",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023229.1.17",
          "form": "간식은 저도 잘 몰라서",
          "original_form": "간식은 저도 잘 몰라서",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023229.1.18",
          "form": "사쥬진 못했어요",
          "original_form": "사쥬진 못했어요",
          "speaker_id": "2",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100023229.1.19",
          "form": "님은 반려동물 있으세여?",
          "original_form": "님은 반려동물 있으세여?",
          "speaker_id": "2",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100023229.1.20",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023229.1.21",
          "form": "저도 반려동물을 키울여건이 안되서요 ",
          "original_form": "저도 반려동물을 키울여건이 안되서요 ",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023229.1.22",
          "form": "그냥 유튜브로 지켜만 봐요",
          "original_form": "그냥 유튜브로 지켜만 봐요",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023229.1.23",
          "form": "hahaha님이라고 양어장의 길고양이들 돌봐주시는 분의 채널ㅇ 있는데",
          "original_form": "hahaha님이라고 양어장의 길고양이들 돌봐주시는 분의 채널ㅇ 있는데",
          "speaker_id": "1",
          "time": "20210829 23:13"
        },
        {
          "id": "MDRW2100023229.1.24",
          "form": "고양이를 지켜보는 재미로 대리만족중입니다",
          "original_form": "고양이를 지켜보는 재미로 대리만족중입니다",
          "speaker_id": "1",
          "time": "20210829 23:14"
        },
        {
          "id": "MDRW2100023229.1.25",
          "form": "오 저도 금방 검색해봤는데",
          "original_form": "오 저도 금방 검색해봤는데",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023229.1.26",
          "form": "뭔가 고화질에",
          "original_form": "뭔가 고화질에",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023229.1.27",
          "form": "귀여운 고양이들이 많아서",
          "original_form": "귀여운 고양이들이 많아서",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023229.1.28",
          "form": "한번 봐야겠어요",
          "original_form": "한번 봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023229.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100023229.1.30",
          "form": "뭔가 고양이 안 좋아한다 내 생선을 건들이지말고 주는거나 먹어라 분위기인데 고양이 장난감이나",
          "original_form": "뭔가 고양이 안 좋아한다 내 생선을 건들이지말고 주는거나 먹어라 분위기인데 고양이 장난감이나",
          "speaker_id": "1",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100023229.1.31",
          "form": "간식도 잘챙겨주고 집도 뚝딱 만들어 줘서 대리만족을 느끼는 랜선집사입니다 ㅋㅋㅋ",
          "original_form": "간식도 잘챙겨주고 집도 뚝딱 만들어 줘서 대리만족을 느끼는 랜선집사입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023229.1.32",
          "form": "네네 참치캔도 먹나요?",
          "original_form": "네네 참치캔도 먹나요?",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023229.1.33",
          "form": "네네 참치캐도 먹더라구요",
          "original_form": "네네 참치캐도 먹더라구요",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100023229.1.34",
          "form": "고양이는 건들이지만 않으면 무해해서 지켜보기 좋아요..",
          "original_form": "고양이는 건들이지만 않으면 무해해서 지켜보기 좋아요..",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023229.1.35",
          "form": "모든 반려동물들이 그렇지만요 ㅋㅋㅋㅋㅋ",
          "original_form": "모든 반려동물들이 그렇지만요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023229.1.36",
          "form": "도마뱀도 키우시던대",
          "original_form": "도마뱀도 키우시던대",
          "speaker_id": "2",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023229.1.37",
          "form": "네 다양한 반려동물이 있는거 같아요 \r\n제 친구는 날다람쥐를 키워요",
          "original_form": "네 다양한 반려동물이 있는거 같아요 \r\n제 친구는 날다람쥐를 키워요",
          "speaker_id": "1",
