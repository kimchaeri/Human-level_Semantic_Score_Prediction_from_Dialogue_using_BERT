      "utterance": [
        {
          "id": "MDRW2100042398.1.1",
          "form": "아유 그래도 요새는",
          "original_form": "아유 그래도 요새는",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.2",
          "form": "논다고 영어학원 늦지도 않고",
          "original_form": "논다고 영어학원 늦지도 않고",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.3",
          "form": "가방도 잘메고오고",
          "original_form": "가방도 잘메고오고",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.4",
          "form": "이어 씩씩해졌디",
          "original_form": "이어 씩씩해졌디",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.5",
          "form": "우산 만 잘 챙겨오면 되는데",
          "original_form": "우산 만 잘 챙겨오면 되는데",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.6",
          "form": "비록 우산은 어디 갔는지 모르겠지만",
          "original_form": "비록 우산은 어디 갔는지 모르겠지만",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.7",
          "form": "내 말이ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "내 말이ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.8",
          "form": "아 생각하는 거 똑같음ㅋㅋㅋㅋ",
          "original_form": "아 생각하는 거 똑같음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.9",
          "form": "그래도 우산은 싸잖아",
          "original_form": "그래도 우산은 싸잖아",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.11",
          "form": "생각보다 애들은 다 잘하는 듯ㅋㅋ",
          "original_form": "생각보다 애들은 다 잘하는 듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.12",
          "form": "가방은 잃어버리면 안 되는데ㅋㅋㅋㅋ",
          "original_form": "가방은 잃어버리면 안 되는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.13",
          "form": "엄마가 걱정이 많지요",
          "original_form": "엄마가 걱정이 많지요",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.14",
          "form": "그렇다",
          "original_form": "그렇다",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042398.1.15",
          "form": "우리가 뭘 하는지 알면 되는데",
          "original_form": "우리가 뭘 하는지 알면 되는데",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042398.1.16",
          "form": "학교 가기 싫다는 소리 안 하고",
          "original_form": "학교 가기 싫다는 소리 안 하고",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042398.1.17",
          "form": "모르니까 자꾸 걱정되는 거",
          "original_form": "모르니까 자꾸 걱정되는 거",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042398.1.18",
          "form": "가방 메고 왔다갔다 잘하니",
          "original_form": "가방 메고 왔다갔다 잘하니",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042398.1.19",
          "form": "맞아요 그것만 해도 기특하다",
          "original_form": "맞아요 그것만 해도 기특하다",
          "speaker_id": "1",
          "time": "20210906 21:59"
        }
