      "utterance": [
        {
          "id": "MDRW2100007588.1.1",
          "form": "게임을 하시나요",
          "original_form": "게임을 하시나요",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007588.1.2",
          "form": "헉 저 게임은 잘못하지만",
          "original_form": "헉 저 게임은 잘못하지만",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.3",
          "form": "크아나 ㄴ",
          "original_form": "크아나 ㄴ",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.4",
          "form": "카트라이더나",
          "original_form": "카트라이더나",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.5",
          "form": "크아재밌어요",
          "original_form": "크아재밌어요",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.6",
          "form": "그런건 알아요",
          "original_form": "그런건 알아요",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.7",
          "form": "크아 진짜",
          "original_form": "크아 진짜",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.8",
          "form": "명겜이죠",
          "original_form": "명겜이죠",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.9",
          "form": "겜알못인 저도 할 줄 압니다",
          "original_form": "겜알못인 저도 할 줄 압니다",
          "speaker_id": "2",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.10",
          "form": "진짜 나온지 20년된거같은데",
          "original_form": "진짜 나온지 20년된거같은데",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.11",
          "form": "아직도 재밌어요",
          "original_form": "아직도 재밌어요",
          "speaker_id": "1",
          "time": "20210822 03:38"
        },
        {
          "id": "MDRW2100007588.1.12",
          "form": "좋아하는 맵 있어요?",
          "original_form": "좋아하는 맵 있어요?",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.13",
          "form": "저는 바다맵좋아했어요",
          "original_form": "저는 바다맵좋아했어요",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.14",
          "form": "홀!!!",
          "original_form": "홀!!!",
          "speaker_id": "2",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.16",
          "form": "그 파도 느낌",
          "original_form": "그 파도 느낌",
          "speaker_id": "2",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.17",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.18",
          "form": "아시는구나",
          "original_form": "아시는구나",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.19",
          "form": "브금도좋고해서",
          "original_form": "브금도좋고해서",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.20",
          "form": "근데 렙낮아서 맨날 방 찾아다녔어요ㅋ기",
          "original_form": "근데 렙낮아서 맨날 방 찾아다녔어요ㅋ기",
          "speaker_id": "1",
          "time": "20210822 03:39"
        },
        {
          "id": "MDRW2100007588.1.21",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007588.1.22",
          "form": "저도ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007588.1.23",
          "form": "이스포츠 즐겨보시는 건 혹시 있으신가요",
          "original_form": "이스포츠 즐겨보시는 건 혹시 있으신가요",
          "speaker_id": "2",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007588.1.24",
          "form": "음.. 아니요ㅠ",
          "original_form": "음.. 아니요ㅠ",
          "speaker_id": "1",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007588.1.25",
          "form": "즐겨보시나요?",
          "original_form": "즐겨보시나요?",
          "speaker_id": "1",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007588.1.26",
          "form": "아뇨 저도",
          "original_form": "아뇨 저도",
          "speaker_id": "2",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007588.1.27",
          "form": "페이커만 알아요",
          "original_form": "페이커만 알아요",
          "speaker_id": "2",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007588.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007588.1.29",
          "form": "진짴ㅋㅋ 게임알못이죠ㅠㅠ",
          "original_form": "진짴ㅋㅋ 게임알못이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007588.1.30",
          "form": "카트나 오버워치 롤 이런거 있던데 잘은 몰라요ㅣㅣㅣㅣ",
          "original_form": "카트나 오버워치 롤 이런거 있던데 잘은 몰라요ㅣㅣㅣㅣ",
          "speaker_id": "1",
          "time": "20210822 03:42"
        },
        {
          "id": "MDRW2100007588.1.31",
          "form": "저도 겜알못이라ㅋㅋ 괜찮아요",
          "original_form": "저도 겜알못이라ㅋㅋ 괜찮아요",
          "speaker_id": "1",
          "time": "20210822 03:43"
        },
        {
          "id": "MDRW2100007588.1.32",
          "form": "크아에서 자주이용했던 캐릭터는?",
          "original_form": "크아에서 자주이용했던 캐릭터는?",
          "speaker_id": "1",
          "time": "20210822 03:43"
        },
        {
          "id": "MDRW2100007588.1.33",
          "form": "그 빨간거 위에 두개 달린애요",
          "original_form": "그 빨간거 위에 두개 달린애요",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007588.1.34",
          "form": "힘겹다",
          "original_form": "힘겹다",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007588.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007588.1.36",
          "form": "배찌 말하는건가",
          "original_form": "배찌 말하는건가",
          "speaker_id": "1",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007588.1.37",
          "form": "그게 아무래도 밸런스가 좋죠",
