      "utterance": [
        {
          "id": "MDRW2100014983.1.1",
          "form": "편의점에 재미난 상품이 많은 것같아요",
          "original_form": "편의점에 재미난 상품이 많은 것같아요",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014983.1.2",
          "form": "네 요즘 특이한거 많더라구요",
          "original_form": "네 요즘 특이한거 많더라구요",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014983.1.3",
          "form": "메로나 소주 궁금한데 아직 못봤어요",
          "original_form": "메로나 소주 궁금한데 아직 못봤어요",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014983.1.4",
          "form": "오 저도 못 봤어요! 있는줄도 몰랐네요!",
          "original_form": "오 저도 못 봤어요! 있는줄도 몰랐네요!",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014983.1.5",
          "form": "저는 아이셔에 이슬이 궁금한데 그건 편의점에서는 안 판다고 하더라구요",
          "original_form": "저는 아이셔에 이슬이 궁금한데 그건 편의점에서는 안 판다고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014983.1.6",
          "form": "그 전설의 소주",
          "original_form": "그 전설의 소주",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014983.1.7",
          "form": "저도 본 적도 없어요",
          "original_form": "저도 본 적도 없어요",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014983.1.8",
          "form": "조리퐁 막걸리도 먹어봐야 하는데ㅎㅎ",
          "original_form": "조리퐁 막걸리도 먹어봐야 하는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014983.1.9",
          "form": "근데 가끔은 콜라보상품이 과한것도 있더라구요",
          "original_form": "근데 가끔은 콜라보상품이 과한것도 있더라구요",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014983.1.10",
          "form": "그거 죠리퐁맛 일도않나요",
          "original_form": "그거 죠리퐁맛 일도않나요",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014983.1.11",
          "form": "아 그런가요?",
          "original_form": "아 그런가요?",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014983.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014983.1.13",
          "form": "서울우유 바디워시는 진짜 우유인줄",
          "original_form": "서울우유 바디워시는 진짜 우유인줄",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014983.1.14",
          "form": "ㅋㅋㅋㅋㅋ 그건 마트에서 우유진열장 앞에 두고 팔아서 말도 많았잖아요",
          "original_form": "ㅋㅋㅋㅋㅋ 그건 마트에서 우유진열장 앞에 두고 팔아서 말도 많았잖아요",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014983.1.15",
          "form": "그런건 좀 위험한 것 같아요",
          "original_form": "그런건 좀 위험한 것 같아요",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014983.1.16",
          "form": "진로 디퓨저도 정말 소주같이 생겨서 먹으면 어떡하냐는 얘기 있었는데 제가 선물용으로 산 적이 있었는데 먹을 수 있게 생기진 않았더라구요ㅋㅋ",
          "original_form": "진로 디퓨저도 정말 소주같이 생겨서 먹으면 어떡하냐는 얘기 있었는데 제가 선물용으로 산 적이 있었는데 먹을 수 있게 생기진 않았더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014983.1.17",
          "form": "그쵸? 선을 넘었어요",
          "original_form": "그쵸? 선을 넘었어요",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014983.1.18",
          "form": "예전 바나나우유까지가 딱",
          "original_form": "예전 바나나우유까지가 딱",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014983.1.19",
          "form": "앜 진짜요?",
          "original_form": "앜 진짜요?",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014983.1.20",
          "form": "두껍 그거말하는 거죠?",
          "original_form": "두껍 그거말하는 거죠?",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014983.1.21",
          "form": "네네 맞아요ㅋㅋ",
          "original_form": "네네 맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014983.1.22",
          "form": "하늘색 병ㅋㅋ",
          "original_form": "하늘색 병ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014983.1.23",
          "form": "그거 귀엽던데",
          "original_form": "그거 귀엽던데",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014983.1.24",
          "form": "참이슬 가방 보셨어요? 팩소주모양의",
          "original_form": "참이슬 가방 보셨어요? 팩소주모양의",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014983.1.25",
          "form": "오 아니요! 백팩인가요?",
          "original_form": "오 아니요! 백팩인가요?",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014983.1.26",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100014983.1.27",
          "form": "무신사에서 매진나고",
          "original_form": "무신사에서 매진나고",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100014983.1.28",
          "form": "요즘 마트에서 죽을만큼 소주사면 사은품으로 주는것같더라구요",
          "original_form": "요즘 마트에서 죽을만큼 소주사면 사은품으로 주는것같더라구요",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100014983.1.29",
          "form": "아 그렇구나ㅋㅋ",
          "original_form": "아 그렇구나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014983.1.30",
          "form": "곰표도 굿즈 예쁘게 잘 나오는 것 같더라구요ㅎㅎ",
          "original_form": "곰표도 굿즈 예쁘게 잘 나오는 것 같더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014983.1.31",
          "form": "굿즈요?",
          "original_form": "굿즈요?",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014983.1.32",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014983.1.33",
          "form": "가방이랑 맨투맨 이런거ㅋㅋ",
          "original_form": "가방이랑 맨투맨 이런거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014983.1.34",
          "form": "아무래도 캐릭터가 귀여워서 다 귀여운 것 같아요ㅎㅎ",
          "original_form": "아무래도 캐릭터가 귀여워서 다 귀여운 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014983.1.35",
          "form": "앗 진짜요? 귀여울듯",
          "original_form": "앗 진짜요? 귀여울듯",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014983.1.36",
          "form": "곰표는 원래 밀가루인데. ㅋㅋ",
          "original_form": "곰표는 원래 밀가루인데. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014983.1.37",
          "form": "그쵸ㅋㅋㅋ 저는 솔직히 맥주는 조금 실망했어요ㅜㅜ",
          "original_form": "그쵸ㅋㅋㅋ 저는 솔직히 맥주는 조금 실망했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014983.1.38",
          "form": "원래 밀맥주 별로 안 좋아하기는 하는데 그래도 편의점에 계속 품절이길래 벼르다가 샀었는데 제 취향은 아니더라구요ㅜㅜㅋㅋ",
          "original_form": "원래 밀맥주 별로 안 좋아하기는 하는데 그래도 편의점에 계속 품절이길래 벼르다가 샀었는데 제 취향은 아니더라구요ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014983.1.39",
          "form": "곰표맥주 전 사랑합니다",
          "original_form": "곰표맥주 전 사랑합니다",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014983.1.40",
          "form": "아~ 향나는거 싫어하시는구나",
          "original_form": "아~ 향나는거 싫어하시는구나",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014983.1.41",
          "form": "아 그러시구나ㅎㅎ 네 저는 입맛이 고급지지 못해서 라거나 필스너가 제일 맛있는 것 같아요ㅋㅋ",
          "original_form": "아 그러시구나ㅎㅎ 네 저는 입맛이 고급지지 못해서 라거나 필스너가 제일 맛있는 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014983.1.42",
          "form": "전 ipa계열 완죤 사랑해요",
          "original_form": "전 ipa계열 완죤 사랑해요",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014983.1.43",
          "form": "라거가 목넘김이 좋죠",
          "original_form": "라거가 목넘김이 좋죠",
          "speaker_id": "1",
