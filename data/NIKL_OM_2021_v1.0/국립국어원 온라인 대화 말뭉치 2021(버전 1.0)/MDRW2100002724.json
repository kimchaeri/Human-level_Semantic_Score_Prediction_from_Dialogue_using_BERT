          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100002724.1.25",
          "form": "근데 전 손재주 똥이라",
          "original_form": "근데 전 손재주 똥이라",
          "speaker_id": "2",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100002724.1.26",
          "form": "ㅋㅋㅋ 나도 남편한테 부탁",
          "original_form": "ㅋㅋㅋ 나도 남편한테 부탁",
          "speaker_id": "1",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100002724.1.27",
          "form": "꼼꼼히 해주라고",
          "original_form": "꼼꼼히 해주라고",
          "speaker_id": "1",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100002724.1.28",
          "form": "잘 못바르겟어여ㅋ",
          "original_form": "잘 못바르겟어여ㅋ",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100002724.1.29",
          "form": "첫구매는 역시 사은품을 많이줘",
          "original_form": "첫구매는 역시 사은품을 많이줘",
          "speaker_id": "1",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100002724.1.30",
          "form": "ㅋㅋㅋㅋ그치 손님끌어야해",
          "original_form": "ㅋㅋㅋㅋ그치 손님끌어야해",
          "speaker_id": "2",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100002724.1.31",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100002724.1.32",
          "form": "어서오세요",
          "original_form": "어서오세요",
          "speaker_id": "2",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100002724.1.33",
          "form": "그 마켓컬리 이런것도 첫구매는 마니줘",
          "original_form": "그 마켓컬리 이런것도 첫구매는 마니줘",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100002724.1.34",
          "form": "난 그래성첫구매하고 잘 안사기는함 ㅋㅋ",
          "original_form": "난 그래성첫구매하고 잘 안사기는함 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100002724.1.35",
          "form": "ㅋㅋㅋ 저도 아디만 맨날 신규가입",
          "original_form": "ㅋㅋㅋ 저도 아디만 맨날 신규가입",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100002724.1.36",
          "form": "ㅎㅎ 맞아 그래서 혜택받고 또받고",
          "original_form": "ㅎㅎ 맞아 그래서 혜택받고 또받고",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100002724.1.37",
