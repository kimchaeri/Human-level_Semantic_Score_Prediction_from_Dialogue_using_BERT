      "utterance": [
        {
          "id": "MDRW2100005014.1.1",
          "form": "요즘에 입을 옷이 없다ㅠㅠ 옷 사고싶어",
          "original_form": "요즘에 입을 옷이 없다ㅠㅠ 옷 사고싶어",
          "speaker_id": "1",
          "time": "20210824 19:34"
        },
        {
          "id": "MDRW2100005014.1.2",
          "form": "ㅇㅈㅇㅈ 근데 벌써 패딩 팔더랔ㅋㅋ 패션의 세계..;",
          "original_form": "ㅇㅈㅇㅈ 근데 벌써 패딩 팔더랔ㅋㅋ 패션의 세계..;",
          "speaker_id": "2",
          "time": "20210824 20:23"
        },
        {
          "id": "MDRW2100005014.1.3",
          "form": "벌써..패딩을?",
          "original_form": "벌써..패딩을?",
          "speaker_id": "1",
          "time": "20210825 00:33"
        },
        {
          "id": "MDRW2100005014.1.4",
          "form": "코트 사고싶당",
          "original_form": "코트 사고싶당",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100005014.1.5",
          "form": "엉..; ㅁㅈㅁㅈ 코트랑 패딩이랑..",
          "original_form": "엉..; ㅁㅈㅁㅈ 코트랑 패딩이랑..",
          "speaker_id": "2",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100005014.1.6",
          "form": "가을옷이 필요한뎈ㅋㅋㅋ",
          "original_form": "가을옷이 필요한뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100005014.1.7",
          "form": "미리미리사야하ㅏㄴ버..",
          "original_form": "미리미리사야하ㅏㄴ버..",
          "speaker_id": "2",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100005014.1.8",
          "form": "근데 철 안 맞는 옷이 싸긴하더라",
          "original_form": "근데 철 안 맞는 옷이 싸긴하더라",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100005014.1.9",
          "form": "트렌치코트 있니?",
          "original_form": "트렌치코트 있니?",
          "speaker_id": "1",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100005014.1.10",
          "form": "맞어 올해도 거의집에있을거같은데 걍 있다가 겨울에 봄옷살깤ㅋ",
          "original_form": "맞어 올해도 거의집에있을거같은데 걍 있다가 겨울에 봄옷살깤ㅋ",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100005014.1.11",
          "form": "ㄴㄴㄴㄴ 나 아예 코트가 없서..",
          "original_form": "ㄴㄴㄴㄴ 나 아예 코트가 없서..",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100005014.1.12",
          "form": "하나 있었는데 작아져소 ㅎ",
          "original_form": "하나 있었는데 작아져소 ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:35"
        },
        {
          "id": "MDRW2100005014.1.13",
          "form": "야 나두",
          "original_form": "야 나두",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100005014.1.14",
          "form": "코트 다 안 맞아",
          "original_form": "코트 다 안 맞아",
          "speaker_id": "1",
          "time": "20210825 00:36"
        },
        {
          "id": "MDRW2100005014.1.15",
          "form": "슬프다ㅠ",
          "original_form": "슬프다ㅠ",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100005014.1.16",
          "form": "ㄱㅊ아 다시사면되지..^^",
          "original_form": "ㄱㅊ아 다시사면되지..^^",
          "speaker_id": "2",
          "time": "20210825 00:37"
        },
        {
          "id": "MDRW2100005014.1.17",
          "form": "나중에 나랑 옷 사러가자",
          "original_form": "나중에 나랑 옷 사러가자",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100005014.1.18",
          "form": "근데 이제 옷 어디서 사야하는지도 모르겠음",
          "original_form": "근데 이제 옷 어디서 사야하는지도 모르겠음",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100005014.1.19",
          "form": "죠아",
          "original_form": "죠아",
          "speaker_id": "2",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100005014.1.20",
          "form": "아 ㄹㅇ 인정따리..",
          "original_form": "아 ㄹㅇ 인정따리..",
          "speaker_id": "2",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100005014.1.21",
          "form": "너 보통 옷 어디서 삼?",
          "original_form": "너 보통 옷 어디서 삼?",
          "speaker_id": "1",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100005014.1.22",
          "form": "인터넷..",
          "original_form": "인터넷..",
