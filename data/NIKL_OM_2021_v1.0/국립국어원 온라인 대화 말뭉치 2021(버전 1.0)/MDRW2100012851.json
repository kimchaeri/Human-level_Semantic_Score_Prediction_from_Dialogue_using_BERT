      "utterance": [
        {
          "id": "MDRW2100012851.1.1",
          "form": "내일 몇시 공연이에요?",
          "original_form": "내일 몇시 공연이에요?",
          "speaker_id": "2",
          "time": "20210825 02:49"
        },
        {
          "id": "MDRW2100012851.1.2",
          "form": "7시랑 2시반 중에 고민중이긴 한데",
          "original_form": "7시랑 2시반 중에 고민중이긴 한데",
          "speaker_id": "1",
          "time": "20210825 02:49"
        },
        {
          "id": "MDRW2100012851.1.3",
          "form": "2시반꺼로 볼 것 같아요",
          "original_form": "2시반꺼로 볼 것 같아요",
          "speaker_id": "1",
          "time": "20210825 02:50"
        },
        {
          "id": "MDRW2100012851.1.4",
          "form": "어디서 해요?",
          "original_form": "어디서 해요?",
          "speaker_id": "2",
          "time": "20210825 02:50"
        },
        {
          "id": "MDRW2100012851.1.5",
          "form": "예술의 전당이요",
          "original_form": "예술의 전당이요",
          "speaker_id": "1",
          "time": "20210825 02:50"
        },
        {
          "id": "MDRW2100012851.1.6",
          "form": "너무 멀어서 그게 유일한 흠ㅜㅜ",
          "original_form": "너무 멀어서 그게 유일한 흠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 02:50"
        },
        {
          "id": "MDRW2100012851.1.7",
          "form": "집에서 얼마정도 걸려요?",
          "original_form": "집에서 얼마정도 걸려요?",
          "speaker_id": "2",
          "time": "20210825 02:50"
        },
        {
          "id": "MDRW2100012851.1.8",
          "form": "한시간 반정도? ㅠ",
          "original_form": "한시간 반정도? ㅠ",
          "speaker_id": "1",
          "time": "20210825 02:50"
        },
        {
          "id": "MDRW2100012851.1.9",
          "form": "그리고 예술의전당은 소리가 너무 울리더라구요",
          "original_form": "그리고 예술의전당은 소리가 너무 울리더라구요",
          "speaker_id": "1",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012851.1.10",
          "form": "이름값을 잘 못하네요 ㅎㅎ",
          "original_form": "이름값을 잘 못하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012851.1.11",
          "form": "네 ㅋㅋㅋ 거기다 오페라하우스에서 뮤지컬 해서",
          "original_form": "네 ㅋㅋㅋ 거기다 오페라하우스에서 뮤지컬 해서",
          "speaker_id": "1",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012851.1.12",
          "form": "더 안들릴 거 같긴 해요",
          "original_form": "더 안들릴 거 같긴 해요",
          "speaker_id": "1",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012851.1.13",
          "form": "그래도 부럽네요",
          "original_form": "그래도 부럽네요",
          "speaker_id": "2",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012851.1.14",
          "form": "네 저도 일단은 보고싶긴 해요",
          "original_form": "네 저도 일단은 보고싶긴 해요",
          "speaker_id": "1",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012851.1.15",
          "form": "캐스팅이 어떻게 돼요?",
          "original_form": "캐스팅이 어떻게 돼요?",
          "speaker_id": "2",
          "time": "20210825 02:52"
        },
        {
          "id": "MDRW2100012851.1.16",
          "form": "아마 강필석 차지연이요",
          "original_form": "아마 강필석 차지연이요",
          "speaker_id": "1",
          "time": "20210825 02:52"
        },
        {
          "id": "MDRW2100012851.1.17",
          "form": "차지연씨 예전에 서편제 본 적 있어요",
          "original_form": "차지연씨 예전에 서편제 본 적 있어요",
          "speaker_id": "2",
          "time": "20210825 02:52"
        },
        {
          "id": "MDRW2100012851.1.18",
          "form": "헉 저두 그거 봤어요",
          "original_form": "헉 저두 그거 봤어요",
          "speaker_id": "1",
          "time": "20210825 02:52"
        },
        {
          "id": "MDRW2100012851.1.19",
          "form": "서편제 차지연!!!!",
          "original_form": "서편제 차지연!!!!",
          "speaker_id": "1",
          "time": "20210825 02:52"
        },
        {
          "id": "MDRW2100012851.1.20",
          "form": "정말 잘하더라구요",
          "original_form": "정말 잘하더라구요",
          "speaker_id": "2",
          "time": "20210825 02:53"
        },
        {
          "id": "MDRW2100012851.1.21",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210825 02:53"
        },
        {
          "id": "MDRW2100012851.1.22",
          "form": "저도 거의 차지연때문에 보러가는 ㅋ",
          "original_form": "저도 거의 차지연때문에 보러가는 ㅋ",
          "speaker_id": "1",
          "time": "20210825 02:53"
