      "utterance": [
        {
          "id": "MDRW2100013001.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 10:16"
        },
        {
          "id": "MDRW2100013001.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013001.1.3",
          "form": "지금 일하고 계신가요?",
          "original_form": "지금 일하고 계신가요?",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013001.1.4",
          "form": "퇴근했습니다ㅎㅎ",
          "original_form": "퇴근했습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013001.1.5",
          "form": "퇴근이 늦으시네요 ㅠㅠㅠ",
          "original_form": "퇴근이 늦으시네요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100013001.1.6",
          "form": "알바많이 해보셨나요",
          "original_form": "알바많이 해보셨나요",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100013001.1.7",
          "form": "아뇨 전 딱 한번 했는데 그 한번을 오래 했어요",
          "original_form": "아뇨 전 딱 한번 했는데 그 한번을 오래 했어요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100013001.1.8",
          "form": "알바 경험 많으세요??",
          "original_form": "알바 경험 많으세요??",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100013001.1.9",
          "form": "네 여러가지 해봤어요",
          "original_form": "네 여러가지 해봤어요",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100013001.1.10",
          "form": "다양한 경험 하셨겠어요",
          "original_form": "다양한 경험 하셨겠어요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100013001.1.11",
          "form": "해보셨던 알바 중에 가장 힘들었던건 어떤거셨나요?",
          "original_form": "해보셨던 알바 중에 가장 힘들었던건 어떤거셨나요?",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100013001.1.12",
          "form": "전 카페 알바만 해봐서 다른 알바가 얼마나 힘든지 가늠이 안되네요",
          "original_form": "전 카페 알바만 해봐서 다른 알바가 얼마나 힘든지 가늠이 안되네요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100013001.1.13",
          "form": "출장부페 알바요ㅠ",
          "original_form": "출장부페 알바요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100013001.1.14",
          "form": "와.. 계속 접시 나르고 채워넣고 하는거죠?",
          "original_form": "와.. 계속 접시 나르고 채워넣고 하는거죠?",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100013001.1.15",
          "form": "쉬는 시간 같은건 없나요? ㅠㅠㅠ",
          "original_form": "쉬는 시간 같은건 없나요? ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100013001.1.16",
          "form": "보면 계속 서서 일하던데 ㅠㅠㅠ",
          "original_form": "보면 계속 서서 일하던데 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100013001.1.17",
          "form": "허리가 나갈뻔했어요ㅎㅎ",
          "original_form": "허리가 나갈뻔했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100013001.1.18",
          "form": "무거운거 많이 날라요??",
          "original_form": "무거운거 많이 날라요??",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100013001.1.19",
          "form": "그래도 밥은 잘 나온거죠? \r\n일이 그렇게 힘들면 밥이라도 잘나와야 버티는데 ㅠㅠㅠ",
          "original_form": "그래도 밥은 잘 나온거죠? \r\n일이 그렇게 힘들면 밥이라도 잘나와야 버티는데 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100013001.1.20",
          "form": "밥은 잘나왔던것같아요ㅎㅎ",
          "original_form": "밥은 잘나왔던것같아요ㅎㅎ",
          "speaker_id": "1",
