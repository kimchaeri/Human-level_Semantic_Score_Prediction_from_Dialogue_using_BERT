        },
        {
          "id": "MDRW2100011835.1.2",
          "form": "ㅋㅋㅋㅋㅋ어떻게 그럴 수가 있어?",
          "original_form": "ㅋㅋㅋㅋㅋ어떻게 그럴 수가 있어?",
          "speaker_id": "1",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011835.1.3",
          "form": "ㅋㅋㅋ 입맛이 없어서 그런가?",
          "original_form": "ㅋㅋㅋ 입맛이 없어서 그런가?",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011835.1.4",
          "form": "그러니까 어떻게 그럴 수가 있냐고",
          "original_form": "그러니까 어떻게 그럴 수가 있냐고",
          "speaker_id": "1",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011835.1.5",
          "form": "칼럼리스트 해 ㅋㅋ",
          "original_form": "칼럼리스트 해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011835.1.6",
          "form": "하고 싶은데 아무도 안 시켜줘 서 못하고 있는 거야",
          "original_form": "하고 싶은데 아무도 안 시켜줘 서 못하고 있는 거야",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011835.1.7",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011835.1.8",
          "form": "근데 그러면 그나마 맛있게 먹는 음식이라도 없어?",
          "original_form": "근데 그러면 그나마 맛있게 먹는 음식이라도 없어?",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011835.1.9",
          "form": "음료도 음식인가?",
          "original_form": "음료도 음식인가?",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011835.1.10",
          "form": "음료도 음식이긴 하지",
          "original_form": "음료도 음식이긴 하지",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011835.1.11",
          "form": "딸기요거트",
          "original_form": "딸기요거트",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011835.1.12",
          "form": "그거진짜 좋아하는구나",
          "original_form": "그거진짜 좋아하는구나",
