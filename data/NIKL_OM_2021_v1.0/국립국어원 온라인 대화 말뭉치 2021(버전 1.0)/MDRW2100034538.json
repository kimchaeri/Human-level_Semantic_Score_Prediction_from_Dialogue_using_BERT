      "utterance": [
        {
          "id": "MDRW2100034538.1.1",
          "form": "맛있는 매장이기를 ㅋㅋㅋ",
          "original_form": "맛있는 매장이기를 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100034538.1.2",
          "form": "제발ㅠㅋㅋㅋ",
          "original_form": "제발ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034538.1.3",
          "form": "시켰는데",
          "original_form": "시켰는데",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034538.1.4",
          "form": "맛없으면 전 짜증나더라구여 ㅜㅠ",
          "original_form": "맛없으면 전 짜증나더라구여 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034538.1.5",
          "form": "그쵸 그건 다 그럴거에요ㅠ",
          "original_form": "그쵸 그건 다 그럴거에요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034538.1.6",
          "form": "내 소중한 한끼를 버린거같은 느낌ㅠ",
          "original_form": "내 소중한 한끼를 버린거같은 느낌ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034538.1.7",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034538.1.8",
          "form": "돈도 아깝구여 ㅠㅠ",
          "original_form": "돈도 아깝구여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034538.1.9",
          "form": "맛없는데 양만 많으면 젤 지옥이에요..",
          "original_form": "맛없는데 양만 많으면 젤 지옥이에요..",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034538.1.10",
          "form": "그니까요ㅠ진짜 버리는것도 일이고",
          "original_form": "그니까요ㅠ진짜 버리는것도 일이고",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034538.1.11",
          "form": "먹기도 애매하고ㅠ",
          "original_form": "먹기도 애매하고ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034538.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034538.1.13",
          "form": "많은게 달갑지않아요",
          "original_form": "많은게 달갑지않아요",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034538.1.14",
          "form": "맛없는데는 꼭 많이주더라구요ㅋㅋ",
          "original_form": "맛없는데는 꼭 많이주더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100034538.1.15",
          "form": "과일은 뭐 좋아하세요?",
          "original_form": "과일은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100034538.1.16",
          "form": "공감해여 ㅋㅋㅋㅋ",
          "original_form": "공감해여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100034538.1.17",
          "form": "전 요새 복숭아랑 자두요!",
          "original_form": "전 요새 복숭아랑 자두요!",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100034538.1.18",
          "form": "딱복이신가요 물복이신가요?",
          "original_form": "딱복이신가요 물복이신가요?",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034538.1.19",
          "form": "하저는 무조건 물복입니다",
          "original_form": "하저는 무조건 물복입니다",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034538.1.20",
          "form": "요새 딱  철이죠",
          "original_form": "요새 딱  철이죠",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034538.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034538.1.22",
          "form": "저두요ㅋㅋㅋㄱ",
          "original_form": "저두요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034538.1.23",
          "form": "물복이 최고죠ㅋㅋ",
          "original_form": "물복이 최고죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034538.1.24",
          "form": "맞아요 달콤하구 ㅋㅋㅋ",
          "original_form": "맞아요 달콤하구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:50"
        }
      ]
