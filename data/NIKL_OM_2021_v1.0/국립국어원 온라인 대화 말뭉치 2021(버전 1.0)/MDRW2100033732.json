        {
          "id": "MDRW2100033732.1.1",
          "form": "네 정말요ㅠㅠ 충격ㅋㄱ",
          "original_form": "네 정말요ㅠㅠ 충격ㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033732.1.2",
          "form": "시댁어른들이랑 같이 드셨어요?",
          "original_form": "시댁어른들이랑 같이 드셨어요?",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033732.1.3",
          "form": "아니요ㅋㅋ 점심때 주고 가시고ㅋㅋ",
          "original_form": "아니요ㅋㅋ 점심때 주고 가시고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033732.1.4",
          "form": "저희끼리",
          "original_form": "저희끼리",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033732.1.5",
          "form": "가격도 되게 비싸더라구요",
          "original_form": "가격도 되게 비싸더라구요",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033732.1.6",
          "form": "2680원인가100그램에",
          "original_form": "2680원인가100그램에",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033732.1.7",
          "form": "삼겹이 넘 가격이 많이 오른듯해여ㅠ",
          "original_form": "삼겹이 넘 가격이 많이 오른듯해여ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033732.1.8",
          "form": "헙 비싸네요~~",
          "original_form": "헙 비싸네요~~",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033732.1.9",
          "form": "저도가격보고 띠용 했어요",
          "original_form": "저도가격보고 띠용 했어요",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033732.1.10",
          "form": "사실 마트서 삼겹 잘 안사서",
          "original_form": "사실 마트서 삼겹 잘 안사서",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033732.1.11",
          "form": "아직 많이 남으셨겠어요",
          "original_form": "아직 많이 남으셨겠어요",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033732.1.12",
          "form": "걍다 구워서 냄겼어요ㅠ",
          "original_form": "걍다 구워서 냄겼어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033732.1.13",
          "form": "반찬통에 두고",
          "original_form": "반찬통에 두고",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100033732.1.14",
          "form": "낼 김치넣고 애랑 삼겹김치 볶음밥을~~",
          "original_form": "낼 김치넣고 애랑 삼겹김치 볶음밥을~~",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100033732.1.15",
          "form": "오~굿굿~삼겹김치볶음밥 좋네요",
          "original_form": "오~굿굿~삼겹김치볶음밥 좋네요",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033732.1.16",
          "form": "마무리는 볶음밥이죵ㅋ",
          "original_form": "마무리는 볶음밥이죵ㅋ",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033732.1.17",
          "form": "내일 한끼해결",
          "original_form": "내일 한끼해결",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033732.1.18",
          "form": "메뉴고민 하나 덜으셨네요 ㅎ",
          "original_form": "메뉴고민 하나 덜으셨네요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100033732.1.19",
          "form": "좋아요ㅋㅋ",
          "original_form": "좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100033732.1.20",
          "form": "삼겹김치볶음밥 만드실때 삼겹살 이랑 김치 같이 볶고 밥 넣어 볶으면 될까요?",
          "original_form": "삼겹김치볶음밥 만드실때 삼겹살 이랑 김치 같이 볶고 밥 넣어 볶으면 될까요?",
          "speaker_id": "2",
          "time": "20210825 00:25"
        },
        {
          "id": "MDRW2100033732.1.21",
          "form": "네 ㅋㅋ 삼겹 잘게 자르고 김치랑 볶고 밥 볶음 되용",
          "original_form": "네 ㅋㅋ 삼겹 잘게 자르고 김치랑 볶고 밥 볶음 되용",
          "speaker_id": "1",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100033732.1.22",
          "form": "간단한 조리법이라 너무 좋네요 ㅎ",
          "original_form": "간단한 조리법이라 너무 좋네요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 08:30"
        },
        {
          "id": "MDRW2100033732.1.23",
          "form": "한국인의 마무리는 볶음밥",
          "original_form": "한국인의 마무리는 볶음밥",
