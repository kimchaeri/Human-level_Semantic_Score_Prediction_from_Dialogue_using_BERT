          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000950.1.3",
          "form": "카이야코 너무 귀여워...",
          "original_form": "카이야코 너무 귀여워...",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000950.1.4",
          "form": "해쭈 조카 카야 너무 귀여워",
          "original_form": "해쭈 조카 카야 너무 귀여워",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000950.1.5",
          "form": "민준이도 너무 귀엽더라",
          "original_form": "민준이도 너무 귀엽더라",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000950.1.6",
          "form": "쁘큐보이랑 해쭈도 너무 잘 어울려",
          "original_form": "쁘큐보이랑 해쭈도 너무 잘 어울려",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.7",
          "form": "나도 그런 가족있으면",
          "original_form": "나도 그런 가족있으면",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.8",
          "form": "유튜브 바로 시작할듯",
          "original_form": "유튜브 바로 시작할듯",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.9",
          "form": "나도.. 어떻개 그렇게 매일 재미있을까",
          "original_form": "나도.. 어떻개 그렇게 매일 재미있을까",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.10",
          "form": "꿀잼 귀여운 가족",
          "original_form": "꿀잼 귀여운 가족",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.11",
          "form": "근데 해쭈 영상 너무 자주 안올려",
          "original_form": "근데 해쭈 영상 너무 자주 안올려",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.12",
          "form": "내가 좀 많이 봐서 그런 거 같기도 한데",
          "original_form": "내가 좀 많이 봐서 그런 거 같기도 한데",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.13",
          "form": "그리고 예랑가랑 영상도 봤어",
          "original_form": "그리고 예랑가랑 영상도 봤어",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.14",
          "form": "그니까ㅠ",
          "original_form": "그니까ㅠ",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.15",
          "form": "근데 그건 맞는거같아",
          "original_form": "근데 그건 맞는거같아",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.16",
          "form": "해쭈 유독 영상텀이 길어ㅠ",
          "original_form": "해쭈 유독 영상텀이 길어ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.17",
          "form": "예랑가랑 그림 잘 그려서 부럽다",
          "original_form": "예랑가랑 그림 잘 그려서 부럽다",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000950.1.18",
          "form": "물론 전공자라 그런거겜ㅅ지맘~~~",
          "original_form": "물론 전공자라 그런거겜ㅅ지맘~~~",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
