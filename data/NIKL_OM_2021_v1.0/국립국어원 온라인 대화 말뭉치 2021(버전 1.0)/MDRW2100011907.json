      "utterance": [
        {
          "id": "MDRW2100011907.1.1",
          "form": "비오는날은",
          "original_form": "비오는날은",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011907.1.2",
          "form": "파전이땡기고...",
          "original_form": "파전이땡기고...",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011907.1.3",
          "form": "저녁 뭐 드시나요..",
          "original_form": "저녁 뭐 드시나요..",
          "speaker_id": "2",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011907.1.4",
          "form": "원래는 그냥",
          "original_form": "원래는 그냥",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011907.1.5",
          "form": "라면먹고싶었는데",
          "original_form": "라면먹고싶었는데",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011907.1.6",
          "form": "친구가 청첩장준다해서 스테이크로 메뉴변경..",
          "original_form": "친구가 청첩장준다해서 스테이크로 메뉴변경..",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011907.1.7",
          "form": "뭐드세요??",
          "original_form": "뭐드세요??",
          "speaker_id": "1",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011907.1.8",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011907.1.9",
          "form": "저는 그냥",
          "original_form": "저는 그냥",
          "speaker_id": "2",
          "time": "20210824 17:17"
        },
        {
          "id": "MDRW2100011907.1.10",
          "form": "시판 소스로 간단하게 스파게티 해먹으려구요",
          "original_form": "시판 소스로 간단하게 스파게티 해먹으려구요",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.11",
          "form": "스파게키가 근데",
          "original_form": "스파게키가 근데",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.12",
          "form": "예상외로 건강식이라곸ㅋㅋㅋㅋ",
          "original_form": "예상외로 건강식이라곸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.13",
          "form": "아진짜요?? ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아진짜요?? ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.14",
          "form": "넼ㅋㅋㅋㅋ오히려 살도안찌구",
          "original_form": "넼ㅋㅋㅋㅋ오히려 살도안찌구",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.15",
          "form": "상상도 못햤는데..",
          "original_form": "상상도 못햤는데..",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.16",
          "form": "탄단지 보충도되고",
          "original_form": "탄단지 보충도되고",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.17",
          "form": "오호..",
          "original_form": "오호..",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.18",
          "form": "저도그래서 그거듣고 스파게티 면 사왔어요",
          "original_form": "저도그래서 그거듣고 스파게티 면 사왔어요",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.19",
          "form": "두부면으로하면 더 좋다던데?!",
          "original_form": "두부면으로하면 더 좋다던데?!",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011907.1.21",
          "form": "아 두부면 맛닜죠",
          "original_form": "아 두부면 맛닜죠",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011907.1.22",
          "form": "두부면에 바질페스토로 하면",
          "original_form": "두부면에 바질페스토로 하면",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011907.1.23",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011907.1.24",
          "form": "바질페스토 솔직히 비주얼은 별로였는데맛있어서",
          "original_form": "바질페스토 솔직히 비주얼은 별로였는데맛있어서",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011907.1.25",
          "form": "놀랐쟈나여",
          "original_form": "놀랐쟈나여",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011907.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011907.1.27",
          "form": "깻잎페스토도 맛있어요",
          "original_form": "깻잎페스토도 맛있어요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        }
      ]
