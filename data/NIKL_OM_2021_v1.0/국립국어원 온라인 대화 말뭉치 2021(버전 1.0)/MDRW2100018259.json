      "utterance": [
        {
          "id": "MDRW2100018259.1.1",
          "form": "우와싱기",
          "original_form": "우와싱기",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018259.1.2",
          "form": "그나저나 요즘 진짜 가을오나봐요",
          "original_form": "그나저나 요즘 진짜 가을오나봐요",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018259.1.3",
          "form": "얼른 트렌치 입고싳어요",
          "original_form": "얼른 트렌치 입고싳어요",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018259.1.4",
          "form": "저는 가디건이요",
          "original_form": "저는 가디건이요",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018259.1.5",
          "form": "오 가디건",
          "original_form": "오 가디건",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018259.1.6",
          "form": "잘입으면 예쁜데 저는 아직 맘에드는 걸 못찾았어요",
          "original_form": "잘입으면 예쁜데 저는 아직 맘에드는 걸 못찾았어요",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018259.1.7",
          "form": "가을 겨울 사랑",
          "original_form": "가을 겨울 사랑{emoji:♡}",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018259.1.8",
          "form": "전 주로 겹겹이 양파처럼 입어요",
          "original_form": "전 주로 겹겹이 양파처럼 입어요",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018259.1.9",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018259.1.10",
          "form": "저는 그치만 두꺼운 후드티입어요",
          "original_form": "저는 그치만 두꺼운 후드티입어요",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018259.1.11",
          "form": "그게편하고요",
          "original_form": "그게편하고요",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018259.1.12",
          "form": "앞주머니에 폰넣기 개꿀입니다",
          "original_form": "앞주머니에 폰넣기 개꿀입니다",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018259.1.13",
          "form": "오오 후드티",
          "original_form": "오오 후드티",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.14",
          "form": "전 후드티 더워서 잘 못입어요",
          "original_form": "전 후드티 더워서 잘 못입어요",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.15",
          "form": "시무룩",
          "original_form": "시무룩",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.16",
          "form": "땀이많아서",
          "original_form": "땀이많아서",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.17",
          "form": "맨투맨도 못입는다는",
          "original_form": "맨투맨도 못입는다는",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.18",
          "form": "겨울엔 기모가짱",
          "original_form": "겨울엔 기모가짱",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.19",
          "form": "근데 저도 땀많아요 ㅜㅜ",
          "original_form": "근데 저도 땀많아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.20",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.21",
          "form": "안더우세요?!",
          "original_form": "안더우세요?!",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018259.1.22",
          "form": "여름엔 냉장고바지입구요",
          "original_form": "여름엔 냉장고바지입구요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018259.1.23",
          "form": "겨울엔 후드입어요",
          "original_form": "겨울엔 후드입어요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018259.1.24",
          "form": "퓨즈서울 냉장고바지강추에요",
          "original_form": "퓨즈서울 냉장고바지강추에요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018259.1.25",
          "form": "오 퓨즈서울 많이입더라고요",
          "original_form": "오 퓨즈서울 많이입더라고요",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018259.1.26",
          "form": "가성비가내려와",
          "original_form": "가성비가내려와",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018259.1.27",
          "form": "저는 제이브로스? 거기 슬랙스 냥장고바지 사봤는데",
          "original_form": "저는 제이브로스? 거기 슬랙스 냥장고바지 사봤는데",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018259.1.28",
          "form": "슬랙스유행은 사랑이에요 올여름을 견디게해줬어요",
          "original_form": "슬랙스유행은 사랑이에요 올여름을 견디게해줬어요",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018259.1.29",
          "form": "세일할때사면 완전 괜춘해요",
          "original_form": "세일할때사면 완전 괜춘해요",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018259.1.30",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
