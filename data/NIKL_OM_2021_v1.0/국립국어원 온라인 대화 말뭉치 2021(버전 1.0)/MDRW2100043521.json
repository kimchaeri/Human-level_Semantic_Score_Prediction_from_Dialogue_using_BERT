      "utterance": [
        {
          "id": "MDRW2100043521.1.1",
          "form": "출근함?",
          "original_form": "출근함?",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100043521.1.2",
          "form": "했죠 아침부터 바빴어",
          "original_form": "했죠 아침부터 바빴어",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100043521.1.3",
          "form": "나는 아직 한가한데 뭐했어?",
          "original_form": "나는 아직 한가한데 뭐했어?",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100043521.1.4",
          "form": "곧 시행정감사야",
          "original_form": "곧 시행정감사야",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100043521.1.5",
          "form": "게속바빠?",
          "original_form": "게속바빠?",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100043521.1.6",
          "form": "3년치정리햇어",
          "original_form": "3년치정리햇어",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100043521.1.7",
          "form": "아무래도? 휴가언제써..못가게생김",
          "original_form": "아무래도? 휴가언제써..못가게생김",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100043521.1.8",
          "form": "10월 정검있고, 시행정감사하고..곧 연말이고 1월에는 또 월초라 바쁘겠지",
          "original_form": "10월 정검있고, 시행정감사하고..곧 연말이고 1월에는 또 월초라 바쁘겠지",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100043521.1.9",
          "form": "겨울에도갈수있는거야?",
          "original_form": "겨울에도갈수있는거야?",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100043521.1.10",
          "form": "그러면 11월...",
          "original_form": "그러면 11월...",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100043521.1.11",
          "form": "갈수는있지 근데 그때가면 눈치보여",
          "original_form": "갈수는있지 근데 그때가면 눈치보여",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100043521.1.12",
          "form": "따흑",
          "original_form": "따흑",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100043521.1.13",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100043521.1.14",
          "form": "다들갈때가야돼 휴가..갑자기 쉬는거같아져서",
          "original_form": "다들갈때가야돼 휴가..갑자기 쉬는거같아져서",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100043521.1.15",
          "form": "휴가 솔직히 한달줘야해",
          "original_form": "휴가 솔직히 한달줘야해",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100043521.1.16",
          "form": "아인정 휴가 더줘라",
          "original_form": "아인정 휴가 더줘라",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100043521.1.17",
          "form": "진심 유급으로",
          "original_form": "진심 유급으로",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100043521.1.18",
          "form": "월급도 쥐꼬리니까 휴가비도 줘",
