      "utterance": [
        {
          "id": "MDRW2100035184.1.1",
          "form": "안녕하세요? 아침은 드셨나요?",
          "original_form": "안녕하세요? 아침은 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100035184.1.2",
          "form": "아침은 잘 안 먹어서요ㅜ",
          "original_form": "아침은 잘 안 먹어서요ㅜ",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100035184.1.3",
          "form": "요거트 한잔 마시고 나왔어요",
          "original_form": "요거트 한잔 마시고 나왔어요",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100035184.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100035184.1.5",
          "form": "전 오늘 늦잠ㅇ르 자서 걸렸어요 ㅋㅋㅋ",
          "original_form": "전 오늘 늦잠ㅇ르 자서 걸렸어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100035184.1.6",
          "form": "아이고ㅎㅎ",
          "original_form": "아이고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100035184.1.7",
          "form": "그냥 대화를 트기 위해 여쭤봤어요ㅎㅎㅎ",
          "original_form": "그냥 대화를 트기 위해 여쭤봤어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100035184.1.8",
          "form": "모닝커피랑 과자를 먹고 있어요",
          "original_form": "모닝커피랑 과자를 먹고 있어요",
          "speaker_id": "1",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100035184.1.9",
          "form": "평소에는 아침 챙겨 드시나요?",
          "original_form": "평소에는 아침 챙겨 드시나요?",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100035184.1.10",
          "form": "평일엔 억지로 먹어요 안그러면 중간에 너무 배고파서요",
          "original_form": "평일엔 억지로 먹어요 안그러면 중간에 너무 배고파서요",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100035184.1.11",
          "form": "시리얼이라도 먹고 나가요",
          "original_form": "시리얼이라도 먹고 나가요",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100035184.1.12",
          "form": "name2님은 어때요?",
          "original_form": "&name2&님은 어때요?",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100035184.1.13",
          "form": "전 그냥 점심까지 죽 굶어요",
          "original_form": "전 그냥 점심까지 죽 굶어요",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100035184.1.14",
          "form": "대신 야식을 늦게 꼭 챙겨먹고요",
          "original_form": "대신 야식을 늦게 꼭 챙겨먹고요",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100035184.1.15",
          "form": "그럼 안되는데ㅜ",
          "original_form": "그럼 안되는데ㅜ",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100035184.1.16",
          "form": "시리얼은 어떤거 드시나요?",
          "original_form": "시리얼은 어떤거 드시나요?",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035184.1.17",
          "form": "저도 야식은 되게 좋아하는데 야식중에 맵고 짠거먹음",
          "original_form": "저도 야식은 되게 좋아하는데 야식중에 맵고 짠거먹음",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035184.1.18",
          "form": "위염이도져요 ㅠㅠㅋㅋ",
          "original_form": "위염이도져요 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035184.1.19",
          "form": "위염이 심하신가요ㅜㅜㅜ",
          "original_form": "위염이 심하신가요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100035184.1.20",
          "form": "밤이면 맵고짜고기름진게 당기는데",
          "original_form": "밤이면 맵고짜고기름진게 당기는데",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100035184.1.21",
          "form": "아주 심하진 않는데 밤에 매운?고기 볶음? 이런거 먹으면 다음날 아침에 자꾸 기침해요 ㅋㅋ 위염증상으로,,",
          "original_form": "아주 심하진 않는데 밤에 매운?고기 볶음? 이런거 먹으면 다음날 아침에 자꾸 기침해요 ㅋㅋ 위염증상으로,,",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100035184.1.22",
          "form": "아 기침도 위염증상이군요ㅜㅜ",
          "original_form": "아 기침도 위염증상이군요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100035184.1.23",
          "form": "사람이 잘때 위산이 나오면",
          "original_form": "사람이 잘때 위산이 나오면",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100035184.1.24",
          "form": "꼭 아침 챙겨 드셔야겠어요",
          "original_form": "꼭 아침 챙겨 드셔야겠어요",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100035184.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100035184.1.26",
          "form": "목에 있는 괄약근이 녹으면서 목쪽에 위산자극이 가거든요",
          "original_form": "목에 있는 괄약근이 녹으면서 목쪽에 위산자극이 가거든요",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100035184.1.27",
          "form": "목이 따가워지만 전 기침이 나와요",
          "original_form": "목이 따가워지만 전 기침이 나와요",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100035184.1.28",
          "form": "헉 그런 구조였군요ㄷㄷㄷ",
