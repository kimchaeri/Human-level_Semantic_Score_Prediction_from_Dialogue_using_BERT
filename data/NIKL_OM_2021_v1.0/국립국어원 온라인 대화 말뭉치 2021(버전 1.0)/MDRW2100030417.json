        {
          "id": "MDRW2100030417.1.1",
          "form": "싸이월드 했어?",
          "original_form": "싸이월드 했어?",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030417.1.2",
          "form": "아 죄송합니다",
          "original_form": "아 죄송합니다",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030417.1.3",
          "form": "1도 안했어요",
          "original_form": "1도 안했어요",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030417.1.4",
          "form": "그럸 수 있죠",
          "original_form": "그럸 수 있죠",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030417.1.5",
          "form": "나도 안했는걸",
          "original_form": "나도 안했는걸",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030417.1.6",
          "form": "계정은 있지만",
          "original_form": "계정은 있지만",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030417.1.7",
          "form": "고스트란다",
          "original_form": "고스트란다",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030417.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030417.1.9",
          "form": "뭐에욬ㅋㅋ",
          "original_form": "뭐에욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100030417.1.10",
          "form": "물어봤지 해봤냐고는 안해ㅛ는걸",
          "original_form": "물어봤지 해봤냐고는 안해ㅛ는걸",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030417.1.11",
          "form": "근데 왜 닏네임이name2이야?",
          "original_form": "근데 왜 닏네임이&name2&이야?",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030417.1.12",
          "form": "name2",
          "original_form": "&name2&",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030417.1.13",
          "form": "싸이월드 부활한김에 그걸 닉네임으로 써보면?",
          "original_form": "싸이월드 부활한김에 그걸 닉네임으로 써보면?",
          "speaker_id": "1",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030417.1.14",
          "form": "잉 닉네임 이전에 싸이월드를 하라구요?",
          "original_form": "잉 닉네임 이전에 싸이월드를 하라구요?",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030417.1.15",
          "form": "저 블로그도 안하는데요 ㅋㅋㅋㅋ",
          "original_form": "저 블로그도 안하는데요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030417.1.16",
          "form": "싸이 그때 안했으면 그때는 뭐했데",
          "original_form": "싸이 그때 안했으면 그때는 뭐했데",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030417.1.17",
          "form": "애니메이션을.. 열심히 본거 같습니다",
          "original_form": "애니메이션을.. 열심히 본거 같습니다",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030417.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030417.1.19",
          "form": "싸이월드 이야기가 적합하지 못했나요 ㅋㅋㅋ",
          "original_form": "싸이월드 이야기가 적합하지 못했나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030417.1.20",
          "form": "이거 부활한거 나름 되게 큰 이슈라서 골라본건데",
          "original_form": "이거 부활한거 나름 되게 큰 이슈라서 골라본건데",
          "speaker_id": "1",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030417.1.21",
          "form": "음 안해봐서 크게 할말이 있는건 아니지만",
          "original_form": "음 안해봐서 크게 할말이 있는건 아니지만",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030417.1.22",
          "form": "그냥 부활해서 흑역사가 다 살아났잖아",
          "original_form": "그냥 부활해서 흑역사가 다 살아났잖아",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030417.1.23",
          "form": "아름다운(?) 과거는 그냥 과거로 두는게 좋다는 생각이 들어",
          "original_form": "아름다운(?) 과거는 그냥 과거로 두는게 좋다는 생각이 들어",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030417.1.24",
          "form": "블로그도 최근에 챌린지하면서",
          "original_form": "블로그도 최근에 챌린지하면서",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030417.1.25",
          "form": "다 살아나버렸고",
          "original_form": "다 살아나버렸고",
