      "utterance": [
        {
          "id": "MDRW2100002475.1.1",
          "form": "여행에 대해서",
          "original_form": "여행에 대해서",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002475.1.2",
          "form": "말해보자",
          "original_form": "말해보자",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002475.1.3",
          "form": "지금 당장 떠나고 싶은",
          "original_form": "지금 당장 떠나고 싶은",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002475.1.4",
          "form": "여행지 같은거",
          "original_form": "여행지 같은거",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002475.1.5",
          "form": "있음?",
          "original_form": "있음?",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002475.1.6",
          "form": "난 기승전결 일본이지",
          "original_form": "난 기승전결 일본이지",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002475.1.7",
          "form": "온천 가보고 싶음",
          "original_form": "온천 가보고 싶음",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002475.1.8",
          "form": "일본도 좋긴하지",
          "original_form": "일본도 좋긴하지",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002475.1.9",
          "form": "난 동해랑",
          "original_form": "난 동해랑",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002475.1.10",
          "form": "제주도 정도?",
          "original_form": "제주도 정도?",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002475.1.11",
          "form": "해외라면 일본쪽이고",
          "original_form": "해외라면 일본쪽이고",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002475.1.12",
          "form": "일본간다면 한 1달정도 그냥 기차나 이런거 쭉 타면서 전국적으로 돌까싶음",
          "original_form": "일본간다면 한 1달정도 그냥 기차나 이런거 쭉 타면서 전국적으로 돌까싶음",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002475.1.13",
          "form": "난 우리집 옆이 바다고 산이고 그러니까 국내는 모르겠더라",
          "original_form": "난 우리집 옆이 바다고 산이고 그러니까 국내는 모르겠더라",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002475.1.14",
          "form": "국내에 의외로",
          "original_form": "국내에 의외로",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002475.1.15",
          "form": "여행지 음청 많음",
          "original_form": "여행지 음청 많음",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002475.1.16",
          "form": "막 여행지 가끔씩 검색해보면",
          "original_form": "막 여행지 가끔씩 검색해보면",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.17",
          "form": "숨겨진 곳 많긴하더아",
          "original_form": "숨겨진 곳 많긴하더아",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.18",
          "form": "제주도도 안가본곳 천지고 ㅋㅋ",
          "original_form": "제주도도 안가본곳 천지고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.19",
          "form": "제주도는 가고 싶지 않더라",
          "original_form": "제주도는 가고 싶지 않더라",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.20",
          "form": "우선 제주도는 올레길 절반의 절반도 못돌았더라",
          "original_form": "우선 제주도는 올레길 절반의 절반도 못돌았더라",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.21",
          "form": "바람만 불것 같음",
          "original_form": "바람만 불것 같음",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.22",
          "form": "올레길 트레킹하고싶은데",
          "original_form": "올레길 트레킹하고싶은데",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.23",
          "form": "제주도 실제로 가면",
          "original_form": "제주도 실제로 가면",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.24",
          "form": "바람 엄청 붐",
          "original_form": "바람 엄청 붐",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002475.1.25",
          "form": "근데 바람이",
          "original_form": "근데 바람이",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002475.1.26",
          "form": "그냥 우리가 흔히 받는 바람이 아니라",
          "original_form": "그냥 우리가 흔히 받는 바람이 아니라",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002475.1.27",
          "form": "살짝 바닷가랑 육지섞인느낌",
          "original_form": "살짝 바닷가랑 육지섞인느낌",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002475.1.28",
          "form": "암튼 제주도를 차후에 가게된다면",
          "original_form": "암튼 제주도를 차후에 가게된다면",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002475.1.29",
          "form": "꼭 마라도는 가보셈",
          "original_form": "꼭 마라도는 가보셈",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002475.1.30",
          "form": "아 물론 배멀미있으면 안됨 ㅋㅋ",
          "original_form": "아 물론 배멀미있으면 안됨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002475.1.31",
          "form": "배로 10분정돈가 가거든",
          "original_form": "배로 10분정돈가 가거든",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002475.1.32",
          "form": "그거 우리집이 산이고 바다고 해서 바람 오지는데",
          "original_form": "그거 우리집이 산이고 바다고 해서 바람 오지는데",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002475.1.33",
          "form": "가야될 이유를 모르겠어",
          "original_form": "가야될 이유를 모르겠어",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002475.1.34",
          "form": "난 분지지형이다",
          "original_form": "난 분지지형이다",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002475.1.35",
          "form": "그래서 가고싶다 제주도",
          "original_form": "그래서 가고싶다 제주도",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002475.1.36",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002475.1.37",
          "form": "암튼 일본간다면",
          "original_form": "암튼 일본간다면",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002475.1.38",
          "form": "나가사끼 짬뽕이랑 타코야끼 오코노미야끼는 꼭 먹고",
          "original_form": "나가사끼 짬뽕이랑 타코야끼 오코노미야끼는 꼭 먹고",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002475.1.39",
          "form": "만약 일본여행 가면 같이 갈래?",
          "original_form": "만약 일본여행 가면 같이 갈래?",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002475.1.40",
          "form": "그러면 나도 좋지",
          "original_form": "그러면 나도 좋지",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002475.1.41",
          "form": "얘들도 몇명껴서 같이 가자",
          "original_form": "얘들도 몇명껴서 같이 가자",
          "speaker_id": "1",
          "time": "20210820 12:48"
        }
      ]
    }
  ]
