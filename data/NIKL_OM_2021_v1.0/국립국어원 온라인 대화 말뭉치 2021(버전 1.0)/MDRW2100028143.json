      "utterance": [
        {
          "id": "MDRW2100028143.1.1",
          "form": "맛집....가고싶다...",
          "original_form": "맛집....가고싶다...",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028143.1.2",
          "form": "멋진 맛집에서 몇집 대기타서 들어가서 즐기고 싶다",
          "original_form": "멋진 맛집에서 몇집 대기타서 들어가서 즐기고 싶다",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028143.1.3",
          "form": "이거 점점 코로나 위시리스트",
          "original_form": "이거 점점 코로나 위시리스트",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028143.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028143.1.5",
          "form": "인정합니다...",
          "original_form": "인정합니다...",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028143.1.6",
          "form": "전 원래 맛집 줄 서는 거 진짜 싫어하는데",
          "original_form": "전 원래 맛집 줄 서는 거 진짜 싫어하는데",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028143.1.7",
          "form": "그게 그리울줄은...",
          "original_form": "그게 그리울줄은...",
          "speaker_id": "2",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028143.1.8",
          "form": "ㅠㅠ현재: 방역수칙 지키세요",
          "original_form": "ㅠㅠ현재: 방역수칙 지키세요",
          "speaker_id": "1",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028143.1.9",
          "form": "근데 사실 가까운 곳은 그냥 가도 되지 싶은데 이 시국에...를 못 이기네",
          "original_form": "근데 사실 가까운 곳은 그냥 가도 되지 싶은데 이 시국에...를 못 이기네",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028143.1.10",
          "form": "가까운 곳은 가도 돼죠",
          "original_form": "가까운 곳은 가도 돼죠",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028143.1.11",
          "form": "저두 가까운 곳은...일찍 가니까 줄 없던데...",
          "original_form": "저두 가까운 곳은...일찍 가니까 줄 없던데...",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028143.1.12",
          "form": "비양심 고백...",
          "original_form": "비양심 고백...",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100028143.1.13",
          "form": "맞아 가까운 곳은 가도 돼 먹고 살아야지",
          "original_form": "맞아 가까운 곳은 가도 돼 먹고 살아야지",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100028143.1.14",
          "form": "아 여름에 냉우동 먹을 타이밍을 놓쳤다 요즘 가면 냉우동 들어갔을지",
          "original_form": "아 여름에 냉우동 먹을 타이밍을 놓쳤다 요즘 가면 냉우동 들어갔을지",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100028143.1.15",
          "form": "헐 아슬아슬하게 있지 않을까요...",
          "original_form": "헐 아슬아슬하게 있지 않을까요...",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028143.1.16",
          "form": "벌써 들어가면 너무 슬퍼...",
          "original_form": "벌써 들어가면 너무 슬퍼...",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028143.1.17",
          "form": "부산 냉우동 맛집 있는 건가요?",
          "original_form": "부산 냉우동 맛집 있는 건가요?",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028143.1.18",
          "form": "광안리에 다케다야라고....",
          "original_form": "광안리에 다케다야라고....",
          "speaker_id": "1",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028143.1.19",
          "form": "거기 냉우동이랑 붓카케우동이 맛있어",
          "original_form": "거기 냉우동이랑 붓카케우동이 맛있어",
          "speaker_id": "1",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028143.1.20",
          "form": "헐 대박 완전 꿀팁...",
          "original_form": "헐 대박 완전 꿀팁...",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028143.1.21",
          "form": "꼭 기억할게요...코로나 끝나고 꼭 가야지...",
          "original_form": "꼭 기억할게요...코로나 끝나고 꼭 가야지...",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028143.1.22",
          "form": "광주는 뭔가 답례로 알려드릴 맛집이 없네...",
          "original_form": "광주는 뭔가 답례로 알려드릴 맛집이 없네...",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028143.1.23",
          "form": "동명동 소복 마제면이 맛있었답니당...",
          "original_form": "동명동 소복 마제면이 맛있었답니당...",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028143.1.24",
          "form": "기억해두겠어..나도 언젠가 광주 가서 맛집 투어할 테다",
          "original_form": "기억해두겠어..나도 언젠가 광주 가서 맛집 투어할 테다",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100028143.1.25",
          "form": "길잡이로 언제든 불러주셔도 됩니다ㅋㅋㅋ!!",
          "original_form": "길잡이로 언제든 불러주셔도 됩니다ㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028143.1.26",
          "form": "근데 광주는 맛집 잘 없어서...",
          "original_form": "근데 광주는 맛집 잘 없어서...",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028143.1.27",
          "form": "ㅋㅋㅋㅋ그 지역 음식 먹어보는 게 여행의 묘미죠",
          "original_form": "ㅋㅋㅋㅋ그 지역 음식 먹어보는 게 여행의 묘미죠",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028143.1.28",
          "form": "부산도...뭐...사실 남들한테 맛집 추천하라 하면 뭘 해야 할지 모르겠어 ㅋㅋ",
          "original_form": "부산도...뭐...사실 남들한테 맛집 추천하라 하면 뭘 해야 할지 모르겠어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028143.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜여? 근데 일단 밀면이랑 돼지국밥이랑 물회만 먹어두",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜여? 근데 일단 밀면이랑 돼지국밥이랑 물회만 먹어두",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028143.1.30",
