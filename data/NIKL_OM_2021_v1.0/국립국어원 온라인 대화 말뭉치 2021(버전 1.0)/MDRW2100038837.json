      "utterance": [
        {
          "id": "MDRW2100038837.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038837.1.2",
          "form": "다 잘먹어요",
          "original_form": "다 잘먹어요",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038837.1.3",
          "form": "매운거 빼고",
          "original_form": "매운거 빼고",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038837.1.4",
          "form": "저도 없어서 못 먹는답니다ㅋㅋ",
          "original_form": "저도 없어서 못 먹는답니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038837.1.5",
          "form": "아 전 매운 거 좋아하는데",
          "original_form": "아 전 매운 거 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038837.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100038837.1.7",
          "form": "인생 뭐 있나요 먹기 위해 사는 거죠 ㅋ",
          "original_form": "인생 뭐 있나요 먹기 위해 사는 거죠 ㅋ",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100038837.1.8",
          "form": "매운거잘드시는군요",
          "original_form": "매운거잘드시는군요",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100038837.1.9",
          "form": "맞습니다 먹는 거 외엔 낙이 없어요ㅋㅋㅋ",
          "original_form": "맞습니다 먹는 거 외엔 낙이 없어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100038837.1.10",
          "form": "저도 처음에 못 먹었는데",
          "original_form": "저도 처음에 못 먹었는데",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100038837.1.11",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038837.1.12",
          "form": "어느 순간 매운 걸 찾고 있더라구요",
          "original_form": "어느 순간 매운 걸 찾고 있더라구요",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038837.1.13",
          "form": "갈수록 적응되셨나봐요",
          "original_form": "갈수록 적응되셨나봐요",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038837.1.14",
          "form": "내 갈수록 더 자극적인 걸 찾게 되더라구요",
          "original_form": "내 갈수록 더 자극적인 걸 찾게 되더라구요",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038837.1.15",
          "form": "매운거 먹고나면 스트레스가 풀리시나요?",
          "original_form": "매운거 먹고나면 스트레스가 풀리시나요?",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038837.1.16",
          "form": "이제 한 해 한 해 나이가 드니 속이 아프긴 해요",
          "original_form": "이제 한 해 한 해 나이가 드니 속이 아프긴 해요",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038837.1.17",
          "form": "아이고 이제 조금 줄이셔야겠네요ㅠ",
          "original_form": "아이고 이제 조금 줄이셔야겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038837.1.18",
          "form": "저는 스트레스보다",
          "original_form": "저는 스트레스보다",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038837.1.19",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038837.1.20",
          "form": "안 매운 걸 먹으면 배가 덜 찬다고 해야 되나??",
          "original_form": "안 매운 걸 먹으면 배가 덜 찬다고 해야 되나??",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038837.1.21",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038837.1.22",
          "form": "그래서 계속 자극적인걸 더 찾게 돼요",
          "original_form": "그래서 계속 자극적인걸 더 찾게 돼요",
