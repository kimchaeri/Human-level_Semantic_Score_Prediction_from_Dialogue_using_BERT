      "utterance": [
        {
          "id": "MDRW2100007227.1.1",
          "form": "진짜 강추해요 꼭보세요",
          "original_form": "진짜 강추해요 꼭보세요",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007227.1.2",
          "form": "다시보고싶을정도예요",
          "original_form": "다시보고싶을정도예요",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007227.1.3",
          "form": "그리고 중간에 실수가 있었던거같은데 그것도 애드립으로 잘 넘어가더라구요 ㅎ",
          "original_form": "그리고 중간에 실수가 있었던거같은데 그것도 애드립으로 잘 넘어가더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007227.1.4",
          "form": "역시 베테랑이랑 ㅎ",
          "original_form": "역시 베테랑이랑 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007227.1.5",
          "form": "요즘도 공연하는거같긴 하던데 찾아봐야겠네요",
          "original_form": "요즘도 공연하는거같긴 하던데 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007227.1.6",
          "form": "공연해요?",
          "original_form": "공연해요?",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007227.1.7",
          "form": "서울쪽이겠죠?ㅜㅠ",
          "original_form": "서울쪽이겠죠?ㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007227.1.8",
          "form": "저번에 유퀴즈조승우나와서 얘기했던거같은데",
          "original_form": "저번에 유퀴즈조승우나와서 얘기했던거같은데",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007227.1.9",
          "form": "요즘은 드라큘라? 그뮤지컬하나본데요",
          "original_form": "요즘은 드라큘라? 그뮤지컬하나본데요",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007227.1.10",
          "form": "김준수 나오는",
          "original_form": "김준수 나오는",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007227.1.11",
          "form": "유퀴즈에 많은사람이 나오는군요",
          "original_form": "유퀴즈에 많은사람이 나오는군요",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007227.1.12",
          "form": "홍보차 나온거인듯ㅎㅎ 코로나땜에 관객수도 적고하니 공연은 서울만 할거같아요",
          "original_form": "홍보차 나온거인듯ㅎㅎ 코로나땜에 관객수도 적고하니 공연은 서울만 할거같아요",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007227.1.13",
          "form": "대학땐 대학로가서 연극도 보고했는데",
          "original_form": "대학땐 대학로가서 연극도 보고했는데",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007227.1.14",
          "form": "저두 연극 좋아했어요",
          "original_form": "저두 연극 좋아했어요",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007227.1.15",
          "form": "혹시 염쟁이 유씨 연극 보셨어요?",
          "original_form": "혹시 염쟁이 유씨 연극 보셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007227.1.16",
          "form": "아뇨 첨들어봐요ㅎ",
          "original_form": "아뇨 첨들어봐요ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007227.1.17",
          "form": "유명한거예요?",
          "original_form": "유명한거예요?",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007227.1.18",
          "form": "아..,좀 오래됐는지도..근데 진짜 볼만해요",
          "original_form": "아..,좀 오래됐는지도..근데 진짜 볼만해요",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007227.1.19",
          "form": "유명한건 대학로가면 항상 하더라구요",
          "original_form": "유명한건 대학로가면 항상 하더라구요",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007227.1.20",
          "form": "소극장에서 보는것도 나름 좋아요 배우도 가까이봐ㅡ",
          "original_form": "소극장에서 보는것도 나름 좋아요 배우도 가까이봐ㅡ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007227.1.21",
          "form": "보고",
          "original_form": "보고",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007227.1.22",
          "form": "맞아요 대학로에서도 했을커예요",
          "original_form": "맞아요 대학로에서도 했을커예요",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007227.1.23",
          "form": "나이먹으니 제목 생각도 안나네요ㅋㅋ",
          "original_form": "나이먹으니 제목 생각도 안나네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007227.1.24",
          "form": "저 소극장에서 봤어요",
          "original_form": "저 소극장에서 봤어요",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007227.1.25",
          "form": "서울 홍대인가? 거기 가서도 보고",
          "original_form": "서울 홍대인가? 거기 가서도 보고",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007227.1.26",
          "form": "이젠 좀 큰홀에서 하는거 보고싶긴해요ㅎ",
          "original_form": "이젠 좀 큰홀에서 하는거 보고싶긴해요ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007227.1.27",
          "form": "vip석에서",
          "original_form": "vip석에서",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007227.1.28",
          "form": "저는 뮤지컬같은거 큰거는 VIP아니면 알석으로봐요",
          "original_form": "저는 뮤지컬같은거 큰거는 VIP아니면 알석으로봐요",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007227.1.29",
          "form": "에스석 인가? 뒤에서한번보니 안보이더라구요",
          "original_form": "에스석 인가? 뒤에서한번보니 안보이더라구요",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007227.1.30",
          "form": "그냥 돈좀주고 좋은좌석이 좋더라구요",
          "original_form": "그냥 돈좀주고 좋은좌석이 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007227.1.31",
          "form": "이왕보는거ㅎ",
          "original_form": "이왕보는거ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:18"
        }
      ]
