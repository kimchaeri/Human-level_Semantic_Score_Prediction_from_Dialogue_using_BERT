      "utterance": [
        {
          "id": "MDRW2100039819.1.1",
          "form": "아 그거 첨에 예약을 못해서 ㅋㅋㅋ",
          "original_form": "아 그거 첨에 예약을 못해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039819.1.2",
          "form": "가시나요?",
          "original_form": "가시나요?",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039819.1.3",
          "form": "아... 수도권4단계일땐 갈 수 없죠",
          "original_form": "아... 수도권4단계일땐 갈 수 없죠",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039819.1.4",
          "form": "3단계내려가고",
          "original_form": "3단계내려가고",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039819.1.5",
          "form": "예약성공하면 당장 가고싶네요",
          "original_form": "예약성공하면 당장 가고싶네요",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039819.1.6",
          "form": "네 작품라인업이 어마어마하더라고요",
          "original_form": "네 작품라인업이 어마어마하더라고요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039819.1.7",
          "form": "그니까요!!!",
          "original_form": "그니까요!!!",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039819.1.8",
          "form": "진짜진짜 이번에안보면",
          "original_form": "진짜진짜 이번에안보면",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039819.1.9",
          "form": "다음이없을거같지않아요?",
          "original_form": "다음이없을거같지않아요?",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039819.1.10",
          "form": "그래서 완전 죽어도 봐야한다",
          "original_form": "그래서 완전 죽어도 봐야한다",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039819.1.11",
          "form": "뭐 이런 생각 들더라구요",
          "original_form": "뭐 이런 생각 들더라구요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039819.1.12",
          "form": "평생 못볼거같아서",
          "original_form": "평생 못볼거같아서",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039819.1.13",
          "form": "ㅋㅋㅋㅋ 그건 그래요",
          "original_form": "ㅋㅋㅋㅋ 그건 그래요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039819.1.14",
          "form": "한자리에서 볼수있는 거니까요",
          "original_form": "한자리에서 볼수있는 거니까요",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039819.1.15",
          "form": "해외안나가면 못볼 화가들도있구..",
          "original_form": "해외안나가면 못볼 화가들도있구..",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039819.1.16",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039819.1.17",
          "form": "그쵸 이럴때 봐둬야해요..",
          "original_form": "그쵸 이럴때 봐둬야해요..",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039819.1.18",
          "form": "그쵸그쵸!!",
          "original_form": "그쵸그쵸!!",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039819.1.19",
          "form": "완전 가고싶어요....",
          "original_form": "완전 가고싶어요....",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039819.1.20",
          "form": "가본 전시중에 기억에 남는 전시 있으세요?",
          "original_form": "가본 전시중에 기억에 남는 전시 있으세요?",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039819.1.21",
          "form": "음.......... 최근에 간게업서엇.... 기억이 하나도..... ㅠㅠ",
          "original_form": "음.......... 최근에 간게업서엇.... 기억이 하나도..... ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039819.1.22",
          "form": "아 있다고 말하고 싶어요 ㅠㅠㅠ",
          "original_form": "아 있다고 말하고 싶어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039819.1.23",
          "form": "아 예전거도 기억 안 나시나요 ㅋㅋ",
          "original_form": "아 예전거도 기억 안 나시나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039819.1.24",
          "form": "기억나는건",
          "original_form": "기억나는건",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039819.1.25",
          "form": "중딩때 방학숙제가",
          "original_form": "중딩때 방학숙제가",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039819.1.26",
          "form": "미술관가기라",
          "original_form": "미술관가기라",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039819.1.27",
          "form": "비엔날레가고",
          "original_form": "비엔날레가고",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039819.1.28",
          "form": "뭐그랬던거 기억나는데",
          "original_form": "뭐그랬던거 기억나는데",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039819.1.29",
          "form": "뭘 봤는지는....",
          "original_form": "뭘 봤는지는....",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039819.1.30",
          "form": "아 비엔날레 추억이네요 ㅋㅋㅋ",
          "original_form": "아 비엔날레 추억이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100039819.1.31",
          "form": "단체로 가고 그런건 기억 잘 안 나죠 ㅎㅎ",
          "original_form": "단체로 가고 그런건 기억 잘 안 나죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100039819.1.32",
          "form": "그쵸 ㅋㅋㅋ 완전 추억이에요",
          "original_form": "그쵸 ㅋㅋㅋ 완전 추억이에요",
