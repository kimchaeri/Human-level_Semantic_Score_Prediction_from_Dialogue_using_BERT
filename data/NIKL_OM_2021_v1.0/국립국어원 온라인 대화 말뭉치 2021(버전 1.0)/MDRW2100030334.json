        },
        {
          "id": "MDRW2100030334.1.2",
          "form": "무슨 김밥 좋아해요",
          "original_form": "무슨 김밥 좋아해요",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030334.1.3",
          "form": "돈까스나 참치",
          "original_form": "돈까스나 참치",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030334.1.4",
          "form": "저도 돈까스 좋아해요",
          "original_form": "저도 돈까스 좋아해요",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030334.1.5",
          "form": "들어간거좋아해요",
          "original_form": "들어간거좋아해요",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030334.1.6",
          "form": "김치김밥도 맛있구요",
          "original_form": "김치김밥도 맛있구요",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030334.1.7",
          "form": "치즈도",
          "original_form": "치즈도",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.8",
          "form": "땡초김받도 좋아해요",
          "original_form": "땡초김받도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.9",
          "form": "김밥은 다 맛있네요",
          "original_form": "김밥은 다 맛있네요",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.10",
          "form": "매워서 속이쓰리지만",
          "original_form": "매워서 속이쓰리지만",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.11",
          "form": "몇 줄 드세요",
          "original_form": "몇 줄 드세요",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.12",
          "form": "느끼한걸 잡아주죠",
          "original_form": "느끼한걸 잡아주죠",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.13",
          "form": "한줄에서 두줄사이요",
          "original_form": "한줄에서 두줄사이요",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.14",
          "form": "고봉민 김밥 아세요",
          "original_form": "고봉민 김밥 아세요",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.15",
          "form": "압니다",
          "original_form": "압니다",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.16",
          "form": "존맛이죠",
          "original_form": "존맛이죠",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030334.1.17",
          "form": "아직 안 먹어 봤어요",
          "original_form": "아직 안 먹어 봤어요",
