      "utterance": [
        {
          "id": "MDRW2100034348.1.1",
          "form": "오늘 저녁 뭐드셨어요?",
          "original_form": "오늘 저녁 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100034348.1.2",
          "form": "짜장 짬뽕 고르라면 뭘 고르시나요?",
          "original_form": "짜장 짬뽕 고르라면 뭘 고르시나요?",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100034348.1.3",
          "form": "저는 떡볶이 먹었어요ㅎㅎ",
          "original_form": "저는 떡볶이 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100034348.1.4",
          "form": "저는 짬뽕이요",
          "original_form": "저는 짬뽕이요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034348.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034348.1.6",
          "form": "저는....저도 짬뽕 고를 것 같아요ㅋㅋ",
          "original_form": "저는....저도 짬뽕 고를 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100034348.1.7",
          "form": "뭔가 부재료가 더 많이 들어가는 느낌",
          "original_form": "뭔가 부재료가 더 많이 들어가는 느낌",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100034348.1.8",
          "form": "그럼 탕수육은 찍먹이세요. 부먹이세요?",
          "original_form": "그럼 탕수육은 찍먹이세요. 부먹이세요?",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034348.1.9",
          "form": "저는 담먹?이예요ㅋㅋ",
          "original_form": "저는 담먹?이예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034348.1.10",
          "form": "탕수육 소스에 탕수육 담궈놓고",
          "original_form": "탕수육 소스에 탕수육 담궈놓고",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034348.1.11",
          "form": "먹고싶을때 꺼내먹어요",
          "original_form": "먹고싶을때 꺼내먹어요",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034348.1.12",
          "form": "저는 찍먹이요",
          "original_form": "저는 찍먹이요",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034348.1.13",
          "form": "이렇게된거 호불호 갑니다",
          "original_form": "이렇게된거 호불호 갑니다",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034348.1.14",
          "form": "딱복? 물복?",
          "original_form": "딱복? 물복?",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034348.1.15",
          "form": "ㅋㅋㅋㅋㅋ딱복",
          "original_form": "ㅋㅋㅋㅋㅋ딱복",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100034348.1.16",
          "form": "저는 물복이요ㅋㅋㅋㅋ",
          "original_form": "저는 물복이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100034348.1.17",
          "form": "사실 크게 상관안하고 먹긴해요",
          "original_form": "사실 크게 상관안하고 먹긴해요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100034348.1.18",
          "form": "민트는 좋아하시나요?",
          "original_form": "민트는 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100034348.1.19",
          "form": "복숭아는 뭔들 맛있어요ㅋㅋ",
          "original_form": "복숭아는 뭔들 맛있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100034348.1.20",
          "form": "민트는 따지자면 호!",
          "original_form": "민트는 따지자면 호!",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100034348.1.21",
          "form": "저도 호!",
          "original_form": "저도 호!",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100034348.1.22",
          "form": "민트 맛있죠 시원한 맛이라",
          "original_form": "민트 맛있죠 시원한 맛이라",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100034348.1.23",
          "form": "전 차갑게 먹는게 더 좋아요",
          "original_form": "전 차갑게 먹는게 더 좋아요",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100034348.1.24",
          "form": "근데 요즘 민트 뇌절인게 넘 많아서 점점...별로예요",
          "original_form": "근데 요즘 민트 뇌절인게 넘 많아서 점점...별로예요",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100034348.1.25",
          "form": "맞아요 온갖 과자에 다들어가요...",
          "original_form": "맞아요 온갖 과자에 다들어가요...",
          "speaker_id": "1",
