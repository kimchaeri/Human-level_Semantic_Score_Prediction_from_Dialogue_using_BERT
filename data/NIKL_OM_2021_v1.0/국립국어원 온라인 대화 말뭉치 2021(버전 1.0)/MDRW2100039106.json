      "utterance": [
        {
          "id": "MDRW2100039106.1.1",
          "form": "커피 좋아하세요?^^",
          "original_form": "커피 좋아하세요?^^",
          "speaker_id": "2",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100039106.1.2",
          "form": "네 매일 마셔요",
          "original_form": "네 매일 마셔요",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100039106.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100039106.1.4",
          "form": "저는 좋아는 합니다만 잠을 못자서 어쩌다가 마셔요",
          "original_form": "저는 좋아는 합니다만 잠을 못자서 어쩌다가 마셔요",
          "speaker_id": "2",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100039106.1.5",
          "form": "아 카페인땜에",
          "original_form": "아 카페인땜에",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100039106.1.6",
          "form": "저는 하루2잔은 마시는거같아요",
          "original_form": "저는 하루2잔은 마시는거같아요",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100039106.1.7",
          "form": "그렇군요.. 제가 커피향을 정말 좋아하거든요",
          "original_form": "그렇군요.. 제가 커피향을 정말 좋아하거든요",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100039106.1.8",
          "form": "어떨땐 내려놓고 향만 맡아요",
          "original_form": "어떨땐 내려놓고 향만 맡아요",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100039106.1.9",
          "form": "향은 정말 좋죠",
          "original_form": "향은 정말 좋죠",
          "speaker_id": "1",
          "time": "20210903 12:07"
        },
        {
          "id": "MDRW2100039106.1.10",
          "form": "식사는 하셨어요?",
          "original_form": "식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210903 12:07"
        },
        {
          "id": "MDRW2100039106.1.11",
          "form": "지금 막 먹고 왔어요",
          "original_form": "지금 막 먹고 왔어요",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039106.1.12",
          "form": "오늘 돼지고기 반찬으로 먹었네요",
          "original_form": "오늘 돼지고기 반찬으로 먹었네요",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039106.1.13",
          "form": "돼지고기 맛나셯겠어요",
          "original_form": "돼지고기 맛나셯겠어요",
          "speaker_id": "1",
          "time": "20210903 12:56"
        },
        {
          "id": "MDRW2100039106.1.14",
          "form": "저는 구내식당에 탕수육나와서 맛나게 먹었네요 ㅎ",
          "original_form": "저는 구내식당에 탕수육나와서 맛나게 먹었네요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100039106.1.15",
          "form": "탕수육도 맛있죠.. 구내식당에서 먹는군요",
          "original_form": "탕수육도 맛있죠.. 구내식당에서 먹는군요",
          "speaker_id": "2",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100039106.1.16",
          "form": "일단 점심메뉴 걱정은 안해도 되겠네요",
          "original_form": "일단 점심메뉴 걱정은 안해도 되겠네요",
          "speaker_id": "2",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100039106.1.17",
          "form": "네 주는대로 먹으면되서 좋아요",
          "original_form": "네 주는대로 먹으면되서 좋아요",
          "speaker_id": "1",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100039106.1.18",
          "form": "저는 주부라서 아침점심저녁 메뉴를 항상 생각해야되거든요",
          "original_form": "저는 주부라서 아침점심저녁 메뉴를 항상 생각해야되거든요",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100039106.1.19",
          "form": "그래서 패키지 여행이 좋아요.. 알아서 밥 주니까요..ㅎㅎ",
          "original_form": "그래서 패키지 여행이 좋아요.. 알아서 밥 주니까요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100039106.1.20",
          "form": "3끼를 다 차리세요?",
          "original_form": "3끼를 다 차리세요?",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039106.1.21",
          "form": "네.. 전업주부라서요",
          "original_form": "네.. 전업주부라서요",
          "speaker_id": "2",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100039106.1.22",
          "form": "그래도 아침은 간단히 토스트 시리얼 정도로",
          "original_form": "그래도 아침은 간단히 토스트 시리얼 정도로",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039106.1.23",
          "form": "해결 많이 하잖아요",
          "original_form": "해결 많이 하잖아요",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039106.1.24",
          "form": "저는 제가 밥이랑 국을 먹어야 속이 편한 사람이라..ㅎㅎ",
          "original_form": "저는 제가 밥이랑 국을 먹어야 속이 편한 사람이라..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039106.1.25",
          "form": "남편이 해달라고  한건 아니고 저 땜에 해요",
          "original_form": "남편이 해달라고  한건 아니고 저 땜에 해요",
          "speaker_id": "2",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100039106.1.26",
          "form": "아...그러시군요",
          "original_form": "아...그러시군요",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039106.1.27",
          "form": "습관인거같아요",
          "original_form": "습관인거같아요",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039106.1.28",
          "form": "아침도 챙겨먹는게 좋은데",
          "original_form": "아침도 챙겨먹는게 좋은데",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039106.1.29",
          "form": "시간에 쫓기니 맘편히 먹을수가 없어서 빵으로",
          "original_form": "시간에 쫓기니 맘편히 먹을수가 없어서 빵으로",
          "speaker_id": "1",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100039106.1.30",
          "form": "후다닥하고 나갑니다 ㅎㅎ",
          "original_form": "후다닥하고 나갑니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100039106.1.31",
          "form": "그래도 빵이라도 먹어서 끼니를 거르진 않네요",
          "original_form": "그래도 빵이라도 먹어서 끼니를 거르진 않네요",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039106.1.32",
          "form": "요즘 아침 안먹는 사람들 많잖아요",
          "original_form": "요즘 아침 안먹는 사람들 많잖아요",
          "speaker_id": "2",
          "time": "20210903 13:21"
        }
      ]
