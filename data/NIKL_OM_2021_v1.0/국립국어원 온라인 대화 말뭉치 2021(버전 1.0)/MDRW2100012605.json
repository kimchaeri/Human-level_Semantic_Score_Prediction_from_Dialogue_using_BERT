      "utterance": [
        {
          "id": "MDRW2100012605.1.1",
          "form": "스포츠 좋아해여?",
          "original_form": "스포츠 좋아해여?",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012605.1.2",
          "form": "배구랑 양궁 좋아해요!",
          "original_form": "배구랑 양궁 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012605.1.3",
          "form": "얼마전에 한 도쿄올림픽 보셨어요?",
          "original_form": "얼마전에 한 도쿄올림픽 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012605.1.4",
          "form": "네 양궁 잼나요",
          "original_form": "네 양궁 잼나요",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012605.1.5",
          "form": "양궁 진짜 재밌었던거 같아요",
          "original_form": "양궁 진짜 재밌었던거 같아요",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012605.1.6",
          "form": "대박이져",
          "original_form": "대박이져",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012605.1.7",
          "form": "이번에 출전한 선수분들은 앞으로가 기대되는 분들이 많아서",
          "original_form": "이번에 출전한 선수분들은 앞으로가 기대되는 분들이 많아서",
          "speaker_id": "1",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012605.1.8",
          "form": "다음 파리올림픽이 기대돼요",
          "original_form": "다음 파리올림픽이 기대돼요",
          "speaker_id": "1",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012605.1.9",
          "form": "네 잘하드라요",
          "original_form": "네 잘하드라요",
          "speaker_id": "2",
          "time": "20210825 07:09"
        },
        {
          "id": "MDRW2100012605.1.10",
          "form": "내년에 베이징 하게 올림픽하니",
          "original_form": "내년에 베이징 하게 올림픽하니",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100012605.1.11",
          "form": "벌쎠 기대되네요",
          "original_form": "벌쎠 기대되네요",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100012605.1.12",
          "form": "저도여",
          "original_form": "저도여",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100012605.1.13",
          "form": "스포츠 어떤거 좋아하세요?",
          "original_form": "스포츠 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100012605.1.14",
          "form": "축구여",
          "original_form": "축구여",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100012605.1.15",
          "form": "내년에 월드컵도 하잖아요!",
          "original_form": "내년에 월드컵도 하잖아요!",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100012605.1.16",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "2",
