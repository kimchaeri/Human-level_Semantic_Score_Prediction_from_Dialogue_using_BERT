      "utterance": [
        {
          "id": "MDRW2100019232.1.1",
          "form": "걍 양식장으로 바꾸고",
          "original_form": "걍 양식장으로 바꾸고",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019232.1.2",
          "form": "당장 고양이 밥주기 쓰다듬기 강아지풀 흔들기",
          "original_form": "당장 고양이 밥주기 쓰다듬기 강아지풀 흔들기",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019232.1.3",
          "form": "집어넣어",
          "original_form": "집어넣어",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019232.1.4",
          "form": "내가 단순 액션 알피지가 아니라 오픈월드 하우징 키워드를 왜 선택했겠냐구...",
          "original_form": "내가 단순 액션 알피지가 아니라 오픈월드 하우징 키워드를 왜 선택했겠냐구...",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019232.1.5",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019232.1.6",
          "form": "식물도 솔직히",
          "original_form": "식물도 솔직히",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019232.1.7",
          "form": "맨날 보는 허브말고",
          "original_form": "맨날 보는 허브말고",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019232.1.8",
          "form": "ㅇㅇ....하우징에서만 기를수있는",
          "original_form": "ㅇㅇ....하우징에서만 기를수있는",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019232.1.9",
          "form": "원소 영향받은 풀 채집 가능했으면 좋겠음",
          "original_form": "원소 영향받은 풀 채집 가능했으면 좋겠음",
          "speaker_id": "1",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.10",
          "form": "특수식물같은거",
          "original_form": "특수식물같은거",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.11",
          "form": "그리고 하우징 좀",
          "original_form": "그리고 하우징 좀",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.12",
          "form": "겹쳐서 가능하면 좋겠어",
          "original_form": "겹쳐서 가능하면 좋겠어",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.13",
          "form": "각 범위 너무 넓어서",
          "original_form": "각 범위 너무 넓어서",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.14",
          "form": "예쁘게 배치가 안돼",
          "original_form": "예쁘게 배치가 안돼",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.16",
          "form": "글킨 함",
          "original_form": "글킨 함",
          "speaker_id": "1",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.17",
          "form": "얘넨 격자?도 없어서",
          "original_form": "얘넨 격자?도 없어서",
          "speaker_id": "1",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019232.1.18",
          "form": "좀만 틀어져도 겹친다고 배치 안되고",
          "original_form": "좀만 틀어져도 겹친다고 배치 안되고",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019232.1.19",
          "form": "하여튼 자유도가 너무 높아...",
          "original_form": "하여튼 자유도가 너무 높아...",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019232.1.20",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019232.1.21",
          "form": "이런 컨텐츠만;;;",
          "original_form": "이런 컨텐츠만;;;",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019232.1.22",
          "form": "이건 자유도가 높은게 아니라",
          "original_form": "이건 자유도가 높은게 아니라",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019232.1.23",
          "form": "게으른거야",
          "original_form": "게으른거야",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019232.1.24",
          "form": "그리고 집도 지을수있었으면",
          "original_form": "그리고 집도 지을수있었으면",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019232.1.25",
          "form": "좋겠어",
          "original_form": "좋겠어",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.26",
          "form": "이왕 이렇게 나무토막부터 모을거면",
          "original_form": "이왕 이렇게 나무토막부터 모을거면",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.27",
          "form": "벽부터 내가 세우게 해달라",
          "original_form": "벽부터 내가 세우게 해달라",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.28",
          "form": "맞아 솔직히 디자인 바꾸는 것까지는 기대 안하는데",
          "original_form": "맞아 솔직히 디자인 바꾸는 것까지는 기대 안하는데",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.29",
          "form": "난 탑같은거 짓고싶었다구",
          "original_form": "난 탑같은거 짓고싶었다구",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.30",
          "form": "아니면 채색이라도 하게 해달라",
          "original_form": "아니면 채색이라도 하게 해달라",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.31",
          "form": "아ㅇㅇ",
          "original_form": "아ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.32",
          "form": "솔직히 나무색",
          "original_form": "솔직히 나무색",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.33",
          "form": "칙칙하단말야",
          "original_form": "칙칙하단말야",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.34",
          "form": "그리고 집안에서",
          "original_form": "그리고 집안에서",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.35",
          "form": "창문 좀",
          "original_form": "창문 좀",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019232.1.36",
          "form": "열게해주면 좋겠어",
          "original_form": "열게해주면 좋겠어",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.38",
          "form": "뭔가 그 자연의 맛이 안나지?",
          "original_form": "뭔가 그 자연의 맛이 안나지?",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.39",
          "form": "집안 너무 우중충해",
          "original_form": "집안 너무 우중충해",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.40",
          "form": "아니 애초에",
          "original_form": "아니 애초에",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.41",
          "form": "집 안이랑 밖을",
          "original_form": "집 안이랑 밖을",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.42",
          "form": "왜 분리한건지",
          "original_form": "왜 분리한건지",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.43",
          "form": "모르겠어",
          "original_form": "모르겠어",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.44",
          "form": "내가 지을수 잏었으면",
          "original_form": "내가 지을수 잏었으면",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019232.1.45",
          "form": "당장 큰 연못부터 만들고",
          "original_form": "당장 큰 연못부터 만들고",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019232.1.46",
          "form": "기둥 세워서",
          "original_form": "기둥 세워서",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019232.1.47",
          "form": "물위의 탑 만드라",
          "original_form": "물위의 탑 만드라",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019232.1.48",
          "form": "폭포도 만들게 해줬으면",
          "original_form": "폭포도 만들게 해줬으면",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019232.1.49",
          "form": "진짜 이건",
          "original_form": "진짜 이건",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019232.1.50",
          "form": "자유도 높은게 아니야......",
          "original_form": "자유도 높은게 아니야......",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019232.1.51",
          "form": "지형도 다듬게 해줘....",
          "original_form": "지형도 다듬게 해줘....",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019232.1.52",
          "form": "언덕이랑 구릉 만들고싶어....",
          "original_form": "언덕이랑 구릉 만들고싶어....",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019232.1.53",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019232.1.54",
          "form": "지금 바위 큰거 세워서 그 위에 집올리는건 가능함",
          "original_form": "지금 바위 큰거 세워서 그 위에 집올리는건 가능함",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019232.1.55",
          "form": "그치만 너무",
          "original_form": "그치만 너무",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019232.1.56",
          "form": "엉성한 그래픽이야....",
          "original_form": "엉성한 그래픽이야....",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019232.1.57",
          "form": "전에 리월양식으로 연못있는 저택 만들기 하는 영상 봤는데",
          "original_form": "전에 리월양식으로 연못있는 저택 만들기 하는 영상 봤는데",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019232.1.58",
          "form": "그리고 난 집이",
          "original_form": "그리고 난 집이",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019232.1.59",
          "form": "따라하겟다고 맘만 먹어놓고 영상 틀지도 않음..",
          "original_form": "따라하겟다고 맘만 먹어놓고 영상 틀지도 않음..",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019232.1.60",
          "form": "옆으로 넓은것보단",
          "original_form": "옆으로 넓은것보단",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019232.1.61",
          "form": "위로 솓아있으면",
          "original_form": "위로 솓아있으면",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019232.1.62",
          "form": "좋겠다구",
          "original_form": "좋겠다구",
          "speaker_id": "2",
          "time": "20210827 16:37"
        }
      ]
