      "utterance": [
        {
          "id": "MDRW2100024744.1.1",
          "form": "오늘 홈트 요가하는날인것 같아요",
          "original_form": "오늘 홈트 요가하는날인것 같아요",
          "speaker_id": "1",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100024744.1.2",
          "form": "우울해요",
          "original_form": "우울해요",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024744.1.3",
          "form": "먹은건 쉐이크 두잔인데",
          "original_form": "먹은건 쉐이크 두잔인데",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024744.1.4",
          "form": "오늘 요가의날",
          "original_form": "오늘 요가의날",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024744.1.5",
          "form": "저도 온라인피트니스 서비스 이용한적있어요",
          "original_form": "저도 온라인피트니스 서비스 이용한적있어요",
          "speaker_id": "2",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024744.1.6",
          "form": "생각보다 홈트 힘들더라구요",
          "original_form": "생각보다 홈트 힘들더라구요",
          "speaker_id": "2",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024744.1.7",
          "form": "은근히 힘들어요",
          "original_form": "은근히 힘들어요",
          "speaker_id": "1",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024744.1.8",
          "form": "단순해 보이는 동작이였는데 땀이 비오듯 나서 놀랬어요ㅎㅎ",
          "original_form": "단순해 보이는 동작이였는데 땀이 비오듯 나서 놀랬어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024744.1.9",
          "form": "홈트 어플 추천해주세요 ㅎㅎㅎ 뭐 쓰세요?",
          "original_form": "홈트 어플 추천해주세요 ㅎㅎㅎ 뭐 쓰세요?",
          "speaker_id": "2",
          "time": "20210831 18:03"
        },
        {
          "id": "MDRW2100024744.1.10",
          "form": "이번에새로나온피클라이란거 체험중인데",
          "original_form": "이번에새로나온피클라이란거 체험중인데",
          "speaker_id": "1",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024744.1.11",
          "form": "오ㅋㅋ어때요?",
          "original_form": "오ㅋㅋ어때요?",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024744.1.12",
          "form": "쓸만한가요?",
          "original_form": "쓸만한가요?",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100024744.1.13",
          "form": "실제로 강사님이 저를보면서 피티해주시는거라 더 좋드라구요",
          "original_form": "실제로 강사님이 저를보면서 피티해주시는거라 더 좋드라구요",
          "speaker_id": "1",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024744.1.14",
          "form": "근데비싸요",
          "original_form": "근데비싸요",
          "speaker_id": "1",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024744.1.15",
          "form": "실제로 강사님이?",
          "original_form": "실제로 강사님이?",
          "speaker_id": "2",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024744.1.16",
          "form": "오ㅋ효과좋겠다",
          "original_form": "오ㅋ효과좋겠다",
          "speaker_id": "2",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024744.1.17",
          "form": "근데 챙피해서",
          "original_form": "근데 챙피해서",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024744.1.18",
          "form": "쑥스러워요",
          "original_form": "쑥스러워요",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024744.1.19",
          "form": "ㅋㅋㅋ체험판이 좋은데...",
          "original_form": "ㅋㅋㅋ체험판이 좋은데...",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024744.1.20",
          "form": "앉아서하는 요가가 저한테는 좋은거같아요",
          "original_form": "앉아서하는 요가가 저한테는 좋은거같아요",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024744.1.21",
          "form": "가격은 얼마인가요?",
          "original_form": "가격은 얼마인가요?",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024744.1.22",
          "form": "1달무제한90000원?",
          "original_form": "1달무제한90000원?",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024744.1.23",
          "form": "선택해서 수업들을 수있으니 좋더라구요",
          "original_form": "선택해서 수업들을 수있으니 좋더라구요",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024744.1.24",
          "form": "어머...생각보다 비싸네요ㅎㅎㅎㅎ",
          "original_form": "어머...생각보다 비싸네요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024744.1.25",
          "form": "어플이라 더 저렴할거라 생각했어요ㅎㅎㅎ",
          "original_form": "어플이라 더 저렴할거라 생각했어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024744.1.26",
          "form": "그쵸 비대면피트니스지만 가격이 싸진 않아요",
          "original_form": "그쵸 비대면피트니스지만 가격이 싸진 않아요",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024744.1.27",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:13"
        }
      ]
