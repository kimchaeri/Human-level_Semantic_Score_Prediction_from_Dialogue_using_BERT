      "utterance": [
        {
          "id": "MDRW2100014320.1.1",
          "form": "좋아하는 음식 있으세료?",
          "original_form": "좋아하는 음식 있으세료?",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014320.1.2",
          "form": "다좋아해요 떡볶이 치킨..",
          "original_form": "다좋아해요 떡볶이 치킨..",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014320.1.3",
          "form": "앜ㅋㅋㅋ 걸작 떡볶이 엄청 좋아하시겠어요",
          "original_form": "앜ㅋㅋㅋ 걸작 떡볶이 엄청 좋아하시겠어요",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014320.1.4",
          "form": "한번도 안먹어봤어요 ㅠㅠ 맛있나요",
          "original_form": "한번도 안먹어봤어요 ㅠㅠ 맛있나요",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014320.1.5",
          "form": "치킨+떡볶이에요!!",
          "original_form": "치킨+떡볶이에요!!",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014320.1.6",
          "form": "와 좋다",
          "original_form": "와 좋다",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014320.1.7",
          "form": "느끼하지 않고 무난무난해요",
          "original_form": "느끼하지 않고 무난무난해요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014320.1.8",
          "form": "담에시도해볼게요 뭐좋아하세요 ㅎㅎ",
          "original_form": "담에시도해볼게요 뭐좋아하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014320.1.9",
          "form": "전 이름처럼 샤인머스켓이요",
          "original_form": "전 이름처럼 샤인머스켓이요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014320.1.10",
          "form": "방금도 먹엇어요 ㅎㅎ",
          "original_form": "방금도 먹엇어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014320.1.11",
          "form": "맛있죠 이번년도에는 한번도 못먹었지만 ㅎㅎ",
          "original_form": "맛있죠 이번년도에는 한번도 못먹었지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014320.1.12",
          "form": "상큼",
          "original_form": "상큼",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014320.1.13",
          "form": "ㅋㅋㅋㅋ 맞아요 상큼하니 맛있어요",
          "original_form": "ㅋㅋㅋㅋ 맞아요 상큼하니 맛있어요",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014320.1.14",
          "form": "요즘도 비싼가요 ㅋㅋ",
          "original_form": "요즘도 비싼가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014320.1.15",
          "form": "한송이에 만원은 하더라구요 ㅜ",
          "original_form": "한송이에 만원은 하더라구요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014320.1.16",
          "form": "와 비싸네요 ㅠ 그래도 맛있으니까 요즘 과일 못먹어서 먹고싶네여..",
          "original_form": "와 비싸네요 ㅠ 그래도 맛있으니까 요즘 과일 못먹어서 먹고싶네여..",
          "speaker_id": "2",
          "time": "20210825 18:27"
        }
      ]
