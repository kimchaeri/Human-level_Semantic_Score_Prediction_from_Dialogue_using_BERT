      "utterance": [
        {
          "id": "MDRW2100036311.1.1",
          "form": "여행 가고픈 곳 잇으세요?",
          "original_form": "여행 가고픈 곳 잇으세요?",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036311.1.2",
          "form": "저는 이탈리아요",
          "original_form": "저는 이탈리아요",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100036311.1.3",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100036311.1.4",
          "form": "저는 몽골이요",
          "original_form": "저는 몽골이요",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100036311.1.5",
          "form": "오 특이하네요 이유가 있을까요??",
          "original_form": "오 특이하네요 이유가 있을까요??",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036311.1.6",
          "form": "친구가 추천해줫어요",
          "original_form": "친구가 추천해줫어요",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036311.1.7",
          "form": "너무 좋앗다고",
          "original_form": "너무 좋앗다고",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036311.1.8",
          "form": "우와 어디를 가봤다고 말해주셨나요??",
          "original_form": "우와 어디를 가봤다고 말해주셨나요??",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036311.1.9",
          "form": "초원에서 밤에 별보는게 너무 좋앗대요",
          "original_form": "초원에서 밤에 별보는게 너무 좋앗대요",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100036311.1.10",
          "form": "그렇군요 그럼 name1님도 별보러가고 싶으신건가요??",
          "original_form": "그렇군요 그럼 &name1&님도 별보러가고 싶으신건가요??",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100036311.1.11",
          "form": "별도 보고 싶지만ㅋㅋㅋㅋㅋ드넓은 초원을 본 적이 없어서 초원도 궁금해요",
          "original_form": "별도 보고 싶지만ㅋㅋㅋㅋㅋ드넓은 초원을 본 적이 없어서 초원도 궁금해요",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036311.1.12",
          "form": "우와 장활한 초원도 멋있을 것 같아요",
          "original_form": "우와 장활한 초원도 멋있을 것 같아요",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036311.1.13",
          "form": "여행 가보신 적 있으신가요?",
          "original_form": "여행 가보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036311.1.14",
          "form": "여행 좋아해요",
          "original_form": "여행 좋아해요",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100036311.1.15",
          "form": "특히 이탈리아에 가고픈 이유가 잇으신가요?",
          "original_form": "특히 이탈리아에 가고픈 이유가 잇으신가요?",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100036311.1.16",
          "form": "부라노섬에 가고 싶어서요",
          "original_form": "부라노섬에 가고 싶어서요",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100036311.1.17",
          "form": "들어본적 잇는것 같은데... 그 막 알록달록하다는 섬이 거긴가요?",
          "original_form": "들어본적 잇는것 같은데... 그 막 알록달록하다는 섬이 거긴가요?",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036311.1.18",
          "form": "맞아요 거기가 동화같아서 가보고 싶어요",
