      "utterance": [
        {
          "id": "MDRW2100022092.1.1",
          "form": "썰어서 먹어보니까 완전 스펀지에여.",
          "original_form": "썰어서 먹어보니까 완전 스펀지에여.",
          "speaker_id": "2",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022092.1.2",
          "form": "못먹겠더라구여 ㅠㅠ",
          "original_form": "못먹겠더라구여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022092.1.3",
          "form": "음 신기하네",
          "original_form": "음 신기하네",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022092.1.4",
          "form": "섬유질이 훨씬 질겨요",
          "original_form": "섬유질이 훨씬 질겨요",
          "speaker_id": "2",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022092.1.5",
          "form": "진짜 안가리는 음식없이 잘드시나보네ㅛ",
          "original_form": "진짜 안가리는 음식없이 잘드시나보네ㅛ",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022092.1.6",
          "form": "두부는 약한거구나 했어요",
          "original_form": "두부는 약한거구나 했어요",
          "speaker_id": "2",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022092.1.7",
          "form": "ㅋㅋㅋ 다 잘먹어요 진짜",
          "original_form": "ㅋㅋㅋ 다 잘먹어요 진짜",
          "speaker_id": "2",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022092.1.8",
          "form": "ㅋㅋㅋㅋ 많이 드시는 편이에요?",
          "original_form": "ㅋㅋㅋㅋ 많이 드시는 편이에요?",
          "speaker_id": "1",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022092.1.9",
          "form": "옹 네 양도 많은 편이에요",
          "original_form": "옹 네 양도 많은 편이에요",
          "speaker_id": "2",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022092.1.10",
          "form": "가아끔 짜파게티2봉 성공할 정도?",
          "original_form": "가아끔 짜파게티2봉 성공할 정도?",
          "speaker_id": "2",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022092.1.11",
          "form": "체중관리 어렵지 않아요?",
          "original_form": "체중관리 어렵지 않아요?",
          "speaker_id": "1",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100022092.1.12",
          "form": "저는 최근에 다이어트하려고",
          "original_form": "저는 최근에 다이어트하려고",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.13",
          "form": "제가 장이 안좋아서 흡수력이 딸리는 편이라 ㅠㅡㅠ 살 안빠지는것만 신경쓰고 있어요 ㅎㅎ",
          "original_form": "제가 장이 안좋아서 흡수력이 딸리는 편이라 ㅠㅡㅠ 살 안빠지는것만 신경쓰고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.14",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.15",
          "form": "지금 살 빠져서 스트레스 으..",
          "original_form": "지금 살 빠져서 스트레스 으..",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.16",
          "form": "축복받은 체질이다",
          "original_form": "축복받은 체질이다",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.17",
          "form": "안아픈게 좋은거에요! 전 안좋은거에요!",
          "original_form": "안아픈게 좋은거에요! 전 안좋은거에요!",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.18",
          "form": "축복 아니에요 노노 병원 정기적으로 가요!",
          "original_form": "축복 아니에요 노노 병원 정기적으로 가요!",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.19",
          "form": "진차 축복이다ㅋㅋ 맘껏먹어도 되고",
          "original_form": "진차 축복이다ㅋㅋ 맘껏먹어도 되고",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.20",
          "form": "병원은 왜요?",
          "original_form": "병원은 왜요?",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.21",
          "form": "아니라니깐여..;ㅁ;으헝.....",
          "original_form": "아니라니깐여..;ㅁ;으헝.....",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.22",
          "form": "저체중으로?",
          "original_form": "저체중으로?",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100022092.1.23",
          "form": "맘껏 못먹어요 ㅠㅠ 위장이 안받아줘서 너무 과식하거나 맵거나 기름지면 탈나요.. 흑.. 그래서 가려먹어요",
          "original_form": "맘껏 못먹어요 ㅠㅠ 위장이 안받아줘서 너무 과식하거나 맵거나 기름지면 탈나요.. 흑.. 그래서 가려먹어요",
          "speaker_id": "2",
