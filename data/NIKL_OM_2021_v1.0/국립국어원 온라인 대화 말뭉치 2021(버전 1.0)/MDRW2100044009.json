      "utterance": [
        {
          "id": "MDRW2100044009.1.1",
          "form": "나는 새우깡이지",
          "original_form": "나는 새우깡이지",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044009.1.2",
          "form": "오 ㅋㅋ 맞다 ㅋㅋ 엄마 좋아했지",
          "original_form": "오 ㅋㅋ 맞다 ㅋㅋ 엄마 좋아했지",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044009.1.3",
          "form": "새우깡이 제일 맛있다",
          "original_form": "새우깡이 제일 맛있다",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.4",
          "form": "웅 맛나긴 하다 ㅎㅎ",
          "original_form": "웅 맛나긴 하다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.5",
          "form": "짭조름한게 좋아",
          "original_form": "짭조름한게 좋아",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.6",
          "form": "웅웅 딱 짭쪼롬 그자체 ㅋㅋ",
          "original_form": "웅웅 딱 짭쪼롬 그자체 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.7",
          "form": "너는 별로야",
          "original_form": "너는 별로야",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.8",
          "form": "나는 달달한 과자가 더 좋아 ㅋㅋ",
          "original_form": "나는 달달한 과자가 더 좋아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.9",
          "form": "새우깡은 안 튀긴 거라서 더 좋다",
          "original_form": "새우깡은 안 튀긴 거라서 더 좋다",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.10",
          "form": "오 몰랐다",
          "original_form": "오 몰랐다",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.11",
          "form": "구워서 만든거다",
          "original_form": "구워서 만든거다",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.12",
          "form": "좋네..!",
          "original_form": "좋네..!",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044009.1.13",
          "form": "하나 사 먹어야겠다",
