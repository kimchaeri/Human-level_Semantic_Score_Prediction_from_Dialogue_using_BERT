      "utterance": [
        {
          "id": "MDRW2100000995.1.1",
          "form": "너는 결혼할거야?",
          "original_form": "너는 결혼할거야?",
          "speaker_id": "1",
          "time": "20210820 11:30"
        },
        {
          "id": "MDRW2100000995.1.2",
          "form": "음 전에는 할생각이 없었는데 지금은 할것 같아",
          "original_form": "음 전에는 할생각이 없었는데 지금은 할것 같아",
          "speaker_id": "2",
          "time": "20210820 11:30"
        },
        {
          "id": "MDRW2100000995.1.3",
          "form": "나는 결혼 안할것 같아",
          "original_form": "나는 결혼 안할것 같아",
          "speaker_id": "1",
          "time": "20210820 11:30"
        },
        {
          "id": "MDRW2100000995.1.4",
          "form": "너는 왜 결혼할거같아?",
          "original_form": "너는 왜 결혼할거같아?",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000995.1.5",
          "form": "전에는 진짜 아무생각도 없이 살아서 그랬지 지금은 남친도 있어서 그런것 같아",
          "original_form": "전에는 진짜 아무생각도 없이 살아서 그랬지 지금은 남친도 있어서 그런것 같아",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000995.1.6",
          "form": "지금 만나는 사람 있었구나",
          "original_form": "지금 만나는 사람 있었구나",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000995.1.7",
          "form": "어떤 사람이야?",
          "original_form": "어떤 사람이야?",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000995.1.8",
          "form": "하는 직업을 물어보는거라면 회사 사장님..?",
          "original_form": "하는 직업을 물어보는거라면 회사 사장님..?",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000995.1.9",
          "form": "물론 예비",
          "original_form": "물론 예비",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000995.1.10",
          "form": "헐 사장님이야?",
          "original_form": "헐 사장님이야?",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000995.1.11",
          "form": "예비 사장님 뭔데 ㅋㅋ",
          "original_form": "예비 사장님 뭔데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000995.1.12",
          "form": "한 회사 사장님 아들이라서 그래",
          "original_form": "한 회사 사장님 아들이라서 그래",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000995.1.13",
          "form": "성격같은것도 궁금해",
          "original_form": "성격같은것도 궁금해",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000995.1.14",
          "form": "난 챙겨줄 필요없다고 하는데 되게 챙겨줄려고 해",
          "original_form": "난 챙겨줄 필요없다고 하는데 되게 챙겨줄려고 해",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000995.1.15",
          "form": "다정한 사람이네",
          "original_form": "다정한 사람이네",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000995.1.16",
          "form": "다정하다 못해 착해서 문제지",
          "original_form": "다정하다 못해 착해서 문제지",
          "speaker_id": "2",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000995.1.17",
          "form": "너한테 안정감을 주는 사람이랑 잘 만나고 있는것 같아",
          "original_form": "너한테 안정감을 주는 사람이랑 잘 만나고 있는것 같아",
          "speaker_id": "1",
          "time": "20210820 11:33"
        }
