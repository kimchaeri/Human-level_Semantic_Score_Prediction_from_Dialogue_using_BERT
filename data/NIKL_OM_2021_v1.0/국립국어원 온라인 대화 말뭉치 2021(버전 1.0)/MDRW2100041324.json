      "utterance": [
        {
          "id": "MDRW2100041324.1.1",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041324.1.2",
          "form": "저 원래 배달 많이 시켜먹었는데",
          "original_form": "저 원래 배달 많이 시켜먹었는데",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100041324.1.3",
          "form": "오히려 일하고나니까 안시켜먹어요",
          "original_form": "오히려 일하고나니까 안시켜먹어요",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100041324.1.4",
          "form": "네네네!!",
          "original_form": "네네네!!",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100041324.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100041324.1.6",
          "form": "예전에는 엥겔지수 높아서",
          "original_form": "예전에는 엥겔지수 높아서",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.7",
          "form": "돈을 벌어보니까 이게 얼마나",
          "original_form": "돈을 벌어보니까 이게 얼마나",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.8",
          "form": "네네ㅜㅜㅜ 제가 딱 그래요...",
          "original_form": "네네ㅜㅜㅜ 제가 딱 그래요...",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.9",
          "form": "아 내가 이렇게 힘들게 일했으니 먹자 ㅋ 쓰자 ㅋ",
          "original_form": "아 내가 이렇게 힘들게 일했으니 먹자 ㅋ 쓰자 ㅋ",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.10",
          "form": "이랬는데 어우 이제는 ㅋㅋㅋㅋ",
          "original_form": "이랬는데 어우 이제는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.11",
          "form": "안되겠더라구요",
          "original_form": "안되겠더라구요",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.12",
          "form": "정말 공감해요ㅠ",
          "original_form": "정말 공감해요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.13",
          "form": "저도 오늘 친구가 생일이어서 만난거였어요 8ㅅ8",
          "original_form": "저도 오늘 친구가 생일이어서 만난거였어요 8ㅅ8",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.14",
          "form": "맞아요 웬만한 약속은 그냥 좀",
          "original_form": "맞아요 웬만한 약속은 그냥 좀",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.15",
          "form": "피하려는 편",
          "original_form": "피하려는 편",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041324.1.16",
          "form": "아니면 최대한 밖에서 한끼만",
          "original_form": "아니면 최대한 밖에서 한끼만",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.17",
          "form": "앗 저도 진짜",
          "original_form": "앗 저도 진짜",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.18",
          "form": "점심먹고 만나거나 저녁 전에 가거나",
          "original_form": "점심먹고 만나거나 저녁 전에 가거나",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.19",
          "form": "이제 그래야할 것 같아요ㅠ",
          "original_form": "이제 그래야할 것 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.20",
          "form": "이러다 어느 세월에 돈 모아...ㅠ",
          "original_form": "이러다 어느 세월에 돈 모아...ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.21",
          "form": "그리고 씀씀이 비슷한 사람끼리 만나서 놀아야할 것 같아요",
          "original_form": "그리고 씀씀이 비슷한 사람끼리 만나서 놀아야할 것 같아요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.22",
          "form": "근데 저는 술을 좋아하고 많이 먹다보니까",
          "original_form": "근데 저는 술을 좋아하고 많이 먹다보니까",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.23",
          "form": "솔직히 술값 진짜 제일크거든요..",
          "original_form": "솔직히 술값 진짜 제일크거든요..",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.24",
          "form": "그래서 요즘 좀 절주를 생각하는것도",
          "original_form": "그래서 요즘 좀 절주를 생각하는것도",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.25",
          "form": "돈이큽니다",
          "original_form": "돈이큽니다",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100041324.1.26",
          "form": "밖에서 술한번먹으면 오육만원은 우습게 깨지니까..",
          "original_form": "밖에서 술한번먹으면 오육만원은 우습게 깨지니까..",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100041324.1.27",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100041324.1.28",
          "form": "네네네 맞아요..",
          "original_form": "네네네 맞아요..",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100041324.1.29",
          "form": "술은 한 잔이 만 원 그냥 넘으니까요ㅠ",
          "original_form": "술은 한 잔이 만 원 그냥 넘으니까요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100041324.1.30",
          "form": "저 저번에 포비피엠 가서",
          "original_form": "저 저번에 포비피엠 가서",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100041324.1.31",
          "form": "몇 만원 쓰고 현타 왔잖아요ㅠ",
          "original_form": "몇 만원 쓰고 현타 왔잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100041324.1.32",
          "form": "ㅋㅋㅋㅋㅋ저 거기가면 절제안돼서",
          "original_form": "ㅋㅋㅋㅋㅋ저 거기가면 절제안돼서",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100041324.1.33",
          "form": "진짜 칠팔마넌 나오고",
          "original_form": "진짜 칠팔마넌 나오고",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100041324.1.34",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100041324.1.35",
          "form": "정말 현타",
          "original_form": "정말 현타",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100041324.1.36",
          "form": "아 그쵸 저만 그런거 아니죠",
          "original_form": "아 그쵸 저만 그런거 아니죠",
          "speaker_id": "1",
          "time": "20210905 22:08"
        }
      ]
    }
