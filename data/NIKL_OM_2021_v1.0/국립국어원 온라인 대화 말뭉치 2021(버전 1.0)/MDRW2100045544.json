        {
          "id": "MDRW2100045544.1.1",
          "form": "이번에 다른 스파이디들도 다 나온대요",
          "original_form": "이번에 다른 스파이디들도 다 나온대요",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045544.1.2",
          "form": "아 그거 진짜예요?",
          "original_form": "아 그거 진짜예요?",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045544.1.3",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045544.1.4",
          "form": "네네 스포봤는데 꿈은 아니었겠죠?",
          "original_form": "네네 스포봤는데 꿈은 아니었겠죠?",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045544.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 루머인줄 알앗어요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 루머인줄 알앗어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045544.1.6",
          "form": "기대되네요",
          "original_form": "기대되네요",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045544.1.7",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045544.1.8",
          "form": "어????",
          "original_form": "어????",
          "speaker_id": "1",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045544.1.9",
          "form": "네???",
          "original_form": "네???",
          "speaker_id": "2",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045544.1.10",
          "form": "루먼데 제가 낚인건가요??",
          "original_form": "루먼데 제가 낚인건가요??",
          "speaker_id": "1",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045544.1.11",
          "form": "영상뜨고 다들 좋아햏는데??",
          "original_form": "영상뜨고 다들 좋아햏는데??",
          "speaker_id": "1",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045544.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제가 영상을 못보고 그냥 나온대!! 뭐 이런식으로 봐서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제가 영상을 못보고 그냥 나온대!! 뭐 이런식으로 봐서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100045544.1.13",
          "form": "맞겟죠? ㅋㅋㅋㅋㅋㅋ 찾아봐야겟네요",
          "original_form": "맞겟죠? ㅋㅋㅋㅋㅋㅋ 찾아봐야겟네요",
          "speaker_id": "2",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100045544.1.14",
          "form": "ㅋㅋ 전 영상 올라온거 댓글 삼백개달린거 봤거든요",
          "original_form": "ㅋㅋ 전 영상 올라온거 댓글 삼백개달린거 봤거든요",
          "speaker_id": "1",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045544.1.15",
          "form": "에이 그럼 맞겟죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "에이 그럼 맞겟죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045544.1.16",
          "form": "제가 낚은거라면 미리 사과드릴게요",
          "original_form": "제가 낚은거라면 미리 사과드릴게요",
          "speaker_id": "1",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045544.1.17",
          "form": "마블팬들은 그 두명 더 좋아하는거같아요",
          "original_form": "마블팬들은 그 두명 더 좋아하는거같아요",
          "speaker_id": "1",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100045544.1.18",
          "form": "ㅋㅋㅋㅋㅋ괜찮아요",
          "original_form": "ㅋㅋㅋㅋㅋ괜찮아요",
          "speaker_id": "2",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100045544.1.19",
          "form": "ㅋㅋ 영화러닝타임 되게 길게 나왓다고ㅜ",
