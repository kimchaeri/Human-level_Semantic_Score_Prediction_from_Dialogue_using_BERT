      "utterance": [
        {
          "id": "MDRW2100032259.1.1",
          "form": "트위터하니까 커뮤니티 활동은 많이 하잖아",
          "original_form": "트위터하니까 커뮤니티 활동은 많이 하잖아",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032259.1.2",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032259.1.3",
          "form": "근데 넌 할말있냐",
          "original_form": "근데 넌 할말있냐",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100032259.1.4",
          "form": "정모해본적있어?",
          "original_form": "정모해본적있어?",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032259.1.5",
          "form": "각잡고 한 적은 없고 행사 때 만난 적은 있지",
          "original_form": "각잡고 한 적은 없고 행사 때 만난 적은 있지",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032259.1.6",
          "form": "나는 온라인으로 사람만나는거 거의 해본적없어서 신기해",
          "original_form": "나는 온라인으로 사람만나는거 거의 해본적없어서 신기해",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100032259.1.7",
          "form": "나더 실제로 만나본 적은 거의 없어",
          "original_form": "나더 실제로 만나본 적은 거의 없어",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032259.1.8",
          "form": "지방사람이라 힘듬ㅋㅋ",
          "original_form": "지방사람이라 힘듬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032259.1.9",
          "form": "다들 잘 만나더라",
          "original_form": "다들 잘 만나더라",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032259.1.10",
          "form": "마져",
          "original_form": "마져",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032259.1.11",
          "form": "커뮤니티로 만나서 사귀기까지 하고 일본까지 간다고 하는 사람도 있는데",
          "original_form": "커뮤니티로 만나서 사귀기까지 하고 일본까지 간다고 하는 사람도 있는데",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032259.1.12",
          "form": "나더 약속은 함 언잰가 만나자고",
          "original_form": "나더 약속은 함 언잰가 만나자고",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032259.1.13",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100032259.1.14",
          "form": "그건 진짜 찐사랑이네..",
          "original_form": "그건 진짜 찐사랑이네..",
          "speaker_id": "2",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032259.1.15",
          "form": "요즘은 이런식으로 인연이 되는게 흔한거같음",
          "original_form": "요즘은 이런식으로 인연이 되는게 흔한거같음",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032259.1.16",
          "form": "어떻게 만나냐고 묻는 글같은거 보면",
          "original_form": "어떻게 만나냐고 묻는 글같은거 보면",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032259.1.17",
          "form": "채팅에서 만나다고 하는 사람이 많더라",
          "original_form": "채팅에서 만나다고 하는 사람이 많더라",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100032259.1.18",
          "form": "대단하네..",
          "original_form": "대단하네..",
          "speaker_id": "2",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032259.1.19",
          "form": "뭔어플인지 알거같다",
          "original_form": "뭔어플인지 알거같다",
          "speaker_id": "2",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032259.1.20",
          "form": "그게 추측이 되나?",
          "original_form": "그게 추측이 되나?",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032259.1.21",
          "form": "그게 진짜 되는구나",
          "original_form": "그게 진짜 되는구나",
          "speaker_id": "2",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032259.1.22",
          "form": "나는 아예 안하니까 활동을",
          "original_form": "나는 아예 안하니까 활동을",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032259.1.23",
          "form": "알고잇으니간 추측이 되는거지",
          "original_form": "알고잇으니간 추측이 되는거지",
          "speaker_id": "2",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100032259.1.24",
          "form": "주변에 그 앺 깐 사람잇었어",
          "original_form": "주변에 그 앺 깐 사람잇었어",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032259.1.25",
          "form": "앺이래 앱",
          "original_form": "앺이래 앱",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032259.1.26",
          "form": "요즘은 커뮤니티같은 그런 관계가 되게 부럽기도 하던데",
          "original_form": "요즘은 커뮤니티같은 그런 관계가 되게 부럽기도 하던데",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032259.1.27",
          "form": "잘 활영하면 좋은 관걔 얻는거지",
          "original_form": "잘 활영하면 좋은 관걔 얻는거지",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032259.1.28",
          "form": "인맥도 넓어지고 시야도 넓어지고",
          "original_form": "인맥도 넓어지고 시야도 넓어지고",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032259.1.29",
          "form": "오프라인에서는 비슷한 직종만 만나기 쉬운데 온라인에선 어니니깐",
          "original_form": "오프라인에서는 비슷한 직종만 만나기 쉬운데 온라인에선 어니니깐",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032259.1.30",
          "form": "성인들은 그런걸 장점으로 생각하더라고",
          "original_form": "성인들은 그런걸 장점으로 생각하더라고",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032259.1.31",
          "form": "그렇네",
          "original_form": "그렇네",
          "speaker_id": "1",
          "time": "20210905 14:53"
        }
      ]
    }
