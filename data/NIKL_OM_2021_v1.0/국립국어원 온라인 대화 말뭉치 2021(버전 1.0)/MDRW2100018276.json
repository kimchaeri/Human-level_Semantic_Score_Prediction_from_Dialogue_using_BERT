      "utterance": [
        {
          "id": "MDRW2100018276.1.1",
          "form": "게임 즐겨하시나요",
          "original_form": "게임 즐겨하시나요",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018276.1.2",
          "form": "게임 잘 몰라서요",
          "original_form": "게임 잘 몰라서요",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018276.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018276.1.4",
          "form": "10대때는 게임이 일상이었어요",
          "original_form": "10대때는 게임이 일상이었어요",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018276.1.5",
          "form": "요즘은 가끔 취미로 하는 중입니다",
          "original_form": "요즘은 가끔 취미로 하는 중입니다",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018276.1.6",
          "form": "어떤거하세요?",
          "original_form": "어떤거하세요?",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018276.1.7",
          "form": "롤이라는 게임 해요",
          "original_form": "롤이라는 게임 해요",
          "speaker_id": "1",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018276.1.8",
          "form": "들어만 봤어요 ㅋㅋㅋ",
          "original_form": "들어만 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018276.1.9",
          "form": "가장 최근에 해보신 게임이나 어릴때 해보신 게임 있으신가요",
          "original_form": "가장 최근에 해보신 게임이나 어릴때 해보신 게임 있으신가요",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018276.1.10",
          "form": "어릴때 카트라이더나 라테일했었어요 ㅋㅋㅋ",
          "original_form": "어릴때 카트라이더나 라테일했었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018276.1.11",
          "form": "라테일은 들어만봤구 저도 카트라이더는 열심히했었어요 ㅋㅋ",
          "original_form": "라테일은 들어만봤구 저도 카트라이더는 열심히했었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018276.1.12",
          "form": "카트라이더는 모두가 했던 게임같아요 ㅌㅋㅋㅋ",
          "original_form": "카트라이더는 모두가 했던 게임같아요 ㅌㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018276.1.13",
          "form": "아직까지도 안없어져서 가끔보면 추억돋네요 ㅋㅋ",
          "original_form": "아직까지도 안없어져서 가끔보면 추억돋네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018276.1.14",
          "form": "맞아요  ㅋㅋㅋㅋ 크아도 하셨어요?",
          "original_form": "맞아요  ㅋㅋㅋㅋ 크아도 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018276.1.15",
          "form": "완전 어릴때 시작한 게임이 크아였었어요 ㅋㅋㅋ",
          "original_form": "완전 어릴때 시작한 게임이 크아였었어요 ㅋㅋㅋ",
