      "utterance": [
        {
          "id": "MDRW2100005390.1.1",
          "form": "나 오늘",
          "original_form": "나 오늘",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005390.1.2",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005390.1.3",
          "form": "설거지를 두 번 했어",
          "original_form": "설거지를 두 번 했어",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005390.1.4",
          "form": "설거지 보통 바로 해 아니면",
          "original_form": "설거지 보통 바로 해 아니면",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005390.1.5",
          "form": "쌓아뒀다 해?",
          "original_form": "쌓아뒀다 해?",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005390.1.6",
          "form": "내가 먹은 건 바로 하는데",
          "original_form": "내가 먹은 건 바로 하는데",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005390.1.7",
          "form": "가족들이랑 먹은 건 양이 많아서",
          "original_form": "가족들이랑 먹은 건 양이 많아서",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005390.1.8",
          "form": "미뤘다가 그 다음 식사 차리기 전에 부랴부랴 해",
          "original_form": "미뤘다가 그 다음 식사 차리기 전에 부랴부랴 해",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005390.1.9",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005390.1.10",
          "form": "자취할때",
          "original_form": "자취할때",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005390.1.11",
          "form": "집안일이 은근",
          "original_form": "집안일이 은근",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005390.1.12",
          "form": "손이 많이가더라",
          "original_form": "손이 많이가더라",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005390.1.13",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005390.1.14",
          "form": "자잘하게",
          "original_form": "자잘하게",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005390.1.15",
          "form": "손 많이 가는 일들 진짜 많은데",
          "original_form": "손 많이 가는 일들 진짜 많은데",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005390.1.16",
          "form": "다시 금방 더러워져서",
          "original_form": "다시 금방 더러워져서",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005390.1.17",
          "form": "허무함",
          "original_form": "허무함",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005390.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005390.1.19",
          "form": "특히 바닥닦기",
          "original_form": "특히 바닥닦기",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005390.1.20",
          "form": "헐ㅇㅈㅇㅈ",
          "original_form": "헐ㅇㅈㅇㅈ",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005390.1.21",
          "form": "어제 닦았는데",
          "original_form": "어제 닦았는데",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005390.1.22",
          "form": "오늘 또 발에 뭐가 밟힘",
          "original_form": "오늘 또 발에 뭐가 밟힘",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005390.1.23",
          "form": "나는 요즘 고양이 화장실 모래 때문에",
          "original_form": "나는 요즘 고양이 화장실 모래 때문에",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005390.1.24",
          "form": "바닥 아침에 닦으면 저녁에 더러워져있어",
          "original_form": "바닥 아침에 닦으면 저녁에 더러워져있어",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005390.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005390.1.26",
          "form": "닦아도 티안나는데",
          "original_form": "닦아도 티안나는데",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005390.1.27",
          "form": "안닦으면 티남",
          "original_form": "안닦으면 티남",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005390.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ'ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ'ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005390.1.29",
          "form": "화장실청소도",
          "original_form": "화장실청소도",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005390.1.30",
          "form": "엄마가할땐몰랏는데",
          "original_form": "엄마가할땐몰랏는데",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005390.1.31",
          "form": "안하면 바로 곰팡ㅇㅣ",
          "original_form": "안하면 바로 곰팡ㅇㅣ",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005390.1.32",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005390.1.33",
          "form": "물때 진짜 금방 낌",
