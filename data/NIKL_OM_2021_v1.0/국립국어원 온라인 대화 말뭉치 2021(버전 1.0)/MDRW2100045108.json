      "utterance": [
        {
          "id": "MDRW2100045108.1.1",
          "form": "게임 좋아해요?",
          "original_form": "게임 좋아해요?",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045108.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.3",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.4",
          "form": "요즘 뭐하세요?",
          "original_form": "요즘 뭐하세요?",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.5",
          "form": "저 요즘 테런해요",
          "original_form": "저 요즘 테런해요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.6",
          "form": "테런 해보셨어요??",
          "original_form": "테런 해보셨어요??",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.7",
          "form": "전 그거 안해요",
          "original_form": "전 그거 안해요",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.8",
          "form": "테런 재미있는데..",
          "original_form": "테런 재미있는데..",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.9",
          "form": "재미있나요?",
          "original_form": "재미있나요?",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.11",
          "form": "해보신 적은 있어요??",
          "original_form": "해보신 적은 있어요??",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.12",
          "form": "저는 적응안되서",
          "original_form": "저는 적응안되서",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.13",
          "form": "옛날에요",
          "original_form": "옛날에요",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045108.1.14",
          "form": "나중에.. 나중에 하시면 말씀해주세요",
          "original_form": "나중에.. 나중에 하시면 말씀해주세요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045108.1.15",
          "form": "요즘읔 엄청 이상하던데",
          "original_form": "요즘읔 엄청 이상하던데",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045108.1.16",
          "form": "뉴비 지원 가능 ~",
          "original_form": "뉴비 지원 가능 ~",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045108.1.17",
          "form": "아뇨 뉴비없어요",
          "original_form": "아뇨 뉴비없어요",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045108.1.18",
          "form": "..쳇",
          "original_form": "..쳇",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045108.1.19",
          "form": "스토리가 엄청 이상해졌어요",
          "original_form": "스토리가 엄청 이상해졌어요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045108.1.20",
          "form": "온라인겜 안좋아해요",
          "original_form": "온라인겜 안좋아해요",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045108.1.21",
          "form": "엥 어케요?",
          "original_form": "엥 어케요?",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045108.1.22",
          "form": "동화나라 공주 정체 아세요??",
          "original_form": "동화나라 공주 정체 아세요??",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045108.1.23",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045108.1.24",
          "form": "걔가 사실 인조인간이래요",
          "original_form": "걔가 사실 인조인간이래요",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045108.1.25",
          "form": "안하는데 어케알아",
          "original_form": "안하는데 어케알아",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045108.1.26",
          "form": "아 알수도 있죠 ; 알음알음",
          "original_form": "아 알수도 있죠 ; 알음알음",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045108.1.27",
          "form": "그래서 시간이 빨리 흐르는 탑에서 외부와 단절돼서 컸데요",
          "original_form": "그래서 시간이 빨리 흐르는 탑에서 외부와 단절돼서 컸데요",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045108.1.28",
          "form": "완전 아동학대 스토리 아닙니까?",
          "original_form": "완전 아동학대 스토리 아닙니까?",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045108.1.29",
          "form": "흠 약간",
          "original_form": "흠 약간",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045108.1.30",
          "form": "미국 외출금지의 심화버전",
          "original_form": "미국 외출금지의 심화버전",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045108.1.31",
          "form": "거기에",
          "original_form": "거기에",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045108.1.32",
          "form": "요즘 게임들이",
          "original_form": "요즘 게임들이",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045108.1.33",
          "form": "치료 하라고 데려온 애가 친구한테 집착해서",
          "original_form": "치료 하라고 데려온 애가 친구한테 집착해서",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045108.1.34",
          "form": "어른들노리고 스토리 자극적으로씀",
          "original_form": "어른들노리고 스토리 자극적으로씀",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045108.1.35",
          "form": "허...",
          "original_form": "허...",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045108.1.36",
          "form": "친구 부활시키려고 해요 ; 그것도 손절한 친구를",
          "original_form": "친구 부활시키려고 해요 ; 그것도 손절한 친구를",
          "speaker_id": "2",
          "time": "20210908 00:56"
        }
      ]
    }
