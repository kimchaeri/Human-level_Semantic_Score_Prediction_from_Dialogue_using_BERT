      "utterance": [
        {
          "id": "MDRW2100045840.1.1",
          "form": "3박4일 있었어요",
          "original_form": "3박4일 있었어요",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100045840.1.2",
          "form": "와 좋네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 좋네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045840.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045840.1.4",
          "form": "저도 그쯤 있었던 것 같아요",
          "original_form": "저도 그쯤 있었던 것 같아요",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045840.1.5",
          "form": "그 정도가 적당한 거 같아요ㅋㅋ",
          "original_form": "그 정도가 적당한 거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045840.1.6",
          "form": "근데 갈 때 저녁 올 때 아침 비행기라",
          "original_form": "근데 갈 때 저녁 올 때 아침 비행기라",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045840.1.7",
          "form": "살짝 아쉬웟어요",
          "original_form": "살짝 아쉬웟어요",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045840.1.8",
          "form": "저도 그랬어요ㅠㅠ 더 있고 싶더라고요",
          "original_form": "저도 그랬어요ㅠㅠ 더 있고 싶더라고요",
          "speaker_id": "2",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045840.1.9",
          "form": "게다가 면세점이 공사중이라 더 아쉬웠어요",
          "original_form": "게다가 면세점이 공사중이라 더 아쉬웠어요",
          "speaker_id": "1",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045840.1.10",
          "form": "아ㅠㅠㅠ너무 아쉽네요",
          "original_form": "아ㅠㅠㅠ너무 아쉽네요",
          "speaker_id": "2",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100045840.1.11",
          "form": "그래도 저는 비행기 기다리면서 빵을 먹었는데",
          "original_form": "그래도 저는 비행기 기다리면서 빵을 먹었는데",
          "speaker_id": "2",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100045840.1.12",
          "form": "그 빵이 너무 맛있어서 놀랐어요ㅋㅋ제주가 빵 맛집인가봐요",
          "original_form": "그 빵이 너무 맛있어서 놀랐어요ㅋㅋ제주가 빵 맛집인가봐요",
          "speaker_id": "2",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100045840.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ공항에 제주 한정 빵 드셨나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ공항에 제주 한정 빵 드셨나요?",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045840.1.14",
          "form": "네! 맛있더라구요ㅎㅎ 그거랑, 제주 빵집 갔는데 거기도 맛있었어요",
          "original_form": "네! 맛있더라구요ㅎㅎ 그거랑, 제주 빵집 갔는데 거기도 맛있었어요",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045840.1.15",
          "form": "우도 땅콩 빵도 팔더라구요ㅋㅋ",
          "original_form": "우도 땅콩 빵도 팔더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045840.1.16",
          "form": "저도 분점은 식빵만 파는데 갔는데",
          "original_form": "저도 분점은 식빵만 파는데 갔는데",
          "speaker_id": "1",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100045840.1.17",
          "form": "식빵이 진짜 맛있더라구요",
          "original_form": "식빵이 진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100045840.1.18",
          "form": "진짜 빵만 한 보따리 사오고 싶었어요ㅋㅋ",
          "original_form": "진짜 빵만 한 보따리 사오고 싶었어요ㅋㅋ",
