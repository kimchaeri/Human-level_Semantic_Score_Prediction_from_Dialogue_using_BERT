      "utterance": [
        {
          "id": "MDRW2100024062.1.1",
          "form": "왜 서울엔타워 ㅋㅋㅋㅋㅋ",
          "original_form": "왜 서울엔타워 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024062.1.2",
          "form": "남산타워보다 촌스러운 이름인데...",
          "original_form": "남산타워보다 촌스러운 이름인데...",
          "speaker_id": "2",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024062.1.3",
          "form": "와 여기에 광고도 뜨네",
          "original_form": "와 여기에 광고도 뜨네",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024062.1.4",
          "form": "그쵸 ? 나는 남산타워가 더 좋은데",
          "original_form": "그쵸 ? 나는 남산타워가 더 좋은데",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024062.1.5",
          "form": "왜 이름 바꿨대요 참나",
          "original_form": "왜 이름 바꿨대요 참나",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024062.1.6",
          "form": "글로벌해보이려고 애쓰는게 너무 노티나요...",
          "original_form": "글로벌해보이려고 애쓰는게 너무 노티나요...",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.8",
          "form": "혹시 제가 캄보디아 여행기도 들려줬나요?",
          "original_form": "혹시 제가 캄보디아 여행기도 들려줬나요?",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.9",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.10",
          "form": "와 마니 들리셧다 진짜",
          "original_form": "와 마니 들리셧다 진짜",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.11",
          "form": "잘됐다 캄보디아는 저 고등학교 때 갔거든요",
          "original_form": "잘됐다 캄보디아는 저 고등학교 때 갔거든요",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.12",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.13",
          "form": "고등학교 1학년인가 2학년 때 친구들이랑 갔어요",
          "original_form": "고등학교 1학년인가 2학년 때 친구들이랑 갔어요",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.14",
          "form": "우와 부모님 없이요?",
          "original_form": "우와 부모님 없이요?",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.15",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024062.1.16",
          "form": "네네 학교선생님이랑요 헤헤",
          "original_form": "네네 학교선생님이랑요 헤헤",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.18",
          "form": "선생님의 출장이셧구나...",
          "original_form": "선생님의 출장이셧구나...",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.19",
          "form": "캄보디아는 베트남 태국 이런 국가들이랑 같이 있어요",
          "original_form": "캄보디아는 베트남 태국 이런 국가들이랑 같이 있어요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.20",
          "form": "넨넹",
          "original_form": "넨넹",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.21",
          "form": "그리고 수도는 프놈펜이구 앙코르와트가 굉장히 유명한 유적지라서",
          "original_form": "그리고 수도는 프놈펜이구 앙코르와트가 굉장히 유명한 유적지라서",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.22",
          "form": "국기에도 이걸 그려넣어놨어요",
          "original_form": "국기에도 이걸 그려넣어놨어요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.23",
          "form": "우와 똑똑하다",
          "original_form": "우와 똑똑하다",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.24",
          "form": "근데 ㅋㅋㅋ 캄보디아 숙소 너무 충격받았어요",
          "original_form": "근데 ㅋㅋㅋ 캄보디아 숙소 너무 충격받았어요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024062.1.25",
          "form": "왜용?",
          "original_form": "왜용?",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.26",
          "form": "아니 글쎄 방 배정 받고 들어갔는데 되게",
          "original_form": "아니 글쎄 방 배정 받고 들어갔는데 되게",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.27",
          "form": "허름하고 낡았어요 ㅜㅜㅜㅜ",
          "original_form": "허름하고 낡았어요 ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.28",
          "form": "ㅋㅋㅋㅋ반전은 없네영ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ반전은 없네영ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.29",
          "form": "더 놀라운건",
          "original_form": "더 놀라운건",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.30",
          "form": "도마뱀 나오진 않앗어요?",
          "original_form": "도마뱀 나오진 않앗어요?",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.31",
          "form": "침대 위에 벽지에 도마뱀이 딱 붙어있더라니까요",
          "original_form": "침대 위에 벽지에 도마뱀이 딱 붙어있더라니까요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.32",
          "form": "처음에 이거 장식인 줄 알았네요",
          "original_form": "처음에 이거 장식인 줄 알았네요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.33",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024062.1.34",
          "form": "갑자기 움직여가지고 저 그날 꺅!!!하고 소리질러서 사람들 다 나왔어요 ㅋㅋㅋㅋ",
          "original_form": "갑자기 움직여가지고 저 그날 꺅!!!하고 소리질러서 사람들 다 나왔어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.35",
          "form": "계속보면 귀여워서 같이 잘만하다던데",
          "original_form": "계속보면 귀여워서 같이 잘만하다던데",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.36",
          "form": "전혀요...",
          "original_form": "전혀요...",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.37",
          "form": "그래서 친구가 방 바꿔줬어요 ㅜㅜ",
          "original_form": "그래서 친구가 방 바꿔줬어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.38",
          "form": "아니구나ㅋㅋㅋㅋㅋ",
          "original_form": "아니구나ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.39",
          "form": "캄보디아 치안이 안좋아가지고",
          "original_form": "캄보디아 치안이 안좋아가지고",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.40",
          "form": "숙소 입구에 총들고 지키고 있었어요",
          "original_form": "숙소 입구에 총들고 지키고 있었어요",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.42",
          "form": "그게 더 충격이네요",
          "original_form": "그게 더 충격이네요",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.43",
          "form": "아니 근데 그런 상황에 여행을 갔네요",
          "original_form": "아니 근데 그런 상황에 여행을 갔네요",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.44",
          "form": "그쳐.. 밤에 약탈하는 일이 빈번하더래요",
          "original_form": "그쳐.. 밤에 약탈하는 일이 빈번하더래요",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024062.1.45",
          "form": "제가 알고 갔나요... ㅠㅠ",
          "original_form": "제가 알고 갔나요... ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024062.1.46",
          "form": "아..ㅋㅋㅋ",
          "original_form": "아..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024062.1.47",
          "form": "학교에서 프로그램으로 경비 지원해줬어요",
          "original_form": "학교에서 프로그램으로 경비 지원해줬어요",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024062.1.48",
          "form": "총으로 지켜주다니 그래도ㅋㅋㅋ 나름 안전했네요",
          "original_form": "총으로 지켜주다니 그래도ㅋㅋㅋ 나름 안전했네요",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024062.1.49",
          "form": "시장에서도 총들고 서있더라구요",
          "original_form": "시장에서도 총들고 서있더라구요",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024062.1.50",
          "form": "눈마주쳤는데 제가 쫄았어요",
          "original_form": "눈마주쳤는데 제가 쫄았어요",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024062.1.51",
          "form": "와... 앙코르와트 진짜 가보고싶은데",
          "original_form": "와... 앙코르와트 진짜 가보고싶은데",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024062.1.52",
          "form": "거기서 도둑으로 오해 받으면 저 세상 가는건 한순간이겠다 싶어서 조용히 구경만 했어요",
          "original_form": "거기서 도둑으로 오해 받으면 저 세상 가는건 한순간이겠다 싶어서 조용히 구경만 했어요",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024062.1.53",
          "form": "얘기를 들으니 겁나는 곳이네용",
          "original_form": "얘기를 들으니 겁나는 곳이네용",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024062.1.54",
          "form": "앙코르와트 진짜",
          "original_form": "앙코르와트 진짜",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024062.1.55",
          "form": "어우 되게 신기한 곳인데",
          "original_form": "어우 되게 신기한 곳인데",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024062.1.56",
          "form": "입구에 원숭이가 저 물병 뺏어먹은 것도 생각나네요",
          "original_form": "입구에 원숭이가 저 물병 뺏어먹은 것도 생각나네요",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
