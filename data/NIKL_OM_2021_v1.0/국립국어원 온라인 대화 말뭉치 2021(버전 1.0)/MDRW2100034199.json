      "utterance": [
        {
          "id": "MDRW2100034199.1.1",
          "form": "건강 잘 챙기고 계시나용 ~",
          "original_form": "건강 잘 챙기고 계시나용 ~",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034199.1.2",
          "form": "네 나름 챙기는중이에요",
          "original_form": "네 나름 챙기는중이에요",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100034199.1.3",
          "form": "주로 뭐로 하시나요?",
          "original_form": "주로 뭐로 하시나요?",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034199.1.4",
          "form": "아침에 비타민 먹고있어요",
          "original_form": "아침에 비타민 먹고있어요",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100034199.1.5",
          "form": "비타민 뭐 드세오?",
          "original_form": "비타민 뭐 드세오?",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100034199.1.6",
          "form": "고려은단이요ㅋㅋ",
          "original_form": "고려은단이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034199.1.7",
          "form": "ㅋㅋㅋㅋ 어떤가요",
          "original_form": "ㅋㅋㅋㅋ 어떤가요",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034199.1.8",
          "form": "유명한 거 드시네여",
          "original_form": "유명한 거 드시네여",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034199.1.9",
          "form": "잘 모르겠어서 유명한걸루 사봤어요",
          "original_form": "잘 모르겠어서 유명한걸루 사봤어요",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100034199.1.10",
          "form": "괜찮은거 같아요ㅋㅋㅋ",
          "original_form": "괜찮은거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100034199.1.11",
          "form": "저도 영양제 먹어야해서",
          "original_form": "저도 영양제 먹어야해서",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034199.1.12",
          "form": "궁금했어요..",
          "original_form": "궁금했어요..",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034199.1.13",
          "form": "거의 다 홍보 같이 보여서요ㅋㅋ",
          "original_form": "거의 다 홍보 같이 보여서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034199.1.14",
          "form": "유명한거는 괜찮은거 같아요",
          "original_form": "유명한거는 괜찮은거 같아요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034199.1.15",
          "form": "sns에서 첨들어보는 데 광고가 많아서 헷갈리죠",
          "original_form": "sns에서 첨들어보는 데 광고가 많아서 헷갈리죠",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034199.1.16",
          "form": "맞아요.. 광고 대박이에오",
          "original_form": "맞아요.. 광고 대박이에오",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034199.1.17",
          "form": "저두 너무 헷갈려서 많이 들어본 거 샀어요ㅋㅋ",
          "original_form": "저두 너무 헷갈려서 많이 들어본 거 샀어요ㅋㅋ",
