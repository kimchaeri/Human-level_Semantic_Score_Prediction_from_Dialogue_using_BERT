      "utterance": [
        {
          "id": "MDRW2100047558.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100047558.1.2",
          "form": "백수예요",
          "original_form": "백수예요",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100047558.1.3",
          "form": "아하 졸업한지 좀 되셨어요?",
          "original_form": "아하 졸업한지 좀 되셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047558.1.4",
          "form": "네네 님은요?",
          "original_form": "네네 님은요?",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100047558.1.5",
          "form": "저두 꽤 되어서 직장인이에요",
          "original_form": "저두 꽤 되어서 직장인이에요",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100047558.1.6",
          "form": "저는 학교에서 일하거든요",
          "original_form": "저는 학교에서 일하거든요",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100047558.1.7",
          "form": "선생님이세요?",
          "original_form": "선생님이세요?",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100047558.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100047558.1.9",
          "form": "애들 열심히 과제 내줬는데",
          "original_form": "애들 열심히 과제 내줬는데",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100047558.1.10",
          "form": "과제 검사해야하는 입장",
          "original_form": "과제 검사해야하는 입장",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100047558.1.11",
          "form": "잘해오나요?",
          "original_form": "잘해오나요?",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100047558.1.12",
          "form": "잘안해와요ㅋㅋㅋ 온라인 수업이라 더더욱 좀 그렇네여",
          "original_form": "잘안해와요ㅋㅋㅋ 온라인 수업이라 더더욱 좀 그렇네여",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047558.1.13",
          "form": "학창시절에 어떤 학생이셨나요?",
          "original_form": "학창시절에 어떤 학생이셨나요?",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047558.1.14",
          "form": "저는 착실한 학생이었죠",
          "original_form": "저는 착실한 학생이었죠",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100047558.1.15",
          "form": "도덕적이고 규칙 잘지키고 융통성 없고ㅋㅋ",
          "original_form": "도덕적이고 규칙 잘지키고 융통성 없고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100047558.1.16",
          "form": "ㅋㅋㅋ 공부도 잘하셨을거같아요",
          "original_form": "ㅋㅋㅋ 공부도 잘하셨을거같아요",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100047558.1.17",
          "form": "네 뭐 적당히 사범대 갈만큼ㅋㅋ",
          "original_form": "네 뭐 적당히 사범대 갈만큼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:30"
        }
      ]
