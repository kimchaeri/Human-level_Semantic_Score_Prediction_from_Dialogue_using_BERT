      "utterance": [
        {
          "id": "MDRW2100028550.1.1",
          "form": "여행 떠나고 싶네요",
          "original_form": "여행 떠나고 싶네요",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028550.1.2",
          "form": "여행을... 갑쉬다",
          "original_form": "여행을... 갑쉬다",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028550.1.3",
          "form": "어디가땡기나용",
          "original_form": "어디가땡기나용",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028550.1.4",
          "form": "전 가보고싶은 곳이 너무 많아서 한곳만 집긴 어렵네요",
          "original_form": "전 가보고싶은 곳이 너무 많아서 한곳만 집긴 어렵네요",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028550.1.5",
          "form": "일본,유럽,호주,로마,그리스 너무너무 많아요",
          "original_form": "일본,유럽,호주,로마,그리스 너무너무 많아요",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028550.1.6",
          "form": "근데 죽기 전에 유우니 사막을 한번 꼭 가보고싶긴 하네여",
          "original_form": "근데 죽기 전에 유우니 사막을 한번 꼭 가보고싶긴 하네여",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028550.1.7",
          "form": "ㅜㅜㅜ 다진짜 넘넘가고싶다",
          "original_form": "ㅜㅜㅜ 다진짜 넘넘가고싶다",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028550.1.8",
          "form": "유우니사막 저두 가고싶네여",
          "original_form": "유우니사막 저두 가고싶네여",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028550.1.9",
          "form": "어쩔수업다 돈을 모아봅시다",
          "original_form": "어쩔수업다 돈을 모아봅시다",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028550.1.10",
          "form": "가야만합니다이건",
          "original_form": "가야만합니다이건",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028550.1.11",
          "form": "가야합니다! 목표는 죽기 전까지!",
          "original_form": "가야합니다! 목표는 죽기 전까지!",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028550.1.12",
          "form": "조금씩 모아서 꼭 가봐요",
          "original_form": "조금씩 모아서 꼭 가봐요",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028550.1.13",
          "form": "혹시나 저 중에서도 제일 먼저 가보고 싶은 곳이 있나요?",
          "original_form": "혹시나 저 중에서도 제일 먼저 가보고 싶은 곳이 있나요?",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028550.1.14",
          "form": "저는 젤 가고보픈건 유럽이지만",
          "original_form": "저는 젤 가고보픈건 유럽이지만",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028550.1.15",
          "form": "가능성을 고려하면 역시 일본이 가장 빨리가게되지않을까",
          "original_form": "가능성을 고려하면 역시 일본이 가장 빨리가게되지않을까",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028550.1.16",
          "form": "생각하고잇다네용",
          "original_form": "생각하고잇다네용",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028550.1.17",
          "form": "마자요 일본이 아무래도 가깝고 가기쉬우니까!",
          "original_form": "마자요 일본이 아무래도 가깝고 가기쉬우니까!",
          "speaker_id": "1",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100028550.1.18",
          "form": "저는 일본에 가게된다면 야간열차랑 유니버셜 스튜디오",
          "original_form": "저는 일본에 가게된다면 야간열차랑 유니버셜 스튜디오",
          "speaker_id": "1",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100028550.1.19",
          "form": "그리고 홋카이도 눈꽃축제 꼭 가보고싶네요",
          "original_form": "그리고 홋카이도 눈꽃축제 꼭 가보고싶네요",
          "speaker_id": "1",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100028550.1.20",
          "form": "너무 좋군요...",
          "original_form": "너무 좋군요...",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100028550.1.21",
          "form": "유니버셜 정말 조왓습니다 꼭 가봅십쇼",
          "original_form": "유니버셜 정말 조왓습니다 꼭 가봅십쇼",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100028550.1.22",
          "form": "아니지 같이갑시다(?",
          "original_form": "아니지 같이갑시다(?",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100028550.1.23",
          "form": "그럽시다! 같이가요 ",
          "original_form": "그럽시다! 같이가요 ",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100028550.1.24",
          "form": "유니버셜의 어떤게 가장 재밌었나요?",
          "original_form": "유니버셜의 어떤게 가장 재밌었나요?",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100028550.1.25",
          "form": "저느은 흠... 머가좋앗더라",
          "original_form": "저느은 흠... 머가좋앗더라",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100028550.1.26",
          "form": "그냥 그 테마가 있는 놀이기구들은",
          "original_form": "그냥 그 테마가 있는 놀이기구들은",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100028550.1.27",
          "form": "다조앗습니다",
          "original_form": "다조앗습니다",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100028550.1.28",
          "form": "쓰리디 안경 없이도 눈앞에 펼쳐지더라구용",
          "original_form": "쓰리디 안경 없이도 눈앞에 펼쳐지더라구용",
          "speaker_id": "2",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100028550.1.29",
          "form": "모든게 재밌었다니...유명한만큼 그 값을 하나보네요!!",
          "original_form": "모든게 재밌었다니...유명한만큼 그 값을 하나보네요!!",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100028550.1.30",
          "form": "쓸디 안경 안써도 펼쳐진다니 상상만으로도 멋지다",
          "original_form": "쓸디 안경 안써도 펼쳐진다니 상상만으로도 멋지다",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100028550.1.31",
          "form": "제가 안경을 써서 이중안경 별로 안좋아하는데 대박이네요 ㅎㅎ",
          "original_form": "제가 안경을 써서 이중안경 별로 안좋아하는데 대박이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100028550.1.32",
          "form": "가서 기념품은 사셨나요?",
          "original_form": "가서 기념품은 사셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100028550.1.33",
          "form": "네ㅋㅋㅋ 그때 코난 어트랙션?이 있던 시즌이라",
          "original_form": "네ㅋㅋㅋ 그때 코난 어트랙션?이 있던 시즌이라",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100028550.1.34",
          "form": "코난 관련 컵을 사서 왔었어용",
          "original_form": "코난 관련 컵을 사서 왔었어용",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100028550.1.35",
          "form": "유니버셜은 가끔 그렇게 일본 게임이나 애니메이션 테마도",
          "original_form": "유니버셜은 가끔 그렇게 일본 게임이나 애니메이션 테마도",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100028550.1.36",
          "form": "하더라구요",
          "original_form": "하더라구요",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100028550.1.37",
          "form": "핫 코난 콜라보라니 대박!",
          "original_form": "핫 코난 콜라보라니 대박!",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100028550.1.38",
          "form": "내가 좋아하는 테마로 콜라보 할때 가면 진짜 몇배로 더 재밌겟어요",
          "original_form": "내가 좋아하는 테마로 콜라보 할때 가면 진짜 몇배로 더 재밌겟어요",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100028550.1.39",
          "form": "그 컵은 지금도 쓰고 계신가요?",
          "original_form": "그 컵은 지금도 쓰고 계신가요?",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100028550.1.40",
          "form": "아뇽 요즘은 그냥 장식품이 됐어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇽 요즘은 그냥 장식품이 됐어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100028550.1.41",
          "form": "그냥 잘 모셔놧슴다",
          "original_form": "그냥 잘 모셔놧슴다",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100028550.1.42",
          "form": "name1시두 언젠가 함...",
          "original_form": "&name1&시두 언젠가 함...",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100028550.1.43",
          "form": "좋아하는테마할때 들려보세여",
          "original_form": "좋아하는테마할때 들려보세여",
