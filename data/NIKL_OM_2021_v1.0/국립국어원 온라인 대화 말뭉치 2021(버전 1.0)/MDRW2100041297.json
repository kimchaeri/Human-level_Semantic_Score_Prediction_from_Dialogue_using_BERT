      "utterance": [
        {
          "id": "MDRW2100041297.1.1",
          "form": "name1님! 잇지 노래도 자주 들으시나요 ㅎ.ㅎ ?",
          "original_form": "&name1&님! 잇지 노래도 자주 들으시나요 ㅎ.ㅎ ?",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100041297.1.2",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100041297.1.3",
          "form": "잇지는 수록곡이 별로 없는걸로 아는데 맞나요,,?",
          "original_form": "잇지는 수록곡이 별로 없는걸로 아는데 맞나요,,?",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100041297.1.4",
          "form": "뭔가 타이틀곡만 들었던 기억이?!",
          "original_form": "뭔가 타이틀곡만 들었던 기억이?!",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100041297.1.5",
          "form": "헉 네네! 저도 타이틀곡만 들어요!",
          "original_form": "헉 네네! 저도 타이틀곡만 들어요!",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041297.1.6",
          "form": "아까 문득 워너비 듣다가 너무 좋아서",
          "original_form": "아까 문득 워너비 듣다가 너무 좋아서",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041297.1.7",
          "form": "한 번 여쭤봤어요 ㅎ.ㅎ",
          "original_form": "한 번 여쭤봤어요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041297.1.8",
          "form": "앜ㅋㅋ 그럼 저랑 비슷한 정도로 들으시겠어요!",
          "original_form": "앜ㅋㅋ 그럼 저랑 비슷한 정도로 들으시겠어요!",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100041297.1.9",
          "form": "아니근데 마피아 인더모닝은",
          "original_form": "아니근데 마피아 인더모닝은",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100041297.1.10",
          "form": "박진영이 무슨 생각으로",
          "original_form": "박진영이 무슨 생각으로",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100041297.1.11",
          "form": "제목을 그따위로 지었는지;;;",
          "original_form": "제목을 그따위로 지었는지;;;",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100041297.1.12",
          "form": "아 그니까요 걍 마피아라고만 하던가",
          "original_form": "아 그니까요 걍 마피아라고만 하던가",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100041297.1.13",
          "form": "가사도 진짜....",
          "original_form": "가사도 진짜....",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100041297.1.14",
          "form": "진짜...........",
          "original_form": "진짜...........",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100041297.1.15",
          "form": "가만안둬.....",
          "original_form": "가만안둬.....",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100041297.1.16",
          "form": "너무 구려요 텔미 가사보다 더",
          "original_form": "너무 구려요 텔미 가사보다 더",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100041297.1.17",
          "form": "아니그니까요",
          "original_form": "아니그니까요",
          "speaker_id": "1",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100041297.1.18",
          "form": "아니면 그냥 마피아 라고만 짓던가ㅜ",
          "original_form": "아니면 그냥 마피아 라고만 짓던가ㅜ",
          "speaker_id": "1",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100041297.1.19",
          "form": "그래도 잇지 멤버들이 너무 무대를 잘해서",
          "original_form": "그래도 잇지 멤버들이 너무 무대를 잘해서",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041297.1.20",
          "form": "살아남은 거 같아요",
          "original_form": "살아남은 거 같아요",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041297.1.21",
          "form": "마피아 안무 짱 멋지잖아요",
          "original_form": "마피아 안무 짱 멋지잖아요",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041297.1.22",
          "form": "맞아요!!!@",
          "original_form": "맞아요!!!@",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041297.1.23",
          "form": "아니 어쩜 그렇게 다들 무대를 잘하는지?!",
          "original_form": "아니 어쩜 그렇게 다들 무대를 잘하는지?!",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041297.1.24",
          "form": "사실 저는...스엠 아이돌이",
          "original_form": "사실 저는...스엠 아이돌이",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041297.1.25",
          "form": "점점 무대를 못하고 있는 것 같다고 생각이 드는데",
          "original_form": "점점 무대를 못하고 있는 것 같다고 생각이 드는데",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041297.1.26",
          "form": "재왑피는 안그런거같아요",
          "original_form": "재왑피는 안그런거같아요",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041297.1.27",
          "form": "아 ㅠㅠ 저도 그 생각해요 점점 라이브도 불안정해지교",
          "original_form": "아 ㅠㅠ 저도 그 생각해요 점점 라이브도 불안정해지교",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041297.1.28",
          "form": "뭔가 칼군무도 사라지는 기분 ㅠㅠ !",
          "original_form": "뭔가 칼군무도 사라지는 기분 ㅠㅠ !",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041297.1.29",
          "form": "제와피는 노래실력은 몰라도 퍼포먼스는 정말",
          "original_form": "제와피는 노래실력은 몰라도 퍼포먼스는 정말",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041297.1.30",
          "form": "잘하는 거 같아요",
          "original_form": "잘하는 거 같아요",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041297.1.31",
          "form": "맞아요 맞아요!!!!",
          "original_form": "맞아요 맞아요!!!!",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100041297.1.32",
          "form": "와이지도 여전히 무대는 잘하는데",
          "original_form": "와이지도 여전히 무대는 잘하는데",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041297.1.33",
          "form": "스엠 왜그러지 ㅠ.^",
          "original_form": "스엠 왜그러지 ㅠ.^",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041297.1.34",
          "form": "그래도 이번 넥스트 레벨은 괜찬ㄹ았던 거 같아요",
          "original_form": "그래도 이번 넥스트 레벨은 괜찬ㄹ았던 거 같아요",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041297.1.35",
          "form": "아 맞아요 넥스트 레벨은",
          "original_form": "아 맞아요 넥스트 레벨은",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041297.1.36",
          "form": "진짜 히트 쳤죠",
          "original_form": "진짜 히트 쳤죠",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041297.1.37",
          "form": "음악을 누가 골라오는건지",
          "original_form": "음악을 누가 골라오는건지",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041297.1.38",
          "form": "저는 원곡도 좋아서 원곡도 듣거든요!!!",
          "original_form": "저는 원곡도 좋아서 원곡도 듣거든요!!!",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041297.1.39",
          "form": "그 원곡이 그그",
          "original_form": "그 원곡이 그그",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041297.1.40",
          "form": "그 원곡이 그",
          "original_form": "그 원곡이 그",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041297.1.41",
          "form": "분노의 질주?",
          "original_form": "분노의 질주?",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041297.1.42",
          "form": "ost 맞죠!",
          "original_form": "ost 맞죠!",
