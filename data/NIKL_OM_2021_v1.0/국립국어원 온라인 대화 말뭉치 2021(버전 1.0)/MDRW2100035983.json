      "utterance": [
        {
          "id": "MDRW2100035983.1.1",
          "form": "헉 아재버거 단종됐나요",
          "original_form": "헉 아재버거 단종됐나요",
          "speaker_id": "1",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035983.1.2",
          "form": "그거 비싸고 맛있다던데",
          "original_form": "그거 비싸고 맛있다던데",
          "speaker_id": "1",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035983.1.3",
          "form": "넹넹ㅜㅜ 어느샌가 안보이더니",
          "original_form": "넹넹ㅜㅜ 어느샌가 안보이더니",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035983.1.4",
          "form": "얼마전에 먹으러가니까 이제 안나온다고 하더라구여",
          "original_form": "얼마전에 먹으러가니까 이제 안나온다고 하더라구여",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035983.1.5",
          "form": "맥도날드 신상 갈릭버거 드셔보셨어용?",
          "original_form": "맥도날드 신상 갈릭버거 드셔보셨어용?",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035983.1.6",
          "form": "헉 아뇨",
          "original_form": "헉 아뇨",
          "speaker_id": "1",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035983.1.7",
          "form": "맛잇을거같네요",
          "original_form": "맛잇을거같네요",
          "speaker_id": "1",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035983.1.8",
          "form": "창녕갈릭버거인가 그거맞죠?",
          "original_form": "창녕갈릭버거인가 그거맞죠?",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035983.1.9",
          "form": "조금있음 한정기간 끝난다는데",
          "original_form": "조금있음 한정기간 끝난다는데",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035983.1.10",
          "form": "이번주에 먹으러가려구여",
          "original_form": "이번주에 먹으러가려구여",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035983.1.11",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035983.1.12",
          "form": "헐 한정판이구나",
          "original_form": "헐 한정판이구나",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035983.1.13",
          "form": "넹 곧 사라진대요",
          "original_form": "넹 곧 사라진대요",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035983.1.14",
          "form": "그전에 드셔보시길...!",
          "original_form": "그전에 드셔보시길...!",
          "speaker_id": "2",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035983.1.15",
          "form": "아 가봐야겠네욬ㅋㅋㅋ",
          "original_form": "아 가봐야겠네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035983.1.16",
          "form": "맥날에서는 뭐 제일 좋아하세요?",
          "original_form": "맥날에서는 뭐 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035983.1.17",
          "form": "저 베토디요!",
          "original_form": "저 베토디요!",
          "speaker_id": "2",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035983.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100035983.1.19",
          "form": "베토딬ㅋㅋㅋ 순간 뭔가했네요",
          "original_form": "베토딬ㅋㅋㅋ 순간 뭔가했네요",
          "speaker_id": "1",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100035983.1.20",
          "form": "저는 빅맥 가끔 먹으면 맛잇더라구요",
          "original_form": "저는 빅맥 가끔 먹으면 맛잇더라구요",
          "speaker_id": "1",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100035983.1.21",
          "form": "약간 불량식품맛",
          "original_form": "약간 불량식품맛",
          "speaker_id": "1",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100035983.1.22",
          "form": "좀 잘 느껴지는데",
          "original_form": "좀 잘 느껴지는데",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035983.1.23",
          "form": "빅맥",
          "original_form": "빅맥",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035983.1.24",
          "form": "고기맛이",
          "original_form": "고기맛이",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035983.1.25",
          "form": "잘 느껴지는 햄버거 좋아하시나봐용!",
          "original_form": "잘 느껴지는 햄버거 좋아하시나봐용!",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100035983.1.26",
          "form": "아 맞아요 ㅋㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:25"
        }
      ]
    }
