      "utterance": [
        {
          "id": "MDRW2100041715.1.1",
          "form": "코로나때문이지않을까?",
          "original_form": "코로나때문이지않을까?",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041715.1.2",
          "form": "몰러...",
          "original_form": "몰러...",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041715.1.3",
          "form": "안되면",
          "original_form": "안되면",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041715.1.4",
          "form": "게하영업을어케해",
          "original_form": "게하영업을어케해",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041715.1.5",
          "form": "좀신기하긴하군",
          "original_form": "좀신기하긴하군",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041715.1.6",
          "form": "ㅜ고생이구먼",
          "original_form": "ㅜ고생이구먼",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041715.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041715.1.8",
          "form": "게하리모델링한뎌...",
          "original_form": "게하리모델링한뎌...",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041715.1.9",
          "form": "방잡았다",
          "original_form": "방잡았다",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041715.1.10",
          "form": "오늘어케됨",
          "original_form": "오늘어케됨",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041715.1.11",
          "form": "오 안산을갔군",
          "original_form": "오 안산을갔군",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041715.1.12",
          "form": "담주부터는안산에서출퇴근이여..",
          "original_form": "담주부터는안산에서출퇴근이여..",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041715.1.13",
          "form": "이번주는자택",
          "original_form": "이번주는자택",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041715.1.14",
          "form": ":Q...",
          "original_form": ":Q...",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041715.1.15",
          "form": "네...?",
          "original_form": "네...?",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.16",
          "form": "아이번주는",
          "original_form": "아이번주는",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.17",
          "form": "자택이군여",
          "original_form": "자택이군여",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.18",
          "form": "그럼혹시 이번주에....",
          "original_form": "그럼혹시 이번주에....",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.19",
          "form": "어우지쳐....",
          "original_form": "어우지쳐....",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.20",
          "form": "오드윅을보자하면",
          "original_form": "오드윅을보자하면",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.21",
          "form": "무리겄지",
          "original_form": "무리겄지",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.22",
          "form": "아마쪼끔힘들지않을까시포요",
          "original_form": "아마쪼끔힘들지않을까시포요",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.23",
          "form": "어쨌든거기살아야해서이거저거준비하려면",
          "original_form": "어쨌든거기살아야해서이거저거준비하려면",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.24",
          "form": "그래용",
          "original_form": "그래용",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.25",
          "form": ":Q....",
          "original_form": ":Q....",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.26",
          "form": "ㅋㅋㅋㅋㅋ ㅇㅋㅇㅋ시간은많으니까여",
          "original_form": "ㅋㅋㅋㅋㅋ ㅇㅋㅇㅋ시간은많으니까여",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041715.1.27",
          "form": "주말도있고",
          "original_form": "주말도있고",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041715.1.28",
          "form": "좀안정되면가댜",
          "original_form": "좀안정되면가댜",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041715.1.29",
          "form": "ㅋㅋㅋㅋㅇㅇ",
          "original_form": "ㅋㅋㅋㅋㅇㅇ",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041715.1.30",
