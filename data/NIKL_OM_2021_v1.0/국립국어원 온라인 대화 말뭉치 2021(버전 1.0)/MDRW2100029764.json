          "id": "MDRW2100029764.1.42",
          "form": "박사하면뭐",
          "original_form": "박사하면뭐",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029764.1.43",
          "form": "굴지의건설회사나",
          "original_form": "굴지의건설회사나",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029764.1.44",
          "form": "아님 설계업체나",
          "original_form": "아님 설계업체나",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029764.1.45",
          "form": "굿이져",
          "original_form": "굿이져",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029764.1.46",
          "form": "ㅋㅋㅋㅋ 일마니해야할거같아서 가기시러유",
          "original_form": "ㅋㅋㅋㅋ 일마니해야할거같아서 가기시러유",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029764.1.47",
          "form": "백수가 꿈이에요",
          "original_form": "백수가 꿈이에요",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029764.1.48",
          "form": "로또!!!!파워볼!!!",
          "original_form": "로또!!!!파워볼!!!",
          "speaker_id": "1",
