      "utterance": [
        {
          "id": "MDRW2100029764.1.1",
          "form": "회사다니실만한가요",
          "original_form": "회사다니실만한가요",
          "speaker_id": "1",
          "time": "20210903 13:13"
        },
        {
          "id": "MDRW2100029764.1.2",
          "form": "회사 그냥 뭐 대충다니고 있어요 ㅋㅋ",
          "original_form": "회사 그냥 뭐 대충다니고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029764.1.3",
          "form": "회사 왜 쉬시나요?",
          "original_form": "회사 왜 쉬시나요?",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029764.1.4",
          "form": "ㅋㅋㅋ 잠시... 학위따느라",
          "original_form": "ㅋㅋㅋ 잠시... 학위따느라",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029764.1.5",
          "form": "출근 제일 싫어요 영원히 백수이고싶어요",
          "original_form": "출근 제일 싫어요 영원히 백수이고싶어요",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029764.1.6",
          "form": "ㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029764.1.7",
          "form": "박사중인가요?",
          "original_form": "박사중인가요?",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029764.1.8",
          "form": "넹.... .",
          "original_form": "넹.... .",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029764.1.9",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029764.1.10",
          "form": "주변에 박사중인 친구들이 많아서",
          "original_form": "주변에 박사중인 친구들이 많아서",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029764.1.11",
          "form": "고생이 많으십니당..",
          "original_form": "고생이 많으십니당..",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029764.1.12",
          "form": "전공은요?",
          "original_form": "전공은요?",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029764.1.13",
          "form": "토목....공대... ㅎㅎㅎㅎㅎ",
          "original_form": "토목....공대... ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100029764.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029764.1.15",
          "form": "토목 박사라",
          "original_form": "토목 박사라",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029764.1.16",
          "form": "여자분이?",
          "original_form": "여자분이?",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029764.1.17",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029764.1.18",
          "form": "대단하십니다",
          "original_form": "대단하십니다",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029764.1.19",
          "form": "어스앙카 장점좀 알려주세유",
          "original_form": "어스앙카 장점좀 알려주세유",
          "speaker_id": "2",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100029764.1.20",
          "form": "ㅋㅋㅋㅋㅋ 그런거몰라요.....ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 그런거몰라요.....ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029764.1.21",
          "form": "실무 무지 ㅋㅋㅋㅋㅋ",
          "original_form": "실무 무지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029764.1.22",
          "form": "연구분야가 뭐에요?",
          "original_form": "연구분야가 뭐에요?",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029764.1.23",
          "form": "저 건설업무도 맡고있어서 ㅋㅋ",
          "original_form": "저 건설업무도 맡고있어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029764.1.24",
          "form": "지금 건물만 5개 짓고있고",
          "original_form": "지금 건물만 5개 짓고있고",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029764.1.25",
          "form": "설계단계가 2개더 있어요 ㅋㅋ",
          "original_form": "설계단계가 2개더 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029764.1.26",
          "form": "우와...건물주?",
          "original_form": "우와...건물주?",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100029764.1.27",
          "form": "저 구조해석입니닼ㅋㅋㅋㅋㅋ ㅋ",
          "original_form": "저 구조해석입니닼ㅋㅋㅋㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100029764.1.28",
          "form": "아녀 ㅋㅋ시설건립단?",
          "original_form": "아녀 ㅋㅋ시설건립단?",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029764.1.29",
          "form": "뭐 그런겁니다 ㅋㅋ",
          "original_form": "뭐 그런겁니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029764.1.30",
          "form": "공공기관다니구요",
          "original_form": "공공기관다니구요",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029764.1.31",
          "form": "그렇군요 ㅋㅋ",
          "original_form": "그렇군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029764.1.32",
          "form": "전 기계분야라",
          "original_form": "전 기계분야라",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029764.1.33",
          "form": "기계발주 감독",
          "original_form": "기계발주 감독",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029764.1.34",
          "form": "이렇게 합니다",
          "original_form": "이렇게 합니다",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029764.1.35",
          "form": "토목 박사과정 쉽지않은 결정일텐데",
          "original_form": "토목 박사과정 쉽지않은 결정일텐데",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029764.1.36",
          "form": "어찌하시게됫서요?",
          "original_form": "어찌하시게됫서요?",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029764.1.37",
          "form": "아항..그니까요 이거왜했는지...재밋어보여서 햇는데요",
          "original_form": "아항..그니까요 이거왜했는지...재밋어보여서 햇는데요",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029764.1.38",
          "form": "기계과나갈걸",
          "original_form": "기계과나갈걸",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029764.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029764.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029764.1.41",
          "form": "빛볼날 있을겁니다",
          "original_form": "빛볼날 있을겁니다",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
