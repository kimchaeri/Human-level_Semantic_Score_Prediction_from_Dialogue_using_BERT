      "utterance": [
        {
          "id": "MDRW2100045850.1.1",
          "form": "아침식사 하셨나요",
          "original_form": "아침식사 하셨나요",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045850.1.2",
          "form": "계란밥이요 뭐없을때 그게최고요ㅋ",
          "original_form": "계란밥이요 뭐없을때 그게최고요ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045850.1.3",
          "form": "저도 계란밥 좋아해요",
          "original_form": "저도 계란밥 좋아해요",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045850.1.4",
          "form": "간단하고 먹기 편하죠",
          "original_form": "간단하고 먹기 편하죠",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045850.1.5",
          "form": "네 김치만잇으면 맛잇어요ㅋ",
          "original_form": "네 김치만잇으면 맛잇어요ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045850.1.6",
          "form": "김치도 알타리하고 먹으면 더 맛있는것 같아요",
          "original_form": "김치도 알타리하고 먹으면 더 맛있는것 같아요",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045850.1.7",
          "form": "와 저도 그조합 좋아해요ㅋㅋ",
          "original_form": "와 저도 그조합 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045850.1.8",
          "form": "전 아침으로 호박죽 먹고 커피 한잔 마셔요",
          "original_form": "전 아침으로 호박죽 먹고 커피 한잔 마셔요",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045850.1.9",
          "form": "전 커피는 안마셔서요\r\n물 많이 마시려고해도 잘 안되더라고요",
          "original_form": "전 커피는 안마셔서요\r\n물 많이 마시려고해도 잘 안되더라고요",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045850.1.10",
          "form": "전 하루 한 두잔 마셔요",
          "original_form": "전 하루 한 두잔 마셔요",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045850.1.11",
          "form": "텀블러 세잔은 마셔야지 하는데 한잔 겨우ㅋ",
          "original_form": "텀블러 세잔은 마셔야지 하는데 한잔 겨우ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045850.1.12",
          "form": "평일 일할때만 마시고 주말엔 안 마셔요",
          "original_form": "평일 일할때만 마시고 주말엔 안 마셔요",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045850.1.13",
          "form": "저도 물 먹는게 더 어려워요",
          "original_form": "저도 물 먹는게 더 어려워요",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045850.1.14",
          "form": "맞아요ㅋ\r\n전 과일음료 좋아해요",
          "original_form": "맞아요ㅋ\r\n전 과일음료 좋아해요",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100045850.1.15",
          "form": "커피말고는 꽃 차를 좋아해쇼",
          "original_form": "커피말고는 꽃 차를 좋아해쇼",
          "speaker_id": "2",
          "time": "20210908 08:22"
        }
      ]
