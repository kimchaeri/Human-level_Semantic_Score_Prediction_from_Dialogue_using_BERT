      "utterance": [
        {
          "id": "MDRW2100014656.1.1",
          "form": "요즘 날씨 이상해",
          "original_form": "요즘 날씨 이상해",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014656.1.2",
          "form": "근데 오늘 시원하던데",
          "original_form": "근데 오늘 시원하던데",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014656.1.3",
          "form": "딱 나가서 맥주 먹으면 좋을 날씨",
          "original_form": "딱 나가서 맥주 먹으면 좋을 날씨",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014656.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014656.1.5",
          "form": "비가 왔다가 안왔다가",
          "original_form": "비가 왔다가 안왔다가",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014656.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014656.1.7",
          "form": "창문 계속 열었다가 닫았다가",
          "original_form": "창문 계속 열었다가 닫았다가",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.8",
          "form": "그리고 좀 습함 ㅋㅋ",
          "original_form": "그리고 좀 습함 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.9",
          "form": "그래서 찝찝해 ㅠ",
          "original_form": "그래서 찝찝해 ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.10",
          "form": "가을 언제 와!",
          "original_form": "가을 언제 와!",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.11",
          "form": "가을 와야 어디도 좀 가는데",
          "original_form": "가을 와야 어디도 좀 가는데",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.12",
          "form": "가을은 이제 스쳐가지 않나요",
          "original_form": "가을은 이제 스쳐가지 않나요",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.13",
          "form": "1주 오고 겨울 올 듯",
          "original_form": "1주 오고 겨울 올 듯",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.15",
          "form": "잠깐 오고 겨울인 듯 ㅋㅋㅋㅋ",
          "original_form": "잠깐 오고 겨울인 듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.16",
          "form": "그래서 더욱 즐겨야 됨 가을 오면",
          "original_form": "그래서 더욱 즐겨야 됨 가을 오면",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.17",
          "form": "아 가을 겨울오면 이제 또 뭐 입나",
          "original_form": "아 가을 겨울오면 이제 또 뭐 입나",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014656.1.18",
          "form": "부산은 아까 진짜 난리던데 폭우때문에",
          "original_form": "부산은 아까 진짜 난리던데 폭우때문에",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014656.1.19",
          "form": "아 태풍 그쪽으로 갔지",
          "original_form": "아 태풍 그쪽으로 갔지",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014656.1.20",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014656.1.21",
          "form": "뭔가 지방쪽으로 가서 딱히 기사를 많이 못 본 거 같은 기분",
          "original_form": "뭔가 지방쪽으로 가서 딱히 기사를 많이 못 본 거 같은 기분",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014656.1.22",
          "form": "나도 커뮤니티에서 부산 천둥이랑 비로 난리난거 보고 알았어",
          "original_form": "나도 커뮤니티에서 부산 천둥이랑 비로 난리난거 보고 알았어",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014656.1.23",
          "form": "여기는 비 왔다 안왔다 정도라 ㅋㅋ",
          "original_form": "여기는 비 왔다 안왔다 정도라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014656.1.24",
          "form": "아까 비 안오길래 후딱 나갔는데",
          "original_form": "아까 비 안오길래 후딱 나갔는데",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014656.1.25",
          "form": "나가니까 비와가지고",
          "original_form": "나가니까 비와가지고",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014656.1.26",
          "form": "그래도 바람은 안불어서",
          "original_form": "그래도 바람은 안불어서",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014656.1.27",
          "form": "바람 불고 비오면 너무 다 젖으니까",
          "original_form": "바람 불고 비오면 너무 다 젖으니까",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014656.1.28",
          "form": "맞아 나도 몇일전에 나왔는데 비오길래 다시 들어감 ㅋㅋ",
          "original_form": "맞아 나도 몇일전에 나왔는데 비오길래 다시 들어감 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:25"
        }
      ]
    }
