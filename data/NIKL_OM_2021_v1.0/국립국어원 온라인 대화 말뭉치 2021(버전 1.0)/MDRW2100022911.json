      "utterance": [
        {
          "id": "MDRW2100022911.1.1",
          "form": "놀러가고싶은데 있어?",
          "original_form": "놀러가고싶은데 있어?",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100022911.1.2",
          "form": "응 있어",
          "original_form": "응 있어",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022911.1.3",
          "form": "어디",
          "original_form": "어디",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022911.1.4",
          "form": "나는 제주도!",
          "original_form": "나는 제주도!",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022911.1.5",
          "form": "누구랑",
          "original_form": "누구랑",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022911.1.6",
          "form": "혼자!",
          "original_form": "혼자!",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022911.1.7",
          "form": "왜 혼자가",
          "original_form": "왜 혼자가",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022911.1.8",
          "form": "혼자 한번 가보고 싶어",
          "original_form": "혼자 한번 가보고 싶어",
          "speaker_id": "2",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022911.1.9",
          "form": "언제가보게?",
          "original_form": "언제가보게?",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100022911.1.10",
          "form": "조만간!",
          "original_form": "조만간!",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022911.1.11",
          "form": "난 재주도말고 하늘나라",
          "original_form": "난 재주도말고 하늘나라",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022911.1.12",
          "form": "하늘나라에 진짜 천사있나 가보고싶어",
          "original_form": "하늘나라에 진짜 천사있나 가보고싶어",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022911.1.13",
          "form": "비행기 타고싶어?",
          "original_form": "비행기 타고싶어?",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022911.1.14",
          "form": "그치그치 비행기타고 날아가야지",
          "original_form": "그치그치 비행기타고 날아가야지",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022911.1.15",
          "form": "나도 비행기 타는거 좋아해",
          "original_form": "나도 비행기 타는거 좋아해",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022911.1.16",
          "form": "미욘세도 해야돼",
          "original_form": "미욘세도 해야돼",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022911.1.17",
          "form": "안좋아하자나",
          "original_form": "안좋아하자나",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022911.1.18",
          "form": "미욘세가 뭐야?",
          "original_form": "미욘세가 뭐야?",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022911.1.19",
          "form": "면세야 면세",
          "original_form": "면세야 면세",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022911.1.20",
          "form": "일본갔는데 미욘세? 이러드라",
          "original_form": "일본갔는데 미욘세? 이러드라",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022911.1.21",
          "form": "아 그렇구마",
          "original_form": "아 그렇구마",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022911.1.22",
          "form": "여기는 일본이 아니야",
          "original_form": "여기는 일본이 아니야",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022911.1.23",
          "form": "너가 물어봤잖아 ㅡㅡ",
          "original_form": "너가 물어봤잖아 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022911.1.24",
          "form": "너가 미욘세라매 면세라고 했어야지",
          "original_form": "너가 미욘세라매 면세라고 했어야지",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022911.1.25",
          "form": "면세는 한국말 미욘세는 일본말 일본갔을때 거기서 직원이 미욘세? 이랬다고 ㅡㅡ",
          "original_form": "면세는 한국말 미욘세는 일본말 일본갔을때 거기서 직원이 미욘세? 이랬다고 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022911.1.26",
          "form": "너는 하늘나라 말고 다른곳은 가보고싶은 곳 없어?",
          "original_form": "너는 하늘나라 말고 다른곳은 가보고싶은 곳 없어?",
          "speaker_id": "2",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022911.1.27",
          "form": "나 미국",
          "original_form": "나 미국",
          "speaker_id": "1",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022911.1.28",
          "form": "미국 왜?",
          "original_form": "미국 왜?",
          "speaker_id": "2",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022911.1.29",
          "form": "검정고무신 보고 패드립하길래 미국가보고싶어졌어",
          "original_form": "검정고무신 보고 패드립하길래 미국가보고싶어졌어",
          "speaker_id": "1",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022911.1.30",
          "form": "독특하네",
          "original_form": "독특하네",
          "speaker_id": "2",
          "time": "20210829 19:08"
        }
      ]
    }
  ]
