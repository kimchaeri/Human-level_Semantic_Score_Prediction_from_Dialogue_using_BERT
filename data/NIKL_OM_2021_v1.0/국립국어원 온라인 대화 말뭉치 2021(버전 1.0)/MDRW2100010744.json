      "utterance": [
        {
          "id": "MDRW2100010744.1.1",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010744.1.2",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010744.1.3",
          "form": "네 저는 직장인입니다",
          "original_form": "네 저는 직장인입니다",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010744.1.4",
          "form": "저는 직장인이에요",
          "original_form": "저는 직장인이에요",
          "speaker_id": "1",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010744.1.5",
          "form": "알바해본 적 있으세요?",
          "original_form": "알바해본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010744.1.6",
          "form": "저는 이마트단기알바 해봣어요\n오요디님은요?",
          "original_form": "저는 이마트단기알바 해봣어요\n오요디님은요?",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010744.1.7",
          "form": "떡집에서 알바 해봤어요 이마트단기알바는 무슨일하나요?",
          "original_form": "떡집에서 알바 해봤어요 이마트단기알바는 무슨일하나요?",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010744.1.8",
          "form": "고객센터에있었는데 사람대하는게 젤 힘들더라구요\n오 떡맛있었겠다 힘들었나요 업무는?",
          "original_form": "고객센터에있었는데 사람대하는게 젤 힘들더라구요\n오 떡맛있었겠다 힘들었나요 업무는?",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010744.1.9",
          "form": "저도 사람대하는게 힘들더라구요 인턴해보신적 있으세요?",
          "original_form": "저도 사람대하는게 힘들더라구요 인턴해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010744.1.10",
          "form": "네 인턴도 해봤죠ㅎㅎ 월백만원 주더라구요",
          "original_form": "네 인턴도 해봤죠ㅎㅎ 월백만원 주더라구요",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010744.1.11",
          "form": "구직활동 힘드셨어요?",
          "original_form": "구직활동 힘드셨어요?",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010744.1.12",
          "form": "급여가 너무 낮네요 구직활동 힘들었죠 자리가 없더라구요 구직활동 힘드셨나요,",
          "original_form": "급여가 너무 낮네요 구직활동 힘들었죠 자리가 없더라구요 구직활동 힘드셨나요,",
          "speaker_id": "1",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010744.1.13",
          "form": "교수님추천으로 쉽게취업했는데\n이직할때 힘들었어요\n이직하셨나요?",
          "original_form": "교수님추천으로 쉽게취업했는데\n이직할때 힘들었어요\n이직하셨나요?",
          "speaker_id": "2",
          "time": "20210823 21:33"
        },
        {
          "id": "MDRW2100010744.1.14",
          "form": "아니요 해본적없네요 이직 여러번 하셨나요?",
          "original_form": "아니요 해본적없네요 이직 여러번 하셨나요?",
          "speaker_id": "1",
          "time": "20210823 21:33"
        }
      ]
