      "utterance": [
        {
          "id": "MDRW2100028195.1.1",
          "form": "야구는 잘 모르고 기록경기 좋아해요",
          "original_form": "야구는 잘 모르고 기록경기 좋아해요",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028195.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028195.1.3",
          "form": "사격 양궁 등이요",
          "original_form": "사격 양궁 등이요",
          "speaker_id": "2",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028195.1.4",
          "form": "저는 탁구좋아해요",
          "original_form": "저는 탁구좋아해요",
          "speaker_id": "1",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.5",
          "form": "올림픽도 열심히 봤어요",
          "original_form": "올림픽도 열심히 봤어요",
          "speaker_id": "2",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.6",
          "form": "사격은 올림픽때나 인기가...",
          "original_form": "사격은 올림픽때나 인기가...",
          "speaker_id": "1",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.7",
          "form": "탁구는 몇번 해봤어요",
          "original_form": "탁구는 몇번 해봤어요",
          "speaker_id": "2",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.8",
          "form": "사람들이 좀 냄비근성으로 잠깐 좋아하는듯",
          "original_form": "사람들이 좀 냄비근성으로 잠깐 좋아하는듯",
          "speaker_id": "1",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.9",
          "form": "농구도 좋아해요",
          "original_form": "농구도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.10",
          "form": "탁구는 전 좀 해요",
          "original_form": "탁구는 전 좀 해요",
          "speaker_id": "1",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.11",
          "form": "오 얼마나 하셨나요",
          "original_form": "오 얼마나 하셨나요",
          "speaker_id": "2",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.12",
          "form": "농구는 전 별로에요",
          "original_form": "농구는 전 별로에요",
          "speaker_id": "1",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100028195.1.13",
          "form": "전 동계 종목도 좋아해요",
          "original_form": "전 동계 종목도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028195.1.14",
          "form": "그냥 생활체육이죠 요즘도 하루 한 시간씫내요",
          "original_form": "그냥 생활체육이죠 요즘도 하루 한 시간씫내요",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028195.1.15",
          "form": "쇼트트랙 잼나요",
          "original_form": "쇼트트랙 잼나요",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028195.1.16",
