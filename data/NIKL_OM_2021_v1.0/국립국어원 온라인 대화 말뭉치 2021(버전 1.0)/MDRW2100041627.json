      "utterance": [
        {
          "id": "MDRW2100041627.1.1",
          "form": "마른오징어 좋아하세요?",
          "original_form": "마른오징어 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 13:27"
        },
        {
          "id": "MDRW2100041627.1.2",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210906 13:27"
        },
        {
          "id": "MDRW2100041627.1.3",
          "form": "저는 바싹 마른것도 좋아하지만 반건조가 더 좋아요",
          "original_form": "저는 바싹 마른것도 좋아하지만 반건조가 더 좋아요",
          "speaker_id": "1",
          "time": "20210906 13:27"
        },
        {
          "id": "MDRW2100041627.1.4",
          "form": "피데기요?",
          "original_form": "피데기요?",
          "speaker_id": "2",
          "time": "20210906 13:27"
        },
        {
          "id": "MDRW2100041627.1.5",
          "form": "피데기가 뭐죠?",
          "original_form": "피데기가 뭐죠?",
          "speaker_id": "1",
          "time": "20210906 13:27"
        },
        {
          "id": "MDRW2100041627.1.6",
          "form": "반건조 오징어를 피데기라고 해요 ㅋㅋ",
          "original_form": "반건조 오징어를 피데기라고 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100041627.1.7",
          "form": "아 네 좀 더 오동통해서 좋아해요 ㅋㅋ",
          "original_form": "아 네 좀 더 오동통해서 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100041627.1.8",
          "form": "씹는맛이 더 있지요",
          "original_form": "씹는맛이 더 있지요",
          "speaker_id": "2",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100041627.1.9",
          "form": "맞아요 요즘 오징어 너무 비싸졋어요ㅠㅠ",
          "original_form": "맞아요 요즘 오징어 너무 비싸졋어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100041627.1.10",
          "form": "맞아요 가격이 너무 올라서..",
          "original_form": "맞아요 가격이 너무 올라서..",
          "speaker_id": "2",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100041627.1.11",
          "form": "네 못먹은지 오래되었네요ㅠㅠ",
          "original_form": "네 못먹은지 오래되었네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100041627.1.12",
          "form": "마요네즈 찍어먹으면 맛있는데..",
          "original_form": "마요네즈 찍어먹으면 맛있는데..",
          "speaker_id": "2",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100041627.1.13",
          "form": "아 저도 좋아해요",
          "original_form": "아 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100041627.1.14",
          "form": "와사비 넣어서 와사마요 만들면 더 맛있어요",
          "original_form": "와사비 넣어서 와사마요 만들면 더 맛있어요",
          "speaker_id": "2",
          "time": "20210906 13:29"
        }
