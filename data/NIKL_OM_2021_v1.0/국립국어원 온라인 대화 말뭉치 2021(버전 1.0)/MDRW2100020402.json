      "utterance": [
        {
          "id": "MDRW2100020402.1.1",
          "form": "저 드디어 광화문 연가 예매했어요",
          "original_form": "저 드디어 광화문 연가 예매했어요",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100020402.1.2",
          "form": "ㅎㅎ 드디어",
          "original_form": "ㅎㅎ 드디어",
          "speaker_id": "2",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100020402.1.3",
          "form": "네 ㅋㅋㅋ 내일가요",
          "original_form": "네 ㅋㅋㅋ 내일가요",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100020402.1.4",
          "form": "오 캐스팅 누구?",
          "original_form": "오 캐스팅 누구?",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.5",
          "form": "윤도현 차지연이요",
          "original_form": "윤도현 차지연이요",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.6",
          "form": "와ㅜ윤도현",
          "original_form": "와ㅜ윤도현",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.7",
          "form": "엄기준으로 원래 예약했다가",
          "original_form": "엄기준으로 원래 예약했다가",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.8",
          "form": "윤도현으로 바꿨ㅇㅓ요",
          "original_form": "윤도현으로 바꿨ㅇㅓ요",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.9",
          "form": "오 왜요?",
          "original_form": "오 왜요?",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.10",
          "form": "엄기준 후기들이",
          "original_form": "엄기준 후기들이",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.11",
          "form": "안 좋더라구요",
          "original_form": "안 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.12",
          "form": "헉 의외네요",
          "original_form": "헉 의외네요",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.13",
          "form": "네 노래를 잘 못한대서",
          "original_form": "네 노래를 잘 못한대서",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100020402.1.14",
          "form": "음 그렇구나",
          "original_form": "음 그렇구나",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100020402.1.15",
          "form": "네 저도 전에 봤는데",
          "original_form": "네 저도 전에 봤는데",
          "speaker_id": "1",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100020402.1.16",
          "form": "별로였어요",
          "original_form": "별로였어요",
          "speaker_id": "1",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100020402.1.17",
          "form": "와 놀랍네요",
          "original_form": "와 놀랍네요",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100020402.1.18",
          "form": "네 어차피 4층이라",
          "original_form": "네 어차피 4층이라",
          "speaker_id": "1",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100020402.1.19",
          "form": "노래만 들릴거 같아서 ㅋㅋ",
          "original_form": "노래만 들릴거 같아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100020402.1.20",
          "form": "4층 가세요?",
          "original_form": "4층 가세요?",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100020402.1.21",
          "form": "네 ㅜㅜ 다 매진이라..",
          "original_form": "네 ㅜㅜ 다 매진이라..",
          "speaker_id": "1",
          "time": "20210903 18:53"
        }
      ]
