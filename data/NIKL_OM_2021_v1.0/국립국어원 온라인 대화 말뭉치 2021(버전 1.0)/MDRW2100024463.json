      "utterance": [
        {
          "id": "MDRW2100024463.1.1",
          "form": "건강을 위해 챙겨먹는 영양제 같은 거 있나요?",
          "original_form": "건강을 위해 챙겨먹는 영양제 같은 거 있나요?",
          "speaker_id": "2",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100024463.1.2",
          "form": "전 종합비타민만 챙겨먹어요!!",
          "original_form": "전 종합비타민만 챙겨먹어요!!",
          "speaker_id": "1",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100024463.1.3",
          "form": "근데 그것도 가끔 까먹고....못먹을때가 많아요ㅎㅎ",
          "original_form": "근데 그것도 가끔 까먹고....못먹을때가 많아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100024463.1.4",
          "form": "종비 좋죠.. 매일 잘 챙겨먹어야 효과 좋대요",
          "original_form": "종비 좋죠.. 매일 잘 챙겨먹어야 효과 좋대요",
          "speaker_id": "2",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100024463.1.5",
          "form": "전 비타민 씨 먹어요",
          "original_form": "전 비타민 씨 먹어요",
          "speaker_id": "2",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100024463.1.6",
          "form": "잘 챙겨드시나요?",
          "original_form": "잘 챙겨드시나요?",
          "speaker_id": "1",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100024463.1.7",
          "form": "전 자주 까먹어서",
          "original_form": "전 자주 까먹어서",
          "speaker_id": "1",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100024463.1.8",
          "form": "고민이에요..ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "고민이에요..ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100024463.1.9",
          "form": "매일 먹어요~ 전 챙겨먹는건 잘해요.. ㅎㅎ",
          "original_form": "매일 먹어요~ 전 챙겨먹는건 잘해요.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024463.1.10",
          "form": "근데 이거 다 먹으면 종바 사먹으려구요",
          "original_form": "근데 이거 다 먹으면 종바 사먹으려구요",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024463.1.11",
          "form": "한번에 있는게 좋아요",
          "original_form": "한번에 있는게 좋아요",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024463.1.12",
          "form": "그쵸ㅎㅎ",
          "original_form": "그쵸ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024463.1.13",
          "form": "다른 건강에 좋은거 또 하시는거 있나요?",
          "original_form": "다른 건강에 좋은거 또 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024463.1.14",
          "form": "전 얼마전에 야식 끊었어요ㅎㅋㅋㅋ",
          "original_form": "전 얼마전에 야식 끊었어요ㅎㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100024463.1.15",
          "form": "야식을 엄청 많이 먹었었는데, 7시이후엔 아무것도 안먹고 있습니다ㅋㅋ",
          "original_form": "야식을 엄청 많이 먹었었는데, 7시이후엔 아무것도 안먹고 있습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100024463.1.16",
          "form": "야식.. 진짜.. 끊기 어렵지 않나요? 같은 음식도 밤에 먹으면 왜그리 맛있는지",
          "original_form": "야식.. 진짜.. 끊기 어렵지 않나요? 같은 음식도 밤에 먹으면 왜그리 맛있는지",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100024463.1.17",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100024463.1.18",
          "form": "근데 역류성 식도염이 왔어서",
          "original_form": "근데 역류성 식도염이 왔어서",
          "speaker_id": "1",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100024463.1.19",
          "form": "어쩔수없이....ㅎㅎ 끊었습니닷",
          "original_form": "어쩔수없이....ㅎㅎ 끊었습니닷",
          "speaker_id": "1",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100024463.1.20",
          "form": "아.. 그거 저 한번 겪었어요.. 괴롭더만요",
          "original_form": "아.. 그거 저 한번 겪었어요.. 괴롭더만요",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100024463.1.21",
          "form": "지금은 괜찮으세요?",
          "original_form": "지금은 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100024463.1.22",
          "form": "음 가끔 쓰린것 빼고는",
          "original_form": "음 가끔 쓰린것 빼고는",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024463.1.23",
          "form": "괜찮아요!",
          "original_form": "괜찮아요!",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024463.1.24",
          "form": "name2님은 괜찮으세요?",
          "original_form": "&name2&님은 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024463.1.25",
          "form": "이게 스트레스받으면 더 심해지다고 하더라구요ㅠ",
          "original_form": "이게 스트레스받으면 더 심해지다고 하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024463.1.26",
          "form": "전 지금은 괜찮아요.. 약 2주분 주던데.. 그거 먹고 싹 나았어요",
          "original_form": "전 지금은 괜찮아요.. 약 2주분 주던데.. 그거 먹고 싹 나았어요",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100024463.1.27",
          "form": "ㅜㅜㅜㅠ저도 약먹다가 지금은 안먹는데",
          "original_form": "ㅜㅜㅜㅠ저도 약먹다가 지금은 안먹는데",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024463.1.28",
          "form": "정말 조심해야겠어요ㅜㅠ",
          "original_form": "정말 조심해야겠어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024463.1.29",
