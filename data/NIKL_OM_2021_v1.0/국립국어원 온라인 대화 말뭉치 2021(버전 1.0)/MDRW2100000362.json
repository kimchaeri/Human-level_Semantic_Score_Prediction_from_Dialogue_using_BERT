      "utterance": [
        {
          "id": "MDRW2100000362.1.1",
          "form": "혹시 대학생이신가요?",
          "original_form": "혹시 대학생이신가요?",
          "speaker_id": "1",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000362.1.2",
          "form": "졸업하고 취업준비 중이예요!",
          "original_form": "졸업하고 취업준비 중이예요!",
          "speaker_id": "2",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000362.1.3",
          "form": "그러시구낭",
          "original_form": "그러시구낭",
          "speaker_id": "1",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000362.1.4",
          "form": "아 혹시 당근마켓 자주 보세요??",
          "original_form": "아 혹시 당근마켓 자주 보세요??",
          "speaker_id": "1",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000362.1.5",
          "form": "네 당근마켓 자주 이용하고 있어요!",
          "original_form": "네 당근마켓 자주 이용하고 있어요!",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000362.1.6",
          "form": "저는 주로 파는 쪽인 것 같아요 ㅎㅎ",
          "original_form": "저는 주로 파는 쪽인 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000362.1.7",
          "form": "우와 전 팔아보려고 해도 막상 물건 정리하고 글 올리는게 귀찮아서 사기만 해요ㅋㅋㅋ",
          "original_form": "우와 전 팔아보려고 해도 막상 물건 정리하고 글 올리는게 귀찮아서 사기만 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000362.1.8",
          "form": "당근마켓에서 주로 구매하시는 편이세요? 아니면 판매하시는 편이세요?",
          "original_form": "당근마켓에서 주로 구매하시는 편이세요? 아니면 판매하시는 편이세요?",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000362.1.9",
          "form": "주로 뭐뭐 파세요?",
          "original_form": "주로 뭐뭐 파세요?",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000362.1.10",
          "form": "주로 구매요!!ㅎㅎ",
          "original_form": "주로 구매요!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000362.1.11",
          "form": "구매하시는 편이시구나!!",
          "original_form": "구매하시는 편이시구나!!",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000362.1.12",
          "form": "저는 화장품 판매 많이 해요",
          "original_form": "저는 화장품 판매 많이 해요",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000362.1.13",
          "form": "온라인에서 화장품 샀는데 안맞는 화장품들이요 ㅠㅠ",
          "original_form": "온라인에서 화장품 샀는데 안맞는 화장품들이요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000362.1.14",
          "form": "주로 어떤 것들 구매하세요?",
          "original_form": "주로 어떤 것들 구매하세요?",
          "speaker_id": "2",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000362.1.15",
          "form": "아하 하긴 화장품은 테스트 안하고 온라인으로 사면 안맞는게 너무 많은거 같아요ㅠㅠ",
          "original_form": "아하 하긴 화장품은 테스트 안하고 온라인으로 사면 안맞는게 너무 많은거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000362.1.16",
          "form": "전 되게 다양하게요ㅋㅋㅋ",
          "original_form": "전 되게 다양하게요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000362.1.17",
          "form": "옷이나 미용도구 이런것들요!!",
          "original_form": "옷이나 미용도구 이런것들요!!",
          "speaker_id": "1",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000362.1.18",
          "form": "옷 구매하시는 팁 있나요?",
          "original_form": "옷 구매하시는 팁 있나요?",
          "speaker_id": "2",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000362.1.19",
          "form": "괜찮아 보이는 옷들이 가끔 있는데 막상 사려고 하면 망설여지더라구요",
          "original_form": "괜찮아 보이는 옷들이 가끔 있는데 막상 사려고 하면 망설여지더라구요",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000362.1.20",
          "form": "요즘에는 옷 실사를 안 올리고 쇼핑몰 사진을 그대로 가져다쓰는 경우가 많아서",
          "original_form": "요즘에는 옷 실사를 안 올리고 쇼핑몰 사진을 그대로 가져다쓰는 경우가 많아서",
          "speaker_id": "1",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000362.1.21",
          "form": "그럴땐 그 쇼핑몰 찾아서 올라온 사진들 꼼꼼히 보는 편이에요!",
          "original_form": "그럴땐 그 쇼핑몰 찾아서 올라온 사진들 꼼꼼히 보는 편이에요!",
          "speaker_id": "1",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000362.1.22",
          "form": "옷도 온라인으로 많이 사서 막상 왔는데 안맞으면 당근에 거래 많이 하나보네용!",
          "original_form": "옷도 온라인으로 많이 사서 막상 왔는데 안맞으면 당근에 거래 많이 하나보네용!",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000362.1.23",
          "form": "맞아요ㅋㅋㅋ 환불하면 배송비가 더 들어서",
          "original_form": "맞아요ㅋㅋㅋ 환불하면 배송비가 더 들어서",
          "speaker_id": "1",
          "time": "20210819 15:52"
        }
      ]
    }
