      "utterance": [
        {
          "id": "MDRW2100006186.1.1",
          "form": "요리도 하시나요?",
          "original_form": "요리도 하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006186.1.2",
          "form": "가끔해요 ㅋㅋㅋ",
          "original_form": "가끔해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006186.1.3",
          "form": "주방은 엄마꺼라서",
          "original_form": "주방은 엄마꺼라서",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006186.1.4",
          "form": "그럼 어떤 가사일 하시나요",
          "original_form": "그럼 어떤 가사일 하시나요",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006186.1.5",
          "form": "전 청소?",
          "original_form": "전 청소?",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006186.1.6",
          "form": "음쓰 버리기",
          "original_form": "음쓰 버리기",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006186.1.7",
          "form": "재활용버리기",
          "original_form": "재활용버리기",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006186.1.8",
          "form": "그런것들....",
          "original_form": "그런것들....",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006186.1.9",
          "form": "많이하시네요 음쓰만 버려주셔도 어머님이 한결 수월하시겠어요",
          "original_form": "많이하시네요 음쓰만 버려주셔도 어머님이 한결 수월하시겠어요",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006186.1.10",
          "form": "음쓰 버리러 나가는것도 일이에ㅛ",
          "original_form": "음쓰 버리러 나가는것도 일이에ㅛ",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006186.1.11",
          "form": "그래서 음식물처리기 만족도가 높다고 하더라구요",
          "original_form": "그래서 음식물처리기 만족도가 높다고 하더라구요",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006186.1.12",
          "form": "그거 말려서 가루처럼 해주는거요?",
          "original_form": "그거 말려서 가루처럼 해주는거요?",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100006186.1.13",
          "form": "네 따로 버릴필요없이요",
          "original_form": "네 따로 버릴필요없이요",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006186.1.14",
          "form": "가지고계세요?",
          "original_form": "가지고계세요?",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006186.1.15",
          "form": "아니요...요즘은 이사가면 많이들 하더라구요",
          "original_form": "아니요...요즘은 이사가면 많이들 하더라구요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006186.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006186.1.17",
          "form": "요즘 또 필수품이군요",
          "original_form": "요즘 또 필수품이군요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006186.1.18",
          "form": "필수품이 늘어나네요",
          "original_form": "필수품이 늘어나네요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        }
      ]
