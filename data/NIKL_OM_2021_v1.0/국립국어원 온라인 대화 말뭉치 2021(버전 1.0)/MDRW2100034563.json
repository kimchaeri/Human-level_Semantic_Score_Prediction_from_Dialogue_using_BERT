      "utterance": [
        {
          "id": "MDRW2100034563.1.1",
          "form": "잠깐 나왔는데 날이 맑아요",
          "original_form": "잠깐 나왔는데 날이 맑아요",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034563.1.2",
          "form": "아직 살짝 덥고",
          "original_form": "아직 살짝 덥고",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034563.1.3",
          "form": "아그래요??",
          "original_form": "아그래요??",
          "speaker_id": "2",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100034563.1.4",
          "form": "오늘은 안나가서 몰랐는데 글쿤요 ㅋㅋㅋ",
          "original_form": "오늘은 안나가서 몰랐는데 글쿤요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100034563.1.5",
          "form": "저녁되면 시원해지겠죠?",
          "original_form": "저녁되면 시원해지겠죠?",
          "speaker_id": "2",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100034563.1.6",
          "form": "네 저녁엔 선선한 바람이 부니까",
          "original_form": "네 저녁엔 선선한 바람이 부니까",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034563.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100034563.1.8",
          "form": "시원한 저녁바람 맞아면 좋더라구요",
          "original_form": "시원한 저녁바람 맞아면 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100034563.1.9",
          "form": "네 요즘 딱 걷기좋은 온도예요",
          "original_form": "네 요즘 딱 걷기좋은 온도예요",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100034563.1.10",
          "form": "전 밤바람이 그리 좋더라구요",
          "original_form": "전 밤바람이 그리 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034563.1.11",
          "form": "걸으면 건강에도 좋죠",
          "original_form": "걸으면 건강에도 좋죠",
          "speaker_id": "2",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034563.1.12",
          "form": "어느새 계절이 바뀌는 때가 됐네요",
          "original_form": "어느새 계절이 바뀌는 때가 됐네요",
          "speaker_id": "1",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034563.1.13",
          "form": "그러게요 지금이 간절기같아요",
          "original_form": "그러게요 지금이 간절기같아요",
          "speaker_id": "2",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034563.1.14",
          "form": "네 그래서 옷차림도 어떻게 해안하나 싶어요",
          "original_form": "네 그래서 옷차림도 어떻게 해안하나 싶어요",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034563.1.15",
          "form": "해야하나",
          "original_form": "해야하나",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034563.1.16",
          "form": "간절기가 제일 애매하죠",
          "original_form": "간절기가 제일 애매하죠",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034563.1.17",
          "form": "긴팔도 반팔도..",
          "original_form": "긴팔도 반팔도..",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034563.1.18",
          "form": "네 가디건 하나 정도 챙겨다녀요",
          "original_form": "네 가디건 하나 정도 챙겨다녀요",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034563.1.19",
          "form": "그럼 딱 좋죠!",
          "original_form": "그럼 딱 좋죠!",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034563.1.20",
          "form": "전 번거로워서 못 챙기겠더라구여 ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "전 번거로워서 못 챙기겠더라구여 ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034563.1.21",
          "form": "가볍게 걸칠만한 옷을 챙겨야해요",
          "original_form": "가볍게 걸칠만한 옷을 챙겨야해요",
          "speaker_id": "1",
          "time": "20210828 16:04"
        }
      ]
