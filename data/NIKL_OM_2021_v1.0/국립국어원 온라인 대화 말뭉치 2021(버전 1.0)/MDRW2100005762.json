        },
        {
          "id": "MDRW2100005762.1.2",
          "form": "네 안녕하세요 ~",
          "original_form": "네 안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210907 11:14"
        },
        {
          "id": "MDRW2100005762.1.3",
          "form": "하시는모임있으세요?",
          "original_form": "하시는모임있으세요?",
          "speaker_id": "1",
          "time": "20210907 11:15"
        },
        {
          "id": "MDRW2100005762.1.4",
          "form": "아니요ㅋㅋ",
          "original_form": "아니요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:15"
        },
        {
          "id": "MDRW2100005762.1.5",
          "form": "들게된다면",
          "original_form": "들게된다면",
          "speaker_id": "2",
          "time": "20210907 11:16"
        },
        {
          "id": "MDRW2100005762.1.6",
          "form": "저는 복싱을 좋아해서",
          "original_form": "저는 복싱을 좋아해서",
          "speaker_id": "2",
          "time": "20210907 11:16"
        },
        {
          "id": "MDRW2100005762.1.7",
          "form": "복싱동호회를 들고싶네요",
          "original_form": "복싱동호회를 들고싶네요",
          "speaker_id": "2",
          "time": "20210907 11:16"
        },
        {
          "id": "MDRW2100005762.1.8",
          "form": "오. 복싱멋있네요",
          "original_form": "오. 복싱멋있네요",
          "speaker_id": "1",
          "time": "20210907 11:16"
        },
        {
          "id": "MDRW2100005762.1.9",
          "form": "94님은요? 모임이 있으신가요",
          "original_form": "94님은요? 모임이 있으신가요",
          "speaker_id": "2",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100005762.1.10",
          "form": "음대학때봉사동아리했었어요",
          "original_form": "음대학때봉사동아리했었어요",
          "speaker_id": "1",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100005762.1.11",
          "form": "오 대단하시네요 !",
          "original_form": "오 대단하시네요 !",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100005762.1.12",
          "form": "봉사동아리는 활동을 얼마나 하셨나요?",
          "original_form": "봉사동아리는 활동을 얼마나 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100005762.1.13",
          "form": "ㅎㅎ6년했어요!",
          "original_form": "ㅎㅎ6년했어요!",
          "speaker_id": "1",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100005762.1.14",
          "form": "재밌었어요 ㅎㅎ",
          "original_form": "재밌었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:20"
        },
        {
          "id": "MDRW2100005762.1.15",
          "form": "와 6년이나요? 대단하시네요",
          "original_form": "와 6년이나요? 대단하시네요",
          "speaker_id": "2",
          "time": "20210907 11:20"
        },
        {
          "id": "MDRW2100005762.1.16",
          "form": "주로 무슨활동을 하신건가요",
          "original_form": "주로 무슨활동을 하신건가요",
          "speaker_id": "2",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100005762.1.17",
          "form": "음 의료봉사동아리라가볍게했어요ㅎㅎ",
          "original_form": "음 의료봉사동아리라가볍게했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100005762.1.18",
          "form": "아ㅎㅎ 의료봉사라니 아무나 할수있는게 아니군요",
          "original_form": "아ㅎㅎ 의료봉사라니 아무나 할수있는게 아니군요",
