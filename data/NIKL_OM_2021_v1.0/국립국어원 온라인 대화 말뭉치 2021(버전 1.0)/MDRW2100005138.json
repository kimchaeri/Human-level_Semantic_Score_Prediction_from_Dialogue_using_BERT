      "utterance": [
        {
          "id": "MDRW2100005138.1.1",
          "form": "자 한식 일식 중식 등등 뭐가 제일좋으세요?",
          "original_form": "자 한식 일식 중식 등등 뭐가 제일좋으세요?",
          "speaker_id": "1",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005138.1.2",
          "form": "앗 너무 티났나요ㅋㅋㅋㅋ  먹는 걸 낙으로 삼고 있어서ㅠ",
          "original_form": "앗 너무 티났나요ㅋㅋㅋㅋ  먹는 걸 낙으로 삼고 있어서ㅠ",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005138.1.3",
          "form": "한식입니다!",
          "original_form": "한식입니다!",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005138.1.4",
          "form": "name1님은요???",
          "original_form": "&name1&님은요???",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005138.1.5",
          "form": "대부분 좋아하지먀 저는 일식이요",
          "original_form": "대부분 좋아하지먀 저는 일식이요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005138.1.6",
          "form": "음식이야기할때 신나시는거 같아서ㅋㅋㅋ",
          "original_form": "음식이야기할때 신나시는거 같아서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005138.1.7",
          "form": "저는 해산물을 좋아해서 일식으로 먹으면 주로 만족스러워요",
          "original_form": "저는 해산물을 좋아해서 일식으로 먹으면 주로 만족스러워요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005138.1.8",
          "form": "한식중에서 유독 맛있는건 뭐예요?",
          "original_form": "한식중에서 유독 맛있는건 뭐예요?",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005138.1.9",
          "form": "저는 갈비탕이요!!",
          "original_form": "저는 갈비탕이요!!",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005138.1.10",
          "form": "말고기로 만든 갈비탕이 진짜 좋았거든요",
          "original_form": "말고기로 만든 갈비탕이 진짜 좋았거든요",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005138.1.11",
          "form": "헉 그말고기가 여기서 등장하는군요",
          "original_form": "헉 그말고기가 여기서 등장하는군요",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005138.1.12",
          "form": "갈비탕이라니 너무 좋다",
          "original_form": "갈비탕이라니 너무 좋다",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005138.1.13",
          "form": "맞습니다ㅠ 코스요리도 있으니 코스 꼭꼭 추천 드려요",
          "original_form": "맞습니다ㅠ 코스요리도 있으니 코스 꼭꼭 추천 드려요",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005138.1.14",
          "form": "일식 입문으로는 역시 회가 무난하겠죠?",
          "original_form": "일식 입문으로는 역시 회가 무난하겠죠?",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005138.1.15",
          "form": "코스요리 언젠가 꼭 먹어볼께요",
          "original_form": "코스요리 언젠가 꼭 먹어볼께요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005138.1.16",
          "form": "회도좋구요 초밥도 무난하고",
          "original_form": "회도좋구요 초밥도 무난하고",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005138.1.17",
          "form": "아무래도 접하기 쉽기로는 돈까스나 라멘 메밀국수같은것도 있지요",
          "original_form": "아무래도 접하기 쉽기로는 돈까스나 라멘 메밀국수같은것도 있지요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005138.1.18",
          "form": "아 라멘 메밀도 있구나 너무 해산물에만 집착했네요",
          "original_form": "아 라멘 메밀도 있구나 너무 해산물에만 집착했네요",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005138.1.19",
          "form": "저 사실 일식도 좋아하나 봐요",
          "original_form": "저 사실 일식도 좋아하나 봐요",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005138.1.20",
          "form": "저도 한식 중식 다 좋아해요",
          "original_form": "저도 한식 중식 다 좋아해요",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005138.1.21",
          "form": "한끗차이죠",
          "original_form": "한끗차이죠",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005138.1.22",
          "form": "오늘은 뭐드셨나요?",
          "original_form": "오늘은 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005138.1.23",
          "form": "돼지김치찜 먹었어요!",
          "original_form": "돼지김치찜 먹었어요!",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005138.1.24",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005138.1.25",
          "form": "저는 떡갈비 냉동식품 먹었어요 자취중이라 간단하게!",
          "original_form": "저는 떡갈비 냉동식품 먹었어요 자취중이라 간단하게!",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005138.1.26",
          "form": "앗 저도 자취! 자취하면 챙겨 먹기 귀찮죠ㅠ",
          "original_form": "앗 저도 자취! 자취하면 챙겨 먹기 귀찮죠ㅠ",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005138.1.27",
          "form": "과일 같은 거 좋아하시나요??",
          "original_form": "과일 같은 거 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005138.1.28",
          "form": "챙겨먹기 귀찮기도하구 재료관리가 힘들어서 장보고 시간이지나면 좀 대충먹게돼요",
          "original_form": "챙겨먹기 귀찮기도하구 재료관리가 힘들어서 장보고 시간이지나면 좀 대충먹게돼요",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005138.1.29",
          "form": "과일 좋죠! 비싸고 관리힘들지만 매번사먹고있어요",
          "original_form": "과일 좋죠! 비싸고 관리힘들지만 매번사먹고있어요",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005138.1.30",
          "form": "과일좋아하세요?",
          "original_form": "과일좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005138.1.31",
          "form": "맞아요ㅠㅠ 자취인들에게는 재료 가성비가 별로기도 하고요",
          "original_form": "맞아요ㅠㅠ 자취인들에게는 재료 가성비가 별로기도 하고요",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005138.1.32",
          "form": "앗 깎는 것도 어려워서... 냉동 망고 같은 것만 간간이 챙겨 먹고 있어요",
          "original_form": "앗 깎는 것도 어려워서... 냉동 망고 같은 것만 간간이 챙겨 먹고 있어요",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005138.1.33",
          "form": "과일이 비싸기도 하고요... 잘 안 먹게 되더라고요",
          "original_form": "과일이 비싸기도 하고요... 잘 안 먹게 되더라고요",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005138.1.34",
          "form": "맞아요 저는 같이사는 친구가 과일을 좋아해서 먹다버릇하니까 저도 좋아하게되었어요",
          "original_form": "맞아요 저는 같이사는 친구가 과일을 좋아해서 먹다버릇하니까 저도 좋아하게되었어요",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005138.1.35",
          "form": "뒷처리와 관리가 어렵긴하지만요",
          "original_form": "뒷처리와 관리가 어렵긴하지만요",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005138.1.36",
          "form": "썩혀버린 과일도 많아요",
          "original_form": "썩혀버린 과일도 많아요",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005138.1.37",
          "form": "헉 룸메분과 선순환이시네요",
          "original_form": "헉 룸메분과 선순환이시네요",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005138.1.38",
          "form": "부러워요ㅠㅠ 저는 살짝 늦바람식으로 엄청 시켜 먹게 되더라고요",
          "original_form": "부러워요ㅠㅠ 저는 살짝 늦바람식으로 엄청 시켜 먹게 되더라고요",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005138.1.39",
          "form": "name1님 젤 좋아하는 과일은 어떤 과일인가요?",
          "original_form": "&name1&님 젤 좋아하는 과일은 어떤 과일인가요?",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005138.1.40",
          "form": "저는 요즘에는 복숭아 좋아해요",
          "original_form": "저는 요즘에는 복숭아 좋아해요",
          "speaker_id": "1",
