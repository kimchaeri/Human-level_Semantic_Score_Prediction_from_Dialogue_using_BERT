      "utterance": [
        {
          "id": "MDRW2100043653.1.1",
          "form": "간장이 들가는 집도 있우느ㅡㅅ",
          "original_form": "간장이 들가는 집도 있우느ㅡㅅ",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043653.1.2",
          "form": "간장이 들어가는 돼지요리..?",
          "original_form": "간장이 들어가는 돼지요리..?",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043653.1.3",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043653.1.4",
          "form": "뼈가 붙은 부위를 쓰는 돼지고기요리야?",
          "original_form": "뼈가 붙은 부위를 쓰는 돼지고기요리야?",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043653.1.5",
          "form": "아니 오돌뼈는 있을지도",
          "original_form": "아니 오돌뼈는 있을지도",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043653.1.6",
          "form": "뼈가 없는 요리란 말이지...",
          "original_form": "뼈가 없는 요리란 말이지...",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043653.1.7",
          "form": "구운 요리야?",
          "original_form": "구운 요리야?",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043653.1.8",
          "form": "아니 삶은요리야",
          "original_form": "아니 삶은요리야",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043653.1.9",
          "form": "튀긴 요리 아니고?",
          "original_form": "튀긴 요리 아니고?",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043653.1.10",
          "form": "응 튀긴요리아니야",
          "original_form": "응 튀긴요리아니야",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043653.1.11",
          "form": "돼지를 삶는다고... 간장넣기도 하고?",
          "original_form": "돼지를 삶는다고... 간장넣기도 하고?",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043653.1.12",
          "form": "정답! 장조림!",
          "original_form": "정답! 장조림!",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043653.1.13",
          "form": "아니야!! 장조림",
          "original_form": "아니야!! 장조림",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043653.1.14",
          "form": "간장을 넣는 집도 있겠지만 대다수는 안넣을걸..?",
          "original_form": "간장을 넣는 집도 있겠지만 대다수는 안넣을걸..?",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043653.1.15",
          "form": "그럼 돼지고기 무슨 부위를 주로 써?",
          "original_form": "그럼 돼지고기 무슨 부위를 주로 써?",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043653.1.16",
          "form": "돼지고기 삼겹살부분 넣었어 이번에",
          "original_form": "돼지고기 삼겹살부분 넣었어 이번에",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043653.1.17",
          "form": "삼겹살...",
          "original_form": "삼겹살...",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043653.1.18",
          "form": "야채를 곁들여 먹어?",
          "original_form": "야채를 곁들여 먹어?",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043653.1.19",
          "form": "삼겹살 맛있지않냐",
          "original_form": "삼겹살 맛있지않냐",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043653.1.20",
          "form": "응 곁들여서 많이 먹어",
          "original_form": "응 곁들여서 많이 먹어",
          "speaker_id": "2",
          "time": "20210907 19:58"
        }
      ]
