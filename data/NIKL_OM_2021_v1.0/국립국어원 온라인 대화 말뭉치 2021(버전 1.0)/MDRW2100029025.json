      "utterance": [
        {
          "id": "MDRW2100029025.1.1",
          "form": "응 용량 엄청큰거",
          "original_form": "응 용량 엄청큰거",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.2",
          "form": "용량 쭐엇다구 뭐라하지 마 ㅎㅎ",
          "original_form": "용량 쭐엇다구 뭐라하지 마 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.3",
          "form": "허락은받았어?",
          "original_form": "허락은받았어?",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.4",
          "form": "안받았겠지",
          "original_form": "안받았겠지",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.5",
          "form": "당연히 받앗지",
          "original_form": "당연히 받앗지",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.6",
          "form": "뭔게임인지 알어?",
          "original_form": "뭔게임인지 알어?",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.7",
          "form": "웬일이래 ㅎㅎ",
          "original_form": "웬일이래 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.8",
          "form": "알아",
          "original_form": "알아",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.9",
          "form": "사실 몰라",
          "original_form": "사실 몰라",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.10",
          "form": "말 해봐",
          "original_form": "말 해봐",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.11",
          "form": "어쌔신크리드라구..",
          "original_form": "어쌔신크리드라구..",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.12",
          "form": "빨리!!",
          "original_form": "빨리!!",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.13",
          "form": "유명한 씨리즈야",
          "original_form": "유명한 씨리즈야",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029025.1.14",
          "form": "그렇구나!!",
          "original_form": "그렇구나!!",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.15",
          "form": "뭐하는 게임인데?",
          "original_form": "뭐하는 게임인데?",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.16",
          "form": "실제 도시의 과거 배경이야",
          "original_form": "실제 도시의 과거 배경이야",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.17",
          "form": "그래서 역사를 체험하는거지...",
          "original_form": "그래서 역사를 체험하는거지...",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.18",
          "form": "그렇구나.!!!!!",
          "original_form": "그렇구나.!!!!!",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.19",
          "form": "유익하지 ㅋㅋ",
          "original_form": "유익하지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.20",
          "form": "재밌겠다 ㅎㅎ 나도 하고 싶어",
          "original_form": "재밌겠다 ㅎㅎ 나도 하고 싶어",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.21",
          "form": "그렇구나!!",
          "original_form": "그렇구나!!",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.22",
          "form": "너두 하게 해줄게 ㅎㅎ",
          "original_form": "너두 하게 해줄게 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.23",
          "form": "게임 장르가 어떻게돼!",
          "original_form": "게임 장르가 어떻게돼!",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.24",
          "form": "와!",
          "original_form": "와!",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.25",
          "form": "게임은 같이할수록 재밋으니깐",
          "original_form": "게임은 같이할수록 재밋으니깐",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.26",
          "form": "어떻게돼?!?",
          "original_form": "어떻게돼?!?",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.27",
          "form": "맞아!!",
          "original_form": "맞아!!",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.28",
          "form": "장르는 잘 모르겟어 ㅋ",
          "original_form": "장르는 잘 모르겟어 ㅋ",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.29",
          "form": "rpg겠지?",
          "original_form": "rpg겠지?",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029025.1.30",
          "form": "나도 아직 그렇게 많이 해보진 않았어 ㅎㅎ",
          "original_form": "나도 아직 그렇게 많이 해보진 않았어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029025.1.31",
          "form": "렇군 ㅎㅎ",
          "original_form": "렇군 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029025.1.32",
          "form": "rpg도 섞여있는 것 같애",
          "original_form": "rpg도 섞여있는 것 같애",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029025.1.33",
          "form": "재밌는 게임 없어?",
