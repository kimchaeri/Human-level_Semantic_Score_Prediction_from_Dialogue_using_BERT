      "utterance": [
        {
          "id": "MDRW2100007240.1.1",
          "form": "공연보니?",
          "original_form": "공연보니?",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007240.1.2",
          "form": "콘서트?",
          "original_form": "콘서트?",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007240.1.3",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007240.1.4",
          "form": "ㅋㅋㅋㅋㅋ콘서트 보고싶어서 운다",
          "original_form": "ㅋㅋㅋㅋㅋ콘서트 보고싶어서 운다",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007240.1.5",
          "form": "킄킄킄 오늘 브이앱도 애들 본거 아니냐고",
          "original_form": "킄킄킄 오늘 브이앱도 애들 본거 아니냐고",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007240.1.6",
          "form": "나는 태연이 처음이자 마지막이였는데",
          "original_form": "나는 태연이 처음이자 마지막이였는데",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007240.1.7",
          "form": "처음 그 문화 잊지 못해",
          "original_form": "처음 그 문화 잊지 못해",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007240.1.8",
          "form": "친구에게 감사를",
          "original_form": "친구에게 감사를",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007240.1.9",
          "form": "ㅋㅋㅋㅋ애들 브이앱 아니면 볼일없죠",
          "original_form": "ㅋㅋㅋㅋ애들 브이앱 아니면 볼일없죠",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007240.1.10",
          "form": "태연콘 진짜 아련...",
          "original_form": "태연콘 진짜 아련...",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007240.1.11",
          "form": "그렇게 흔들어 재껴야 하다니",
          "original_form": "그렇게 흔들어 재껴야 하다니",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007240.1.12",
          "form": "ㅋㅋㅋㅋㅋ코로나때문에 애들도 못보고",
          "original_form": "ㅋㅋㅋㅋㅋ코로나때문에 애들도 못보고",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007240.1.13",
          "form": "콘서트 언제 가나",
          "original_form": "콘서트 언제 가나",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007240.1.14",
          "form": "난 아이유 콘서트 가보고 싶어",
          "original_form": "난 아이유 콘서트 가보고 싶어",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007240.1.15",
          "form": "진짜 그렇게 잘부를까?",
          "original_form": "진짜 그렇게 잘부를까?",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007240.1.16",
          "form": "아이유는 뭐 오지겠지",
          "original_form": "아이유는 뭐 오지겠지",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007240.1.17",
          "form": "꼭.... 볼거야",
          "original_form": "꼭.... 볼거야",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007240.1.18",
          "form": "하지민 메인자리 아니면 안볼래",
          "original_form": "하지민 메인자리 아니면 안볼래",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007240.1.19",
          "form": "그럴게 못보고~",
          "original_form": "그럴게 못보고~",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007240.1.20",
          "form": "ㅋㅋㅋㅋ언니 아이유는 덕후들과 대중들과 티켓팅하는거야",
          "original_form": "ㅋㅋㅋㅋ언니 아이유는 덕후들과 대중들과 티켓팅하는거야",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007240.1.21",
          "form": "메인?ㅋㅋㅋ꿈이 크시네요",
          "original_form": "메인?ㅋㅋㅋ꿈이 크시네요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007240.1.22",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007240.1.23",
          "form": "수강신청도 올킬할거 같아 그분들은",
          "original_form": "수강신청도 올킬할거 같아 그분들은",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007240.1.24",
          "form": "티켓팅할때 용병써야함",
          "original_form": "티켓팅할때 용병써야함",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007240.1.25",
          "form": "첫콘 중콘 막콘 다 잡아야하니까",
          "original_form": "첫콘 중콘 막콘 다 잡아야하니까",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007240.1.26",
          "form": "신기한 문화다",
          "original_form": "신기한 문화다",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.27",
          "form": "콘서트 얼마나 가봤어?",
          "original_form": "콘서트 얼마나 가봤어?",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.28",
          "form": "한 4번?",
          "original_form": "한 4번?",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.29",
          "form": "너는 가본적있니?",
          "original_form": "너는 가본적있니?",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.30",
          "form": "와 베테랑",
          "original_form": "와 베테랑",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.31",
          "form": "나는 한번이 다야",
          "original_form": "나는 한번이 다야",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.32",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.33",
          "form": "전시화는 가봤니",
          "original_form": "전시화는 가봤니",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.34",
          "form": "그럼 전시회좋아해",
          "original_form": "그럼 전시회좋아해",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.35",
          "form": "전시회",
          "original_form": "전시회",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.36",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007240.1.37",
          "form": "나도 좋아해",
          "original_form": "나도 좋아해",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007240.1.38",
          "form": "미술 전시회 좋아해",
          "original_form": "미술 전시회 좋아해",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007240.1.39",
          "form": "최근엔 피카소 봤어",
          "original_form": "최근엔 피카소 봤어",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007240.1.40",
          "form": "ㅋㅋ뮤지컬은 본적있니?",
          "original_form": "ㅋㅋ뮤지컬은 본적있니?",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007240.1.41",
          "form": "고등학교때 딱 한번",
          "original_form": "고등학교때 딱 한번",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007240.1.42",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007240.1.43",
          "form": "나는 2년전에 마리 앙투아네트 봤어",
          "original_form": "나는 2년전에 마리 앙투아네트 봤어",
