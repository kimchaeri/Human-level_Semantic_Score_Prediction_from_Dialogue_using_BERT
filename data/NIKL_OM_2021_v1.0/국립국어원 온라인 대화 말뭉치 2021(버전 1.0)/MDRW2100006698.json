      "utterance": [
        {
          "id": "MDRW2100006698.1.1",
          "form": "그 남성복회사?",
          "original_form": "그 남성복회사?",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100006698.1.2",
          "form": "응 비록 파견직이였지만 차장님 덕분에 잘 다녔지",
          "original_form": "응 비록 파견직이였지만 차장님 덕분에 잘 다녔지",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100006698.1.3",
          "form": "거기 차장님이 너 잘 챙겨 주셨지",
          "original_form": "거기 차장님이 너 잘 챙겨 주셨지",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100006698.1.4",
          "form": "내가 본인 일처리 다 도맡아해줬으니까",
          "original_form": "내가 본인 일처리 다 도맡아해줬으니까",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100006698.1.5",
          "form": "그래도 부서 회식할때 꼬박꼬박 데려가고",
          "original_form": "그래도 부서 회식할때 꼬박꼬박 데려가고",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100006698.1.6",
          "form": "점심 비싼거사주고",
          "original_form": "점심 비싼거사주고",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100006698.1.7",
          "form": "아침마다 커피사주고 좋으셨지",
          "original_form": "아침마다 커피사주고 좋으셨지",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100006698.1.8",
          "form": "ㅋㅋㅋ그렇긴했지 그래도 월급 나오고 나서부터는 내가 모닝커피 쐈다규ㅋㅋ",
          "original_form": "ㅋㅋㅋ그렇긴했지 그래도 월급 나오고 나서부터는 내가 모닝커피 쐈다규ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100006698.1.9",
          "form": "잘했네ㅋㅋ 얻어먹기만하면 안돼",
          "original_form": "잘했네ㅋㅋ 얻어먹기만하면 안돼",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100006698.1.10",
          "form": "당근이지!!  그래도 우리 차장님은 연장근무하면 그대로 다 적으라고했거든",
          "original_form": "당근이지!!  그래도 우리 차장님은 연장근무하면 그대로 다 적으라고했거든",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100006698.1.11",
          "form": "근데 아동복 수석님은 칼같이 6시로 하라고 하더라ㅋㅋ",
          "original_form": "근데 아동복 수석님은 칼같이 6시로 하라고 하더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100006698.1.12",
          "form": "차장님이 너 생각해준거지ㅋㅋ",
          "original_form": "차장님이 너 생각해준거지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100006698.1.13",
          "form": "아동복은 어떻게 알았어?",
          "original_form": "아동복은 어떻게 알았어?",
          "speaker_id": "2",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100006698.1.14",
          "form": "아동복에 후배 소개시켜줬거든ㅋㅋ",
          "original_form": "아동복에 후배 소개시켜줬거든ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100006698.1.15",
          "form": "나보다 일찍 출근하고 늦게 퇴근하는데도... 얄짤없었어ㅜ",
          "original_form": "나보다 일찍 출근하고 늦게 퇴근하는데도... 얄짤없었어ㅜ",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006698.1.16",
          "form": "너는 차장님 만난게 행운이였네",
          "original_form": "너는 차장님 만난게 행운이였네",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006698.1.17",
          "form": "맞아ㅋㅋ근데 요즘 회사 계속 다니시나몰라",
          "original_form": "맞아ㅋㅋ근데 요즘 회사 계속 다니시나몰라",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006698.1.18",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006698.1.19",
          "form": "엄청 일하기 싫어하셨거든ㅋㅋㅋ",
          "original_form": "엄청 일하기 싫어하셨거든ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006698.1.20",
          "form": "ㅋㅋㅋ그래도 돈버실려면 하셔야지ㅋㅋ별수있으시겠어",
          "original_form": "ㅋㅋㅋ그래도 돈버실려면 하셔야지ㅋㅋ별수있으시겠어",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006698.1.21",
          "form": "그치ㅋㅋㅋ가끔 안부가 궁금해ㅋㅋ",
