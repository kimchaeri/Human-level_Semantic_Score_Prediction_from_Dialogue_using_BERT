      "utterance": [
        {
          "id": "MDRW2100035908.1.1",
          "form": "국내여행지로 좋아하는곳 있으세요?",
          "original_form": "국내여행지로 좋아하는곳 있으세요?",
          "speaker_id": "1",
          "time": "20210830 11:12"
        },
        {
          "id": "MDRW2100035908.1.2",
          "form": "안녕하세요~~!",
          "original_form": "안녕하세요~~!",
          "speaker_id": "2",
          "time": "20210830 11:13"
        },
        {
          "id": "MDRW2100035908.1.3",
          "form": "저는 여행을 자주 해보진 않았지만",
          "original_form": "저는 여행을 자주 해보진 않았지만",
          "speaker_id": "2",
          "time": "20210830 11:13"
        },
        {
          "id": "MDRW2100035908.1.4",
          "form": "제주도가 너무 가고싶네요!",
          "original_form": "제주도가 너무 가고싶네요!",
          "speaker_id": "2",
          "time": "20210830 11:13"
        },
        {
          "id": "MDRW2100035908.1.5",
          "form": "name1님은요~~?",
          "original_form": "&name1&님은요~~?",
          "speaker_id": "2",
          "time": "20210830 11:13"
        },
        {
          "id": "MDRW2100035908.1.6",
          "form": "저는 매년 이맘때쯤에 경주를 항상갔었거든요",
          "original_form": "저는 매년 이맘때쯤에 경주를 항상갔었거든요",
          "speaker_id": "1",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035908.1.7",
          "form": "2년째 못가고있어서 경주 가고싶 네요",
          "original_form": "2년째 못가고있어서 경주 가고싶 네요",
          "speaker_id": "1",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035908.1.8",
          "form": "제주도 좋죠 언제 가보셨어요?",
          "original_form": "제주도 좋죠 언제 가보셨어요?",
          "speaker_id": "1",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100035908.1.9",
          "form": "경주도 엄청 좋죠",
          "original_form": "경주도 엄청 좋죠",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100035908.1.10",
          "form": "안가봤지만,,",
          "original_form": "안가봤지만,,",
          "speaker_id": "2",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035908.1.11",
          "form": "저희 엄빠 신혼여행지가 경주였어서요,,",
          "original_form": "저희 엄빠 신혼여행지가 경주였어서요,,",
          "speaker_id": "2",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035908.1.12",
          "form": "제주도 다녀온지도 엄청 오래됐어요..",
          "original_form": "제주도 다녀온지도 엄청 오래됐어요..",
          "speaker_id": "2",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035908.1.13",
          "form": "한........ 10년 됐나..ㅎ",
          "original_form": "한........ 10년 됐나..ㅎ",
          "speaker_id": "2",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035908.1.14",
          "form": "헉...근데 저도요 10년되었어요ㅋㅋㅋㅋ",
          "original_form": "헉...근데 저도요 10년되었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035908.1.15",
          "form": "수학여행때가고 그 뒤로는 못갔어요",
          "original_form": "수학여행때가고 그 뒤로는 못갔어요",
          "speaker_id": "1",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035908.1.16",
          "form": "헉 저도",
          "original_form": "헉 저도",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100035908.1.17",
          "form": "마지막 제주도가 수학여행이었어요",
          "original_form": "마지막 제주도가 수학여행이었어요",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100035908.1.18",
          "form": "그 사이에 제주도에 몰라보게",
          "original_form": "그 사이에 제주도에 몰라보게",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100035908.1.19",
          "form": "좋은 데가 많아진 것 같더라구요?!",
          "original_form": "좋은 데가 많아진 것 같더라구요?!",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100035908.1.20",
          "form": "저도 너무 궁금해요 언제쯤 가볼수있을까 싶구요..ㅠ",
          "original_form": "저도 너무 궁금해요 언제쯤 가볼수있을까 싶구요..ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035908.1.21",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210830 12:11"
        },
        {
          "id": "MDRW2100035908.1.22",
          "form": "제주도는 이니스프리도 다르다던데...",
          "original_form": "제주도는 이니스프리도 다르다던데...",
          "speaker_id": "2",
          "time": "20210830 12:11"
        },
        {
          "id": "MDRW2100035908.1.23",
          "form": "카카오프렌즈도 다르대요.....",
          "original_form": "카카오프렌즈도 다르대요.....",
          "speaker_id": "2",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100035908.1.24",
          "form": "너무 궁금해요!!!",
          "original_form": "너무 궁금해요!!!",
          "speaker_id": "2",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100035908.1.25",
          "form": "아 너무 궁금해요!! 렌트하는게 좋다던데 운전하는 친구데려가야겠어요",
          "original_form": "아 너무 궁금해요!! 렌트하는게 좋다던데 운전하는 친구데려가야겠어요",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100035908.1.26",
          "form": "딱새우도 먹구싶구요",
          "original_form": "딱새우도 먹구싶구요",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100035908.1.27",
          "form": "우와 딱새우!!!!!",
          "original_form": "우와 딱새우!!!!!",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035908.1.28",
          "form": "제주도 가서 기념품도 쓸어오고싶고",
          "original_form": "제주도 가서 기념품도 쓸어오고싶고",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035908.1.29",
          "form": "예전에는... 국내여행이라면 별 감흥 없었는데",
          "original_form": "예전에는... 국내여행이라면 별 감흥 없었는데",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035908.1.30",
          "form": "이젠 정말 어디든 가고싶네요 ㅠㅠ",
          "original_form": "이젠 정말 어디든 가고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035908.1.31",
          "form": "그럼 해외여행은 어디어디 가보셨나요",
          "original_form": "그럼 해외여행은 어디어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100035908.1.32",
          "form": "저 해외여행 경험이 별로 없어요ㅠ",
          "original_form": "저 해외여행 경험이 별로 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035908.1.33",
          "form": "고등학생 때 수학여행으로 일본 한 번...?!",
          "original_form": "고등학생 때 수학여행으로 일본 한 번...?!",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035908.1.34",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035908.1.35",
          "form": "저는 일본하고 대만 몇번 갔었어요",
          "original_form": "저는 일본하고 대만 몇번 갔었어요",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100035908.1.36",
          "form": "앗 그럼 제주도는 중학교때 다녀오신건가요?",
          "original_form": "앗 그럼 제주도는 중학교때 다녀오신건가요?",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100035908.1.37",
          "form": "대만이랑 일본 몇번갔어요!",
          "original_form": "대만이랑 일본 몇번갔어요!",
          "speaker_id": "1",
          "time": "20210830 13:44"
        }
      ]
