      "utterance": [
        {
          "id": "MDRW2100018525.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018525.1.2",
          "form": "네. 근데 요즘은 영화관을 잘 못가요ㅠ",
          "original_form": "네. 근데 요즘은 영화관을 잘 못가요ㅠ",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018525.1.3",
          "form": "최근에 재밌게 본 영화 있으세요?",
          "original_form": "최근에 재밌게 본 영화 있으세요?",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018525.1.4",
          "form": "최근에는 본게 없네요ㅜㅜ",
          "original_form": "최근에는 본게 없네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 08:41"
        },
        {
          "id": "MDRW2100018525.1.5",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210827 08:41"
        },
        {
          "id": "MDRW2100018525.1.6",
          "form": "저도 본거는 없고 재밌다고 얘기 듣기만 해서",
          "original_form": "저도 본거는 없고 재밌다고 얘기 듣기만 해서",
          "speaker_id": "2",
          "time": "20210827 08:41"
        },
        {
          "id": "MDRW2100018525.1.7",
          "form": "크루엘라 풀리면 보려고 기다리고 있어요",
          "original_form": "크루엘라 풀리면 보려고 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018525.1.8",
          "form": "오! 저도요!",
          "original_form": "오! 저도요!",
          "speaker_id": "1",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018525.1.9",
          "form": "그거 보고싶더라구요",
          "original_form": "그거 보고싶더라구요",
          "speaker_id": "1",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018525.1.10",
          "form": "연기도 잘하고 내용도 괜찮다더라구요",
          "original_form": "연기도 잘하고 내용도 괜찮다더라구요",
          "speaker_id": "1",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018525.1.11",
          "form": "디즈니 영화는 어느정도 퀄이 보장되는거같아요 ㅋㅋㅋ",
          "original_form": "디즈니 영화는 어느정도 퀄이 보장되는거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018525.1.12",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:42"
        },
        {
          "id": "MDRW2100018525.1.13",
          "form": "처음 디즈니 했을때는 ?!?!?! 이런반응이였는데 말이죠ㅋㅋ",
          "original_form": "처음 디즈니 했을때는 ?!?!?! 이런반응이였는데 말이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:43"
        },
        {
          "id": "MDRW2100018525.1.14",
          "form": "그쵸 ㅋㅋㅋ 디즈니 실사화라고 해서 다들 기대 안했는데",
          "original_form": "그쵸 ㅋㅋㅋ 디즈니 실사화라고 해서 다들 기대 안했는데",
          "speaker_id": "2",
          "time": "20210827 08:43"
        },
        {
          "id": "MDRW2100018525.1.15",
          "form": "알라딘부터 다 대박나는거같아요",
          "original_form": "알라딘부터 다 대박나는거같아요",
          "speaker_id": "2",
          "time": "20210827 08:43"
        },
        {
          "id": "MDRW2100018525.1.16",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018525.1.17",
          "form": "퀄리티가 좋아서 놀랐어요",
          "original_form": "퀄리티가 좋아서 놀랐어요",
          "speaker_id": "1",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018525.1.18",
          "form": "노래도 좋고요!",
          "original_form": "노래도 좋고요!",
          "speaker_id": "1",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018525.1.19",
          "form": "그쵸 ㅋㅋㅋ 뮤지컬영화는 맘마미아 같은 것만 생각했는데 신선하더라구요",
          "original_form": "그쵸 ㅋㅋㅋ 뮤지컬영화는 맘마미아 같은 것만 생각했는데 신선하더라구요",
          "speaker_id": "2",
          "time": "20210827 08:45"
        },
        {
          "id": "MDRW2100018525.1.20",
          "form": "국내 영화 중에는 모가디슈 평이 괜찮은거같아서",
          "original_form": "국내 영화 중에는 모가디슈 평이 괜찮은거같아서",
          "speaker_id": "2",
          "time": "20210827 08:46"
        },
        {
          "id": "MDRW2100018525.1.21",
          "form": "나중에 풀리면 보려고 기다리고 있어요",
          "original_form": "나중에 풀리면 보려고 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210827 08:46"
        },
        {
          "id": "MDRW2100018525.1.22",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018525.1.23",
          "form": "오 그것도 궁금하긴 하더라구요ㅋㅋ",
          "original_form": "오 그것도 궁금하긴 하더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018525.1.24",
          "form": "빨리 풀림 좋겠어요ㅠ",
          "original_form": "빨리 풀림 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018525.1.25",
          "form": "그쵸 배우들이 다 유명해서",
          "original_form": "그쵸 배우들이 다 유명해서",
          "speaker_id": "2",
          "time": "20210827 08:53"
        },
        {
          "id": "MDRW2100018525.1.26",
          "form": "연기력은 믿고 볼 수 있을거같아요",
          "original_form": "연기력은 믿고 볼 수 있을거같아요",
          "speaker_id": "2",
          "time": "20210827 08:53"
        },
        {
          "id": "MDRW2100018525.1.27",
          "form": "인질이랑 씽크홀은 평이 별로더라구요",
          "original_form": "인질이랑 씽크홀은 평이 별로더라구요",
          "speaker_id": "2",
          "time": "20210827 08:54"
        },
        {
          "id": "MDRW2100018525.1.28",
          "form": "맞아요!!\r\n아 씽크홀 재밌을 것 같았는데 말이죠..",
          "original_form": "맞아요!!\r\n아 씽크홀 재밌을 것 같았는데 말이죠..",
          "speaker_id": "1",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018525.1.29",
          "form": "별론가보네요",
