      "utterance": [
        {
          "id": "MDRW2100009602.1.1",
          "form": "그 학교가 진짜 작아서",
          "original_form": "그 학교가 진짜 작아서",
          "speaker_id": "2",
          "time": "20210823 01:30"
        },
        {
          "id": "MDRW2100009602.1.2",
          "form": "선생님들이 전교생 이름을 다 외우세요",
          "original_form": "선생님들이 전교생 이름을 다 외우세요",
          "speaker_id": "2",
          "time": "20210823 01:30"
        },
        {
          "id": "MDRW2100009602.1.3",
          "form": "그니까 피해자도 아는 애였던 거예요",
          "original_form": "그니까 피해자도 아는 애였던 거예요",
          "speaker_id": "2",
          "time": "20210823 01:30"
        },
        {
          "id": "MDRW2100009602.1.4",
          "form": "에고",
          "original_form": "에고",
          "speaker_id": "1",
          "time": "20210823 01:30"
        },
        {
          "id": "MDRW2100009602.1.5",
          "form": "얼마나 미안했을까",
          "original_form": "얼마나 미안했을까",
          "speaker_id": "1",
          "time": "20210823 01:30"
        },
        {
          "id": "MDRW2100009602.1.6",
          "form": "있을 수 없는 일이라고",
          "original_form": "있을 수 없는 일이라고",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.7",
          "form": "대노하시더니",
          "original_form": "대노하시더니",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.8",
          "form": "그래서 그 교사는 어떻게 됐나요",
          "original_form": "그래서 그 교사는 어떻게 됐나요",
          "speaker_id": "1",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.9",
          "form": "저희한테 말해줘서 고맙다고",
          "original_form": "저희한테 말해줘서 고맙다고",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.10",
          "form": "그날이 마지막날이라서",
          "original_form": "그날이 마지막날이라서",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.11",
          "form": "어떻게 됐는지 보지는 못 했는데",
          "original_form": "어떻게 됐는지 보지는 못 했는데",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.12",
          "form": "아마 잘 해결됐을거라 믿어요",
          "original_form": "아마 잘 해결됐을거라 믿어요",
          "speaker_id": "1",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.13",
          "form": "교장선생님이나 부장선생님 말씀으로는",
          "original_form": "교장선생님이나 부장선생님 말씀으로는",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.14",
          "form": "반드시 강력하게 조치할 거고",
          "original_form": "반드시 강력하게 조치할 거고",
          "speaker_id": "2",
          "time": "20210823 01:31"
        },
        {
          "id": "MDRW2100009602.1.15",
          "form": "학교폭력도 반드시 해결할 거라고",
          "original_form": "학교폭력도 반드시 해결할 거라고",
          "speaker_id": "2",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.16",
          "form": "그러면서",
          "original_form": "그러면서",
          "speaker_id": "2",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.17",
          "form": "다음에도 우리 학교로 실습 와달래요",
          "original_form": "다음에도 우리 학교로 실습 와달래요",
          "speaker_id": "2",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.18",
          "form": "와서 직접 봐줬으면 좋겠다고",
          "original_form": "와서 직접 봐줬으면 좋겠다고",
          "speaker_id": "2",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.19",
          "form": "개선의지가 있어서 다행이에요",
          "original_form": "개선의지가 있어서 다행이에요",
          "speaker_id": "1",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.20",
          "form": "그래서 좀 희망을 느꼈어요",
          "original_form": "그래서 좀 희망을 느꼈어요",
          "speaker_id": "2",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.21",
          "form": "초등교사가",
          "original_form": "초등교사가",
          "speaker_id": "2",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.22",
          "form": "생각보다 수평적이에요",
          "original_form": "생각보다 수평적이에요",
          "speaker_id": "2",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.23",
          "form": "그 사회가",
          "original_form": "그 사회가",
          "speaker_id": "2",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.24",
          "form": "진짜 그마저도 없었다면 너무 절망적이었을거같아요",
          "original_form": "진짜 그마저도 없었다면 너무 절망적이었을거같아요",
          "speaker_id": "1",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009602.1.25",
          "form": "지역마다 다르긴 한데",
          "original_form": "지역마다 다르긴 한데",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄲㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄲㅋ",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.27",
          "form": "맞아오",
          "original_form": "맞아오",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.28",
          "form": "진짜 절망스러웠을 것 같아요",
          "original_form": "진짜 절망스러웠을 것 같아요",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.29",
          "form": "실습 전에는",
          "original_form": "실습 전에는",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.30",
          "form": "교사되면 잘 가르쳐야지",
          "original_form": "교사되면 잘 가르쳐야지",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.31",
          "form": "학생들에게 잘해야지",
          "original_form": "학생들에게 잘해야지",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.32",
          "form": "다들 처음엔 그렇게 시작하는거같아요",
          "original_form": "다들 처음엔 그렇게 시작하는거같아요",
          "speaker_id": "1",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.33",
          "form": "이런 마인드였는데",
          "original_form": "이런 마인드였는데",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.34",
          "form": "가서 진짜 많이 느끼고",
          "original_form": "가서 진짜 많이 느끼고",
          "speaker_id": "2",
          "time": "20210823 01:33"
        },
        {
          "id": "MDRW2100009602.1.35",
          "form": "죽어도 저렇게는 살지 말아야지",
          "original_form": "죽어도 저렇게는 살지 말아야지",
          "speaker_id": "2",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009602.1.36",
          "form": "역시 실전은 다르죠",
          "original_form": "역시 실전은 다르죠",
          "speaker_id": "1",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009602.1.37",
          "form": "진짜 잘 가르치고",
          "original_form": "진짜 잘 가르치고",
          "speaker_id": "2",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009602.1.38",
          "form": "좋은 교사가 되실거에요",
          "original_form": "좋은 교사가 되실거에요",
          "speaker_id": "1",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009602.1.39",
          "form": "진짜 노력해야지",
          "original_form": "진짜 노력해야지",
          "speaker_id": "2",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009602.1.40",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "2",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009602.1.41",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "2",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009602.1.42",
          "form": "쏘 스윗하시네요",
          "original_form": "쏘 스윗하시네요",
          "speaker_id": "2",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009602.1.43",
          "form": "앗 저는",
          "original_form": "앗 저는",
          "speaker_id": "1",
          "time": "20210823 01:35"
        },
        {
          "id": "MDRW2100009602.1.44",
          "form": "안타깝게도..",
          "original_form": "안타깝게도..",
          "speaker_id": "1",
          "time": "20210823 01:35"
        },
        {
          "id": "MDRW2100009602.1.45",
          "form": "코로나로 인해서 쉬고있어요",
          "original_form": "코로나로 인해서 쉬고있어요",
          "speaker_id": "1",
          "time": "20210823 01:35"
        }
      ]
    }
