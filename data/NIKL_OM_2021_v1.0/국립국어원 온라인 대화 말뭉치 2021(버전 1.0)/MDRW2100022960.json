      "utterance": [
        {
          "id": "MDRW2100022960.1.1",
          "form": "안녕하세요~^^",
          "original_form": "안녕하세요~^^",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022960.1.2",
          "form": "게임좋아하세요?",
          "original_form": "게임좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022960.1.3",
          "form": "저 게임을 별로 안좋아해요 ㅜㅜ",
          "original_form": "저 게임을 별로 안좋아해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022960.1.4",
          "form": "ㅎㅎ 그러시구나ㅎ",
          "original_form": "ㅎㅎ 그러시구나ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022960.1.5",
          "form": "모바일도 안해보셨음요?",
          "original_form": "모바일도 안해보셨음요?",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022960.1.6",
          "form": "아니면 어릴때 고무줄도?ㅎㅎ",
          "original_form": "아니면 어릴때 고무줄도?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022960.1.7",
          "form": "네 게임 자체를 잘 안해요 ㅋㅋ",
          "original_form": "네 게임 자체를 잘 안해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022960.1.8",
          "form": "마작은 해봤어요",
          "original_form": "마작은 해봤어요",
          "speaker_id": "2",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022960.1.9",
          "form": "어릴땐 오디션이나 카트라이더 정도?",
          "original_form": "어릴땐 오디션이나 카트라이더 정도?",
          "speaker_id": "2",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022960.1.10",
          "form": "오오ㅎㅎ 카트라이더ㅎ 저해봤어요ㅎ",
          "original_form": "오오ㅎㅎ 카트라이더ㅎ 저해봤어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022960.1.11",
          "form": "어릴때 해보고 이번엧모바일로나와서ㅎ",
          "original_form": "어릴때 해보고 이번엧모바일로나와서ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022960.1.12",
          "form": "모바일도 재밌나요?",
          "original_form": "모바일도 재밌나요?",
          "speaker_id": "2",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022960.1.13",
          "form": "정말재밌옸는데... 자꾸 카트바꿔라해서ㅠ",
          "original_form": "정말재밌옸는데... 자꾸 카트바꿔라해서ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022960.1.14",
          "form": "현질유도ㅠㅠ",
          "original_form": "현질유도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022960.1.15",
          "form": "재밌어요ㅎㅎ",
          "original_form": "재밌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022960.1.16",
          "form": "심심풀이로 하기에는 좋아요ㅎ",
          "original_form": "심심풀이로 하기에는 좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022960.1.17",
          "form": "ㅋㅋ 전 한번하면 너무 한도끝도없이 해서 잘 시작을 안해요",
          "original_form": "ㅋㅋ 전 한번하면 너무 한도끝도없이 해서 잘 시작을 안해요",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022960.1.18",
          "form": "ㅎㅎ 저랑비슷하군요ㅎ",
          "original_form": "ㅎㅎ 저랑비슷하군요ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022960.1.19",
          "form": "너무 중독임ㅠㅍ",
          "original_form": "너무 중독임ㅠㅍ",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022960.1.20",
          "form": "밤새요거의ㅎㅎㅎ",
          "original_form": "밤새요거의ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022960.1.21",
          "form": "네 생활이 힘들더라고요 ㅎ",
          "original_form": "네 생활이 힘들더라고요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022960.1.22",
          "form": "ㅎㅎ 맞아요 ㅡㅎㅎ 게임중독은 힘들어요ㅎ",
          "original_form": "ㅎㅎ 맞아요 ㅡㅎㅎ 게임중독은 힘들어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022960.1.23",
          "form": "저는 축구게임좋아해서",
          "original_form": "저는 축구게임좋아해서",
          "speaker_id": "1",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022960.1.24",
          "form": "자주했었는데ㅠ 출근까지 한적이ㅍ",
          "original_form": "자주했었는데ㅠ 출근까지 한적이ㅍ",
          "speaker_id": "1",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022960.1.25",
          "form": "근데.. 그게 온라인게임이아닌 오프라인이었어요ㅎㅎ",
          "original_form": "근데.. 그게 온라인게임이아닌 오프라인이었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022960.1.26",
          "form": "아 ㅎㅎ 축구 좋아하시는군요",
          "original_form": "아 ㅎㅎ 축구 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100022960.1.27",
          "form": "남자들은 다 좋아할껄요ㅎ",
          "original_form": "남자들은 다 좋아할껄요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100022960.1.28",
          "form": "최장 몇시간 해보셨어요?",
          "original_form": "최장 몇시간 해보셨어요?",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100022960.1.29",
          "form": "게임이요?",
          "original_form": "게임이요?",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100022960.1.30",
          "form": "음.. 최장 5시간?",
          "original_form": "음.. 최장 5시간?",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100022960.1.31",
          "form": "에잉?ㅎ 진짜요?",
          "original_form": "에잉?ㅎ 진짜요?",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100022960.1.32",
          "form": "여자분이아니신가?ㅎㅎ",
          "original_form": "여자분이아니신가?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100022960.1.33",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100022960.1.34",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100022960.1.35",
          "form": "맞는데 게임을 엄청열정적으로 하셨구나ㅎㅎ",
          "original_form": "맞는데 게임을 엄청열정적으로 하셨구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100022960.1.36",
          "form": "한번 빠지면 그래서 시작을 안해요 ㅋㅋ",
          "original_form": "한번 빠지면 그래서 시작을 안해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100022960.1.37",
          "form": "맞네요ㅎㅎ 그렇게하셔야겟네요ㅎㅎ",
          "original_form": "맞네요ㅎㅎ 그렇게하셔야겟네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100022960.1.38",
          "form": "혹시 다른것도 그래요? ㅎ 뭐드라마나?",
          "original_form": "혹시 다른것도 그래요? ㅎ 뭐드라마나?",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100022960.1.39",
          "form": "네 한번 시작하면 ㅎㅎ",
          "original_form": "네 한번 시작하면 ㅎㅎ",
