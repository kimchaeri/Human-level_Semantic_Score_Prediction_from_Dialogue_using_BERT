      "utterance": [
        {
          "id": "MDRW2100031062.1.1",
          "form": "공연 관심있으세요?",
          "original_form": "공연 관심있으세요?",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100031062.1.2",
          "form": "공연은 한번도 안본거 같아요",
          "original_form": "공연은 한번도 안본거 같아요",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100031062.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100031062.1.4",
          "form": "공연 예전엔 관심있어서",
          "original_form": "공연 예전엔 관심있어서",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031062.1.5",
          "form": "오 어떤 공연 보셨어요?",
          "original_form": "오 어떤 공연 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100031062.1.6",
          "form": "오페라의 유령 엘리자벳 레미제라블 헤드웍 지저스크라이스트수퍼스타",
          "original_form": "오페라의 유령 엘리자벳 레미제라블 헤드웍 지저스크라이스트수퍼스타",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031062.1.7",
          "form": "드림걸즈",
          "original_form": "드림걸즈",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031062.1.8",
          "form": "근데 요즘은 그정도관심이 안가서",
          "original_form": "근데 요즘은 그정도관심이 안가서",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031062.1.9",
          "form": "많이 보셨네요",
          "original_form": "많이 보셨네요",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031062.1.10",
          "form": "돈아까워서",
          "original_form": "돈아까워서",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031062.1.11",
          "form": "좀 비싸죠",
          "original_form": "좀 비싸죠",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031062.1.12",
          "form": "그냥 문화생활은 책으로 합니당",
          "original_form": "그냥 문화생활은 책으로 합니당",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100031062.1.13",
          "form": "책은 뭐 읽으세요?",
          "original_form": "책은 뭐 읽으세요?",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100031062.1.14",
          "form": "책은 추리소설이나 고전같은거 많이  읽어요",
          "original_form": "책은 추리소설이나 고전같은거 많이  읽어요",
          "speaker_id": "2",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100031062.1.15",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100031062.1.16",
          "form": "웹소설만 봐요 요즘엔 ᆢ ㅎ",
          "original_form": "웹소설만 봐요 요즘엔 ᆢ ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100031062.1.17",
          "form": "그래도 학생때는 책많이 봤었는데",
          "original_form": "그래도 학생때는 책많이 봤었는데",
          "speaker_id": "1",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100031062.1.18",
          "form": "도서관가서 빌리기도 귀찮고해서 ᆢ",
          "original_form": "도서관가서 빌리기도 귀찮고해서 ᆢ",
          "speaker_id": "1",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100031062.1.19",
          "form": "직접 사서 읽으세요?",
          "original_form": "직접 사서 읽으세요?",
          "speaker_id": "1",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100031062.1.20",
          "form": "아니요 직장에 도서관이 있어서",
          "original_form": "아니요 직장에 도서관이 있어서",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031062.1.21",
          "form": "희망도서 신청할수있어서",
          "original_form": "희망도서 신청할수있어서",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031062.1.22",
          "form": "직장 도서관 이용해요",
          "original_form": "직장 도서관 이용해요",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031062.1.23",
          "form": "직장근처도 도서관있구요",
          "original_form": "직장근처도 도서관있구요",
          "speaker_id": "2",
          "time": "20210904 16:38"
        }
      ]
