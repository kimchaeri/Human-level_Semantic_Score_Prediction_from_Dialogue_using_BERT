      "utterance": [
        {
          "id": "MDRW2100031088.1.1",
          "form": "해외포도 샤인머스캣같은거?",
          "original_form": "해외포도 샤인머스캣같은거?",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031088.1.2",
          "form": "안먹어봤어 아쉽게도",
          "original_form": "안먹어봤어 아쉽게도",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031088.1.3",
          "form": "옥수수 맛있지",
          "original_form": "옥수수 맛있지",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031088.1.4",
          "form": "청포도같은거 있잖아",
          "original_form": "청포도같은거 있잖아",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031088.1.5",
          "form": "청포도 새콤새콤하제잉",
          "original_form": "청포도 새콤새콤하제잉",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031088.1.6",
          "form": "나도 옥수수 좋아해",
          "original_form": "나도 옥수수 좋아해",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031088.1.7",
          "form": "어제 내가 옥수수를 먹었게 안 먹었게",
          "original_form": "어제 내가 옥수수를 먹었게 안 먹었게",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031088.1.8",
          "form": "먹었으니 물어보는것 아니오",
          "original_form": "먹었으니 물어보는것 아니오",
          "speaker_id": "1",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031088.1.9",
          "form": "젠장 또 간파당함",
          "original_form": "젠장 또 간파당함",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031088.1.10",
          "form": "넌 팝콘 좋아하냐",
          "original_form": "넌 팝콘 좋아하냐",
          "speaker_id": "1",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031088.1.11",
          "form": "난 팝콘 껍질 싫던디",
          "original_form": "난 팝콘 껍질 싫던디",
          "speaker_id": "1",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031088.1.12",
          "form": "응 팝콘 좋아해",
          "original_form": "응 팝콘 좋아해",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031088.1.13",
          "form": "껍질은 나도 좀 싫어",
          "original_form": "껍질은 나도 좀 싫어",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031088.1.14",
          "form": "걸리적거려",
          "original_form": "걸리적거려",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031088.1.15",
          "form": "이에 낄때가",
          "original_form": "이에 낄때가",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031088.1.16",
          "form": "제일 짱나",
          "original_form": "제일 짱나",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031088.1.17",
          "form": "왜이렇게 아파야 하는건인가",
          "original_form": "왜이렇게 아파야 하는건인가",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031088.1.18",
          "form": "ㅋㅋㅋ맞아 얇아서 잘 떨어지지도 않아",
          "original_form": "ㅋㅋㅋ맞아 얇아서 잘 떨어지지도 않아",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031088.1.19",
          "form": "무슨 팝콘 좋아해?",
          "original_form": "무슨 팝콘 좋아해?",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031088.1.20",
          "form": "목구녕에 가끔붙으면 괴롭다",
          "original_form": "목구녕에 가끔붙으면 괴롭다",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031088.1.21",
          "form": "난 캬라멜팝콘은",
          "original_form": "난 캬라멜팝콘은",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031088.1.22",
          "form": "아 공감 ㅋㅋㅋㅋㅋ",
          "original_form": "아 공감 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031088.1.23",
          "form": "좀 먹어",
          "original_form": "좀 먹어",
          "speaker_id": "1",
