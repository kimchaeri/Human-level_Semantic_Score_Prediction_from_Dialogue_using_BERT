      "utterance": [
        {
          "id": "MDRW2100015239.1.1",
          "form": "앞에 대화가 짤려서 안보여요ㅠㅠ",
          "original_form": "앞에 대화가 짤려서 안보여요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015239.1.2",
          "form": "그냥 저도 다가본곳이라고 ㅎ",
          "original_form": "그냥 저도 다가본곳이라고 ㅎ",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015239.1.3",
          "form": "아 ㅋㅋ 많이 다녀보셨네요",
          "original_form": "아 ㅋㅋ 많이 다녀보셨네요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015239.1.4",
          "form": "전 일년에한번씩",
          "original_form": "전 일년에한번씩",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015239.1.5",
          "form": "쉽지 않은데!",
          "original_form": "쉽지 않은데!",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015239.1.6",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015239.1.7",
          "form": "일년에 4번이 더대단하신데요ㅎ",
          "original_form": "일년에 4번이 더대단하신데요ㅎ",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015239.1.8",
          "form": "ㅋㅋ 와이프랑 막 삘 받아서",
          "original_form": "ㅋㅋ 와이프랑 막 삘 받아서",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015239.1.9",
          "form": "엄청 치고 빠졌어요",
          "original_form": "엄청 치고 빠졌어요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015239.1.10",
          "form": "ㅎㅎ 부럽네요 휴가를 글케쓸수두도있고",
          "original_form": "ㅎㅎ 부럽네요 휴가를 글케쓸수두도있고",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015239.1.11",
          "form": "어디가 젤 좋던가요",
          "original_form": "어디가 젤 좋던가요",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015239.1.12",
          "form": "베트남이 제일이었던거 같아요 ㅎㅎ",
          "original_form": "베트남이 제일이었던거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015239.1.13",
          "form": "하롱베이 이쁘더라구요",
          "original_form": "하롱베이 이쁘더라구요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015239.1.14",
          "form": "거긴안가봤네요ㅎ",
          "original_form": "거긴안가봤네요ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015239.1.15",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015239.1.16",
          "form": "베트남은 다낭이요",
          "original_form": "베트남은 다낭이요",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015239.1.17",
          "form": "다낭 좋다던데요!!",
          "original_form": "다낭 좋다던데요!!",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015239.1.18",
          "form": "가보고 싶었는데ㅠㅠ",
          "original_form": "가보고 싶었는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015239.1.19",
          "form": "좋더라구요ㅎ 전 시간없어서 패키지로 갔다왔는데 자유로가면 좋을듯",
          "original_form": "좋더라구요ㅎ 전 시간없어서 패키지로 갔다왔는데 자유로가면 좋을듯",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015239.1.20",
          "form": "맞아요 패키지도 나름 장점 있는듯요",
          "original_form": "맞아요 패키지도 나름 장점 있는듯요",
          "speaker_id": "2",
          "time": "20210826 00:24"
        }
