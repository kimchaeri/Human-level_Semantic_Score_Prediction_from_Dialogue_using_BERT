      "utterance": [
        {
          "id": "MDRW2100046750.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 14:42"
        },
        {
          "id": "MDRW2100046750.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 14:43"
        },
        {
          "id": "MDRW2100046750.1.3",
          "form": "좋아하는 가수 있으신가요?",
          "original_form": "좋아하는 가수 있으신가요?",
          "speaker_id": "2",
          "time": "20211104 14:43"
        },
        {
          "id": "MDRW2100046750.1.4",
          "form": "아이유 좋아합니다",
          "original_form": "아이유 좋아합니다",
          "speaker_id": "1",
          "time": "20211104 14:43"
        },
        {
          "id": "MDRW2100046750.1.5",
          "form": "와 저 지금 아이유 노래 듣고있어요",
          "original_form": "와 저 지금 아이유 노래 듣고있어요",
          "speaker_id": "2",
          "time": "20211104 14:44"
        },
        {
          "id": "MDRW2100046750.1.6",
          "form": "ㅋㅋㅋ 저도 아이유님 좋아해요!!",
          "original_form": "ㅋㅋㅋ 저도 아이유님 좋아해요!!",
          "speaker_id": "2",
          "time": "20211104 14:44"
        },
        {
          "id": "MDRW2100046750.1.7",
          "form": "여기서 만나네요! ㅋ",
          "original_form": "여기서 만나네요! ㅋ",
          "speaker_id": "1",
          "time": "20211104 14:44"
        },
        {
          "id": "MDRW2100046750.1.8",
          "form": "그러게요! ㅋㅋㅋ",
          "original_form": "그러게요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 14:44"
        },
        {
          "id": "MDRW2100046750.1.9",
          "form": "최애곡 있으신가요?",
          "original_form": "최애곡 있으신가요?",
          "speaker_id": "2",
          "time": "20211104 14:44"
        },
        {
          "id": "MDRW2100046750.1.10",
          "form": "다최애곡 이지만 좋은날이 최애곡입니다",
          "original_form": "다최애곡 이지만 좋은날이 최애곡입니다",
          "speaker_id": "1",
          "time": "20211104 14:45"
        },
        {
          "id": "MDRW2100046750.1.11",
          "form": "맞아요 다 최애곡이죠 ㅋㅋㅋ",
          "original_form": "맞아요 다 최애곡이죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 14:45"
        },
        {
          "id": "MDRW2100046750.1.12",
          "form": "좋은날은 안질리더라구요",
          "original_form": "좋은날은 안질리더라구요",
          "speaker_id": "2",
          "time": "20211104 14:45"
        },
        {
          "id": "MDRW2100046750.1.13",
          "form": "들어도 들어도 좋은 노래예요",
          "original_form": "들어도 들어도 좋은 노래예요",
          "speaker_id": "2",
          "time": "20211104 14:45"
        },
        {
          "id": "MDRW2100046750.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211104 14:45"
        },
        {
          "id": "MDRW2100046750.1.15",
          "form": "저는 금요일에 만나요 좋아하구요",
          "original_form": "저는 금요일에 만나요 좋아하구요",
          "speaker_id": "2",
          "time": "20211104 14:46"
        },
        {
          "id": "MDRW2100046750.1.16",
          "form": "최근에는 라일락을 가장 열심히 들었네요!",
          "original_form": "최근에는 라일락을 가장 열심히 들었네요!",
          "speaker_id": "2",
          "time": "20211104 14:46"
        },
        {
          "id": "MDRW2100046750.1.17",
          "form": "둘다 좋죠",
          "original_form": "둘다 좋죠",
          "speaker_id": "1",
          "time": "20211104 14:46"
        },
        {
          "id": "MDRW2100046750.1.18",
          "form": "아 그리고 얼마 전에 악동뮤지션이랑 같이 부른 낙하 아세요?",
          "original_form": "아 그리고 얼마 전에 악동뮤지션이랑 같이 부른 낙하 아세요?",
          "speaker_id": "2",
          "time": "20211104 14:47"
        },
        {
          "id": "MDRW2100046750.1.19",
          "form": "그 노래도 좋더라구요!",
          "original_form": "그 노래도 좋더라구요!",
          "speaker_id": "2",
          "time": "20211104 14:47"
        },
        {
          "id": "MDRW2100046750.1.20",
          "form": "네 악뮤랑도 잘 어울리더라구요",
          "original_form": "네 악뮤랑도 잘 어울리더라구요",
          "speaker_id": "1",
          "time": "20211104 14:47"
        }
