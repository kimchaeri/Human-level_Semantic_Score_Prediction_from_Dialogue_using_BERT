      "utterance": [
        {
          "id": "MDRW2100026977.1.1",
          "form": "애들도 보여달라하니 조만간 가자",
          "original_form": "애들도 보여달라하니 조만간 가자",
          "speaker_id": "2",
          "time": "20210902 02:20"
        },
        {
          "id": "MDRW2100026977.1.2",
          "form": "응 조심해서 가면 괜찮겠지뭐",
          "original_form": "응 조심해서 가면 괜찮겠지뭐",
          "speaker_id": "1",
          "time": "20210902 02:20"
        },
        {
          "id": "MDRW2100026977.1.3",
          "form": "name3가있어서 모봐야할지",
          "original_form": "&name3&가있어서 모봐야할지",
          "speaker_id": "1",
          "time": "20210902 02:23"
        },
        {
          "id": "MDRW2100026977.1.4",
          "form": "명절때쯤이면 애들볼꺼있겠지",
          "original_form": "명절때쯤이면 애들볼꺼있겠지",
          "speaker_id": "2",
          "time": "20210902 02:24"
        },
        {
          "id": "MDRW2100026977.1.5",
          "form": "어디안가니 그때 가야겠다.  맛있는것도 사먹고",
          "original_form": "어디안가니 그때 가야겠다.  맛있는것도 사먹고",
          "speaker_id": "1",
          "time": "20210902 02:24"
        },
        {
          "id": "MDRW2100026977.1.6",
          "form": "그래. 애들도 어디 안다녀서 엄청 답답할꺼야",
          "original_form": "그래. 애들도 어디 안다녀서 엄청 답답할꺼야",
          "speaker_id": "2",
          "time": "20210902 02:25"
        },
        {
          "id": "MDRW2100026977.1.7",
          "form": "다들 놀러다니는데 우리만 집에있으니",
          "original_form": "다들 놀러다니는데 우리만 집에있으니",
          "speaker_id": "2",
          "time": "20210902 02:25"
        },
        {
          "id": "MDRW2100026977.1.8",
          "form": "그래~ 넘 집에만 있긴했어",
          "original_form": "그래~ 넘 집에만 있긴했어",
          "speaker_id": "1",
          "time": "20210902 02:25"
        },
        {
          "id": "MDRW2100026977.1.9",
          "form": "백신맞기전까진 조심해야지",
          "original_form": "백신맞기전까진 조심해야지",
          "speaker_id": "2",
          "time": "20210902 02:26"
        },
        {
          "id": "MDRW2100026977.1.10",
          "form": "응 name3때매 조심해야지",
          "original_form": "응 &name3&때매 조심해야지",
          "speaker_id": "1",
          "time": "20210902 02:26"
        },
        {
          "id": "MDRW2100026977.1.11",
          "form": "10대도 맞는다는거같아",
          "original_form": "10대도 맞는다는거같아",
          "speaker_id": "1",
          "time": "20210902 02:27"
        },
        {
          "id": "MDRW2100026977.1.12",
          "form": "그럼 name4도 맞으니 name3만 잘 데리고있음 되겠네",
          "original_form": "그럼 &name4&도 맞으니 &name3&만 잘 데리고있음 되겠네",
          "speaker_id": "2",
          "time": "20210902 02:27"
        },
        {
          "id": "MDRW2100026977.1.13",
          "form": "꼬마들은 어찌 되려는지",
          "original_form": "꼬마들은 어찌 되려는지",
          "speaker_id": "2",
          "time": "20210902 02:27"
        },
        {
          "id": "MDRW2100026977.1.14",
          "form": "그러게말야.  걸려도 살짝이라고는 하는데...",
          "original_form": "그러게말야.  걸려도 살짝이라고는 하는데...",
          "speaker_id": "1",
          "time": "20210902 02:28"
        },
        {
          "id": "MDRW2100026977.1.15",
          "form": "에버랜드 노래부르는데 미안하더라",
          "original_form": "에버랜드 노래부르는데 미안하더라",
          "speaker_id": "2",
          "time": "20210902 02:28"
        },
        {
          "id": "MDRW2100026977.1.16",
          "form": "좋아하는 동물도 못보여주고",
          "original_form": "좋아하는 동물도 못보여주고",
          "speaker_id": "2",
          "time": "20210902 02:28"
        },
        {
          "id": "MDRW2100026977.1.17",
          "form": "쫌 줄어들면 그때가자",
          "original_form": "쫌 줄어들면 그때가자",
          "speaker_id": "1",
          "time": "20210902 02:29"
        },
        {
          "id": "MDRW2100026977.1.18",
          "form": "지금은 야외라도 무서워~",
          "original_form": "지금은 야외라도 무서워~",
          "speaker_id": "1",
          "time": "20210902 02:29"
        },
        {
          "id": "MDRW2100026977.1.19",
          "form": "화장실이고 선물가게고 안들릴자신있음 가도 괜찮아",
          "original_form": "화장실이고 선물가게고 안들릴자신있음 가도 괜찮아",
          "speaker_id": "2",
          "time": "20210902 02:30"
        }
      ]
