      "utterance": [
        {
          "id": "MDRW2100030451.1.1",
          "form": "그래 쉬운거 하자 쉬운거",
          "original_form": "그래 쉬운거 하자 쉬운거",
          "speaker_id": "1",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030451.1.2",
          "form": "애니메이션 요샌 뭐보십니까 ㅋㅋㅋㅋㅋ",
          "original_form": "애니메이션 요샌 뭐보십니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030451.1.3",
          "form": "안본다 못본다",
          "original_form": "안본다 못본다",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030451.1.4",
          "form": "볼 새가 어딧나요",
          "original_form": "볼 새가 어딧나요",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030451.1.5",
          "form": "양심잇?",
          "original_form": "양심잇?",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030451.1.6",
          "form": "아 왜요...",
          "original_form": "아 왜요...",
          "speaker_id": "1",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030451.1.7",
          "form": "양심은 항상 있지",
          "original_form": "양심은 항상 있지",
          "speaker_id": "1",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030451.1.8",
          "form": "늘 있는 양심을 왜 없애고 그래",
          "original_form": "늘 있는 양심을 왜 없애고 그래",
          "speaker_id": "1",
          "time": "20210904 01:20"
        },
        {
          "id": "MDRW2100030451.1.9",
          "form": "아니 없는것 같아요",
          "original_form": "아니 없는것 같아요",
          "speaker_id": "2",
          "time": "20210904 01:23"
        },
        {
          "id": "MDRW2100030451.1.10",
          "form": "저 딴거 볼 짬도 없이",
          "original_form": "저 딴거 볼 짬도 없이",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030451.1.11",
          "form": "영업이 휘몰아치던걸",
          "original_form": "영업이 휘몰아치던걸",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030451.1.12",
          "form": "결국 계속 미뤄두고 잇지만 ㅋㅋㅋ",
          "original_form": "결국 계속 미뤄두고 잇지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030451.1.13",
          "form": "아 최근에 본거 잇다",
          "original_form": "아 최근에 본거 잇다",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030451.1.14",
          "form": "주제가 어려웠다면 책으로 할걸",
          "original_form": "주제가 어려웠다면 책으로 할걸",
          "speaker_id": "1",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030451.1.15",
          "form": "뭐봤어?",
          "original_form": "뭐봤어?",
          "speaker_id": "1",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030451.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030451.1.17",
          "form": "'불멸의 그대에게'라고",
          "original_form": "'불멸의 그대에게'라고",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030451.1.18",
          "form": "전에 한번 말한적잇는데 그게 애니메이션으로 나왔거든",
          "original_form": "전에 한번 말한적잇는데 그게 애니메이션으로 나왔거든",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030451.1.19",
          "form": "무슨내용이...",
          "original_form": "무슨내용이...",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030451.1.20",
          "form": "기억이..없... 뭔내용이냐",
          "original_form": "기억이..없... 뭔내용이냐",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030451.1.21",
          "form": "ㅋㅋㅋ기억안날거야",
          "original_form": "ㅋㅋㅋ기억안날거야",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030451.1.22",
          "form": "그게 무슨 내용인데?",
          "original_form": "그게 무슨 내용인데?",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030451.1.23",
          "form": "그전에",
          "original_form": "그전에",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030451.1.24",
          "form": "아마 극장판이엇던거 같은데",
          "original_form": "아마 극장판이엇던거 같은데",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030451.1.25",
          "form": "무슨 극장판?",
          "original_form": "무슨 극장판?",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030451.1.26",
          "form": "수화로 대화하는 여자애 나오는 애니메이션 알아?",
          "original_form": "수화로 대화하는 여자애 나오는 애니메이션 알아?",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.27",
          "form": "아 알아",
          "original_form": "아 알아",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.28",
          "form": "그... 제목이 생각이 안나는데",
          "original_form": "그... 제목이 생각이 안나는데",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.29",
          "form": "엉어",
          "original_form": "엉어",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.30",
          "form": "목소리의 형태?",
          "original_form": "목소리의 형태?",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.31",
          "form": "그 작가님인데 ㅇㅇ",
          "original_form": "그 작가님인데 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.32",
          "form": "그거 좀 미묘했어",
          "original_form": "그거 좀 미묘했어",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.33",
          "form": "그때 이 두개 듣더니",
          "original_form": "그때 이 두개 듣더니",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.34",
          "form": "결국 받아줘야 하는건가",
          "original_form": "결국 받아줘야 하는건가",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.35",
          "form": "싶어서",
          "original_form": "싶어서",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030451.1.36",
          "form": "네가 어려운걸 쓰시네 이런 뉘앙스로 말한적잇음 ㅋㅋㅋ",
          "original_form": "네가 어려운걸 쓰시네 이런 뉘앙스로 말한적잇음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030451.1.37",
          "form": "나도 목소리의 형태는 별로..",
          "original_form": "나도 목소리의 형태는 별로..",
          "speaker_id": "2",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030451.1.38",
          "form": "암튼 음 불멸의 그대에게?",
          "original_form": "암튼 음 불멸의 그대에게?",
          "speaker_id": "2",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030451.1.39",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030451.1.40",
          "form": "내가 그런 말을 했었구나...",
          "original_form": "내가 그런 말을 했었구나...",
          "speaker_id": "1",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030451.1.41",
          "form": "어허헣...",
          "original_form": "어허헣...",
          "speaker_id": "1",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030451.1.42",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:29"
        }
      ]
    }
