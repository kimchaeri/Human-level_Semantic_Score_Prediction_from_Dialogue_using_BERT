        },
        {
          "id": "MDRW2100016233.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100016233.1.3",
          "form": "커뮤니티 뭐하시는거있나요?",
          "original_form": "커뮤니티 뭐하시는거있나요?",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016233.1.4",
          "form": "주식이요ㅋㅋㅋ",
          "original_form": "주식이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016233.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016233.1.6",
          "form": "저는코인이요",
          "original_form": "저는코인이요",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016233.1.7",
          "form": "주식은 하다가말았엉ㅅ",
          "original_form": "주식은 하다가말았엉ㅅ",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016233.1.8",
          "form": "오 코인 커뮤니티도 활발한가요?",
          "original_form": "오 코인 커뮤니티도 활발한가요?",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016233.1.9",
          "form": "전 잘 몰라서 요즘 다시 오르는거 같아서 지켜보고있어요 ㅋㅋㅋ",
          "original_form": "전 잘 몰라서 요즘 다시 오르는거 같아서 지켜보고있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016233.1.10",
          "form": "이제 코인에도 다시\r\n상승장이 온건가요? ㅋㅋ",
          "original_form": "이제 코인에도 다시\r\n상승장이 온건가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016233.1.11",
          "form": "혹시 정모 같은거\r\n참석해보신적 있으세요?",
          "original_form": "혹시 정모 같은거\r\n참석해보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016233.1.12",
          "form": "제발 상승장..ㅠㅠㅋㅋㅋ",
          "original_form": "제발 상승장..ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016233.1.13",
          "form": "아 아뇨 정모 나가본 경험은 없어요",
          "original_form": "아 아뇨 정모 나가본 경험은 없어요",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016233.1.14",
          "form": "그럼 커뮤니티도 주로 눈팅용이세요?",
          "original_form": "그럼 커뮤니티도 주로 눈팅용이세요?",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016233.1.15",
          "form": "거의 눈팅만해요",
          "original_form": "거의 눈팅만해요",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016233.1.16",
          "form": "커뮤니티 애용?하시나봐요",
          "original_form": "커뮤니티 애용?하시나봐요",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016233.1.17",
          "form": "저도 눈팅용이에요:)",
          "original_form": "저도 눈팅용이에요:)",
          "speaker_id": "2",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016233.1.18",
          "form": "궁금한거 있거나 할때\r\n들어가서 검색해보고",
          "original_form": "궁금한거 있거나 할때\r\n들어가서 검색해보고",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016233.1.19",
          "form": "종종 올라오는 글들 보는 정도요:)",
          "original_form": "종종 올라오는 글들 보는 정도요:)",
          "speaker_id": "2",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100016233.1.20",
          "form": "저도 그게 그냥 편한거같아요 ㅎㅎ",
          "original_form": "저도 그게 그냥 편한거같아요 ㅎㅎ",
