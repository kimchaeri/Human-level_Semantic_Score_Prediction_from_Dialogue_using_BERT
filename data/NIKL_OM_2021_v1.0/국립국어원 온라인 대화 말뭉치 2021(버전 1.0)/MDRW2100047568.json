      "utterance": [
        {
          "id": "MDRW2100047568.1.1",
          "form": "뮤지컬 재밌게 보신 거 있나요?",
          "original_form": "뮤지컬 재밌게 보신 거 있나요?",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100047568.1.2",
          "form": "저는 뮤지컬 본게 시카고밖에 없어서 시카고요",
          "original_form": "저는 뮤지컬 본게 시카고밖에 없어서 시카고요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100047568.1.3",
          "form": "재밌게 보신거 있으세요?",
          "original_form": "재밌게 보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100047568.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100047568.1.5",
          "form": "저는 어렸을 때 본 건데",
          "original_form": "저는 어렸을 때 본 건데",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100047568.1.6",
          "form": "그리스 좋았어요",
          "original_form": "그리스 좋았어요",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100047568.1.7",
          "form": "기억나는 배우있으세요?",
          "original_form": "기억나는 배우있으세요?",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100047568.1.8",
          "form": "진짜 어렸을 때라ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 어렸을 때라ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100047568.1.9",
          "form": "근데 남자주인공은 좋았아요",
          "original_form": "근데 남자주인공은 좋았아요",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100047568.1.10",
          "form": "그러셨구나!",
          "original_form": "그러셨구나!",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047568.1.11",
          "form": "콘서트나 전시회는 재밌게 보신거 있나요?",
          "original_form": "콘서트나 전시회는 재밌게 보신거 있나요?",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100047568.1.12",
          "form": "재밌는 건 아니엏는데",
          "original_form": "재밌는 건 아니엏는데",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047568.1.13",
          "form": "아니 재밌었던가",
          "original_form": "아니 재밌었던가",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047568.1.14",
          "form": "학생 때 아이돌을 좋아했어서",
          "original_form": "학생 때 아이돌을 좋아했어서",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047568.1.15",
          "form": "엑소 콘서트를 갔었어요ㅋㅋㅋㅋ",
          "original_form": "엑소 콘서트를 갔었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100047568.1.16",
          "form": "제친구도 좋아해서 엑소콘서트 몇번 갔어요",
          "original_form": "제친구도 좋아해서 엑소콘서트 몇번 갔어요",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047568.1.17",
          "form": "티켓팅 능력자시네요",
          "original_form": "티켓팅 능력자시네요",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047568.1.18",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047568.1.19",
          "form": "취켓팅으로 잡고 갔었거든요",
          "original_form": "취켓팅으로 잡고 갔었거든요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047568.1.20",
          "form": "친구분도 엑소를 좋아하셨군요",
          "original_form": "친구분도 엑소를 좋아하셨군요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047568.1.21",
          "form": "친구 콘서트 티켓팅 용병이였거든요저...",
          "original_form": "친구 콘서트 티켓팅 용병이였거든요저...",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100047568.1.22",
          "form": "엄청 빡세던데요ㅠ",
          "original_form": "엄청 빡세던데요ㅠ",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.24",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㄱㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.25",
          "form": "아니 들어가지질 않는데 어떻게...",
          "original_form": "아니 들어가지질 않는데 어떻게...",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.26",
          "form": "저랑 같은 시간에 클릭하고 있으셨겠네요..",
          "original_form": "저랑 같은 시간에 클릭하고 있으셨겠네요..",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.27",
          "form": "아 너무 으ㅡㅅ겨",
          "original_form": "아 너무 으ㅡㅅ겨",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.28",
          "form": "다들그렇게 하시는지",
          "original_form": "다들그렇게 하시는지",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.29",
          "form": "그러게 말이에요..",
          "original_form": "그러게 말이에요..",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.30",
          "form": "피시방데려가지고 그랬어요...",
          "original_form": "피시방데려가지고 그랬어요...",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.31",
          "form": "저도 친구 데리고 피시방 가서ㅋㅋㅋㅋㅋ",
          "original_form": "저도 친구 데리고 피시방 가서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.32",
          "form": "반강제요",
          "original_form": "반강제요",
          "speaker_id": "2",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.33",
          "form": "했었습니다..",
          "original_form": "했었습니다..",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100047568.1.34",
          "form": "좋은 친구네요",
          "original_form": "좋은 친구네요",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100047568.1.35",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100047568.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100047568.1.37",
          "form": "앜ㅋㅋㅋㅋㄱㅋ다들 비슷하군요",
          "original_form": "앜ㅋㅋㅋㅋㄱㅋ다들 비슷하군요",
          "speaker_id": "2",
          "time": "20210826 20:57"
        }
