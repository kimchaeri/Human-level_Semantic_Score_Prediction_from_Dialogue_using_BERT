      "utterance": [
        {
          "id": "MDRW2100009688.1.1",
          "form": "계절이라",
          "original_form": "계절이라",
          "speaker_id": "2",
          "time": "20210823 03:51"
        },
        {
          "id": "MDRW2100009688.1.2",
          "form": "덥다는 생각이 1순위인데요 ㅋㅋㅋ",
          "original_form": "덥다는 생각이 1순위인데요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 03:51"
        },
        {
          "id": "MDRW2100009688.1.3",
          "form": "그렇긴하지 ㅋㅋㅋㅋ",
          "original_form": "그렇긴하지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 03:51"
        },
        {
          "id": "MDRW2100009688.1.4",
          "form": "그럼 본론으로 가죠",
          "original_form": "그럼 본론으로 가죠",
          "speaker_id": "2",
          "time": "20210823 03:52"
        },
        {
          "id": "MDRW2100009688.1.5",
          "form": "요즘 장마철이네요",
          "original_form": "요즘 장마철이네요",
          "speaker_id": "2",
          "time": "20210823 03:52"
        },
        {
          "id": "MDRW2100009688.1.6",
          "form": "꽃도 벌써 떨어지고요",
          "original_form": "꽃도 벌써 떨어지고요",
          "speaker_id": "2",
          "time": "20210823 03:52"
        },
        {
          "id": "MDRW2100009688.1.7",
          "form": "흠 꽃구경은 작년부터 글러먹었지만요",
          "original_form": "흠 꽃구경은 작년부터 글러먹었지만요",
          "speaker_id": "2",
          "time": "20210823 03:53"
        },
        {
          "id": "MDRW2100009688.1.8",
          "form": "꽃은 커녕 바다구경도 못했네",
          "original_form": "꽃은 커녕 바다구경도 못했네",
          "speaker_id": "1",
          "time": "20210823 03:53"
        },
        {
          "id": "MDRW2100009688.1.9",
          "form": "내일도 비온다던거 같은데",
          "original_form": "내일도 비온다던거 같은데",
          "speaker_id": "1",
          "time": "20210823 03:53"
        },
        {
          "id": "MDRW2100009688.1.10",
          "form": "모기 늘어나겠다",
          "original_form": "모기 늘어나겠다",
          "speaker_id": "1",
          "time": "20210823 03:53"
        },
        {
          "id": "MDRW2100009688.1.11",
          "form": "모기약은 항상 지참이죠",
          "original_form": "모기약은 항상 지참이죠",
          "speaker_id": "2",
          "time": "20210823 03:53"
        },
        {
          "id": "MDRW2100009688.1.12",
          "form": "이거 근데",
          "original_form": "이거 근데",
          "speaker_id": "2",
          "time": "20210823 03:53"
        },
        {
          "id": "MDRW2100009688.1.13",
          "form": "카운트 단위를 모르겠네요",
          "original_form": "카운트 단위를 모르겠네요",
          "speaker_id": "2",
          "time": "20210823 03:53"
        },
        {
          "id": "MDRW2100009688.1.14",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210823 03:54"
        },
        {
          "id": "MDRW2100009688.1.15",
          "form": "단어가 많은거 아닐까",
          "original_form": "단어가 많은거 아닐까",
          "speaker_id": "1",
          "time": "20210823 03:54"
        },
        {
          "id": "MDRW2100009688.1.16",
          "form": "치킨좀 먹었으면 ㅋㅋㅋ",
          "original_form": "치킨좀 먹었으면 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 03:54"
        },
        {
          "id": "MDRW2100009688.1.17",
          "form": "비올때는 치킨이닭!",
          "original_form": "비올때는 치킨이닭!",
          "speaker_id": "1",
          "time": "20210823 03:54"
        },
        {
          "id": "MDRW2100009688.1.18",
          "form": "양념치킨?",
          "original_form": "양념치킨?",
          "speaker_id": "2",
          "time": "20210823 03:54"
        },
        {
          "id": "MDRW2100009688.1.19",
          "form": "스노우 치킨이지",
          "original_form": "스노우 치킨이지",
          "speaker_id": "1",
          "time": "20210823 03:54"
        },
        {
          "id": "MDRW2100009688.1.20",
          "form": "비오는데요?",
          "original_form": "비오는데요?",
          "speaker_id": "2",
          "time": "20210823 03:54"
        },
        {
          "id": "MDRW2100009688.1.21",
          "form": "어... 웃을까?",
          "original_form": "어... 웃을까?",
          "speaker_id": "1",
          "time": "20210823 03:54"
        },
        {
          "id": "MDRW2100009688.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 03:55"
        },
        {
          "id": "MDRW2100009688.1.23",
          "form": "난 약한자의 명령을  듣지않아서",
          "original_form": "난 약한자의 명령을  듣지않아서",
          "speaker_id": "1",
          "time": "20210823 03:55"
        },
        {
          "id": "MDRW2100009688.1.24",
          "form": "제가 약하긴 하죠",
          "original_form": "제가 약하긴 하죠",
          "speaker_id": "2",
          "time": "20210823 03:55"
        },
        {
          "id": "MDRW2100009688.1.25",
          "form": "경찰아저씨?",
          "original_form": "경찰아저씨?",
          "speaker_id": "1",
          "time": "20210823 03:55"
        },
        {
          "id": "MDRW2100009688.1.26",
          "form": "너무하네요",
          "original_form": "너무하네요",
          "speaker_id": "2",
          "time": "20210823 03:55"
        }
      ]
