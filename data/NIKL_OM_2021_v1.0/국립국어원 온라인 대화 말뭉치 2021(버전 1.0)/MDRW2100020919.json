      "utterance": [
        {
          "id": "MDRW2100020919.1.1",
          "form": "안녕하세요 점심 드셨어요?",
          "original_form": "안녕하세요 점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020919.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020919.1.3",
          "form": "닭갈비먹었어요!",
          "original_form": "닭갈비먹었어요!",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020919.1.4",
          "form": "앗 맛있었겠네요",
          "original_form": "앗 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020919.1.5",
          "form": "로제떡볶이 좋아하세요?",
          "original_form": "로제떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020919.1.6",
          "form": "너무 좋아하죠",
          "original_form": "너무 좋아하죠",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020919.1.7",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020919.1.8",
          "form": "네 저도 좋아해요",
          "original_form": "네 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020919.1.9",
          "form": "어느 브랜드 좋아하세요?",
          "original_form": "어느 브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020919.1.10",
          "form": "저는 신전이요!",
          "original_form": "저는 신전이요!",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020919.1.11",
          "form": "양 적당해서 혼자먹기 좋더라구여",
          "original_form": "양 적당해서 혼자먹기 좋더라구여",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020919.1.12",
          "form": "오 저는 엽떡좋아해요",
          "original_form": "오 저는 엽떡좋아해요",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020919.1.13",
          "form": "신전은 안먹어봐서 먹어보고싶네요",
          "original_form": "신전은 안먹어봐서 먹어보고싶네요",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020919.1.14",
          "form": "전 엽떡을 안먹어봤는데 반대네요 ㅋㅋㅋ",
          "original_form": "전 엽떡을 안먹어봤는데 반대네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020919.1.15",
          "form": "엽떡은 어떤가요",
          "original_form": "엽떡은 어떤가요",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020919.1.16",
          "form": "약간 매콤한 맛인거같아요",
          "original_form": "약간 매콤한 맛인거같아요",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020919.1.17",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020919.1.18",
          "form": "순한맛으로드세요?",
          "original_form": "순한맛으로드세요?",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020919.1.19",
          "form": "신전은 어떤맛이예요?",
          "original_form": "신전은 어떤맛이예요?",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020919.1.20",
          "form": "살짝 매운맛이요",
          "original_form": "살짝 매운맛이요",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020919.1.21",
          "form": "저는 무조건 순한맛이여 ㅋㅋㅋ",
          "original_form": "저는 무조건 순한맛이여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020919.1.22",
          "form": "어렸을땐 매운거 잘 먹었는데 요샌 속이아프더라고요 ㅠ",
          "original_form": "어렸을땐 매운거 잘 먹었는데 요샌 속이아프더라고요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020919.1.23",
          "form": "아 순한맛도 맛있죠",
          "original_form": "아 순한맛도 맛있죠",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020919.1.24",
          "form": "그쳐 ㅋㅋㅋ",
          "original_form": "그쳐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:17"
        }
      ]
