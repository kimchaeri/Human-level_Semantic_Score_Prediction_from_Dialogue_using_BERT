      "utterance": [
        {
          "id": "MDRW2100011551.1.1",
          "form": "글쎄 남은 김밥 재료로 한 번 더 만들까 하는데",
          "original_form": "글쎄 남은 김밥 재료로 한 번 더 만들까 하는데",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011551.1.2",
          "form": "그건 내일 오전에 먹고 다른 거 먹자",
          "original_form": "그건 내일 오전에 먹고 다른 거 먹자",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011551.1.3",
          "form": "뭐 먹고 싶은 거 있어?",
          "original_form": "뭐 먹고 싶은 거 있어?",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011551.1.4",
          "form": "ㅇㅇ 도미노 피자 ㅎㅎ",
          "original_form": "ㅇㅇ 도미노 피자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011551.1.5",
          "form": "오오 맛나지 ㅋㅋ",
          "original_form": "오오 맛나지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011551.1.6",
          "form": "오늘 비가 와서 그런가 피자가 땡기네",
          "original_form": "오늘 비가 와서 그런가 피자가 땡기네",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011551.1.7",
          "form": "나도 그래~ 좀 기름진 게 땡겨",
          "original_form": "나도 그래~ 좀 기름진 게 땡겨",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011551.1.8",
          "form": "피자 중엔 도미노가 제일 맛있는 것 같아",
          "original_form": "피자 중엔 도미노가 제일 맛있는 것 같아",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011551.1.9",
          "form": "ㅇㅇ 요즘 다른 피자집들도 가격이 다 올라서 도미노가 짱임",
          "original_form": "ㅇㅇ 요즘 다른 피자집들도 가격이 다 올라서 도미노가 짱임",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011551.1.10",
          "form": "알볼로도 좋았는데 가격 너무 많이 올림 ㅠ",
          "original_form": "알볼로도 좋았는데 가격 너무 많이 올림 ㅠ",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011551.1.11",
          "form": "ㅇㅇ 같은 가격이면 도미노지",
          "original_form": "ㅇㅇ 같은 가격이면 도미노지",
          "speaker_id": "2",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011551.1.12",
          "form": "도우도 토핑도 최고 ㅎㅎ",
          "original_form": "도우도 토핑도 최고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011551.1.13",
          "form": "난 그럼 저녁 준비 필요없네 아싸 ㅎㅎ",
          "original_form": "난 그럼 저녁 준비 필요없네 아싸 ㅎㅎ",
