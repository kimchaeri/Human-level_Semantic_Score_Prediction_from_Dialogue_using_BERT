      "utterance": [
        {
          "id": "MDRW2100002144.1.1",
          "form": "요즘 뭐 할인하는건 없나",
          "original_form": "요즘 뭐 할인하는건 없나",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.2",
          "form": "나 바디스크럽 샀는데 어제 왔어",
          "original_form": "나 바디스크럽 샀는데 어제 왔어",
          "speaker_id": "1",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.3",
          "form": "바디스트럽 네이버에서 사면 할인한다 오픈마켓",
          "original_form": "바디스트럽 네이버에서 사면 할인한다 오픈마켓",
          "speaker_id": "1",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.4",
          "form": "오 그거 쓰고 후기 알려달라고했지",
          "original_form": "오 그거 쓰고 후기 알려달라고했지",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.5",
          "form": "오늘 써봤나?",
          "original_form": "오늘 써봤나?",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.6",
          "form": "향기 대박 좋더라 그거 닮은 향수 찾아봤는데 그 브랜드 거 있더라",
          "original_form": "향기 대박 좋더라 그거 닮은 향수 찾아봤는데 그 브랜드 거 있더라",
          "speaker_id": "1",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.7",
          "form": "우와 그거랑 깔맞춤하면 완전 좋겠다",
          "original_form": "우와 그거랑 깔맞춤하면 완전 좋겠다",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.8",
          "form": "어제 써봤는데 몸이 아주 부드러워져.",
          "original_form": "어제 써봤는데 몸이 아주 부드러워져.",
          "speaker_id": "1",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ몸이 부드럽다니까 웃긴다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ몸이 부드럽다니까 웃긴다",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100002144.1.10",
          "form": "향수에는 색이 없지만",
          "original_form": "향수에는 색이 없지만",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.11",
          "form": "맞아 맞춰서 쓰면 좋을 거 같아.",
          "original_form": "맞아 맞춰서 쓰면 좋을 거 같아.",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.12",
          "form": "때미는거보다 좋을까?",
          "original_form": "때미는거보다 좋을까?",
          "speaker_id": "2",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.13",
          "form": "바디스크럽은 색이 있어..?",
          "original_form": "바디스크럽은 색이 있어..?",
          "speaker_id": "2",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.14",
          "form": "옥색?이야",
          "original_form": "옥색?이야",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.15",
          "form": "소금이랑 오일이 색이 있는 거 같아.",
          "original_form": "소금이랑 오일이 색이 있는 거 같아.",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.16",
          "form": "외국인들은 바디스크럽 쓴다는데",
          "original_form": "외국인들은 바디스크럽 쓴다는데",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.17",
          "form": "오 쓴다고 니가 옥색이 되지는 않지?",
          "original_form": "오 쓴다고 니가 옥색이 되지는 않지?",
          "speaker_id": "2",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.18",
          "form": "아무래도 소금이니까 자연친화적",
          "original_form": "아무래도 소금이니까 자연친화적",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.19",
          "form": "오 당연하지^^",
          "original_form": "오 당연하지^^",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002144.1.20",
          "form": "소금이면...먹으면 짤까m",
          "original_form": "소금이면...먹으면 짤까m",
          "speaker_id": "2",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002144.1.21",
          "form": "근데 계속 문지르면 아프더라 어쨌든 재구매 의사 있다",
          "original_form": "근데 계속 문지르면 아프더라 어쨌든 재구매 의사 있다",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
