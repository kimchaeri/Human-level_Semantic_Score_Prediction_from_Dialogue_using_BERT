      "utterance": [
        {
          "id": "MDRW2100039459.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039459.1.2",
          "form": "언제 개강하셨나용",
          "original_form": "언제 개강하셨나용",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039459.1.3",
          "form": "저용 9월 1일요ㅜㅜ",
          "original_form": "저용 9월 1일요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039459.1.4",
          "form": "아하,,,혹시 요새는 대학도 다 대면 수업인가요?",
          "original_form": "아하,,,혹시 요새는 대학도 다 대면 수업인가요?",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039459.1.5",
          "form": "전 아직 고등학교 다녀서용..",
          "original_form": "전 아직 고등학교 다녀서용..",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039459.1.6",
          "form": "아뇨! 저는 전공만 대면이고 교양은 비대면이에요~",
          "original_form": "아뇨! 저는 전공만 대면이고 교양은 비대면이에요~",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039459.1.7",
          "form": "아마 내년에는 싹 대면할거같아요!",
          "original_form": "아마 내년에는 싹 대면할거같아요!",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039459.1.8",
          "form": "헉 고등학생이시구나... 대학 어디 생각하고 계세요...?",
          "original_form": "헉 고등학생이시구나... 대학 어디 생각하고 계세요...?",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100039459.1.9",
          "form": "아하,, 저는 서울여대, 덕성여대, 아주대, 숭실대 이렇게 수시 쓰려구요!",
          "original_form": "아하,, 저는 서울여대, 덕성여대, 아주대, 숭실대 이렇게 수시 쓰려구요!",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039459.1.10",
          "form": "헐 멋있다... 전공 뭐로 하실거에요??",
          "original_form": "헐 멋있다... 전공 뭐로 하실거에요??",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039459.1.11",
          "form": "제 전공은 도자기라...",
          "original_form": "제 전공은 도자기라...",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039459.1.12",
          "form": "헉 도예과 신가요",
          "original_form": "헉 도예과 신가요",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039459.1.13",
          "form": "멋있으신데요,,,",
          "original_form": "멋있으신데요,,,",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039459.1.14",
          "form": "전 it미디어공학과 쪽이요!",
          "original_form": "전 it미디어공학과 쪽이요!",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039459.1.15",
          "form": "네 맞아요ㅋㅋ 고등학교를 그쪽으로 나왔어요",
          "original_form": "네 맞아요ㅋㅋ 고등학교를 그쪽으로 나왔어요",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039459.1.16",
          "form": "와 멋있다 저랑은 한참 먼 전공이네요ㅜㅜ",
          "original_form": "와 멋있다 저랑은 한참 먼 전공이네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039459.1.17",
          "form": "고등학교부터 길을 정해놓으셨다니 멋져요,,,",
          "original_form": "고등학교부터 길을 정해놓으셨다니 멋져요,,,",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039459.1.18",
          "form": "분야가 좀 다르긴 하네용",
          "original_form": "분야가 좀 다르긴 하네용",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039459.1.19",
          "form": "근데 전 항상 미대 친구들이랑 친해지고 싶었어요 ㅋㅋ",
          "original_form": "근데 전 항상 미대 친구들이랑 친해지고 싶었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039459.1.20",
          "form": "할 수 있는게 그거뿐이라 중학교 때 진로쌤이 고등학교 끌고갔어용ㅋㅋㅋㅋㅋㅋ",
          "original_form": "할 수 있는게 그거뿐이라 중학교 때 진로쌤이 고등학교 끌고갔어용ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039459.1.21",
          "form": "솔직히 말해서",
          "original_form": "솔직히 말해서",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039459.1.22",
          "form": "미대친구들 싹바가지 많아요 조심하세용,,,",
          "original_form": "미대친구들 싹바가지 많아요 조심하세용,,,",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039459.1.23",
          "form": "중학교 떄부터 도예에 재능이 있으셨다구요?? 능력자시네요,,,",
          "original_form": "중학교 떄부터 도예에 재능이 있으셨다구요?? 능력자시네요,,,",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039459.1.24",
          "form": "허헉...조심할게요....",
          "original_form": "허헉...조심할게요....",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039459.1.25",
          "form": "근데 약간 미대 특유의 자유로운? 분위기? 그런게 부러워 보였어요",
          "original_form": "근데 약간 미대 특유의 자유로운? 분위기? 그런게 부러워 보였어요",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039459.1.26",
          "form": "뭐든 당사자들은 생각이 다를 수 있겠지만..ㅎㅎ....",
          "original_form": "뭐든 당사자들은 생각이 다를 수 있겠지만..ㅎㅎ....",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039459.1.27",
          "form": "아눀ㅋㅋㅋ 도예는 고등학교 가서부터 했고 중학생때는 미니어쳐요 손재주가 있어서요!",
          "original_form": "아눀ㅋㅋㅋ 도예는 고등학교 가서부터 했고 중학생때는 미니어쳐요 손재주가 있어서요!",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039459.1.28",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039459.1.29",
          "form": "피어싱하고...",
          "original_form": "피어싱하고...",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039459.1.30",
          "form": "문신하고...",
          "original_form": "문신하고...",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039459.1.31",
          "form": "ㅋㅋㅋㅋ제가...그렇네요...",
          "original_form": "ㅋㅋㅋㅋ제가...그렇네요...",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039459.1.32",
          "form": "염색도 자유분방하게 하고 악세 장난아니고... 근데 또 그림쪽 아닌 친구들은 작업복 입고 학교가요",
          "original_form": "염색도 자유분방하게 하고 악세 장난아니고... 근데 또 그림쪽 아닌 친구들은 작업복 입고 학교가요",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039459.1.33",
          "form": "작업복이 따로 있어요?",
          "original_form": "작업복이 따로 있어요?",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039459.1.34",
          "form": "각자 알아서 구비하는 건가요?",
          "original_form": "각자 알아서 구비하는 건가요?",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039459.1.35",
          "form": "네! 고등학교때는 작업용 앞치마가 있었고",
          "original_form": "네! 고등학교때는 작업용 앞치마가 있었고",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039459.1.36",
          "form": "대학교때는 점프수트같은거 각자 사서 이름 새겨요!",
          "original_form": "대학교때는 점프수트같은거 각자 사서 이름 새겨요!",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039459.1.37",
          "form": "와...멋있어요",
          "original_form": "와...멋있어요",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039459.1.38",
          "form": "간지난다",
          "original_form": "간지난다",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039459.1.39",
          "form": "대학교 갈 때 옷 우중충하고 구리게 입는 친구들 있으면 도예과일거에요ㅋㅋㅋㅋ",
          "original_form": "대학교 갈 때 옷 우중충하고 구리게 입는 친구들 있으면 도예과일거에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:08"
        }
      ]
