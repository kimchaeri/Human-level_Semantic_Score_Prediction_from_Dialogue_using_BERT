      "utterance": [
        {
          "id": "MDRW2100010657.1.1",
          "form": "좋아하는 음식 무엇인가요,",
          "original_form": "좋아하는 음식 무엇인가요,",
          "speaker_id": "1",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010657.1.2",
          "form": "전 회,초밥이요",
          "original_form": "전 회,초밥이요",
          "speaker_id": "2",
          "time": "20210823 20:22"
        },
        {
          "id": "MDRW2100010657.1.3",
          "form": "오 저도 좋아해요 여름에도 드시는편인가요?",
          "original_form": "오 저도 좋아해요 여름에도 드시는편인가요?",
          "speaker_id": "1",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010657.1.4",
          "form": "신랑은 말리는데 전 자꾸 배달음식으로...끌리더라고요",
          "original_form": "신랑은 말리는데 전 자꾸 배달음식으로...끌리더라고요",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010657.1.5",
          "form": "저도 지난주에 물회랑 초밥먹었어요 ㅋ",
          "original_form": "저도 지난주에 물회랑 초밥먹었어요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010657.1.6",
          "form": "가을이 회가 단단해요 봄엔 살이 흐물거리는데ㅡ 지금부터 산란전까지 맛있대요",
          "original_form": "가을이 회가 단단해요 봄엔 살이 흐물거리는데ㅡ 지금부터 산란전까지 맛있대요",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010657.1.7",
          "form": "아 그렇군요 전 얼마전에 전어회도 먹었어요",
          "original_form": "아 그렇군요 전 얼마전에 전어회도 먹었어요",
          "speaker_id": "1",
          "time": "20210824 03:04"
        },
        {
          "id": "MDRW2100010657.1.8",
          "form": "전어회 뭐에 찍어드시는 걸 좋아하세요?",
          "original_form": "전어회 뭐에 찍어드시는 걸 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 12:57"
        },
        {
          "id": "MDRW2100010657.1.9",
          "form": "쌈장과 깻잎이랑 먹었어요 ㅋ",
          "original_form": "쌈장과 깻잎이랑 먹었어요 ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100010657.1.10",
          "form": "생각만해도 맛나겠네요!! 저녁엔 뭐해먹을까요 매일고민이네요",
          "original_form": "생각만해도 맛나겠네요!! 저녁엔 뭐해먹을까요 매일고민이네요",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100010657.1.11",
          "form": "맞아요... 오늘은 전복이 와서 먹었네요 뭐드셨나요",
          "original_form": "맞아요... 오늘은 전복이 와서 먹었네요 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100010657.1.12",
          "form": "와 어디서 사셨어요?",
          "original_form": "와 어디서 사셨어요?",
          "speaker_id": "2",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100010657.1.13",
          "form": "전단백질바랑 두유요..",
          "original_form": "전단백질바랑 두유요..",
          "speaker_id": "2",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100010657.1.14",
          "form": "지마켓 농활쿠폰으로 샀어요 식단관리중이신가요?",
          "original_form": "지마켓 농활쿠폰으로 샀어요 식단관리중이신가요?",
          "speaker_id": "1",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100010657.1.15",
          "form": "식욕이 안생기네요ㅜ",
          "original_form": "식욕이 안생기네요ㅜ",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100010657.1.16",
          "form": "뭘먹어야 맛있을까요?",
          "original_form": "뭘먹어야 맛있을까요?",
          "speaker_id": "2",
          "time": "20210824 18:48"
        }
      ]
