      "utterance": [
        {
          "id": "MDRW2100026143.1.1",
          "form": "ㅋㅋ아 제동생 이 어려운 시기에 남친 생겼어요",
          "original_form": "ㅋㅋ아 제동생 이 어려운 시기에 남친 생겼어요",
          "speaker_id": "2",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026143.1.2",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026143.1.3",
          "form": "우와 동생분 능력자시네요...!!!",
          "original_form": "우와 동생분 능력자시네요...!!!",
          "speaker_id": "1",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026143.1.4",
          "form": "저는 미혼입니다",
          "original_form": "저는 미혼입니다",
          "speaker_id": "1",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026143.1.5",
          "form": "저도 미혼이요ㅜ",
          "original_form": "저도 미혼이요ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026143.1.6",
          "form": "딱히 적극적으로 비혼이야 이건아닌데ㅜ",
          "original_form": "딱히 적극적으로 비혼이야 이건아닌데ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026143.1.7",
          "form": "저도요 ㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026143.1.8",
          "form": "소개팅 좋아하세요?",
          "original_form": "소개팅 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026143.1.9",
          "form": "근데 사람 만나는거에 적극적이지도 않고 ㅋㅋ",
          "original_form": "근데 사람 만나는거에 적극적이지도 않고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100026143.1.10",
          "form": "걍 내코가 석자 ㅋㅋㅋ",
          "original_form": "걍 내코가 석자 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026143.1.11",
          "form": "사실 저도요 ㅋㅋㅋ",
          "original_form": "사실 저도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026143.1.12",
          "form": "그리고 혼자가 편하더라구요",
          "original_form": "그리고 혼자가 편하더라구요",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026143.1.13",
          "form": "시국이 시국이라 소개팅도 찜찜하구요 ㅋㅋㅋ",
          "original_form": "시국이 시국이라 소개팅도 찜찜하구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100026143.1.14",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026143.1.15",
          "form": "저 그 mbti도 내향적으로 나왔어요",
          "original_form": "저 그 mbti도 내향적으로 나왔어요",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026143.1.16",
          "form": "혼자 잘 놀아요 ㅋㅋ",
          "original_form": "혼자 잘 놀아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026143.1.17",
          "form": "역시 mbti가 보증하는 내향성이 ㅋㅋㅋ",
          "original_form": "역시 mbti가 보증하는 내향성이 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026143.1.18",
          "form": "보통 혼자 놀러 다니시나요??",
          "original_form": "보통 혼자 놀러 다니시나요??",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026143.1.19",
          "form": "저는 종종 카페나 커플들 많이가는 곳 혼자 가면 커플들이 조금 부럽긴 하더라구요 솔로가 편한 거랑은 별개로 ㅋㅋ",
          "original_form": "저는 종종 카페나 커플들 많이가는 곳 혼자 가면 커플들이 조금 부럽긴 하더라구요 솔로가 편한 거랑은 별개로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026143.1.20",
          "form": "아 그 느낌 좀알거같아요",
          "original_form": "아 그 느낌 좀알거같아요",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100026143.1.21",
          "form": "평소에는 혼자라서 참편한데 가끔 쓸쓸한?",
          "original_form": "평소에는 혼자라서 참편한데 가끔 쓸쓸한?",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026143.1.22",
          "form": "그쵸그쵸 ㅋㅋㅋ",
          "original_form": "그쵸그쵸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026143.1.23",
          "form": "데이트 코스는 정말 혼자 돌아다니면 괜히 부러지고요 막 ㅋㅋㅋ",
          "original_form": "데이트 코스는 정말 혼자 돌아다니면 괜히 부러지고요 막 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026143.1.24",
          "form": "악 그느낌 알거같아요",
          "original_form": "악 그느낌 알거같아요",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026143.1.25",
          "form": "남들 둘이서하는걸",
          "original_form": "남들 둘이서하는걸",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026143.1.26",
          "form": "나는 혼자서 하고있을때 그느낌!",
          "original_form": "나는 혼자서 하고있을때 그느낌!",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026143.1.27",
          "form": "맞아요 마치 일당백처럼 ㅋㅋㅋ",
          "original_form": "맞아요 마치 일당백처럼 ㅋㅋㅋ",
