      "utterance": [
        {
          "id": "MDRW2100020727.1.1",
          "form": "밥 드셨나요",
          "original_form": "밥 드셨나요",
          "speaker_id": "2",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020727.1.2",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020727.1.3",
          "form": "전 아직 안먹었어요",
          "original_form": "전 아직 안먹었어요",
          "speaker_id": "2",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020727.1.4",
          "form": "아직이요 ㅎㅎ",
          "original_form": "아직이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020727.1.5",
          "form": "저 12시 넘어서 먹으려구요 좀 일찍 먹으면",
          "original_form": "저 12시 넘어서 먹으려구요 좀 일찍 먹으면",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020727.1.6",
          "form": "금방 배고파지고 저녁도 일찍 먹게 되고",
          "original_form": "금방 배고파지고 저녁도 일찍 먹게 되고",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020727.1.7",
          "form": "그러다보면 밤에 또 배고파서 야식의 유혹이...",
          "original_form": "그러다보면 밤에 또 배고파서 야식의 유혹이...",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020727.1.8",
          "form": "저도 12시 이후에 먹어야겠어요",
          "original_form": "저도 12시 이후에 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020727.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020727.1.10",
          "form": "야식 먹게되죠",
          "original_form": "야식 먹게되죠",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020727.1.11",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020727.1.12",
          "form": "그러다보면 살만 쪄요 ㅠㅠ",
          "original_form": "그러다보면 살만 쪄요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020727.1.13",
          "form": "그래서 아까부터 배고픈데 일부러 참고 있어요",
          "original_form": "그래서 아까부터 배고픈데 일부러 참고 있어요",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020727.1.14",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020727.1.15",
          "form": "점심 뭐 드시려구용?",
          "original_form": "점심 뭐 드시려구용?",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020727.1.16",
          "form": "그냥 빵이요",
          "original_form": "그냥 빵이요",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.17",
          "form": "근데 달걀좀 먹으려구요",
          "original_form": "근데 달걀좀 먹으려구요",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.18",
          "form": "빵으론 해결할 수 없는 허기",
          "original_form": "빵으론 해결할 수 없는 허기",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.19",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.21",
          "form": "안에 속재료 가득 좀 넣어봐요",
          "original_form": "안에 속재료 가득 좀 넣어봐요",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.22",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.23",
          "form": "베이컨 달걀 채소 등등",
          "original_form": "베이컨 달걀 채소 등등",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.24",
          "form": "토스트를 만들어 드세요",
          "original_form": "토스트를 만들어 드세요",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.25",
          "form": "냉장고에 있는 것들 다 넣어먹고",
          "original_form": "냉장고에 있는 것들 다 넣어먹고",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.26",
          "form": "토스트 맛있겠네요",
          "original_form": "토스트 맛있겠네요",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.27",
          "form": "급 의욕가득",
          "original_form": "급 의욕가득",
          "speaker_id": "2",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.28",
          "form": "옆에 우유 같은 것도 먹어요",
          "original_form": "옆에 우유 같은 것도 먹어요",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020727.1.29",
          "form": "우유에 에스프레소 하나 부어주면 더 좋구",
          "original_form": "우유에 에스프레소 하나 부어주면 더 좋구",
          "speaker_id": "1",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020727.1.30",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020727.1.31",
          "form": "든든한 한끼",
          "original_form": "든든한 한끼",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020727.1.32",
          "form": "먹어여겠어요",
          "original_form": "먹어여겠어요",
          "speaker_id": "2",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020727.1.33",
          "form": "저도 예전에 토스트 한때 빠졌어요",
          "original_form": "저도 예전에 토스트 한때 빠졌어요",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020727.1.34",
          "form": "그래서 슬라이스 치즈 한박스 사고 그랬는데 ㅋㅋㅋ",
          "original_form": "그래서 슬라이스 치즈 한박스 사고 그랬는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020727.1.35",
          "form": "슬라이스치즈",
          "original_form": "슬라이스치즈",
          "speaker_id": "2",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100020727.1.36",
          "form": "제대로된 토스트였네요",
          "original_form": "제대로된 토스트였네요",
          "speaker_id": "2",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100020727.1.37",
          "form": "거의 파는거 같은데요?",
          "original_form": "거의 파는거 같은데요?",
          "speaker_id": "2",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100020727.1.38",
          "form": "ㅋㅋㅋㅋㅋ파는 것보다 더 푸짐하게 했어요",
          "original_form": "ㅋㅋㅋㅋㅋ파는 것보다 더 푸짐하게 했어요",
          "speaker_id": "1",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100020727.1.39",
          "form": "그러니 거의 매일 토스트 먹었죠 ㅜㅜ",
          "original_form": "그러니 거의 매일 토스트 먹었죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100020727.1.40",
          "form": "이야~ 점심시간!",
          "original_form": "이야~ 점심시간!",
          "speaker_id": "1",
          "time": "20210828 12:05"
        }
      ]
    }
