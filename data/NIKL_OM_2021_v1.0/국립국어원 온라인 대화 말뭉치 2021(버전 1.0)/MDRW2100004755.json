      "utterance": [
        {
          "id": "MDRW2100004755.1.1",
          "form": "저녁인데 아직 좀 덥네",
          "original_form": "저녁인데 아직 좀 덥네",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.2",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.3",
          "form": "갑자기 비 오다 갯다가",
          "original_form": "갑자기 비 오다 갯다가",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.4",
          "form": "불지옥이다가 좀 나아지나 했더니",
          "original_form": "불지옥이다가 좀 나아지나 했더니",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.5",
          "form": "변화무쌍",
          "original_form": "변화무쌍",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.6",
          "form": "기후이상을 지나 기후위기라는 말이 실감나네",
          "original_form": "기후이상을 지나 기후위기라는 말이 실감나네",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.7",
          "form": "응 진짜",
          "original_form": "응 진짜",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.8",
          "form": "전세계가 다 이상하니까",
          "original_form": "전세계가 다 이상하니까",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.9",
          "form": "우리만 그런 것도 아니고",
          "original_form": "우리만 그런 것도 아니고",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004755.1.10",
          "form": "하루에도 날씨가 오락가락하고",
          "original_form": "하루에도 날씨가 오락가락하고",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004755.1.11",
          "form": "여름엔 이상고온 겨울엔 한파...",
          "original_form": "여름엔 이상고온 겨울엔 한파...",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004755.1.12",
          "form": "유럽에서 더위에 사람들 죽고",
          "original_form": "유럽에서 더위에 사람들 죽고",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004755.1.13",
          "form": "캐나다에선 홍합이 바닷물에 익고",
          "original_form": "캐나다에선 홍합이 바닷물에 익고",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004755.1.14",
          "form": "아주 몸살을 앓네",
          "original_form": "아주 몸살을 앓네",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004755.1.15",
          "form": "ㅠㅠ 걱정이다 진짜",
          "original_form": "ㅠㅠ 걱정이다 진짜",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004755.1.16",
          "form": "우리도 걱정인데 우리보다 이런 기후상황에서 오래살아야 할 다음 세대들도 큰일이야",
          "original_form": "우리도 걱정인데 우리보다 이런 기후상황에서 오래살아야 할 다음 세대들도 큰일이야",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004755.1.17",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004755.1.18",
          "form": "미세먼지나 이상고온, 한파가 없는 날이 거의 없으니까",
          "original_form": "미세먼지나 이상고온, 한파가 없는 날이 거의 없으니까",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004755.1.19",
          "form": "점점 심해지고 있고...",
          "original_form": "점점 심해지고 있고...",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004755.1.20",
          "form": "그러게말야",
          "original_form": "그러게말야",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004755.1.21",
          "form": "지금 코로나도 기후위기랑 연관되는거 같고",
          "original_form": "지금 코로나도 기후위기랑 연관되는거 같고",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004755.1.22",
          "form": "응 위기지 위기야",
          "original_form": "응 위기지 위기야",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004755.1.23",
          "form": "얼마전에 기사보니까 빙하가 녹으면서 그속에 있는 바이러스도 풀려날 수 있다 하더라고",
          "original_form": "얼마전에 기사보니까 빙하가 녹으면서 그속에 있는 바이러스도 풀려날 수 있다 하더라고",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004755.1.24",
          "form": "우리가 뭘 실천해야하나 이런 고민을 해야하는데",
          "original_form": "우리가 뭘 실천해야하나 이런 고민을 해야하는데",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004755.1.25",
          "form": "고대의 병 걸리면 무슨 영화도 아니고",
          "original_form": "고대의 병 걸리면 무슨 영화도 아니고",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004755.1.26",
          "form": "나는 너무 문외한이라 반성합니다",
          "original_form": "나는 너무 문외한이라 반성합니다",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004755.1.27",
          "form": "플라스틱 줄이기 프로젝트도 하시고",
          "original_form": "플라스틱 줄이기 프로젝트도 하시고",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004755.1.28",
          "form": "열심이시잖아요",
          "original_form": "열심이시잖아요",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004755.1.29",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004755.1.30",
          "form": "그래도 코로나 이후 마스크 쓰면서 미세먼지 걱정은 좀 덜하는듯",
          "original_form": "그래도 코로나 이후 마스크 쓰면서 미세먼지 걱정은 좀 덜하는듯",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004755.1.31",
          "form": "어쨌든 94쓰니까.. ㅋㅋ",
          "original_form": "어쨌든 94쓰니까.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004755.1.32",
          "form": "미세먼지 얘기는 전보다 좀 줄어든 것 같기도..?",
          "original_form": "미세먼지 얘기는 전보다 좀 줄어든 것 같기도..?",
          "speaker_id": "1",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004755.1.33",
          "form": "그러네 코로나가 더 심해서 그런가",
          "original_form": "그러네 코로나가 더 심해서 그런가",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004755.1.34",
          "form": "아님 코로나땜에 외출을 많이 안해서 미세먼지 와도 안나가서 관계가 없었나",
          "original_form": "아님 코로나땜에 외출을 많이 안해서 미세먼지 와도 안나가서 관계가 없었나",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004755.1.35",
          "form": "올해는 그 추위랑 더위땜에 미세먼지 별로 없었단 뉴스는 본거같아",
          "original_form": "올해는 그 추위랑 더위땜에 미세먼지 별로 없었단 뉴스는 본거같아",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004755.1.36",
          "form": "그렇구만!",
          "original_form": "그렇구만!",
          "speaker_id": "1",
          "time": "20210821 18:34"
        }
      ]
    }
