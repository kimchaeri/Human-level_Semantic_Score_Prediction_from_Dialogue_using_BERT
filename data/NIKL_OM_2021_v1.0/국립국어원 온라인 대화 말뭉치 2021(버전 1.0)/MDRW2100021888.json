      "utterance": [
        {
          "id": "MDRW2100021888.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021888.1.2",
          "form": "여행 이동하는게 싫어서 그렇지 가면 잘 놀긴해요 ㅋㅋㅋ",
          "original_form": "여행 이동하는게 싫어서 그렇지 가면 잘 놀긴해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021888.1.3",
          "form": "저도 이동하는 거 힘들어해요ㅠㅠ 차멀미해서ㅠㅠ",
          "original_form": "저도 이동하는 거 힘들어해요ㅠㅠ 차멀미해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021888.1.4",
          "form": "헐 저도요저도요 ㅜㅜ멀미심하다하면 다들 이해못해줘요 ㅜㅜ",
          "original_form": "헐 저도요저도요 ㅜㅜ멀미심하다하면 다들 이해못해줘요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021888.1.5",
          "form": "왜 이해 못해주지ㅠㅠ 진짜 심할 땐 차에서 뛰어내리고 싶은데ㅠㅠ",
          "original_form": "왜 이해 못해주지ㅠㅠ 진짜 심할 땐 차에서 뛰어내리고 싶은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021888.1.6",
          "form": "전 아빠차탈때는 거의 누워만있어욬ㅋㅋㅋㅋㅋ",
          "original_form": "전 아빠차탈때는 거의 누워만있어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021888.1.7",
          "form": "저도 차 타면 자려고 노력하고 거의 기절 잠 자요 ㅋㅋㅋ",
          "original_form": "저도 차 타면 자려고 노력하고 거의 기절 잠 자요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:53"
        },
        {
          "id": "MDRW2100021888.1.8",
          "form": "혹시 배타고 여행해보셨어요? ㅋㅋ 최악이었어요...",
          "original_form": "혹시 배타고 여행해보셨어요? ㅋㅋ 최악이었어요...",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100021888.1.9",
          "form": "배멀미를 더 심하게 해서 안 해욬ㅋㅋㅋㅋㅌ",
          "original_form": "배멀미를 더 심하게 해서 안 해욬ㅋㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100021888.1.10",
          "form": "여행 기념품중 가장 맘에드는거 뭐에요?",
          "original_form": "여행 기념품중 가장 맘에드는거 뭐에요?",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100021888.1.11",
          "form": "악세사리 종류요 예쁜 거 다 좋아해요",
          "original_form": "악세사리 종류요 예쁜 거 다 좋아해요",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100021888.1.12",
          "form": "악세사리 좋아하시는구나!! 전 몸에 뭐걸치는게 싫어서 귀도 안뚫었는데 악세사리 구경은 좋아해요! 반짝이고 작은것들 ㅋㅋㅋ",
          "original_form": "악세사리 좋아하시는구나!! 전 몸에 뭐걸치는게 싫어서 귀도 안뚫었는데 악세사리 구경은 좋아해요! 반짝이고 작은것들 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100021888.1.13",
          "form": "저도 무서워서 귀는 안 뚫었어요",
          "original_form": "저도 무서워서 귀는 안 뚫었어요",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100021888.1.14",
          "form": "반짝이는 거 넘 좋지 않아요? 보기만 해도 좋아요",
          "original_form": "반짝이는 거 넘 좋지 않아요? 보기만 해도 좋아요",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100021888.1.15",
          "form": "그럼 주로 목걸이나 반지사시나요?",
          "original_form": "그럼 주로 목걸이나 반지사시나요?",
          "speaker_id": "2",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100021888.1.16",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210829 19:49"
        }
      ]
