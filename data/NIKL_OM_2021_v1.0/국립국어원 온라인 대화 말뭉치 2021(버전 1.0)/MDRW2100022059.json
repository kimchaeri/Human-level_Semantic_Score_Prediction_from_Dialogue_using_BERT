      "utterance": [
        {
          "id": "MDRW2100022059.1.1",
          "form": "요즘 심심하면 어떤 거 하니",
          "original_form": "요즘 심심하면 어떤 거 하니",
          "speaker_id": "2",
          "time": "20210829 01:43"
        },
        {
          "id": "MDRW2100022059.1.2",
          "form": "요즘은 귀찮아서 누워서 핸드폰만 해",
          "original_form": "요즘은 귀찮아서 누워서 핸드폰만 해",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.3",
          "form": "핸드폰을 무슨 재미로 해",
          "original_form": "핸드폰을 무슨 재미로 해",
          "speaker_id": "2",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.4",
          "form": "온라인 커뮤니티 모르는구나",
          "original_form": "온라인 커뮤니티 모르는구나",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.5",
          "form": "그게 뭔데",
          "original_form": "그게 뭔데",
          "speaker_id": "2",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.6",
          "form": "나와 같은 관심사에 사람들이 인터넷에 모여 있는 거야",
          "original_form": "나와 같은 관심사에 사람들이 인터넷에 모여 있는 거야",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.7",
          "form": "친구들을 인터넷에서 만나는 거구나",
          "original_form": "친구들을 인터넷에서 만나는 거구나",
          "speaker_id": "2",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.8",
          "form": "그렇다고 볼 수 있지",
          "original_form": "그렇다고 볼 수 있지",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.9",
          "form": "그거 하면 시간이 잘 가니",
          "original_form": "그거 하면 시간이 잘 가니",
          "speaker_id": "2",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.10",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.11",
          "form": "너가 좋아하는 것에 대한 이야기를 나누니",
          "original_form": "너가 좋아하는 것에 대한 이야기를 나누니",
          "speaker_id": "2",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.12",
          "form": "맞아 그래서 무척 즐거워",
          "original_form": "맞아 그래서 무척 즐거워",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022059.1.13",
          "form": "온라인 커뮤니티는 활성화되어 있니",
          "original_form": "온라인 커뮤니티는 활성화되어 있니",
          "speaker_id": "2",
          "time": "20210829 01:45"
        },
        {
          "id": "MDRW2100022059.1.14",
          "form": "그렇게 된 지 오래 됐을걸",
          "original_form": "그렇게 된 지 오래 됐을걸",
          "speaker_id": "1",
          "time": "20210829 01:45"
