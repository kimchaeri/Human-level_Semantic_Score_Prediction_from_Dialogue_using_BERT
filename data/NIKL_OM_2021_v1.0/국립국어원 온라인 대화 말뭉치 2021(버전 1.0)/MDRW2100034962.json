      "utterance": [
        {
          "id": "MDRW2100034962.1.1",
          "form": "저는 친구따라다녀서 잘모르는데",
          "original_form": "저는 친구따라다녀서 잘모르는데",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100034962.1.2",
          "form": "거긴안가봤네요",
          "original_form": "거긴안가봤네요",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100034962.1.3",
          "form": "어떤곳이죠",
          "original_form": "어떤곳이죠",
          "speaker_id": "2",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100034962.1.4",
          "form": "여기 영화관 스파 화장품 음식점 옷 신발 가전",
          "original_form": "여기 영화관 스파 화장품 음식점 옷 신발 가전",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100034962.1.5",
          "form": "다 들어가있어요",
          "original_form": "다 들어가있어요",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100034962.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.7",
          "form": "이번에 또 새로 짓는다고 하던데",
          "original_form": "이번에 또 새로 짓는다고 하던데",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.8",
          "form": "크리스마스쯤 가면 세일 많이해요",
          "original_form": "크리스마스쯤 가면 세일 많이해요",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.9",
          "form": "찾아봐야겠네요",
          "original_form": "찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.10",
          "form": "오호라",
          "original_form": "오호라",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.11",
          "form": "브랜드마다 독립된 매장들이 있어서",
          "original_form": "브랜드마다 독립된 매장들이 있어서",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.12",
          "form": "쇼핑하기 편해요",
          "original_form": "쇼핑하기 편해요",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.13",
          "form": "근데 사람붐비는데에 안가고싶은데 아 혹하네요",
          "original_form": "근데 사람붐비는데에 안가고싶은데 아 혹하네요",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.14",
          "form": "신세계라 트레이더스도 붙어있어요",
          "original_form": "신세계라 트레이더스도 붙어있어요",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.15",
          "form": "아아ㅠㅠ 그쵸ㅠㅠ",
          "original_form": "아아ㅠㅠ 그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034962.1.16",
          "form": "오 유용하네요",
          "original_form": "오 유용하네요",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.17",
          "form": "노브랜드도있고 진짜 여기 들어가면",
          "original_form": "노브랜드도있고 진짜 여기 들어가면",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.18",
          "form": "개미지옥이에요ㅋㅋㅋㅋㅋ",
          "original_form": "개미지옥이에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.19",
          "form": "코스트코에 딥티크향수도 싸게파는거 인상적이었어요",
          "original_form": "코스트코에 딥티크향수도 싸게파는거 인상적이었어요",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.20",
          "form": "십만원씩은 못해도 쓰고와요",
          "original_form": "십만원씩은 못해도 쓰고와요",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.21",
          "form": "노브랜드도 살게 많죠",
          "original_form": "노브랜드도 살게 많죠",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.22",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.23",
          "form": "으억 ㅋㅋ",
          "original_form": "으억 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.24",
          "form": "딥디크!",
          "original_form": "딥디크!",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.25",
          "form": "단위가 크다",
          "original_form": "단위가 크다",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.26",
          "form": "좀 비싸요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "좀 비싸요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034962.1.27",
          "form": "노브랜드 싸다고 소문난덴데",
          "original_form": "노브랜드 싸다고 소문난덴데",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100034962.1.28",
          "form": "하나둘사고나면비싼가봐요",
          "original_form": "하나둘사고나면비싼가봐요",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100034962.1.29",
          "form": "노브랜드 그래도 생필품은 좀 싼것같아요!",
          "original_form": "노브랜드 그래도 생필품은 좀 싼것같아요!",
          "speaker_id": "1",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100034962.1.30",
          "form": "저 핫딜 사이트 이런거 자주 가는데",
          "original_form": "저 핫딜 사이트 이런거 자주 가는데",
          "speaker_id": "1",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100034962.1.31",
          "form": "네이버에 이런 핫딜 공유하는 카페 많더라구요",
          "original_form": "네이버에 이런 핫딜 공유하는 카페 많더라구요",
          "speaker_id": "1",
          "time": "20210828 22:59"
        }
      ]
    }
