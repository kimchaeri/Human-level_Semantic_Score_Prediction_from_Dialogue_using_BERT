      "utterance": [
        {
          "id": "MDRW2100039076.1.1",
          "form": "오늘 정말 가을같아요ㅎㅎ",
          "original_form": "오늘 정말 가을같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039076.1.2",
          "form": "너무좋네요 날씨",
          "original_form": "너무좋네요 날씨",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039076.1.3",
          "form": "와 그렁가요 여긴 좀 흐려요 ㅠㅠ며칠내내",
          "original_form": "와 그렁가요 여긴 좀 흐려요 ㅠㅠ며칠내내",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100039076.1.4",
          "form": "오그러시구나 이쪽은 어제는 그랬고 오늘은 맑아요 ㅎㅎ",
          "original_form": "오그러시구나 이쪽은 어제는 그랬고 오늘은 맑아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100039076.1.5",
          "form": "가을옷을 입고나왔답니닼ㅋㅋ",
          "original_form": "가을옷을 입고나왔답니닼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100039076.1.6",
          "form": "우와 개시하셨군요",
          "original_form": "우와 개시하셨군요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039076.1.7",
          "form": "넵 ㅎㅎ 니트에 카멜색 바지에 워커입니다!",
          "original_form": "넵 ㅎㅎ 니트에 카멜색 바지에 워커입니다!",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039076.1.8",
          "form": "왕왕 너무 이쁘겠어요오오",
          "original_form": "왕왕 너무 이쁘겠어요오오",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100039076.1.9",
          "form": "니트 아직 안더우세용?ㅠㅠ",
          "original_form": "니트 아직 안더우세용?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100039076.1.10",
          "form": "니트가 7부라 괜찮네요ㅎㅎ",
          "original_form": "니트가 7부라 괜찮네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100039076.1.11",
          "form": "근데 매미가 우네요..ㅋㅋ ㅠ",
          "original_form": "근데 매미가 우네요..ㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100039076.1.12",
          "form": "맞아요 매미랑 귀뚜라미 동시에 울어요",
          "original_form": "맞아요 매미랑 귀뚜라미 동시에 울어요",
          "speaker_id": "1",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100039076.1.13",
          "form": "마자옄ㅋㅋㅋㅋ하늘은 쨍해서 예쁘네요",
          "original_form": "마자옄ㅋㅋㅋㅋ하늘은 쨍해서 예쁘네요",
          "speaker_id": "2",
          "time": "20210903 15:11"
        },
        {
          "id": "MDRW2100039076.1.14",
          "form": "와 저희는 내내 흐리네요 오늘 어디가세요?",
          "original_form": "와 저희는 내내 흐리네요 오늘 어디가세요?",
          "speaker_id": "1",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100039076.1.15",
          "form": "날씨는조은데ㅠㅠㅋ퇴근후 바로집이요ㅎㅎ",
          "original_form": "날씨는조은데ㅠㅠㅋ퇴근후 바로집이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039076.1.16",
          "form": "오늘 어디라도 가셔요ㅠㅠ 날좋은날이 요줌 며칠 안됑슈ㅠㅋㅋㅋ",
          "original_form": "오늘 어디라도 가셔요ㅠㅠ 날좋은날이 요줌 며칠 안됑슈ㅠㅋㅋㅋ",
