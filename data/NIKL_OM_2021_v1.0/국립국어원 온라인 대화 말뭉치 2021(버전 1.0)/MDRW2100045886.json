          "original_form": "후..",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.5",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.6",
          "form": "같은제품 깔별로모으기",
          "original_form": "같은제품 깔별로모으기",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.7",
          "form": "그 가죽 원피스도",
          "original_form": "그 가죽 원피스도",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.8",
          "form": "검정색 분홍색 샀어요",
          "original_form": "검정색 분홍색 샀어요",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.10",
          "form": "왜냐면",
          "original_form": "왜냐면",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.11",
          "form": "다 갖고싶으니까",
          "original_form": "다 갖고싶으니까",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.12",
          "form": "하ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "하ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.13",
          "form": "근데그건너무다르네요",
          "original_form": "근데그건너무다르네요",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.14",
          "form": "다른제품수준",
          "original_form": "다른제품수준",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.15",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.16",
          "form": "색이너무달라서",
          "original_form": "색이너무달라서",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.17",
          "form": "검정은 약간 기본이고",
          "original_form": "검정은 약간 기본이고",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.19",
          "form": "분홍은 좀 도발적이죠",
          "original_form": "분홍은 좀 도발적이죠",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.20",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "2",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100045886.1.21",
          "form": "저도 어제 가방사는데",
          "original_form": "저도 어제 가방사는데",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045886.1.22",
          "form": "요즘은 가방이 너무 갖고싶",
          "original_form": "요즘은 가방이 너무 갖고싶",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045886.1.23",
          "form": "헐",
          "original_form": "헐",
