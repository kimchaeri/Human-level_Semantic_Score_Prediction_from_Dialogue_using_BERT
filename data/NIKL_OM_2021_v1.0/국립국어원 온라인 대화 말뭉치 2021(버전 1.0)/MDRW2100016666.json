      "utterance": [
        {
          "id": "MDRW2100016666.1.1",
          "form": "가장 좋았던 여행지 있으세요 ?",
          "original_form": "가장 좋았던 여행지 있으세요 ?",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016666.1.2",
          "form": "코타키나발루가 좋았어요",
          "original_form": "코타키나발루가 좋았어요",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016666.1.3",
          "form": "아 저도 가보고 싶은 곳이였는데 ㅠㅠ",
          "original_form": "아 저도 가보고 싶은 곳이였는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016666.1.4",
          "form": "힐링하러 가기 좋다더라구요!",
          "original_form": "힐링하러 가기 좋다더라구요!",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016666.1.5",
          "form": "이쁘고 물가 싼 휴양지라 정말 만족했어요",
          "original_form": "이쁘고 물가 싼 휴양지라 정말 만족했어요",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016666.1.6",
          "form": "오 물가가 싼가요 ??",
          "original_form": "오 물가가 싼가요 ??",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016666.1.7",
          "form": "그런곳은 뭔가 잔뜩 사서 들어와야되는데 ㅎㅎ",
          "original_form": "그런곳은 뭔가 잔뜩 사서 들어와야되는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016666.1.8",
          "form": "망고젤리 엄청 사왔어요ㅋㅋ",
          "original_form": "망고젤리 엄청 사왔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016666.1.9",
          "form": "아 망고젤리 엄청 좋아요 ㅋㅋ 약간 필수템인듯한 ㅎㅎ",
          "original_form": "아 망고젤리 엄청 좋아요 ㅋㅋ 약간 필수템인듯한 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016666.1.10",
          "form": "게리 과자 같은것도 있나요?",
          "original_form": "게리 과자 같은것도 있나요?",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016666.1.11",
          "form": "그건 못 봤던 것 같아요!",
          "original_form": "그건 못 봤던 것 같아요!",
          "speaker_id": "2",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016666.1.12",
          "form": "하긴 그거는 한국도 싸더라구요 ㅎㅎ",
          "original_form": "하긴 그거는 한국도 싸더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016666.1.13",
          "form": "호텔에서 막 수영하고 그러던데 수영도 하셨나요!",
          "original_form": "호텔에서 막 수영하고 그러던데 수영도 하셨나요!",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016666.1.14",
          "form": "호텔 수영장에서도 하고 바다에서도 했어요ㅎㅎ",
          "original_form": "호텔 수영장에서도 하고 바다에서도 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016666.1.15",
          "form": "부럽네요 ㅠㅠ 바다에서 수영 안한지가 언젠지 ㅎㅎ",
          "original_form": "부럽네요 ㅠㅠ 바다에서 수영 안한지가 언젠지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016666.1.16",
          "form": "가는데 몇시간정도 걸려요?",
          "original_form": "가는데 몇시간정도 걸려요?",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016666.1.17",
          "form": "비행기시간은 잘 기억이 안 나요ㅠ",
          "original_form": "비행기시간은 잘 기억이 안 나요ㅠ",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016666.1.18",
          "form": "한 5시간정도 걸리나봐요 ㅎㅎ 가깝네요 !",
          "original_form": "한 5시간정도 걸리나봐요 ㅎㅎ 가깝네요 !",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016666.1.19",
          "form": "전 외국은 일본만 가봤어요 예전에 ㅠㅠ",
          "original_form": "전 외국은 일본만 가봤어요 예전에 ㅠㅠ",
