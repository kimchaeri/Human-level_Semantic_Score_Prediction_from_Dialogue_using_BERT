      "utterance": [
        {
          "id": "MDRW2100019216.1.1",
          "form": "발목이 원래 약하셨나요 ㅋㅋㅋ",
          "original_form": "발목이 원래 약하셨나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019216.1.2",
          "form": "원래는 튼튼했는데 살찌니까 한순간이더라구요ㅋㅋㅋ",
          "original_form": "원래는 튼튼했는데 살찌니까 한순간이더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019216.1.3",
          "form": "운동화 어떤데 발목 잡아주지ㅠㅠ 안정화?",
          "original_form": "운동화 어떤데 발목 잡아주지ㅠㅠ 안정화?",
          "speaker_id": "1",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019216.1.4",
          "form": "진짜 건강 잘챙겨야해여",
          "original_form": "진짜 건강 잘챙겨야해여",
          "speaker_id": "1",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019216.1.5",
          "form": "맞아요 요즘 그래서 열심히 다이어트하고 있어요",
          "original_form": "맞아요 요즘 그래서 열심히 다이어트하고 있어요",
          "speaker_id": "2",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100019216.1.6",
          "form": "다이어트 어케하세요",
          "original_form": "다이어트 어케하세요",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019216.1.7",
          "form": "저 식이는 안하고 홈트 열심히해요",
          "original_form": "저 식이는 안하고 홈트 열심히해요",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019216.1.8",
          "form": "실내사이클 막 돌리고",
          "original_form": "실내사이클 막 돌리고",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019216.1.9",
          "form": "유튜브도 따라하고",
          "original_form": "유튜브도 따라하고",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019216.1.10",
          "form": "오 ㅋㅋㅋ  유튭 조아하시는 홈트 잇으세요?",
          "original_form": "오 ㅋㅋㅋ  유튭 조아하시는 홈트 잇으세요?",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019216.1.11",
          "form": "저 땅크부부랑 빅씨스 자주해요!",
          "original_form": "저 땅크부부랑 빅씨스 자주해요!",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019216.1.12",
          "form": "빅씨스 영상 풍경이 시원시원해서 좋아요",
          "original_form": "빅씨스 영상 풍경이 시원시원해서 좋아요",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019216.1.13",
          "form": "빅씨스는 첨들어봤는데 함 봐야겟네요 ㅋㅋ",
          "original_form": "빅씨스는 첨들어봤는데 함 봐야겟네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019216.1.14",
          "form": "네네ㅋㅋㅋ 40대 여자분이신데 롤모델 삼고싶어요",
          "original_form": "네네ㅋㅋㅋ 40대 여자분이신데 롤모델 삼고싶어요",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019216.1.15",
          "form": "운동 유툽하는분들 정말 멋지죵",
          "original_form": "운동 유툽하는분들 정말 멋지죵",
          "speaker_id": "1",
          "time": "20210827 18:19"
        },
        {
          "id": "MDRW2100019216.1.16",
          "form": "네 다른 유튜버분들도 다 대단하시더라구요",
          "original_form": "네 다른 유튜버분들도 다 대단하시더라구요",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019216.1.17",
          "form": "어떻게 영상 내내 웃으며 운동을 하는지..",
          "original_form": "어떻게 영상 내내 웃으며 운동을 하는지..",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
