      "utterance": [
        {
          "id": "MDRW2100018823.1.1",
          "form": "그리고 무슨 악마? 도 나와 로아에",
          "original_form": "그리고 무슨 악마? 도 나와 로아에",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.2",
          "form": "악마??",
          "original_form": "악마??",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.3",
          "form": "내가 전사고 악마랑 싸우마봐",
          "original_form": "내가 전사고 악마랑 싸우마봐",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.4",
          "form": "나봐",
          "original_form": "나봐",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.6",
          "form": "먼가 웃기다",
          "original_form": "먼가 웃기다",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.8",
          "form": "약간 중세시대 컨셉같애",
          "original_form": "약간 중세시대 컨셉같애",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.9",
          "form": "그런가뷰ㅏ",
          "original_form": "그런가뷰ㅏ",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.10",
          "form": "나폴레옹 이때 시댄가?",
          "original_form": "나폴레옹 이때 시댄가?",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.11",
          "form": "오..그럴듯",
          "original_form": "오..그럴듯",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.12",
          "form": "스토리 스킵만 된다면 해보고싶다",
          "original_form": "스토리 스킵만 된다면 해보고싶다",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.13",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018823.1.14",
          "form": "피시방가서 큰 화면으로",
          "original_form": "피시방가서 큰 화면으로",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.15",
          "form": "맞어ㅜ",
          "original_form": "맞어ㅜ",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.16",
          "form": "피시방 게임이 젤 재밋어",
          "original_form": "피시방 게임이 젤 재밋어",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.17",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.18",
          "form": "이젠 피시방도 코로나때메못가고",
          "original_form": "이젠 피시방도 코로나때메못가고",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.19",
          "form": "게임하고싶지만",
          "original_form": "게임하고싶지만",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.20",
          "form": "참아야지..어쩌겟어",
          "original_form": "참아야지..어쩌겟어",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.21",
          "form": "그래ㅠㅠ",
          "original_form": "그래ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.22",
          "form": "로아 노트북으로는 안되려나",
          "original_form": "로아 노트북으로는 안되려나",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.23",
          "form": "그 사양이 커서",
          "original_form": "그 사양이 커서",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.24",
          "form": "컴퓨터 좋은 게이밍컴퓨터로 해야한대",
          "original_form": "컴퓨터 좋은 게이밍컴퓨터로 해야한대",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.25",
          "form": "아그래?",
          "original_form": "아그래?",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018823.1.26",
          "form": "피시방에서 메이플하구싶다",
          "original_form": "피시방에서 메이플하구싶다",
          "speaker_id": "1",
          "time": "20210827 12:33"
        }
      ]
    }
