      "utterance": [
        {
          "id": "MDRW2100043251.1.1",
          "form": "얼마죠얼마죠???",
          "original_form": "얼마죠얼마죠???",
          "speaker_id": "2",
          "time": "20210907 16:17"
        },
        {
          "id": "MDRW2100043251.1.2",
          "form": "팔렸나봐요",
          "original_form": "팔렸나봐요",
          "speaker_id": "1",
          "time": "20210907 16:17"
        },
        {
          "id": "MDRW2100043251.1.3",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "1",
          "time": "20210907 16:17"
        },
        {
          "id": "MDRW2100043251.1.4",
          "form": "당근이대단해요",
          "original_form": "당근이대단해요",
          "speaker_id": "1",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100043251.1.5",
          "form": "지금 다들 플립 키알걸고",
          "original_form": "지금 다들 플립 키알걸고",
          "speaker_id": "2",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100043251.1.6",
          "form": "플립 세컨폰으로 쓰시는 분도 봤어요",
          "original_form": "플립 세컨폰으로 쓰시는 분도 봤어요",
          "speaker_id": "1",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100043251.1.7",
          "form": "아낀다고 ㅋㅋ",
          "original_form": "아낀다고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100043251.1.8",
          "form": "그 경제력이 부럽다",
          "original_form": "그 경제력이 부럽다",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043251.1.9",
          "form": "플립이 세컨폰이라나",
          "original_form": "플립이 세컨폰이라나",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043251.1.10",
          "form": "니",
          "original_form": "니",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043251.1.11",
          "form": "스포츠카도 아닌데",
          "original_form": "스포츠카도 아닌데",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043251.1.12",
          "form": "두개임",
          "original_form": "두개임",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043251.1.13",
          "form": "무슨색 갖고싶으세요",
          "original_form": "무슨색 갖고싶으세요",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043251.1.14",
          "form": "라벤더요!",
          "original_form": "라벤더요!",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043251.1.15",
          "form": "그죠그죠 라벤더",
          "original_form": "그죠그죠 라벤더",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043251.1.16",
          "form": "예쁘죠아주",
          "original_form": "예쁘죠아주",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043251.1.17",
          "form": "이번에 그린도 색깔 너무 고급지게 잘뽑아서",
          "original_form": "이번에 그린도 색깔 너무 고급지게 잘뽑아서",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043251.1.18",
          "form": "진짜 눈돌아가요",
          "original_form": "진짜 눈돌아가요",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043251.1.19",
          "form": "예판 말고는 할인소식 없죠?",
          "original_form": "예판 말고는 할인소식 없죠?",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100043251.1.20",
          "form": "네... 그래서 지금사면 호구",
          "original_form": "네... 그래서 지금사면 호구",
          "speaker_id": "2",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100043251.1.21",
          "form": "아이폰으로 바꿀생각중이었는데 갑자기 엄청 이쁘게나와서",
          "original_form": "아이폰으로 바꿀생각중이었는데 갑자기 엄청 이쁘게나와서",
          "speaker_id": "2",
          "time": "20210907 16:56"
        },
        {
          "id": "MDRW2100043251.1.22",
          "form": "흔드네요",
          "original_form": "흔드네요",
          "speaker_id": "2",
          "time": "20210907 16:56"
        }
      ]
