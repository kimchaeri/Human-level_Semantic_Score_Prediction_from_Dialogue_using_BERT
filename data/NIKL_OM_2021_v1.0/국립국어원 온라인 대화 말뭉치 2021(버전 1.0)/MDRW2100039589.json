          "original_form": "그렇게 만든듯요",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.4",
          "form": "어떰데요?",
          "original_form": "어떰데요?",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.5",
          "form": "휘리릭 돌아가요",
          "original_form": "휘리릭 돌아가요",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.6",
          "form": "일자로 안가고요?",
          "original_form": "일자로 안가고요?",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.8",
          "form": "왕답답!!",
          "original_form": "왕답답!!",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.9",
          "form": "헉 그럼 어떡해요",
          "original_form": "헉 그럼 어떡해요",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.10",
          "form": "두 손으로 하고 있어요 ㅠㅠㅠ",
          "original_form": "두 손으로 하고 있어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.11",
          "form": "불편하겠어요",
          "original_form": "불편하겠어요",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.12",
          "form": "네 오히려 더 불편해졌어요",
          "original_form": "네 오히려 더 불편해졌어요",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.13",
          "form": "반품 안되죠?",
          "original_form": "반품 안되죠?",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039589.1.14",
          "form": "네 ㅜ 이미 써서 ㅠㅠ",
          "original_form": "네 ㅜ 이미 써서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039589.1.15",
          "form": "삼성 걸러야겠군요",
          "original_form": "삼성 걸러야겠군요",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039589.1.16",
          "form": "그래도 새로 산 청소기는",
          "original_form": "그래도 새로 산 청소기는",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039589.1.17",
