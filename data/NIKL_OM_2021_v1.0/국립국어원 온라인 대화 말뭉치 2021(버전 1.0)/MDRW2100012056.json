      "utterance": [
        {
          "id": "MDRW2100012056.1.1",
          "form": "스포츠 관심 있으신가요?",
          "original_form": "스포츠 관심 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012056.1.2",
          "form": "스포츠 올림픽만 보는듯요",
          "original_form": "스포츠 올림픽만 보는듯요",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012056.1.3",
          "form": "저도 이번 올림픽 봤어요",
          "original_form": "저도 이번 올림픽 봤어요",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012056.1.4",
          "form": "양궁 보셨어요?",
          "original_form": "양궁 보셨어요?",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012056.1.5",
          "form": "그럼요~",
          "original_form": "그럼요~",
          "speaker_id": "1",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012056.1.6",
          "form": "심박수 나오는거 너무신기하지않던가요?",
          "original_form": "심박수 나오는거 너무신기하지않던가요?",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012056.1.7",
          "form": "저는 너무 신기하저라구요",
          "original_form": "저는 너무 신기하저라구요",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012056.1.8",
          "form": "맞아요 색달랐어요",
          "original_form": "맞아요 색달랐어요",
          "speaker_id": "1",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012056.1.9",
          "form": "센서를 부착했나봐요",
          "original_form": "센서를 부착했나봐요",
          "speaker_id": "1",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012056.1.10",
          "form": "그게 안면인식으로 나온데요",
          "original_form": "그게 안면인식으로 나온데요",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012056.1.11",
          "form": "저도 센서인줄알았어요",
          "original_form": "저도 센서인줄알았어요",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012056.1.12",
          "form": "와 정말요?",
          "original_form": "와 정말요?",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012056.1.13",
          "form": "세상 좋아졌네요",
          "original_form": "세상 좋아졌네요",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012056.1.14",
          "form": "네 진짜 더신기했어요",
          "original_form": "네 진짜 더신기했어요",
          "speaker_id": "2",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012056.1.15",
          "form": "여자배구 보셨나요",
          "original_form": "여자배구 보셨나요",
          "speaker_id": "1",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100012056.1.16",
          "form": "다른나라선수들은 130이러는데",
          "original_form": "다른나라선수들은 130이러는데",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100012056.1.17",
          "form": "안산은 80이고 너무침착",
          "original_form": "안산은 80이고 너무침착",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100012056.1.18",
          "form": "네 배구봤죠",
          "original_form": "네 배구봤죠",
          "speaker_id": "2",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012056.1.19",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
