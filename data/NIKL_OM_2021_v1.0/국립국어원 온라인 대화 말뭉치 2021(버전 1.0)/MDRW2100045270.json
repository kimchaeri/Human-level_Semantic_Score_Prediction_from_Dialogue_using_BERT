      "utterance": [
        {
          "id": "MDRW2100045270.1.1",
          "form": "넵? 토마토면 스파게티 아닌가요??",
          "original_form": "넵? 토마토면 스파게티 아닌가요??",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045270.1.2",
          "form": "앗 아니예요!!! 토마토가 들어가지만",
          "original_form": "앗 아니예요!!! 토마토가 들어가지만",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045270.1.3",
          "form": "면은 안 들어가는 요리랍니다!",
          "original_form": "면은 안 들어가는 요리랍니다!",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045270.1.4",
          "form": "ㅋㅋㅋㅋ저 모르겠어용",
          "original_form": "ㅋㅋㅋㅋ저 모르겠어용",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045270.1.5",
          "form": "아란치니?",
          "original_form": "아란치니?",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045270.1.6",
          "form": "헉 아란치니는 뭐예요 처음 들어봐요",
          "original_form": "헉 아란치니는 뭐예요 처음 들어봐요",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045270.1.7",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045270.1.8",
          "form": "볶은 요리예요!",
          "original_form": "볶은 요리예요!",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045270.1.9",
          "form": "다 왔다 다 왔다",
          "original_form": "다 왔다 다 왔다",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045270.1.10",
          "form": "어 슈돌에 나와서 아는 요리에욬ㅋㅋㅋ",
          "original_form": "어 슈돌에 나와서 아는 요리에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045270.1.11",
          "form": "후엥 몰라여ㅠㅠ 토마토라니...",
          "original_form": "후엥 몰라여ㅠㅠ 토마토라니...",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045270.1.12",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045270.1.13",
          "form": "토마토와",
          "original_form": "토마토와",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045270.1.14",
          "form": "이것을 함께 볶아요!",
          "original_form": "이것을 함께 볶아요!",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045270.1.15",
          "form": "밥? 밥?? 아 리조또 인ㄱㅏ여?",
          "original_form": "밥? 밥?? 아 리조또 인ㄱㅏ여?",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045270.1.16",
          "form": "앗!!!!!!! 쌀은 따로 밥을 해서",
          "original_form": "앗!!!!!!! 쌀은 따로 밥을 해서",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045270.1.17",
          "form": "이것을 위에 덮밥처럼 먹곤 합니다",
          "original_form": "이것을 위에 덮밥처럼 먹곤 합니다",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045270.1.18",
          "form": "토마토와 무엇 볶음!",
          "original_form": "토마토와 무엇 볶음!",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045270.1.19",
          "form": "노란 무엇",
          "original_form": "노란 무엇",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045270.1.20",
          "form": "완전식품이예요",
          "original_form": "완전식품이예요",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045270.1.21",
          "form": "와 GG할레용...",
          "original_form": "와 GG할레용...",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045270.1.22",
          "form": "멀라여...",
          "original_form": "멀라여...",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045270.1.23",
          "form": "ㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045270.1.24",
          "form": "정답은 토마토 달걀 볶음이었습니다!",
          "original_form": "정답은 토마토 달걀 볶음이었습니다!",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045270.1.25",
          "form": "토달볶",
          "original_form": "토달볶",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045270.1.26",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045270.1.27",
          "form": "아 모르는 요리에요.. 안먹어봤영요",
          "original_form": "아 모르는 요리에요.. 안먹어봤영요",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045270.1.28",
          "form": "슈돌에서 들어본 거 같긴한뎋ㅎㅎ. 말이죠ㅛ",
          "original_form": "슈돌에서 들어본 거 같긴한뎋ㅎㅎ. 말이죠ㅛ",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045270.1.29",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045270.1.30",
          "form": "ㅋㅋ이게 호불호가 갈려서 모르시는 분들도 계신 것 같더라구여",
          "original_form": "ㅋㅋ이게 호불호가 갈려서 모르시는 분들도 계신 것 같더라구여",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045270.1.31",
          "form": "문제가 너무 어랴웠나봐요",
          "original_form": "문제가 너무 어랴웠나봐요",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045270.1.32",
          "form": "혹시 점심메뉴 기억하시나요?",
          "original_form": "혹시 점심메뉴 기억하시나요?",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045270.1.33",
          "form": "기억하시면 제가 맞춰볼게요!",
          "original_form": "기억하시면 제가 맞춰볼게요!",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045270.1.34",
          "form": "앗 넵 기억하긴 해욬ㅋㅋㅋ",
          "original_form": "앗 넵 기억하긴 해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045270.1.35",
          "form": "좋아요 점심맞추기 갑니다 ㅋㅋㅋㅋ",
          "original_form": "좋아요 점심맞추기 갑니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
