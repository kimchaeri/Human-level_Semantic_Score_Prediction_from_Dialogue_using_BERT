        },
        {
          "id": "MDRW2100006759.1.2",
          "form": "나를 못알아보다니",
          "original_form": "나를 못알아보다니",
          "speaker_id": "1",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100006759.1.3",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100006759.1.4",
          "form": "관심법으로 다 봤느니라",
          "original_form": "관심법으로 다 봤느니라",
          "speaker_id": "1",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100006759.1.5",
          "form": "최근에 우리 구매한 수비드 기계들",
          "original_form": "최근에 우리 구매한 수비드 기계들",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100006759.1.6",
          "form": "이번주에 다오면 고기도 사러 가자~!",
          "original_form": "이번주에 다오면 고기도 사러 가자~!",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100006759.1.7",
          "form": "다왔는디여?",
          "original_form": "다왔는디여?",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100006759.1.8",
          "form": "아직 내가 산 오일스프레이가 안왔어요",
          "original_form": "아직 내가 산 오일스프레이가 안왔어요",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100006759.1.9",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100006759.1.10",
          "form": "빨리빨리",
          "original_form": "빨리빨리",
          "speaker_id": "1",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100006759.1.11",
          "form": "스테끼!!!",
          "original_form": "스테끼!!!",
          "speaker_id": "1",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100006759.1.12",
          "form": "요새 택배 파업이라 그런지 좀 느리게 오는거같긴한데",
          "original_form": "요새 택배 파업이라 그런지 좀 느리게 오는거같긴한데",
          "speaker_id": "2",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100006759.1.13",
          "form": "해외배송이러 그런가??",
          "original_form": "해외배송이러 그런가??",
          "speaker_id": "2",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100006759.1.14",
          "form": "인천에서 2일인가 3일 걸린거보면..?",
          "original_form": "인천에서 2일인가 3일 걸린거보면..?",
          "speaker_id": "2",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100006759.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100006759.1.16",
          "form": "그렇군여",
          "original_form": "그렇군여",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100006759.1.17",
          "form": "코코가서",
          "original_form": "코코가서",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100006759.1.18",
          "form": "고기 두꺼운거사고",
          "original_form": "고기 두꺼운거사고",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100006759.1.19",
          "form": "우리 코코에서 고기 사본적이있었나??",
          "original_form": "우리 코코에서 고기 사본적이있었나??",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006759.1.20",
          "form": "진공포장",
          "original_form": "진공포장",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006759.1.21",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006759.1.22",
          "form": "코코 고기는",
          "original_form": "코코 고기는",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006759.1.23",
          "form": "비싼거",
          "original_form": "비싼거",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006759.1.24",
          "form": "빨갛고",
          "original_form": "빨갛고",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006759.1.25",
          "form": "두꺼운거",
          "original_form": "두꺼운거",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006759.1.26",
          "form": "근데 스테이크",
          "original_form": "근데 스테이크",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006759.1.27",
          "form": "용은 안사본거 같아",
          "original_form": "용은 안사본거 같아",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006759.1.28",
          "form": "수비드 카페 가입해서",
          "original_form": "수비드 카페 가입해서",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100006759.1.29",
          "form": "두깨랑 시간 봐야게쒀",
          "original_form": "두깨랑 시간 봐야게쒀",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100006759.1.30",
          "form": "먹기만 해서 산 기억이 없나봐...",
          "original_form": "먹기만 해서 산 기억이 없나봐...",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006759.1.31",
          "form": "뀨익",
          "original_form": "뀨익",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006759.1.32",
          "form": "로봇청소기도",
          "original_form": "로봇청소기도",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006759.1.33",
          "form": "사고싶고",
          "original_form": "사고싶고",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006759.1.34",
          "form": "걍 청소기도",
          "original_form": "걍 청소기도",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006759.1.35",
          "form": "사고시파",
          "original_form": "사고시파",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006759.1.36",
          "form": "늘 사고 싶은게 많군",
          "original_form": "늘 사고 싶은게 많군",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006759.1.37",
          "form": "아닝",
          "original_form": "아닝",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100006759.1.38",
          "form": "청소기는 잘못산거자나ㅠ",
          "original_form": "청소기는 잘못산거자나ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100006759.1.39",
          "form": "머리카락을 흡입을 못하니까",
          "original_form": "머리카락을 흡입을 못하니까",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006759.1.40",
          "form": "로봇청소기는 사고싶은겣맞아",
          "original_form": "로봇청소기는 사고싶은겣맞아",
          "speaker_id": "1",
