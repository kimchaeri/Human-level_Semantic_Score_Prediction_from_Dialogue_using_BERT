      "utterance": [
        {
          "id": "MDRW2100020984.1.1",
          "form": "전 남쪽은 홍콩밖에..",
          "original_form": "전 남쪽은 홍콩밖에..",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020984.1.2",
          "form": "전 라오스가 제일 좋아요",
          "original_form": "전 라오스가 제일 좋아요",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020984.1.3",
          "form": "라오스가먄 주위 셋트로 돌곤하지 않나여? 벳남 캄보디아",
          "original_form": "라오스가먄 주위 셋트로 돌곤하지 않나여? 벳남 캄보디아",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020984.1.4",
          "form": "길게여행하면 세트로 돌기도 하더라구요",
          "original_form": "길게여행하면 세트로 돌기도 하더라구요",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020984.1.5",
          "form": "라오스 어떤점이 좋앗어요? 코끼리?",
          "original_form": "라오스 어떤점이 좋앗어요? 코끼리?",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100020984.1.6",
          "form": "아니요 폭포들이 예뻣구요",
          "original_form": "아니요 폭포들이 예뻣구요",
          "speaker_id": "2",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100020984.1.7",
          "form": "먹을게 맛있었겠네여? ㅋㅋㅋ",
          "original_form": "먹을게 맛있었겠네여? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100020984.1.8",
          "form": "야시장도 좋앗어요",
          "original_form": "야시장도 좋앗어요",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100020984.1.9",
          "form": "음 고수를 싫어하시면 입에맞지않으실거예요",
          "original_form": "음 고수를 싫어하시면 입에맞지않으실거예요",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100020984.1.10",
          "form": "전 동남아 향신료 좋아요..",
          "original_form": "전 동남아 향신료 좋아요..",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100020984.1.11",
          "form": "지금도 가끔 동남아 라면 사먹어요",
          "original_form": "지금도 가끔 동남아 라면 사먹어요",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100020984.1.12",
          "form": "저도 동남아음식 좋아해요",
          "original_form": "저도 동남아음식 좋아해요",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100020984.1.13",
          "form": "동남아 과자도 가성비 좋더라구요",
          "original_form": "동남아 과자도 가성비 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100020984.1.14",
          "form": "맞아요 간이 좀 세긴하지만 ㅋㅋㅋㅋ",
          "original_form": "맞아요 간이 좀 세긴하지만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100020984.1.15",
          "form": "국내여행은 자주 하세요?",
          "original_form": "국내여행은 자주 하세요?",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100020984.1.16",
          "form": "1년정도 집콕인듯해요.. \r\n예전엔 일적으로 많이 돌아댕겼는데",
          "original_form": "1년정도 집콕인듯해요.. \r\n예전엔 일적으로 많이 돌아댕겼는데",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100020984.1.17",
          "form": "그럼 가본곳 중에는 어디가 제일 좋으셧어요?",
          "original_form": "그럼 가본곳 중에는 어디가 제일 좋으셧어요?",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100020984.1.18",
          "form": "동해바다 이름이 갑자기..",
          "original_form": "동해바다 이름이 갑자기..",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100020984.1.19",
          "form": "경포대..",
          "original_form": "경포대..",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100020984.1.20",
          "form": "정비 많이해놔서 옛날이랑 다르더라구요..",
          "original_form": "정비 많이해놔서 옛날이랑 다르더라구요..",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100020984.1.21",
          "form": "아 정비 많이해놧나요?",
          "original_form": "아 정비 많이해놧나요?",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100020984.1.22",
          "form": "예전에 가봣을때 좋앗엇는데",
          "original_form": "예전에 가봣을때 좋앗엇는데",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100020984.1.23",
          "form": "저도 어릴때가고 2년전인가 갔는데 엄청 변했더라구요.. ㅋㅋㅋ",
          "original_form": "저도 어릴때가고 2년전인가 갔는데 엄청 변했더라구요.. ㅋㅋㅋ",
          "speaker_id": "1",
