      "utterance": [
        {
          "id": "MDRW2100018462.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100018462.1.2",
          "form": "8월 휴가는 잘 보내셨나요?",
          "original_form": "8월 휴가는 잘 보내셨나요?",
          "speaker_id": "1",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100018462.1.3",
          "form": "네~ 코로나로 멀리는 못갔지만",
          "original_form": "네~ 코로나로 멀리는 못갔지만",
          "speaker_id": "2",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100018462.1.4",
          "form": "가족들이랑 여행했습니다",
          "original_form": "가족들이랑 여행했습니다",
          "speaker_id": "2",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100018462.1.5",
          "form": "오오 어디 다녀오셨어요?",
          "original_form": "오오 어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100018462.1.6",
          "form": "저는 제주도 다녀왔어요",
          "original_form": "저는 제주도 다녀왔어요",
          "speaker_id": "1",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018462.1.7",
          "form": "허걱 저도 제주도 여행 갔다왔어요",
          "original_form": "허걱 저도 제주도 여행 갔다왔어요",
          "speaker_id": "2",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018462.1.8",
          "form": "대박 저희 이렇게 여행지가 겹치네요",
          "original_form": "대박 저희 이렇게 여행지가 겹치네요",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018462.1.9",
          "form": "어느 지역 관광하셨어요?",
          "original_form": "어느 지역 관광하셨어요?",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018462.1.10",
          "form": "저는 서귀포요",
          "original_form": "저는 서귀포요",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018462.1.11",
          "form": "서귀포 바다가 좋더라구요",
          "original_form": "서귀포 바다가 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018462.1.12",
          "form": "아하 전 제주시에서 주로 있었어요ㅠㅠ",
          "original_form": "아하 전 제주시에서 주로 있었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018462.1.13",
          "form": "함덕 해수욕장도 좋더라고요",
          "original_form": "함덕 해수욕장도 좋더라고요",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018462.1.14",
          "form": "맑은 바다 보면서 쉬다 오니까 행복했어영",
          "original_form": "맑은 바다 보면서 쉬다 오니까 행복했어영",
          "speaker_id": "1",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018462.1.15",
          "form": "맞아요 바다를 바라보면 마음이 평화로워지잖아요",
          "original_form": "맞아요 바다를 바라보면 마음이 평화로워지잖아요",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018462.1.16",
          "form": "이게 여행의 매력인 것 같아요ㅠㅠ",
          "original_form": "이게 여행의 매력인 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018462.1.17",
          "form": "공감되네요ㅠㅠ 하루 빨리 해외 여행도 가고 싶어요",
          "original_form": "공감되네요ㅠㅠ 하루 빨리 해외 여행도 가고 싶어요",
          "speaker_id": "1",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018462.1.18",
          "form": "해외는 어디 가보셨어요?",
          "original_form": "해외는 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018462.1.19",
          "form": "전 일본이랑 필리핀 간적 있어용",
          "original_form": "전 일본이랑 필리핀 간적 있어용",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018462.1.20",
          "form": "오 저도 일본 오사카 간적 있네요",
          "original_form": "오 저도 일본 오사카 간적 있네요",
          "speaker_id": "1",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018462.1.21",
          "form": "2017년이니까 4년 전에요 ㅎㅎ",
          "original_form": "2017년이니까 4년 전에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018462.1.22",
          "form": "아하 전 후쿠오카 갔었어요 온천이 좋더라고요",
          "original_form": "아하 전 후쿠오카 갔었어요 온천이 좋더라고요",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018462.1.23",
          "form": "우와 온천 부러워요 전 온천은 아쉽게도 못해봤어요",
          "original_form": "우와 온천 부러워요 전 온천은 아쉽게도 못해봤어요",
          "speaker_id": "1",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018462.1.24",
          "form": "숙박료가 비싸긴 했는데 아깝지가 않았어요 강추합니다!!",
          "original_form": "숙박료가 비싸긴 했는데 아깝지가 않았어요 강추합니다!!",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018462.1.25",
          "form": "추천 감사해요 다음에 가게 되면 고려해볼게요 ㅎㅎ",
          "original_form": "추천 감사해요 다음에 가게 되면 고려해볼게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 02:25"
        }
      ]
    }
