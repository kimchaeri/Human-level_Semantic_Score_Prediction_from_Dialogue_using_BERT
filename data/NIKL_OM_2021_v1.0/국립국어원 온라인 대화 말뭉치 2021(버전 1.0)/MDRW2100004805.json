      "utterance": [
        {
          "id": "MDRW2100004805.1.1",
          "form": "연애 관심있으신가요?",
          "original_form": "연애 관심있으신가요?",
          "speaker_id": "2",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004805.1.2",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004805.1.3",
          "form": "음 전 아직은 없어요!",
          "original_form": "음 전 아직은 없어요!",
          "speaker_id": "1",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004805.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004805.1.5",
          "form": "혹시 이상형이..?!",
          "original_form": "혹시 이상형이..?!",
          "speaker_id": "1",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004805.1.6",
          "form": "저도 별로 관심은 없는데",
          "original_form": "저도 별로 관심은 없는데",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004805.1.7",
          "form": "귀여운 사람 좋아하는 것 같아요 ㅎㅎ",
          "original_form": "귀여운 사람 좋아하는 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004805.1.8",
          "form": "그냥 사람이 좋은 것같기도하고",
          "original_form": "그냥 사람이 좋은 것같기도하고",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004805.1.9",
          "form": "오 귀여운 사람!",
          "original_form": "오 귀여운 사람!",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004805.1.10",
          "form": "그럼 연상 동갑 연하중에선 연하가 끌리시나요?",
          "original_form": "그럼 연상 동갑 연하중에선 연하가 끌리시나요?",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004805.1.11",
          "form": "ㅋㅋㅋㅋ저는 연하는 별로 안좋아하는 것같아요",
          "original_form": "ㅋㅋㅋㅋ저는 연하는 별로 안좋아하는 것같아요",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004805.1.12",
          "form": "귀여운게 약간..",
          "original_form": "귀여운게 약간..",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004805.1.13",
          "form": "안그럴것같은 사람이 은근히 귀여운거?",
          "original_form": "안그럴것같은 사람이 은근히 귀여운거?",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004805.1.14",
          "form": "이런걸 좋아해서",
          "original_form": "이런걸 좋아해서",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004805.1.15",
          "form": "아 갭차이 이런..?!",
          "original_form": "아 갭차이 이런..?!",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004805.1.16",
          "form": "전 귀여운건 예전엔 안끌렸는데 뭔가 나이 좀 먹어가니까 왜 좋은지 알겠어요",
          "original_form": "전 귀여운건 예전엔 안끌렸는데 뭔가 나이 좀 먹어가니까 왜 좋은지 알겠어요",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004805.1.17",
          "form": "그냥 웃음이 나오는 그런 .. ㅎㅎㅋㅋ",
          "original_form": "그냥 웃음이 나오는 그런 .. ㅎㅎㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004805.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아여",
          "speaker_id": "2",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004805.1.19",
          "form": "사랑스러워 보이는 게 큰 거 같아요",
          "original_form": "사랑스러워 보이는 게 큰 거 같아요",
          "speaker_id": "2",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004805.1.20",
          "form": "그치만 연애는 좋아지는 것과는 별개로 생각하게되는것같아요",
          "original_form": "그치만 연애는 좋아지는 것과는 별개로 생각하게되는것같아요",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004805.1.21",
          "form": "생각할 게 많아지더라고요",
          "original_form": "생각할 게 많아지더라고요",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004805.1.22",
          "form": "맞아요 특히 연애에서 결혼으로 넘어갈 그 시기?",
          "original_form": "맞아요 특히 연애에서 결혼으로 넘어갈 그 시기?",
          "speaker_id": "1",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004805.1.23",
          "form": "약간 저는 아직 제 나이도 결혼하기에 엥 벌써? 하는 생각이 있는데",
          "original_form": "약간 저는 아직 제 나이도 결혼하기에 엥 벌써? 하는 생각이 있는데",
          "speaker_id": "1",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004805.1.24",
          "form": "주변에 남친이랑 오래 사귄 애들 보면 슬슬 내년이나 내후년에 해야지~하는데 결혼이라고??? 이런 생각도 들고..",
          "original_form": "주변에 남친이랑 오래 사귄 애들 보면 슬슬 내년이나 내후년에 해야지~하는데 결혼이라고??? 이런 생각도 들고..",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004805.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004805.1.26",
          "form": "아직 저한테는 먼 얘기같은데..",
          "original_form": "아직 저한테는 먼 얘기같은데..",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004805.1.27",
          "form": "부모님도 애인없냐 물어보시는 것도 부담스럽고 ㅠㅠ",
          "original_form": "부모님도 애인없냐 물어보시는 것도 부담스럽고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004805.1.28",
          "form": "또 저는 비혼주의자라 결혼할 생각이 전혀없어서 더 그렇네요",
          "original_form": "또 저는 비혼주의자라 결혼할 생각이 전혀없어서 더 그렇네요",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004805.1.29",
          "form": "헉 저도요",
          "original_form": "헉 저도요",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004805.1.30",
          "form": "결혼 딱히 해야하나 싶고",
          "original_form": "결혼 딱히 해야하나 싶고",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004805.1.31",
          "form": "또 제대로 된 남자도 없고ㅋㅋ뉴스만 봐도 결혼할 생각 제로..",
          "original_form": "또 제대로 된 남자도 없고ㅋㅋ뉴스만 봐도 결혼할 생각 제로..",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004805.1.32",
          "form": "결혼이 필수가 된 문화가 싫어요ㅠㅠ",
          "original_form": "결혼이 필수가 된 문화가 싫어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004805.1.33",
          "form": "진짜로요 ㅠㅠ",
          "original_form": "진짜로요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004805.1.34",
          "form": "1인가구가 이렇게 많아지고있는데",
          "original_form": "1인가구가 이렇게 많아지고있는데",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004805.1.35",
          "form": "상황개선이 하나도 안해주고 결혼만 강요하는 것도 열받고..",
          "original_form": "상황개선이 하나도 안해주고 결혼만 강요하는 것도 열받고..",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004805.1.36",
          "form": "이상한 놈들이라도 좀 싹 잡아가면 모르겠는데 하.. 답답하기만해요",
          "original_form": "이상한 놈들이라도 좀 싹 잡아가면 모르겠는데 하.. 답답하기만해요",
          "speaker_id": "2",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100004805.1.37",
          "form": "그러니까요ㅠㅠ 범죄자 형량도 그렇고",
          "original_form": "그러니까요ㅠㅠ 범죄자 형량도 그렇고",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004805.1.38",
          "form": "주먹구구식으로 결혼 출산 장려하는거 보면 으휴",
          "original_form": "주먹구구식으로 결혼 출산 장려하는거 보면 으휴",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004805.1.39",
          "form": "되겠냐?",
          "original_form": "되겠냐?",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004805.1.40",
          "form": "이런 생각ㅠㅋㅋㅋ",
          "original_form": "이런 생각ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004805.1.41",
          "form": "공감해요 ㅠㅠ",
          "original_form": "공감해요 ㅠㅠ",
