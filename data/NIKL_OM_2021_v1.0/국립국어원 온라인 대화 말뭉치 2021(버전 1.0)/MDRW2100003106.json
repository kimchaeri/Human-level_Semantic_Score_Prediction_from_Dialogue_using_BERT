      "utterance": [
        {
          "id": "MDRW2100003106.1.1",
          "form": "엉 몇년전에",
          "original_form": "엉 몇년전에",
          "speaker_id": "2",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100003106.1.2",
          "form": "그 허브동산에도 별빛놀이라고 빛축제한다",
          "original_form": "그 허브동산에도 별빛놀이라고 빛축제한다",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003106.1.3",
          "form": "여기저기 다 빛축제네",
          "original_form": "여기저기 다 빛축제네",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003106.1.4",
          "form": "젤 무난하니까 호불호 없고",
          "original_form": "젤 무난하니까 호불호 없고",
          "speaker_id": "1",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003106.1.5",
          "form": "너무 많다 전력낭비",
          "original_form": "너무 많다 전력낭비",
          "speaker_id": "2",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003106.1.6",
          "form": "ㅋㅋ허브랑 정원 구경만 하기에는 아쉬웠나보지",
          "original_form": "ㅋㅋ허브랑 정원 구경만 하기에는 아쉬웠나보지",
          "speaker_id": "1",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003106.1.7",
          "form": "시간은?",
          "original_form": "시간은?",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003106.1.8",
          "form": "오전 9시부터 밤 10시까지",
          "original_form": "오전 9시부터 밤 10시까지",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003106.1.9",
          "form": "밤에 하는거때문에 길게하네",
          "original_form": "밤에 하는거때문에 길게하네",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003106.1.10",
          "form": "그러게 미디어 파사드 공연도 있어 이건 7시8시",
          "original_form": "그러게 미디어 파사드 공연도 있어 이건 7시8시",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003106.1.11",
          "form": "이게 빛축제인가보네",
          "original_form": "이게 빛축제인가보네",
          "speaker_id": "2",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100003106.1.12",
          "form": "그거말고도 각종체험들도 많다",
          "original_form": "그거말고도 각종체험들도 많다",
          "speaker_id": "1",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100003106.1.13",
          "form": "난 구지 체험은 별로다 ㅎㅎ",
          "original_form": "난 구지 체험은 별로다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 18:56"
        },
        {
          "id": "MDRW2100003106.1.14",
          "form": "성인은 그렇지",
          "original_form": "성인은 그렇지",
