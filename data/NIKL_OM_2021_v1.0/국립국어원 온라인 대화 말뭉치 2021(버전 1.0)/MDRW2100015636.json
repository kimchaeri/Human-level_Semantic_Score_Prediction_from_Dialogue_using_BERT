        {
          "id": "MDRW2100015636.1.1",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015636.1.2",
          "form": "오 도서관여나 거기는",
          "original_form": "오 도서관여나 거기는",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015636.1.3",
          "form": "ㅇㅇ우린 50프로만 개방",
          "original_form": "ㅇㅇ우린 50프로만 개방",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015636.1.4",
          "form": "최근 내가 읽은책은",
          "original_form": "최근 내가 읽은책은",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015636.1.5",
          "form": "음 수면에관련되는책",
          "original_form": "음 수면에관련되는책",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015636.1.6",
          "form": "오 잠을못자능가?",
          "original_form": "오 잠을못자능가?",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015636.1.7",
          "form": "책제목을 말해드릴게요",
          "original_form": "책제목을 말해드릴게요",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015636.1.8",
          "form": "적게 자도 괜찮습니다",
          "original_form": "적게 자도 괜찮습니다",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015636.1.9",
          "form": "자 어떤가요? 저 책제목을 들었을때",
          "original_form": "자 어떤가요? 저 책제목을 들었을때",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015636.1.10",
          "form": "많이자고픈데",
          "original_form": "많이자고픈데",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015636.1.11",
          "form": "ㅋㅋ부제가 심지어",
          "original_form": "ㅋㅋ부제가 심지어",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015636.1.12",
          "form": "하루 5시간만 자도 충분하다",
          "original_form": "하루 5시간만 자도 충분하다",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015636.1.13",
          "form": "였어요",
          "original_form": "였어요",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015636.1.14",
          "form": "노우ㅜㅜㅜ",
          "original_form": "노우ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015636.1.15",
          "form": "와 진짜 내용이 궁금했었다는",
          "original_form": "와 진짜 내용이 궁금했었다는",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015636.1.16",
          "form": "나는 먹는거보다 자는게더 우선임ㅜㅜㅜㅜ",
          "original_form": "나는 먹는거보다 자는게더 우선임ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015636.1.17",
          "form": "처음에 예스24출간 되었을때부터",
          "original_form": "처음에 예스24출간 되었을때부터",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015636.1.18",
          "form": "도움이 됐능가?",
          "original_form": "도움이 됐능가?",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015636.1.19",
          "form": "되게 혹했음 대체 무슨 내용일것인가",
