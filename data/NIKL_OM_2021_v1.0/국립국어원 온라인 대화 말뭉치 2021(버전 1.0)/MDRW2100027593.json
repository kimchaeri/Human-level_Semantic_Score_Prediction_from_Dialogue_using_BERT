      "utterance": [
        {
          "id": "MDRW2100027593.1.1",
          "form": "강아지랑 고양이가 무난하지",
          "original_form": "강아지랑 고양이가 무난하지",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027593.1.2",
          "form": "반려동물 넘이쁘다",
          "original_form": "반려동물 넘이쁘다",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027593.1.3",
          "form": "너구리도 키우더라",
          "original_form": "너구리도 키우더라",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027593.1.4",
          "form": "너구리는 손을 쓰고 똑똑해",
          "original_form": "너구리는 손을 쓰고 똑똑해",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027593.1.5",
          "form": "이쁘지 무늬가",
          "original_form": "이쁘지 무늬가",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027593.1.6",
          "form": "원숭이도 키우더라",
          "original_form": "원숭이도 키우더라",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027593.1.7",
          "form": "원숭이는 불법이래",
          "original_form": "원숭이는 불법이래",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027593.1.8",
          "form": "라마도 키우던걸",
          "original_form": "라마도 키우던걸",
          "speaker_id": "1",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027593.1.9",
          "form": "사료값이 만만치않아",
          "original_form": "사료값이 만만치않아",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027593.1.10",
          "form": "병원비가 더비싸",
          "original_form": "병원비가 더비싸",
          "speaker_id": "1",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027593.1.11",
          "form": "반려동물은 특수병원 가야해",
          "original_form": "반려동물은 특수병원 가야해",
          "speaker_id": "1",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027593.1.12",
          "form": "알러지있으면 키우지못해",
          "original_form": "알러지있으면 키우지못해",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027593.1.13",
          "form": "반려동물 간식도 다양해",
          "original_form": "반려동물 간식도 다양해",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027593.1.14",
          "form": "수제간식이 유행이야",
          "original_form": "수제간식이 유행이야",
          "speaker_id": "2",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027593.1.15",
          "form": "오리뼈. 돼지뼈 등 뼈도 많이 줘",
          "original_form": "오리뼈. 돼지뼈 등 뼈도 많이 줘",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027593.1.16",
          "form": "닭발도 좋아한대",
          "original_form": "닭발도 좋아한대",
          "speaker_id": "2",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027593.1.17",
          "form": "고양이는 어때",
          "original_form": "고양이는 어때",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027593.1.18",
