      "utterance": [
        {
          "id": "MDRW2100038834.1.1",
          "form": "안녕라세요",
          "original_form": "안녕라세요",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100038834.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100038834.1.3",
          "form": "중고거래 관련 미신 믿으시는 편인가요?ㅠㅠ (무섭)",
          "original_form": "중고거래 관련 미신 믿으시는 편인가요?ㅠㅠ (무섭)",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100038834.1.4",
          "form": "음 평소에 믿는 편은 아닌데",
          "original_form": "음 평소에 믿는 편은 아닌데",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038834.1.5",
          "form": "가끙 무서운이야기 듣거나 하면 좀 신경쓰여요",
          "original_form": "가끙 무서운이야기 듣거나 하면 좀 신경쓰여요",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038834.1.6",
          "form": "저도요ㅠ 그치만 파는건 몇번 해봤어요 언니가ㅎ",
          "original_form": "저도요ㅠ 그치만 파는건 몇번 해봤어요 언니가ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038834.1.7",
          "form": "대신ㅎㅎ",
          "original_form": "대신ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038834.1.8",
          "form": "어떤  물건들을 판매했나요?",
          "original_form": "어떤  물건들을 판매했나요?",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100038834.1.9",
          "form": "제 책이랑 미니 냉장고를 언니가 대신 팔아줬죠ㅎ",
          "original_form": "제 책이랑 미니 냉장고를 언니가 대신 팔아줬죠ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100038834.1.10",
          "form": "오 대신 팔아주고 좋네요",
          "original_form": "오 대신 팔아주고 좋네요",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100038834.1.11",
          "form": "친절한 언니네요",
          "original_form": "친절한 언니네요",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100038834.1.12",
          "form": "ㅋㅋㅋ사실 전 안 팔고 싶었지만 강요당햇네여",
          "original_form": "ㅋㅋㅋ사실 전 안 팔고 싶었지만 강요당햇네여",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038834.1.13",
          "form": "ㅋㅋㅋㅋㅋ판매하게 되어서 좀 아쉬웠나요?",
          "original_form": "ㅋㅋㅋㅋㅋ판매하게 되어서 좀 아쉬웠나요?",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038834.1.14",
          "form": "아니면 만족스럽나요?",
          "original_form": "아니면 만족스럽나요?",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038834.1.15",
          "form": "어차피 지금까지 쓸 일은 없었을거 같아서 아쉽진 않죠",
          "original_form": "어차피 지금까지 쓸 일은 없었을거 같아서 아쉽진 않죠",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100038834.1.16",
          "form": "그럼 괜찮네요ㅎㅎ",
          "original_form": "그럼 괜찮네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038834.1.17",
          "form": "그쵸 근데 언니가 대신 팔아준 중고거래 금액들은 어디오 증발한걸까요?",
          "original_form": "그쵸 근데 언니가 대신 팔아준 중고거래 금액들은 어디오 증발한걸까요?",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100038834.1.18",
          "form": "못 받으셨어요?!?",
          "original_form": "못 받으셨어요?!?",
          "speaker_id": "2",
          "time": "20210902 22:59"
        }
      ]
