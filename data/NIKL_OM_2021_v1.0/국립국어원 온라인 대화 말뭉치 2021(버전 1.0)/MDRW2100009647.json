      "utterance": [
        {
          "id": "MDRW2100009647.1.1",
          "form": "웹툰 웹소 관련 커뮤니티 활동하세요?",
          "original_form": "웹툰 웹소 관련 커뮤니티 활동하세요?",
          "speaker_id": "1",
          "time": "20210823 02:17"
        },
        {
          "id": "MDRW2100009647.1.2",
          "form": "헉 아뇨 혼자만 좋아하고 있어요ㅜㅜ",
          "original_form": "헉 아뇨 혼자만 좋아하고 있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009647.1.3",
          "form": "웹툰 웹소 커뮤가 따로 있나요??",
          "original_form": "웹툰 웹소 커뮤가 따로 있나요??",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009647.1.4",
          "form": "덕질 커뮤니티 하나 궁금해서 여쭤봤어요",
          "original_form": "덕질 커뮤니티 하나 궁금해서 여쭤봤어요",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009647.1.5",
          "form": "저도 딱히 활동하지는 않고 그냥 읽어요",
          "original_form": "저도 딱히 활동하지는 않고 그냥 읽어요",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009647.1.6",
          "form": "아 덕질커뮤니티",
          "original_form": "아 덕질커뮤니티",
          "speaker_id": "2",
          "time": "20210823 02:32"
        },
        {
          "id": "MDRW2100009647.1.7",
          "form": "그그 사이트 중에 더쿠랑 디미토리는 가입해놨어요!!! 거기가 덕질 커뮤중에 웹소 웹툰 많이 올라논다 하더라고요",
          "original_form": "그그 사이트 중에 더쿠랑 디미토리는 가입해놨어요!!! 거기가 덕질 커뮤중에 웹소 웹툰 많이 올라논다 하더라고요",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009647.1.8",
          "form": "근데 하 읽느라 바빠서",
          "original_form": "근데 하 읽느라 바빠서",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009647.1.9",
          "form": "덕질하시는 거 중에 정모 나가보신 거 있으신가요?",
          "original_form": "덕질하시는 거 중에 정모 나가보신 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009647.1.10",
          "form": "저는 가본적 없어요",
          "original_form": "저는 가본적 없어요",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009647.1.11",
          "form": "위험할까봐 걱정되더라고요",
          "original_form": "위험할까봐 걱정되더라고요",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009647.1.12",
          "form": "님은 가본적 있어요?",
          "original_form": "님은 가본적 있어요?",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009647.1.13",
          "form": "헉 그러게요",
          "original_form": "헉 그러게요",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009647.1.14",
          "form": "앗 저는 그",
          "original_form": "앗 저는 그",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009647.1.15",
          "form": "덕톡회인가 웹툰 사이트에서 주최하는 모임은 가본적 있어요",
          "original_form": "덕톡회인가 웹툰 사이트에서 주최하는 모임은 가본적 있어요",
          "speaker_id": "2",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009647.1.16",
          "form": "근데 사무적인 분위기더라고욬ㅋㅋㅋㅋ",
          "original_form": "근데 사무적인 분위기더라고욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009647.1.17",
          "form": "그 게임 정모에서 모였다가 안좋은 일 당한 사례도 많고 그래서..개인이 주최하는건 맞아요.. 좀 꺼려지죠",
          "original_form": "그 게임 정모에서 모였다가 안좋은 일 당한 사례도 많고 그래서..개인이 주최하는건 맞아요.. 좀 꺼려지죠",
          "speaker_id": "2",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009647.1.18",
          "form": "분위기가 색다르네요",
          "original_form": "분위기가 색다르네요",
          "speaker_id": "1",
          "time": "20210823 02:50"
        },
        {
          "id": "MDRW2100009647.1.19",
          "form": "저는 정모는 아닌데",
          "original_form": "저는 정모는 아닌데",
          "speaker_id": "1",
          "time": "20210823 02:50"
        },
        {
          "id": "MDRW2100009647.1.20",
          "form": "코스프레 대회?는 친구 따라 간 적 있어요",
          "original_form": "코스프레 대회?는 친구 따라 간 적 있어요",
          "speaker_id": "1",
          "time": "20210823 02:50"
        },
        {
          "id": "MDRW2100009647.1.21",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210823 02:51"
        },
        {
          "id": "MDRW2100009647.1.22",
          "form": "그그 양재시민의 숲에서 하는 행사요?!",
          "original_form": "그그 양재시민의 숲에서 하는 행사요?!",
          "speaker_id": "2",
          "time": "20210823 02:51"
        },
        {
          "id": "MDRW2100009647.1.23",
          "form": "신기합니다",
          "original_form": "신기합니다",
          "speaker_id": "2",
          "time": "20210823 02:52"
        },
        {
          "id": "MDRW2100009647.1.24",
          "form": "저는 부산에 있는 행사에 갔었어요",
          "original_form": "저는 부산에 있는 행사에 갔었어요",
          "speaker_id": "1",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009647.1.25",
          "form": "서울에서는 거기서 하나봐요",
          "original_form": "서울에서는 거기서 하나봐요",
          "speaker_id": "1",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009647.1.26",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "2",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009647.1.27",
          "form": "부산은 어디서 하나요?",
          "original_form": "부산은 어디서 하나요?",
          "speaker_id": "2",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009647.1.28",
          "form": "부산 행사 많은데 꽤 크게 할 것 같어요",
          "original_form": "부산 행사 많은데 꽤 크게 할 것 같어요",
          "speaker_id": "2",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009647.1.29",
          "form": "부산은 해운대에서 해요",
          "original_form": "부산은 해운대에서 해요",
          "speaker_id": "1",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009647.1.30",
          "form": "헉 멋있겠다",
          "original_form": "헉 멋있겠다",
          "speaker_id": "2",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009647.1.31",
          "form": "보통 게임 캐릭터 코스프레를 많이 하나요 아니면 그 애니에 나오는 캐릭터들이 많나요?",
          "original_form": "보통 게임 캐릭터 코스프레를 많이 하나요 아니면 그 애니에 나오는 캐릭터들이 많나요?",
          "speaker_id": "2",
          "time": "20210823 02:56"
        },
        {
          "id": "MDRW2100009647.1.32",
          "form": "둘 다 많아요",
          "original_form": "둘 다 많아요",
          "speaker_id": "1",
          "time": "20210823 02:56"
        },
        {
          "id": "MDRW2100009647.1.33",
          "form": "게임이나 애니도 많고",
          "original_form": "게임이나 애니도 많고",
          "speaker_id": "1",
          "time": "20210823 02:56"
        },
        {
          "id": "MDRW2100009647.1.34",
          "form": "그냥 군복이나 경찰복도 많더라고요",
          "original_form": "그냥 군복이나 경찰복도 많더라고요",
          "speaker_id": "1",
          "time": "20210823 02:56"
        },
        {
          "id": "MDRW2100009647.1.35",
          "form": "사실 불법이지만",
          "original_form": "사실 불법이지만",
          "speaker_id": "1",
          "time": "20210823 02:56"
        }
      ]
