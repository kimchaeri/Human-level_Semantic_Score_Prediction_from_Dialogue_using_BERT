      "utterance": [
        {
          "id": "MDRW2100019687.1.1",
          "form": "점심식사하셨나요~?",
          "original_form": "점심식사하셨나요~?",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100019687.1.2",
          "form": "아직이요.",
          "original_form": "아직이요.",
          "speaker_id": "1",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100019687.1.3",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100019687.1.4",
          "form": "늦게 드시나봐요~ 저는 칼국수랑 냉면 먹고왔어요~",
          "original_form": "늦게 드시나봐요~ 저는 칼국수랑 냉면 먹고왔어요~",
          "speaker_id": "2",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100019687.1.5",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100019687.1.6",
          "form": "전 들깨 칼국수 좋아하거든요",
          "original_form": "전 들깨 칼국수 좋아하거든요",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100019687.1.7",
          "form": "들깨칼국수도 맛있죠~ㅎ",
          "original_form": "들깨칼국수도 맛있죠~ㅎ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100019687.1.8",
          "form": "싫어하는 음식 있으세요~?",
          "original_form": "싫어하는 음식 있으세요~?",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100019687.1.9",
          "form": "딱히 없는거 같네요",
          "original_form": "딱히 없는거 같네요",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100019687.1.10",
          "form": "홍어 같은건 못먹어요ㅋㅋ",
          "original_form": "홍어 같은건 못먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100019687.1.11",
          "form": "저도 홍어는ㅠㅠ",
          "original_form": "저도 홍어는ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100019687.1.12",
          "form": "저는 회종류를 잘 안먹어요ㅠ",
          "original_form": "저는 회종류를 잘 안먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100019687.1.13",
          "form": "초창을 싫어해서 그런가 회는 잘 안맞더라구요ㅠ!",
          "original_form": "초창을 싫어해서 그런가 회는 잘 안맞더라구요ㅠ!",
          "speaker_id": "2",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100019687.1.14",
          "form": "정말요ㅠㅠ 전 연어 좋아하거든요",
          "original_form": "정말요ㅠㅠ 전 연어 좋아하거든요",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100019687.1.15",
          "form": "회가 아니어도 연어 스테이크도 맛있어요",
          "original_form": "회가 아니어도 연어 스테이크도 맛있어요",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100019687.1.16",
          "form": "연어는 좋아하시는분들은 진짜 좋아하시더라구요!!",
          "original_form": "연어는 좋아하시는분들은 진짜 좋아하시더라구요!!",
          "speaker_id": "2",
          "time": "20210830 18:36"
        },
        {
          "id": "MDRW2100019687.1.17",
          "form": "스테이크 초밥는 맛있게 먹은것같아용ㅎ",
          "original_form": "스테이크 초밥는 맛있게 먹은것같아용ㅎ",
          "speaker_id": "2",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100019687.1.18",
          "form": "계란 초밥도 좋아하세요? 약간 달달한 게 맛있더라구요",
          "original_form": "계란 초밥도 좋아하세요? 약간 달달한 게 맛있더라구요",
          "speaker_id": "1",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100019687.1.19",
          "form": "오 저ㅜ초밥집가묜 진짜ㅜ계란 새우 스테이크?! 만 먹는것같아욬ㅋ",
          "original_form": "오 저ㅜ초밥집가묜 진짜ㅜ계란 새우 스테이크?! 만 먹는것같아욬ㅋ",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100019687.1.20",
          "form": "잘하는집은 계란초밥 진짜 맛있더라구요ㅋㅋㅋ 촉촉하구 달달하게ㅎㅎ",
          "original_form": "잘하는집은 계란초밥 진짜 맛있더라구요ㅋㅋㅋ 촉촉하구 달달하게ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100019687.1.21",
          "form": "맞아요~ 제 주변에 상무초밥?! 들어보셨나요?! 그게 생겨서 갔는데 진짜 맛있었여요!!",
          "original_form": "맞아요~ 제 주변에 상무초밥?! 들어보셨나요?! 그게 생겨서 갔는데 진짜 맛있었여요!!",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100019687.1.22",
          "form": "들어본 적 있어요. 한번 먹으러 가봐야겠네요ㅋㅋ",
          "original_form": "들어본 적 있어요. 한번 먹으러 가봐야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:38"
        }
      ]
    }
  ]
