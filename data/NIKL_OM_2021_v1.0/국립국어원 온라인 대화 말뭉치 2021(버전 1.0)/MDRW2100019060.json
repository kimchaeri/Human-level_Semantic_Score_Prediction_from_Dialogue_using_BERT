      "utterance": [
        {
          "id": "MDRW2100019060.1.1",
          "form": "팔이 슬슬 무거워진당",
          "original_form": "팔이 슬슬 무거워진당",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019060.1.2",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019060.1.3",
          "form": "주말까진 운동도 하지 말고 푹 쉬어야지",
          "original_form": "주말까진 운동도 하지 말고 푹 쉬어야지",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019060.1.4",
          "form": "그러려고",
          "original_form": "그러려고",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019060.1.5",
          "form": "정신건강을 위해 시장돈까스 주문했다",
          "original_form": "정신건강을 위해 시장돈까스 주문했다",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019060.1.6",
          "form": "그래 잘 먹고 건강히 지내야지",
          "original_form": "그래 잘 먹고 건강히 지내야지",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019060.1.7",
          "form": "그래도 백신 당겨 맞아서 잘됐네",
          "original_form": "그래도 백신 당겨 맞아서 잘됐네",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019060.1.8",
          "form": "응 일주일 당겨져서",
          "original_form": "응 일주일 당겨져서",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019060.1.9",
          "form": "개강전이라 좋네",
          "original_form": "개강전이라 좋네",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019060.1.10",
          "form": "응응 잘됐어",
          "original_form": "응응 잘됐어",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019060.1.11",
          "form": "코로나 심한데 조금이라도 빨리 면역력 생기면 좋지",
          "original_form": "코로나 심한데 조금이라도 빨리 면역력 생기면 좋지",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019060.1.12",
          "form": "나 오늘 운동 가는데",
          "original_form": "나 오늘 운동 가는데",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019060.1.13",
          "form": "먹고싶은 것 있음 사갈게",
          "original_form": "먹고싶은 것 있음 사갈게",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019060.1.14",
          "form": "오 생각해볼게",
          "original_form": "오 생각해볼게",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019060.1.15",
          "form": "몇시끝나?",
          "original_form": "몇시끝나?",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019060.1.16",
          "form": "코로나 백신 후기들이 다 식욕이 오른다고",
          "original_form": "코로나 백신 후기들이 다 식욕이 오른다고",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019060.1.17",
          "form": "오늘은 그룹레슨이라 7시 20분에 끝나",
          "original_form": "오늘은 그룹레슨이라 7시 20분에 끝나",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019060.1.18",
          "form": "오키오키 그룹레슨하면서 운동 잘하구",
          "original_form": "오키오키 그룹레슨하면서 운동 잘하구",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019060.1.19",
          "form": "건강을 위해 필요한게 있음 말할게 ㅋㅋ",
          "original_form": "건강을 위해 필요한게 있음 말할게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019060.1.20",
          "form": "그랴그랴",
          "original_form": "그랴그랴",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019060.1.21",
          "form": "잘 먹어야 건강히 이겨낼 수 있는겨",
          "original_form": "잘 먹어야 건강히 이겨낼 수 있는겨",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019060.1.22",
          "form": "ㅋㅋ좀있으면 퇴근하겠네",
          "original_form": "ㅋㅋ좀있으면 퇴근하겠네",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019060.1.23",
          "form": "퇴근이 정신건강에 젤 좋은거 아닐쥐",
          "original_form": "퇴근이 정신건강에 젤 좋은거 아닐쥐",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019060.1.24",
          "form": "응 이제 퇴근하고 운동 갑니다~",
          "original_form": "응 이제 퇴근하고 운동 갑니다~",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019060.1.25",
          "form": "운동 가기 전 정신건강을 위해 카페타임 가질 예정",
          "original_form": "운동 가기 전 정신건강을 위해 카페타임 가질 예정",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019060.1.26",
          "form": "오 좋아 마음의 안정을 찾으세요",
          "original_form": "오 좋아 마음의 안정을 찾으세요",
          "speaker_id": "2",
          "time": "20210827 17:18"
        }
      ]
    }
  ]
