      "utterance": [
        {
          "id": "MDRW2100034405.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100034405.1.2",
          "form": "네 저는 키우고있어요",
          "original_form": "네 저는 키우고있어요",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100034405.1.3",
          "form": "name1님은 반려동물 키우시나여?",
          "original_form": "&name1&님은 반려동물 키우시나여?",
          "speaker_id": "2",
          "time": "20210828 08:52"
        },
        {
          "id": "MDRW2100034405.1.4",
          "form": "네 고양이 키우고 있어요",
          "original_form": "네 고양이 키우고 있어요",
          "speaker_id": "1",
          "time": "20210828 08:56"
        },
        {
          "id": "MDRW2100034405.1.5",
          "form": "어떤 동물 키우세요?",
          "original_form": "어떤 동물 키우세요?",
          "speaker_id": "1",
          "time": "20210828 08:57"
        },
        {
          "id": "MDRW2100034405.1.6",
          "form": "저도 고양이키우고있어요 코숏치즈태비에요",
          "original_form": "저도 고양이키우고있어요 코숏치즈태비에요",
          "speaker_id": "2",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100034405.1.7",
          "form": "1마리키우시나요",
          "original_form": "1마리키우시나요",
          "speaker_id": "2",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100034405.1.8",
          "form": "오 저는 2마리요!",
          "original_form": "오 저는 2마리요!",
          "speaker_id": "1",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100034405.1.9",
          "form": "엄청 귀여워요",
          "original_form": "엄청 귀여워요",
          "speaker_id": "1",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100034405.1.10",
          "form": "와 2마리 처음부터 같이키우셨나요?",
          "original_form": "와 2마리 처음부터 같이키우셨나요?",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034405.1.11",
          "form": "네 쌍둥이라 아는분께 분양받아서 같이 키웠어요!",
          "original_form": "네 쌍둥이라 아는분께 분양받아서 같이 키웠어요!",
          "speaker_id": "1",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034405.1.12",
          "form": "와 둘이 가족이라 진짜 사이넘좋겠어요 귀엽고ㅜㅜ",
          "original_form": "와 둘이 가족이라 진짜 사이넘좋겠어요 귀엽고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100034405.1.13",
          "form": "맞아욥 근데 또 생활을 같이하진 않더라구요",
          "original_form": "맞아욥 근데 또 생활을 같이하진 않더라구요",
          "speaker_id": "1",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034405.1.14",
          "form": "그쳐 애들이 자기만의 생활공간이 딱있드라고요 저희애도 낮에는 보이지가않아요",
          "original_form": "그쳐 애들이 자기만의 생활공간이 딱있드라고요 저희애도 낮에는 보이지가않아요",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100034405.1.15",
          "form": "낮에 숨어있나보네요?!",
          "original_form": "낮에 숨어있나보네요?!",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100034405.1.16",
          "form": "네 여름엔 더워서그런지 시원하니까 석에서 낮잠 푸지게자고 배고플때나 화장실갈때 저부르러나와요",
          "original_form": "네 여름엔 더워서그런지 시원하니까 석에서 낮잠 푸지게자고 배고플때나 화장실갈때 저부르러나와요",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100034405.1.17",
          "form": "밥챙기고 똥치우라고 ㅋㅋ..",
          "original_form": "밥챙기고 똥치우라고 ㅋㅋ..",
          "speaker_id": "2",
          "time": "20210828 19:10"
        },
        {
          "id": "MDRW2100034405.1.18",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100034405.1.19",
          "form": "배고플때나 필요할때 나오는건 다 똑같은거군요ㅋㅋ",
          "original_form": "배고플때나 필요할때 나오는건 다 똑같은거군요ㅋㅋ",
