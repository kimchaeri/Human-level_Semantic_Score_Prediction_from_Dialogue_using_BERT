        },
        {
          "id": "MDRW2100017069.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017069.1.3",
          "form": "동물 뭐 좋아하세요",
          "original_form": "동물 뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017069.1.4",
          "form": "저 강아지좋아해쇼",
          "original_form": "저 강아지좋아해쇼",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100017069.1.5",
          "form": "좋아하는 동물 있으신가요",
          "original_form": "좋아하는 동물 있으신가요",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017069.1.6",
          "form": "전 토끼랑 부엉이 좋아해요",
          "original_form": "전 토끼랑 부엉이 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017069.1.7",
          "form": "토끼는 키우고 있어요",
          "original_form": "토끼는 키우고 있어요",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017069.1.8",
          "form": "토끼 너무귀엽겠어요",
          "original_form": "토끼 너무귀엽겠어요",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017069.1.9",
          "form": "토끼는 어떤가요? 음 저는 허스키키우는데 토끼는 잘 몰라 궁금하네요",
          "original_form": "토끼는 어떤가요? 음 저는 허스키키우는데 토끼는 잘 몰라 궁금하네요",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017069.1.10",
          "form": "와 허스키",
          "original_form": "와 허스키",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017069.1.11",
          "form": "대형견 키우기 어려울텐데 신기해요",
          "original_form": "대형견 키우기 어려울텐데 신기해요",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017069.1.12",
          "form": "여름 많이 타지 않아요?",
          "original_form": "여름 많이 타지 않아요?",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017069.1.13",
          "form": "맞아요 아무래도 털도 빠지고 여름도타고 해서",
          "original_form": "맞아요 아무래도 털도 빠지고 여름도타고 해서",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017069.1.14",
          "form": "에어컨 필수로 틀어놔요",
          "original_form": "에어컨 필수로 틀어놔요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017069.1.15",
          "form": "덥겠다 애기 ㅠㅠ",
          "original_form": "덥겠다 애기 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017069.1.16",
          "form": "토끼도 털 동물이라 여름 에어컨 필수인대",
          "original_form": "토끼도 털 동물이라 여름 에어컨 필수인대",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017069.1.17",
          "form": "허스키는 에어컨 붙박이겠어요",
          "original_form": "허스키는 에어컨 붙박이겠어요",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017069.1.18",
          "form": "토끼도 필수구납..!",
          "original_form": "토끼도 필수구납..!",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017069.1.19",
          "form": "네 집안온도 25도 위로는 안올려요",
          "original_form": "네 집안온도 25도 위로는 안올려요",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017069.1.20",
          "form": "얘도 땀샘이 없어서",
          "original_form": "얘도 땀샘이 없어서",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017069.1.21",
          "form": "더위를 많이타서",
          "original_form": "더위를 많이타서",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017069.1.22",
          "form": "25도;;;",
          "original_form": "25도;;;",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017069.1.23",
          "form": "아 토끼도 땀샘이 없구나",
          "original_form": "아 토끼도 땀샘이 없구나",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017069.1.24",
          "form": "여름이 없네요",
          "original_form": "여름이 없네요",
          "speaker_id": "2",
          "time": "20210826 17:21"
        }
      ]
