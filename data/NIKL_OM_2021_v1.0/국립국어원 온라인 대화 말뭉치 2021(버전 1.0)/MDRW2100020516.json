        },
        {
          "id": "MDRW2100020516.1.2",
          "form": "풉했습니다",
          "original_form": "풉했습니다",
          "speaker_id": "1",
          "time": "20210828 08:54"
        },
        {
          "id": "MDRW2100020516.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:54"
        },
        {
          "id": "MDRW2100020516.1.4",
          "form": "같은 학과 나오셨나봐요",
          "original_form": "같은 학과 나오셨나봐요",
          "speaker_id": "1",
          "time": "20210828 08:54"
        },
        {
          "id": "MDRW2100020516.1.5",
          "form": "웃겼다니 다행입니다",
          "original_form": "웃겼다니 다행입니다",
          "speaker_id": "2",
          "time": "20210828 08:54"
        },
        {
          "id": "MDRW2100020516.1.6",
          "form": "와 먹잘대에서도 A반을 졸업하신듯",
          "original_form": "와 먹잘대에서도 A반을 졸업하신듯",
          "speaker_id": "2",
          "time": "20210828 08:54"
        },
        {
          "id": "MDRW2100020516.1.7",
          "form": "그쵸 성실하게 공부했습니다",
          "original_form": "그쵸 성실하게 공부했습니다",
          "speaker_id": "1",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.8",
          "form": "볶음밥은 좋아하시나요",
          "original_form": "볶음밥은 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.9",
          "form": "볶음밥 좋아하는데 사실",
          "original_form": "볶음밥 좋아하는데 사실",
          "speaker_id": "1",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.10",
          "form": "잡채밥을 더 선호합니다",
          "original_form": "잡채밥을 더 선호합니다",
          "speaker_id": "1",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.11",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.12",
          "form": "말이 필요없죠",
          "original_form": "말이 필요없죠",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.13",
          "form": "볶음밥은 잘하는 집이 드물거든요",
          "original_form": "볶음밥은 잘하는 집이 드물거든요",
          "speaker_id": "1",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.14",
          "form": "잡채너무맛있어",
          "original_form": "잡채너무맛있어",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.15",
          "form": "그쵸 잡채는 그럭저럭 중타는 다쳐요",
          "original_form": "그쵸 잡채는 그럭저럭 중타는 다쳐요",
          "speaker_id": "1",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.16",
          "form": "맞아요 약간 중식자체를 잘하는집이",
          "original_form": "맞아요 약간 중식자체를 잘하는집이",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.17",
          "form": "드물어요",
          "original_form": "드물어요",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020516.1.18",
          "form": "맞아요  ㅠㅠ",
          "original_form": "맞아요  ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 08:56"
        },
        {
          "id": "MDRW2100020516.1.19",
