      "utterance": [
        {
          "id": "MDRW2100033036.1.1",
          "form": "우와 리유저블 컵 쓰니?",
          "original_form": "우와 리유저블 컵 쓰니?",
          "speaker_id": "2",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100033036.1.2",
          "form": "텀블러가급적이면갖고다니려구해!",
          "original_form": "텀블러가급적이면갖고다니려구해!",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100033036.1.3",
          "form": "근처카페갈때는!",
          "original_form": "근처카페갈때는!",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100033036.1.4",
          "form": "뚭은?",
          "original_form": "뚭은?",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100033036.1.5",
          "form": "글쿠나 난 회사에서만 쓰고 무거워서 잘 안들고다녀",
          "original_form": "글쿠나 난 회사에서만 쓰고 무거워서 잘 안들고다녀",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033036.1.6",
          "form": "요즘 대나무 칫솔 써",
          "original_form": "요즘 대나무 칫솔 써",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033036.1.7",
          "form": "오잉 대나무칫솔? 아그때그건가",
          "original_form": "오잉 대나무칫솔? 아그때그건가",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033036.1.8",
          "form": "아로마티카에서 치약 사면서 받았어",
          "original_form": "아로마티카에서 치약 사면서 받았어",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033036.1.9",
          "form": "친환경적인칫솔을쓰는구낭",
          "original_form": "친환경적인칫솔을쓰는구낭",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033036.1.10",
          "form": "좋은것같아 근데 개인이 그러능건 한계같아",
          "original_form": "좋은것같아 근데 개인이 그러능건 한계같아",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033036.1.11",
          "form": "그치 ㅜㅜ아무래두",
          "original_form": "그치 ㅜㅜ아무래두",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033036.1.12",
          "form": "정부나 기업에서 나서줘야하는데",
          "original_form": "정부나 기업에서 나서줘야하는데",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033036.1.13",
          "form": "에어컨 아끼며 트는데 쇼핑센터는 펑펑 화장실에도 22도 더라고",
          "original_form": "에어컨 아끼며 트는데 쇼핑센터는 펑펑 화장실에도 22도 더라고",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033036.1.14",
          "form": "일하면서느끼지만",
          "original_form": "일하면서느끼지만",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033036.1.15",
          "form": "허무했어",
          "original_form": "허무했어",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033036.1.16",
          "form": "재활용도개판이야",
          "original_form": "재활용도개판이야",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033036.1.17",
          "form": "그치 그럴거같아 다들",
          "original_form": "그치 그럴거같아 다들",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033036.1.18",
          "form": "쇼핑센터는춥다추워",
          "original_form": "쇼핑센터는춥다추워",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033036.1.19",
          "form": "얼어 죽것어",
          "original_form": "얼어 죽것어",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033036.1.20",
          "form": "맞아 놀랬어 화장실 22도 미쳤냐고",
          "original_form": "맞아 놀랬어 화장실 22도 미쳤냐고",
