      "utterance": [
        {
          "id": "MDRW2100003481.1.1",
          "form": "보통 싸우고 헤어지셨어요?",
          "original_form": "보통 싸우고 헤어지셨어요?",
          "speaker_id": "2",
          "time": "20210820 23:38"
        },
        {
          "id": "MDRW2100003481.1.2",
          "form": "2년차 고비 넘기고는 쭉 갔는데",
          "original_form": "2년차 고비 넘기고는 쭉 갔는데",
          "speaker_id": "2",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003481.1.3",
          "form": "그랬던거 같아요ㅋㅋㅋ",
          "original_form": "그랬던거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003481.1.4",
          "form": "아름다운 이별은 없었어요 ...  아 있었네요ㅋ",
          "original_form": "아름다운 이별은 없었어요 ...  아 있었네요ㅋ",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003481.1.5",
          "form": "있었다구요?",
          "original_form": "있었다구요?",
          "speaker_id": "2",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003481.1.6",
          "form": "헤어지고 친구가 되셨나요?",
          "original_form": "헤어지고 친구가 되셨나요?",
          "speaker_id": "2",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003481.1.7",
          "form": "ㅋㅋㅋㅋㅋ헤어지고 친구가 된\r\n친구도 있긴 하네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ헤어지고 친구가 된\r\n친구도 있긴 하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003481.1.8",
          "form": "전 헤어지고 친구로 지내본적 한번도 없어요",
          "original_form": "전 헤어지고 친구로 지내본적 한번도 없어요",
          "speaker_id": "2",
          "time": "20210820 23:49"
        },
        {
          "id": "MDRW2100003481.1.9",
          "form": "연애 많이 해보셨나요?",
          "original_form": "연애 많이 해보셨나요?",
          "speaker_id": "1",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003481.1.10",
          "form": "지금처럼 긴 연애를 하셨다면\r\n많은 분을 만나시진 않으셨을듯 한데",
          "original_form": "지금처럼 긴 연애를 하셨다면\r\n많은 분을 만나시진 않으셨을듯 한데",
          "speaker_id": "1",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003481.1.11",
          "form": "저는 한번사귀면 기본3년이어서",
          "original_form": "저는 한번사귀면 기본3년이어서",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003481.1.12",
          "form": "많이는 안해봤어요 ㅋ",
          "original_form": "많이는 안해봤어요 ㅋ",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003481.1.13",
          "form": "와... 기본이 3년",
          "original_form": "와... 기본이 3년",
          "speaker_id": "1",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003481.1.14",
          "form": "제일긴게 6년",
          "original_form": "제일긴게 6년",
          "speaker_id": "2",
          "time": "20210821 00:05"
        },
        {
          "id": "MDRW2100003481.1.15",
          "form": "짧았던건 다 썸이었어요",
          "original_form": "짧았던건 다 썸이었어요",
          "speaker_id": "2",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003481.1.16",
          "form": "실례지만 name2님 나이가 어떻게 되세요?",
          "original_form": "실례지만 &name2&님 나이가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210821 00:08"
        },
        {
          "id": "MDRW2100003481.1.17",
          "form": "저요?",
          "original_form": "저요?",
          "speaker_id": "2",
          "time": "20210821 08:14"
        },
        {
          "id": "MDRW2100003481.1.18",
          "form": "아마 님보다 많을지도....???",
          "original_form": "아마 님보다 많을지도....???",
          "speaker_id": "2",
          "time": "20210821 08:14"
        },
        {
          "id": "MDRW2100003481.1.19",
          "form": "30후반?",
          "original_form": "30후반?",
          "speaker_id": "2",
          "time": "20210821 08:14"
        },
        {
          "id": "MDRW2100003481.1.20",
          "form": "후반이면 저보다는 많으시네요^^",
          "original_form": "후반이면 저보다는 많으시네요^^",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003481.1.21",
          "form": "주말인데 데이트 하세요?",
          "original_form": "주말인데 데이트 하세요?",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003481.1.22",
          "form": "그럴줄 알았어요 ㅋㅋ",
          "original_form": "그럴줄 알았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003481.1.23",
          "form": "데이트해야죠",
          "original_form": "데이트해야죠",
          "speaker_id": "2",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003481.1.24",
          "form": "어디가는게 좋을까요?",
          "original_form": "어디가는게 좋을까요?",
          "speaker_id": "2",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003481.1.25",
          "form": "저도 궁금해서요ㅋㅋㅋ",
          "original_form": "저도 궁금해서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003481.1.26",
          "form": "코시국 이후 데이트들은\r\n어떻게 하는지 여쭤보려 했어여 ㅋㅋ",
          "original_form": "코시국 이후 데이트들은\r\n어떻게 하는지 여쭤보려 했어여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003481.1.27",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003481.1.28",
          "form": "여친이 진짜 코로나에 민감해서",
          "original_form": "여친이 진짜 코로나에 민감해서",
          "speaker_id": "2",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003481.1.29",
          "form": "차에서 음식 시켜먹고 그랬는데",
          "original_form": "차에서 음식 시켜먹고 그랬는데",
          "speaker_id": "2",
          "time": "20210821 08:19"
        },
        {
          "id": "MDRW2100003481.1.30",
          "form": "진짜 못할짓.....",
          "original_form": "진짜 못할짓.....",
          "speaker_id": "2",
          "time": "20210821 08:19"
        },
        {
          "id": "MDRW2100003481.1.31",
          "form": "극장도 자동차극장만 가고",
          "original_form": "극장도 자동차극장만 가고",
          "speaker_id": "2",
          "time": "20210821 08:19"
        },
        {
          "id": "MDRW2100003481.1.32",
          "form": "저희도 아이들이 있다보니",
          "original_form": "저희도 아이들이 있다보니",
          "speaker_id": "1",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003481.1.33",
          "form": "외출도 최대한 자제하기도 하고",
          "original_form": "외출도 최대한 자제하기도 하고",
          "speaker_id": "1",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003481.1.34",
          "form": "외출해도 차에서 먹어여ㅠㅠ",
          "original_form": "외출해도 차에서 먹어여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003481.1.35",
          "form": "아이들중 초딩도 있나요?",
          "original_form": "아이들중 초딩도 있나요?",
          "speaker_id": "2",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003481.1.36",
          "form": "아니요:) 미취학아동들입니당ㅋㅋ",
          "original_form": "아니요:) 미취학아동들입니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003481.1.37",
          "form": "자동차 극장은 어때요?",
          "original_form": "자동차 극장은 어때요?",
          "speaker_id": "1",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003481.1.38",
          "form": "초등학생때 엄마아빠랑 가본 이후로",
          "original_form": "초등학생때 엄마아빠랑 가본 이후로",
          "speaker_id": "1",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003481.1.39",
          "form": "가본적이 없는거 같아요^^",
          "original_form": "가본적이 없는거 같아요^^",
          "speaker_id": "1",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003481.1.40",
          "form": "자동차극장 좋아요",
          "original_form": "자동차극장 좋아요",
          "speaker_id": "2",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003481.1.41",
          "form": "아이들데리고 애니메이션보러 가보세요",
          "original_form": "아이들데리고 애니메이션보러 가보세요",
          "speaker_id": "2",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003481.1.42",
          "form": "아마 난장판될듯...??ㅋㅋ",
          "original_form": "아마 난장판될듯...??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003481.1.43",
          "form": "자동차극장 이용 꿀팁 있나여?ㅋㅋㅋ",
          "original_form": "자동차극장 이용 꿀팁 있나여?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:23"
        },
        {
          "id": "MDRW2100003481.1.44",
          "form": "앞자리를 쟁취하세요 ㅋㅋㅋㅋ",
          "original_form": "앞자리를 쟁취하세요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:24"
        },
        {
          "id": "MDRW2100003481.1.45",
          "form": "뒤로갈수록 별로",
          "original_form": "뒤로갈수록 별로",
          "speaker_id": "2",
          "time": "20210821 08:25"
        },
        {
          "id": "MDRW2100003481.1.46",
          "form": "애들데리고 극장 못가니까",
          "original_form": "애들데리고 극장 못가니까",
          "speaker_id": "2",
          "time": "20210821 08:26"
        },
        {
          "id": "MDRW2100003481.1.47",
          "form": "너무 앞쪽이여도\r\n스크린 보기 안불편해요?",
          "original_form": "너무 앞쪽이여도\r\n스크린 보기 안불편해요?",
          "speaker_id": "1",
          "time": "20210821 08:26"
        },
        {
          "id": "MDRW2100003481.1.48",
