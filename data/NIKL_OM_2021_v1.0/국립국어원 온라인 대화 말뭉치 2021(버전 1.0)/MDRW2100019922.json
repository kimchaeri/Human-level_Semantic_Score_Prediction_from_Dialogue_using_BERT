      "utterance": [
        {
          "id": "MDRW2100019922.1.1",
          "form": "게임하세요",
          "original_form": "게임하세요",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019922.1.2",
          "form": "네 농사지어요 레알팜",
          "original_form": "네 농사지어요 레알팜",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100019922.1.3",
          "form": "게임하세요?",
          "original_form": "게임하세요?",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100019922.1.4",
          "form": "아 뭐 수확하셨어요",
          "original_form": "아 뭐 수확하셨어요",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100019922.1.5",
          "form": "전 안해요ㅠ",
          "original_form": "전 안해요ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100019922.1.6",
          "form": "관심있는게임도 없으시죵 \r\n예전에짜요짜요타이쿤이런거해보셨나요?",
          "original_form": "관심있는게임도 없으시죵 \r\n예전에짜요짜요타이쿤이런거해보셨나요?",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100019922.1.7",
          "form": "ㅔ네 붕어빵이랑 스시",
          "original_form": "ㅔ네 붕어빵이랑 스시",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100019922.1.8",
          "form": "재밌겟다",
          "original_form": "재밌겟다",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100019922.1.9",
          "form": "와붕어빵..스시..재밌겠다미쳤당",
          "original_form": "와붕어빵..스시..재밌겠다미쳤당",
          "speaker_id": "2",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100019922.1.10",
          "form": "무슨 타이쿤 했오",
          "original_form": "무슨 타이쿤 했오",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100019922.1.11",
          "form": "붕어빵  짜요짜요  생과일!!!!!",
          "original_form": "붕어빵  짜요짜요  생과일!!!!!",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100019922.1.12",
          "form": "생과일도 터이쿤잇나요",
          "original_form": "생과일도 터이쿤잇나요",
          "speaker_id": "1",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100019922.1.13",
          "form": "네네 생과일타이쿤겁나재밌어요",
          "original_form": "네네 생과일타이쿤겁나재밌어요",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100019922.1.14",
          "form": "오 궁금래요",
          "original_form": "오 궁금래요",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100019922.1.15",
          "form": "저는 게임 한두번 하면 질리는거같아여ㅠ",
          "original_form": "저는 게임 한두번 하면 질리는거같아여ㅠ",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100019922.1.16",
          "form": "그런성격이좋은거같기도해요",
          "original_form": "그런성격이좋은거같기도해요",
          "speaker_id": "2",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100019922.1.17",
          "form": "게임에시간너무쓰면아깝자나용",
          "original_form": "게임에시간너무쓰면아깝자나용",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100019922.1.18",
          "form": "그래도스트레스 풀린다몀 좋죠",
          "original_form": "그래도스트레스 풀린다몀 좋죠",
