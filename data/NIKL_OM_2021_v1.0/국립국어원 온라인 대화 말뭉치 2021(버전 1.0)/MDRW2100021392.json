      },
      "utterance": [
        {
          "id": "MDRW2100021392.1.1",
          "form": "메뉴 정하셧어요?",
          "original_form": "메뉴 정하셧어요?",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100021392.1.2",
          "form": "아니요\n일단 계란말이 결정이요",
          "original_form": "아니요\n일단 계란말이 결정이요",
          "speaker_id": "2",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100021392.1.3",
          "form": "오 그렇군요.",
          "original_form": "오 그렇군요.",
          "speaker_id": "1",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100021392.1.4",
          "form": "엄마가 정하셨어요",
          "original_form": "엄마가 정하셨어요",
          "speaker_id": "2",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100021392.1.5",
          "form": "오 계란말이 잘하셔요?",
          "original_form": "오 계란말이 잘하셔요?",
          "speaker_id": "1",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100021392.1.6",
          "form": "그냥 잘 돌돌 말아요ㅋㅋㅋ",
          "original_form": "그냥 잘 돌돌 말아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100021392.1.7",
          "form": "잘하는지는 모르겠네요",
          "original_form": "잘하는지는 모르겠네요",
          "speaker_id": "2",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100021392.1.8",
          "form": "어머님은요?",
          "original_form": "어머님은요?",
          "speaker_id": "1",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100021392.1.9",
          "form": "계란말이는 항상 저보고 하라고 하셔요",
          "original_form": "계란말이는 항상 저보고 하라고 하셔요",
          "speaker_id": "2",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021392.1.10",
          "form": "오 잘하시나보다",
          "original_form": "오 잘하시나보다",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100021392.1.11",
          "form": "귀찮다고 저한테 시키는 종류가 몇가지 있는데 그 중 하나에요ㅋㅋㅋ",
          "original_form": "귀찮다고 저한테 시키는 종류가 몇가지 있는데 그 중 하나에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:39"
        },
        {
          "id": "MDRW2100021392.1.12",
          "form": "오.. 저는 엄마 못따라가는데",
          "original_form": "오.. 저는 엄마 못따라가는데",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021392.1.13",
          "form": "엄마가 요리 잘하시나봐요",
          "original_form": "엄마가 요리 잘하시나봐요",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021392.1.14",
          "form": "가끔 복불복이잇지만 오늘 요리도 성공하신것같아요.ㅋㅋ",
          "original_form": "가끔 복불복이잇지만 오늘 요리도 성공하신것같아요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100021392.1.15",
          "form": "오 오늘은 어떤 요리를 하셯ㄴ었나요?",
          "original_form": "오 오늘은 어떤 요리를 하셯ㄴ었나요?",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021392.1.16",
          "form": "된장국이요!\n근데 너무 칼칼한 냄새가나서 걱정이네요",
          "original_form": "된장국이요!\n근데 너무 칼칼한 냄새가나서 걱정이네요",
          "speaker_id": "1",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021392.1.17",
          "form": "매콤하게 만드셨나봐요",
          "original_form": "매콤하게 만드셨나봐요",
          "speaker_id": "2",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100021392.1.18",
          "form": "아빠가 고.추를 너무 많이 따와서 그거 해치우느라 그런것같아요 ㅠ",
          "original_form": "아빠가 고.추를 너무 많이 따와서 그거 해치우느라 그런것같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 18:47"
        }
      ]
