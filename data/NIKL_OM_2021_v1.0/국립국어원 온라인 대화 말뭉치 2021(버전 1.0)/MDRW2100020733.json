      "utterance": [
        {
          "id": "MDRW2100020733.1.1",
          "form": "연애중이신가여?",
          "original_form": "연애중이신가여?",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020733.1.2",
          "form": "네네 이제 근데 일주일됐어요",
          "original_form": "네네 이제 근데 일주일됐어요",
          "speaker_id": "2",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020733.1.3",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "2",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020733.1.4",
          "form": "일주일요?",
          "original_form": "일주일요?",
          "speaker_id": "1",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020733.1.5",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020733.1.6",
          "form": "저도 연애중입니다 ㅎㅎ",
          "original_form": "저도 연애중입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020733.1.7",
          "form": "넹 막 만나기 시작했어요 ㅋㅋㅋ얼마나 만나셨나요",
          "original_form": "넹 막 만나기 시작했어요 ㅋㅋㅋ얼마나 만나셨나요",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020733.1.8",
          "form": "5년째요 ㅋㅋㅋ",
          "original_form": "5년째요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100020733.1.9",
          "form": "새내기 커플이군여",
          "original_form": "새내기 커플이군여",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100020733.1.10",
          "form": "보고싶지 않아요?ㅋㅋ",
          "original_form": "보고싶지 않아요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100020733.1.11",
          "form": "오늘 토욜인데 데이트 안하나요?",
          "original_form": "오늘 토욜인데 데이트 안하나요?",
          "speaker_id": "1",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100020733.1.12",
          "form": "와아 5년 오래만나셨네요 ㅋㅋㅋ",
          "original_form": "와아 5년 오래만나셨네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100020733.1.13",
          "form": "저도 그전 연애가 약 6년의 장기연애여서 그다음 인연이 있을까 걱정했는데",
          "original_form": "저도 그전 연애가 약 6년의 장기연애여서 그다음 인연이 있을까 걱정했는데",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100020733.1.14",
          "form": "소개팅으로 겨우 만났어요 ㅋㅋㅋ 내일 만나려구요",
          "original_form": "소개팅으로 겨우 만났어요 ㅋㅋㅋ 내일 만나려구요",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100020733.1.15",
          "form": "너무 축하드려요~",
          "original_form": "너무 축하드려요~",
          "speaker_id": "1",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020733.1.16",
          "form": "저도 소개팅",
          "original_form": "저도 소개팅",
          "speaker_id": "1",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020733.1.17",
          "form": "우와 소개팅으로 만나셨군요",
          "original_form": "우와 소개팅으로 만나셨군요",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020733.1.18",
          "form": "장기 연애후 소개팅 몇번만에 만나신거에요?",
          "original_form": "장기 연애후 소개팅 몇번만에 만나신거에요?",
          "speaker_id": "1",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020733.1.19",
          "form": "소개팅 한 5번?",
          "original_form": "소개팅 한 5번?",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020733.1.20",
          "form": "저도 이전연애가 3년이어서",
          "original_form": "저도 이전연애가 3년이어서",
          "speaker_id": "1",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020733.1.21",
          "form": "자만추 좋아하다가 소개팅이 너무 낯설더라구요 ㅋㅋㅋ",
          "original_form": "자만추 좋아하다가 소개팅이 너무 낯설더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020733.1.22",
          "form": "1년쉬고 소개팅 10번 넘게하고 포기직전에 만났어요 ㅋㅋ",
          "original_form": "1년쉬고 소개팅 10번 넘게하고 포기직전에 만났어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020733.1.23",
          "form": "와 10번 ㅋㅋㅋㅋ",
          "original_form": "와 10번 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020733.1.24",
          "form": "한 5번만 해도 현타오더라구요 ㅋㅋㅋㅋ",
          "original_form": "한 5번만 해도 현타오더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020733.1.25",
          "form": "진짜 인연도 없는거 같고",
          "original_form": "진짜 인연도 없는거 같고",
          "speaker_id": "1",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020733.1.26",
          "form": "시간낭비인거 같아서",
          "original_form": "시간낭비인거 같아서",
          "speaker_id": "1",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020733.1.27",
          "form": "마자요 현타",
          "original_form": "마자요 현타",
          "speaker_id": "1",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020733.1.28",
          "form": "그래서 마지막이다했는데 그때 만났다는...",
          "original_form": "그래서 마지막이다했는데 그때 만났다는...",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020733.1.29",
          "form": "ㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020733.1.30",
          "form": "저두 그랬어요ㅋㅋㅋ 5년이면 결혼 준비도 하시나요?",
          "original_form": "저두 그랬어요ㅋㅋㅋ 5년이면 결혼 준비도 하시나요?",
          "speaker_id": "2",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020733.1.31",
          "form": "때가 있고 인연도 다 있긴 한거 같아요 ㅎㅎ",
          "original_form": "때가 있고 인연도 다 있긴 한거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020733.1.32",
          "form": "네 결혼하려구여",
          "original_form": "네 결혼하려구여",
          "speaker_id": "1",
          "time": "20210828 12:09"
        },
        {
          "id": "MDRW2100020733.1.33",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:09"
        },
        {
          "id": "MDRW2100020733.1.34",
          "form": "꺄 이미 날이 정해졌나요",
          "original_form": "꺄 이미 날이 정해졌나요",
          "speaker_id": "2",
          "time": "20210828 12:09"
        },
        {
          "id": "MDRW2100020733.1.35",
          "form": "날정하고 하려고했는데",
          "original_form": "날정하고 하려고했는데",
          "speaker_id": "1",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100020733.1.36",
          "form": "4단계.....",
          "original_form": "4단계.....",
          "speaker_id": "1",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100020733.1.37",
          "form": "만나는게 불가",
          "original_form": "만나는게 불가",
          "speaker_id": "1",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100020733.1.38",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100020733.1.39",
          "form": "일주일됐으면",
          "original_form": "일주일됐으면",
          "speaker_id": "1",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100020733.1.40",
          "form": "데이트할때 어디가요??",
          "original_form": "데이트할때 어디가요??",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020733.1.41",
          "form": "음 아직 식당 카페 정도를 제일 자주갔고",
          "original_form": "음 아직 식당 카페 정도를 제일 자주갔고",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020733.1.42",
          "form": "얼마전에 영화관 한번 가고",
          "original_form": "얼마전에 영화관 한번 가고",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020733.1.43",
          "form": "이제 내일 동물원 가려고요ㅋㅋㅋㅋ",
          "original_form": "이제 내일 동물원 가려고요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020733.1.44",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020733.1.45",
          "form": "동물원",
          "original_form": "동물원",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020733.1.46",
          "form": "4년전에 갔던",
          "original_form": "4년전에 갔던",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020733.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020733.1.48",
          "form": "진짜 풋풋하다요",
          "original_form": "진짜 풋풋하다요",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020733.1.49",
          "form": "ㅋㅋㅋㅋㅋ새내기를 위한 코스죠? ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ새내기를 위한 코스죠? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020733.1.50",
          "form": "요즘 진짜 갈곳없어요 ㅋㅋㅋㅋ",
          "original_form": "요즘 진짜 갈곳없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020733.1.51",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020733.1.52",
          "form": "놀이공원도 가줘야죠 ㅋㅋㅋ",
          "original_form": "놀이공원도 가줘야죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020733.1.53",
          "form": "영화관데이트도 좋은데",
          "original_form": "영화관데이트도 좋은데",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020733.1.54",
          "form": "자동차극장도 가보세요",
          "original_form": "자동차극장도 가보세요",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020733.1.55",
          "form": "앜 안그래도 저 부산 사는데 부산에 놀이공원 짓는 중이라",
          "original_form": "앜 안그래도 저 부산 사는데 부산에 놀이공원 짓는 중이라",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020733.1.56",
          "form": "기다리고 잇어요",
          "original_form": "기다리고 잇어요",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020733.1.57",
          "form": "자동차극장두 가고싶네요ㅎㅎㅎ",
          "original_form": "자동차극장두 가고싶네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020733.1.58",
          "form": "꼭 가보세요",
          "original_form": "꼭 가보세요",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020733.1.59",
          "form": "차안에서 맛난거 먹구",
          "original_form": "차안에서 맛난거 먹구",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020733.1.60",
          "form": "좋아요 ㅋㅋ",
          "original_form": "좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020733.1.61",
          "form": "네ㅋㅋㅋ 오늘도 보면 좋긴한데 이번주에 벌써 한 세번은 만나서 쉬기로 했어요...",
          "original_form": "네ㅋㅋㅋ 오늘도 보면 좋긴한데 이번주에 벌써 한 세번은 만나서 쉬기로 했어요...",
          "speaker_id": "2",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020733.1.62",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020733.1.63",
          "form": "세번",
          "original_form": "세번",
          "speaker_id": "1",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020733.1.64",
