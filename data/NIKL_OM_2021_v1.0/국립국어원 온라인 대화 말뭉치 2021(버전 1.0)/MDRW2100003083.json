      "utterance": [
        {
          "id": "MDRW2100003083.1.1",
          "form": "최근에 영화 뭐 봤어요?",
          "original_form": "최근에 영화 뭐 봤어요?",
          "speaker_id": "1",
          "time": "20210820 18:32"
        },
        {
          "id": "MDRW2100003083.1.2",
          "form": "안봤어요 뭐봤어요?",
          "original_form": "안봤어요 뭐봤어요?",
          "speaker_id": "2",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003083.1.3",
          "form": "전 좀 됐는데 블랙위도우 봤어요",
          "original_form": "전 좀 됐는데 블랙위도우 봤어요",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003083.1.4",
          "form": "오 그거 개봉 했어요?",
          "original_form": "오 그거 개봉 했어요?",
          "speaker_id": "2",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003083.1.5",
          "form": "네 7월에 개봉 했어요!",
          "original_form": "네 7월에 개봉 했어요!",
          "speaker_id": "1",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003083.1.6",
          "form": "아 코로나 이후로 극장을 안가서 못봤어요 재밌어요?",
          "original_form": "아 코로나 이후로 극장을 안가서 못봤어요 재밌어요?",
          "speaker_id": "2",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003083.1.7",
          "form": "네 전 마블시리즈 아예 안 봐서 이해 못 할까봐 걱정했는데",
          "original_form": "네 전 마블시리즈 아예 안 봐서 이해 못 할까봐 걱정했는데",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003083.1.8",
          "form": "그래도 되게 재밌더라구요",
          "original_form": "그래도 되게 재밌더라구요",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003083.1.9",
          "form": "아 저 마블시리즈 좋아해요 다봤는데.. 블랙위도우도 봐야겠어요",
          "original_form": "아 저 마블시리즈 좋아해요 다봤는데.. 블랙위도우도 봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003083.1.10",
          "form": "다른거는 보신거 있어요?",
          "original_form": "다른거는 보신거 있어요?",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003083.1.11",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003083.1.12",
          "form": "이제 2천명 가까이 되니까 무서워서 못 가겠어요..",
          "original_form": "이제 2천명 가까이 되니까 무서워서 못 가겠어요..",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003083.1.13",
          "form": "극장에서 팝콘 먹는 재미가 좋았는데 말이죠",
          "original_form": "극장에서 팝콘 먹는 재미가 좋았는데 말이죠",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003083.1.14",
          "form": "맞아요 다른 곳에서 먹는거랑 다른 느낌이에요",
          "original_form": "맞아요 다른 곳에서 먹는거랑 다른 느낌이에요",
          "speaker_id": "1",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003083.1.15",
          "form": "요즘엔 극장 내에서 음식을 못 먹게 하니까 입이 심심하더라구요",
          "original_form": "요즘엔 극장 내에서 음식을 못 먹게 하니까 입이 심심하더라구요",
          "speaker_id": "1",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003083.1.16",
          "form": "카라멜팝콘하고 어니언팝콘 반반해서 콜라에 먹었는데 아쉬워요",
          "original_form": "카라멜팝콘하고 어니언팝콘 반반해서 콜라에 먹었는데 아쉬워요",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003083.1.17",
          "form": "맛잘알이시네요",
          "original_form": "맛잘알이시네요",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003083.1.18",
          "form": "저도 딱 그렇게 먹어요ㅋㅋ",
          "original_form": "저도 딱 그렇게 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003083.1.19",
          "form": "소름!",
          "original_form": "소름!",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003083.1.20",
          "form": "앗..영화관도 역시 반반 탄산마니죠",
          "original_form": "앗..영화관도 역시 반반 탄산마니죠",
          "speaker_id": "2",
          "time": "20210820 18:44"
        }
