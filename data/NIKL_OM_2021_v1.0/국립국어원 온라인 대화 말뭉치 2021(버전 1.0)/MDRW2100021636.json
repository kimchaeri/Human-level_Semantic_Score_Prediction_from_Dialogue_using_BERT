      "utterance": [
        {
          "id": "MDRW2100021636.1.1",
          "form": "딴 사람이랑도 어쩌다 겜으로 빠졌는데",
          "original_form": "딴 사람이랑도 어쩌다 겜으로 빠졌는데",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021636.1.2",
          "form": "이분 은근 잘알인데 일코하는 것 같은데",
          "original_form": "이분 은근 잘알인데 일코하는 것 같은데",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021636.1.3",
          "form": "무슨 얘기 하는데?",
          "original_form": "무슨 얘기 하는데?",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021636.1.4",
          "form": "스위치 게임 얘기",
          "original_form": "스위치 게임 얘기",
          "speaker_id": "1",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021636.1.5",
          "form": "젤다랑 포켓몬",
          "original_form": "젤다랑 포켓몬",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021636.1.6",
          "form": "오... 젤다아",
          "original_form": "오... 젤다아",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021636.1.7",
          "form": "젤다 재미ㅆ아",
          "original_form": "젤다 재미ㅆ아",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021636.1.8",
          "form": "근데 졸업한지 언젠데 학교 생활이야",
          "original_form": "근데 졸업한지 언젠데 학교 생활이야",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021636.1.9",
          "form": "글게... 학교 얘기 뭐 해바",
          "original_form": "글게... 학교 얘기 뭐 해바",
          "speaker_id": "2",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021636.1.10",
          "form": "시험 시러맨...",
          "original_form": "시험 시러맨...",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021636.1.11",
          "form": "요즘은 개학시즌이에요",
          "original_form": "요즘은 개학시즌이에요",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021636.1.12",
          "form": "아 알어",
          "original_form": "아 알어",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021636.1.13",
          "form": "어떤 사람이랑 학교 얘기 했어",
          "original_form": "어떤 사람이랑 학교 얘기 했어",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021636.1.14",
          "form": "학생이랑 햤어?",
          "original_form": "학생이랑 햤어?",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021636.1.15",
          "form": "ㅇㅇ대학생인듯",
          "original_form": "ㅇㅇ대학생인듯",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021636.1.16",
          "form": "대학생도 요즘 대면수업하나",
          "original_form": "대학생도 요즘 대면수업하나",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021636.1.17",
          "form": "비대면이랑 같이 한대",
          "original_form": "비대면이랑 같이 한대",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021636.1.18",
          "form": "병행이구나",
          "original_form": "병행이구나",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
