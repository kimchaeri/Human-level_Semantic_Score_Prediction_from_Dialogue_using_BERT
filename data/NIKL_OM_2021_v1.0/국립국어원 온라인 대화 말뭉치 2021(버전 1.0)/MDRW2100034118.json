      "utterance": [
        {
          "id": "MDRW2100034118.1.1",
          "form": "맞아요ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100034118.1.2",
          "form": "그래도 제 체감상 2018보단 덜 더웠던 것 같아요",
          "original_form": "그래도 제 체감상 2018보단 덜 더웠던 것 같아요",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100034118.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그나마...다행.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그나마...다행.",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100034118.1.4",
          "form": "아 정말요? 저는 이번이 제일 더웠던 거 같아요",
          "original_form": "아 정말요? 저는 이번이 제일 더웠던 거 같아요",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100034118.1.5",
          "form": "매년 선풍기로 버틸만 했는데 이번에는 에어컨은 안 틀 수가 없겠더라고요",
          "original_form": "매년 선풍기로 버틸만 했는데 이번에는 에어컨은 안 틀 수가 없겠더라고요",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100034118.1.6",
          "form": "헉 그래요?!",
          "original_form": "헉 그래요?!",
          "speaker_id": "2",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100034118.1.7",
          "form": "저는 올해도 에어컨없이 보냈답니다...",
          "original_form": "저는 올해도 에어컨없이 보냈답니다...",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100034118.1.8",
          "form": "흑흑 그나저나 대체 언제까지 비가올까요",
          "original_form": "흑흑 그나저나 대체 언제까지 비가올까요",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100034118.1.9",
          "form": "이번주까지는 오는 거 같아요ㅠㅠ",
          "original_form": "이번주까지는 오는 거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 19:00"
        },
        {
          "id": "MDRW2100034118.1.10",
          "form": "하ㅠㅠㅠㅠ 날씨보고 오니 다음주도 비가 오네요....",
          "original_form": "하ㅠㅠㅠㅠ 날씨보고 오니 다음주도 비가 오네요....",
          "speaker_id": "2",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100034118.1.11",
          "form": "빨래 안마르겠어요ㅠㅠㅠㅠㅠ",
          "original_form": "빨래 안마르겠어요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100034118.1.12",
          "form": "다음주도 오는 군요ㅠㅠ 빨래가 너무 안 말라요ㅠㅠ",
          "original_form": "다음주도 오는 군요ㅠㅠ 빨래가 너무 안 말라요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034118.1.13",
          "form": "아 맞아요ㅠㅠㅠㅠㅠ",
          "original_form": "아 맞아요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034118.1.14",
          "form": "저희집도 수건이...하나도 안마르더라고요 아놔!",
          "original_form": "저희집도 수건이...하나도 안마르더라고요 아놔!",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034118.1.15",
          "form": "제대로 못 마르면 냄새도 안 좋아서 다시 세탁해야 하고...ㅠㅠ",
          "original_form": "제대로 못 마르면 냄새도 안 좋아서 다시 세탁해야 하고...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034118.1.16",
          "form": "하 맞아요!!!!!!!",
          "original_form": "하 맞아요!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034118.1.17",
          "form": "큰일이에요 정말 ㅠㅠㅠ 아직도 안 말랐다는게...",
          "original_form": "큰일이에요 정말 ㅠㅠㅠ 아직도 안 말랐다는게...",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034118.1.18",
          "form": "쓸 수건이 줄어들어가면서 막 조급해져요 ㅋㅋㅋㅋ",
          "original_form": "쓸 수건이 줄어들어가면서 막 조급해져요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100034118.1.19",
          "form": "아 뭔지 알 거 같아요ㅋㅋㅠㅠ 축축한 수건으로 물기릉 닦을 순 없으니까...ㅠㅠ",
          "original_form": "아 뭔지 알 거 같아요ㅋㅋㅠㅠ 축축한 수건으로 물기릉 닦을 순 없으니까...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100034118.1.20",
          "form": "장마 때마다 건조기 사야 하나 고민한다니까요ㅋㅋㅋ",
          "original_form": "장마 때마다 건조기 사야 하나 고민한다니까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100034118.1.21",
          "form": "으악 맞아요!!!!!!!!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "으악 맞아요!!!!!!!!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100034118.1.22",
          "form": "진짜 장마 때마다 너무 괴로워요ㅠ",
          "original_form": "진짜 장마 때마다 너무 괴로워요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100034118.1.23",
          "form": "신발 젖으면 정말 최악이에요",
          "original_form": "신발 젖으면 정말 최악이에요",
          "speaker_id": "1",
