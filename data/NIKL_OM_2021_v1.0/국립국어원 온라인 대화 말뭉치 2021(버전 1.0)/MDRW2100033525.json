      "utterance": [
        {
          "id": "MDRW2100033525.1.1",
          "form": "여긴 비오다 해 떴어요\r\n거기는요",
          "original_form": "여긴 비오다 해 떴어요\r\n거기는요",
          "speaker_id": "2",
          "time": "20210824 11:14"
        },
        {
          "id": "MDRW2100033525.1.2",
          "form": "오 해떴어요?여긴 흐려요",
          "original_form": "오 해떴어요?여긴 흐려요",
          "speaker_id": "1",
          "time": "20210824 11:14"
        },
        {
          "id": "MDRW2100033525.1.3",
          "form": "출근길에 살짝 비오긴했는데",
          "original_form": "출근길에 살짝 비오긴했는데",
          "speaker_id": "1",
          "time": "20210824 11:14"
        },
        {
          "id": "MDRW2100033525.1.4",
          "form": "구름 사이 해가 있어요~",
          "original_form": "구름 사이 해가 있어요~",
          "speaker_id": "2",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100033525.1.5",
          "form": "남부지방우 비엄청 오나본데요",
          "original_form": "남부지방우 비엄청 오나본데요",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100033525.1.6",
          "form": "부산은 차가 물에 둥둥 떠나니던데요",
          "original_form": "부산은 차가 물에 둥둥 떠나니던데요",
          "speaker_id": "2",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100033525.1.7",
          "form": "그렇더라구요 제주도도 난리고",
          "original_form": "그렇더라구요 제주도도 난리고",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100033525.1.8",
          "form": "제주도도 그래요? 몰랐네요.",
          "original_form": "제주도도 그래요? 몰랐네요.",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100033525.1.9",
          "form": "네 제주 400미리까지 온대요ㅠ",
          "original_form": "네 제주 400미리까지 온대요ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100033525.1.10",
          "form": "제주도 비 엄청 오네요..",
          "original_form": "제주도 비 엄청 오네요..",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100033525.1.11",
          "form": "비피해 없어야될텐데",
          "original_form": "비피해 없어야될텐데",
          "speaker_id": "1",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100033525.1.12",
          "form": "날씨도 선선해서 슬슬 긴팔입어야할듯요",
          "original_form": "날씨도 선선해서 슬슬 긴팔입어야할듯요",
          "speaker_id": "1",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100033525.1.13",
          "form": "전 아직.. 몸에 열이 지금도 더워요 ㅎ",
          "original_form": "전 아직.. 몸에 열이 지금도 더워요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100033525.1.14",
          "form": "저도요 에어컨 아직 켜요",
          "original_form": "저도요 에어컨 아직 켜요",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100033525.1.15",
          "form": "날씨 예보 보니까 여긴 내일 새벽에나 비 올거 같아요.",
          "original_form": "날씨 예보 보니까 여긴 내일 새벽에나 비 올거 같아요.",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100033525.1.16",
          "form": "이번주는 계속 비소식있나봐요",
          "original_form": "이번주는 계속 비소식있나봐요",
