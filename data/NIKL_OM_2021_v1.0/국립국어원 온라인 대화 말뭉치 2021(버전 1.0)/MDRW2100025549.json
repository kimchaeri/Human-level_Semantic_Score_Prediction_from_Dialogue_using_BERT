      "utterance": [
        {
          "id": "MDRW2100025549.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ웅 이상이도 했었어 이번에",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ웅 이상이도 했었어 이번에",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025549.1.2",
          "form": "잘햇다고 하더라",
          "original_form": "잘햇다고 하더라",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025549.1.3",
          "form": "오.... 노래잘하던데",
          "original_form": "오.... 노래잘하던데",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025549.1.4",
          "form": "전 그분을 슬빵에서 나쁜병장으로만 봐서",
          "original_form": "전 그분을 슬빵에서 나쁜병장으로만 봐서",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025549.1.5",
          "form": "첨에 매칭이안되더라고",
          "original_form": "첨에 매칭이안되더라고",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025549.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나한테는 그 주말 드라마에서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나한테는 그 주말 드라마에서",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025549.1.7",
          "form": "사돈커플인가",
          "original_form": "사돈커플인가",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025549.1.8",
          "form": "그 이미지도 있어서",
          "original_form": "그 이미지도 있어서",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025549.1.9",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025549.1.10",
          "form": "맞아 그거 거기서 이미지 완전바뀜",
          "original_form": "맞아 그거 거기서 이미지 완전바뀜",
          "speaker_id": "1",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025549.1.11",
          "form": "그리고 놀뭐로 또 변신...",
          "original_form": "그리고 놀뭐로 또 변신...",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025549.1.12",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ 뮤배였다는데",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ 뮤배였다는데",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025549.1.13",
          "form": "놀뭐 보니깐 젠가 보러갔어도",
          "original_form": "놀뭐 보니깐 젠가 보러갔어도",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025549.1.14",
          "form": "후회안했을듯ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
