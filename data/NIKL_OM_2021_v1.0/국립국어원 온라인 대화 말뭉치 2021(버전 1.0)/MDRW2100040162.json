      "utterance": [
        {
          "id": "MDRW2100040162.1.1",
          "form": "아ㅎㅎ 실은 우리집도 고기 세팅하기 전 피자 한조각씩 뜯고 있어요",
          "original_form": "아ㅎㅎ 실은 우리집도 고기 세팅하기 전 피자 한조각씩 뜯고 있어요",
          "speaker_id": "2",
          "time": "20210904 12:25"
        },
        {
          "id": "MDRW2100040162.1.2",
          "form": "아ㅋㅋ 그러다가 배불러서 고기 못먹는거 아니에요? ㅋㅋ",
          "original_form": "아ㅋㅋ 그러다가 배불러서 고기 못먹는거 아니에요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100040162.1.3",
          "form": "그게 좀 걱정이긴 한덷ㄷㄷ",
          "original_form": "그게 좀 걱정이긴 한덷ㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100040162.1.4",
          "form": "딴가족들이 잘 못먹으면 삼겹살은 다 제 뱃속으로ㅎ",
          "original_form": "딴가족들이 잘 못먹으면 삼겹살은 다 제 뱃속으로ㅎ",
          "speaker_id": "2",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100040162.1.5",
          "form": "제가 대식가입니당",
          "original_form": "제가 대식가입니당",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100040162.1.6",
          "form": "아 사실 전략이었던건가요ㅋㅋㅋㅋㅋ",
          "original_form": "아 사실 전략이었던건가요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100040162.1.7",
          "form": "계획대로 되었네요ㅎ",
          "original_form": "계획대로 되었네요ㅎ",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100040162.1.8",
          "form": "식빵에 잼 발라 드시나요?",
          "original_form": "식빵에 잼 발라 드시나요?",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100040162.1.9",
          "form": "아님 그냥 우유랑?",
          "original_form": "아님 그냥 우유랑?",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100040162.1.10",
          "form": "쨈 발라서 먹고있어요",
          "original_form": "쨈 발라서 먹고있어요",
          "speaker_id": "1",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100040162.1.11",
          "form": "이것만 먹기엔 약간 부족한데 고민중이네요ㅋㅋㅋ",
          "original_form": "이것만 먹기엔 약간 부족한데 고민중이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100040162.1.12",
          "form": "아니 간식인데 당연하죠!!",
          "original_form": "아니 간식인데 당연하죠!!",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100040162.1.13",
          "form": "근데 너무 귀찮아요ㅋㅋㅋ",
          "original_form": "근데 너무 귀찮아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100040162.1.14",
          "form": "먹을땐 행복한데 맨날 요리는 귀찮아요ㅋㅋㅋ",
          "original_form": "먹을땐 행복한데 맨날 요리는 귀찮아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100040162.1.15",
          "form": "먹기전ㅜ 먹기후ㅜ 다 스킵하고 중간만 있었음 좋겠어요ㅎㅎ",
          "original_form": "먹기전ㅜ 먹기후ㅜ 다 스킵하고 중간만 있었음 좋겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100040162.1.16",
          "form": "맘같아선 맨날 배달음식 시켜먹고 싶어요ㅋㅋ",
          "original_form": "맘같아선 맨날 배달음식 시켜먹고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100040162.1.17",
          "form": "ㅎㅎ 요새 배달음식 너무 잘돼 있죠",
          "original_form": "ㅎㅎ 요새 배달음식 너무 잘돼 있죠",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100040162.1.18",
          "form": "돈만 있으면 맨날 배민 켰어요ㅋㅋㅋ",
          "original_form": "돈만 있으면 맨날 배민 켰어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100040162.1.19",
          "form": "배민ㅜㅜ 근데 음식값은 안 아까운데 배달비가 아까워요ㅜ",
          "original_form": "배민ㅜㅜ 근데 음식값은 안 아까운데 배달비가 아까워요ㅜ",
          "speaker_id": "2",
          "time": "20210904 12:38"
        }
      ]
