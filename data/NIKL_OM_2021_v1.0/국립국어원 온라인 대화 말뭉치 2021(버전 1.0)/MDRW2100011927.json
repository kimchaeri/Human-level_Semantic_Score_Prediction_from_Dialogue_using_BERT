      "utterance": [
        {
          "id": "MDRW2100011927.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100011927.1.2",
          "form": "좋아하시는 스포츠있으신가요?",
          "original_form": "좋아하시는 스포츠있으신가요?",
          "speaker_id": "1",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011927.1.3",
          "form": "안녕하세요! 저는 보는건 축구요! 나머진 아무래도 익숙하지않다보니까",
          "original_form": "안녕하세요! 저는 보는건 축구요! 나머진 아무래도 익숙하지않다보니까",
          "speaker_id": "2",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011927.1.4",
          "form": "최근엔 올림픽때문에 많이 봤네요!",
          "original_form": "최근엔 올림픽때문에 많이 봤네요!",
          "speaker_id": "2",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011927.1.5",
          "form": "자주 스포츠 관람하시나요?",
          "original_form": "자주 스포츠 관람하시나요?",
          "speaker_id": "2",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011927.1.6",
          "form": "저도 항상 올림픽시즌에 급관심가졌다가 다시사그라들어요",
          "original_form": "저도 항상 올림픽시즌에 급관심가졌다가 다시사그라들어요",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011927.1.7",
          "form": "제가사는지역은 스포츠경기관람하랴면 버스타고 가야해서ㅜ",
          "original_form": "제가사는지역은 스포츠경기관람하랴면 버스타고 가야해서ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011927.1.8",
          "form": "축구 직접 보러도 가시나요?",
          "original_form": "축구 직접 보러도 가시나요?",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011927.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ저두 올림픽 기간만 그런거같아요 축구도 뭔가 크게할때?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저두 올림픽 기간만 그런거같아요 축구도 뭔가 크게할때?",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011927.1.10",
          "form": "아니요 저두 직관은 해본적 없어요!",
          "original_form": "아니요 저두 직관은 해본적 없어요!",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011927.1.11",
          "form": "스포츠는 직관이 진짜 재밌다던데 한번쯤은 구경가보고싶긴해요 ㅋㅋㅋ",
          "original_form": "스포츠는 직관이 진짜 재밌다던데 한번쯤은 구경가보고싶긴해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011927.1.12",
          "form": "특히 다음 파리올림픽 때 직접 가보고싶어요ㅜ",
          "original_form": "특히 다음 파리올림픽 때 직접 가보고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011927.1.13",
          "form": "와..거기까지 가서 보면 진짜 두고두고 기억날꺼같아요",
          "original_form": "와..거기까지 가서 보면 진짜 두고두고 기억날꺼같아요",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011927.1.14",
          "form": "낯선곳이라 새롭고",
          "original_form": "낯선곳이라 새롭고",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011927.1.15",
          "form": "타지에서 우리나라 선수들 경기보면 뭉클할꺼같아요",
          "original_form": "타지에서 우리나라 선수들 경기보면 뭉클할꺼같아요",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011927.1.16",
          "form": "네 그러니까요 겸사겸사 관광도하고요ㅜ",
          "original_form": "네 그러니까요 겸사겸사 관광도하고요ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011927.1.17",
          "form": "경기장도  엄청 이쁘더라구요",
          "original_form": "경기장도  엄청 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011927.1.18",
          "form": "그때 어떤 경기 직관하고싶으세요?",
          "original_form": "그때 어떤 경기 직관하고싶으세요?",
          "speaker_id": "2",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011927.1.19",
          "form": "전 양궁이랑 수영 배구",
          "original_form": "전 양궁이랑 수영 배구",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011927.1.20",
          "form": "맘같아선 다보고싶네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맘같아선 다보고싶네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011927.1.21",
          "form": "뚜또님은 직관하고싶은 경기어떤거에요",
          "original_form": "뚜또님은 직관하고싶은 경기어떤거에요",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011927.1.22",
          "form": "저두 배구요! 사실 맘같아서는 다 보고싶겠지만",
          "original_form": "저두 배구요! 사실 맘같아서는 다 보고싶겠지만",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011927.1.23",
          "form": "그중에선 배구가 제일 재미있을거같아요!",
          "original_form": "그중에선 배구가 제일 재미있을거같아요!",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011927.1.24",
          "form": "동계에선 스피드스케이팅이요!",
          "original_form": "동계에선 스피드스케이팅이요!",
          "speaker_id": "2",
          "time": "20210824 18:00"
        },
        {
          "id": "MDRW2100011927.1.25",
          "form": "맞아요 배구이번에보면서 긴장감넘치고 선수들 넘잘하고ㅜㅜ진짜 직관가고싶드라구요",
          "original_form": "맞아요 배구이번에보면서 긴장감넘치고 선수들 넘잘하고ㅜㅜ진짜 직관가고싶드라구요",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011927.1.26",
          "form": "동계는 저도 스피드스케이팅이랑 피겨보고싶어요",
          "original_form": "동계는 저도 스피드스케이팅이랑 피겨보고싶어요",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011927.1.27",
          "form": "연아님있었을때 봤어야했는데ㅜ",
          "original_form": "연아님있었을때 봤어야했는데ㅜ",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011927.1.28",
          "form": "아ㅠㅠ그쵸...연아님 정말 최고였을텐데....",
          "original_form": "아ㅠㅠ그쵸...연아님 정말 최고였을텐데....",
          "speaker_id": "2",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011927.1.29",
          "form": "홀린듯 보고있을거같아요",
          "original_form": "홀린듯 보고있을거같아요",
          "speaker_id": "2",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011927.1.30",
          "form": "네ㅜ완전 생눈으로 봤으면 정말좋았을것같아요",
          "original_form": "네ㅜ완전 생눈으로 봤으면 정말좋았을것같아요",
          "speaker_id": "1",
          "time": "20210824 18:03"
        }
      ]
    }
