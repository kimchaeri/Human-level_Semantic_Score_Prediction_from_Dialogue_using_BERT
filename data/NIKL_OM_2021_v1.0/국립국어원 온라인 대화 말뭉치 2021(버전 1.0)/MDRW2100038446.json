      "utterance": [
        {
          "id": "MDRW2100038446.1.1",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038446.1.2",
          "form": "너무 좋내요",
          "original_form": "너무 좋내요",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038446.1.3",
          "form": "그죠 제 최애교촌메뉴예요",
          "original_form": "그죠 제 최애교촌메뉴예요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038446.1.4",
          "form": "단거좋아하시는분은 허니콤보",
          "original_form": "단거좋아하시는분은 허니콤보",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038446.1.5",
          "form": "매운게좋으신분은 레드 오리지널 반반콤보가",
          "original_form": "매운게좋으신분은 레드 오리지널 반반콤보가",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038446.1.6",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038446.1.7",
          "form": "교촌 잘알이시네요",
          "original_form": "교촌 잘알이시네요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038446.1.8",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038446.1.9",
          "form": "저는 레드 도전해보겟습니다",
          "original_form": "저는 레드 도전해보겟습니다",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038446.1.10",
          "form": "본가가 집앞에 바로 교촌이 있어서가지구",
          "original_form": "본가가 집앞에 바로 교촌이 있어서가지구",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038446.1.11",
          "form": "치팅데이오면 꼬옥..",
          "original_form": "치팅데이오면 꼬옥..",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038446.1.12",
          "form": "포장자주먹었어요ㅋㅋㅋㄱ",
          "original_form": "포장자주먹었어요ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038446.1.13",
          "form": "와 좋내요",
          "original_form": "와 좋내요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038446.1.14",
          "form": "배송비도 안들고",
          "original_form": "배송비도 안들고",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038446.1.15",
          "form": "배달료ㅠ...",
          "original_form": "배달료ㅠ...",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.16",
          "form": "요듬 배달료 넘 ㅠ..",
          "original_form": "요듬 배달료 넘 ㅠ..",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.17",
          "form": "비싸요",
          "original_form": "비싸요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.18",
          "form": "너무 부담스럽죠...",
          "original_form": "너무 부담스럽죠...",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.19",
          "form": "4000원씩하는곳은 손이안가요",
          "original_form": "4000원씩하는곳은 손이안가요",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.20",
          "form": "거의 밥값의 반..",
          "original_form": "거의 밥값의 반..",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.21",
          "form": "저희동네는 식당이 별로없는대",
          "original_form": "저희동네는 식당이 별로없는대",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.22",
          "form": "전부 4천원이에요 ㅠ",
          "original_form": "전부 4천원이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.23",
          "form": "강제로 배달음식 끊었어요",
          "original_form": "강제로 배달음식 끊었어요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038446.1.24",
          "form": "헉....너무심하네요ㅠㅠ 거리가 머신편인가ㅠㅠ",
          "original_form": "헉....너무심하네요ㅠㅠ 거리가 머신편인가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038446.1.25",
          "form": "그런가봐여 ㅠㅠ",
          "original_form": "그런가봐여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038446.1.26",
          "form": "하나둘씩 오르더니...",
          "original_form": "하나둘씩 오르더니...",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038446.1.27",
          "form": "중국집밖에 못시키는 ㅠㅠ",
          "original_form": "중국집밖에 못시키는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038446.1.28",
          "form": "제가 다 속상하네요",
          "original_form": "제가 다 속상하네요",
          "speaker_id": "1",
          "time": "20210902 13:33"
        }
      ]
    }
  ]
