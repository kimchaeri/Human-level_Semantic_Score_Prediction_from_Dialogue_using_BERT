      "utterance": [
        {
          "id": "MDRW2100026911.1.1",
          "form": "저 아까 영화봤어요",
          "original_form": "저 아까 영화봤어요",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026911.1.2",
          "form": "공포요?",
          "original_form": "공포요?",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026911.1.3",
          "form": "심리스릴러?",
          "original_form": "심리스릴러?",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100026911.1.4",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100026911.1.5",
          "form": "듣기만해도 심장쫄려요",
          "original_form": "듣기만해도 심장쫄려요",
          "speaker_id": "1",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100026911.1.6",
          "form": "엔딩이 진짜 인상깊었는데",
          "original_form": "엔딩이 진짜 인상깊었는데",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026911.1.7",
          "form": "말로 듣는건 괜찮나요??",
          "original_form": "말로 듣는건 괜찮나요??",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026911.1.8",
          "form": "음... 눈으로 보는것만 아니라면 ㄱㅊ",
          "original_form": "음... 눈으로 보는것만 아니라면 ㄱㅊ",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026911.1.9",
          "form": "뭔내용인데요??",
          "original_form": "뭔내용인데요??",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026911.1.10",
          "form": "종교에 심취한 사람이 있는데요",
          "original_form": "종교에 심취한 사람이 있는데요",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026911.1.11",
          "form": "과몰입 심하게해서 본인이 하늘로 올라간다고",
          "original_form": "과몰입 심하게해서 본인이 하늘로 올라간다고",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026911.1.12",
          "form": "하늘로 올라가길 바라면서",
          "original_form": "하늘로 올라가길 바라면서",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026911.1.13",
          "form": "자기몸에 불을 붙이는데",
          "original_form": "자기몸에 불을 붙이는데",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026911.1.14",
          "form": "마지막 연출이 그걸 바라보던 사람들이 무릎꿇고 주인공도 천사 날개 달리는...거였거든요",
          "original_form": "마지막 연출이 그걸 바라보던 사람들이 무릎꿇고 주인공도 천사 날개 달리는...거였거든요",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026911.1.15",
          "form": "예???네?",
          "original_form": "예???네?",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.16",
          "form": "근데 마지막 1초에서 고통스러워하며 화상자국 장난아닌 주인공의 실제 모습을 보여주는데",
          "original_form": "근데 마지막 1초에서 고통스러워하며 화상자국 장난아닌 주인공의 실제 모습을 보여주는데",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.17",
          "form": "되게 인상깊었어요",
          "original_form": "되게 인상깊었어요",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.18",
          "form": "자기몸에 불을붙인다고요?",
          "original_form": "자기몸에 불을붙인다고요?",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.19",
          "form": "마지막 1초가 정말 대박이었음",
          "original_form": "마지막 1초가 정말 대박이었음",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.20",
          "form": "그게인상깊엇다고요?",
          "original_form": "그게인상깊엇다고요?",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.21",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.22",
          "form": "아니 그게 그렇게 압축이 되면",
          "original_form": "아니 그게 그렇게 압축이 되면",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.23",
          "form": "제가 좀 이상한것같잖아요",
          "original_form": "제가 좀 이상한것같잖아요",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.24",
          "form": "인상깊다 단어 뜻이",
          "original_form": "인상깊다 단어 뜻이",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.25",
          "form": "뭐죠?",
          "original_form": "뭐죠?",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.26",
          "form": "환상이 아니라 현실을 마지막에 잠깐 보여줘서",
          "original_form": "환상이 아니라 현실을 마지막에 잠깐 보여줘서",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.27",
          "form": "정말 인상이 깊었다는거죠",
          "original_form": "정말 인상이 깊었다는거죠",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026911.1.28",
          "form": "네......",
          "original_form": "네......",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
