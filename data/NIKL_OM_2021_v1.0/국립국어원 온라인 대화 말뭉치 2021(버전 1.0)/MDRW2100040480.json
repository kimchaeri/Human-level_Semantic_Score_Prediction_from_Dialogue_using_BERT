      "utterance": [
        {
          "id": "MDRW2100040480.1.1",
          "form": "오늘 완전 선선해요",
          "original_form": "오늘 완전 선선해요",
          "speaker_id": "1",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100040480.1.2",
          "form": "밤 되니까 살짝 쌀쌀하네요 ㅎㅎ",
          "original_form": "밤 되니까 살짝 쌀쌀하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100040480.1.3",
          "form": "맞아요 !!",
          "original_form": "맞아요 !!",
          "speaker_id": "1",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100040480.1.4",
          "form": "여름 다 간거같아요",
          "original_form": "여름 다 간거같아요",
          "speaker_id": "1",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100040480.1.5",
          "form": "낮엔 살짝 덥긴한데 그래도 살만하더라구요",
          "original_form": "낮엔 살짝 덥긴한데 그래도 살만하더라구요",
          "speaker_id": "2",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100040480.1.6",
          "form": "오 저 낮에는 안나가서",
          "original_form": "오 저 낮에는 안나가서",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100040480.1.7",
          "form": "반팔입으셧나요?",
          "original_form": "반팔입으셧나요?",
          "speaker_id": "1",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100040480.1.8",
          "form": "낮에 반팔입고 나갔다왔는데 딱 좋더라구요",
          "original_form": "낮에 반팔입고 나갔다왔는데 딱 좋더라구요",
          "speaker_id": "2",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100040480.1.9",
          "form": "이제 긴팔 슬슬 꺼낼려구요 !!",
          "original_form": "이제 긴팔 슬슬 꺼낼려구요 !!",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100040480.1.10",
          "form": "옷장정리의 계절이 왔군요ㅎㅎ",
          "original_form": "옷장정리의 계절이 왔군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100040480.1.11",
          "form": "이제 땀이 안나서 너무 좋아요 ㅋㅋ",
          "original_form": "이제 땀이 안나서 너무 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040480.1.12",
          "form": "아 그건 진짜 좋아요ㅋㅋ",
          "original_form": "아 그건 진짜 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100040480.1.13",
          "form": "담주에도 계속 선선햇음 좋겟어요 ㅋㅋ",
          "original_form": "담주에도 계속 선선햇음 좋겟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100040480.1.14",
          "form": "중간에 비 한 번 온다고는 하는데 그외에는 날 좋을 것 같더라구요 ㅋㅋ",
          "original_form": "중간에 비 한 번 온다고는 하는데 그외에는 날 좋을 것 같더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100040480.1.15",
