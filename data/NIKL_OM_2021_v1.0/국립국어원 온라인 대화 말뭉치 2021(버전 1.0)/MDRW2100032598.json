      "utterance": [
        {
          "id": "MDRW2100032598.1.1",
          "form": "공연은자주보시나요?",
          "original_form": "공연은자주보시나요?",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032598.1.2",
          "form": "코로나이전에",
          "original_form": "코로나이전에",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032598.1.3",
          "form": "뮤지컬좋아하세요?",
          "original_form": "뮤지컬좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100032598.1.4",
          "form": "공연 잘 안봐요 학교에서 보여준 것 정도??",
          "original_form": "공연 잘 안봐요 학교에서 보여준 것 정도??",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032598.1.5",
          "form": "뮤지컬 하니 브로드웨이42번가 공연 당첨됐다가 코로나때문에 서울 못간게 기억나네요..ㅋㅋㅋㅋ",
          "original_form": "뮤지컬 하니 브로드웨이42번가 공연 당첨됐다가 코로나때문에 서울 못간게 기억나네요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032598.1.6",
          "form": "아고ㅠ 저는작년에레베카 보고싶어서 예약했는데",
          "original_form": "아고ㅠ 저는작년에레베카 보고싶어서 예약했는데",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032598.1.7",
          "form": "두번이나 코로나땜에 취소댔어요",
          "original_form": "두번이나 코로나땜에 취소댔어요",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032598.1.8",
          "form": "좋아하는영화도 못보고",
          "original_form": "좋아하는영화도 못보고",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032598.1.9",
          "form": "속상하네요ㅠㅠ",
          "original_form": "속상하네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032598.1.10",
          "form": "그러니까요 ㅠㅠㅠ",
          "original_form": "그러니까요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032598.1.11",
          "form": "레베카 많이 들어는 봤는데 어떤 내용이에요?",
          "original_form": "레베카 많이 들어는 봤는데 어떤 내용이에요?",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032598.1.12",
          "form": "저도자세히는몰라요 못봐서ㅋㅋㅋ",
          "original_form": "저도자세히는몰라요 못봐서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032598.1.13",
          "form": "옥주현 공연이 꼭보고싶었거든요ㅠ",
          "original_form": "옥주현 공연이 꼭보고싶었거든요ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032598.1.14",
          "form": "올해는 힘들 것 같고 내년엔 꼭 티켓구해서가보고싶어요",
          "original_form": "올해는 힘들 것 같고 내년엔 꼭 티켓구해서가보고싶어요",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032598.1.15",
          "form": "아 그렇군요 ㅋㅋ",
          "original_form": "아 그렇군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032598.1.16",
          "form": "내년엔 꼭 볼 수 있음 좋겠네요!",
          "original_form": "내년엔 꼭 볼 수 있음 좋겠네요!",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032598.1.17",
          "form": "레베카도 뮤지컬이죠?",
          "original_form": "레베카도 뮤지컬이죠?",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032598.1.18",
          "form": "네 맞아요^^",
          "original_form": "네 맞아요^^",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.19",
          "form": "엄청유명하드라구요ㅡ",
          "original_form": "엄청유명하드라구요ㅡ",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.20",
          "form": "노래한번들어보셔요",
          "original_form": "노래한번들어보셔요",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.21",
          "form": "뮤지컬은 보통 티켓이 얼마예요?",
          "original_form": "뮤지컬은 보통 티켓이 얼마예요?",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.22",
          "form": "옥주현의 레베파",
          "original_form": "옥주현의 레베파",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.23",
          "form": "네 꼭 들어볼게요!!",
          "original_form": "네 꼭 들어볼게요!!",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.24",
          "form": "10만원이상이드라구요ㅠ",
          "original_form": "10만원이상이드라구요ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.25",
          "form": "비싸요",
          "original_form": "비싸요",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.26",
          "form": "유명한 배우가 나와서 그런건가요?",
          "original_form": "유명한 배우가 나와서 그런건가요?",
          "speaker_id": "2",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032598.1.27",
          "form": "배우에따라비싼거같지는않아요",
          "original_form": "배우에따라비싼거같지는않아요",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032598.1.28",
          "form": "대극장에서하는건 거의 이정도하드라구요",
          "original_form": "대극장에서하는건 거의 이정도하드라구요",
