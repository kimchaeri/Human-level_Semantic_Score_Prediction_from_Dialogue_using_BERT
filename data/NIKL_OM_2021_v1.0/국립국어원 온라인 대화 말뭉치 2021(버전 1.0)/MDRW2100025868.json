        {
          "id": "MDRW2100025868.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025868.1.3",
          "form": "정말 잘 됐네요",
          "original_form": "정말 잘 됐네요",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025868.1.4",
          "form": "아예 이렇게 엎어진건 한번이고",
          "original_form": "아예 이렇게 엎어진건 한번이고",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025868.1.5",
