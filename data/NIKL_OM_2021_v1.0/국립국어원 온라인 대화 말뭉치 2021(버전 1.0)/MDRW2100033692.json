      "utterance": [
        {
          "id": "MDRW2100033692.1.1",
          "form": "아 10년정도 키워야하니 잘생각하고 키워보심 될거예요",
          "original_form": "아 10년정도 키워야하니 잘생각하고 키워보심 될거예요",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033692.1.2",
          "form": "어렵진 않아요",
          "original_form": "어렵진 않아요",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033692.1.3",
          "form": "근데 아플때가 힘들어요",
          "original_form": "근데 아플때가 힘들어요",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033692.1.4",
          "form": "다치면 갈병원이 없어요",
          "original_form": "다치면 갈병원이 없어요",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033692.1.5",
          "form": "파충류 카페 가입해서 정보 얻으셔애해요",
          "original_form": "파충류 카페 가입해서 정보 얻으셔애해요",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033692.1.6",
          "form": "일단 저희집 사람아이나 좀 잘 키우고 여유생긴후 알아봐야겠어요",
          "original_form": "일단 저희집 사람아이나 좀 잘 키우고 여유생긴후 알아봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033692.1.7",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033692.1.8",
          "form": "저희집 아이가 벌레를 좋아해서 산에가서 사슴벌레를.계속 잡아와요",
          "original_form": "저희집 아이가 벌레를 좋아해서 산에가서 사슴벌레를.계속 잡아와요",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033692.1.9",
          "form": "그리고 사육장이 워나커요",
          "original_form": "그리고 사육장이 워나커요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033692.1.10",
          "form": "공간도 봐야하구요",
          "original_form": "공간도 봐야하구요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033692.1.11",
          "form": "세상에 대단해요 사슴벌레가 산에 있어요",
          "original_form": "세상에 대단해요 사슴벌레가 산에 있어요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033692.1.12",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033692.1.13",
          "form": "계속늘어 나겠네요",
          "original_form": "계속늘어 나겠네요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033692.1.14",
          "form": "전 사슴벌레 사오시나 했어요",
          "original_form": "전 사슴벌레 사오시나 했어요",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033692.1.15",
          "form": "젤리 먹죠?",
          "original_form": "젤리 먹죠?",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033692.1.16",
          "form": "그게 잡았을때 성취감이 무지 크나봐요.",
          "original_form": "그게 잡았을때 성취감이 무지 크나봐요.",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033692.1.17",
          "form": "젤리도 비싸던데",
          "original_form": "젤리도 비싸던데",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033692.1.18",
          "form": "신기하긴해요.",
          "original_form": "신기하긴해요.",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033692.1.19",
          "form": "그렇죠 저도 신나서 잡겠어요",
          "original_form": "그렇죠 저도 신나서 잡겠어요",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033692.1.20",
          "form": "워낙 많이 먹고 지금 15마리 이상 있다보니 은근 돈 드네요 ㅋㅋ",
          "original_form": "워낙 많이 먹고 지금 15마리 이상 있다보니 은근 돈 드네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033692.1.21",
          "form": "아이가 감각이 좋은가봐요",
          "original_form": "아이가 감각이 좋은가봐요",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033692.1.22",
          "form": "세상에 농장인데요",
          "original_form": "세상에 농장인데요",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033692.1.23",
          "form": "아빠랑 같이 다니죠~",
          "original_form": "아빠랑 같이 다니죠~",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033692.1.24",
          "form": "밤에만 나오고 가로등 불빛을 좋아해서 산근처 산책로에 많아요.",
          "original_form": "밤에만 나오고 가로등 불빛을 좋아해서 산근처 산책로에 많아요.",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033692.1.25",
          "form": "아 산책로에 나오는군요",
          "original_form": "아 산책로에 나오는군요",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033692.1.26",
          "form": "저희아들가면 신나할 동네네요",
          "original_form": "저희아들가면 신나할 동네네요",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033692.1.27",
          "form": "요녀석 한때 개미 잡으러 다녀서 저 산에 끌려 다녔어요 ㅋㅋ",
          "original_form": "요녀석 한때 개미 잡으러 다녀서 저 산에 끌려 다녔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033692.1.28",
          "form": "집근처는 아니고 광명동굴 근처로 가요",
          "original_form": "집근처는 아니고 광명동굴 근처로 가요",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033692.1.29",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033692.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033692.1.31",
          "form": "개미를 산에까지 가서 잡나요?ㅋㅋㅋㅋㅋ",
          "original_form": "개미를 산에까지 가서 잡나요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033692.1.32",
          "form": "거기 가끔?놀러갔어요",
          "original_form": "거기 가끔?놀러갔어요",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033692.1.33",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033692.1.34",
          "form": "특별한 개미를 잡는걸까요?",
          "original_form": "특별한 개미를 잡는걸까요?",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033692.1.35",
          "form": "종류별로 꼭 여왕새미 잡아야 해서 ㅜㅜ고생이 ㅜㅜ",
          "original_form": "종류별로 꼭 여왕새미 잡아야 해서 ㅜㅜ고생이 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033692.1.36",
          "form": "여왕개미 포획 하느라고요",
          "original_form": "여왕개미 포획 하느라고요",
          "speaker_id": "2",
          "time": "20210824 23:12"
        }
      ]
