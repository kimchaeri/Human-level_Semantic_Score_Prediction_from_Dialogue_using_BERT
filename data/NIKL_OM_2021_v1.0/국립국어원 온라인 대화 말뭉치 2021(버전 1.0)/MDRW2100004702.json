      "utterance": [
        {
          "id": "MDRW2100004702.1.1",
          "form": "요즘 코로나 때문에 집콕을 많이하니",
          "original_form": "요즘 코로나 때문에 집콕을 많이하니",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004702.1.2",
          "form": "온라인으로 피트니스 하는 게 많더라고요",
          "original_form": "온라인으로 피트니스 하는 게 많더라고요",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004702.1.3",
          "form": "줌으로 요가하는 친구가 있는데",
          "original_form": "줌으로 요가하는 친구가 있는데",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004702.1.4",
          "form": "꽤 괜찮대요",
          "original_form": "꽤 괜찮대요",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004702.1.5",
          "form": "오 줌으로도 배우나보네요!",
          "original_form": "오 줌으로도 배우나보네요!",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004702.1.6",
          "form": "저는 유튜브에서 홈트찾아서 가끔 보긴해요",
          "original_form": "저는 유튜브에서 홈트찾아서 가끔 보긴해요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004702.1.7",
          "form": "오 홈트하시는구나",
          "original_form": "오 홈트하시는구나",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004702.1.8",
          "form": "저도 몇번 했는데 아무래도 자발적으로 하는 건 꾸준히 하기 어렵더라고요",
          "original_form": "저도 몇번 했는데 아무래도 자발적으로 하는 건 꾸준히 하기 어렵더라고요",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004702.1.9",
          "form": "맞아요 꾸준히가 너무 어려워요 저도 그래서 진짜 가끔 하고 자기만족해요",
          "original_form": "맞아요 꾸준히가 너무 어려워요 저도 그래서 진짜 가끔 하고 자기만족해요",
          "speaker_id": "2",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004702.1.10",
          "form": "의지박약이라ㅠㅠㅠㅠ",
          "original_form": "의지박약이라ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004702.1.11",
          "form": "ㅎㅎ가끔이라도 하는 자신에게",
          "original_form": "ㅎㅎ가끔이라도 하는 자신에게",
          "speaker_id": "1",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004702.1.12",
          "form": "셀프칭찬 많이 해주세요",
          "original_form": "셀프칭찬 많이 해주세요",
          "speaker_id": "1",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004702.1.13",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100004702.1.14",
          "form": "오늘 오랜만에 홈트를 해야겠네요",
          "original_form": "오늘 오랜만에 홈트를 해야겠네요",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100004702.1.15",
          "form": "오 응원합니다",
          "original_form": "오 응원합니다",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100004702.1.16",
          "form": "어떤 홈트를 즐겨하세요?",
          "original_form": "어떤 홈트를 즐겨하세요?",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100004702.1.17",
          "form": "저는 유튜브에서 그냥 홈트라고 쳐서 나오는 거 돌아가면서 따라해요",
          "original_form": "저는 유튜브에서 그냥 홈트라고 쳐서 나오는 거 돌아가면서 따라해요",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100004702.1.18",
          "form": "같은 거 하면 너무 지겹더라구요!",
          "original_form": "같은 거 하면 너무 지겹더라구요!",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100004702.1.19",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004702.1.20",
          "form": "저는 땅끄부부 그것 많이 따라했었는데",
          "original_form": "저는 땅끄부부 그것 많이 따라했었는데",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004702.1.21",
          "form": "요가소년이랑",
          "original_form": "요가소년이랑",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004702.1.22",
          "form": "오땅끄부부",
          "original_form": "오땅끄부부",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100004702.1.23",
          "form": "요가소년은 한 번 봐야겠어요",
          "original_form": "요가소년은 한 번 봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100004702.1.24",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100004702.1.25",
          "form": "ㅎㅎ 홈트하고 오신 건가요?",
          "original_form": "ㅎㅎ 홈트하고 오신 건가요?",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100004702.1.26",
          "form": "앗 아니요ㅎㅎㅎㅎ",
          "original_form": "앗 아니요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100004702.1.27",
          "form": "유튜브에서 다른 거 보다 왔어요",
          "original_form": "유튜브에서 다른 거 보다 왔어요",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100004702.1.28",
          "form": "ㅎㅎㅎ홈트의 단점이죠",
          "original_form": "ㅎㅎㅎ홈트의 단점이죠",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100004702.1.29",
          "form": "역시 직접 가야하는건가 생각이 드네요,,,,,ㅋㅋㅋㅋ",
          "original_form": "역시 직접 가야하는건가 생각이 드네요,,,,,ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100004702.1.30",
          "form": "운동 영상 보러 들어갔다가 다른 것 기웃기웃",
          "original_form": "운동 영상 보러 들어갔다가 다른 것 기웃기웃",
          "speaker_id": "1",
