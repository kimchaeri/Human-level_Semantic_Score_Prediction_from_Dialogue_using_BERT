      "utterance": [
        {
          "id": "MDRW2100010147.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010147.1.2",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:52"
        },
        {
          "id": "MDRW2100010147.1.3",
          "form": "아르바이트 해 보신거 있으신가요",
          "original_form": "아르바이트 해 보신거 있으신가요",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010147.1.4",
          "form": "네 저 집에서 조그맣게 해요",
          "original_form": "네 저 집에서 조그맣게 해요",
          "speaker_id": "2",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010147.1.5",
          "form": "아르바이트 하세요?",
          "original_form": "아르바이트 하세요?",
          "speaker_id": "2",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010147.1.6",
          "form": "우와 멋지네요 저는 직장인이라 알바는 대학교때 해봤어요",
          "original_form": "우와 멋지네요 저는 직장인이라 알바는 대학교때 해봤어요",
          "speaker_id": "1",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010147.1.7",
          "form": "저도 알바는 학생때 많이했죠",
          "original_form": "저도 알바는 학생때 많이했죠",
          "speaker_id": "2",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010147.1.8",
          "form": "직장인 이시군요",
          "original_form": "직장인 이시군요",
          "speaker_id": "2",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010147.1.9",
          "form": "전 알바로 소소하게 벌어요",
          "original_form": "전 알바로 소소하게 벌어요",
          "speaker_id": "2",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010147.1.10",
          "form": "공모주식 사고 팔아서요",
          "original_form": "공모주식 사고 팔아서요",
          "speaker_id": "2",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010147.1.11",
          "form": "제 요즘 알바예요",
          "original_form": "제 요즘 알바예요",
          "speaker_id": "2",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100010147.1.12",
          "form": "저는 주린이라..",
          "original_form": "저는 주린이라..",
          "speaker_id": "1",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100010147.1.13",
          "form": "저도요 ㅜㅜ",
          "original_form": "저도요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100010147.1.14",
          "form": "공모주만 해서",
          "original_form": "공모주만 해서",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010147.1.15",
          "form": "용돈 벌어요",
          "original_form": "용돈 벌어요",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010147.1.16",
          "form": "주식으로 돈벌긴 힘들어요ㅜㅜ",
          "original_form": "주식으로 돈벌긴 힘들어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010147.1.17",
          "form": "알바는 요새 절벽이고",
          "original_form": "알바는 요새 절벽이고",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010147.1.18",
          "form": "저도 집에서 근무하고싶어요",
          "original_form": "저도 집에서 근무하고싶어요",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010147.1.19",
          "form": "직장 스트레스..",
          "original_form": "직장 스트레스..",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010147.1.20",
          "form": "내 다들 그렇죠",
          "original_form": "내 다들 그렇죠",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010147.1.21",
          "form": "제 친구는 재택하니 넘좋다고",
          "original_form": "제 친구는 재택하니 넘좋다고",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010147.1.22",
          "form": "코로나 안끝남 좋겠데여",
          "original_form": "코로나 안끝남 좋겠데여",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010147.1.23",
          "form": "아이는 방치하고 출근하는게 참 슬퍼요",
          "original_form": "아이는 방치하고 출근하는게 참 슬퍼요",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010147.1.24",
          "form": "참나 한대 때렸네요",
          "original_form": "참나 한대 때렸네요",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010147.1.25",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010147.1.26",
          "form": "그렇죠 ㅜㅜ",
          "original_form": "그렇죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010147.1.27",
          "form": "아이가 학교나 원에 안가나요?",
          "original_form": "아이가 학교나 원에 안가나요?",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010147.1.28",
          "form": "네 코로나로 안보내고 부모님이 봐주세요",
          "original_form": "네 코로나로 안보내고 부모님이 봐주세요",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010147.1.29",
          "form": "저희는 전면등교라 저번주 부터 갔어요",
          "original_form": "저희는 전면등교라 저번주 부터 갔어요",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010147.1.30",
          "form": "아 다행이네요 부모님이 봐주셔서",
          "original_form": "아 다행이네요 부모님이 봐주셔서",
          "speaker_id": "2",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010147.1.31",
          "form": "진짜 일하려면 부모님 도움 필요해요",
          "original_form": "진짜 일하려면 부모님 도움 필요해요",
          "speaker_id": "2",
          "time": "20210823 14:59"
        }
      ]
    }
  ]
