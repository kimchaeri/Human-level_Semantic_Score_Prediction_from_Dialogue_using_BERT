      "utterance": [
        {
          "id": "MDRW2100015939.1.1",
          "form": "안녕하세용 내일 비온대요!",
          "original_form": "안녕하세용 내일 비온대요!",
          "speaker_id": "1",
          "time": "20210826 03:50"
        },
        {
          "id": "MDRW2100015939.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100015939.1.3",
          "form": "일기예보보니 \n말씀하신것처럼 \n비소식ㅇㅣ 있더라구요",
          "original_form": "일기예보보니 \n말씀하신것처럼 \n비소식ㅇㅣ 있더라구요",
          "speaker_id": "2",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100015939.1.4",
          "form": "비오는날 좋아하세요?",
          "original_form": "비오는날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100015939.1.5",
          "form": "저 비 냄새 맡는거 좋아해요ㅎㅎ",
          "original_form": "저 비 냄새 맡는거 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100015939.1.6",
          "form": "비랑 눈중에 뭐가 더 좋으세요?",
          "original_form": "비랑 눈중에 뭐가 더 좋으세요?",
          "speaker_id": "1",
          "time": "20210826 11:42"
        },
        {
          "id": "MDRW2100015939.1.7",
          "form": "아....... 저는 눈 선택하겠습니다ㅋㅋ",
          "original_form": "아....... 저는 눈 선택하겠습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100015939.1.8",
          "form": "눈은 내리면 예쁜것같아요ㅎㅎ",
          "original_form": "눈은 내리면 예쁜것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100015939.1.9",
          "form": "비오는날의 갬성을 아직 잘 몰라요ㅠ",
          "original_form": "비오는날의 갬성을 아직 잘 몰라요ㅠ",
          "speaker_id": "2",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100015939.1.10",
          "form": "아하 약간 습하고 우중충하긴하죠ㅠㅜㅋㅋ",
          "original_form": "아하 약간 습하고 우중충하긴하죠ㅠㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100015939.1.11",
          "form": "네ㅠㅠ 맞아요ㅋㅋ",
          "original_form": "네ㅠㅠ 맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100015939.1.12",
          "form": "그리고 눈도 직장인 되고부터는\n걍 예쁜 쓰레기 같아졌어요ㅠㅋ",
          "original_form": "그리고 눈도 직장인 되고부터는\n걍 예쁜 쓰레기 같아졌어요ㅠㅋ",
          "speaker_id": "2",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100015939.1.13",
          "form": "아이쿠ㅠㅠㅋㅋ 녹는순간 도로가 지저분해보이긴하죠ㅎㅎ",
          "original_form": "아이쿠ㅠㅠㅋㅋ 녹는순간 도로가 지저분해보이긴하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100015939.1.14",
          "form": "길도 미끄러워서\n지각한적도 여러번 있어서요ㅠㅋ",
          "original_form": "길도 미끄러워서\n지각한적도 여러번 있어서요ㅠㅋ",
          "speaker_id": "2",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100015939.1.15",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100015939.1.16",
          "form": "위험한것같아요",
          "original_form": "위험한것같아요",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100015939.1.17",
          "form": "골절사고도 많고",
          "original_form": "골절사고도 많고",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100015939.1.18",
          "form": "맞아요! 비도 눈도\n적당히 올때 좋은거 같아요",
          "original_form": "맞아요! 비도 눈도\n적당히 올때 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100015939.1.19",
          "form": "지금은 날이 많이 갰네요",
          "original_form": "지금은 날이 많이 갰네요",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100015939.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:57"
        }
      ]
