      "utterance": [
        {
          "id": "MDRW2100026648.1.1",
          "form": "이번에 맥북 새로 나오는거 사고싶어..",
          "original_form": "이번에 맥북 새로 나오는거 사고싶어..",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026648.1.2",
          "form": "맥북? 왜 지금도 있잖아",
          "original_form": "맥북? 왜 지금도 있잖아",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026648.1.3",
          "form": "나 맥북 가진거 망가졌대이ㅠ",
          "original_form": "나 맥북 가진거 망가졌대이ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026648.1.4",
          "form": "헐 어떻게 망가졌어?",
          "original_form": "헐 어떻게 망가졌어?",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026648.1.5",
          "form": "배터리가 망가진건가봐ㅠㅠ 켜졌다가 금방 꺼져ㅠㅠ",
          "original_form": "배터리가 망가진건가봐ㅠㅠ 켜졌다가 금방 꺼져ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026648.1.6",
          "form": "헉 그럼 그냥 충전기 연결해서 쓰면 안되나?",
          "original_form": "헉 그럼 그냥 충전기 연결해서 쓰면 안되나?",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026648.1.7",
          "form": "ㅠ충전기 연결해도 안켜져...ㅠㅠ",
          "original_form": "ㅠ충전기 연결해도 안켜져...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026648.1.8",
          "form": "ㅠㅠ완전 맛갔네 새로사면 얼만데",
          "original_form": "ㅠㅠ완전 맛갔네 새로사면 얼만데",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026648.1.9",
          "form": "180정도?",
          "original_form": "180정도?",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026648.1.10",
          "form": "넘비싸ㅠㅠ 근데 데탑있잖아 필요해?",
          "original_form": "넘비싸ㅠㅠ 근데 데탑있잖아 필요해?",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026648.1.11",
          "form": "그러니까 욕심이죠...ㅎ",
          "original_form": "그러니까 욕심이죠...ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026648.1.12",
          "form": "그럼 참으세요..아니면 가로수길 가져가보면 안되나?",
          "original_form": "그럼 참으세요..아니면 가로수길 가져가보면 안되나?",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026648.1.13",
          "form": "예전에 사설 수리 받았어가지구ㅠㅠ",
          "original_form": "예전에 사설 수리 받았어가지구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026648.1.14",
          "form": "헉 그럼 그냥 보내주자...사설수리도 비싸지 맥북은",
          "original_form": "헉 그럼 그냥 보내주자...사설수리도 비싸지 맥북은",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026648.1.15",
