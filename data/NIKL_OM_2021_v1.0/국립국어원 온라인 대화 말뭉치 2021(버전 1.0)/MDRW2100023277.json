      "utterance": [
        {
          "id": "MDRW2100023277.1.1",
          "form": "헛 그래도 그 정도면 투자할만 하네요!!",
          "original_form": "헛 그래도 그 정도면 투자할만 하네요!!",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023277.1.2",
          "form": "빙수 좋아하시는 거죠!!",
          "original_form": "빙수 좋아하시는 거죠!!",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023277.1.3",
          "form": "설빙도 자주 드시나요?!",
          "original_form": "설빙도 자주 드시나요?!",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100023277.1.4",
          "form": "설빙도 코로나 전엔..!자주 갔어요!",
          "original_form": "설빙도 코로나 전엔..!자주 갔어요!",
          "speaker_id": "2",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023277.1.5",
          "form": "제가 거기 멜론빙수랑 딸기 빙수 진짜 좋아하거든요",
          "original_form": "제가 거기 멜론빙수랑 딸기 빙수 진짜 좋아하거든요",
          "speaker_id": "2",
          "time": "20210829 23:36"
        },
        {
          "id": "MDRW2100023277.1.6",
          "form": "헐 저도 딸기빙수 완전 좋아해요!!!!!!!!!!!",
          "original_form": "헐 저도 딸기빙수 완전 좋아해요!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023277.1.7",
          "form": "그 겨울에 나오는 한딸기 설빙인가",
          "original_form": "그 겨울에 나오는 한딸기 설빙인가",
          "speaker_id": "1",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023277.1.8",
          "form": "케익 같으 ㄴ거! 그게 제 최애에요",
          "original_form": "케익 같으 ㄴ거! 그게 제 최애에요",
          "speaker_id": "1",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023277.1.9",
          "form": "와 진짜진짜 맛있잖아요...ㅠㅠㅠ",
          "original_form": "와 진짜진짜 맛있잖아요...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:39"
        },
        {
          "id": "MDRW2100023277.1.10",
          "form": "그리고 은근 설빙이 과일맛집이지 않나요?",
          "original_form": "그리고 은근 설빙이 과일맛집이지 않나요?",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100023277.1.11",
          "form": "맞아요!!!!! 근데 점바점이 좀 있더라고요 ㅠ",
          "original_form": "맞아요!!!!! 근데 점바점이 좀 있더라고요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100023277.1.12",
          "form": "저 학교 주변은 완전 맛없는데",
          "original_form": "저 학교 주변은 완전 맛없는데",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100023277.1.13",
          "form": "본가쪽은 너무 맛있어서 과일 섭취를 핑계로",
          "original_form": "본가쪽은 너무 맛있어서 과일 섭취를 핑계로",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100023277.1.14",
          "form": "일주일에 한 번은 간답니다 ㅎㅎ",
          "original_form": "일주일에 한 번은 간답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100023277.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ최고네요!!아 근데 점바점있는건 처음 알았어요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ최고네요!!아 근데 점바점있는건 처음 알았어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100023277.1.16",
          "form": "과일이 다하는 빙수인데ㅠㅠ",
          "original_form": "과일이 다하는 빙수인데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100023277.1.17",
          "form": "그니가요 -.- 학교 옆은 너무 비린맛이? 나서",
          "original_form": "그니가요 -.- 학교 옆은 너무 비린맛이? 나서",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023277.1.18",
          "form": "한 번 가고 다시는 안 갔어요 -.-",
          "original_form": "한 번 가고 다시는 안 갔어요 -.-",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023277.1.19",
          "form": "빙수 비싼데 넘 아까워서 ㅠㅠ",
          "original_form": "빙수 비싼데 넘 아까워서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023277.1.20",
          "form": "헐!!어떻게 빙수가ㅠ비리죠ㅠㅠㅠㅠㅠ",
          "original_form": "헐!!어떻게 빙수가ㅠ비리죠ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023277.1.21",
          "form": "말도 안 돼...",
          "original_form": "말도 안 돼...",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023277.1.22",
          "form": "이건 빙수문화의 붕괴라구요...",
          "original_form": "이건 빙수문화의 붕괴라구요...",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023277.1.23",
          "form": "그니까요 완전 모독 -.- !!!!!!!",
          "original_form": "그니까요 완전 모독 -.- !!!!!!!",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023277.1.24",
          "form": "얍님 음식은 뭐 좋아하세요!",
          "original_form": "얍님 음식은 뭐 좋아하세요!",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023277.1.25",
          "form": "디저트 말구!",
          "original_form": "디저트 말구!",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023277.1.26",
          "form": "음........지금은 샤브샤브가 먹고 싶어졌어요...!",
          "original_form": "음........지금은 샤브샤브가 먹고 싶어졌어요...!",
          "speaker_id": "2",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023277.1.27",
          "form": "헐 저 샤브샤브 집착녀인대",
          "original_form": "헐 저 샤브샤브 집착녀인대",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023277.1.28",
          "form": "완전 좋아해요 ㅠ.",
          "original_form": "완전 좋아해요 ㅠ.",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023277.1.29",
          "form": "버섯 그냥은 안 먹는데 샤브샤브로는 먹어요 ㅎ",
          "original_form": "버섯 그냥은 안 먹는데 샤브샤브로는 먹어요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023277.1.30",
          "form": "와 저두요!",
          "original_form": "와 저두요!",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023277.1.31",
          "form": "버섯나물 진짜 싫은데",
          "original_form": "버섯나물 진짜 싫은데",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023277.1.32",
          "form": "샤브샤브 해서 소스찍으면 여기가 극락입니다",
          "original_form": "샤브샤브 해서 소스찍으면 여기가 극락입니다",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023277.1.33",
          "form": "맞아요!!!!!!!!!!!@ ㅠ ㅠ 아 너무 먹고 싶다...",
          "original_form": "맞아요!!!!!!!!!!!@ ㅠ ㅠ 아 너무 먹고 싶다...",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023277.1.34",
          "form": "이 시간에 먹고 싶게 만드는 건... 유죄 아니냐구요 ㅠ.^",
          "original_form": "이 시간에 먹고 싶게 만드는 건... 유죄 아니냐구요 ㅠ.^",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023277.1.35",
          "form": "ㅠㅠㅠㅠ흑흑 마지막에 죽 드시나요?면 드시나요??",
          "original_form": "ㅠㅠㅠㅠ흑흑 마지막에 죽 드시나요?면 드시나요??",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023277.1.36",
          "form": "오마이갓. 전 무조건 죽파입미다",
          "original_form": "오마이갓. 전 무조건 죽파입미다",
          "speaker_id": "1",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100023277.1.37",
