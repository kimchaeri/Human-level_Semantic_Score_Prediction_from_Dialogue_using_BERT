      "utterance": [
        {
          "id": "MDRW2100037728.1.1",
          "form": "구냥 사먹읍시다!!!!",
          "original_form": "구냥 사먹읍시다!!!!",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037728.1.2",
          "form": "전 목표를 세웠어요",
          "original_form": "전 목표를 세웠어요",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037728.1.3",
          "form": "칼을 빼들었음 뭐라도 썰어야죠",
          "original_form": "칼을 빼들었음 뭐라도 썰어야죠",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037728.1.4",
          "form": "올해안에 밀가루 6키로 소비",
          "original_form": "올해안에 밀가루 6키로 소비",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037728.1.5",
          "form": "ㅋㅋㅋ긍데 6키로면",
          "original_form": "ㅋㅋㅋ긍데 6키로면",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037728.1.6",
          "form": "막 엄청 오래 걸리지두 않아여",
          "original_form": "막 엄청 오래 걸리지두 않아여",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037728.1.7",
          "form": "주변에도 나눠주고 하면 더더..",
          "original_form": "주변에도 나눠주고 하면 더더..",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037728.1.8",
          "form": "근데 그러면",
          "original_form": "근데 그러면",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037728.1.9",
          "form": "빵집 가는대신 집에서 구워서 절약한다는",
          "original_form": "빵집 가는대신 집에서 구워서 절약한다는",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037728.1.10",
          "form": "홈베이킹의 장점이 사라지지 않나요",
          "original_form": "홈베이킹의 장점이 사라지지 않나요",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037728.1.11",
          "form": "그리고 전 에프 바스켓 형이라서, ㅠ 빵을 ㅕㅇ 러개는 못구워요,",
          "original_form": "그리고 전 에프 바스켓 형이라서, ㅠ 빵을 ㅕㅇ 러개는 못구워요,",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037728.1.12",
          "form": "절약하려고 하시는 거군여 따흑",
          "original_form": "절약하려고 하시는 거군여 따흑",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037728.1.13",
          "form": "전 그냥 재미있어서 한 거라서",
          "original_form": "전 그냥 재미있어서 한 거라서",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037728.1.14",
          "form": "물론 재미로 하는건긴 한데 바스켓 하나에 빵 두세개 나오면",
          "original_form": "물론 재미로 하는건긴 한데 바스켓 하나에 빵 두세개 나오면",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037728.1.15",
          "form": "저 하나먹고 가족이 슥 가져가면 아무것도 안남아요",
          "original_form": "저 하나먹고 가족이 슥 가져가면 아무것도 안남아요",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037728.1.16",
          "form": "구래서 오븐을!",
          "original_form": "구래서 오븐을!",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037728.1.17",
          "form": "2시간들여 구워서 ㅠ 5분만에 순삭,,",
          "original_form": "2시간들여 구워서 ㅠ 5분만에 순삭,,",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037728.1.18",
          "form": "어차피 하실 거면 그냥 오븐사시는 게 여러모로 나아여..",
          "original_form": "어차피 하실 거면 그냥 오븐사시는 게 여러모로 나아여..",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037728.1.19",
          "form": "집에서 구우면 치케를 잔뜩먹겠지 랄랄 하고 구웠는데",
          "original_form": "집에서 구우면 치케를 잔뜩먹겠지 랄랄 하고 구웠는데",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037728.1.20",
          "form": "가족들이 한입씩 먹으면 전 두입먹나,,,",
          "original_form": "가족들이 한입씩 먹으면 전 두입먹나,,,",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100037728.1.21",
          "form": "보통크기 크로와상 3개가 한계에요 제 에프는",
          "original_form": "보통크기 크로와상 3개가 한계에요 제 에프는",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100037728.1.22",
          "form": "ㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037728.1.23",
          "form": "진짜 완전 쫌쫌따리네여...",
          "original_form": "진짜 완전 쫌쫌따리네여...",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037728.1.24",
          "form": "에프가 몇리터짜리예요?",
          "original_form": "에프가 몇리터짜리예요?",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037728.1.25",
          "form": "모르겠구 내부직경이 24센티에요",
          "original_form": "모르겠구 내부직경이 24센티에요",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100037728.1.26",
          "form": "그래서 18센티 원형 빵틀 하나루 다 써요",
          "original_form": "그래서 18센티 원형 빵틀 하나루 다 써요",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100037728.1.27",
          "form": "어이쿠..",
          "original_form": "어이쿠..",
          "speaker_id": "1",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100037728.1.28",
          "form": "굉장히 협소하네여....",
          "original_form": "굉장히 협소하네여....",
          "speaker_id": "1",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100037728.1.29",
          "form": "아무래도 오븐이 아니니까 에프로는 좀 제약이 크죠ㅠㅠ",
          "original_form": "아무래도 오븐이 아니니까 에프로는 좀 제약이 크죠ㅠㅠ",
          "speaker_id": "1",
