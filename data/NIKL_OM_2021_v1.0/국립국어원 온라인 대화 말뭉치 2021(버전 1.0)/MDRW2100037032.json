      "utterance": [
        {
          "id": "MDRW2100037032.1.1",
          "form": "어후 담에 장바구니 한자리 찜입니닷ㅋㅋ",
          "original_form": "어후 담에 장바구니 한자리 찜입니닷ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037032.1.2",
          "form": "아생각해보니",
          "original_form": "아생각해보니",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037032.1.3",
          "form": "오뚜기였어요..",
          "original_form": "오뚜기였어요..",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037032.1.4",
          "form": "ㄱㅋㅈㄱㅈㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㄱㅋㅈㄱㅈㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037032.1.5",
          "form": "농심은배홍동비빔면인거같네요",
          "original_form": "농심은배홍동비빔면인거같네요",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037032.1.6",
          "form": "그것도맛있대요",
          "original_form": "그것도맛있대요",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037032.1.7",
          "form": "오융 두 개를 알아가는구욥",
          "original_form": "오융 두 개를 알아가는구욥",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100037032.1.8",
          "form": "갑자기 면이 땡기네욥ㅋㅋㅋ",
          "original_form": "갑자기 면이 땡기네욥ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100037032.1.9",
          "form": "저녁 고르셨어요?",
          "original_form": "저녁 고르셨어요?",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100037032.1.10",
          "form": "네네 삼각김밥이랑컵라면때렷습니다ㅋㅋㅋㅋ\n벌써 후식으로 포도먹구있습ㄴ디ㅏ",
          "original_form": "네네 삼각김밥이랑컵라면때렷습니다ㅋㅋㅋㅋ\n벌써 후식으로 포도먹구있습ㄴ디ㅏ",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100037032.1.11",
          "form": "저녁은뭐드시나용",
          "original_form": "저녁은뭐드시나용",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100037032.1.12",
          "form": "빠르시네욥ㅋㅋ",
          "original_form": "빠르시네욥ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100037032.1.13",
          "form": "저는 김치전에 된장국이욥!",
          "original_form": "저는 김치전에 된장국이욥!",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100037032.1.14",
          "form": "오 맛있겠네요ㅜㅜ!!!!",
          "original_form": "오 맛있겠네요ㅜㅜ!!!!",
          "speaker_id": "1",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100037032.1.15",
          "form": "넵! 엄마표는 언제나 최고죠 흐흐",
          "original_form": "넵! 엄마표는 언제나 최고죠 흐흐",
          "speaker_id": "2",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100037032.1.16",
          "form": "맞아요 엄마음식이짱이예요ㅜㅜ",
          "original_form": "맞아요 엄마음식이짱이예요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100037032.1.17",
          "form": "맞아요ㅠ 나이들수록 감사함을 느끼구요",
          "original_form": "맞아요ㅠ 나이들수록 감사함을 느끼구요",
          "speaker_id": "2",
          "time": "20210831 20:46"
        },
        {
          "id": "MDRW2100037032.1.18",
          "form": "맞아요ㅜㅜ 전 강제독립하게되었는데 얼마나그리운지몰라요..ㅋㅋㅜㅜ",
          "original_form": "맞아요ㅜㅜ 전 강제독립하게되었는데 얼마나그리운지몰라요..ㅋㅋㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100037032.1.19",
          "form": "앗ㅜㅠ 강제독립..! 그리우시겠어요ㅠ",
          "original_form": "앗ㅜㅠ 강제독립..! 그리우시겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100037032.1.20",
          "form": "네ㅠㅡㅠ..",
          "original_form": "네ㅠㅡㅠ..",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100037032.1.21",
          "form": "진짜요ㅜㅜ 강제독립당하기전에 많이드세요!!!",
          "original_form": "진짜요ㅜㅜ 강제독립당하기전에 많이드세요!!!",
