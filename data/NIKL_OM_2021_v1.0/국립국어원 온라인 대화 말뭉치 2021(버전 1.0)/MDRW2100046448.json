      "utterance": [
        {
          "id": "MDRW2100046448.1.1",
          "form": "안녕하세요 !",
          "original_form": "안녕하세요 !",
          "speaker_id": "1",
          "time": "20211022 15:53"
        },
        {
          "id": "MDRW2100046448.1.2",
          "form": "안녕하세요! 날씨가 참 좋아요!",
          "original_form": "안녕하세요! 날씨가 참 좋아요!",
          "speaker_id": "2",
          "time": "20211022 15:55"
        },
        {
          "id": "MDRW2100046448.1.3",
          "form": "맞습니다 ㅎㅎ",
          "original_form": "맞습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211022 15:56"
        },
        {
          "id": "MDRW2100046448.1.4",
          "form": "이제 금방 겨울이 다가오겠죠?ㅋㅋ",
          "original_form": "이제 금방 겨울이 다가오겠죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20211022 15:56"
        },
        {
          "id": "MDRW2100046448.1.5",
          "form": "네 겨울 종아요",
          "original_form": "네 겨울 종아요",
          "speaker_id": "1",
          "time": "20211022 15:57"
        },
        {
          "id": "MDRW2100046448.1.6",
          "form": "저도 붕어빵 사먹는게 좋아서 겨울 좋아해요",
          "original_form": "저도 붕어빵 사먹는게 좋아서 겨울 좋아해요",
          "speaker_id": "2",
          "time": "20211022 16:00"
        },
        {
          "id": "MDRW2100046448.1.7",
          "form": "저는 호떡 좋아해요",
          "original_form": "저는 호떡 좋아해요",
          "speaker_id": "1",
          "time": "20211022 16:01"
        },
        {
          "id": "MDRW2100046448.1.8",
          "form": "와 호떡!! 호떡을 까먹을 뻔 했네요 ",
          "original_form": "와 호떡!! 호떡을 까먹을 뻔 했네요 ",
          "speaker_id": "2",
          "time": "20211022 16:02"
        },
        {
          "id": "MDRW2100046448.1.9",
          "form": "네 정말 맛있어요",
          "original_form": "네 정말 맛있어요",
          "speaker_id": "1",
          "time": "20211022 16:02"
        },
        {
          "id": "MDRW2100046448.1.10",
          "form": "씨앗호떡 드셔보셨나요!",
          "original_form": "씨앗호떡 드셔보셨나요!",
          "speaker_id": "2",
          "time": "20211022 16:04"
        },
        {
          "id": "MDRW2100046448.1.11",
          "form": "그게 무엇인가요",
          "original_form": "그게 무엇인가요",
          "speaker_id": "1",
          "time": "20211022 16:05"
        },
        {
          "id": "MDRW2100046448.1.12",
          "form": "곡물 씨앗이 들어있는 호떡인데",
          "original_form": "곡물 씨앗이 들어있는 호떡인데",
          "speaker_id": "2",
          "time": "20211022 16:06"
        },
        {
          "id": "MDRW2100046448.1.13",
          "form": "한창 유행했었어요",
          "original_form": "한창 유행했었어요",
          "speaker_id": "2",
          "time": "20211022 16:06"
        },
        {
          "id": "MDRW2100046448.1.14",
          "form": "이번 겨울에 한번 드셔보세요! 맛있어요 ㅎㅎ",
          "original_form": "이번 겨울에 한번 드셔보세요! 맛있어요 ㅎㅎ",
