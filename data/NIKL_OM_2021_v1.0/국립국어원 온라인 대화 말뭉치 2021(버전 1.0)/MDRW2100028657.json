      "utterance": [
        {
          "id": "MDRW2100028657.1.1",
          "form": "회사 생활 어떠니?",
          "original_form": "회사 생활 어떠니?",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028657.1.2",
          "form": "그냥 다닐만해 너는",
          "original_form": "그냥 다닐만해 너는",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028657.1.3",
          "form": "나도 괜찮아 급여만 올려줬으면 좋겠어",
          "original_form": "나도 괜찮아 급여만 올려줬으면 좋겠어",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028657.1.4",
          "form": "너희 회사는 떡값 나오니?",
          "original_form": "너희 회사는 떡값 나오니?",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028657.1.5",
          "form": "상품권 20만원 주더라고",
          "original_form": "상품권 20만원 주더라고",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028657.1.6",
          "form": "다른선물은 없는듯",
          "original_form": "다른선물은 없는듯",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028657.1.7",
          "form": "그럼 딱 다이슨 살 수 있겠다",
          "original_form": "그럼 딱 다이슨 살 수 있겠다",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028657.1.8",
          "form": "우리는 10만 원 줘",
          "original_form": "우리는 10만 원 줘",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028657.1.9",
          "form": "현금으로 줘?",
          "original_form": "현금으로 줘?",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028657.1.10",
          "form": "상품권이지 물론",
          "original_form": "상품권이지 물론",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028657.1.11",
          "form": "상사 스트레스는 없니?",
          "original_form": "상사 스트레스는 없니?",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028657.1.12",
          "form": "멍청하고 답답한거 빼면 그냥 그저 그래",
          "original_form": "멍청하고 답답한거 빼면 그냥 그저 그래",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028657.1.13",
          "form": "그냥 또 저러네 하고 넘어갈 정도",
          "original_form": "그냥 또 저러네 하고 넘어갈 정도",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028657.1.14",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028657.1.15",
          "form": "맞아 그 정도면 무난하지",
          "original_form": "맞아 그 정도면 무난하지",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028657.1.16",
          "form": "나는 학생들이 악플 다는 거 빼고는 괜찮아",
          "original_form": "나는 학생들이 악플 다는 거 빼고는 괜찮아",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028657.1.17",
          "form": "악플을 왜다냐 버르장머리 없는 것들",
          "original_form": "악플을 왜다냐 버르장머리 없는 것들",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028657.1.18",
          "form": "그게 스트레스야 나쁜 학생들",
          "original_form": "그게 스트레스야 나쁜 학생들",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028657.1.19",
          "form": "요즘 애들 참 무섭다",
          "original_form": "요즘 애들 참 무섭다",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028657.1.20",
          "form": "너네 직원들은 다 좋니?",
          "original_form": "너네 직원들은 다 좋니?",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028657.1.21",
          "form": "안친해서 모르겠어",
          "original_form": "안친해서 모르겠어",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028657.1.22",
          "form": "개발팀은 잘난맛에 살아 재수없어",
          "original_form": "개발팀은 잘난맛에 살아 재수없어",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028657.1.23",
          "form": "친해질 필요 없지 뭐",
          "original_form": "친해질 필요 없지 뭐",
          "speaker_id": "1",
          "time": "20210902 21:22"
        }
      ]
