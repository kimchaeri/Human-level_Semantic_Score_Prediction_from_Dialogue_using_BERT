      "utterance": [
        {
          "id": "MDRW2100023088.1.1",
          "form": "데이트 잘 하고 왔어요?",
          "original_form": "데이트 잘 하고 왔어요?",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023088.1.2",
          "form": "ㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023088.1.3",
          "form": "연애해요?",
          "original_form": "연애해요?",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023088.1.4",
          "form": "갑자기 제출해서 당황함 ㅋㅋㅋ",
          "original_form": "갑자기 제출해서 당황함 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.5",
          "form": "전 연애 안해요 지금",
          "original_form": "전 연애 안해요 지금",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.6",
          "form": "데이트 어디 다녀오셨나용",
          "original_form": "데이트 어디 다녀오셨나용",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.7",
          "form": "저는 오늘 동물원에 다녀왔어요",
          "original_form": "저는 오늘 동물원에 다녀왔어요",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.8",
          "form": "우와 동물원이요 ?",
          "original_form": "우와 동물원이요 ?",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.9",
          "form": "엄청 풋풋하죠?",
          "original_form": "엄청 풋풋하죠?",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.10",
          "form": "실내요 야외요?",
          "original_form": "실내요 야외요?",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.11",
          "form": "실내였어여",
          "original_form": "실내였어여",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.12",
          "form": "오 그럼 동물 직접 만져보기도 했어요?",
          "original_form": "오 그럼 동물 직접 만져보기도 했어요?",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.13",
          "form": "먹이 주면 직접 만질 수 있는데",
          "original_form": "먹이 주면 직접 만질 수 있는데",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023088.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.15",
          "form": "먹이는 안 사서 그냥 보기만 했어요",
          "original_form": "먹이는 안 사서 그냥 보기만 했어요",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.16",
          "form": "아 ㅎㅎ 그럼 무슨 동물들 기억에 남았나요",
          "original_form": "아 ㅎㅎ 그럼 무슨 동물들 기억에 남았나요",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.17",
          "form": "동물보단 남친 손 잡으랴고!!!",
          "original_form": "동물보단 남친 손 잡으랴고!!!",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.18",
          "form": "오늘 있던 동물중엔 사막여우?",
          "original_form": "오늘 있던 동물중엔 사막여우?",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.19",
          "form": "ㅋㅋ 같이 알콩달콩 먹이 주셨어야죠",
          "original_form": "ㅋㅋ 같이 알콩달콩 먹이 주셨어야죠",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.20",
          "form": "오 사막여우 진짜 귀여운데",
          "original_form": "오 사막여우 진짜 귀여운데",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.21",
          "form": "걔 맨날 자고 있던데요 ㅜㅜ",
          "original_form": "걔 맨날 자고 있던데요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.22",
          "form": "마자요 오늘도 자더라구요",
          "original_form": "마자요 오늘도 자더라구요",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ역시나 사막여우 실망시키지 않네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ역시나 사막여우 실망시키지 않네요",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023088.1.24",
          "form": "먹이는 다른 사람들이 마니 주니깐...굳이..ㅋㅋㅋ",
          "original_form": "먹이는 다른 사람들이 마니 주니깐...굳이..ㅋㅋㅋ",
