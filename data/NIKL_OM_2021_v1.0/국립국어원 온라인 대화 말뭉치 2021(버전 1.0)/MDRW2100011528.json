      "utterance": [
        {
          "id": "MDRW2100011528.1.1",
          "form": "여행 다녀온적있으세요",
          "original_form": "여행 다녀온적있으세요",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011528.1.2",
          "form": "예전에 강원도로 놀러간 적이 있네요",
          "original_form": "예전에 강원도로 놀러간 적이 있네요",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011528.1.3",
          "form": "강원도 어디가 좋으셨나요?",
          "original_form": "강원도 어디가 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011528.1.4",
          "form": "강원도에 양떼목장이 좋았고 그 만석닭강정이었나 거기가 맛있더라고요",
          "original_form": "강원도에 양떼목장이 좋았고 그 만석닭강정이었나 거기가 맛있더라고요",
          "speaker_id": "2",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011528.1.5",
          "form": "오 만석닭강정 들어봤어요",
          "original_form": "오 만석닭강정 들어봤어요",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011528.1.6",
          "form": "양 먹이주기 체험하는곳 맞나요?",
          "original_form": "양 먹이주기 체험하는곳 맞나요?",
          "speaker_id": "1",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011528.1.7",
          "form": "네 맞아요 양떼목장이 공기도 좋고 양들이 귀엽더라고요",
          "original_form": "네 맞아요 양떼목장이 공기도 좋고 양들이 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011528.1.8",
          "form": "저도 한번 가봐야겠어요 저는 송도가 좋더라고요",
          "original_form": "저도 한번 가봐야겠어요 저는 송도가 좋더라고요",
          "speaker_id": "1",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011528.1.9",
          "form": "오 송도에는 뭐가 있나요?",
          "original_form": "오 송도에는 뭐가 있나요?",
          "speaker_id": "2",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100011528.1.10",
          "form": "바다 야경이 예뻐요",
          "original_form": "바다 야경이 예뻐요",
          "speaker_id": "1",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100011528.1.11",
          "form": "도심 하늘에 갈매기가 날아다니는게 웃기고요ㅋㅋ",
          "original_form": "도심 하늘에 갈매기가 날아다니는게 웃기고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100011528.1.12",
          "form": "오ㅋㅋㅋㅋ 바다 야경이 좋은 건 처음 알아가는 정보네요",
          "original_form": "오ㅋㅋㅋㅋ 바다 야경이 좋은 건 처음 알아가는 정보네요",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011528.1.13",
          "form": "나중에 송도 꼭 가봐야겠어요",
          "original_form": "나중에 송도 꼭 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011528.1.14",
          "form": "저는 코로나 끝나고 스페인가고싶어요",
          "original_form": "저는 코로나 끝나고 스페인가고싶어요",
          "speaker_id": "1",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011528.1.15",
          "form": "오 스페인에서 뭘 해보고 싶으세요?",
          "original_form": "오 스페인에서 뭘 해보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100011528.1.16",
          "form": "맛있는거 먹고 관광지 보러다니고 싶어요",
          "original_form": "맛있는거 먹고 관광지 보러다니고 싶어요",
          "speaker_id": "1",
          "time": "20210824 14:07"
        }
      ]
