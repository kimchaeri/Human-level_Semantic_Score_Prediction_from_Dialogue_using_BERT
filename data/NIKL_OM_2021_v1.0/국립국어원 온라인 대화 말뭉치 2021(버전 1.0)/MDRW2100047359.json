      "utterance": [
        {
          "id": "MDRW2100047359.1.1",
          "form": "좋아하는 연애인이 있으신가요?",
          "original_form": "좋아하는 연애인이 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100047359.1.2",
          "form": "예능인들 좋아해요",
          "original_form": "예능인들 좋아해요",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100047359.1.3",
          "form": "유재석이요ㅋㅋ",
          "original_form": "유재석이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100047359.1.4",
          "form": "유퀴즈랑 놀면 뭐하니 잘 보시겠어요ㅎㅎ",
          "original_form": "유퀴즈랑 놀면 뭐하니 잘 보시겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100047359.1.5",
          "form": "네 항상 챙겨봐요ㅋㅋ",
          "original_form": "네 항상 챙겨봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100047359.1.6",
          "form": "자주 보시는 거 있나요?",
          "original_form": "자주 보시는 거 있나요?",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100047359.1.7",
          "form": "저는 유퀴즈 자주 봐요",
          "original_form": "저는 유퀴즈 자주 봐요",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100047359.1.8",
          "form": "어제도 보셨나요?",
          "original_form": "어제도 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100047359.1.9",
          "form": "저는 실시간보다는 유튜브로 봐서 아직이요",
          "original_form": "저는 실시간보다는 유튜브로 봐서 아직이요",
          "speaker_id": "2",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100047359.1.10",
          "form": "아하. 요새 방송사들이 유튜브 잘 쓰더라구요",
          "original_form": "아하. 요새 방송사들이 유튜브 잘 쓰더라구요",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100047359.1.11",
          "form": "클립도 잘 올려주고요ㅋㅋ",
          "original_form": "클립도 잘 올려주고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100047359.1.12",
          "form": "썸네일도 재밌게 만들고요ㅋㅋ",
          "original_form": "썸네일도 재밌게 만들고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100047359.1.13",
          "form": "보고 싶도록 만들어요ㅋㅋ",
          "original_form": "보고 싶도록 만들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100047359.1.14",
          "form": "맞아요ㅋㅋ 봤던것도 클릭하게 되요",
          "original_form": "맞아요ㅋㅋ 봤던것도 클릭하게 되요",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100047359.1.15",
          "form": "보다가 아 이거 봤었지 해요ㅋㅋ",
          "original_form": "보다가 아 이거 봤었지 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:14"
        }
