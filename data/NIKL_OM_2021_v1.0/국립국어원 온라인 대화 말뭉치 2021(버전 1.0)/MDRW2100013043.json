      "utterance": [
        {
          "id": "MDRW2100013043.1.1",
          "form": "아뇨ㅎㅎㅎ대학생은 진작지났죠",
          "original_form": "아뇨ㅎㅎㅎ대학생은 진작지났죠",
          "speaker_id": "1",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100013043.1.2",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100013043.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100013043.1.4",
          "form": "안녕하세요 ㅎ",
          "original_form": "안녕하세요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100013043.1.5",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100013043.1.6",
          "form": "지금 코로나 잔여백신 맞으러 병원왔어요. 맞고나서 밥 먹으러 갈 예정이예요!",
          "original_form": "지금 코로나 잔여백신 맞으러 병원왔어요. 맞고나서 밥 먹으러 갈 예정이예요!",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100013043.1.7",
          "form": "와우 화이자인가요?",
          "original_form": "와우 화이자인가요?",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013043.1.8",
          "form": "점심메뉴는무엇인가요",
          "original_form": "점심메뉴는무엇인가요",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013043.1.9",
          "form": "네 화이자요",
          "original_form": "네 화이자요",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013043.1.10",
          "form": "이 주변에 뭐가 있는지 몰라서 둘러보다가 맛 있는데 들어가서 먹을 것 같아요",
          "original_form": "이 주변에 뭐가 있는지 몰라서 둘러보다가 맛 있는데 들어가서 먹을 것 같아요",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013043.1.11",
          "form": "점심 먹으셨어요???",
          "original_form": "점심 먹으셨어요???",
          "speaker_id": "2",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100013043.1.12",
          "form": "백신맞으시면",
          "original_form": "백신맞으시면",
          "speaker_id": "1",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100013043.1.13",
          "form": "든든하게 먹으셔야겠어요ㄷㄷ",
          "original_form": "든든하게 먹으셔야겠어요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100013043.1.14",
          "form": "네네 저 먹었답니다",
          "original_form": "네네 저 먹었답니다",
          "speaker_id": "1",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100013043.1.15",
          "form": "네엡ㅋㅋ",
          "original_form": "네엡ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100013043.1.16",
          "form": "백신맞고 돈까스 먹으러 왔어요",
          "original_form": "백신맞고 돈까스 먹으러 왔어요",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013043.1.17",
          "form": "점심 맛있는거 드셨어요????",
          "original_form": "점심 맛있는거 드셨어요????",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013043.1.18",
          "form": "저 치킨먹었어요ㅋㅋㅋ",
          "original_form": "저 치킨먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013043.1.19",
          "form": "돈까스좋죠",
          "original_form": "돈까스좋죠",
          "speaker_id": "1",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013043.1.20",
          "form": "옛날경양식인가요 일본식인가요?",
          "original_form": "옛날경양식인가요 일본식인가요?",
          "speaker_id": "1",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013043.1.21",
          "form": "경양식돈까스예요",
          "original_form": "경양식돈까스예요",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013043.1.22",
          "form": "점심부터 치킨;;;",
          "original_form": "점심부터 치킨;;;",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013043.1.23",
          "form": "생각못한 메뉴라 당황ㅋㅋ",
          "original_form": "생각못한 메뉴라 당황ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100013043.1.24",
          "form": "ㅋㅋㅋㅋ 많이당황하셨나봐요ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 많이당황하셨나봐요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:02"
        },
        {
          "id": "MDRW2100013043.1.25",
          "form": "경양식 좋은데 요즘 잘없더라고요ㅠ",
          "original_form": "경양식 좋은데 요즘 잘없더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:02"
        },
        {
          "id": "MDRW2100013043.1.26",
          "form": "순간 폰자판에서 손떼고 1분 멍 때렸어여 ㅋㅋㅋㅋ",
          "original_form": "순간 폰자판에서 손떼고 1분 멍 때렸어여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013043.1.27",
          "form": "맞아여 경양식 돈까스 점점 없어지는듯한 ㅠ",
          "original_form": "맞아여 경양식 돈까스 점점 없어지는듯한 ㅠ",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100013043.1.28",
          "form": "원래 점심 빨리 드시는거예요????",
          "original_form": "원래 점심 빨리 드시는거예요????",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100013043.1.29",
          "form": "ㅋㅋㅋㅋ 1분멍이라니ㅠ",
          "original_form": "ㅋㅋㅋㅋ 1분멍이라니ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100013043.1.30",
          "form": "저 직딩이아니라 그냥",
          "original_form": "저 직딩이아니라 그냥",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100013043.1.31",
          "form": "먹고싶을때먹습니다ㅎㅎ..자유롭죠...",
          "original_form": "먹고싶을때먹습니다ㅎㅎ..자유롭죠...",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100013043.1.32",
          "form": "그렇다고 학생도아니예요ㅎㅎ..",
          "original_form": "그렇다고 학생도아니예요ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100013043.1.33",
          "form": "아하 화이팅!",
          "original_form": "아하 화이팅!",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013043.1.34",
          "form": "점심먹고 회사들어가는중인데 너무 가기 싫은 ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅜㅠㅠㅠ",
          "original_form": "점심먹고 회사들어가는중인데 너무 가기 싫은 ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅜㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100013043.1.35",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013043.1.36",
          "form": "그래도 돈벌잖아요!",
          "original_form": "그래도 돈벌잖아요!",
          "speaker_id": "1",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100013043.1.37",
          "form": "개잔인... 백신맞고 아픈팔 이끌고 출근하는 사람한테 ㅠ",
          "original_form": "개잔인... 백신맞고 아픈팔 이끌고 출근하는 사람한테 ㅠ",
          "speaker_id": "2",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013043.1.38",
          "form": "악덕사장에 재능이 있으시네 ㅠㅜㅠㅜㅠ",
          "original_form": "악덕사장에 재능이 있으시네 ㅠㅜㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013043.1.39",
          "form": "ㅋㅋㅋㅋ 저도 돈주면 출근하고싶다고요!",
          "original_form": "ㅋㅋㅋㅋ 저도 돈주면 출근하고싶다고요!",
          "speaker_id": "1",
          "time": "20210825 12:49"
        },
        {
          "id": "MDRW2100013043.1.40",
          "form": "점심도 먹으면서!",
          "original_form": "점심도 먹으면서!",
          "speaker_id": "1",
          "time": "20210825 12:49"
        }
      ]
    }
