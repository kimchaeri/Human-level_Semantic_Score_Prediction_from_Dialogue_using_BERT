      "utterance": [
        {
          "id": "MDRW2100025272.1.1",
          "form": "안녕하세요 아침식사 하셨나요?",
          "original_form": "안녕하세요 아침식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 06:18"
        },
        {
          "id": "MDRW2100025272.1.2",
          "form": "아직요!",
          "original_form": "아직요!",
          "speaker_id": "2",
          "time": "20210901 06:21"
        },
        {
          "id": "MDRW2100025272.1.3",
          "form": "7시도 안됐는걸요?!ㅎㅎ",
          "original_form": "7시도 안됐는걸요?!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 06:21"
        },
        {
          "id": "MDRW2100025272.1.4",
          "form": "뭘로 하실 계획이신가요?",
          "original_form": "뭘로 하실 계획이신가요?",
          "speaker_id": "1",
          "time": "20210901 06:21"
        },
        {
          "id": "MDRW2100025272.1.5",
          "form": "어제 남긴 찜닭이 있어서",
          "original_form": "어제 남긴 찜닭이 있어서",
          "speaker_id": "2",
          "time": "20210901 06:22"
        },
        {
          "id": "MDRW2100025272.1.6",
          "form": "볶음밥 해먹으려구요~",
          "original_form": "볶음밥 해먹으려구요~",
          "speaker_id": "2",
          "time": "20210901 06:22"
        },
        {
          "id": "MDRW2100025272.1.7",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 06:22"
        },
        {
          "id": "MDRW2100025272.1.8",
          "form": "아뇨! 저는 아직이고 간단하게 씨리얼을 먹을까봐요!",
          "original_form": "아뇨! 저는 아직이고 간단하게 씨리얼을 먹을까봐요!",
          "speaker_id": "1",
          "time": "20210901 06:22"
        },
        {
          "id": "MDRW2100025272.1.9",
          "form": "볶음밥 맛있죠~!~!~",
          "original_form": "볶음밥 맛있죠~!~!~",
          "speaker_id": "1",
          "time": "20210901 06:22"
        },
        {
          "id": "MDRW2100025272.1.10",
          "form": "맛있는 음식 추천해봐요!",
          "original_form": "맛있는 음식 추천해봐요!",
          "speaker_id": "1",
          "time": "20210901 06:23"
        },
        {
          "id": "MDRW2100025272.1.11",
          "form": "저는 요즘 곱창전골에 꽂혀서 자주 먹어요",
          "original_form": "저는 요즘 곱창전골에 꽂혀서 자주 먹어요",
          "speaker_id": "2",
          "time": "20210901 06:30"
        },
        {
          "id": "MDRW2100025272.1.12",
          "form": "기름진맛 최고ㅜ",
          "original_form": "기름진맛 최고ㅜ",
          "speaker_id": "2",
          "time": "20210901 06:30"
        },
        {
          "id": "MDRW2100025272.1.13",
          "form": "헉 곱창 최고져ㅠㅠㅠ",
          "original_form": "헉 곱창 최고져ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 06:30"
        },
        {
          "id": "MDRW2100025272.1.14",
          "form": "원래는 구운걸 더 좋아하는데",
          "original_form": "원래는 구운걸 더 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 06:31"
        },
        {
          "id": "MDRW2100025272.1.15",
          "form": "저는 밀푀유나베같은 탕 요리 좋아해요",
          "original_form": "저는 밀푀유나베같은 탕 요리 좋아해요",
          "speaker_id": "1",
          "time": "20210901 06:31"
        },
        {
          "id": "MDRW2100025272.1.16",
          "form": "시국인지라 가서 먹기가 그렇더라고요ㅠ 전골은 배달해도 맛있으니까...",
          "original_form": "시국인지라 가서 먹기가 그렇더라고요ㅠ 전골은 배달해도 맛있으니까...",
          "speaker_id": "2",
          "time": "20210901 06:31"
        },
        {
          "id": "MDRW2100025272.1.17",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210901 06:31"
        },
        {
          "id": "MDRW2100025272.1.18",
          "form": "맞아요 맞아요ㅠㅜㅜ단골집 못 간지 엄청 된거같아요",
          "original_form": "맞아요 맞아요ㅠㅜㅜ단골집 못 간지 엄청 된거같아요",
          "speaker_id": "1",
          "time": "20210901 06:31"
        },
        {
          "id": "MDRW2100025272.1.19",
          "form": "국물파시구나",
          "original_form": "국물파시구나",
          "speaker_id": "2",
          "time": "20210901 06:31"
        },
        {
          "id": "MDRW2100025272.1.20",
          "form": "혹시 서울 사시면 건대 코후쿠라고 나베랑 대창 이런거 파는곳이 있는데 좋아하는 맛집이에요ㅎ!",
          "original_form": "혹시 서울 사시면 건대 코후쿠라고 나베랑 대창 이런거 파는곳이 있는데 좋아하는 맛집이에요ㅎ!",
          "speaker_id": "1",
          "time": "20210901 06:32"
        },
        {
          "id": "MDRW2100025272.1.21",
          "form": "ㅠㅠ멀리살아요",
          "original_form": "ㅠㅠ멀리살아요",
          "speaker_id": "2",
          "time": "20210901 06:32"
        },
        {
          "id": "MDRW2100025272.1.22",
          "form": "기억했다가 꼭 가보겠습니다!!",
          "original_form": "기억했다가 꼭 가보겠습니다!!",
          "speaker_id": "2",
          "time": "20210901 06:32"
        },
        {
          "id": "MDRW2100025272.1.23",
          "form": "곱창은 어떤거 좋아하세요?!",
          "original_form": "곱창은 어떤거 좋아하세요?!",
