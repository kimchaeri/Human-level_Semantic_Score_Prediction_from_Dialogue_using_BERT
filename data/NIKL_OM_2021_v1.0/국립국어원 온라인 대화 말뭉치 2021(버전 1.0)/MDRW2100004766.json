      "utterance": [
        {
          "id": "MDRW2100004766.1.1",
          "form": "무슨 음식 좋아하새요?",
          "original_form": "무슨 음식 좋아하새요?",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004766.1.2",
          "form": "저는 매운 음식 좋아해요",
          "original_form": "저는 매운 음식 좋아해요",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004766.1.3",
          "form": "최근엔 마라탕 자주 먹었어요",
          "original_form": "최근엔 마라탕 자주 먹었어요",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004766.1.4",
          "form": "매운 거 잘 드세요?",
          "original_form": "매운 거 잘 드세요?",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004766.1.5",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004766.1.6",
          "form": "저도 엄청 좋아해요",
          "original_form": "저도 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004766.1.7",
          "form": "잘먹진 못하는데 좋아해요",
          "original_form": "잘먹진 못하는데 좋아해요",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004766.1.8",
          "form": "마라탕 마라샹궈 한창 즐겨먹었었네요 ㅠㅠ",
          "original_form": "마라탕 마라샹궈 한창 즐겨먹었었네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004766.1.9",
          "form": "맛집 아시나요?",
          "original_form": "맛집 아시나요?",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004766.1.10",
          "form": "저는 마라홀릭이랑 탕화쿵푸요!",
          "original_form": "저는 마라홀릭이랑 탕화쿵푸요!",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004766.1.11",
          "form": "가던 곳만 가서 많이는 모르겠네요",
          "original_form": "가던 곳만 가서 많이는 모르겠네요",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004766.1.12",
          "form": "자주가는 마라탕 집 있나요??",
          "original_form": "자주가는 마라탕 집 있나요??",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004766.1.13",
          "form": "저도 탕화쿵푸 좋아해요!",
          "original_form": "저도 탕화쿵푸 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 18:59"
        },
        {
          "id": "MDRW2100004766.1.14",
          "form": "어딜가든 탕화쿵푸면 맛있더하라고요 ㅠㅠ",
          "original_form": "어딜가든 탕화쿵푸면 맛있더하라고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004766.1.15",
          "form": "훠궈집 마라탕도 좋아하는데",
          "original_form": "훠궈집 마라탕도 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004766.1.16",
          "form": "도삭면 먹으면 행복해져요",
          "original_form": "도삭면 먹으면 행복해져요",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004766.1.17",
          "form": "꼭 넣어드시는 재료 있어요?",
          "original_form": "꼭 넣어드시는 재료 있어요?",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004766.1.18",
          "form": "먹을 줄 아시는 분이네요 ㅋㅋㅋ",
          "original_form": "먹을 줄 아시는 분이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004766.1.19",
          "form": "저는 푹 익힌 야채 좋아해서",
          "original_form": "저는 푹 익힌 야채 좋아해서",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004766.1.20",
          "form": "배추랑 청경채 많이 넣어요",
          "original_form": "배추랑 청경채 많이 넣어요",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004766.1.21",
          "form": "지점마다 재료가 조금씩 달라서인지 도삭면은 못 본 것 같아요",
          "original_form": "지점마다 재료가 조금씩 달라서인지 도삭면은 못 본 것 같아요",
          "speaker_id": "1",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004766.1.22",
          "form": "아 배추 너무 맛나죠",
          "original_form": "아 배추 너무 맛나죠",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004766.1.23",
          "form": "저는 청경채는 색내려고 넣어요 ㅋㅋ",
          "original_form": "저는 청경채는 색내려고 넣어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004766.1.24",
          "form": "도삭면은 인기가없는지 파는곳 저도 자주가는 훠궈집 말고는 본적이 없어요",
          "original_form": "도삭면은 인기가없는지 파는곳 저도 자주가는 훠궈집 말고는 본적이 없어요",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004766.1.25",
          "form": "근데 그 꾸덕헌개 정말 ㅠㅠbb",
          "original_form": "근데 그 꾸덕헌개 정말 ㅠㅠbb",
          "speaker_id": "2",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004766.1.26",
          "form": "다양한 면요리는 사랑입니다",
          "original_form": "다양한 면요리는 사랑입니다",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004766.1.27",
          "form": "저는 오늘 저녁도 칼국수 해먹거든요 ㅋㅋㅋ",
          "original_form": "저는 오늘 저녁도 칼국수 해먹거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004766.1.28",
          "form": "와 부러워요 ㅠㅠ",
          "original_form": "와 부러워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004766.1.29",
          "form": "칼국수 진짜 너무 사랑하죠",
          "original_form": "칼국수 진짜 너무 사랑하죠",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004766.1.30",
          "form": "바지락 넣고 칼칼하게 끓이고싶네요",
          "original_form": "바지락 넣고 칼칼하게 끓이고싶네요",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004766.1.31",
          "form": "바지락 대신 물총조개 넣고",
          "original_form": "바지락 대신 물총조개 넣고",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004766.1.32",
          "form": "시원하게 먹을려고요!",
          "original_form": "시원하게 먹을려고요!",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004766.1.33",
          "form": "저녁은 드셨나요??",
          "original_form": "저녁은 드셨나요??",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004766.1.34",
          "form": "저는 요즘 운동 중이라",
          "original_form": "저는 요즘 운동 중이라",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004766.1.35",
          "form": "그래도 요즘은 맛있개 나와서 참 다행이에요",
          "original_form": "그래도 요즘은 맛있개 나와서 참 다행이에요",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004766.1.36",
          "form": "운동하시면 단백질 많이 챙겨먹어야 겠네요",
          "original_form": "운동하시면 단백질 많이 챙겨먹어야 겠네요",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004766.1.37",
          "form": "저는 맵고 짠 걸 좋아해서 식단이 제일 어렵네요",
          "original_form": "저는 맵고 짠 걸 좋아해서 식단이 제일 어렵네요",
          "speaker_id": "1",
          "time": "20210821 19:39"
        }
