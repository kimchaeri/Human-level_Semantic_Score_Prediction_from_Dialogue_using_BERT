      "utterance": [
        {
          "id": "MDRW2100013044.1.1",
          "form": "ㅇㅇ 두 분이 같은 해라 좀 부담되긴 하네",
          "original_form": "ㅇㅇ 두 분이 같은 해라 좀 부담되긴 하네",
          "speaker_id": "2",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100013044.1.2",
          "form": "우린 용돈으로 통일?",
          "original_form": "우린 용돈으로 통일?",
          "speaker_id": "1",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100013044.1.3",
          "form": "우린 그냥 용돈 100만원 고정.. 우리 형편에 맞춰야지 ㅎㅎ",
          "original_form": "우린 그냥 용돈 100만원 고정.. 우리 형편에 맞춰야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100013044.1.4",
          "form": "형님은 아버님 칠순에 어떤 거 하신데?",
          "original_form": "형님은 아버님 칠순에 어떤 거 하신데?",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100013044.1.5",
          "form": "엄마는 금목걸이고 아빠는 사파이어 반지",
          "original_form": "엄마는 금목걸이고 아빠는 사파이어 반지",
          "speaker_id": "2",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100013044.1.6",
          "form": "엥? 사파이어?",
          "original_form": "엥? 사파이어?",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100013044.1.7",
          "form": "무슨 용있는 알 큰 반지던데 ㅋㅋㅋ",
          "original_form": "무슨 용있는 알 큰 반지던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100013044.1.8",
          "form": "헉 갑자기 최민수가 생각나는데 ㅋㅋㅋ",
          "original_form": "헉 갑자기 최민수가 생각나는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100013044.1.9",
          "form": "ㅇㅇ 약간 그런 필이야 ㅎㅎ",
          "original_form": "ㅇㅇ 약간 그런 필이야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100013044.1.10",
          "form": "의외네 반지 끼시는거 한번도 못봤는데",
          "original_form": "의외네 반지 끼시는거 한번도 못봤는데",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100013044.1.11",
          "form": "몰라 ㅎㅎ 이번엔 갖고 싶다고 하셨나봐",
          "original_form": "몰라 ㅎㅎ 이번엔 갖고 싶다고 하셨나봐",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100013044.1.12",
          "form": "ㅎㅎ 양가 부모님이 줄줄이 칠순이라 잘 챙겨야겠어",
          "original_form": "ㅎㅎ 양가 부모님이 줄줄이 칠순이라 잘 챙겨야겠어",
