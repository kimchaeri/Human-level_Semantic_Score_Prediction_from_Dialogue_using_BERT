        {
          "id": "MDRW2100045456.1.1",
          "form": "오맵 이 네 개래요",
          "original_form": "오맵 이 네 개래요",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045456.1.2",
          "form": "되게 크네요",
          "original_form": "되게 크네요",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045456.1.3",
          "form": "마트에 먹을 것 덜 때가 재밌는데",
          "original_form": "마트에 먹을 것 덜 때가 재밌는데",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045456.1.4",
          "form": "와 70년 생존",
          "original_form": "와 70년 생존",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045456.1.5",
          "form": "대단하시네",
          "original_form": "대단하시네",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045456.1.6",
          "form": "루 종일 게임만 하실까",
          "original_form": "루 종일 게임만 하실까",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045456.1.7",
          "form": "몇 만 명 죽였다고",
          "original_form": "몇 만 명 죽였다고",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045456.1.8",
          "form": "170시간 넘는데요",
          "original_form": "170시간 넘는데요",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045456.1.9",
          "form": "너무 힘들었겠다",
          "original_form": "너무 힘들었겠다",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045456.1.10",
          "form": "차 없으면 힘들겠네요",
          "original_form": "차 없으면 힘들겠네요",
          "speaker_id": "1",
          "time": "20210908 02:43"
        },
        {
          "id": "MDRW2100045456.1.11",
          "form": "부품 구하는 거 힘들어 보이던데",
          "original_form": "부품 구하는 거 힘들어 보이던데",
