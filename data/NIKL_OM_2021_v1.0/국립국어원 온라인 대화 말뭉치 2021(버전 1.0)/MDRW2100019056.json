      "utterance": [
        {
          "id": "MDRW2100019056.1.1",
          "form": "헉 또요???",
          "original_form": "헉 또요???",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019056.1.2",
          "form": "네ㅠㅠㅠ",
          "original_form": "네ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019056.1.3",
          "form": "완전 작은거.. 유충..",
          "original_form": "완전 작은거.. 유충..",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019056.1.4",
          "form": "유충이라 다행일까요?",
          "original_form": "유충이라 다행일까요?",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019056.1.5",
          "form": "근데 얘네 크면",
          "original_form": "근데 얘네 크면",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019056.1.6",
          "form": "이제 망하잖아요..",
          "original_form": "이제 망하잖아요..",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019056.1.7",
          "form": "그게 무섭죠..",
          "original_form": "그게 무섭죠..",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019056.1.8",
          "form": "네 애들이 변기뒤도 살펴보래서",
          "original_form": "네 애들이 변기뒤도 살펴보래서",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019056.1.9",
          "form": "물탱크 뚜껑 열었는데",
          "original_form": "물탱크 뚜껑 열었는데",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019056.1.10",
          "form": "진짜 바퀴 똥 있더라구요",
          "original_form": "진짜 바퀴 똥 있더라구요",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019056.1.11",
          "form": "아유... 어쩐대요",
          "original_form": "아유... 어쩐대요",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019056.1.12",
          "form": "일단 그 구멍에 약 다 뿌렸어요.. 신경독..",
          "original_form": "일단 그 구멍에 약 다 뿌렸어요.. 신경독..",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019056.1.13",
          "form": "정말 화장실 청소 쫙 하셔야겠어요..",
          "original_form": "정말 화장실 청소 쫙 하셔야겠어요..",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019056.1.14",
          "form": "네ㅜㅜ 주말에 또 한번하고 뿌려야겠어요 ㅠㅠㅠ",
          "original_form": "네ㅜㅜ 주말에 또 한번하고 뿌려야겠어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019056.1.15",
          "form": "화장실 청소만도 싫은데",
          "original_form": "화장실 청소만도 싫은데",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019056.1.16",
          "form": "바퀴까지..",
          "original_form": "바퀴까지..",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019056.1.17",
          "form": "네 최악입니다 ㅠㅠ",
          "original_form": "네 최악입니다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019056.1.18",
          "form": "저도 청소해야겠어요",
          "original_form": "저도 청소해야겠어요",
          "speaker_id": "2",
          "time": "20210827 14:55"
        }
      ]
