      "utterance": [
        {
          "id": "MDRW2100003576.1.1",
          "form": "최근에 국내여행 간 곳 있나요?",
          "original_form": "최근에 국내여행 간 곳 있나요?",
          "speaker_id": "1",
          "time": "20210821 02:31"
        },
        {
          "id": "MDRW2100003576.1.2",
          "form": "저 얼마전에 전주 다녀왔어요!",
          "original_form": "저 얼마전에 전주 다녀왔어요!",
          "speaker_id": "2",
          "time": "20210821 02:32"
        },
        {
          "id": "MDRW2100003576.1.3",
          "form": "우와 전주비빔밥 드셨나요?",
          "original_form": "우와 전주비빔밥 드셨나요?",
          "speaker_id": "1",
          "time": "20210821 02:32"
        },
        {
          "id": "MDRW2100003576.1.4",
          "form": "ㅎㅎㅎ네 한옥마을도 다녀왔어요",
          "original_form": "ㅎㅎㅎ네 한옥마을도 다녀왔어요",
          "speaker_id": "2",
          "time": "20210821 02:33"
        },
        {
          "id": "MDRW2100003576.1.5",
          "form": "확실히 관광도시라 볼 게 많더라구요",
          "original_form": "확실히 관광도시라 볼 게 많더라구요",
          "speaker_id": "2",
          "time": "20210821 02:33"
        },
        {
          "id": "MDRW2100003576.1.6",
          "form": "저도 재작년에 다녀왔는데",
          "original_form": "저도 재작년에 다녀왔는데",
          "speaker_id": "1",
          "time": "20210821 02:33"
        },
        {
          "id": "MDRW2100003576.1.7",
          "form": "성당이 기억에 남더라구요",
          "original_form": "성당이 기억에 남더라구요",
          "speaker_id": "1",
          "time": "20210821 02:33"
        },
        {
          "id": "MDRW2100003576.1.8",
          "form": "저도 이번에 성당 들리고 싶었는데 스케줄상 못 들렀어요",
          "original_form": "저도 이번에 성당 들리고 싶었는데 스케줄상 못 들렀어요",
          "speaker_id": "2",
          "time": "20210821 02:34"
        },
        {
          "id": "MDRW2100003576.1.9",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "2",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003576.1.10",
          "form": "대신 예쁜 카페를 많이 다녔어요",
          "original_form": "대신 예쁜 카페를 많이 다녔어요",
          "speaker_id": "2",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003576.1.11",
          "form": "아 전주 예쁜 카페 많죠 ㅠㅠ!!",
          "original_form": "아 전주 예쁜 카페 많죠 ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003576.1.12",
          "form": "저는 내일 강릉가요",
          "original_form": "저는 내일 강릉가요",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003576.1.13",
          "form": "우와~ 휴가 떠나시는 거예요?",
          "original_form": "우와~ 휴가 떠나시는 거예요?",
          "speaker_id": "2",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003576.1.14",
          "form": "네! 토일월 짧게 다녀오려구요",
          "original_form": "네! 토일월 짧게 다녀오려구요",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003576.1.15",
          "form": "리조트에서 힐링할거에요 ㅎㅎㅎ",
          "original_form": "리조트에서 힐링할거에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003576.1.16",
          "form": "부럽네요ㅎㅎ",
          "original_form": "부럽네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003576.1.17",
          "form": "여행은 떠난다는 것만으로도 기분이 좋아져요",
          "original_form": "여행은 떠난다는 것만으로도 기분이 좋아져요",
          "speaker_id": "2",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003576.1.18",
          "form": "그러니가요",
          "original_form": "그러니가요",
          "speaker_id": "1",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003576.1.19",
          "form": "코로나만 아니면 해외로 갔을텐데",
          "original_form": "코로나만 아니면 해외로 갔을텐데",
          "speaker_id": "1",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003576.1.20",
          "form": "코로나덕분에 국내를 더 알게되기도 하네요 ㅋㅋ",
          "original_form": "코로나덕분에 국내를 더 알게되기도 하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003576.1.21",
          "form": "진짜 그런 것도 있네요",
          "original_form": "진짜 그런 것도 있네요",
          "speaker_id": "2",
          "time": "20210821 02:39"
        },
        {
          "id": "MDRW2100003576.1.22",
          "form": "저는 스페인 꼭 가보고 싶었는데",
          "original_form": "저는 스페인 꼭 가보고 싶었는데",
          "speaker_id": "2",
          "time": "20210821 02:40"
        },
        {
          "id": "MDRW2100003576.1.23",
          "form": "저두 호주 꼭 가고싶어서 코로나끝나면 가볼거에요",
          "original_form": "저두 호주 꼭 가고싶어서 코로나끝나면 가볼거에요",
          "speaker_id": "1",
          "time": "20210821 02:40"
        },
        {
          "id": "MDRW2100003576.1.24",
          "form": "저도 여행 계획만 늘어나고 있어요ㅎㅎ",
          "original_form": "저도 여행 계획만 늘어나고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:40"
        },
        {
          "id": "MDRW2100003576.1.25",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:41"
        }
      ]
    }
