      "utterance": [
        {
          "id": "MDRW2100031204.1.1",
          "form": "승리가 보이네?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "승리가 보이네?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031204.1.2",
          "form": "맞아 이제 투수가 잘해주고 그러고",
          "original_form": "맞아 이제 투수가 잘해주고 그러고",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031204.1.3",
          "form": "또 점수나고",
          "original_form": "또 점수나고",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031204.1.4",
          "form": "",
          "original_form": "{emoji:✊}",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031204.1.5",
          "form": "그치 그러면 이기는거지",
          "original_form": "그치 그러면 이기는거지",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031204.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031204.1.7",
          "form": "요즘 야구는 몇시쯤 끝나?",
          "original_form": "요즘 야구는 몇시쯤 끝나?",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031204.1.8",
          "form": "요즘야구 흠 몇시지",
          "original_form": "요즘야구 흠 몇시지",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031204.1.9",
          "form": "9시?",
          "original_form": "9시?",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031204.1.10",
          "form": "한3시간 하는듯",
          "original_form": "한3시간 하는듯",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031204.1.11",
          "form": "주말에 5시에 시작하는것같애",
          "original_form": "주말에 5시에 시작하는것같애",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031204.1.12",
          "form": "평일은 6시반에 출발하는 것 같구",
          "original_form": "평일은 6시반에 출발하는 것 같구",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031204.1.13",
          "form": "꽤 오래하네",
          "original_form": "꽤 오래하네",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031204.1.14",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031204.1.15",
          "form": "난 축구 보지만 ㅋㅋㅋㅋ",
          "original_form": "난 축구 보지만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031204.1.16",
          "form": "나는축구는 좀안맞아",
          "original_form": "나는축구는 좀안맞아",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031204.1.17",
          "form": "지루행,,,",
          "original_form": "지루행,,,",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031204.1.18",
          "form": "골 1개도 못넣구 끝낫자나",
          "original_form": "골 1개도 못넣구 끝낫자나",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031204.1.19",
          "form": "저번에 예선",
          "original_form": "저번에 예선",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031204.1.20",
          "form": "응응 그래서 아예 해외축구 봐",
          "original_form": "응응 그래서 아예 해외축구 봐",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031204.1.21",
          "form": "진짜 나 말라가는중이었어 90분인가?ㅋㅋㅋ",
          "original_form": "진짜 나 말라가는중이었어 90분인가?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031204.1.22",
          "form": "손흥민나온대서 봫는데",
          "original_form": "손흥민나온대서 봫는데",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031204.1.23",
          "form": "좋아좋아 야구가 점수 나는 맛이 있긴하지",
          "original_form": "좋아좋아 야구가 점수 나는 맛이 있긴하지",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031204.1.24",
          "form": "마자 야구 ㅋㅋㅋ 나랑 맞는듯",
          "original_form": "마자 야구 ㅋㅋㅋ 나랑 맞는듯",
          "speaker_id": "1",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100031204.1.25",
          "form": "심판이 아프셔서 지연중이 됐어",
          "original_form": "심판이 아프셔서 지연중이 됐어",
          "speaker_id": "1",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100031204.1.26",
          "form": "공 맞으신 분?",
          "original_form": "공 맞으신 분?",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031204.1.27",
          "form": "웅웅 지연되고 있다 경기",
          "original_form": "웅웅 지연되고 있다 경기",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031204.1.28",
          "form": "오늘 야구장에 사람많네",
          "original_form": "오늘 야구장에 사람많네",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031204.1.29",
          "form": "ㅋㅋㅋㅋㅋ엥",
          "original_form": "ㅋㅋㅋㅋㅋ엥",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031204.1.30",
          "form": "관중 받아??",
          "original_form": "관중 받아??",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031204.1.31",
          "form": "응!",
          "original_form": "응!",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100031204.1.32",
          "form": "마나 관중",
          "original_form": "마나 관중",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100031204.1.33",
          "form": "다시 시작한당",
          "original_form": "다시 시작한당",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100031204.1.34",
          "form": "그렇구나 4단계라 안 받는 줄 알았는데",
          "original_form": "그렇구나 4단계라 안 받는 줄 알았는데",
          "speaker_id": "2",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100031204.1.35",
          "form": "심판 바뀌었오 여기 대전이라서 4단계가 아닌걸껄??",
          "original_form": "심판 바뀌었오 여기 대전이라서 4단계가 아닌걸껄??",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100031204.1.36",
          "form": "아아 그렇군!!",
          "original_form": "아아 그렇군!!",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100031204.1.37",
          "form": "천재네?",
          "original_form": "천재네?",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100031204.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ타팀팬도 많더라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ타팀팬도 많더라",
