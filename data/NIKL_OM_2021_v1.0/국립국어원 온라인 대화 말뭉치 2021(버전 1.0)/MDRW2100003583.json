      "utterance": [
        {
          "id": "MDRW2100003583.1.1",
          "form": "어떤음식 좋아하시나요?",
          "original_form": "어떤음식 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003583.1.2",
          "form": "저는 요즘 라멘에 빠졌어요",
          "original_form": "저는 요즘 라멘에 빠졌어요",
          "speaker_id": "1",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003583.1.3",
          "form": "일본라멘이요?",
          "original_form": "일본라멘이요?",
          "speaker_id": "2",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003583.1.4",
          "form": "네 일본 라멘이요!",
          "original_form": "네 일본 라멘이요!",
          "speaker_id": "1",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003583.1.5",
          "form": "요즘 라멘집 찾아다니고있어요 ㅋㅋ",
          "original_form": "요즘 라멘집 찾아다니고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003583.1.6",
          "form": "가끔먹는건 맛있는데 느끼할때도 있더라구요",
          "original_form": "가끔먹는건 맛있는데 느끼할때도 있더라구요",
          "speaker_id": "2",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003583.1.7",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003583.1.8",
          "form": "고기국물을 잘못 우려서 그럴거에요",
          "original_form": "고기국물을 잘못 우려서 그럴거에요",
          "speaker_id": "1",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003583.1.9",
          "form": "name2님은 즐겨먹는 음식이 있나요?",
          "original_form": "&name2&님은 즐겨먹는 음식이 있나요?",
          "speaker_id": "1",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003583.1.10",
          "form": "전 고기 제일좋아해요ㅋ1순위",
          "original_form": "전 고기 제일좋아해요ㅋ1순위",
          "speaker_id": "2",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003583.1.11",
          "form": "혼자 집에서도 구워먹어요",
          "original_form": "혼자 집에서도 구워먹어요",
          "speaker_id": "2",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003583.1.12",
          "form": "우와 집에서 구워먹으면",
          "original_form": "우와 집에서 구워먹으면",
          "speaker_id": "1",
          "time": "20210821 02:47"
        },
        {
          "id": "MDRW2100003583.1.13",
          "form": "환기하지 힘들지않아요?",
          "original_form": "환기하지 힘들지않아요?",
          "speaker_id": "1",
          "time": "20210821 02:47"
        },
        {
          "id": "MDRW2100003583.1.14",
          "form": "그래도 맛있겠다 ㅠㅠ",
          "original_form": "그래도 맛있겠다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:47"
        },
        {
          "id": "MDRW2100003583.1.15",
          "form": "그냥 창문열고 팬 돌리고 먹어요",
          "original_form": "그냥 창문열고 팬 돌리고 먹어요",
          "speaker_id": "2",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003583.1.16",
          "form": "먹는게 더중요하니까요ㅎㅎ",
          "original_form": "먹는게 더중요하니까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003583.1.17",
          "form": "아 저는 원룸이라 고기를 못먹겠더라구요",
          "original_form": "아 저는 원룸이라 고기를 못먹겠더라구요",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003583.1.18",
          "form": "맞아요 그렇긴해요 ㅋㅋ",
          "original_form": "맞아요 그렇긴해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003583.1.19",
          "form": "전 자취해서 배달음식 자주시켜먹어요",
          "original_form": "전 자취해서 배달음식 자주시켜먹어요",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003583.1.20",
          "form": "name2님도 배달음식 자주 시켜먹나요?",
          "original_form": "&name2&님도 배달음식 자주 시켜먹나요?",
          "speaker_id": "1",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003583.1.21",
          "form": "저도 오피스텔이긴한데 신경안쓰고 먹는편ㅋ",
          "original_form": "저도 오피스텔이긴한데 신경안쓰고 먹는편ㅋ",
          "speaker_id": "2",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003583.1.22",
          "form": "배달은 아주가끔요 혼자다못먹어서",
          "original_form": "배달은 아주가끔요 혼자다못먹어서",
          "speaker_id": "2",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003583.1.23",
          "form": "아아 ㅋㅋㅋㅋ!! 좋겠다",
          "original_form": "아아 ㅋㅋㅋㅋ!! 좋겠다",
          "speaker_id": "1",
          "time": "20210821 02:50"
        },
        {
          "id": "MDRW2100003583.1.24",
          "form": "저는 혼자사니까 오히려 더 시키게되는거같아요",
          "original_form": "저는 혼자사니까 오히려 더 시키게되는거같아요",
          "speaker_id": "1",
          "time": "20210821 02:50"
        },
        {
          "id": "MDRW2100003583.1.25",
          "form": "남겨도 내일먹으면 되는거니까 ㅋㅋ!!",
          "original_form": "남겨도 내일먹으면 되는거니까 ㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210821 02:50"
        },
        {
          "id": "MDRW2100003583.1.26",
          "form": "장봐서 한끼해서 먹는것보다 배달이 더나은거같아요",
          "original_form": "장봐서 한끼해서 먹는것보다 배달이 더나은거같아요",
          "speaker_id": "2",
          "time": "20210821 02:51"
        },
        {
          "id": "MDRW2100003583.1.27",
          "form": "오히려 돈이 더들더라구요",
          "original_form": "오히려 돈이 더들더라구요",
          "speaker_id": "2",
          "time": "20210821 02:51"
        },
        {
          "id": "MDRW2100003583.1.28",
          "form": "맞아요 마켓컬리에서 주문하다가",
          "original_form": "맞아요 마켓컬리에서 주문하다가",
          "speaker_id": "1",
          "time": "20210821 02:52"
        },
        {
          "id": "MDRW2100003583.1.29",
          "form": "요리하는것도 귀찮아서 포기했어요",
          "original_form": "요리하는것도 귀찮아서 포기했어요",
          "speaker_id": "1",
          "time": "20210821 02:52"
        },
        {
          "id": "MDRW2100003583.1.30",
          "form": "요즘 밀키트 잘나오니 그런거 사서 해드셔도 될듯요^^",
          "original_form": "요즘 밀키트 잘나오니 그런거 사서 해드셔도 될듯요^^",
