        },
        {
          "id": "MDRW2100037774.1.2",
          "form": "대박ㅋㅋㅋ난 아까한 3살? 2살자리남자에",
          "original_form": "대박ㅋㅋㅋ난 아까한 3살? 2살자리남자에",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037774.1.3",
          "form": "얼마나 늠름하게 걸어가던지 ㅋㅋ",
          "original_form": "얼마나 늠름하게 걸어가던지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037774.1.4",
          "form": "우비풀셋입고가는거보고",
          "original_form": "우비풀셋입고가는거보고",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037774.1.5",
          "form": "뒷모습만봣는데엄청귀엽더라",
          "original_form": "뒷모습만봣는데엄청귀엽더라",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037774.1.6",
          "form": "ㅋㅋㅋㅋ아 귀여워",
          "original_form": "ㅋㅋㅋㅋ아 귀여워",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037774.1.7",
          "form": "장화까지 신어줘야 완성",
          "original_form": "장화까지 신어줘야 완성",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037774.1.8",
          "form": "옷이귀엽저라",
          "original_form": "옷이귀엽저라",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037774.1.9",
          "form": "흰색인데 검정 무늬엿는데",
          "original_form": "흰색인데 검정 무늬엿는데",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037774.1.10",
          "form": "좀 거리가 잇어서 모양까진안보엿는데 허리딱",
          "original_form": "좀 거리가 잇어서 모양까진안보엿는데 허리딱",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037774.1.11",
          "form": "조여주고 ㅋㅋ머리까지다써가지고",
          "original_form": "조여주고 ㅋㅋ머리까지다써가지고",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037774.1.12",
          "form": "근데 ㅋㅋㅋㅋ실내엿어 ㅋㅋㅋ주차장에서봄 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 ㅋㅋㅋㅋ실내엿어 ㅋㅋㅋ주차장에서봄 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037774.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ더워 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ더워 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037774.1.14",
          "form": "애기 엄마들 옷입히는 재미도 있겠다 ㅋㅋ",
          "original_form": "애기 엄마들 옷입히는 재미도 있겠다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037774.1.15",
          "form": "ㅋㅋㅋ그니깐",
          "original_form": "ㅋㅋㅋ그니깐",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037774.1.16",
          "form": "크기가 쪼만해서 뭘입어도 귀여울거아녀 ㅋㅋ",
          "original_form": "크기가 쪼만해서 뭘입어도 귀여울거아녀 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:19"
