        {
          "id": "MDRW2100037774.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037774.1.18",
          "form": "나 아까출근할때비안와서좋앗음",
          "original_form": "나 아까출근할때비안와서좋앗음",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037774.1.19",
          "form": "근데 지금또안오네",
          "original_form": "근데 지금또안오네",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
