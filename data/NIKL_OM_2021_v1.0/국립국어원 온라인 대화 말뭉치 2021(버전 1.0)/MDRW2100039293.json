      "utterance": [
        {
          "id": "MDRW2100039293.1.1",
          "form": "자주들어요 유투브로듣는거편하네요",
          "original_form": "자주들어요 유투브로듣는거편하네요",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039293.1.2",
          "form": "헬로비전",
          "original_form": "헬로비전",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039293.1.3",
          "form": "녹화기능이 없어져써요",
          "original_form": "녹화기능이 없어져써요",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039293.1.4",
          "form": "왜그렇죠? 유용한 기능이었는데",
          "original_form": "왜그렇죠? 유용한 기능이었는데",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039293.1.5",
          "form": "방송사 돈벌고 싶대요",
          "original_form": "방송사 돈벌고 싶대요",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039293.1.6",
          "form": "아직 약정전",
          "original_form": "아직 약정전",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039293.1.7",
          "form": "너무하네요",
          "original_form": "너무하네요",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100039293.1.8",
          "form": "어뜨카죠",
          "original_form": "어뜨카죠",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039293.1.9",
          "form": "3만원 너무 비싼거 아닌가요",
          "original_form": "3만원 너무 비싼거 아닌가요",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039293.1.10",
          "form": "저 보이스 영화 시사회 당첨돼써요",
          "original_form": "저 보이스 영화 시사회 당첨돼써요",
          "speaker_id": "1",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100039293.1.11",
          "form": "변경하면 얼마인데요?",
          "original_form": "변경하면 얼마인데요?",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039293.1.12",
          "form": "축하드려요",
          "original_form": "축하드려요",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039293.1.13",
          "form": "달에처넌 깎고 20만원 상품권 준데요",
          "original_form": "달에처넌 깎고 20만원 상품권 준데요",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039293.1.14",
          "form": "수욜인데 보러갈까요?",
          "original_form": "수욜인데 보러갈까요?",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039293.1.15",
          "form": "오 그럼 변경하세요",
          "original_form": "오 그럼 변경하세요",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039293.1.16",
          "form": "3만원대 비싼거 같아 고민이에요",
          "original_form": "3만원대 비싼거 같아 고민이에요",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039293.1.17",
          "form": "20도 작은 느낌 ㅋㅋ",
          "original_form": "20도 작은 느낌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039293.1.18",
          "form": "지금보다 저렴한거아니었어요?",
          "original_form": "지금보다 저렴한거아니었어요?",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039293.1.19",
          "form": "처넌 차이래요",
          "original_form": "처넌 차이래요",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039293.1.20",
          "form": "그렇군요 그럼 더 고민해보세요",
          "original_form": "그렇군요 그럼 더 고민해보세요",
