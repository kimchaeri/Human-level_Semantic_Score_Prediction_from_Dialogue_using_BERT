      "utterance": [
        {
          "id": "MDRW2100005524.1.1",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005524.1.2",
          "form": "내가 또 집안일은 잘하지",
          "original_form": "내가 또 집안일은 잘하지",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005524.1.3",
          "form": "집안일 천재신가요?",
          "original_form": "집안일 천재신가요?",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005524.1.4",
          "form": "자취러가 집안일 꿀팁 여쭤보겠습니다",
          "original_form": "자취러가 집안일 꿀팁 여쭤보겠습니다",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005524.1.5",
          "form": "꿀팁은 없네요",
          "original_form": "꿀팁은 없네요",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005524.1.6",
          "form": "특히 제가 정리를 못합니다",
          "original_form": "특히 제가 정리를 못합니다",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005524.1.7",
          "form": "방청소같은 것도 ㅎㅎ",
          "original_form": "방청소같은 것도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005524.1.8",
          "form": "집안일 잘 하신다면서요",
          "original_form": "집안일 잘 하신다면서요",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005524.1.9",
          "form": "네 뭐..",
          "original_form": "네 뭐..",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005524.1.10",
          "form": "세탁기 안에 빨래넣기",
          "original_form": "세탁기 안에 빨래넣기",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005524.1.11",
          "form": "빨래 널기",
          "original_form": "빨래 널기",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005524.1.12",
          "form": "뭐 기타등등..",
          "original_form": "뭐 기타등등..",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005524.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 잘하네...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 잘하네...",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005524.1.14",
          "form": "그거라도 잘 하면 됐더",
          "original_form": "그거라도 잘 하면 됐더",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005524.1.15",
          "form": "막내 육아 얘기도 들려줘",
          "original_form": "막내 육아 얘기도 들려줘",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005524.1.16",
          "form": "막내 육아",
          "original_form": "막내 육아",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005524.1.17",
          "form": "정말 뭐 없는데",
          "original_form": "정말 뭐 없는데",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005524.1.18",
          "form": "밥달라고 하면 밥주고..",
          "original_form": "밥달라고 하면 밥주고..",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005524.1.19",
          "form": "간식 달라하면 없다고 하고..",
          "original_form": "간식 달라하면 없다고 하고..",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005524.1.20",
          "form": "10시넘으면 자라하고..",
          "original_form": "10시넘으면 자라하고..",
          "speaker_id": "2",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005524.1.21",
          "form": "막내 있으면 누나도 일찍 자는 편인지",
          "original_form": "막내 있으면 누나도 일찍 자는 편인지",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005524.1.22",
          "form": "누나는 밤에 바쁩니다.",
          "original_form": "누나는 밤에 바쁩니다.",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005524.1.23",
          "form": "그래서 동생을 일찍 재웁니다",
          "original_form": "그래서 동생을 일찍 재웁니다",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005524.1.24",
          "form": "누나는 밤에 뭐 한다고 바쁘나요",
          "original_form": "누나는 밤에 뭐 한다고 바쁘나요",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005524.1.25",
          "form": "누나는 낮에 못했던 게임을 합니다",
          "original_form": "누나는 낮에 못했던 게임을 합니다",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005524.1.26",
          "form": "나름 저의 집안일이죠",
          "original_form": "나름 저의 집안일이죠",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005524.1.27",
          "form": "게임 중요하죠",
          "original_form": "게임 중요하죠",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005524.1.28",
          "form": "저도 요새 집안일이 게임인 편이라",
          "original_form": "저도 요새 집안일이 게임인 편이라",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005524.1.29",
          "form": "집안일 엄청 열심히 하는 중",
          "original_form": "집안일 엄청 열심히 하는 중",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005524.1.30",
          "form": "보기 좋네요",
          "original_form": "보기 좋네요",
          "speaker_id": "2",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005524.1.31",
          "form": "집에서 집안일도 열심히 하시고",
          "original_form": "집에서 집안일도 열심히 하시고",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005524.1.32",
          "form": "바람직하네요",
          "original_form": "바람직하네요",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005524.1.33",
          "form": "아무래도... 조금 알차게 사는 편이죠",
          "original_form": "아무래도... 조금 알차게 사는 편이죠",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005524.1.34",
          "form": "저도 집안일만 하면서 살고 싶은데",
          "original_form": "저도 집안일만 하면서 살고 싶은데",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005524.1.35",
          "form": "바깥일이 괴롭히네요",
          "original_form": "바깥일이 괴롭히네요",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005524.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 바깥일도 해야... 그걸로 집안일 하죠...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 바깥일도 해야... 그걸로 집안일 하죠...",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005524.1.37",
          "form": "그건 맞지만 집안일이 너무 재밌는걸 어떡합니까",
          "original_form": "그건 맞지만 집안일이 너무 재밌는걸 어떡합니까",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005524.1.38",
          "form": "밸런스가 안맞는데...",
          "original_form": "밸런스가 안맞는데...",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005524.1.39",
          "form": "재밌는 것만 하고 살고 싶지만...",
          "original_form": "재밌는 것만 하고 살고 싶지만...",
          "speaker_id": "1",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005524.1.40",
          "form": "어쩌겠어요...",
          "original_form": "어쩌겠어요...",
          "speaker_id": "1",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005524.1.41",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005524.1.42",
          "form": "아 정말 집안일만 해도",
          "original_form": "아 정말 집안일만 해도",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005524.1.43",
          "form": "돈줬으면 좋겠내요",
          "original_form": "돈줬으면 좋겠내요",
          "speaker_id": "2",
          "time": "20210821 23:22"
        }
      ]
    }
  ]
