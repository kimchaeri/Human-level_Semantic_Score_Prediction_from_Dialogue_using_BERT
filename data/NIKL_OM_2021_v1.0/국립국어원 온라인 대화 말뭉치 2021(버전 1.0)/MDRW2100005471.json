      "utterance": [
        {
          "id": "MDRW2100005471.1.1",
          "form": "니는 책 뭐보노?",
          "original_form": "니는 책 뭐보노?",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005471.1.2",
          "form": "나는 요즘 카카오페이지 본다",
          "original_form": "나는 요즘 카카오페이지 본다",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005471.1.3",
          "form": "저는 리디북스를 본답니다",
          "original_form": "저는 리디북스를 본답니다",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005471.1.4",
          "form": "리디북스에서 뭐보노",
          "original_form": "리디북스에서 뭐보노",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005471.1.5",
          "form": "저는 판타지 소설을 즐겨본답니다",
          "original_form": "저는 판타지 소설을 즐겨본답니다",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005471.1.6",
          "form": "판타지 소설 뭐 보노 제목이 뭐고",
          "original_form": "판타지 소설 뭐 보노 제목이 뭐고",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005471.1.7",
          "form": "나혼자만 레벨업",
          "original_form": "나혼자만 레벨업",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005471.1.8",
          "form": "재밌음",
          "original_form": "재밌음",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005471.1.9",
          "form": "나혼렙 나도 끝까지 다 봄",
          "original_form": "나혼렙 나도 끝까지 다 봄",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005471.1.10",
          "form": "재밌었음",
          "original_form": "재밌었음",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005471.1.11",
          "form": "근데 웹툰이 더 나은거같음",
          "original_form": "근데 웹툰이 더 나은거같음",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005471.1.12",
          "form": "ㅇㅈ 웹툰이 더 낫더라",
          "original_form": "ㅇㅈ 웹툰이 더 낫더라",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005471.1.13",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005471.1.14",
          "form": "그리고 그 뭐냐 수요웹툰에",
          "original_form": "그리고 그 뭐냐 수요웹툰에",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005471.1.15",
          "form": "전독시도 재미있음",
          "original_form": "전독시도 재미있음",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005471.1.16",
          "form": "전독시?",
          "original_form": "전독시?",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005471.1.17",
          "form": "ㅇㅇ 재밌더라",
          "original_form": "ㅇㅇ 재밌더라",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005471.1.18",
          "form": "판타지말고",
          "original_form": "판타지말고",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005471.1.19",
          "form": "일반소설은 뭐 보노",
          "original_form": "일반소설은 뭐 보노",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005471.1.20",
          "form": "나는 베스트셀러에",
          "original_form": "나는 베스트셀러에",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005471.1.21",
          "form": "조국의 시간이 올라와있는것을 보고",
          "original_form": "조국의 시간이 올라와있는것을 보고",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005471.1.22",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005471.1.23",
          "form": "우리나라 국민들의 수준이 이정도인가 싶어",
          "original_form": "우리나라 국민들의 수준이 이정도인가 싶어",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005471.1.24",
          "form": "통탄을 금치못했다",
          "original_form": "통탄을 금치못했다",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005471.1.25",
          "form": "조선생님 책은 전국민 필독서로 인정해야지",
          "original_form": "조선생님 책은 전국민 필독서로 인정해야지",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005471.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005471.1.27",
          "form": "난 최근에는 일반소설은 안읽었는데",
          "original_form": "난 최근에는 일반소설은 안읽었는데",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005471.1.28",
          "form": "좋아하는 작가는 없나",
          "original_form": "좋아하는 작가는 없나",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005471.1.29",
          "form": "히가시노같은",
          "original_form": "히가시노같은",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005471.1.30",
          "form": "유명한 작가 있잖아",
          "original_form": "유명한 작가 있잖아",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005471.1.31",
          "form": "딱히 없음",
          "original_form": "딱히 없음",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005471.1.32",
          "form": "재미있게 읽은 책들은 꽤 되는데",
          "original_form": "재미있게 읽은 책들은 꽤 되는데",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005471.1.33",
          "form": "기억이 잘 안나네",
          "original_form": "기억이 잘 안나네",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005471.1.34",
          "form": "책 많이 읽는줄알았는데",
          "original_form": "책 많이 읽는줄알았는데",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005471.1.35",
          "form": "그것도 아니었노",
          "original_form": "그것도 아니었노",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005471.1.36",
          "form": "책책책",
          "original_form": "책책책",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005471.1.37",
          "form": "책을 읽읍시자",
          "original_form": "책을 읽읍시자",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005471.1.38",
          "form": "시다",
          "original_form": "시다",
          "speaker_id": "1",
          "time": "20210821 22:42"
        }
      ]
