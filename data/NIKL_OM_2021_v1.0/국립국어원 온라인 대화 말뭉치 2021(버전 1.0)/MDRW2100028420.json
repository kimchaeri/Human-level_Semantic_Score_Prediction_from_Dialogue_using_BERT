      "utterance": [
        {
          "id": "MDRW2100028420.1.1",
          "form": "나 요즘 머리 하고 싶어져",
          "original_form": "나 요즘 머리 하고 싶어져",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028420.1.2",
          "form": "파마같은거",
          "original_form": "파마같은거",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028420.1.3",
          "form": "나도 파마하고 싶어",
          "original_form": "나도 파마하고 싶어",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028420.1.4",
          "form": "그래서 머리 기르는 중이야",
          "original_form": "그래서 머리 기르는 중이야",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028420.1.5",
          "form": "나 머리 꽤 길렀어",
          "original_form": "나 머리 꽤 길렀어",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028420.1.6",
          "form": "등까지 와",
          "original_form": "등까지 와",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028420.1.7",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028420.1.8",
          "form": "2년을 못봤더니 그사이 많이 길었구나",
          "original_form": "2년을 못봤더니 그사이 많이 길었구나",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028420.1.9",
          "form": "나도 근데 이제 그쯤 와",
          "original_form": "나도 근데 이제 그쯤 와",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028420.1.10",
          "form": "엄마가 자르지 말라고 한게 컸지",
          "original_form": "엄마가 자르지 말라고 한게 컸지",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028420.1.11",
          "form": "날개뼈 아래?",
          "original_form": "날개뼈 아래?",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028420.1.12",
          "form": "난 머리 자르고 싶기도 하고 웨이브 넣고싶기도 하고",
          "original_form": "난 머리 자르고 싶기도 하고 웨이브 넣고싶기도 하고",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028420.1.13",
          "form": "근데 웨이브 넣을 돈으로",
          "original_form": "근데 웨이브 넣을 돈으로",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028420.1.14",
          "form": "엄마가 다이슨 롤 되는거 사자고 그래",
          "original_form": "엄마가 다이슨 롤 되는거 사자고 그래",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028420.1.15",
          "form": "그것도 괜찮지",
          "original_form": "그것도 괜찮지",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028420.1.16",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028420.1.17",
          "form": "귀찮은 것 빼면 좋지",
          "original_form": "귀찮은 것 빼면 좋지",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028420.1.18",
          "form": "난 아예 히피펌 젤리펌 그런게 하고 싶어서",
          "original_form": "난 아예 히피펌 젤리펌 그런게 하고 싶어서",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028420.1.19",
          "form": "나도 히피 완전 해보고싶어",
          "original_form": "나도 히피 완전 해보고싶어",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028420.1.20",
          "form": "근데 나 올도 얇고 숱도 없어서 장난 아닐거같아",
          "original_form": "근데 나 올도 얇고 숱도 없어서 장난 아닐거같아",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028420.1.21",
          "form": "그건...나도 그래",
          "original_form": "그건...나도 그래",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028420.1.22",
          "form": "그치만 취업전에 해야지",
          "original_form": "그치만 취업전에 해야지",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028420.1.23",
          "form": "취업후에 해도 괜찮지 않나",
          "original_form": "취업후에 해도 괜찮지 않나",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100028420.1.24",
          "form": "왜냐면 취업전에 하면 그 머리로 구인구직해야해서",
          "original_form": "왜냐면 취업전에 하면 그 머리로 구인구직해야해서",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100028420.1.25",
          "form": "엥 아니 취업때는 자를거야",
          "original_form": "엥 아니 취업때는 자를거야",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100028420.1.26",
          "form": "아 그래? 그럼 전에 해야지",
          "original_form": "아 그래? 그럼 전에 해야지",
          "speaker_id": "1",
          "time": "20210902 19:58"
        }
      ]
    }
