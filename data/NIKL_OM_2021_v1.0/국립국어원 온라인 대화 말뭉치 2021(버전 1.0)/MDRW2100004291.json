      "utterance": [
        {
          "id": "MDRW2100004291.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004291.1.2",
          "form": "온라인 커뮤니티에 자주 참여하시는 편인가요?",
          "original_form": "온라인 커뮤니티에 자주 참여하시는 편인가요?",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004291.1.3",
          "form": "자주는 아니고 관심분야만요~",
          "original_form": "자주는 아니고 관심분야만요~",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004291.1.4",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004291.1.5",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004291.1.6",
          "form": "저도 최근에 관심분야의 온라인 커뮤니티에 자주 접속하고 있는데",
          "original_form": "저도 최근에 관심분야의 온라인 커뮤니티에 자주 접속하고 있는데",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004291.1.7",
          "form": "열심히 활동하시는 분들도 참 많더라고요",
          "original_form": "열심히 활동하시는 분들도 참 많더라고요",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004291.1.8",
          "form": "아무래도 코로나때문에",
          "original_form": "아무래도 코로나때문에",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004291.1.9",
          "form": "활동을 많이 못해서그런가봐요",
          "original_form": "활동을 많이 못해서그런가봐요",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004291.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004291.1.11",
          "form": "어떤 커뮤니티에 관심잇으세요?",
          "original_form": "어떤 커뮤니티에 관심잇으세요?",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004291.1.12",
          "form": "음 저는 좀 다양한 곳에 관심이 있는데",
          "original_form": "음 저는 좀 다양한 곳에 관심이 있는데",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004291.1.13",
          "form": "사진이나 글쓰기 쪽에 요즘 관심이 가더라고요",
          "original_form": "사진이나 글쓰기 쪽에 요즘 관심이 가더라고요",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004291.1.14",
          "form": "찰떡이님은요?",
          "original_form": "찰떡이님은요?",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004291.1.15",
          "form": "사진도 좋네요",
          "original_form": "사진도 좋네요",
          "speaker_id": "2",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004291.1.16",
          "form": "전 운동이나 원데이클래스쪽",
          "original_form": "전 운동이나 원데이클래스쪽",
          "speaker_id": "2",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004291.1.17",
          "form": "아 원데이클래스 좋죠",
          "original_form": "아 원데이클래스 좋죠",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004291.1.18",
          "form": "자주 찾아봣엇어요",
          "original_form": "자주 찾아봣엇어요",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004291.1.19",
          "form": "요즘 원데이도 정말 다양하더라고요",
          "original_form": "요즘 원데이도 정말 다양하더라고요",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004291.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004291.1.21",
          "form": "해보신 것 있어요?",
          "original_form": "해보신 것 있어요?",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004291.1.22",
          "form": "저 샹그리아 만드는거 체험해봣는데 재미잇엇어요",
          "original_form": "저 샹그리아 만드는거 체험해봣는데 재미잇엇어요",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004291.1.23",
          "form": "오오 재미있겠어요",
          "original_form": "오오 재미있겠어요",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004291.1.24",
          "form": "저는 향초만들기랑 떡케이크만들기 해봤어요",
          "original_form": "저는 향초만들기랑 떡케이크만들기 해봤어요",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004291.1.25",
          "form": "오 그런것도 좋네요",
          "original_form": "오 그런것도 좋네요",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004291.1.26",
          "form": "네 재미있더라고요",
          "original_form": "네 재미있더라고요",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004291.1.27",
          "form": "전 등산쪽도 한번참여해보고",
          "original_form": "전 등산쪽도 한번참여해보고",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004291.1.28",
          "form": "오 등산~ 건강도 챙길 수 있어서 좋을 듯해요",
          "original_form": "오 등산~ 건강도 챙길 수 있어서 좋을 듯해요",
          "speaker_id": "1",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004291.1.29",
          "form": "원데이라 부담없더라구요",
          "original_form": "원데이라 부담없더라구요",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004291.1.30",
          "form": "네 원데이는 부담이 없고",
          "original_form": "네 원데이는 부담이 없고",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004291.1.31",
          "form": "근데 또 참여하면 기분전환이 되는 건 좋죠!",
          "original_form": "근데 또 참여하면 기분전환이 되는 건 좋죠!",
          "speaker_id": "1",
          "time": "20210821 16:13"
        }
      ]
