        },
        {
          "id": "MDRW2100031322.1.2",
          "form": "네 했었네 지요",
          "original_form": "네 했었네 지요",
          "speaker_id": "2",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031322.1.3",
          "form": "과거형이 시네요",
          "original_form": "과거형이 시네요",
          "speaker_id": "1",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031322.1.4",
          "form": "그렇죠",
