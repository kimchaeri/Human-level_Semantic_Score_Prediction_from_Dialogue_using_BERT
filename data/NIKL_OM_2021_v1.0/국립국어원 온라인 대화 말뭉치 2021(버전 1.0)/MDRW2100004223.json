        {
          "id": "MDRW2100004223.1.1",
          "form": "나는 맘스터치",
          "original_form": "나는 맘스터치",
          "speaker_id": "2",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004223.1.2",
          "form": "맘스터치 좋지",
          "original_form": "맘스터치 좋지",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004223.1.3",
          "form": "패티가 두꺼워서",
          "original_form": "패티가 두꺼워서",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004223.1.4",
          "form": "맞아맞아 그래서 좋아",
          "original_form": "맞아맞아 그래서 좋아",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004223.1.5",
          "form": "맘스터치 중에 무슨 버거 먹어?",
          "original_form": "맘스터치 중에 무슨 버거 먹어?",
          "speaker_id": "1",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004223.1.6",
          "form": "싸이버거 제일 많이 먹어 ㅋㅋ",
          "original_form": "싸이버거 제일 많이 먹어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004223.1.7",
          "form": "피자는 어디 거 좋아해?",
          "original_form": "피자는 어디 거 좋아해?",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004223.1.8",
          "form": "파파존스 맛있어",
          "original_form": "파파존스 맛있어",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004223.1.9",
          "form": "무슨 맛 좋아해?",
          "original_form": "무슨 맛 좋아해?",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004223.1.10",
          "form": "올미트 ㅋㅋㅋ 씹히는 게 많아서 좋아",
          "original_form": "올미트 ㅋㅋㅋ 씹히는 게 많아서 좋아",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004223.1.11",
          "form": "나는 피자 스쿨",
          "original_form": "나는 피자 스쿨",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004223.1.12",
          "form": "피자스쿨 좋지",
          "original_form": "피자스쿨 좋지",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004223.1.13",
          "form": "응 가성비가 좋아서 맛도 있고",
          "original_form": "응 가성비가 좋아서 맛도 있고",
          "speaker_id": "1",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004223.1.14",
          "form": "피자스쿨 고구마 피자 맛있는데",
          "original_form": "피자스쿨 고구마 피자 맛있는데",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004223.1.15",
          "form": "고구마 피자도 좋고 나는 바이트 피자",
          "original_form": "고구마 피자도 좋고 나는 바이트 피자",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004223.1.16",
          "form": "도우 가장자리에 치즈 있어서 좋아",
          "original_form": "도우 가장자리에 치즈 있어서 좋아",
          "speaker_id": "1",
          "time": "20210821 15:50"
        }
