      "utterance": [
        {
          "id": "MDRW2100046344.1.1",
          "form": "맛집알려주나요",
          "original_form": "맛집알려주나요",
          "speaker_id": "1",
          "time": "20211019 20:45"
        },
        {
          "id": "MDRW2100046344.1.2",
          "form": "좋아하는 음식 말해주세요^_^",
          "original_form": "좋아하는 음식 말해주세요^_^",
          "speaker_id": "2",
          "time": "20211019 20:46"
        },
        {
          "id": "MDRW2100046344.1.3",
          "form": "제주도맛집알려주세요",
          "original_form": "제주도맛집알려주세요",
          "speaker_id": "1",
          "time": "20211019 20:46"
        },
        {
          "id": "MDRW2100046344.1.4",
          "form": "곧 놀러가요~",
          "original_form": "곧 놀러가요~",
          "speaker_id": "1",
          "time": "20211019 20:46"
        },
        {
          "id": "MDRW2100046344.1.5",
          "form": "오예",
          "original_form": "오예",
          "speaker_id": "1",
          "time": "20211019 20:46"
        },
        {
          "id": "MDRW2100046344.1.6",
          "form": "고등어회 !",
          "original_form": "고등어회 !",
          "speaker_id": "1",
          "time": "20211019 20:49"
        },
        {
          "id": "MDRW2100046344.1.7",
          "form": "",
          "original_form": "{emoji}",
          "speaker_id": "1",
          "time": "20211019 20:51"
        },
        {
          "id": "MDRW2100046344.1.8",
          "form": "오!! 꿀팁 알려드릴게요",
          "original_form": "오!! 꿀팁 알려드릴게요",
          "speaker_id": "2",
          "time": "20211019 20:52"
        },
        {
          "id": "MDRW2100046344.1.9",
          "form": "먹고 싶은 음식은 검색하고 무조건 초등학교 옆에 있는 음식점으로 가세요!!",
          "original_form": "먹고 싶은 음식은 검색하고 무조건 초등학교 옆에 있는 음식점으로 가세요!!",
          "speaker_id": "2",
          "time": "20211019 20:52"
        },
        {
          "id": "MDRW2100046344.1.10",
          "form": "초등학교 옆 음식점이 맛있어요?",
          "original_form": "초등학교 옆 음식점이 맛있어요?",
          "speaker_id": "1",
          "time": "20211019 20:53"
        },
        {
          "id": "MDRW2100046344.1.11",
          "form": "제주도민이 알려준 팁이에요!",
          "original_form": "제주도민이 알려준 팁이에요!",
          "speaker_id": "2",
          "time": "20211019 20:56"
        },
        {
          "id": "MDRW2100046344.1.12",
          "form": "초등헉교 옆이 맛집이래요 ㅎㅎ",
          "original_form": "초등헉교 옆이 맛집이래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 20:56"
        },
        {
          "id": "MDRW2100046344.1.13",
          "form": "제주도는 누구랑 가세요?",
          "original_form": "제주도는 누구랑 가세요?",
          "speaker_id": "2",
          "time": "20211019 20:56"
        },
        {
          "id": "MDRW2100046344.1.14",
          "form": "몸과 마음하고 같이 가요",
          "original_form": "몸과 마음하고 같이 가요",
          "speaker_id": "1",
          "time": "20211019 20:57"
        },
        {
          "id": "MDRW2100046344.1.15",
          "form": "그,,,초등학교 옆에는 과연 고등어회를 팔까요?",
          "original_form": "그,,,초등학교 옆에는 과연 고등어회를 팔까요?",
          "speaker_id": "1",
          "time": "20211019 20:58"
        },
        {
          "id": "MDRW2100046344.1.16",
          "form": "ㅋㅋㅋㅋ혼영이시군",
          "original_form": "ㅋㅋㅋㅋ혼영이시군",
          "speaker_id": "2",
          "time": "20211019 20:59"
        },
        {
          "id": "MDRW2100046344.1.17",
          "form": "제주도인데 도처에 고등어회가 널지 않았을까요?",
          "original_form": "제주도인데 도처에 고등어회가 널지 않았을까요?",
          "speaker_id": "2",
          "time": "20211019 20:59"
        },
        {
          "id": "MDRW2100046344.1.18",
          "form": "시장에서 바로 먹는 것도 추천이에요!",
          "original_form": "시장에서 바로 먹는 것도 추천이에요!",
          "speaker_id": "2",
          "time": "20211019 20:59"
        },
        {
          "id": "MDRW2100046344.1.19",
          "form": "고향 어디에요?",
          "original_form": "고향 어디에요?",
          "speaker_id": "1",
          "time": "20211019 20:59"
        },
        {
          "id": "MDRW2100046344.1.20",
          "form": "전 서울이 고향이에요 ㅎㅎ",
          "original_form": "전 서울이 고향이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 21:00"
        },
        {
          "id": "MDRW2100046344.1.21",
          "form": "고향 어디셍",
          "original_form": "고향 어디셍",
          "speaker_id": "2",
          "time": "20211019 21:00"
        },
        {
          "id": "MDRW2100046344.1.22",
          "form": "세요?",
          "original_form": "세요?",
          "speaker_id": "2",
          "time": "20211019 21:00"
        },
        {
          "id": "MDRW2100046344.1.23",
          "form": "평양이에요",
          "original_form": "평양이에요",
          "speaker_id": "1",
          "time": "20211019 21:00"
        },
        {
          "id": "MDRW2100046344.1.24",
          "form": "아하 제주도 놀러가서 어떤 거 계획 중이세요?",
          "original_form": "아하 제주도 놀러가서 어떤 거 계획 중이세요?",
          "speaker_id": "2",
          "time": "20211019 21:04"
        },
        {
          "id": "MDRW2100046344.1.25",
          "form": "아하라뇨ㅋㅋㅋㅋ",
          "original_form": "아하라뇨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 21:04"
        },
        {
          "id": "MDRW2100046344.1.26",
          "form": "평양이라는데",
          "original_form": "평양이라는데",
          "speaker_id": "1",
          "time": "20211019 21:04"
        },
        {
          "id": "MDRW2100046344.1.27",
          "form": "제가 편견이 없어서..ㅋㅋㅋ",
          "original_form": "제가 편견이 없어서..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 21:04"
        },
        {
          "id": "MDRW2100046344.1.28",
          "form": "진짜 고향이 어디세요 ㅋㅋㅋㅋ",
          "original_form": "진짜 고향이 어디세요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 21:05"
        },
        {
          "id": "MDRW2100046344.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 21:05"
        },
        {
          "id": "MDRW2100046344.1.30",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ대전이에요ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ대전이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 21:05"
        },
        {
          "id": "MDRW2100046344.1.31",
          "form": "제주도 계획없이 가는거에요  , 또 다른 제주도 여행 꿀팁 없나요?",
          "original_form": "제주도 계획없이 가는거에요  , 또 다른 제주도 여행 꿀팁 없나요?",
          "speaker_id": "1",
          "time": "20211019 21:06"
        },
        {
          "id": "MDRW2100046344.1.32",
          "form": "초등학교 옆은 너무 광범위하잖아요",
          "original_form": "초등학교 옆은 너무 광범위하잖아요",
          "speaker_id": "1",
          "time": "20211019 21:07"
        },
        {
          "id": "MDRW2100046344.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ...",
          "speaker_id": "2",
          "time": "20211019 21:07"
        },
        {
          "id": "MDRW2100046344.1.34",
          "form": "근데 지금 너무 춥지 않을까요..?",
          "original_form": "근데 지금 너무 춥지 않을까요..?",
          "speaker_id": "2",
          "time": "20211019 21:07"
        },
        {
          "id": "MDRW2100046344.1.35",
          "form": "두꺼운 옷 챙겨가세요!!",
          "original_form": "두꺼운 옷 챙겨가세요!!",
          "speaker_id": "2",
          "time": "20211019 21:08"
        },
        {
          "id": "MDRW2100046344.1.36",
          "form": "다음주 날씨 좋다고 하더라구요",
          "original_form": "다음주 날씨 좋다고 하더라구요",
          "speaker_id": "1",
          "time": "20211019 21:08"
        },
        {
          "id": "MDRW2100046344.1.37",
          "form": "시간되면 놀러오세요 8인용 숙소 혼자써요",
          "original_form": "시간되면 놀러오세요 8인용 숙소 혼자써요",
          "speaker_id": "1",
          "time": "20211019 21:10"
        },
        {
          "id": "MDRW2100046344.1.38",
          "form": "헉 다행이네요 ㅎㅎㅎ",
          "original_form": "헉 다행이네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 21:10"
        },
        {
          "id": "MDRW2100046344.1.39",
          "form": "혼자 8인용 숙소라니.. 부자신가봐요",
          "original_form": "혼자 8인용 숙소라니.. 부자신가봐요",
          "speaker_id": "2",
          "time": "20211019 21:10"
        },
        {
          "id": "MDRW2100046344.1.40",
          "form": "맞아요 저 부자에요",
          "original_form": "맞아요 저 부자에요",
          "speaker_id": "1",
          "time": "20211019 21:11"
        },
        {
