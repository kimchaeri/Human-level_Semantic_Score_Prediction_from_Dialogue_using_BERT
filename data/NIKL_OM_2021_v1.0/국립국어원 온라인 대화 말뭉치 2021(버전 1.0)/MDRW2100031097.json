      "utterance": [
        {
          "id": "MDRW2100031097.1.1",
          "form": "난 샤브샤브용 고기가 좋더라",
          "original_form": "난 샤브샤브용 고기가 좋더라",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031097.1.2",
          "form": "샤브샤브 맞아 넘 얇고 맛있어",
          "original_form": "샤브샤브 맞아 넘 얇고 맛있어",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031097.1.3",
          "form": "샤브샤브를 제일 좋아해서 그런것같기도해",
          "original_form": "샤브샤브를 제일 좋아해서 그런것같기도해",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031097.1.4",
          "form": "우삼겹 짱 맛있어",
          "original_form": "우삼겹 짱 맛있어",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031097.1.5",
          "form": "나도 샤브샤브용 고기는 안 물리고 계속 먹게 되더라",
          "original_form": "나도 샤브샤브용 고기는 안 물리고 계속 먹게 되더라",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031097.1.6",
          "form": "항정살 먹어봤니",
          "original_form": "항정살 먹어봤니",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031097.1.7",
          "form": "우삼격대패도 맛있지",
          "original_form": "우삼격대패도 맛있지",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031097.1.8",
          "form": "항정살 참 맛있어",
          "original_form": "항정살 참 맛있어",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031097.1.9",
          "form": "근데 항정살 너무 기름지고 고소해서 그런가",
          "original_form": "근데 항정살 너무 기름지고 고소해서 그런가",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031097.1.10",
          "form": "김치찌개할때 넣으면 기름기 자글자글해",
          "original_form": "김치찌개할때 넣으면 기름기 자글자글해",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031097.1.11",
          "form": "항정살 먹고 다른 고기 먹으면 비려",
          "original_form": "항정살 먹고 다른 고기 먹으면 비려",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031097.1.12",
          "form": "그것도 그래",
          "original_form": "그것도 그래",
          "speaker_id": "2",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031097.1.13",
          "form": "항정살 김치찌개라니 감동적인 맛이겠다",
          "original_form": "항정살 김치찌개라니 감동적인 맛이겠다",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031097.1.14",
          "form": "삼겹살도 맛있는데",
          "original_form": "삼겹살도 맛있는데",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031097.1.15",
          "form": "삼겹살이 킹옵킹인듯",
          "original_form": "삼겹살이 킹옵킹인듯",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031097.1.16",
          "form": "대박 통했다",
          "original_form": "대박 통했다",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031097.1.17",
          "form": "인정 삼겹살로 통하는 사이로군",
          "original_form": "인정 삼겹살로 통하는 사이로군",
