      "utterance": [
        {
          "id": "MDRW2100020057.1.1",
          "form": "인스타에서도 중국욕이라고 슬쩍 쓰라고 하는거 봤어요",
          "original_form": "인스타에서도 중국욕이라고 슬쩍 쓰라고 하는거 봤어요",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020057.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020057.1.3",
          "form": "그건 뭔가 좀 걸릴 거같고",
          "original_form": "그건 뭔가 좀 걸릴 거같고",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020057.1.4",
          "form": "반말을 은근슬쩍...",
          "original_form": "반말을 은근슬쩍...",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020057.1.5",
          "form": "그거 잇잖아요",
          "original_form": "그거 잇잖아요",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020057.1.6",
          "form": "안녕히개bird야",
          "original_form": "안녕히개bird야",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020057.1.7",
          "form": "전 본부장님한테 좀 개기는데",
          "original_form": "전 본부장님한테 좀 개기는데",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020057.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020057.1.9",
          "form": "하도 본부장님이 꼰 dae마인드가 있으셔서",
          "original_form": "하도 본부장님이 꼰 dae마인드가 있으셔서",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020057.1.10",
          "form": "오 어떻게 개기시져?",
          "original_form": "오 어떻게 개기시져?",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020057.1.11",
          "form": "저는 진짜 쫄보라",
          "original_form": "저는 진짜 쫄보라",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020057.1.12",
          "form": "소심하게 반말정도만 하는편입니닼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "소심하게 반말정도만 하는편입니닼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020057.1.13",
          "form": "ㅋㅋㅋ음 저는 반응을 잘 안하거나 팩폭...?",
          "original_form": "ㅋㅋㅋ음 저는 반응을 잘 안하거나 팩폭...?",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020057.1.14",
          "form": "아니 외근 나갔을 때...본부장님 차를 타고가는데  갑자기  ㅋㅋ자기가 여자들한테 인기가 많았다는거에요?",
          "original_form": "아니 외근 나갔을 때...본부장님 차를 타고가는데  갑자기  ㅋㅋ자기가 여자들한테 인기가 많았다는거에요?",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020057.1.15",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020057.1.16",
          "form": "헛소리를 잘하시네? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헛소리를 잘하시네? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020057.1.17",
          "form": "아니 그걸 저한테 왜 얘기하시는지.. ㅜㅜ",
          "original_form": "아니 그걸 저한테 왜 얘기하시는지.. ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020057.1.18",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020057.1.19",
          "form": "그리고 본인착각일것같은데",
          "original_form": "그리고 본인착각일것같은데",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020057.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020057.1.21",
          "form": "네 정말",
          "original_form": "네 정말",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
