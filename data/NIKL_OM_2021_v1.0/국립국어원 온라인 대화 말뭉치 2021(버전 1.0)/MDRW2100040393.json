        {
          "id": "MDRW2100040393.1.2",
          "form": "아 근데 진짜",
          "original_form": "아 근데 진짜",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100040393.1.3",
          "form": "시뇨라가 뭔소리할지",
          "original_form": "시뇨라가 뭔소리할지",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100040393.1.4",
          "form": "궁금하지 않아.....",
          "original_form": "궁금하지 않아.....",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100040393.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100040393.1.6",
          "form": "시뇨라는",
          "original_form": "시뇨라는",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100040393.1.7",
          "form": "별 말 안해",
          "original_form": "별 말 안해",
          "speaker_id": "1",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040393.1.8",
          "form": "이번 업데이트의 핵심은",
          "original_form": "이번 업데이트의 핵심은",
          "speaker_id": "1",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040393.1.9",
          "form": "야예랑 스카라무슈야",
          "original_form": "야예랑 스카라무슈야",
          "speaker_id": "1",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040393.1.10",
          "form": "아니 야에 짜증나",
          "original_form": "아니 야에 짜증나",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040393.1.11",
          "form": "왜 자꾸 나한테",
          "original_form": "왜 자꾸 나한테",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040393.1.12",
          "form": "다 떠넘기는거야",
          "original_form": "다 떠넘기는거야",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040393.1.13",
          "form": "직접 해",
          "original_form": "직접 해",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040393.1.14",
          "form": "부적쪼가리나 주지 말고",
          "original_form": "부적쪼가리나 주지 말고",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040393.1.15",
          "form": "그치 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100040393.1.16",
          "form": "ㅇ ㅏ",
          "original_form": "ㅇ ㅏ",
          "speaker_id": "1",
          "time": "20210904 19:13"
        },
        {
          "id": "MDRW2100040393.1.17",
          "form": "야타 잡기 싫어하는거 들켯나...",
          "original_form": "야타 잡기 싫어하는거 들켯나...",
          "speaker_id": "1",
          "time": "20210904 19:13"
        },
        {
