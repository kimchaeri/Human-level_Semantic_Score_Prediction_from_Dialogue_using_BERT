      "utterance": [
        {
          "id": "MDRW2100012113.1.1",
          "form": "음식점이라 난 토마토김밥",
          "original_form": "음식점이라 난 토마토김밥",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100012113.1.2",
          "form": "토마토김밥에는 뭐 팔아?",
          "original_form": "토마토김밥에는 뭐 팔아?",
          "speaker_id": "1",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012113.1.3",
          "form": "김밥나라처럼 사람들이 자주먹는 분식!",
          "original_form": "김밥나라처럼 사람들이 자주먹는 분식!",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012113.1.4",
          "form": "아 분식집이야?",
          "original_form": "아 분식집이야?",
          "speaker_id": "1",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012113.1.5",
          "form": "거기서 뭐가 제일 좋아?",
          "original_form": "거기서 뭐가 제일 좋아?",
          "speaker_id": "1",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012113.1.6",
          "form": "김밥 주먹밥 라면 가튼거팔어",
          "original_form": "김밥 주먹밥 라면 가튼거팔어",
          "speaker_id": "2",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012113.1.7",
          "form": "몰라 안먹은지 오래됐어 기억만 나",
          "original_form": "몰라 안먹은지 오래됐어 기억만 나",
          "speaker_id": "2",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012113.1.8",
          "form": "떡볶이도 팔아?",
          "original_form": "떡볶이도 팔아?",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012113.1.9",
          "form": "라볶이도팔아",
          "original_form": "라볶이도팔아",
          "speaker_id": "2",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012113.1.10",
          "form": "라볶이는 싫어",
          "original_form": "라볶이는 싫어",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012113.1.11",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012113.1.12",
          "form": "라볶이 왜싫어해",
          "original_form": "라볶이 왜싫어해",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012113.1.13",
          "form": "라면따로 떡볶이 따로는 좋은데 섞은건 싫어",
          "original_form": "라면따로 떡볶이 따로는 좋은데 섞은건 싫어",
          "speaker_id": "1",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012113.1.14",
          "form": "나는 락앤웍이 좋아",
          "original_form": "나는 락앤웍이 좋아",
          "speaker_id": "1",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012113.1.15",
          "form": "락앤웍이 뭐야",
          "original_form": "락앤웍이 뭐야",
          "speaker_id": "2",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012113.1.16",
          "form": "자장면집인데 엄청 맛있어",
          "original_form": "자장면집인데 엄청 맛있어",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012113.1.17",
          "form": "처음들어봐 유명해?",
          "original_form": "처음들어봐 유명해?",
          "speaker_id": "2",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012113.1.18",
          "form": "응 깔끔하게 해서 인기있어",
          "original_form": "응 깔끔하게 해서 인기있어",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012113.1.19",
          "form": "다른 자장면집은 깔끔한 맛이 안나",
          "original_form": "다른 자장면집은 깔끔한 맛이 안나",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012113.1.20",
          "form": "혹시 짜장이좋아 짬뽕이 좋아",
          "original_form": "혹시 짜장이좋아 짬뽕이 좋아",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012113.1.21",
          "form": "나는 짜장면이 좋은데 짬뽕도 좋아해",
          "original_form": "나는 짜장면이 좋은데 짬뽕도 좋아해",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012113.1.22",
          "form": "그런데 매워서 많이 못먹어",
          "original_form": "그런데 매워서 많이 못먹어",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012113.1.23",
          "form": "근데 왜 좋아해",
          "original_form": "근데 왜 좋아해",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012113.1.24",
          "form": "매워도 맛있거든",
          "original_form": "매워도 맛있거든",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012113.1.25",
          "form": "국물도 먹어?",
          "original_form": "국물도 먹어?",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012113.1.26",
          "form": "응 우리집앞 자장면집에 짬뽕 국물 엄청 맛있어",
          "original_form": "응 우리집앞 자장면집에 짬뽕 국물 엄청 맛있어",
          "speaker_id": "1",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012113.1.27",
          "form": "집앞 자장면집이 락앤웍이야?",
          "original_form": "집앞 자장면집이 락앤웍이야?",
          "speaker_id": "2",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012113.1.28",
          "form": "아니 이름 잘 기억안나는데 맛있아",
          "original_form": "아니 이름 잘 기억안나는데 맛있아",
          "speaker_id": "1",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012113.1.29",
          "form": "락앤웍이 좋아 집앞꺼가 좋아",
          "original_form": "락앤웍이 좋아 집앞꺼가 좋아",
          "speaker_id": "2",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012113.1.30",
          "form": "나는 락앤웍이 좋은데 엄마는 집앞이 더 좋대",
          "original_form": "나는 락앤웍이 좋은데 엄마는 집앞이 더 좋대",
          "speaker_id": "1",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012113.1.31",
          "form": "우아 난 집앞에 우리밀이라고 있는데 거기가좋아",
          "original_form": "우아 난 집앞에 우리밀이라고 있는데 거기가좋아",
          "speaker_id": "2",
          "time": "20210824 19:26"
        }
      ]
    }
