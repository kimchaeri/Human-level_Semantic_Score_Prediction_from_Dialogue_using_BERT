      "utterance": [
        {
          "id": "MDRW2100034183.1.1",
          "form": "저는 친구랑 떡볶이 먹었어요",
          "original_form": "저는 친구랑 떡볶이 먹었어요",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100034183.1.2",
          "form": "떡볶이 맛있겠어요~ 어디꺼요?",
          "original_form": "떡볶이 맛있겠어요~ 어디꺼요?",
          "speaker_id": "1",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100034183.1.3",
          "form": "청년다방 떡볶이요 2인 이상이거든요 혼자면 마라탕먹는데 ㅎㅎ",
          "original_form": "청년다방 떡볶이요 2인 이상이거든요 혼자면 마라탕먹는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100034183.1.4",
          "form": "집에서 해묵질않아서 사묵네요",
          "original_form": "집에서 해묵질않아서 사묵네요",
          "speaker_id": "2",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100034183.1.5",
          "form": "맞죠ㅋㅋ 마라탕도 청년다방껀가요?",
          "original_form": "맞죠ㅋㅋ 마라탕도 청년다방껀가요?",
          "speaker_id": "1",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100034183.1.6",
          "form": "마라는 안팔아요 떡볶이 집이에요ㅎㅎ",
          "original_form": "마라는 안팔아요 떡볶이 집이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100034183.1.7",
          "form": "아 ㅋㅋ 거기에 있는지 알았네요 ㅎㅎ 마라탕은 어디가 맛있나요?",
          "original_form": "아 ㅋㅋ 거기에 있는지 알았네요 ㅎㅎ 마라탕은 어디가 맛있나요?",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100034183.1.8",
          "form": "마라탕은 해외의 얼큰한맛이 맛있는거 같아요 재료도 다양하구",
          "original_form": "마라탕은 해외의 얼큰한맛이 맛있는거 같아요 재료도 다양하구",
          "speaker_id": "2",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100034183.1.9",
          "form": "한번도 안먹어봤는데 ㅎㅎ 어떤맛일지..",
          "original_form": "한번도 안먹어봤는데 ㅎㅎ 어떤맛일지..",
          "speaker_id": "1",
          "time": "20210828 15:04"
        },
        {
          "id": "MDRW2100034183.1.10",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100034183.1.11",
          "form": "애들하고 같이 먹기힘든 음식이에요",
          "original_form": "애들하고 같이 먹기힘든 음식이에요",
          "speaker_id": "2",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100034183.1.12",
          "form": "향신료 냄새 많이나나요?ㅎ",
          "original_form": "향신료 냄새 많이나나요?ㅎ",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034183.1.13",
          "form": "네 매워할거같네요",
          "original_form": "네 매워할거같네요",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034183.1.14",
          "form": "그래도엽떡1단계는 되는데",
          "original_form": "그래도엽떡1단계는 되는데",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034183.1.15",
          "form": "오우 잘먹네요",
          "original_form": "오우 잘먹네요",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034183.1.16",
          "form": "먹여볼만할거같아요",
          "original_form": "먹여볼만할거같아요",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034183.1.17",
          "form": "오ㅋㅋ. 통과인가요ㅋㅋ",
          "original_form": "오ㅋㅋ. 통과인가요ㅋㅋ",
