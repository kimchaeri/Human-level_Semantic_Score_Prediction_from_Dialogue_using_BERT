      "utterance": [
        {
          "id": "MDRW2100023333.1.1",
          "form": "개학시즌이에요",
          "original_form": "개학시즌이에요",
          "speaker_id": "1",
          "time": "20210830 10:04"
        },
        {
          "id": "MDRW2100023333.1.2",
          "form": "대학다니세요?",
          "original_form": "대학다니세요?",
          "speaker_id": "2",
          "time": "20210830 10:07"
        },
        {
          "id": "MDRW2100023333.1.3",
          "form": "졸업했어요",
          "original_form": "졸업했어요",
          "speaker_id": "1",
          "time": "20210830 10:07"
        },
        {
          "id": "MDRW2100023333.1.4",
          "form": "학교다닐때 공부잘하셨나용!!",
          "original_form": "학교다닐때 공부잘하셨나용!!",
          "speaker_id": "2",
          "time": "20210830 10:09"
        },
        {
          "id": "MDRW2100023333.1.5",
          "form": "그냥 보통 정도로 했어요",
          "original_form": "그냥 보통 정도로 했어요",
          "speaker_id": "1",
          "time": "20210830 10:11"
        },
        {
          "id": "MDRW2100023333.1.6",
          "form": "보충은 안 받을 정도 ㅋㅋ",
          "original_form": "보충은 안 받을 정도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:11"
        },
        {
          "id": "MDRW2100023333.1.7",
          "form": "저도요 하하",
          "original_form": "저도요 하하",
          "speaker_id": "2",
          "time": "20210830 10:11"
        },
        {
          "id": "MDRW2100023333.1.8",
          "form": "동아리 하셨어요?",
          "original_form": "동아리 하셨어요?",
          "speaker_id": "2",
          "time": "20210830 10:11"
        },
        {
          "id": "MDRW2100023333.1.9",
          "form": "저는 그냥 쓰잘떼기없는거만 해서",
          "original_form": "저는 그냥 쓰잘떼기없는거만 해서",
          "speaker_id": "2",
          "time": "20210830 10:11"
        },
        {
          "id": "MDRW2100023333.1.10",
          "form": "선후배들이랑 같이 어울리는 그런건 못해봤어요",
          "original_form": "선후배들이랑 같이 어울리는 그런건 못해봤어요",
          "speaker_id": "2",
          "time": "20210830 10:11"
        },
        {
          "id": "MDRW2100023333.1.11",
          "form": "동아리 다녔다가 금방 나왔어요",
          "original_form": "동아리 다녔다가 금방 나왔어요",
          "speaker_id": "1",
          "time": "20210830 10:12"
        },
        {
          "id": "MDRW2100023333.1.12",
          "form": "엠티 같은 거 갔는데 갑자기 거기서 기합 주길래... 탈주...",
          "original_form": "엠티 같은 거 갔는데 갑자기 거기서 기합 주길래... 탈주...",
          "speaker_id": "1",
          "time": "20210830 10:12"
        },
        {
          "id": "MDRW2100023333.1.13",
          "form": "으악 탈주할만해여..",
          "original_form": "으악 탈주할만해여..",
          "speaker_id": "2",
          "time": "20210830 10:15"
        },
        {
          "id": "MDRW2100023333.1.14",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210830 10:15"
        },
        {
          "id": "MDRW2100023333.1.15",
          "form": "동아리가 뭐라고 똥군기를 잡는지 알수가..",
          "original_form": "동아리가 뭐라고 똥군기를 잡는지 알수가..",
          "speaker_id": "2",
          "time": "20210830 10:15"
        },
        {
          "id": "MDRW2100023333.1.16",
          "form": "원래 없었는데 부장이 바뀌면서... 생겼다고... 그러더라고요..ㅎ..",
          "original_form": "원래 없었는데 부장이 바뀌면서... 생겼다고... 그러더라고요..ㅎ..",
          "speaker_id": "1",
          "time": "20210830 10:16"
        },
        {
          "id": "MDRW2100023333.1.17",
          "form": "에휴..",
          "original_form": "에휴..",
          "speaker_id": "2",
          "time": "20210830 10:16"
        },
        {
          "id": "MDRW2100023333.1.18",
          "form": "졸업하셨나요?",
          "original_form": "졸업하셨나요?",
          "speaker_id": "1",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100023333.1.19",
          "form": "저는 회사다니다가 최근에 대학교 진학했어요!",
          "original_form": "저는 회사다니다가 최근에 대학교 진학했어요!",
          "speaker_id": "2",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100023333.1.20",
          "form": "와 대학생이시네요",
          "original_form": "와 대학생이시네요",
          "speaker_id": "1",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100023333.1.21",
          "form": "근데 코로나 때문에 못 나가네요ㅜㅜ",
          "original_form": "근데 코로나 때문에 못 나가네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100023333.1.22",
          "form": "맞아요,,,",
          "original_form": "맞아요,,,",
          "speaker_id": "2",
          "time": "20210830 10:28"
        },
        {
          "id": "MDRW2100023333.1.23",
          "form": "엠티를 못가봤어요",
          "original_form": "엠티를 못가봤어요",
          "speaker_id": "2",
          "time": "20210830 10:28"
        },
        {
          "id": "MDRW2100023333.1.24",
          "form": "아 ㅠㅠ 쭉 화상수업만 하셨나요?",
          "original_form": "아 ㅠㅠ 쭉 화상수업만 하셨나요?",
          "speaker_id": "1",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100023333.1.25",
          "form": "넵 ㅠㅠ",
          "original_form": "넵 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 11:25"
        },
        {
          "id": "MDRW2100023333.1.26",
          "form": "대면 시험만 한두번 있었어요",
          "original_form": "대면 시험만 한두번 있었어요",
          "speaker_id": "2",
          "time": "20210830 11:25"
        }
      ]
    }
