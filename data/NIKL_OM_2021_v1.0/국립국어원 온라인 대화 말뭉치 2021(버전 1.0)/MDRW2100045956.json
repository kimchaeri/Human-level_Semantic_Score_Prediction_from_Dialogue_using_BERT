      "utterance": [
        {
          "id": "MDRW2100045956.1.1",
          "form": "연애프로그램 좋아하세요?",
          "original_form": "연애프로그램 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100045956.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045956.1.3",
          "form": "연애의참견 자주봐요 ㅋㅋㅋ",
          "original_form": "연애의참견 자주봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045956.1.4",
          "form": "오오 그거 너무 재밋어욬ㅋㅋ",
          "original_form": "오오 그거 너무 재밋어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045956.1.5",
          "form": "재연하는게 제일 재밋어요",
          "original_form": "재연하는게 제일 재밋어요",
          "speaker_id": "1",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045956.1.6",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㅋㅋㄱ 전 그 패널들이 카톡 읽는것도 넘 재밌더라고요",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㅋㅋㄱ 전 그 패널들이 카톡 읽는것도 넘 재밌더라고요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045956.1.7",
          "form": "점점 연기가 늘어가요 ㅋㅋㅋㅋ",
          "original_form": "점점 연기가 늘어가요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045956.1.8",
          "form": "ㅋㅋㅋ 그 썰바이벌이랑 애로부부 보세요?",
          "original_form": "ㅋㅋㅋ 그 썰바이벌이랑 애로부부 보세요?",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045956.1.9",
          "form": "썰바이벌은 가끔 알고리즘때문에 뜨면 보고",
          "original_form": "썰바이벌은 가끔 알고리즘때문에 뜨면 보고",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045956.1.10",
          "form": "애로부부는 안봐요!",
          "original_form": "애로부부는 안봐요!",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045956.1.11",
          "form": "두개 다 보시나요?",
          "original_form": "두개 다 보시나요?",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045956.1.12",
          "form": "네네 !! ㅋㅋㅋ 저는 다챙겨보는데",
          "original_form": "네네 !! ㅋㅋㅋ 저는 다챙겨보는데",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045956.1.13",
          "form": "속터지드라구요 ㅠㅠ",
          "original_form": "속터지드라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045956.1.14",
          "form": "그런거 보면 너무 열받는데",
          "original_form": "그런거 보면 너무 열받는데",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045956.1.15",
          "form": "열받는만큼 시간이 잘가요 ㅋㅋㅋ",
          "original_form": "열받는만큼 시간이 잘가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045956.1.16",
          "form": "네넼ㅋㅋㅋㅋㅋㅋㅋㅋㅋ옛날엔 사랑과전쟁도 봣엇어요",
          "original_form": "네넼ㅋㅋㅋㅋㅋㅋㅋㅋㅋ옛날엔 사랑과전쟁도 봣엇어요",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045956.1.17",
          "form": "사랑과전쟁 재밌졐ㅋㅋㅋㅋㅋ",
          "original_form": "사랑과전쟁 재밌졐ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:08"
