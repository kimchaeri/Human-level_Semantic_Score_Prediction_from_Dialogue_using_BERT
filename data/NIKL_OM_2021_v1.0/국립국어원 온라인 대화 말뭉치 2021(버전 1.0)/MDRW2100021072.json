        {
          "id": "MDRW2100021072.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021072.1.2",
          "form": "안래요",
          "original_form": "안래요",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100021072.1.3",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100021072.1.4",
          "form": "요즘 따로하는 운동은 없고 매일 탄천걸어요.",
          "original_form": "요즘 따로하는 운동은 없고 매일 탄천걸어요.",
          "speaker_id": "2",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100021072.1.5",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100021072.1.6",
          "form": "간헐적단식 주1~2회 정도 하구요",
          "original_form": "간헐적단식 주1~2회 정도 하구요",
          "speaker_id": "2",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100021072.1.7",
          "form": "단식 힘들죠",
          "original_form": "단식 힘들죠",
          "speaker_id": "1",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100021072.1.8",
          "form": "아침에 아메리카노 한잔하고 점심까지는 견딜만 하더라구요.",
          "original_form": "아침에 아메리카노 한잔하고 점심까지는 견딜만 하더라구요.",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100021072.1.9",
          "form": "대단한것 같아요",
          "original_form": "대단한것 같아요",
          "speaker_id": "1",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100021072.1.10",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100021072.1.11",
          "form": "메 홈트요",
          "original_form": "메 홈트요",
          "speaker_id": "1",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100021072.1.12",
          "form": "홈트 좋지요",
          "original_form": "홈트 좋지요",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100021072.1.13",
          "form": "저도 유투브 보며 가끔 따라해요",
          "original_form": "저도 유투브 보며 가끔 따라해요",
          "speaker_id": "2",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100021072.1.14",
          "form": "땅끄부부꺼요",
          "original_form": "땅끄부부꺼요",
          "speaker_id": "2",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100021072.1.15",
          "form": "앗 저도",
          "original_form": "앗 저도",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100021072.1.16",
          "form": "땅끄부부 재밌죠? 그런데 저는 좀 힘들더라구요",
          "original_form": "땅끄부부 재밌죠? 그런데 저는 좀 힘들더라구요",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100021072.1.17",
          "form": "제일 쉬운거 찾아 가끔 해요",
          "original_form": "제일 쉬운거 찾아 가끔 해요",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
