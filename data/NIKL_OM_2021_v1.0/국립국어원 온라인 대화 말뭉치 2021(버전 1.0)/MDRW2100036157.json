      "utterance": [
        {
          "id": "MDRW2100036157.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100036157.1.2",
          "form": "아니에요 ㅎㅎ",
          "original_form": "아니에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100036157.1.3",
          "form": "저는 키웁니당 !",
          "original_form": "저는 키웁니당 !",
          "speaker_id": "2",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100036157.1.4",
          "form": "아뇨 키우곤 싶은데 요즘 집값보고 좀 포기했어요",
          "original_form": "아뇨 키우곤 싶은데 요즘 집값보고 좀 포기했어요",
          "speaker_id": "1",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100036157.1.5",
          "form": "개인가요? 고양이인가요?",
          "original_form": "개인가요? 고양이인가요?",
          "speaker_id": "1",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100036157.1.6",
          "form": "ㅠ 전 혼자 키울라면 이게 전세 얻기가 더 힘들어서 전 포기",
          "original_form": "ㅠ 전 혼자 키울라면 이게 전세 얻기가 더 힘들어서 전 포기",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036157.1.7",
          "form": "강아지입니다 ! 너무 이뻐요",
          "original_form": "강아지입니다 ! 너무 이뻐요",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036157.1.8",
          "form": "몇살인가요?",
          "original_form": "몇살인가요?",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036157.1.9",
          "form": "4살입니당",
          "original_form": "4살입니당",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036157.1.10",
          "form": "한창 건강하구 이제 말귀 다 알아들을때네요",
          "original_form": "한창 건강하구 이제 말귀 다 알아들을때네요",
          "speaker_id": "1",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036157.1.11",
          "form": "대형인가요 소형인가요 중형인가요?",
          "original_form": "대형인가요 소형인가요 중형인가요?",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036157.1.12",
          "form": "소형견입니다 치와와",
          "original_form": "소형견입니다 치와와",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036157.1.13",
          "form": "저 치와와 완전 좋아해요",
          "original_form": "저 치와와 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036157.1.14",
          "form": "오정말요 사진기능 되면 보여드리고싶다",
          "original_form": "오정말요 사진기능 되면 보여드리고싶다",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036157.1.15",
          "form": "작은 애라 무서움이 많다보니 소형견 특유의 예민함이 있지만 주인을 사랑하구",
          "original_form": "작은 애라 무서움이 많다보니 소형견 특유의 예민함이 있지만 주인을 사랑하구",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036157.1.16",
          "form": "교감이 엄청 잘되더라구요",
          "original_form": "교감이 엄청 잘되더라구요",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036157.1.17",
          "form": "장모인가요 단모인가요?",
          "original_form": "장모인가요 단모인가요?",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036157.1.18",
          "form": "장모치와와에요 너무예뻐용",
          "original_form": "장모치와와에요 너무예뻐용",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036157.1.19",
          "form": "제심장 제전부",
          "original_form": "제심장 제전부",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036157.1.20",
          "form": "작구 귀엽구 그리고 일단 사람보다 많이 작으니깐",
          "original_form": "작구 귀엽구 그리고 일단 사람보다 많이 작으니깐",
          "speaker_id": "1",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100036157.1.21",
          "form": "산책하거나 이럴대도 통제하구지켜줄 수 있는 점이 소형견의 매력이라고 생각해요",
          "original_form": "산책하거나 이럴대도 통제하구지켜줄 수 있는 점이 소형견의 매력이라고 생각해요",
          "speaker_id": "1",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100036157.1.22",
          "form": "맞아요 어디든 데려갈수있어좋어요",
          "original_form": "맞아요 어디든 데려갈수있어좋어요",
          "speaker_id": "2",
          "time": "20210830 17:54"
        }
      ]
