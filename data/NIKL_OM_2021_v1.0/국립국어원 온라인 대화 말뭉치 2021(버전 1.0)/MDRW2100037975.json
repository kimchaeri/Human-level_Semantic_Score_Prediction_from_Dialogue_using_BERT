      "utterance": [
        {
          "id": "MDRW2100037975.1.1",
          "form": "그래 알아서 들어오겠지ㅎㅎㅎ",
          "original_form": "그래 알아서 들어오겠지ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037975.1.2",
          "form": "맞어맞어ㅋㅋ",
          "original_form": "맞어맞어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037975.1.3",
          "form": "반도 거기 놀이터에 거의 있다 우리원 ㅋㅋㅋ",
          "original_form": "반도 거기 놀이터에 거의 있다 우리원 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100037975.1.4",
          "form": "구석진 놀이터 있제??",
          "original_form": "구석진 놀이터 있제??",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100037975.1.5",
          "form": "105동 옆에 있는",
          "original_form": "105동 옆에 있는",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100037975.1.6",
          "form": "혹시 5시넘어서 그근처 지나다 우리원 보면 안부 좀 전해도이 ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "혹시 5시넘어서 그근처 지나다 우리원 보면 안부 좀 전해도이 ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100037975.1.7",
          "form": "아 진짜 ㅋㅋㅋ",
          "original_form": "아 진짜 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100037975.1.8",
          "form": "웃기노 ㅋㅋ 기린놀이터에서 지나가는 애들 다 보인다 ㅋㅋㅋ",
          "original_form": "웃기노 ㅋㅋ 기린놀이터에서 지나가는 애들 다 보인다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100037975.1.9",
          "form": "지안이 집에서 인사함 ㅋㅋㅋ 밖에 애들이랑",
          "original_form": "지안이 집에서 인사함 ㅋㅋㅋ 밖에 애들이랑",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100037975.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 아진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 아진짜",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037975.1.11",
          "form": "인싸다 인싸",
          "original_form": "인싸다 인싸",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037975.1.12",
          "form": "그러니 애들이 계속 놀자고 전화오지 ㅋㅋㅋ",
          "original_form": "그러니 애들이 계속 놀자고 전화오지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037975.1.13",
          "form": "응응 저번에 가만히 있다가",
          "original_form": "응응 저번에 가만히 있다가",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037975.1.14",
          "form": "지안이 눈감술 하고 싶으면 거기 구름사다리 놀이터로 오라해",
          "original_form": "지안이 눈감술 하고 싶으면 거기 구름사다리 놀이터로 오라해",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037975.1.15",
          "form": "힐데스 아이가 지나갔나봐 ㅋㅋ",
          "original_form": "힐데스 아이가 지나갔나봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037975.1.16",
          "form": "우리 원 항상 있다고 ㅋㅋㅋㅋㅋ",
          "original_form": "우리 원 항상 있다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037975.1.17",
          "form": "막 인사하면서 얘기하더라 ㅋㅋ",
          "original_form": "막 인사하면서 얘기하더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037975.1.18",
          "form": "힐데스 아이ㅋㅋㅋ",
          "original_form": "힐데스 아이ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037975.1.19",
          "form": "웃기노 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "웃기노 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037975.1.20",
          "form": "응응 ㅋㅋㅋ 그래놓고 바로",
          "original_form": "응응 ㅋㅋㅋ 그래놓고 바로",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037975.1.21",
          "form": "약간 방앗간 같은 느낌이네",
          "original_form": "약간 방앗간 같은 느낌이네",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037975.1.22",
          "form": "울집에 와도 되냐고 ㅋㅋㅋ",
          "original_form": "울집에 와도 되냐고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037975.1.23",
