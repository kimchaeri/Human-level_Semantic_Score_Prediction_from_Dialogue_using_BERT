      "utterance": [
        {
          "id": "MDRW2100040530.1.1",
          "form": "넵 너무 배부르네요 식사하셨어요?",
          "original_form": "넵 너무 배부르네요 식사하셨어요?",
          "speaker_id": "1",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100040530.1.2",
          "form": "네 먹었어요\n뭐 드셨나요?",
          "original_form": "네 먹었어요\n뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100040530.1.3",
          "form": "냉동삼겹살이요!",
          "original_form": "냉동삼겹살이요!",
          "speaker_id": "1",
          "time": "20210904 22:22"
        },
        {
          "id": "MDRW2100040530.1.4",
          "form": "뭐드셨어요ㅡ??",
          "original_form": "뭐드셨어요ㅡ??",
          "speaker_id": "1",
          "time": "20210904 22:22"
        },
        {
          "id": "MDRW2100040530.1.5",
          "form": "저는 곱창이요\n외식하신건가요?",
          "original_form": "저는 곱창이요\n외식하신건가요?",
          "speaker_id": "2",
          "time": "20210904 22:22"
        },
        {
          "id": "MDRW2100040530.1.6",
          "form": "네네 오랜만에 친구 만났어요 ㅎㅎ",
          "original_form": "네네 오랜만에 친구 만났어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100040530.1.7",
          "form": "냉삼겹 맛있죠 사이드나 후식으로 뭐 드셨나요?",
          "original_form": "냉삼겹 맛있죠 사이드나 후식으로 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040530.1.8",
          "form": "녹차라떼 마셨어요 ㅎㅎ 아메리카노 마실걸 후회만 ㅎㅎ",
          "original_form": "녹차라떼 마셨어요 ㅎㅎ 아메리카노 마실걸 후회만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040530.1.9",
          "form": "곱창이랑 뭐 드셨어요?",
          "original_form": "곱창이랑 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040530.1.10",
          "form": "저는 곱창이랑 그냥 야채랑 먹었네요ㅋㅋㅋ",
          "original_form": "저는 곱창이랑 그냥 야채랑 먹었네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040530.1.11",
          "form": "좋아하시는 음식 있으신가요?",
          "original_form": "좋아하시는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040530.1.12",
          "form": "저는 초밥 좋아해요!",
          "original_form": "저는 초밥 좋아해요!",
          "speaker_id": "1",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100040530.1.13",
          "form": "어떤음식 좋아하세요?",
          "original_form": "어떤음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100040530.1.14",
          "form": "전 떡볶이요!!!",
          "original_form": "전 떡볶이요!!!",
          "speaker_id": "2",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100040530.1.15",
          "form": "앗 저도 너무 좋아해요",
          "original_form": "앗 저도 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100040530.1.16",
          "form": "어디거 제일 좋아하세요?",
          "original_form": "어디거 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100040530.1.17",
          "form": "저는 엽떡이요",
          "original_form": "저는 엽떡이요",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040530.1.18",
          "form": "어디꺼 좋아하세요?",
          "original_form": "어디꺼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040530.1.19",
          "form": "저는 신전이요!",
          "original_form": "저는 신전이요!",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040530.1.20",
          "form": "친구들 거의 엽떡 좋아하더라구요 ㅎ",
          "original_form": "친구들 거의 엽떡 좋아하더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040530.1.21",
          "form": "그러시군요 신전도 맛있죠",
          "original_form": "그러시군요 신전도 맛있죠",
          "speaker_id": "2",
