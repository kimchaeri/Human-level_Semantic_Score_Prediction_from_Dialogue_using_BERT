      "utterance": [
        {
          "id": "MDRW2100044894.1.1",
          "form": "가깝지 않아서 아쉽더라고요",
          "original_form": "가깝지 않아서 아쉽더라고요",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044894.1.2",
          "form": "르빵이라고 명동성당지하에 있는 빵집",
          "original_form": "르빵이라고 명동성당지하에 있는 빵집",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044894.1.3",
          "form": "케이크 맛집입다",
          "original_form": "케이크 맛집입다",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044894.1.4",
          "form": "아 진짜 맛있져!!!",
          "original_form": "아 진짜 맛있져!!!",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044894.1.5",
          "form": "무슨 케이크 좋아하세여??",
          "original_form": "무슨 케이크 좋아하세여??",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044894.1.6",
          "form": "과일올라간케익은 다 좋아해요ㅋㅋㅋㅋ",
          "original_form": "과일올라간케익은 다 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044894.1.7",
          "form": "저는 생크림 좋아해요!!!!",
          "original_form": "저는 생크림 좋아해요!!!!",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044894.1.8",
          "form": "르빵에세 딸기생크림케익이랑 복숭아생크림케익을 먹었는데",
          "original_form": "르빵에세 딸기생크림케익이랑 복숭아생크림케익을 먹었는데",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044894.1.9",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044894.1.10",
          "form": "아 저 딸기만 먹어봤는데 복숭아도 맛있나보네요",
          "original_form": "아 저 딸기만 먹어봤는데 복숭아도 맛있나보네요",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044894.1.11",
          "form": "꼭 먹어보고 싶습니다!!!",
          "original_form": "꼭 먹어보고 싶습니다!!!",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044894.1.12",
          "form": "과일 좋아하시면 여름 과일 많이 드셨겠어요",
          "original_form": "과일 좋아하시면 여름 과일 많이 드셨겠어요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044894.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044894.1.14",
          "form": "수박이랑 포도 참외ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "수박이랑 포도 참외ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044894.1.15",
          "form": "복숭아 철이 끝나기 전에 한번 더 먹고 싶네요",
          "original_form": "복숭아 철이 끝나기 전에 한번 더 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044894.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044894.1.17",
          "form": "오우 알차게 드셨네요",
          "original_form": "오우 알차게 드셨네요",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044894.1.18",
          "form": "저 수박하니까 꼭 말해드리고 싶어요",
          "original_form": "저 수박하니까 꼭 말해드리고 싶어요",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044894.1.19",
          "form": "비가많이와서 끝물이니 얼른 먹어줘야합니당",
          "original_form": "비가많이와서 끝물이니 얼른 먹어줘야합니당",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044894.1.20",
          "form": "강고집 수박이라고 아시나요??",
          "original_form": "강고집 수박이라고 아시나요??",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044894.1.21",
          "form": "초여름에 파는 수박이 있는데요 거기 수박이 진짜 맛있습니다 기회가 되신다면 꼭!!",
          "original_form": "초여름에 파는 수박이 있는데요 거기 수박이 진짜 맛있습니다 기회가 되신다면 꼭!!",
