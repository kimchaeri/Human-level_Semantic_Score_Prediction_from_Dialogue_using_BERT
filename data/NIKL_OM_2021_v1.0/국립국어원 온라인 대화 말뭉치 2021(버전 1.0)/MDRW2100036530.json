      "utterance": [
        {
          "id": "MDRW2100036530.1.1",
          "form": "지금 내 옆에 누가 있는지 알아?",
          "original_form": "지금 내 옆에 누가 있는지 알아?",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.2",
          "form": "너 루이 알아?",
          "original_form": "너 루이 알아?",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.3",
          "form": "바로 우리집 반려동물이야",
          "original_form": "바로 우리집 반려동물이야",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.4",
          "form": "앵무새야. 노란색이고.",
          "original_form": "앵무새야. 노란색이고.",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.5",
          "form": "음 사람이름은 아닌 것 같은데",
          "original_form": "음 사람이름은 아닌 것 같은데",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.6",
          "form": "앵무새야!",
          "original_form": "앵무새야!",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.7",
          "form": "노란색만 있어?",
          "original_form": "노란색만 있어?",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.8",
          "form": "사람이름중에도 루이 있잖아",
          "original_form": "사람이름중에도 루이 있잖아",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.9",
          "form": "얼굴은 빨개",
          "original_form": "얼굴은 빨개",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.10",
          "form": "날 봐서 부끄러운건지 뭔지",
          "original_form": "날 봐서 부끄러운건지 뭔지",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.11",
          "form": "근데 한국사람이기는 힘들지",
          "original_form": "근데 한국사람이기는 힘들지",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.12",
          "form": "그리고 몸에선 고소한 냄새가 나",
          "original_form": "그리고 몸에선 고소한 냄새가 나",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그건 자의식과잉이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그건 자의식과잉이야",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036530.1.14",
          "form": "그리고 따뜻해",
          "original_form": "그리고 따뜻해",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.15",
          "form": "자의식과잉이라니.",
          "original_form": "자의식과잉이라니.",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.16",
          "form": "흠~뭘먹으면 고소한 냄새가 나지",
          "original_form": "흠~뭘먹으면 고소한 냄새가 나지",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.17",
          "form": "우리 앵무새한테 물리고싶은거랴?",
          "original_form": "우리 앵무새한테 물리고싶은거랴?",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.18",
          "form": "알곡을 먹어서그런지",
          "original_form": "알곡을 먹어서그런지",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.19",
          "form": "걔는 물로 씻겨주니?",
          "original_form": "걔는 물로 씻겨주니?",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.20",
          "form": "고소하고 따뜻해",
          "original_form": "고소하고 따뜻해",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.21",
          "form": "자기가 알아서 씻어",
          "original_form": "자기가 알아서 씻어",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.22",
          "form": "목욕을 좋아해",
          "original_form": "목욕을 좋아해",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036530.1.23",
          "form": "말만 들어도 너무 포근하다",
          "original_form": "말만 들어도 너무 포근하다",
          "speaker_id": "1",
          "time": "20210831 00:09"
        }
      ]
    }
