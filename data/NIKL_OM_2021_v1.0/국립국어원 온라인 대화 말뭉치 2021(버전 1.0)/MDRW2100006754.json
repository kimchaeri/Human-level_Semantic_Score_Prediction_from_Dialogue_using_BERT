      "utterance": [
        {
          "id": "MDRW2100006754.1.1",
          "form": "배달 음식 자주 드세요?",
          "original_form": "배달 음식 자주 드세요?",
          "speaker_id": "1",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100006754.1.2",
          "form": "자주 먹는 편은 아닌 거 같아요",
          "original_form": "자주 먹는 편은 아닌 거 같아요",
          "speaker_id": "2",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100006754.1.3",
          "form": "한달에 두 번 정도...?",
          "original_form": "한달에 두 번 정도...?",
          "speaker_id": "2",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100006754.1.4",
          "form": "오 어떤거 좋아하세요",
          "original_form": "오 어떤거 좋아하세요",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100006754.1.5",
          "form": "치킨 좋아해요!",
          "original_form": "치킨 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100006754.1.6",
          "form": "어느치킨 좋아하세요!",
          "original_form": "어느치킨 좋아하세요!",
          "speaker_id": "1",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100006754.1.7",
          "form": "푸라닭 블랙알리오요",
          "original_form": "푸라닭 블랙알리오요",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100006754.1.8",
          "form": "진짜 맛있더라고요...",
          "original_form": "진짜 맛있더라고요...",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100006754.1.9",
          "form": "우아 무슨 맛인가요?",
          "original_form": "우아 무슨 맛인가요?",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100006754.1.10",
          "form": "단짠단짠이에요",
          "original_form": "단짠단짠이에요",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100006754.1.11",
          "form": "좀 짭쪼롬한데 맛있어요",
          "original_form": "좀 짭쪼롬한데 맛있어요",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100006754.1.12",
          "form": "오오 너무 좋네요 ㅋㅋㅋ",
          "original_form": "오오 너무 좋네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100006754.1.13",
          "form": "치킨이랑 맥주랑 먹고싳네요",
          "original_form": "치킨이랑 맥주랑 먹고싳네요",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100006754.1.14",
          "form": "하니님은 무슨 치킨 좋아하세요??",
          "original_form": "하니님은 무슨 치킨 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100006754.1.15",
          "form": "저는 페리카나요!!!",
          "original_form": "저는 페리카나요!!!",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100006754.1.16",
          "form": "후라이드 ㅎㅎ",
          "original_form": "후라이드 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100006754.1.17",
          "form": "페리카나 치킨 못 먹어본지 오래 됐네요...",
          "original_form": "페리카나 치킨 못 먹어본지 오래 됐네요...",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100006754.1.18",
          "form": "맛나요 ㅎㄹ",
          "original_form": "맛나요 ㅎㄹ",
