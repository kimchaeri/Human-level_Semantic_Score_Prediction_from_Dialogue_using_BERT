      "utterance": [
        {
          "id": "MDRW2100045938.1.1",
          "form": "오 어떤 브랜드 좋아하시나요?",
          "original_form": "오 어떤 브랜드 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045938.1.2",
          "form": "개인카페요!!",
          "original_form": "개인카페요!!",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045938.1.3",
          "form": "공차는 살짝 파우더 맛이 날때가 있더라구요 :)",
          "original_form": "공차는 살짝 파우더 맛이 날때가 있더라구요 :)",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045938.1.4",
          "form": "아... 공차는 그쵸ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아... 공차는 그쵸ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045938.1.5",
          "form": "외국에서는 공차 별로 안 유명하대요",
          "original_form": "외국에서는 공차 별로 안 유명하대요",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045938.1.6",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045938.1.7",
          "form": "갑자기 대만 밀크티 먹고싶어요",
          "original_form": "갑자기 대만 밀크티 먹고싶어요",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045938.1.8",
          "form": "아 맞아요 대만 꺼 맛있다는대",
          "original_form": "아 맞아요 대만 꺼 맛있다는대",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045938.1.9",
          "form": "길거리에서 암데나 들어가도",
          "original_form": "길거리에서 암데나 들어가도",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045938.1.10",
          "form": "맛집이고 그랬어요",
          "original_form": "맛집이고 그랬어요",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045938.1.11",
          "form": "친구도대만갔다오더니",
          "original_form": "친구도대만갔다오더니",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045938.1.12",
          "form": "행복당 밀크티 타령하더라구요",
          "original_form": "행복당 밀크티 타령하더라구요",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045938.1.13",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100045938.1.14",
          "form": "행복당 서울에 있어요!",
          "original_form": "행복당 서울에 있어요!",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100045938.1.15",
          "form": "ㅇ0ㅇ 진짜요? 알려줘야겠어요",
          "original_form": "ㅇ0ㅇ 진짜요? 알려줘야겠어요",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100045938.1.16",
          "form": "ㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100045938.1.17",
          "form": "저 몇달전에 가로수길에서 사마셨어요",
          "original_form": "저 몇달전에 가로수길에서 사마셨어요",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100045938.1.18",
          "form": "저는 지방 살아서 그렇게 자주 못 가겠지만 그래도 한국 내에 있는게 어디에요!",
          "original_form": "저는 지방 살아서 그렇게 자주 못 가겠지만 그래도 한국 내에 있는게 어디에요!",
          "speaker_id": "1",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100045938.1.19",
          "form": "ㅎㅎ맞아요 이시국에",
          "original_form": "ㅎㅎ맞아요 이시국에",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100045938.1.20",
          "form": "비행기 안타는게 어딘가싶어요 ㅠ",
          "original_form": "비행기 안타는게 어딘가싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100045938.1.21",
          "form": "맞아요 거기 가려면 적어도 이년은 기다려야겠다 싶은데ㅠㅠ",
          "original_form": "맞아요 거기 가려면 적어도 이년은 기다려야겠다 싶은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100045938.1.22",
          "form": "헉 내년에 어떻게 안될까요",
          "original_form": "헉 내년에 어떻게 안될까요",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100045938.1.23",
