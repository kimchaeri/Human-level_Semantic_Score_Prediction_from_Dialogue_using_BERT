      "utterance": [
        {
          "id": "MDRW2100029090.1.1",
          "form": "씨씨 부럽",
          "original_form": "씨씨 부럽",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029090.1.2",
          "form": "대학교는 다른데 다녀요",
          "original_form": "대학교는 다른데 다녀요",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029090.1.3",
          "form": "수시 전부 같은데 지망했는데",
          "original_form": "수시 전부 같은데 지망했는데",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029090.1.4",
          "form": "걔가 떨어져버린거 있죠...",
          "original_form": "걔가 떨어져버린거 있죠...",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029090.1.5",
          "form": "ㅋㅋ전산 오류 나서",
          "original_form": "ㅋㅋ전산 오류 나서",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029090.1.6",
          "form": "말이 두번씩 ㅋㅋ",
          "original_form": "말이 두번씩 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029090.1.7",
          "form": "아깝군요 ㅠㅠ",
          "original_form": "아깝군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029090.1.8",
          "form": "오늘 오류가 좀 심하더라구요",
          "original_form": "오늘 오류가 좀 심하더라구요",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029090.1.9",
          "form": "많이 아까워요",
          "original_form": "많이 아까워요",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029090.1.10",
          "form": "같은 동네 아니었으면 울뻔요",
          "original_form": "같은 동네 아니었으면 울뻔요",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029090.1.11",
          "form": "그래도 같은 동네라서 좋네쇼",
          "original_form": "그래도 같은 동네라서 좋네쇼",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029090.1.12",
          "form": "그럼에도 불구하고 자주 만나진 못하고 있습니다",
          "original_form": "그럼에도 불구하고 자주 만나진 못하고 있습니다",
          "speaker_id": "1",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100029090.1.13",
          "form": "공원같은곳 데이트 어떠세요",
          "original_form": "공원같은곳 데이트 어떠세요",
          "speaker_id": "2",
          "time": "20210903 08:18"
        },
        {
          "id": "MDRW2100029090.1.14",
          "form": "집 주변에 공원이 있어서 거긴 이미 점령했죠",
          "original_form": "집 주변에 공원이 있어서 거긴 이미 점령했죠",
          "speaker_id": "1",
          "time": "20210903 08:21"
        },
        {
          "id": "MDRW2100029090.1.15",
          "form": "에어비앤비 같은데서",
          "original_form": "에어비앤비 같은데서",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029090.1.16",
          "form": "레지던스 같은곳 빌려서",
          "original_form": "레지던스 같은곳 빌려서",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029090.1.17",
          "form": "요리하기?",
          "original_form": "요리하기?",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029090.1.18",
          "form": "만약 요리하면 제가 먹는 포지션일거 같아요ㅋㅋ",
          "original_form": "만약 요리하면 제가 먹는 포지션일거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100029090.1.19",
          "form": "먹는 사람도 있고 하죠",
          "original_form": "먹는 사람도 있고 하죠",
          "speaker_id": "2",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029090.1.20",
          "form": "저희집도 와이프가 먹는 포지션입니다 ㅋㅋ",
          "original_form": "저희집도 와이프가 먹는 포지션입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029090.1.21",
          "form": "ㅋㅋㅋ 그런가요",
          "original_form": "ㅋㅋㅋ 그런가요",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100029090.1.22",
          "form": "네 여자만 요리하는 시대가 아니잖아요 더 이상ㅋㅋ",
          "original_form": "네 여자만 요리하는 시대가 아니잖아요 더 이상ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029090.1.23",
          "form": "그쵸ㅋㅋ 모두가 요리하는 시대",
          "original_form": "그쵸ㅋㅋ 모두가 요리하는 시대",
          "speaker_id": "1",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100029090.1.24",
          "form": "그리고 밀키트가 잘 나옵니다 ㅎㅎ",
          "original_form": "그리고 밀키트가 잘 나옵니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100029090.1.25",
          "form": "ㅋㅋㅋㅋㅋ 밀키트가 구세주죠",
          "original_form": "ㅋㅋㅋㅋㅋ 밀키트가 구세주죠",
