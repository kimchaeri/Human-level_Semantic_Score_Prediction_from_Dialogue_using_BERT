      "utterance": [
        {
          "id": "MDRW2100043430.1.1",
          "form": "호박떡 시장에서 팔아",
          "original_form": "호박떡 시장에서 팔아",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043430.1.2",
          "form": "서문시장??",
          "original_form": "서문시장??",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043430.1.3",
          "form": "마다!",
          "original_form": "마다!",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043430.1.4",
          "form": "마자!",
          "original_form": "마자!",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043430.1.5",
          "form": "먹어보고시프다",
          "original_form": "먹어보고시프다",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.6",
          "form": "완전 호떡 같겟지?",
          "original_form": "완전 호떡 같겟지?",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.7",
          "form": "그거랑 다르지않을가",
          "original_form": "그거랑 다르지않을가",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.8",
          "form": "앗 그런가??",
          "original_form": "앗 그런가??",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.9",
          "form": "응응 쫌다를듯",
          "original_form": "응응 쫌다를듯",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.10",
          "form": "그래도 맛은 맛잇을거같아",
          "original_form": "그래도 맛은 맛잇을거같아",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.11",
          "form": "마저마저",
          "original_form": "마저마저",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.12",
          "form": "호박맛이날까?",
          "original_form": "호박맛이날까?",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.13",
          "form": "그렇겟지?",
          "original_form": "그렇겟지?",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.14",
          "form": "완전 달달하면 좋겟어",
          "original_form": "완전 달달하면 좋겟어",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043430.1.15",
          "form": "마쟙",
          "original_form": "마쟙",
          "speaker_id": "1",
