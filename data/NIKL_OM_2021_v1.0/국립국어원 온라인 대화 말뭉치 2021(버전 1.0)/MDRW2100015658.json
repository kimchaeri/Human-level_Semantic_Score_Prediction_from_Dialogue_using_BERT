      "utterance": [
        {
          "id": "MDRW2100015658.1.1",
          "form": "웹소설 뭐 즐겨보세요?",
          "original_form": "웹소설 뭐 즐겨보세요?",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015658.1.2",
          "form": "헉 장르별로 달라요",
          "original_form": "헉 장르별로 달라요",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015658.1.3",
          "form": "혹시 즐겨보시는 거 있으세요?",
          "original_form": "혹시 즐겨보시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015658.1.4",
          "form": "저는 유명한거 거의 다 챙겨봐요!",
          "original_form": "저는 유명한거 거의 다 챙겨봐요!",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015658.1.5",
          "form": "저 로판 좋아해요!",
          "original_form": "저 로판 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015658.1.6",
          "form": "헉 저도요!!",
          "original_form": "헉 저도요!!",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015658.1.7",
          "form": "카카페 위주로 보시나요 아니면 시리즈?!",
          "original_form": "카카페 위주로 보시나요 아니면 시리즈?!",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015658.1.8",
          "form": "저는 둘 다 보는데 카카페를 더 많이봐요!!",
          "original_form": "저는 둘 다 보는데 카카페를 더 많이봐요!!",
          "speaker_id": "1",
          "time": "20210826 00:07"
        },
        {
          "id": "MDRW2100015658.1.9",
          "form": "헉 저도요",
          "original_form": "헉 저도요",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015658.1.10",
          "form": "저는 최근 이가주 다봤는데요 이게 완결이 아니더라고요???",
          "original_form": "저는 최근 이가주 다봤는데요 이게 완결이 아니더라고요???",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015658.1.11",
          "form": "이번생엔 가주 그거 말씀하시는거죠? 그거 웹툰으로만 봤어요!",
          "original_form": "이번생엔 가주 그거 말씀하시는거죠? 그거 웹툰으로만 봤어요!",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015658.1.12",
          "form": "헉 웹툰 너무 귀엽죠ㅠㅠ",
          "original_form": "헉 웹툰 너무 귀엽죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015658.1.13",
          "form": "남미숨도 보세요 웹툰?",
          "original_form": "남미숨도 보세요 웹툰?",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015658.1.14",
          "form": "소설로도 재미있나요?",
          "original_form": "소설로도 재미있나요?",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015658.1.15",
          "form": "이가주요?!",
          "original_form": "이가주요?!",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015658.1.16",
          "form": "네네네 소설도 재미있어요!!!!!!",
          "original_form": "네네네 소설도 재미있어요!!!!!!",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015658.1.17",
          "form": "카카페 로판 뭐보세요?!",
          "original_form": "카카페 로판 뭐보세요?!",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015658.1.18",
          "form": "저는 웹툰위주로 봐요! 아빠 나 이결혼 안할래요 보시나요?",
          "original_form": "저는 웹툰위주로 봐요! 아빠 나 이결혼 안할래요 보시나요?",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015658.1.19",
          "form": "헉 네네",
          "original_form": "헉 네네",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015658.1.20",
          "form": "그림체 진짜 이쁘잔ㄹ아요!!!!",
          "original_form": "그림체 진짜 이쁘잔ㄹ아요!!!!",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015658.1.21",
          "form": "악역의 엔딩은 죽음뿐도 보고있어요!",
          "original_form": "악역의 엔딩은 죽음뿐도 보고있어요!",
          "speaker_id": "1",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015658.1.22",
          "form": "헉 명작이죠",
          "original_form": "헉 명작이죠",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015658.1.23",
          "form": "악엔죽은 소설도 읽으시는 거 추천합니다",
          "original_form": "악엔죽은 소설도 읽으시는 거 추천합니다",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015658.1.24",
          "form": "소설이 명작으로 유명하더라고요",
          "original_form": "소설이 명작으로 유명하더라고요",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015658.1.25",
