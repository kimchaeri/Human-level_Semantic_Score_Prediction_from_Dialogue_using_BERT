      "utterance": [
        {
          "id": "MDRW2100034580.1.1",
          "form": "학생이세요??",
          "original_form": "학생이세요??",
          "speaker_id": "1",
          "time": "20210828 15:38"
        },
        {
          "id": "MDRW2100034580.1.2",
          "form": "아니요 name1님은요?",
          "original_form": "아니요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 15:39"
        },
        {
          "id": "MDRW2100034580.1.3",
          "form": "저도 아니에요 ㅋㅋㅋ",
          "original_form": "저도 아니에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034580.1.4",
          "form": "학교다닐때가 그리워요",
          "original_form": "학교다닐때가 그리워요",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034580.1.5",
          "form": "저도 너무 그리워요",
          "original_form": "저도 너무 그리워요",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034580.1.6",
          "form": "언제가 제일 그리우세요?",
          "original_form": "언제가 제일 그리우세요?",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034580.1.7",
          "form": "음 아무래도",
          "original_form": "음 아무래도",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034580.1.8",
          "form": "쉬는시간에 친구들이랑 놀던거요 ㅋㅋㅋ",
          "original_form": "쉬는시간에 친구들이랑 놀던거요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100034580.1.9",
          "form": "저도요 ㅋㅋㅋㅋ 특별히 뭘 한것도 아닌데 그런게 그립네요",
          "original_form": "저도요 ㅋㅋㅋㅋ 특별히 뭘 한것도 아닌데 그런게 그립네요",
          "speaker_id": "2",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100034580.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100034580.1.11",
          "form": "어른들이 그때가 젤좋은거랄땐 안 믿었는데",
          "original_form": "어른들이 그때가 젤좋은거랄땐 안 믿었는데",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100034580.1.12",
          "form": "명언이에요",
          "original_form": "명언이에요",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100034580.1.13",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034580.1.14",
          "form": "진짜 그때가 젤좋죠",
          "original_form": "진짜 그때가 젤좋죠",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034580.1.15",
          "form": "야자마저 그리워요",
          "original_form": "야자마저 그리워요",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034580.1.16",
          "form": "야자시간에 몰래 딴짓하고 ㅋㅋㅋㅋ",
          "original_form": "야자시간에 몰래 딴짓하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034580.1.17",
          "form": "아 저는 특성화라 야자는 못해봤어요 ㅋㅋㅋ",
          "original_form": "아 저는 특성화라 야자는 못해봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034580.1.18",
          "form": "나름 로망이었는데 ㅋㅋㅋ",
          "original_form": "나름 로망이었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034580.1.19",
          "form": "아 특성화고등학교셧군요",
          "original_form": "아 특성화고등학교셧군요",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
