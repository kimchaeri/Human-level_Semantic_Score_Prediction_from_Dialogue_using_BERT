      "utterance": [
        {
          "id": "MDRW2100043598.1.1",
          "form": "생선은 없지만 고기가 들어가요!",
          "original_form": "생선은 없지만 고기가 들어가요!",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043598.1.2",
          "form": "앗 고기..!",
          "original_form": "앗 고기..!",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043598.1.3",
          "form": "두 발 짐승 고기인가요?",
          "original_form": "두 발 짐승 고기인가요?",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043598.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네 발 짐승이었어요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네 발 짐승이었어요!",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043598.1.5",
          "form": "어엇 어엇 소고기였나요?",
          "original_form": "어엇 어엇 소고기였나요?",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043598.1.6",
          "form": "삼겹살이었어요!",
          "original_form": "삼겹살이었어요!",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043598.1.7",
          "form": "헉 삼겹살이 들어가는 요리인가요?",
          "original_form": "헉 삼겹살이 들어가는 요리인가요?",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043598.1.8",
          "form": "삼겹살이 들어간 빨갛고 자작한 국물이 있는 요리입니다",
          "original_form": "삼겹살이 들어간 빨갛고 자작한 국물이 있는 요리입니다",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043598.1.9",
          "form": "헐 맛있겠어요",
          "original_form": "헐 맛있겠어요",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043598.1.10",
          "form": "으으으으으으음",
          "original_form": "으으으으으으음",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043598.1.11",
          "form": "쌀이 들어가나요?",
          "original_form": "쌀이 들어가나요?",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043598.1.12",
          "form": "아니요! 주재료는 삼겹살과 또 다른 무엇",
          "original_form": "아니요! 주재료는 삼겹살과 또 다른 무엇",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043598.1.13",
          "form": "이렇게 두 개입니다!",
          "original_form": "이렇게 두 개입니다!",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043598.1.14",
          "form": "음 음 음 음 그렇담 야채가 들어가는 요리인가요?",
          "original_form": "음 음 음 음 그렇담 야채가 들어가는 요리인가요?",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043598.1.15",
          "form": "네 주재료도 야채에 들어간다고 할 수 있고",
          "original_form": "네 주재료도 야채에 들어간다고 할 수 있고",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043598.1.16",
          "form": "파나 양파도 조금 들어가요",
          "original_form": "파나 양파도 조금 들어가요",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043598.1.17",
          "form": "고추장이 들어가나요?",
          "original_form": "고추장이 들어가나요?",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043598.1.18",
          "form": "아마 들아갈 거 같아요..!",
          "original_form": "아마 들아갈 거 같아요..!",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043598.1.19",
          "form": "찜 요리입니다!",
          "original_form": "찜 요리입니다!",
          "speaker_id": "2",
          "time": "20210907 19:47"
        }
      ]
