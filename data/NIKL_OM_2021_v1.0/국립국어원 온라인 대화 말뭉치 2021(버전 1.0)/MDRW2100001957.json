        },
        {
          "id": "MDRW2100001957.1.2",
          "form": "나 폴드3 예약하고시퍼여.... 해도 될까?",
          "original_form": "나 폴드3 예약하고시퍼여.... 해도 될까?",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001957.1.3",
          "form": "겁나 사고싶어 너무 이쁘게 잘 나왔어",
          "original_form": "겁나 사고싶어 너무 이쁘게 잘 나왔어",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001957.1.4",
          "form": "내가 폴드1 사줬자나 사준지 2년도 안됐는데 벌써 새폰 사려고???",
          "original_form": "내가 폴드1 사줬자나 사준지 2년도 안됐는데 벌써 새폰 사려고???",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001957.1.5",
          "form": "이번에 나온거 사면서 보상판매 해준다고 가격 높게 쳐준다고 해서",
          "original_form": "이번에 나온거 사면서 보상판매 해준다고 가격 높게 쳐준다고 해서",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001957.1.6",
          "form": "얼마나 해주는데?",
          "original_form": "얼마나 해주는데?",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001957.1.7",
          "form": "A급이면 78만원해준대여 히히",
          "original_form": "A급이면 78만원해준대여 히히",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001957.1.8",
          "form": "그렇게 사고싶어?",
          "original_form": "그렇게 사고싶어?",
          "speaker_id": "2",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001957.1.9",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001957.1.10",
          "form": "이번에 사면 3년이상 쓰세여",
          "original_form": "이번에 사면 3년이상 쓰세여",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001957.1.11",
          "form": "넹 히히",
          "original_form": "넹 히히",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001957.1.12",
          "form": "이번에 나온 z플립도 겁나 이뻐",
          "original_form": "이번에 나온 z플립도 겁나 이뻐",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001957.1.13",
          "form": "자긴 생각없어?",
          "original_form": "자긴 생각없어?",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001957.1.14",
          "form": "난 지금 쓰는폰 고장나야지 새로살거얌",
          "original_form": "난 지금 쓰는폰 고장나야지 새로살거얌",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001957.1.15",
          "form": "자긴 아주 폰 바꾸는게 취미야 아주",
          "original_form": "자긴 아주 폰 바꾸는게 취미야 아주",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001957.1.16",
          "form": "아니 이번에 사전예약으로 사야",
          "original_form": "아니 이번에 사전예약으로 사야",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001957.1.17",
          "form": "버즈2도 주고",
          "original_form": "버즈2도 주고",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001957.1.18",
          "form": "포인트환급도 되고",
          "original_form": "포인트환급도 되고",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001957.1.19",
          "form": "케이스도 준다구",
          "original_form": "케이스도 준다구",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001957.1.20",
          "form": "아주 새폰 살 생각에 신났네",
          "original_form": "아주 새폰 살 생각에 신났네",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001957.1.21",
          "form": "엉 ㅋㅋ",
          "original_form": "엉 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001957.1.22",
          "form": "내 유일한 취미니깐",
          "original_form": "내 유일한 취미니깐",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001957.1.23",
          "form": "새로나온 따끈따끈한 폰 써보기 히히",
          "original_form": "새로나온 따끈따끈한 폰 써보기 히히",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001957.1.24",
          "form": "그래 내가 특별히 사게 해줄게",
          "original_form": "그래 내가 특별히 사게 해줄게",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001957.1.25",
          "form": "근데 이번에 충전기도 안줘서",
          "original_form": "근데 이번에 충전기도 안줘서",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001957.1.26",
          "form": "충전기도 사야해",
          "original_form": "충전기도 사야해",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001957.1.27",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001957.1.28",
          "form": "사은품 많이 준다며",
          "original_form": "사은품 많이 준다며",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001957.1.29",
          "form": "충전기 안줘??",
          "original_form": "충전기 안줘??",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
