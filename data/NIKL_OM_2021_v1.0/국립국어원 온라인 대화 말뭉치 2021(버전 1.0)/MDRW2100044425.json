      "utterance": [
        {
          "id": "MDRW2100044425.1.1",
          "form": "아아 지금은 잠시 쉬고있어요ㅎㅎ",
          "original_form": "아아 지금은 잠시 쉬고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044425.1.2",
          "form": "결혼 생각 잇으신가요? 비혼 ?",
          "original_form": "결혼 생각 잇으신가요? 비혼 ?",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044425.1.3",
          "form": "아 비혼까진 아닌데",
          "original_form": "아 비혼까진 아닌데",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044425.1.4",
          "form": "꼭 결혼해야한다!! 주의도 아니예요",
          "original_form": "꼭 결혼해야한다!! 주의도 아니예요",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044425.1.5",
          "form": "ㅋㅋㅋ 자만추세요?",
          "original_form": "ㅋㅋㅋ 자만추세요?",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044425.1.6",
          "form": "name1님은 연애중이신가요?",
          "original_form": "&name1&님은 연애중이신가요?",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044425.1.7",
          "form": "헤어진지 얼마안됐어요",
          "original_form": "헤어진지 얼마안됐어요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044425.1.8",
          "form": "아 저두요...",
          "original_form": "아 저두요...",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044425.1.9",
          "form": "ㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044425.1.10",
          "form": "슬퍼용",
          "original_form": "슬퍼용",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044425.1.11",
          "form": "ㅜㅜ 전 이제 그냥 인연이 아니었나보다 해요",
          "original_form": "ㅜㅜ 전 이제 그냥 인연이 아니었나보다 해요",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044425.1.12",
          "form": "얼마나 만나셨었나요",
          "original_form": "얼마나 만나셨었나요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044425.1.13",
          "form": "200일 정도요",
          "original_form": "200일 정도요",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044425.1.14",
          "form": "해어지고 괜찮으셨나요",
          "original_form": "해어지고 괜찮으셨나요",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044425.1.15",
          "form": "아 일이 바쁘기도 했고",
          "original_form": "아 일이 바쁘기도 했고",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044425.1.16",
          "form": "헤어질만한 이유가 있어서",
          "original_form": "헤어질만한 이유가 있어서",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044425.1.17",
          "form": "좀 후회하기도 했지만",
          "original_form": "좀 후회하기도 했지만",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044425.1.18",
          "form": "잘한 선택이었다 생각했어요",
          "original_form": "잘한 선택이었다 생각했어요",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
