      "utterance": [
        {
          "id": "MDRW2100012638.1.1",
          "form": "대가족이신가요?",
          "original_form": "대가족이신가요?",
          "speaker_id": "1",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012638.1.2",
          "form": "그런편이죠?",
          "original_form": "그런편이죠?",
          "speaker_id": "2",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100012638.1.3",
          "form": "몇명 가족이세유?",
          "original_form": "몇명 가족이세유?",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012638.1.4",
          "form": "집은 4명이고",
          "original_form": "집은 4명이고",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012638.1.5",
          "form": "사촌들이 많아요",
          "original_form": "사촌들이 많아요",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012638.1.6",
          "form": "와.. 부럽당... \n전 가족이 없어서 부럽네요.. \n명절 죽적북적하겟네요?",
          "original_form": "와.. 부럽당... \n전 가족이 없어서 부럽네요.. \n명절 죽적북적하겟네요?",
          "speaker_id": "1",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012638.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012638.1.8",
          "form": "시끄럽긴하죠ㅋㅋㅋ",
          "original_form": "시끄럽긴하죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012638.1.9",
          "form": "근데 요즘은 코로나라",
          "original_form": "근데 요즘은 코로나라",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012638.1.10",
          "form": "못모인지 꽤 됐어요",
          "original_form": "못모인지 꽤 됐어요",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012638.1.11",
          "form": "사람 많음 명절 음식하는거로 문제는 없엇어요?",
          "original_form": "사람 많음 명절 음식하는거로 문제는 없엇어요?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012638.1.12",
          "form": "아 아무래도 그렇죠",
          "original_form": "아 아무래도 그렇죠",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012638.1.13",
          "form": "전 하기 싫어서 내뺐어요ㅋㅋㅋ",
          "original_form": "전 하기 싫어서 내뺐어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012638.1.14",
          "form": "아무래도 여자들만 하니까",
          "original_form": "아무래도 여자들만 하니까",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012638.1.15",
          "form": "엄마한테 등짝 스매싱 안담함요? ㅋㅋㅋ",
          "original_form": "엄마한테 등짝 스매싱 안담함요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012638.1.16",
          "form": "공부한다고 카페로 피신했어요",
          "original_form": "공부한다고 카페로 피신했어요",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012638.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012638.1.18",
          "form": "하루종일 밖에 잇는것도 좀 뻘쭘하지먼",
          "original_form": "하루종일 밖에 잇는것도 좀 뻘쭘하지먼",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012638.1.19",
          "form": "하기싫어서요!",
          "original_form": "하기싫어서요!",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012638.1.20",
          "form": "그래도 가족 사랑하시죠? ㅋㅋㅋ",
          "original_form": "그래도 가족 사랑하시죠? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012638.1.21",
          "form": "네 사랑하죠",
          "original_form": "네 사랑하죠",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012638.1.22",
          "form": "ㅋㅋ 코로나 끝나고 다시.가족 모엿음 좋겠어요",
          "original_form": "ㅋㅋ 코로나 끝나고 다시.가족 모엿음 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012638.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012638.1.24",
          "form": "얼굴 안본지 꽤 돼서",
          "original_form": "얼굴 안본지 꽤 돼서",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012638.1.25",
          "form": "한번 보고싶네요",
          "original_form": "한번 보고싶네요",
          "speaker_id": "2",
          "time": "20210824 23:45"
        }
      ]
    }
