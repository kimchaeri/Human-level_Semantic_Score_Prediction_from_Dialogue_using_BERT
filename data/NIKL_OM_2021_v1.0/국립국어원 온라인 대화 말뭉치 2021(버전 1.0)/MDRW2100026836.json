      "utterance": [
        {
          "id": "MDRW2100026836.1.1",
          "form": "맛집여행가고싶네요",
          "original_form": "맛집여행가고싶네요",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026836.1.2",
          "form": "전라도쪽으로요",
          "original_form": "전라도쪽으로요",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026836.1.3",
          "form": "이직 계획있으신가요",
          "original_form": "이직 계획있으신가요",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026836.1.4",
          "form": "직업임돠 떼잉ㅋ",
          "original_form": "직업임돠 떼잉ㅋ",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026836.1.5",
          "form": "이직계획은 없어요",
          "original_form": "이직계획은 없어요",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026836.1.6",
          "form": "요즘 코로나라서 이직하기 힘들더라구요",
          "original_form": "요즘 코로나라서 이직하기 힘들더라구요",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026836.1.7",
          "form": "몇년 일하셧지요?",
          "original_form": "몇년 일하셧지요?",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026836.1.8",
          "form": "지금하는일에 만족하고 계신가요",
          "original_form": "지금하는일에 만족하고 계신가요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.9",
          "form": "업무 스트레스가 큰가요",
          "original_form": "업무 스트레스가 큰가요",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.10",
          "form": "저는 이직 준비합니다",
          "original_form": "저는 이직 준비합니다",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.11",
          "form": "전5년정도 되어가네요",
          "original_form": "전5년정도 되어가네요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.12",
          "form": "오래하셨네요",
          "original_form": "오래하셨네요",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.13",
          "form": "코로나로 일이 힘들어저서",
          "original_form": "코로나로 일이 힘들어저서",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.14",
          "form": "부업생각중이에요",
          "original_form": "부업생각중이에요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.15",
          "form": "주식좀해보려구요",
          "original_form": "주식좀해보려구요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.16",
          "form": "부업좋지요",
          "original_form": "부업좋지요",
          "speaker_id": "2",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.17",
          "form": "이직은 어디로 하시게요",
          "original_form": "이직은 어디로 하시게요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100026836.1.18",
          "form": "부업하시는거있나요",
          "original_form": "부업하시는거있나요",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026836.1.19",
          "form": "아직 계획중인데 자영업 생각합니다",
          "original_form": "아직 계획중인데 자영업 생각합니다",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026836.1.20",
          "form": "자유로운 일을 하려합니다",
          "original_form": "자유로운 일을 하려합니다",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026836.1.21",
          "form": "좋은생각이에요",
          "original_form": "좋은생각이에요",
          "speaker_id": "1",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026836.1.22",
          "form": "이직은 어렵지만",
          "original_form": "이직은 어렵지만",
          "speaker_id": "2",
          "time": "20210902 00:47"
        },
        {
          "id": "MDRW2100026836.1.23",
          "form": "도전해야지요",
          "original_form": "도전해야지요",
          "speaker_id": "2",
          "time": "20210902 00:47"
        }
      ]
