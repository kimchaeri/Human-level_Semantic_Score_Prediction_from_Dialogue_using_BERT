      "utterance": [
        {
          "id": "MDRW2100016049.1.1",
          "form": "키우시는 동물있으신가요",
          "original_form": "키우시는 동물있으신가요",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016049.1.2",
          "form": "저 고양이 키워요",
          "original_form": "저 고양이 키워요",
          "speaker_id": "2",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016049.1.3",
          "form": "좋아하시나요",
          "original_form": "좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016049.1.4",
          "form": "오 고양이 너무좋아하죠",
          "original_form": "오 고양이 너무좋아하죠",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016049.1.5",
          "form": "이름이뭔가쇼",
          "original_form": "이름이뭔가쇼",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016049.1.6",
          "form": "name3에요",
          "original_form": "&name3&에요",
          "speaker_id": "2",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016049.1.7",
          "form": "name3?",
          "original_form": "&name3&?",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016049.1.8",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016049.1.9",
          "form": "name3먹다가 주워왔는데 ㅋㅋㅋㅋ",
          "original_form": "&name3&먹다가 주워왔는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016049.1.10",
          "form": "이름이 딱히 생각안나서 name3라고 지었어요",
          "original_form": "이름이 딱히 생각안나서 &name3&라고 지었어요",
          "speaker_id": "2",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016049.1.11",
          "form": "오.. 신기한발상이다",
          "original_form": "오.. 신기한발상이다",
          "speaker_id": "1",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016049.1.12",
          "form": "그런가요ㅋㅋㅋㅋ",
          "original_form": "그런가요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016049.1.13",
          "form": "name3가좋아하는음식은있나요",
          "original_form": "&name3&가좋아하는음식은있나요",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016049.1.14",
          "form": "아무래도 참치",
          "original_form": "아무래도 참치",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016049.1.15",
          "form": "참치...아므래도 그렇겠죠?",
          "original_form": "참치...아므래도 그렇겠죠?",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016049.1.16",
          "form": "근데 고구마도 잘먹더라구요",
          "original_form": "근데 고구마도 잘먹더라구요",
          "speaker_id": "2",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016049.1.17",
          "form": "고구마요? 고양이가 고구마도 먹어오?",
          "original_form": "고구마요? 고양이가 고구마도 먹어오?",
          "speaker_id": "1",
          "time": "20210826 08:50"
        },
        {
          "id": "MDRW2100016049.1.18",
          "form": "네 ㅋㅋ 신기하죠 저도 그래요",
