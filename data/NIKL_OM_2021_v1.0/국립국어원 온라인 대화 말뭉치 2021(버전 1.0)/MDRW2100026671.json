      "utterance": [
        {
          "id": "MDRW2100026671.1.1",
          "form": "동물은 키우시나요?",
          "original_form": "동물은 키우시나요?",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026671.1.2",
          "form": "아니요 근데 동물 보는거 좋아해요 ㅋㅋ",
          "original_form": "아니요 근데 동물 보는거 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026671.1.3",
          "form": "키우시나요??",
          "original_form": "키우시나요??",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026671.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026671.1.5",
          "form": "저도 생각만요",
          "original_form": "저도 생각만요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026671.1.6",
          "form": "고양이 좋아하세요 강아지 좋아하세요?",
          "original_form": "고양이 좋아하세요 강아지 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026671.1.7",
          "form": "전 고양이 보는건 좋은데 키우면 강아지 키우고 싶어요 ㅋㅋㅋ",
          "original_form": "전 고양이 보는건 좋은데 키우면 강아지 키우고 싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026671.1.8",
          "form": "강아지가 애교가 많죠",
          "original_form": "강아지가 애교가 많죠",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.9",
          "form": "근데 전 고양이파예요 ㅋㅋ",
          "original_form": "근데 전 고양이파예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.10",
          "form": "고양이 키우는 친구 맨날 털 떼기 힘들어서 고생하는걸 봤거든요",
          "original_form": "고양이 키우는 친구 맨날 털 떼기 힘들어서 고생하는걸 봤거든요",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.12",
          "form": "저도 고양이가 성격에는 맞아요",
          "original_form": "저도 고양이가 성격에는 맞아요",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.13",
          "form": "강아지도 털많지만",
          "original_form": "강아지도 털많지만",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.14",
          "form": "모시는게 더 나아보여요",
          "original_form": "모시는게 더 나아보여요",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.15",
          "form": "역시 고양이가",
          "original_form": "역시 고양이가",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.16",
          "form": "근데 고양이 진짜 털 장난아니에요",
          "original_form": "근데 고양이 진짜 털 장난아니에요",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026671.1.17",
          "form": "네 털뿜뿜하더라구요",
          "original_form": "네 털뿜뿜하더라구요",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026671.1.18",
          "form": "검은 옷만 입으면 더",
          "original_form": "검은 옷만 입으면 더",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026671.1.19",
          "form": "근데 귀엽긴 해요 ㅠㅠ",
          "original_form": "근데 귀엽긴 해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026671.1.20",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026671.1.21",
          "form": "길냥이 가끔보면",
          "original_form": "길냥이 가끔보면",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026671.1.22",
          "form": "애교있는 애들있더라구요",
          "original_form": "애교있는 애들있더라구요",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026671.1.23",
          "form": "그럴때 키우고싶어요ㅜ",
          "original_form": "그럴때 키우고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026671.1.24",
          "form": "맞아요 ㅠㅠ 귀엽",
          "original_form": "맞아요 ㅠㅠ 귀엽",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026671.1.25",
          "form": "저도요 ㅠㅠ",
          "original_form": "저도요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:36"
        }
      ]
    }
