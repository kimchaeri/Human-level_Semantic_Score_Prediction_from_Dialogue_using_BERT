        },
        {
          "id": "MDRW2100015109.1.2",
          "form": "아이가 내려져요",
          "original_form": "아이가 내려져요",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015109.1.3",
          "form": "전 잘못해요 ㅋㅋ",
          "original_form": "전 잘못해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015109.1.4",
          "form": "아이가 버리스타 2급요",
          "original_form": "아이가 버리스타 2급요",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015109.1.5",
          "form": "바리스타",
          "original_form": "바리스타",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015109.1.6",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015109.1.7",
          "form": "너무 든든하겠네요",
          "original_form": "너무 든든하겠네요",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015109.1.8",
          "form": "네ㅎㅎ 맛도 좋고요",
          "original_form": "네ㅎㅎ 맛도 좋고요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015109.1.9",
          "form": "초등인데",
          "original_form": "초등인데",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015109.1.10",
          "form": "그럼 원두도 갈고 해서 내리겠네요",
          "original_form": "그럼 원두도 갈고 해서 내리겠네요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015109.1.11",
          "form": "땄어요",
          "original_form": "땄어요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015109.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015109.1.13",
          "form": "오 초등학생도 딸수있군요 대단해요",
          "original_form": "오 초등학생도 딸수있군요 대단해요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015109.1.14",
          "form": "멋있네요.. 그럼 원두도 그 친구가 고르나요?",
          "original_form": "멋있네요.. 그럼 원두도 그 친구가 고르나요?",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015109.1.15",
          "form": "네 나이 제한 없어요",
          "original_form": "네 나이 제한 없어요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015109.1.16",
          "form": "같이 한 아이들중 아리긴한데",
          "original_form": "같이 한 아이들중 아리긴한데",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015109.1.17",
          "form": "중등 고등과 같이요",
          "original_form": "중등 고등과 같이요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015109.1.18",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015109.1.19",
          "form": "원두는 제가요 ㅋㅋ",
          "original_form": "원두는 제가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015109.1.20",
          "form": "ㅋㅋㅋㅋㅋ 원두 어떤거 좋아하시나요",
          "original_form": "ㅋㅋㅋㅋㅋ 원두 어떤거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015109.1.21",
          "form": "쓴거 안좋아해수 살짝 로스팅한거요",
          "original_form": "쓴거 안좋아해수 살짝 로스팅한거요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015109.1.22",
          "form": "전 그냥 그때그때사요",
          "original_form": "전 그냥 그때그때사요",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015109.1.23",
          "form": "제가 바리스타 아니라서 ㅜㅜ",
          "original_form": "제가 바리스타 아니라서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015109.1.24",
          "form": "추천있으세요?",
          "original_form": "추천있으세요?",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015109.1.25",
          "form": "아하 저도 사실 잘 모르는데 그냥 수프리모나 케냐 제일 무난하게 먹는거 같아요 ㅋㅋㅋ",
          "original_form": "아하 저도 사실 잘 모르는데 그냥 수프리모나 케냐 제일 무난하게 먹는거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015109.1.26",
          "form": "산미는 예가체프",
          "original_form": "산미는 예가체프",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015109.1.27",
          "form": "오 저도 그거 먹는데",
          "original_form": "오 저도 그거 먹는데",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015109.1.28",
          "form": "가끔 섞어서 맛을 낸것동ㅅ",
          "original_form": "가끔 섞어서 맛을 낸것동ㅅ",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015109.1.29",
          "form": "저 산미가 있는게 좋아요 ㅎㅎ",
          "original_form": "저 산미가 있는게 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015109.1.30",
          "form": "아 정말요 통했네요",
          "original_form": "아 정말요 통했네요",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015109.1.31",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015109.1.32",
          "form": "아 블렌딩한거요!",
          "original_form": "아 블렌딩한거요!",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015109.1.33",
          "form": "네 브랜딩요 ㅋㅋ",
          "original_form": "네 브랜딩요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015109.1.34",
          "form": "전 용어는 기억못해요 ㅜㅜ",
          "original_form": "전 용어는 기억못해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015109.1.35",
          "form": "산미있는건 신선한거는 진짜 맛있죵",
          "original_form": "산미있는건 신선한거는 진짜 맛있죵",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015109.1.36",
          "form": "원두 블랜딩후맛 정보 보고 사요 ㅎㅎ",
          "original_form": "원두 블랜딩후맛 정보 보고 사요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015109.1.37",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015109.1.38",
          "form": "오래되면 쩐내나요 ㅋㅋ",
          "original_form": "오래되면 쩐내나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015109.1.39",
          "form": "그쵸 쾌쾌한..",
          "original_form": "그쵸 쾌쾌한..",
          "speaker_id": "1",
