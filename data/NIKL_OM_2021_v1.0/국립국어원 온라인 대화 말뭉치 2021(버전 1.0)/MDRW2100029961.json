        {
          "id": "MDRW2100029961.1.2",
          "form": "이과셨어요",
          "original_form": "이과셨어요",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029961.1.3",
          "form": "문과셨어요",
          "original_form": "문과셨어요",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100029961.1.4",
          "form": "저 뭐였을거같아요?",
          "original_form": "저 뭐였을거같아요?",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029961.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029961.1.6",
          "form": "문과가 약하다고 하셨으니",
          "original_form": "문과가 약하다고 하셨으니",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029961.1.7",
          "form": "자연스럽게",
          "original_form": "자연스럽게",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029961.1.8",
          "form": "소거법으로",
          "original_form": "소거법으로",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029961.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ마자요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ마자요",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029961.1.10",
          "form": "이과",
          "original_form": "이과",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029961.1.11",
          "form": "근데 다 까먹었어요",
          "original_form": "근데 다 까먹었어요",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029961.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100029961.1.13",
          "form": "근데 사실 그대로 기억하는 사람이",
          "original_form": "근데 사실 그대로 기억하는 사람이",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100029961.1.14",
          "form": "더 적지 않을까요",
          "original_form": "더 적지 않을까요",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100029961.1.15",
          "form": "그죸ㅋㅋㅋㅋㅋ시그마...함수.. 수학 재밌긴했는데..",
          "original_form": "그죸ㅋㅋㅋㅋㅋ시그마...함수.. 수학 재밌긴했는데..",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100029961.1.16",
          "form": "잘하지는 못했는데",
          "original_form": "잘하지는 못했는데",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100029961.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100029961.1.18",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100029961.1.19",
          "form": "항상 흥미랑 능력이랑 비례했으면",
          "original_form": "항상 흥미랑 능력이랑 비례했으면",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100029961.1.20",
          "form": "좋았으려만",
          "original_form": "좋았으려만",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100029961.1.21",
          "form": "아 맞아요..",
          "original_form": "아 맞아요..",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100029961.1.22",
          "form": "좋아하는걸 잘하면 얼마나 좋을까요ㅠㅠ",
          "original_form": "좋아하는걸 잘하면 얼마나 좋을까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100029961.1.23",
          "form": "ㅠ...",
          "original_form": "ㅠ...",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100029961.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ동아리같은거 한 적 있어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ동아리같은거 한 적 있어요?",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100029961.1.25",
          "form": "그 뭐지",
          "original_form": "그 뭐지",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100029961.1.26",
          "form": "고딩때는",
          "original_form": "고딩때는",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100029961.1.27",
          "form": "역사 동아리했고",
          "original_form": "역사 동아리했고",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100029961.1.28",
          "form": "대학생때는",
          "original_form": "대학생때는",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100029961.1.29",
          "form": "영화 시나리오 동아리 했었어요",
          "original_form": "영화 시나리오 동아리 했었어요",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100029961.1.30",
          "form": "우와 영화시나리오..!!!",
          "original_form": "우와 영화시나리오..!!!",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100029961.1.31",
          "form": "신기하네요 진짜",
          "original_form": "신기하네요 진짜",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100029961.1.32",
          "form": "완전 쌉 문과!!!",
          "original_form": "완전 쌉 문과!!!",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100029961.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100029961.1.34",
          "form": "아니 새삼 보니까",
          "original_form": "아니 새삼 보니까",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100029961.1.35",
          "form": "완전 문과긴 하네요;;",
          "original_form": "완전 문과긴 하네요;;",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100029961.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100029961.1.37",
          "form": "글자와 떼려야 뗄 수 없는 사이네요",
          "original_form": "글자와 떼려야 뗄 수 없는 사이네요",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100029961.1.38",
          "form": "완전 신기ㅋㅋㅋㅋㅋ책 언제쓰세요",
          "original_form": "완전 신기ㅋㅋㅋㅋㅋ책 언제쓰세요",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
