      "utterance": [
        {
          "id": "MDRW2100014609.1.1",
          "form": "나 오늘 바닐라 라떼 시켰어",
          "original_form": "나 오늘 바닐라 라떼 시켰어",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014609.1.2",
          "form": "하이하이",
          "original_form": "하이하이",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014609.1.3",
          "form": "나도 먹을래",
          "original_form": "나도 먹을래",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014609.1.4",
          "form": "나는 마카롱 사려다가 안삼",
          "original_form": "나는 마카롱 사려다가 안삼",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014609.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014609.1.6",
          "form": "오 무슨맛???",
          "original_form": "오 무슨맛???",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014609.1.7",
          "form": "안샀음...",
          "original_form": "안샀음...",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014609.1.8",
          "form": "무슨 맛 시키려고 했나요~",
          "original_form": "무슨 맛 시키려고 했나요~",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014609.1.9",
          "form": "가서 보려고 했는데 아깝~",
          "original_form": "가서 보려고 했는데 아깝~",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014609.1.10",
          "form": "커피 맛있어 냠냠",
          "original_form": "커피 맛있어 냠냠",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014609.1.11",
          "form": "저녁은 뭐 드심?",
          "original_form": "저녁은 뭐 드심?",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014609.1.12",
          "form": "나는 쭈꾸미",
          "original_form": "나는 쭈꾸미",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014609.1.13",
          "form": "아직 안먹음",
          "original_form": "아직 안먹음",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014609.1.14",
          "form": "저녁을 늦게먹어서",
          "original_form": "저녁을 늦게먹어서",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014609.1.15",
          "form": "점심?",
          "original_form": "점심?",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014609.1.16",
          "form": "아침 겸 점심!",
          "original_form": "아침 겸 점심!",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014609.1.17",
          "form": "뭐 먹었어",
          "original_form": "뭐 먹었어",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014609.1.18",
          "form": "참치캔에 김치",
          "original_form": "참치캔에 김치",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014609.1.19",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014609.1.20",
          "form": "삼겹살 시켜먹을까 고민하다가",
          "original_form": "삼겹살 시켜먹을까 고민하다가",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014609.1.21",
          "form": "내일 언제 근장 들어올지 몰라서 긴축 정책 중 ㅋㅋㅋ",
          "original_form": "내일 언제 근장 들어올지 몰라서 긴축 정책 중 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014609.1.22",
          "form": "복자식당 가고 싶네",
          "original_form": "복자식당 가고 싶네",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014609.1.23",
          "form": "아 내일인가? 오",
          "original_form": "아 내일인가? 오",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014609.1.24",
          "form": "내일! 근데 내일은 나갈꺼라 뭐 사먹지 ㅠ",
          "original_form": "내일! 근데 내일은 나갈꺼라 뭐 사먹지 ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014609.1.25",
          "form": "에그드랍",
          "original_form": "에그드랍",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014609.1.26",
          "form": "샌드리아",
          "original_form": "샌드리아",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014609.1.27",
          "form": "피시방 밥",
          "original_form": "피시방 밥",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014609.1.28",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014609.1.29",
          "form": "나는야 밥순이 밥을 먹어야 한다!",
          "original_form": "나는야 밥순이 밥을 먹어야 한다!",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014609.1.30",
          "form": "피시방 대패삼겹살 덮밥 먹기",
          "original_form": "피시방 대패삼겹살 덮밥 먹기",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014609.1.31",
          "form": "피시방 무서운걸~ 그래도 피시방 음식 먹고싶다 메뉴 많아졌던데",
          "original_form": "피시방 무서운걸~ 그래도 피시방 음식 먹고싶다 메뉴 많아졌던데",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014609.1.32",
          "form": "막 수도권은 피시방 음식도 배달로 파나봐",
          "original_form": "막 수도권은 피시방 음식도 배달로 파나봐",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014609.1.33",
          "form": "대박 좋겠다",
          "original_form": "대박 좋겠다",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014609.1.34",
          "form": "피카츄 돈까스 먹고싶음",
          "original_form": "피카츄 돈까스 먹고싶음",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014609.1.35",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014609.1.36",
          "form": "롤링파스타 필라프 먹엉",
          "original_form": "롤링파스타 필라프 먹엉",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014609.1.37",
          "form": "이승 나오면 이야기해봐야지",
          "original_form": "이승 나오면 이야기해봐야지",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014609.1.38",
          "form": "피카츄 돈까스 맛있어",
          "original_form": "피카츄 돈까스 맛있어",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014609.1.39",
          "form": "추억의 맛",
          "original_form": "추억의 맛",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014609.1.40",
          "form": "오봉이 집 앞 분식집에서 팔던데",
          "original_form": "오봉이 집 앞 분식집에서 팔던데",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014609.1.41",
          "form": "지금 저녁 먹는다!",
          "original_form": "지금 저녁 먹는다!",
          "speaker_id": "1",
          "time": "20210825 21:29"
        }
      ]
    }
