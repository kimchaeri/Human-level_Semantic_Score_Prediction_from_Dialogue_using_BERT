      "utterance": [
        {
          "id": "MDRW2100022677.1.1",
          "form": "아이들 책은 어디서 사시나요,",
          "original_form": "아이들 책은 어디서 사시나요,",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022677.1.2",
          "form": "전 직접 사거나 인터넷으로요",
          "original_form": "전 직접 사거나 인터넷으로요",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022677.1.3",
          "form": "단행본 사주시나요?",
          "original_form": "단행본 사주시나요?",
          "speaker_id": "2",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100022677.1.4",
          "form": "네 전집은 잘 안보게되네요",
          "original_form": "네 전집은 잘 안보게되네요",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022677.1.5",
          "form": "최근 구매하신 단행본 있으실까요?",
          "original_form": "최근 구매하신 단행본 있으실까요?",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022677.1.6",
          "form": "와이책 갈때마다 한권씩사고",
          "original_form": "와이책 갈때마다 한권씩사고",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022677.1.7",
          "form": "대부분 만화책",
          "original_form": "대부분 만화책",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022677.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022677.1.9",
          "form": "아하 서점을 종종 가시나봐요!",
          "original_form": "아하 서점을 종종 가시나봐요!",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022677.1.10",
          "form": "네 제가 좋아서ㅋ",
          "original_form": "네 제가 좋아서ㅋ",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022677.1.11",
          "form": "서점가는건 아이들과 좋은 여가시간응 보낼 수 있겠어요",
          "original_form": "서점가는건 아이들과 좋은 여가시간응 보낼 수 있겠어요",
          "speaker_id": "2",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022677.1.12",
          "form": "네 교보문고 좋더라구요",
          "original_form": "네 교보문고 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100022677.1.13",
          "form": "시간보내기 꿀 ㅎ",
          "original_form": "시간보내기 꿀 ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100022677.1.14",
          "form": "가시면 아이들 책 한권씩 사주시구요?^^",
          "original_form": "가시면 아이들 책 한권씩 사주시구요?^^",
          "speaker_id": "2",
          "time": "20210830 05:38"
        },
        {
          "id": "MDRW2100022677.1.15",
          "form": "네 책 사는걸 좋아하네요",
          "original_form": "네 책 사는걸 좋아하네요",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100022677.1.16",
          "form": "읽지는않고ㅠ",
          "original_form": "읽지는않고ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100022677.1.17",
          "form": "그래도 서점 가주시는거 넘 좋을것같아요^^",
