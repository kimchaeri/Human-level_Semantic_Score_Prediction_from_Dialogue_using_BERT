      "utterance": [
        {
          "id": "MDRW2100027069.1.1",
          "form": "어제 비가 와서 그런지 많이 쌀쌀하네용",
          "original_form": "어제 비가 와서 그런지 많이 쌀쌀하네용",
          "speaker_id": "1",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100027069.1.2",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027069.1.3",
          "form": "그리고 왠일로 햇빛이 보여요 ㅠㅠ 행복",
          "original_form": "그리고 왠일로 햇빛이 보여요 ㅠㅠ 행복",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027069.1.4",
          "form": "오 ㅠㅠ 부러워요",
          "original_form": "오 ㅠㅠ 부러워요",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027069.1.5",
          "form": "여긴 햇빛은 안 나네요...",
          "original_form": "여긴 햇빛은 안 나네요...",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027069.1.6",
          "form": "이제 비 그만 왔으면 좋겠어요...",
          "original_form": "이제 비 그만 왔으면 좋겠어요...",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027069.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027069.1.8",
          "form": "다음주에도 또 비소식있던데 그만좀 ㅠㅠ",
          "original_form": "다음주에도 또 비소식있던데 그만좀 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027069.1.9",
          "form": "내일 아침에도 비가 온다네요 ㅠㅠ",
          "original_form": "내일 아침에도 비가 온다네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027069.1.10",
          "form": "무슨 계절을 제일 좋아하세요??",
          "original_form": "무슨 계절을 제일 좋아하세요??",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027069.1.11",
          "form": "전 가을이요!",
          "original_form": "전 가을이요!",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027069.1.12",
          "form": "하지만 이런 장마가을은 썩..좋진않지만 ㅠㅜ",
          "original_form": "하지만 이런 장마가을은 썩..좋진않지만 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027069.1.13",
          "form": "맞아요 장마만 없으면 너무 좋을텐데...",
          "original_form": "맞아요 장마만 없으면 너무 좋을텐데...",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027069.1.14",
          "form": "저는 겨울이 제일 좋아요 ㅎㅎ",
          "original_form": "저는 겨울이 제일 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027069.1.15",
          "form": "겨울도 나쁘지 않죠!",
          "original_form": "겨울도 나쁘지 않죠!",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027069.1.16",
          "form": "눈올때 하늘보는 낭만..",
          "original_form": "눈올때 하늘보는 낭만..",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027069.1.17",
          "form": "맞아요 아침에 눈 많이 왔을 때 발자국 남기는 것도 재밌죠 ㅋㅋ",
          "original_form": "맞아요 아침에 눈 많이 왔을 때 발자국 남기는 것도 재밌죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027069.1.18",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027069.1.19",
          "form": "뽀득뽀득 소리도 좋아여 ㅎㅎ",
          "original_form": "뽀득뽀득 소리도 좋아여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:02"
        },
        {
          "id": "MDRW2100027069.1.20",
          "form": "크으 맞아요 ㅠㅠ",
          "original_form": "크으 맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027069.1.21",
          "form": "겨울에 먹는 음식도 좋아용",
          "original_form": "겨울에 먹는 음식도 좋아용",
          "speaker_id": "1",
