      "utterance": [
        {
          "id": "MDRW2100015015.1.1",
          "form": "엄마 연극본적있어?",
          "original_form": "엄마 연극본적있어?",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015015.1.2",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015015.1.3",
          "form": "무슨연극?",
          "original_form": "무슨연극?",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015015.1.4",
          "form": "햄릿같은거 옛날에 봐서 기억이 잘안나",
          "original_form": "햄릿같은거 옛날에 봐서 기억이 잘안나",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015015.1.5",
          "form": "코로나전에 소극장연극 보러갔는데 좋더라",
          "original_form": "코로나전에 소극장연극 보러갔는데 좋더라",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015015.1.6",
          "form": "그래? 좋았겠네",
          "original_form": "그래? 좋았겠네",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015015.1.7",
          "form": "응 코로나끝나면 같이가자",
          "original_form": "응 코로나끝나면 같이가자",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015015.1.8",
          "form": "무슨연극이었는데?",
          "original_form": "무슨연극이었는데?",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015015.1.9",
          "form": "그때는 공포물이었어",
          "original_form": "그때는 공포물이었어",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015015.1.10",
          "form": "난 공포 싫은데",
          "original_form": "난 공포 싫은데",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015015.1.11",
          "form": "공포말고 다른거보면되지",
          "original_form": "공포말고 다른거보면되지",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015015.1.12",
          "form": "뮤지컬도 재밌겠더라",
          "original_form": "뮤지컬도 재밌겠더라",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015015.1.13",
          "form": "맞아 뮤지컬도 한번 보고싶어",
          "original_form": "맞아 뮤지컬도 한번 보고싶어",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015015.1.14",
          "form": "그것도 팬데믹 끝나면 가자",
          "original_form": "그것도 팬데믹 끝나면 가자",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015015.1.15",
          "form": "뮤지컬은 호러장르 없나?",
          "original_form": "뮤지컬은 호러장르 없나?",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015015.1.16",
          "form": "호러싫다니까",
          "original_form": "호러싫다니까",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015015.1.17",
          "form": "엄만 눈감고 손잡아줘",
          "original_form": "엄만 눈감고 손잡아줘",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015015.1.18",
          "form": "그럼 돈아깝잖아",
          "original_form": "그럼 돈아깝잖아",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015015.1.19",
          "form": "소리만들어도 재밌을걸 뮤지컬이라",
          "original_form": "소리만들어도 재밌을걸 뮤지컬이라",
          "speaker_id": "1",
          "time": "20210825 21:39"
        }
      ]
    }
