      "utterance": [
        {
          "id": "MDRW2100002308.1.1",
          "form": "집안일 중 가장 귀찮은 거 있나요?",
          "original_form": "집안일 중 가장 귀찮은 거 있나요?",
          "speaker_id": "1",
          "time": "20210820 09:12"
        },
        {
          "id": "MDRW2100002308.1.2",
          "form": "저는 빨래널기요.",
          "original_form": "저는 빨래널기요.",
          "speaker_id": "1",
          "time": "20210820 09:12"
        },
        {
          "id": "MDRW2100002308.1.3",
          "form": "저는 음 헐 저도!! 빨래 널고 빨개 개키는게 너무 귀찮아요ㅠ",
          "original_form": "저는 음 헐 저도!! 빨래 널고 빨개 개키는게 너무 귀찮아요ㅠ",
          "speaker_id": "2",
          "time": "20210820 09:12"
        },
        {
          "id": "MDRW2100002308.1.4",
          "form": "빨래야 세탁기가 알아서 해주는거지만 너는건 제가 해야하니까요ㅠ",
          "original_form": "빨래야 세탁기가 알아서 해주는거지만 너는건 제가 해야하니까요ㅠ",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100002308.1.5",
          "form": "맞아요 하나하나 집어서 털고..",
          "original_form": "맞아요 하나하나 집어서 털고..",
          "speaker_id": "1",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100002308.1.6",
          "form": "탈탈 털어서 하나하나 널어야 하는게 너무 귀찮아요",
          "original_form": "탈탈 털어서 하나하나 널어야 하는게 너무 귀찮아요",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100002308.1.7",
          "form": "생각만 해도 너무 귀찮아요ㅋㅋ",
          "original_form": "생각만 해도 너무 귀찮아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100002308.1.8",
          "form": "특히 건조대에 널고 걷을 때 너무 귀찮아서 미룬적도 많아요",
          "original_form": "특히 건조대에 널고 걷을 때 너무 귀찮아서 미룬적도 많아요",
          "speaker_id": "1",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100002308.1.9",
          "form": "특히 양말 이런거.. 너무 싫어요ㅠ",
          "original_form": "특히 양말 이런거.. 너무 싫어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100002308.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그래서 그냥 거기서 찾아입고 했던적이 있어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그래서 그냥 거기서 찾아입고 했던적이 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:14"
        },
        {
          "id": "MDRW2100002308.1.11",
          "form": "아ㅋㅋ저도요 그냥 널어놓은 채로 쓰고 다쓰면 또 빨래해서 널고",
          "original_form": "아ㅋㅋ저도요 그냥 널어놓은 채로 쓰고 다쓰면 또 빨래해서 널고",
          "speaker_id": "1",
          "time": "20210820 09:14"
        },
        {
          "id": "MDRW2100002308.1.12",
          "form": "그중에서도 가장 귀찮은건 아무래도 양말..",
          "original_form": "그중에서도 가장 귀찮은건 아무래도 양말..",
          "speaker_id": "1",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002308.1.13",
          "form": "가끔 양말 짝 안맞으면..",
          "original_form": "가끔 양말 짝 안맞으면..",
          "speaker_id": "1",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002308.1.14",
          "form": "그대로 방치하게되지않나요",
          "original_form": "그대로 방치하게되지않나요",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002308.1.15",
          "form": "사람사는거 다 똑같네요 하하\r\n양말.. 그 작은게 많기도 많아ㅠ 저 양말 널기 귀찮아서 그냥 바닥에 널부러트려  놓은적도 있어요 ㅎㅎ...",
          "original_form": "사람사는거 다 똑같네요 하하\r\n양말.. 그 작은게 많기도 많아ㅠ 저 양말 널기 귀찮아서 그냥 바닥에 널부러트려  놓은적도 있어요 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002308.1.16",
          "form": "헐 맞아요 짝 안맞으면 그때부터 짝 나올때까지 놔두고ㅠ",
          "original_form": "헐 맞아요 짝 안맞으면 그때부터 짝 나올때까지 놔두고ㅠ",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002308.1.17",
          "form": "이상하게 양말은 한짝이 사라지는 경우가 많고ㅋㅋ",
          "original_form": "이상하게 양말은 한짝이 사라지는 경우가 많고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002308.1.18",
          "form": "어디 있겠지 하는데 없고",
          "original_form": "어디 있겠지 하는데 없고",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002308.1.19",
          "form": "결국엔 안나오는 경우도 있고",
          "original_form": "결국엔 안나오는 경우도 있고",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002308.1.20",
          "form": "왜그런지 모르겠네요 ㅋㅋㅋㅋ 왜 사라지지???",
          "original_form": "왜그런지 모르겠네요 ㅋㅋㅋㅋ 왜 사라지지???",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002308.1.21",
          "form": "결국 안맞는 짝끼리 뭉쳐놓고ㅋㅋㅋ",
          "original_form": "결국 안맞는 짝끼리 뭉쳐놓고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002308.1.22",
          "form": "진짜요ㅋㅋㅋㅋ 분명 같이 빨았는데",
          "original_form": "진짜요ㅋㅋㅋㅋ 분명 같이 빨았는데",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002308.1.23",
          "form": "꼭 한짝씩 사라져",
          "original_form": "꼭 한짝씩 사라져",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002308.1.24",
          "form": "정말 빨래의 매직입니다 ㅋㅋㅋㅋ",
          "original_form": "정말 빨래의 매직입니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:18"
        },
        {
          "id": "MDRW2100002308.1.25",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210820 09:19"
        },
        {
          "id": "MDRW2100002308.1.26",
          "form": "양말... 언젠가는 꼭 짝이 맞길",
          "original_form": "양말... 언젠가는 꼭 짝이 맞길",
