      "utterance": [
        {
          "id": "MDRW2100042525.1.1",
          "form": "역시 구글 이상하게 쳐도 알아보네요 ㅋㅋㅋㅋㅋ",
          "original_form": "역시 구글 이상하게 쳐도 알아보네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042525.1.2",
          "form": "묘묙을 파는데요?????ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "묘묙을 파는데요?????ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042525.1.3",
          "form": "레이니어였구나...ㅋㅋㅋ",
          "original_form": "레이니어였구나...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042525.1.4",
          "form": "색두 ㅋㅋㅋㅋ 체리안같아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "색두 ㅋㅋㅋㅋ 체리안같아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042525.1.5",
          "form": "색깔 아닌거 같져",
          "original_form": "색깔 아닌거 같져",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042525.1.6",
          "form": "이키로에 51000원해욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이키로에 51000원해욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042525.1.7",
          "form": "저도 사먹을때 과연 이게 맛있을까 하고 먹었는데",
          "original_form": "저도 사먹을때 과연 이게 맛있을까 하고 먹었는데",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042525.1.8",
          "form": "오백그람 샀어욬ㅋㅋㅋ",
          "original_form": "오백그람 샀어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042525.1.9",
          "form": "보통체리보다 달긴 하더라고요 달달한 체리였어요",
          "original_form": "보통체리보다 달긴 하더라고요 달달한 체리였어요",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042525.1.10",
          "form": "작은 그람수가없네요 ㅠ",
          "original_form": "작은 그람수가없네요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042525.1.11",
          "form": "저것고 내년을 기약..",
          "original_form": "저것고 내년을 기약..",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042525.1.12",
          "form": "이마트에서 팔았었는데!!",
          "original_form": "이마트에서 팔았었는데!!",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042525.1.13",
          "form": "이게 철이 있대요",
          "original_form": "이게 철이 있대요",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042525.1.14",
          "form": "기록합니다",
          "original_form": "기록합니다",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042525.1.15",
          "form": "언제래요??",
          "original_form": "언제래요??",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042525.1.16",
          "form": "ㅋㅋㅋㅋㅋ 이건 기록까지 하실 필요 없으실듯여!!",
          "original_form": "ㅋㅋㅋㅋㅋ 이건 기록까지 하실 필요 없으실듯여!!",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042525.1.17",
          "form": "그냥 체리가 훨 맛있어서",
          "original_form": "그냥 체리가 훨 맛있어서",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042525.1.18",
          "form": "사드시지 말라고 하려 했어요 ㅋㅋㅋㅋ",
          "original_form": "사드시지 말라고 하려 했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042525.1.19",
          "form": "그냥 체리중에 맛난걸 먹고 싶어요!",
          "original_form": "그냥 체리중에 맛난걸 먹고 싶어요!",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042525.1.20",
          "form": "아하..맛이없는거였군요",
          "original_form": "아하..맛이없는거였군요",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042525.1.21",
          "form": "달긴 한데 그가격 주고 사먹을 애가 아니었어요",
          "original_form": "달긴 한데 그가격 주고 사먹을 애가 아니었어요",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042525.1.22",
          "form": "단거 좋아하시면 모르겠지만여!!",
          "original_form": "단거 좋아하시면 모르겠지만여!!",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042525.1.23",
          "form": "저 상품평에 홀려서 먹었거든요 ㅋㅋㅋㅋ",
          "original_form": "저 상품평에 홀려서 먹었거든요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042525.1.24",
          "form": "아 저도 자주 상품평에 홀려서 사욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 저도 자주 상품평에 홀려서 사욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042525.1.25",
          "form": "그리고 실망도 자주하고 ㅠ",
          "original_form": "그리고 실망도 자주하고 ㅠ",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042525.1.26",
          "form": "못보셨을거같아서 ㅋㅋㅋㅋ",
          "original_form": "못보셨을거같아서 ㅋㅋㅋㅋ",
