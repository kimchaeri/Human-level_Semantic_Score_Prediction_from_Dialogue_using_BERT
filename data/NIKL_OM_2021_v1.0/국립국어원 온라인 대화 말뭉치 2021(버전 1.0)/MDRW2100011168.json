      "utterance": [
        {
          "id": "MDRW2100011168.1.1",
          "form": "안녕하세요 기상예보 보셨나요?",
          "original_form": "안녕하세요 기상예보 보셨나요?",
          "speaker_id": "1",
          "time": "20210824 02:08"
        },
        {
          "id": "MDRW2100011168.1.2",
          "form": "아니요 님은  보셧어요?",
          "original_form": "아니요 님은  보셧어요?",
          "speaker_id": "2",
          "time": "20210824 02:09"
        },
        {
          "id": "MDRW2100011168.1.3",
          "form": "곧 태풍이 온다네요",
          "original_form": "곧 태풍이 온다네요",
          "speaker_id": "1",
          "time": "20210824 02:09"
        },
        {
          "id": "MDRW2100011168.1.4",
          "form": "아 맞다 며칠전에 뉴스에서 봤는데",
          "original_form": "아 맞다 며칠전에 뉴스에서 봤는데",
          "speaker_id": "2",
          "time": "20210824 02:09"
        },
        {
          "id": "MDRW2100011168.1.5",
          "form": "태풍이름이 오마이스예요",
          "original_form": "태풍이름이 오마이스예요",
          "speaker_id": "1",
          "time": "20210824 02:10"
        },
        {
          "id": "MDRW2100011168.1.6",
          "form": "지금  울릉도 쪽으로 가고있대요",
          "original_form": "지금  울릉도 쪽으로 가고있대요",
          "speaker_id": "2",
          "time": "20210824 02:11"
        },
        {
          "id": "MDRW2100011168.1.7",
          "form": "걱정이네요 피해가 없어야할텐데",
          "original_form": "걱정이네요 피해가 없어야할텐데",
          "speaker_id": "1",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100011168.1.8",
          "form": "아무일없이 지나가길",
          "original_form": "아무일없이 지나가길",
          "speaker_id": "2",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100011168.1.9",
          "form": "지금은 비가 엄청 오네요",
          "original_form": "지금은 비가 엄청 오네요",
          "speaker_id": "1",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100011168.1.10",
          "form": "더위도 간거같아요",
          "original_form": "더위도 간거같아요",
          "speaker_id": "2",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100011168.1.11",
          "form": "새벽되면 엄청 시원해요",
          "original_form": "새벽되면 엄청 시원해요",
          "speaker_id": "1",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100011168.1.12",
          "form": "맞아요 에어컨 튼거같죠",
          "original_form": "맞아요 에어컨 튼거같죠",
          "speaker_id": "2",
          "time": "20210824 02:14"
        },
        {
          "id": "MDRW2100011168.1.13",
          "form": "감기안걸리게 조심하세요~",
          "original_form": "감기안걸리게 조심하세요~",
          "speaker_id": "1",
