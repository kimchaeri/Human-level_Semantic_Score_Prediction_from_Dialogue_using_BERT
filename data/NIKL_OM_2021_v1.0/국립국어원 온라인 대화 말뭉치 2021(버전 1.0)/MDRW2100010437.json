      "utterance": [
        {
          "id": "MDRW2100010437.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210823 18:10"
        },
        {
          "id": "MDRW2100010437.1.2",
          "form": "치킨조아해요",
          "original_form": "치킨조아해요",
          "speaker_id": "2",
          "time": "20210823 18:50"
        },
        {
          "id": "MDRW2100010437.1.3",
          "form": "치킨 너무 좋죠 어디 치킨 가장 좋아하세요?",
          "original_form": "치킨 너무 좋죠 어디 치킨 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010437.1.4",
          "form": "BHC치킨/KFC치킨맛있어요",
          "original_form": "BHC치킨/KFC치킨맛있어요",
          "speaker_id": "2",
          "time": "20210824 07:59"
        },
        {
          "id": "MDRW2100010437.1.5",
          "form": "kfc 치킨 먹어보고 싶은데 주변에 없어서 아쉬워요ㅠㅠ\r\n살짝 매운 맛 나고 바삭한 거 맞나요?",
          "original_form": "kfc 치킨 먹어보고 싶은데 주변에 없어서 아쉬워요ㅠㅠ\r\n살짝 매운 맛 나고 바삭한 거 맞나요?",
          "speaker_id": "1",
          "time": "20210824 08:06"
        },
        {
          "id": "MDRW2100010437.1.6",
          "form": "소스때문에 그럴꺼에요",
          "original_form": "소스때문에 그럴꺼에요",
          "speaker_id": "2",
          "time": "20210824 08:10"
        },
        {
          "id": "MDRW2100010437.1.7",
          "form": "오늘 마침 생각 난 김에 먹어 봐야겠어요",
          "original_form": "오늘 마침 생각 난 김에 먹어 봐야겠어요",
          "speaker_id": "2",
          "time": "20210824 08:10"
        },
        {
          "id": "MDRW2100010437.1.8",
          "form": "매운 맛이 아예 안 나나요?",
          "original_form": "매운 맛이 아예 안 나나요?",
          "speaker_id": "1",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100010437.1.9",
          "form": "소스뿌려먹는것중에 매운것이 있지 그냥 일반 햄버거에는 그리 맵지는않아요",
          "original_form": "소스뿌려먹는것중에 매운것이 있지 그냥 일반 햄버거에는 그리 맵지는않아요",
          "speaker_id": "2",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100010437.1.10",
          "form": "어제 kfc먹었는뎁,,,,,,",
          "original_form": "어제 kfc먹었는뎁,,,,,,",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100010437.1.11",
          "form": "햄버거맛있게먹었어요!!",
          "original_form": "햄버거맛있게먹었어요!!",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100010437.1.12",
          "form": "오늘도 먹어야겠답",
          "original_form": "오늘도 먹어야겠답",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100010437.1.13",
          "form": "햄버거 엄청 좋아하시나봐요ㅎㅎ",
          "original_form": "햄버거 엄청 좋아하시나봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100010437.1.14",
          "form": "전 맘스터치 싸이버거도 매워서 비슷하게 매울 줄 알았어요",
          "original_form": "전 맘스터치 싸이버거도 매워서 비슷하게 매울 줄 알았어요",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100010437.1.15",
          "form": "맘스터치랑 싸이버거도 좋아해요!!!",
          "original_form": "맘스터치랑 싸이버거도 좋아해요!!!",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100010437.1.16",
          "form": "맘터 먹고 다른 햄버거는 못 먹겠어요ㅠ",
          "original_form": "맘터 먹고 다른 햄버거는 못 먹겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100010437.1.17",
          "form": "그건 맞아요ㅋㅋㅋ",
          "original_form": "그건 맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100010437.1.18",
          "form": "오늘 저녁에 먹어야겠어요",
          "original_form": "오늘 저녁에 먹어야겠어요",
