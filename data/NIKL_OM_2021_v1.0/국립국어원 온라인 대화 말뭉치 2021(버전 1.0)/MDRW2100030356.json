      "utterance": [
        {
          "id": "MDRW2100030356.1.1",
          "form": "쇼핑좋아하시나요?",
          "original_form": "쇼핑좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030356.1.2",
          "form": "음 어떤거 살지 찾아보는걸 좀 귀찮아해요 ㅋㅋ",
          "original_form": "음 어떤거 살지 찾아보는걸 좀 귀찮아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030356.1.3",
          "form": "쇼핑좋아하시나요?",
          "original_form": "쇼핑좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030356.1.4",
          "form": "저도 귀찮아해요ㅜㅜ 물건보고 바로 사는걸 더 좋아해요ㅎㅎ",
          "original_form": "저도 귀찮아해요ㅜㅜ 물건보고 바로 사는걸 더 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030356.1.5",
          "form": "그쵸 ㅋㅋ 인터넷에서 찾는것도 한세월이구 ㅜ",
          "original_form": "그쵸 ㅋㅋ 인터넷에서 찾는것도 한세월이구 ㅜ",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030356.1.6",
          "form": "그냥 보고 딱 사는게 편하더랴구요",
          "original_form": "그냥 보고 딱 사는게 편하더랴구요",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030356.1.7",
          "form": "옷 같은건 특히 그런것같아요ㅎㅎ",
          "original_form": "옷 같은건 특히 그런것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030356.1.8",
          "form": "공산품은 인터넷이 싸니까 이제는 인쇼하게 되더라구요",
          "original_form": "공산품은 인터넷이 싸니까 이제는 인쇼하게 되더라구요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030356.1.9",
          "form": "맞아요 사실 인터넷이 다 더 싸니까",
          "original_form": "맞아요 사실 인터넷이 다 더 싸니까",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030356.1.10",
          "form": "인터넷으로 사는게 좋은데",
          "original_form": "인터넷으로 사는게 좋은데",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030356.1.11",
          "form": "그 과정이 좀 귀찮긴해요 ㅠㅠㅋㅋ",
          "original_form": "그 과정이 좀 귀찮긴해요 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030356.1.12",
          "form": "그건 그래요ㅎㅎ최근에 구매한 물건 있으세요?",
          "original_form": "그건 그래요ㅎㅎ최근에 구매한 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030356.1.13",
          "form": "음 최근에는 밀키트 구매했어요! 할인하길래 ㅋㅋ",
          "original_form": "음 최근에는 밀키트 구매했어요! 할인하길래 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030356.1.14",
          "form": "오 어떤건가요?ㅎㅎ",
          "original_form": "오 어떤건가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030356.1.15",
          "form": "프레시지 밀키트 인데 ㅋㅋ 스테이크랑 감바스 사봤어요!!",
          "original_form": "프레시지 밀키트 인데 ㅋㅋ 스테이크랑 감바스 사봤어요!!",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030356.1.16",
          "form": "오 저 먹어본 거 같아요ㅎㅎ",
          "original_form": "오 저 먹어본 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030356.1.17",
          "form": "스테이크는 그저 그랬고 감바스가 맛있었어요ㅋㅋ",
          "original_form": "스테이크는 그저 그랬고 감바스가 맛있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030356.1.18",
          "form": "오 ㅋㅋㅋ정말요? 두근두근 ㅋㅋㅋ내일.감바스 해먹으려구요",
          "original_form": "오 ㅋㅋㅋ정말요? 두근두근 ㅋㅋㅋ내일.감바스 해먹으려구요",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030356.1.19",
          "form": "맛있겠어요ㅎㅎ",
          "original_form": "맛있겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030356.1.20",
          "form": "애슐리 빠네파스타 밀키트도 맛나요",
