      "utterance": [
        {
          "id": "MDRW2100025655.1.1",
          "form": "집안일은 해도해도 끝이 없는거 같아요",
          "original_form": "집안일은 해도해도 끝이 없는거 같아요",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100025655.1.2",
          "form": "맞아요. 오늘은 빨래는 못하겠네요.",
          "original_form": "맞아요. 오늘은 빨래는 못하겠네요.",
          "speaker_id": "2",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100025655.1.3",
          "form": "그럴때 건조기가 좋다던데 아직 없네요 ㅠ",
          "original_form": "그럴때 건조기가 좋다던데 아직 없네요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100025655.1.4",
          "form": "고양이 털도 다 빼준다던데",
          "original_form": "고양이 털도 다 빼준다던데",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100025655.1.5",
          "form": "사고싶은데 비싸기도 하구 공간도 없네요ㅠ",
          "original_form": "사고싶은데 비싸기도 하구 공간도 없네요ㅠ",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025655.1.6",
          "form": "저두요 아직 집사라고 돈 모으는중이라 ㅠ 이사가면 다사고 싶네요",
          "original_form": "저두요 아직 집사라고 돈 모으는중이라 ㅠ 이사가면 다사고 싶네요",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025655.1.7",
          "form": "식기세척기 갖고싶어요",
          "original_form": "식기세척기 갖고싶어요",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025655.1.8",
          "form": "진짜 좋대요",
          "original_form": "진짜 좋대요",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025655.1.9",
          "form": "처음 나왔을땐 의심스러웠는데ㅋㅋ",
          "original_form": "처음 나왔을땐 의심스러웠는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025655.1.10",
          "form": "그러게요 ㅋㅋ 요리보다 힘든 설거지",
          "original_form": "그러게요 ㅋㅋ 요리보다 힘든 설거지",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025655.1.11",
          "form": "저는 집안일중에 화장실 청소 젤 좋아해요",
          "original_form": "저는 집안일중에 화장실 청소 젤 좋아해요",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025655.1.12",
          "form": "전 다 싫던데ㅋㅋ 곰팡이 피면 처리하기 힘들어서요",
          "original_form": "전 다 싫던데ㅋㅋ 곰팡이 피면 처리하기 힘들어서요",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025655.1.13",
          "form": "락스칠 해서 깨끗해지면 뭔가 기분이 좋은 ㅋㅋㅋㅋ",
          "original_form": "락스칠 해서 깨끗해지면 뭔가 기분이 좋은 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025655.1.14",
          "form": "요즘 청소기 자동비워주는거도 탐나요",
          "original_form": "요즘 청소기 자동비워주는거도 탐나요",
          "speaker_id": "1",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100025655.1.15",
          "form": "그런거도 있군요",
          "original_form": "그런거도 있군요",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025655.1.16",
          "form": "집안일 다 자동으로 됐으면 좋겠네요ㅎㅎ",
          "original_form": "집안일 다 자동으로 됐으면 좋겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025655.1.17",
          "form": "그러게요 비싸서 그렇지 별별게 다있네요",
          "original_form": "그러게요 비싸서 그렇지 별별게 다있네요",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025655.1.18",
          "form": "요리 로봇도 있으면 ㅎㅎ",
          "original_form": "요리 로봇도 있으면 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025655.1.19",
          "form": "재료만 넣으면 뚝딱 나오면 좋겠어요ㅋㅋ",
          "original_form": "재료만 넣으면 뚝딱 나오면 좋겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025655.1.20",
          "form": "전 요리를 못해서 더욱더..ㅋㅋㅋ",
          "original_form": "전 요리를 못해서 더욱더..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025655.1.21",
          "form": "그래서 밀키트 자주사먹어요 ㅠㅠㅋㅋㅋ",
          "original_form": "그래서 밀키트 자주사먹어요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025655.1.22",
          "form": "전 배달이요ㅋㅋㅋ",
          "original_form": "전 배달이요ㅋㅋㅋ",
          "speaker_id": "2",
