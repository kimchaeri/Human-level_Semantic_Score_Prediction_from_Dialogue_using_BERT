        {
          "id": "MDRW2100046055.1.1",
          "form": "내가 보는 유튜브 추천해 줄게",
          "original_form": "내가 보는 유튜브 추천해 줄게",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046055.1.2",
          "form": "어떤 유튜브",
          "original_form": "어떤 유튜브",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046055.1.3",
          "form": "고양이 영상이 엄청 귀여워",
          "original_form": "고양이 영상이 엄청 귀여워",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046055.1.4",
          "form": "어떤 고양이야",
          "original_form": "어떤 고양이야",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046055.1.5",
          "form": "시골 고양이인데",
          "original_form": "시골 고양이인데",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046055.1.6",
          "form": "마당에서 자라겠다",
          "original_form": "마당에서 자라겠다",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046055.1.7",
          "form": "엄청 애교가 많아",
          "original_form": "엄청 애교가 많아",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046055.1.8",
          "form": "주인을 좋아하나보네",
          "original_form": "주인을 좋아하나보네",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046055.1.9",
          "form": "고등어 색깔이야",
          "original_form": "고등어 색깔이야",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046055.1.10",
          "form": "나는 노랑색 고양이가 좋은데",
          "original_form": "나는 노랑색 고양이가 좋은데",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046055.1.11",
          "form": "그 고양이도 있긴 있어",
          "original_form": "그 고양이도 있긴 있어",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046055.1.12",
          "form": "그 고양이도 애교가 많니",
          "original_form": "그 고양이도 애교가 많니",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046055.1.13",
          "form": "엄청 많고 귀여워",
          "original_form": "엄청 많고 귀여워",
