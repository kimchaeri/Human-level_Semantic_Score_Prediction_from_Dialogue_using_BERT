      "utterance": [
        {
          "id": "MDRW2100041959.1.1",
          "form": "오늘 학교 가셨나요",
          "original_form": "오늘 학교 가셨나요",
          "speaker_id": "1",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041959.1.2",
          "form": "넹ㅎㅎ 중간에 조퇴!",
          "original_form": "넹ㅎㅎ 중간에 조퇴!",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041959.1.3",
          "form": "헉 조퇴 왜하셨어요",
          "original_form": "헉 조퇴 왜하셨어요",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041959.1.4",
          "form": "아파서..ㅜㅜ 중간에 학교 나오니 좀 그렇네용",
          "original_form": "아파서..ㅜㅜ 중간에 학교 나오니 좀 그렇네용",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041959.1.5",
          "form": "아이고... 쉬셔야겠네 ㅠㅠ",
          "original_form": "아이고... 쉬셔야겠네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041959.1.6",
          "form": "요즘은 비대면 안하나요?",
          "original_form": "요즘은 비대면 안하나요?",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041959.1.7",
          "form": "음 전면등교에요!",
          "original_form": "음 전면등교에요!",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041959.1.8",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041959.1.9",
          "form": "힘드시겠어요",
          "original_form": "힘드시겠어요",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041959.1.10",
          "form": "ㅜㅜㅋㅋ 원래는 이게 당연한데",
          "original_form": "ㅜㅜㅋㅋ 원래는 이게 당연한데",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041959.1.11",
          "form": "온라인에 넘 익숙해진..",
          "original_form": "온라인에 넘 익숙해진..",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041959.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041959.1.13",
          "form": "저도 비대면이라 너무 좋아요,,,",
          "original_form": "저도 비대면이라 너무 좋아요,,,",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041959.1.14",
          "form": "학교 가기 귀찮은",
          "original_form": "학교 가기 귀찮은",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041959.1.15",
          "form": "하 긍께요,, 비대면 최곤데",
          "original_form": "하 긍께요,, 비대면 최곤데",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041959.1.16",
          "form": "고등학생이세요?",
          "original_form": "고등학생이세요?",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041959.1.17",
          "form": "넵 전 고등학생",
          "original_form": "넵 전 고등학생",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041959.1.18",
          "form": "거기는 매점 여나요",
          "original_form": "거기는 매점 여나요",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041959.1.19",
          "form": "아녀 요즘은 매점이 별루 없어서..",
          "original_form": "아녀 요즘은 매점이 별루 없어서..",
          "speaker_id": "2",
          "time": "20210906 18:05"
        }
      ]
