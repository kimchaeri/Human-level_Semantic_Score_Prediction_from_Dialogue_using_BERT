      "utterance": [
        {
          "id": "MDRW2100025256.1.1",
          "form": "전자신가요 후자신가요",
          "original_form": "전자신가요 후자신가요",
          "speaker_id": "2",
          "time": "20210901 03:15"
        },
        {
          "id": "MDRW2100025256.1.2",
          "form": "국물있라면이요! ㅋㅋㅋ",
          "original_form": "국물있라면이요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:15"
        },
        {
          "id": "MDRW2100025256.1.3",
          "form": "주로 뭐 드세요 라면중에?",
          "original_form": "주로 뭐 드세요 라면중에?",
          "speaker_id": "1",
          "time": "20210901 03:16"
        },
        {
          "id": "MDRW2100025256.1.4",
          "form": "역시 라면은 기본이 국물이잖아요",
          "original_form": "역시 라면은 기본이 국물이잖아요",
          "speaker_id": "2",
          "time": "20210901 03:16"
        },
        {
          "id": "MDRW2100025256.1.5",
          "form": "저 틈새라면이요",
          "original_form": "저 틈새라면이요",
          "speaker_id": "2",
          "time": "20210901 03:16"
        },
        {
          "id": "MDRW2100025256.1.6",
          "form": "헐헐",
          "original_form": "헐헐",
          "speaker_id": "1",
          "time": "20210901 03:16"
        },
        {
          "id": "MDRW2100025256.1.7",
          "form": "매운거 잘드시나보다",
          "original_form": "매운거 잘드시나보다",
          "speaker_id": "1",
          "time": "20210901 03:16"
        },
        {
          "id": "MDRW2100025256.1.8",
          "form": "음 안 매운거는 무파마에요",
          "original_form": "음 안 매운거는 무파마에요",
          "speaker_id": "2",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025256.1.9",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025256.1.10",
          "form": "무파마도 매워요! ㅋㅋㅋㅋ",
          "original_form": "무파마도 매워요! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025256.1.11",
          "form": "어린이신가",
          "original_form": "어린이신가",
          "speaker_id": "2",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025256.1.12",
          "form": "전 신라면도 좀 매운거같아요 ㅋㅋㅋㅋ",
          "original_form": "전 신라면도 좀 매운거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025256.1.13",
          "form": "맵찔이라서...",
          "original_form": "맵찔이라서...",
          "speaker_id": "1",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025256.1.14",
          "form": "진순 드십니까 ㅋㅋ",
          "original_form": "진순 드십니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025256.1.15",
          "form": "그것도 먹긴한데 맛은없는거같아요 ㅋㅋㅋ",
          "original_form": "그것도 먹긴한데 맛은없는거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:17"
        },
        {
          "id": "MDRW2100025256.1.16",
          "form": "일단 기본으로 삼양라면 사둬요 ㅋㅋㅋ",
          "original_form": "일단 기본으로 삼양라면 사둬요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:18"
        },
        {
          "id": "MDRW2100025256.1.17",
          "form": "ㅋㅋ삼양도 괜찮죠",
          "original_form": "ㅋㅋ삼양도 괜찮죠",
          "speaker_id": "2",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025256.1.18",
          "form": "냉면은 물냉이십니까 비냉이십니까",
          "original_form": "냉면은 물냉이십니까 비냉이십니까",
          "speaker_id": "2",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025256.1.19",
          "form": "불닭 이런것도 잘드시겠어요",
          "original_form": "불닭 이런것도 잘드시겠어요",
          "speaker_id": "1",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025256.1.20",
          "form": "물냉이요!",
          "original_form": "물냉이요!",
          "speaker_id": "1",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025256.1.21",
          "form": "역시 기본에 충실하신",
          "original_form": "역시 기본에 충실하신",
          "speaker_id": "2",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025256.1.22",
          "form": "정통파이십니다",
          "original_form": "정통파이십니다",
          "speaker_id": "2",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025256.1.23",
          "form": "님은 어느쪽이세요? 물냉비냉?",
          "original_form": "님은 어느쪽이세요? 물냉비냉?",
          "speaker_id": "1",
          "time": "20210901 03:20"
        },
        {
          "id": "MDRW2100025256.1.24",
          "form": "비냉이 더 맛있다는 애들 혼내줘야함",
          "original_form": "비냉이 더 맛있다는 애들 혼내줘야함",
          "speaker_id": "2",
          "time": "20210901 03:20"
        },
        {
          "id": "MDRW2100025256.1.25",
          "form": "당연히 물냉이죠",
          "original_form": "당연히 물냉이죠",
          "speaker_id": "2",
          "time": "20210901 03:20"
        }
