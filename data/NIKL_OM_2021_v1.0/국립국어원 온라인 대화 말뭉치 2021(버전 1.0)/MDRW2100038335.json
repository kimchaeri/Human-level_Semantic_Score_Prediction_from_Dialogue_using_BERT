      "utterance": [
        {
          "id": "MDRW2100038335.1.1",
          "form": "떡볶이도 좋죠 요즘로제 맛있던데요",
          "original_form": "떡볶이도 좋죠 요즘로제 맛있던데요",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100038335.1.2",
          "form": "맞아요 저 엽떡 로제 엄청 좋아해요",
          "original_form": "맞아요 저 엽떡 로제 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100038335.1.3",
          "form": "거기꺼는 안먹어봤는데",
          "original_form": "거기꺼는 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038335.1.4",
          "form": "먹어봐야겠네요",
          "original_form": "먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038335.1.5",
          "form": "신전로제 맛나더라고요",
          "original_form": "신전로제 맛나더라고요",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038335.1.6",
          "form": "저야말로 신전로제를 못먹어봤네요",
          "original_form": "저야말로 신전로제를 못먹어봤네요",
          "speaker_id": "1",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038335.1.7",
          "form": "나중에 먹어보겠습니다",
          "original_form": "나중에 먹어보겠습니다",
          "speaker_id": "1",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038335.1.8",
          "form": "네 제일  괜찮았던곳 같네요ㅎㅎ",
          "original_form": "네 제일  괜찮았던곳 같네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100038335.1.9",
          "form": "찾아봤는데 근처에 신전이 없대요",
          "original_form": "찾아봤는데 근처에 신전이 없대요",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100038335.1.10",
          "form": "아하;;그렇군요ㅜㅜ",
          "original_form": "아하;;그렇군요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100038335.1.11",
          "form": "아쉽네요ㅜㅜ",
          "original_form": "아쉽네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100038335.1.12",
          "form": "기회가 된다면 좀 멀리 나가서라도 먹어볼게요",
          "original_form": "기회가 된다면 좀 멀리 나가서라도 먹어볼게요",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100038335.1.13",
          "form": "가까운데 있으면좋은데요ㅋ 하나 차리고싶네요ㅋㅋ",
          "original_form": "가까운데 있으면좋은데요ㅋ 하나 차리고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038335.1.14",
          "form": "맞아요 프랜차이즈 맛집은 직접 차려서라도 먹고 싶어요",
          "original_form": "맞아요 프랜차이즈 맛집은 직접 차려서라도 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038335.1.15",
          "form": "점심시간이네요ㅋ",
          "original_form": "점심시간이네요ㅋ",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038335.1.16",
          "form": "시간 참 빠르네요",
          "original_form": "시간 참 빠르네요",
          "speaker_id": "1",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038335.1.17",
          "form": "식사 맛있게하세요",
          "original_form": "식사 맛있게하세요",
