      "utterance": [
        {
          "id": "MDRW2100039141.1.1",
          "form": "근데 제 친구도 반도체 이쪽 원해서",
          "original_form": "근데 제 친구도 반도체 이쪽 원해서",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100039141.1.2",
          "form": "히히 좋아요 좋아요",
          "original_form": "히히 좋아요 좋아요",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100039141.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100039141.1.4",
          "form": "혹시 name3 교수님이신가요??",
          "original_form": "혹시 &name3& 교수님이신가요??",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100039141.1.5",
          "form": "아 맞네요",
          "original_form": "아 맞네요",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100039141.1.6",
          "form": "거기는 진짜 출퇴근 자유롭고",
          "original_form": "거기는 진짜 출퇴근 자유롭고",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100039141.1.7",
          "form": "근데 논문은 또 잘 나오고 그래욤",
          "original_form": "근데 논문은 또 잘 나오고 그래욤",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100039141.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100039141.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100039141.1.10",
          "form": "대신",
          "original_form": "대신",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100039141.1.11",
          "form": "대신......",
          "original_form": "대신......",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100039141.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100039141.1.13",
          "form": "등록금이",
          "original_form": "등록금이",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100039141.1.14",
          "form": "지원이 안된대요",
          "original_form": "지원이 안된대요",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100039141.1.15",
          "form": "지원이안되나요...",
          "original_form": "지원이안되나요...",
          "speaker_id": "1",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100039141.1.16",
          "form": "아ㅜㅜㅠ",
          "original_form": "아ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100039141.1.17",
          "form": "아니 뭐가 나쁜말이지!!!",
          "original_form": "아니 뭐가 나쁜말이지!!!",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100039141.1.18",
          "form": "네... 그래서 뭔가",
          "original_form": "네... 그래서 뭔가",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100039141.1.19",
          "form": "아ㅜㅠㅠ",
          "original_form": "아ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 11:17"
        },
        {
