      "utterance": [
        {
          "id": "MDRW2100030078.1.1",
          "form": "미소천사",
          "original_form": "미소천사",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100030078.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100030078.1.3",
          "form": "맨날 이것만부름",
          "original_form": "맨날 이것만부름",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100030078.1.4",
          "form": "최고의 댄수음악은 모다",
          "original_form": "최고의 댄수음악은 모다",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100030078.1.5",
          "form": "그거요?",
          "original_form": "그거요?",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100030078.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100030078.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100030078.1.8",
          "form": "어우 지겨워",
          "original_form": "어우 지겨워",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100030078.1.9",
          "form": "성시경콘서트 세번갔는데",
          "original_form": "성시경콘서트 세번갔는데",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100030078.1.10",
          "form": "그때마다 부름",
          "original_form": "그때마다 부름",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030078.1.11",
          "form": "최고의 댄수곡이니까요",
          "original_form": "최고의 댄수곡이니까요",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030078.1.12",
          "form": "그걸 라이부로 보시다니 그 눈 삽니다",
          "original_form": "그걸 라이부로 보시다니 그 눈 삽니다",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030078.1.13",
          "form": "노래는 참 잘하긴해요",
          "original_form": "노래는 참 잘하긴해요",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030078.1.14",
          "form": "나이먹으면서 성시경 특유의 재수없음이 많이 사라졌어요",
          "original_form": "나이먹으면서 성시경 특유의 재수없음이 많이 사라졌어요",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100030078.1.15",
          "form": "전 목소리만 좋아해요",
          "original_form": "전 목소리만 좋아해요",
          "speaker_id": "1",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100030078.1.16",
          "form": "잘났으니까 재수없는 거겠죠",
          "original_form": "잘났으니까 재수없는 거겠죠",
          "speaker_id": "1",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100030078.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030078.1.18",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030078.1.19",
          "form": "저도 목소리 좋은거 인정",
          "original_form": "저도 목소리 좋은거 인정",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030078.1.20",
          "form": "노래도 좋아해요",
          "original_form": "노래도 좋아해요",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030078.1.21",
          "form": "어제 악뮤 노래들었는데",
          "original_form": "어제 악뮤 노래들었는데",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030078.1.22",
          "form": "좋던데요",
          "original_form": "좋던데요",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030078.1.23",
          "form": "저도 요즘 계속듣고있어요",
          "original_form": "저도 요즘 계속듣고있어요",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030078.1.24",
          "form": "역시 듣고계시군여",
          "original_form": "역시 듣고계시군여",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030078.1.25",
          "form": "엘피 예약했는데 내년쯤으로 연기됐다고 며칠전 문자왔네요 ㅜㅠ",
          "original_form": "엘피 예약했는데 내년쯤으로 연기됐다고 며칠전 문자왔네요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030078.1.26",
          "form": "내년이요?",
          "original_form": "내년이요?",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030078.1.27",
          "form": "그렇게 밀려요?",
          "original_form": "그렇게 밀려요?",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030078.1.28",
          "form": "원래 그래요 쫌 자주있는 일이여서",
          "original_form": "원래 그래요 쫌 자주있는 일이여서",
          "speaker_id": "1",
          "time": "20210904 07:55"
        },
        {
          "id": "MDRW2100030078.1.29",
          "form": "잊고 있음 오더라구요",
          "original_form": "잊고 있음 오더라구요",
          "speaker_id": "1",
          "time": "20210904 07:56"
        },
        {
          "id": "MDRW2100030078.1.30",
          "form": "국내가수 아니죠?",
          "original_form": "국내가수 아니죠?",
          "speaker_id": "2",
          "time": "20210904 08:10"
        },
        {
          "id": "MDRW2100030078.1.31",
          "form": "아뇨 국내요",
          "original_form": "아뇨 국내요",
          "speaker_id": "1",
          "time": "20210904 09:00"
        },
        {
          "id": "MDRW2100030078.1.32",
          "form": "차라리 해외는 만든거 수입하는데",
          "original_form": "차라리 해외는 만든거 수입하는데",
          "speaker_id": "1",
          "time": "20210904 09:00"
        },
        {
          "id": "MDRW2100030078.1.33",
          "form": "국내는 제작전 예약이어서 그래요",
          "original_form": "국내는 제작전 예약이어서 그래요",
          "speaker_id": "1",
          "time": "20210904 09:00"
        },
        {
          "id": "MDRW2100030078.1.34",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210904 09:15"
        },
        {
          "id": "MDRW2100030078.1.35",
          "form": "오히려 그렇게 되는군요",
          "original_form": "오히려 그렇게 되는군요",
          "speaker_id": "2",
          "time": "20210904 09:15"
        },
        {
          "id": "MDRW2100030078.1.36",
          "form": "잊어버릴때쯤 오겠네요 ㅎㅎ",
          "original_form": "잊어버릴때쯤 오겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 09:16"
        },
        {
          "id": "MDRW2100030078.1.37",
          "form": "네 내가 ㅇㅣ런걸 시켰나 싶을때쯔음",
          "original_form": "네 내가 ㅇㅣ런걸 시켰나 싶을때쯔음",
          "speaker_id": "1",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030078.1.38",
          "form": "선물같아요",
          "original_form": "선물같아요",
          "speaker_id": "1",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030078.1.39",
          "form": "좋은거같은데요?",
          "original_form": "좋은거같은데요?",
          "speaker_id": "2",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100030078.1.40",
          "form": "ㅋㅋ 진짜 잊을만하면 오니깐요",
          "original_form": "ㅋㅋ 진짜 잊을만하면 오니깐요",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100030078.1.41",
          "form": "펀딩같은 느낌인데요?ㅋㅋ",
          "original_form": "펀딩같은 느낌인데요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:49"
        }
      ]
    }
