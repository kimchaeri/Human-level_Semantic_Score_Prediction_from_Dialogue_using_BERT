      "utterance": [
        {
          "id": "MDRW2100031069.1.1",
          "form": "이제 에버랜드 나와서 민속촌 가는 길이에요",
          "original_form": "이제 에버랜드 나와서 민속촌 가는 길이에요",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031069.1.2",
          "form": "경전철 차량 귀엽네요",
          "original_form": "경전철 차량 귀엽네요",
          "speaker_id": "2",
          "time": "20210904 16:09"
        },
        {
          "id": "MDRW2100031069.1.3",
          "form": "여행 언제까지하세요?",
          "original_form": "여행 언제까지하세요?",
          "speaker_id": "1",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100031069.1.4",
          "form": "6일 점심까지요!",
          "original_form": "6일 점심까지요!",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100031069.1.5",
          "form": "여행간지 한참되서 부럽네요ㅜ",
          "original_form": "여행간지 한참되서 부럽네요ㅜ",
          "speaker_id": "1",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100031069.1.6",
          "form": "10월에도 갈만한곳이 있으면 가려고 해요",
          "original_form": "10월에도 갈만한곳이 있으면 가려고 해요",
          "speaker_id": "2",
          "time": "20210904 16:14"
        },
        {
          "id": "MDRW2100031069.1.7",
          "form": "대체휴일 있어서",
          "original_form": "대체휴일 있어서",
          "speaker_id": "2",
          "time": "20210904 16:14"
        },
        {
          "id": "MDRW2100031069.1.8",
          "form": "가기 좋겠더라구요",
          "original_form": "가기 좋겠더라구요",
          "speaker_id": "2",
          "time": "20210904 16:14"
        },
        {
          "id": "MDRW2100031069.1.9",
          "form": "여행 자주 다니시나봐요",
          "original_form": "여행 자주 다니시나봐요",
          "speaker_id": "1",
          "time": "20210904 16:14"
        },
        {
          "id": "MDRW2100031069.1.10",
          "form": "가족이나 친구가 꼬시면 거부하지 않아요ㅋㅋ",
          "original_form": "가족이나 친구가 꼬시면 거부하지 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100031069.1.11",
          "form": "자주적이진 않습니다",
          "original_form": "자주적이진 않습니다",
          "speaker_id": "2",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100031069.1.12",
          "form": "6월엔 울릉도 독도 다녀왔어요",
          "original_form": "6월엔 울릉도 독도 다녀왔어요",
          "speaker_id": "2",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100031069.1.13",
          "form": "와 독도 가기힘들지 않나요?",
          "original_form": "와 독도 가기힘들지 않나요?",
          "speaker_id": "1",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100031069.1.14",
          "form": "그쵸, 전 여행 생각도 안해봤는데 친구가 권유해서 같이 가게 됐어요",
          "original_form": "그쵸, 전 여행 생각도 안해봤는데 친구가 권유해서 같이 가게 됐어요",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100031069.1.15",
          "form": "독도 들어갈때 다들 태극기 들고 가더라구요",
          "original_form": "독도 들어갈때 다들 태극기 들고 가더라구요",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100031069.1.16",
          "form": "국뽕 충전되는 느낌",
          "original_form": "국뽕 충전되는 느낌",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100031069.1.17",
          "form": "다들 기념사진 찍으러 ㅎㅎ",
          "original_form": "다들 기념사진 찍으러 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031069.1.18",
          "form": "독도 한시간 있다 나오는건가요?",
          "original_form": "독도 한시간 있다 나오는건가요?",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031069.1.19",
          "form": "그쵸 ㅎㅎ 돌아다닐 수 있는 범위는 진짜 좁았어요",
          "original_form": "그쵸 ㅎㅎ 돌아다닐 수 있는 범위는 진짜 좁았어요",
          "speaker_id": "2",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031069.1.20",
          "form": "30분 정도요",
          "original_form": "30분 정도요",
          "speaker_id": "2",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031069.1.21",
          "form": "되게 짧네요ㅜㅜ",
          "original_form": "되게 짧네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100031069.1.22",
          "form": "울릉도는 좋아요?",
          "original_form": "울릉도는 좋아요?",
          "speaker_id": "1",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100031069.1.23",
          "form": "일주하는 버스가 있긴 한데 차가 있으면 더 좋을듯한?",
          "original_form": "일주하는 버스가 있긴 한데 차가 있으면 더 좋을듯한?",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100031069.1.24",
          "form": "섬들이 다 그런것 같아요",
          "original_form": "섬들이 다 그런것 같아요",
          "speaker_id": "2",
          "time": "20210904 16:20"
        }
      ]
    }
  ]
