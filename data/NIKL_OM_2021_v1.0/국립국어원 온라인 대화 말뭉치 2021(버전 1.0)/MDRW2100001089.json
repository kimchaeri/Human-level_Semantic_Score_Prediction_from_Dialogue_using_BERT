      "utterance": [
        {
          "id": "MDRW2100001089.1.1",
          "form": "동생이나 언니있어?",
          "original_form": "동생이나 언니있어?",
          "speaker_id": "1",
          "time": "20210723 16:19"
        },
        {
          "id": "MDRW2100001089.1.2",
          "form": "동생이나 언니 있어?",
          "original_form": "동생이나 언니 있어?",
          "speaker_id": "1",
          "time": "20210723 16:21"
        },
        {
          "id": "MDRW2100001089.1.3",
          "form": "아니! 오빠 있어 우리 이제 반말하는 거야??",
          "original_form": "아니! 오빠 있어 우리 이제 반말하는 거야??",
          "speaker_id": "2",
          "time": "20210723 16:21"
        },
        {
          "id": "MDRW2100001089.1.4",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210723 16:21"
        },
        {
          "id": "MDRW2100001089.1.5",
          "form": "까먹었어요ㅠ 순간",
          "original_form": "까먹었어요ㅠ 순간",
          "speaker_id": "1",
          "time": "20210723 16:21"
        },
        {
          "id": "MDRW2100001089.1.6",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 16:21"
        },
        {
          "id": "MDRW2100001089.1.7",
          "form": "제가 친구랑도 대화하고있어서",
          "original_form": "제가 친구랑도 대화하고있어서",
          "speaker_id": "1",
          "time": "20210723 16:22"
        },
        {
          "id": "MDRW2100001089.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210723 16:22"
        },
        {
          "id": "MDRW2100001089.1.9",
          "form": "아아 반말 하죠!! 편하게",
          "original_form": "아아 반말 하죠!! 편하게",
          "speaker_id": "2",
          "time": "20210723 16:22"
        },
        {
          "id": "MDRW2100001089.1.10",
          "form": "그럴까?",
          "original_form": "그럴까?",
          "speaker_id": "1",
          "time": "20210723 16:22"
        },
        {
          "id": "MDRW2100001089.1.11",
          "form": "그랴!",
          "original_form": "그랴!",
          "speaker_id": "2",
          "time": "20210723 16:22"
        },
        {
          "id": "MDRW2100001089.1.12",
          "form": "넌 형제 있어??",
          "original_form": "넌 형제 있어??",
          "speaker_id": "2",
          "time": "20210723 16:22"
        },
        {
          "id": "MDRW2100001089.1.13",
          "form": "나 형 있어!",
          "original_form": "나 형 있어!",
          "speaker_id": "1",
          "time": "20210723 16:22"
        },
        {
          "id": "MDRW2100001089.1.14",
          "form": "우와아 나이 얼마나 차이나?",
          "original_form": "우와아 나이 얼마나 차이나?",
          "speaker_id": "2",
          "time": "20210723 16:23"
        },
        {
          "id": "MDRW2100001089.1.15",
          "form": "차이나 중국 아님",
          "original_form": "차이나 중국 아님",
          "speaker_id": "2",
          "time": "20210723 16:23"
        },
        {
          "id": "MDRW2100001089.1.16",
          "form": "ㅋㅋㅋㅋ웃기다",
          "original_form": "ㅋㅋㅋㅋ웃기다",
          "speaker_id": "1",
          "time": "20210723 16:24"
        },
        {
          "id": "MDRW2100001089.1.17",
          "form": "난 4살~",
          "original_form": "난 4살~",
          "speaker_id": "1",
          "time": "20210723 16:24"
        },
        {
          "id": "MDRW2100001089.1.18",
          "form": "name2는?",
          "original_form": "&name2&는?",
          "speaker_id": "1",
          "time": "20210723 16:24"
        },
        {
          "id": "MDRW2100001089.1.19",
          "form": "난 3살!",
          "original_form": "난 3살!",
          "speaker_id": "2",
          "time": "20210723 16:24"
        },
        {
          "id": "MDRW2100001089.1.20",
          "form": "오 안싸워?",
          "original_form": "오 안싸워?",
          "speaker_id": "1",
          "time": "20210723 16:25"
        },
        {
          "id": "MDRW2100001089.1.21",
          "form": "옛날엔 많이 싸웠는데 서로 크면서 어색해져서 안 싸워..ㅋㅋ 글구 지금은 자주 못봥",
          "original_form": "옛날엔 많이 싸웠는데 서로 크면서 어색해져서 안 싸워..ㅋㅋ 글구 지금은 자주 못봥",
          "speaker_id": "2",
          "time": "20210723 16:25"
        },
        {
          "id": "MDRW2100001089.1.22",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210723 16:25"
        },
        {
          "id": "MDRW2100001089.1.23",
          "form": "나도ㅋㅋ 지금은 같이 안살아서!",
          "original_form": "나도ㅋㅋ 지금은 같이 안살아서!",
          "speaker_id": "1",
          "time": "20210723 16:26"
        },
        {
          "id": "MDRW2100001089.1.24",
          "form": "싸울 때 혹시",
          "original_form": "싸울 때 혹시",
          "speaker_id": "2",
          "time": "20210723 16:26"
        },
        {
          "id": "MDRW2100001089.1.25",
          "form": "말싸움 말고 몸싸움도 해??",
          "original_form": "말싸움 말고 몸싸움도 해??",
          "speaker_id": "2",
          "time": "20210723 16:26"
        },
        {
          "id": "MDRW2100001089.1.26",
          "form": "형있으면 그렇대서..ㅋㅋ",
          "original_form": "형있으면 그렇대서..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 16:26"
        },
        {
          "id": "MDRW2100001089.1.27",
          "form": "예전엔 많이그랬어 ㅋㅋㅋ",
          "original_form": "예전엔 많이그랬어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 16:27"
        },
        {
          "id": "MDRW2100001089.1.28",
          "form": "잠깐만",
          "original_form": "잠깐만",
          "speaker_id": "1",
          "time": "20210723 16:27"
        },
        {
          "id": "MDRW2100001089.1.29",
          "form": "응!",
          "original_form": "응!",
          "speaker_id": "2",
          "time": "20210723 16:27"
        },
        {
          "id": "MDRW2100001089.1.30",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 16:28"
        },
        {
          "id": "MDRW2100001089.1.31",
          "form": "넌 오빠랑 안싸워?",
          "original_form": "넌 오빠랑 안싸워?",
          "speaker_id": "1",
          "time": "20210723 16:29"
        },
        {
          "id": "MDRW2100001089.1.32",
          "form": "엉 잘 안 싸워!",
          "original_form": "엉 잘 안 싸워!",
          "speaker_id": "2",
          "time": "20210723 16:35"
        },
        {
          "id": "MDRW2100001089.1.33",
          "form": "가족 말고 친구랑 살면 어떨 것 같아??",
          "original_form": "가족 말고 친구랑 살면 어떨 것 같아??",
          "speaker_id": "2",
          "time": "20210723 16:36"
        },
        {
          "id": "MDRW2100001089.1.34",
          "form": "룸메!",
          "original_form": "룸메!",
          "speaker_id": "2",
          "time": "20210723 16:36"
        },
        {
          "id": "MDRW2100001089.1.35",
          "form": "난 기숙사라서 학교 다닐 땐 룸메랑 살거든!!",
          "original_form": "난 기숙사라서 학교 다닐 땐 룸메랑 살거든!!",
          "speaker_id": "2",
          "time": "20210723 16:37"
        },
        {
          "id": "MDRW2100001089.1.36",
          "form": "아 기숙사살아?",
          "original_form": "아 기숙사살아?",
          "speaker_id": "1",
          "time": "20210723 16:37"
        },
        {
          "id": "MDRW2100001089.1.37",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "1",
          "time": "20210723 16:37"
        },
        {
          "id": "MDRW2100001089.1.38",
          "form": "초반엔 설레고 재밌는데 살다보면 그냥 집 같아 ㅋㅋㅋ",
          "original_form": "초반엔 설레고 재밌는데 살다보면 그냥 집 같아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 16:38"
        },
        {
          "id": "MDRW2100001089.1.39",
          "form": "친한 친구랑 방 써서 재밌어!!",
          "original_form": "친한 친구랑 방 써서 재밌어!!",
          "speaker_id": "2",
          "time": "20210723 16:39"
        },
        {
          "id": "MDRW2100001089.1.40",
          "form": "룸메 한명이야?",
          "original_form": "룸메 한명이야?",
          "speaker_id": "1",
          "time": "20210723 16:39"
        },
        {
          "id": "MDRW2100001089.1.41",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210723 16:39"
        },
        {
          "id": "MDRW2100001089.1.42",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210723 16:39"
        },
        {
          "id": "MDRW2100001089.1.43",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
