      "utterance": [
        {
          "id": "MDRW2100008311.1.1",
          "form": "요즘 날씨 좋죠",
          "original_form": "요즘 날씨 좋죠",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008311.1.2",
          "form": "네 어느계절 좋아하세요?",
          "original_form": "네 어느계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008311.1.3",
          "form": "여름이 젤 싫고",
          "original_form": "여름이 젤 싫고",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008311.1.4",
          "form": "요즘 같은 시기가 좋은데 짧네요",
          "original_form": "요즘 같은 시기가 좋은데 짧네요",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008311.1.5",
          "form": "네 이제 곧 가을이네요",
          "original_form": "네 이제 곧 가을이네요",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008311.1.6",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008311.1.7",
          "form": "이미 가을 접어든거 아닐까요",
          "original_form": "이미 가을 접어든거 아닐까요",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008311.1.8",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008311.1.9",
          "form": "이번 올림픽 이제 끝났는데",
          "original_form": "이번 올림픽 이제 끝났는데",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008311.1.10",
          "form": "겨울에 또 하겠네요",
          "original_form": "겨울에 또 하겠네요",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008311.1.11",
          "form": "글게요",
          "original_form": "글게요",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008311.1.12",
          "form": "겨울은 얼마나 추울지",
          "original_form": "겨울은 얼마나 추울지",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008311.1.13",
          "form": "요즘은 지구온난화때문에 날씨가 오락가락하는거같아요",
          "original_form": "요즘은 지구온난화때문에 날씨가 오락가락하는거같아요",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008311.1.14",
          "form": "기후위기가 걱정이죠",
          "original_form": "기후위기가 걱정이죠",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008311.1.15",
          "form": "다음 여름은 더 더울듯요",
          "original_form": "다음 여름은 더 더울듯요",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008311.1.16",
          "form": "날씨는 비오는게 좋으세요 맑은날이 좋으세요?",
          "original_form": "날씨는 비오는게 좋으세요 맑은날이 좋으세요?",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008311.1.17",
          "form": "창밖으로는 비가 좋은데 나갈 일있으면 맑은날이죠",
          "original_form": "창밖으로는 비가 좋은데 나갈 일있으면 맑은날이죠",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008311.1.18",
          "form": "비오면 나가기 싫어요",
          "original_form": "비오면 나가기 싫어요",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008311.1.19",
          "form": "저는 비오는날 냄새가 되게 좋더라구요",
          "original_form": "저는 비오는날 냄새가 되게 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008311.1.20",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008311.1.21",
          "form": "비 냄새는 좋은데 습도랑 질척이는게",
          "original_form": "비 냄새는 좋은데 습도랑 질척이는게",
          "speaker_id": "1",
          "time": "20210822 15:15"
        }
      ]
