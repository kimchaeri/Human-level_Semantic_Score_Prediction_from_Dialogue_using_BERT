      "utterance": [
        {
          "id": "MDRW2100007832.1.1",
          "form": "요즘 콘서트 인원제한이",
          "original_form": "요즘 콘서트 인원제한이",
          "speaker_id": "1",
          "time": "20210822 11:14"
        },
        {
          "id": "MDRW2100007832.1.2",
          "form": "2천명이었던가요",
          "original_form": "2천명이었던가요",
          "speaker_id": "1",
          "time": "20210822 11:14"
        },
        {
          "id": "MDRW2100007832.1.3",
          "form": "50명이...아니었어요???",
          "original_form": "50명이...아니었어요???",
          "speaker_id": "2",
          "time": "20210822 11:16"
        },
        {
          "id": "MDRW2100007832.1.4",
          "form": "하 저 잠깐 이마트택배온거들여놓느라그만,,,ㅠㅠ!!!",
          "original_form": "하 저 잠깐 이마트택배온거들여놓느라그만,,,ㅠㅠ!!!",
          "speaker_id": "2",
          "time": "20210822 11:16"
        },
        {
          "id": "MDRW2100007832.1.5",
          "form": "괜찮아요ㅎㅎ",
          "original_form": "괜찮아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:16"
        },
        {
          "id": "MDRW2100007832.1.6",
          "form": "2천명까지 가능은 할거예요",
          "original_form": "2천명까지 가능은 할거예요",
          "speaker_id": "1",
          "time": "20210822 11:17"
        },
        {
          "id": "MDRW2100007832.1.7",
          "form": "다만 확진자가 나오면",
          "original_form": "다만 확진자가 나오면",
          "speaker_id": "1",
          "time": "20210822 11:17"
        },
        {
          "id": "MDRW2100007832.1.8",
          "form": "콘서트 주체가 욕을 먹어서ㅠ",
          "original_form": "콘서트 주체가 욕을 먹어서ㅠ",
          "speaker_id": "1",
          "time": "20210822 11:17"
        },
        {
          "id": "MDRW2100007832.1.9",
          "form": "그렇죠....... 2천명이 가능했다니 그렇게 큰 곳도 있을까요?",
          "original_form": "그렇죠....... 2천명이 가능했다니 그렇게 큰 곳도 있을까요?",
          "speaker_id": "2",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007832.1.10",
          "form": "와중에 저 안 가본 티가 나는 게 쬐금 웃기고(..",
          "original_form": "와중에 저 안 가본 티가 나는 게 쬐금 웃기고(..",
          "speaker_id": "2",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007832.1.11",
          "form": "저도 콘서트는 못가봤어요ㅠ",
          "original_form": "저도 콘서트는 못가봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 11:18"
        },
        {
          "id": "MDRW2100007832.1.12",
          "form": "티켓팅에 맨날 실패하기도 했고...",
          "original_form": "티켓팅에 맨날 실패하기도 했고...",
          "speaker_id": "1",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007832.1.13",
          "form": "그나마 온콘이어서 다행이에요(?)",
          "original_form": "그나마 온콘이어서 다행이에요(?)",
          "speaker_id": "1",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007832.1.14",
          "form": "엣~~~~~~ 오래 파셧으니 가보셨을 거라 생각해버렷어요",
          "original_form": "엣~~~~~~ 오래 파셧으니 가보셨을 거라 생각해버렷어요",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007832.1.15",
          "form": "온콘 티켓팅은 몇 번 했었는데!!",
          "original_form": "온콘 티켓팅은 몇 번 했었는데!!",
          "speaker_id": "2",
          "time": "20210822 11:19"
        },
        {
          "id": "MDRW2100007832.1.16",
          "form": "근데 또 은근 VOD를 믿으니 보기가 귀찮더라고요(...",
          "original_form": "근데 또 은근 VOD를 믿으니 보기가 귀찮더라고요(...",
          "speaker_id": "2",
          "time": "20210822 11:20"
        },
        {
          "id": "MDRW2100007832.1.17",
          "form": "온콘은 무한 포도알이라",
          "original_form": "온콘은 무한 포도알이라",
