      "utterance": [
        {
          "id": "MDRW2100032214.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100032214.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032214.1.3",
          "form": "요즘 날씨 젛죠?계신 곳은 어떤가요",
          "original_form": "요즘 날씨 젛죠?계신 곳은 어떤가요",
          "speaker_id": "1",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032214.1.4",
          "form": "여기도 괜찮아요",
          "original_form": "여기도 괜찮아요",
          "speaker_id": "2",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032214.1.5",
          "form": "저번에 비 온 뒤로는 계속 맑아요",
          "original_form": "저번에 비 온 뒤로는 계속 맑아요",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100032214.1.6",
          "form": "그래서 기분도 좋네요!",
          "original_form": "그래서 기분도 좋네요!",
          "speaker_id": "2",
          "time": "20210905 14:09"
        },
        {
          "id": "MDRW2100032214.1.7",
          "form": "그러니까요 ㅎㅎ 그래서인지 요즘 반팔 압을지 긴팔입을지 고민하는 시간이 많아졌어요",
          "original_form": "그러니까요 ㅎㅎ 그래서인지 요즘 반팔 압을지 긴팔입을지 고민하는 시간이 많아졌어요",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032214.1.8",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032214.1.9",
          "form": "저녁되면 반팔은 좀 쌀쌀하더라구요",
          "original_form": "저녁되면 반팔은 좀 쌀쌀하더라구요",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032214.1.10",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100032214.1.11",
          "form": "출근할 때 보통 뭐입고 출근하세뇨?",
          "original_form": "출근할 때 보통 뭐입고 출근하세뇨?",
          "speaker_id": "1",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100032214.1.12",
          "form": "아직 학생이라",
          "original_form": "아직 학생이라",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032214.1.13",
          "form": "반팔에 가디건이나 얇은 외투 챙겨다녀요",
          "original_form": "반팔에 가디건이나 얇은 외투 챙겨다녀요",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032214.1.14",
          "form": "교복입고 다니는 건가뇨?",
          "original_form": "교복입고 다니는 건가뇨?",
          "speaker_id": "1",
          "time": "20210906 08:51"
        },
        {
          "id": "MDRW2100032214.1.15",
          "form": "아뇨 대학생이라 교복은 안입어요",
          "original_form": "아뇨 대학생이라 교복은 안입어요",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100032214.1.16",
          "form": "오늘은 바람이 좀 불더라구요",
          "original_form": "오늘은 바람이 좀 불더라구요",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100032214.1.17",
          "form": "확실히 가을이왔나봐요",
          "original_form": "확실히 가을이왔나봐요",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100032214.1.18",
          "form": "맞아요. 가을에 좋아하는 옷차림이 있나요",
          "original_form": "맞아요. 가을에 좋아하는 옷차림이 있나요",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100032214.1.19",
          "form": "전 니트를 좋아해요",
          "original_form": "전 니트를 좋아해요",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100032214.1.20",
          "form": "니트 조끼도 좋고",
          "original_form": "니트 조끼도 좋고",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100032214.1.21",
          "form": "되게 어디곳에서나 잘어울려서 좋아해요",
          "original_form": "되게 어디곳에서나 잘어울려서 좋아해요",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100032214.1.22",
          "form": "오 가지고 있는 니트 조끼 무슨 색 인가뇨",
          "original_form": "오 가지고 있는 니트 조끼 무슨 색 인가뇨",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100032214.1.23",
          "form": "베이지랑 체크요",
          "original_form": "베이지랑 체크요",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100032214.1.24",
          "form": "남색계열로 하나 더 사고싶어요",
          "original_form": "남색계열로 하나 더 사고싶어요",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100032214.1.25",
          "form": "오 이쁠 것 같아요",
          "original_form": "오 이쁠 것 같아요",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100032214.1.26",
          "form": "셔츠랑 레이어드해서 입나요",
          "original_form": "셔츠랑 레이어드해서 입나요",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100032214.1.27",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100032214.1.28",
          "form": "아님 긴 티랑도 입어요",
          "original_form": "아님 긴 티랑도 입어요",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100032214.1.29",
          "form": "이런 환절기에 입기 딱 좋더라구요",
          "original_form": "이런 환절기에 입기 딱 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 13:37"
        }
      ]
    }
