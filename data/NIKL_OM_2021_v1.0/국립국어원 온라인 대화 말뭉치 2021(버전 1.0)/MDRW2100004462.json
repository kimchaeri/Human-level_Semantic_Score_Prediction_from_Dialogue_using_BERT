      "utterance": [
        {
          "id": "MDRW2100004462.1.1",
          "form": "쇼핑하세요?",
          "original_form": "쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004462.1.2",
          "form": "전 요즘 퓨즈서울 애용해요",
          "original_form": "전 요즘 퓨즈서울 애용해요",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004462.1.3",
          "form": "그건 뭐예용 ?",
          "original_form": "그건 뭐예용 ?",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004462.1.4",
          "form": "의류쇼핑물인데 여성 옷치고 사이즈도 정사이즈로 널널하고 옷 마감도 튼튼해서 좋아요",
          "original_form": "의류쇼핑물인데 여성 옷치고 사이즈도 정사이즈로 널널하고 옷 마감도 튼튼해서 좋아요",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004462.1.5",
          "form": "오 저도함 봐야겟어영",
          "original_form": "오 저도함 봐야겟어영",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004462.1.6",
          "form": "네 엄청 편해요! 거기서 트렁크 하나 샀는데 요즘 엄청 애용하고있어요",
          "original_form": "네 엄청 편해요! 거기서 트렁크 하나 샀는데 요즘 엄청 애용하고있어요",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004462.1.7",
          "form": "오 트렁크 편하죠 ㅠㅠ",
          "original_form": "오 트렁크 편하죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004462.1.8",
          "form": "저도 요즘 빠졌어용",
          "original_form": "저도 요즘 빠졌어용",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004462.1.9",
          "form": "그쵸 너무 편해요!! 어디꺼 쓰세요?",
          "original_form": "그쵸 너무 편해요!! 어디꺼 쓰세요?",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004462.1.10",
          "form": "정해진건 없어용 ㅠㅠ 그때그때 검색해서 최저가로 구매해용",
          "original_form": "정해진건 없어용 ㅠㅠ 그때그때 검색해서 최저가로 구매해용",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004462.1.11",
          "form": "그쵸 요즘 여성용이라고 비싼게 너무 많아서.. 싼게 최곤데",
          "original_form": "그쵸 요즘 여성용이라고 비싼게 너무 많아서.. 싼게 최곤데",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004462.1.12",
          "form": "맞아여 .. 그 노브라티도 이번에 샀는데 엄청편해요 근데 비싸여 아직은 ㅠ",
          "original_form": "맞아여 .. 그 노브라티도 이번에 샀는데 엄청편해요 근데 비싸여 아직은 ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004462.1.13",
          "form": "ㅜㅜ 전 그래서 그냥 너무 덥거나 그러면 패치 붙이고 다녀요",
          "original_form": "ㅜㅜ 전 그래서 그냥 너무 덥거나 그러면 패치 붙이고 다녀요",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004462.1.14",
          "form": "아 그 시원한 패치요?",
          "original_form": "아 그 시원한 패치요?",
          "speaker_id": "1",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004462.1.15",
          "form": "그냥 니플패치요.. ㅎㅎ",
          "original_form": "그냥 니플패치요.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004462.1.16",
          "form": "세상 편해요",
          "original_form": "세상 편해요",
