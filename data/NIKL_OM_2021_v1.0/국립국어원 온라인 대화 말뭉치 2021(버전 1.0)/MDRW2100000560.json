      "utterance": [
        {
          "id": "MDRW2100000560.1.1",
          "form": "그러면 혹시 요즘 무슨 유투브 보세요???",
          "original_form": "그러면 혹시 요즘 무슨 유투브 보세요???",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000560.1.2",
          "form": "저는 해쭈,예랑가랑 이렇게 봐요!!",
          "original_form": "저는 해쭈,예랑가랑 이렇게 봐요!!",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000560.1.3",
          "form": "해쭈 저도 봐요!!",
          "original_form": "해쭈 저도 봐요!!",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000560.1.4",
          "form": "ㅋㅋㅋㅋ햄튜브 덕분에 알게된 존재라죠",
          "original_form": "ㅋㅋㅋㅋ햄튜브 덕분에 알게된 존재라죠",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000560.1.5",
          "form": "맞아욬ㅋㅋㅋㅋㅋ햄튜브가 친구들한테 유투브 많이 추천했다고 들었어요",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋ햄튜브가 친구들한테 유투브 많이 추천했다고 들었어요",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000560.1.6",
          "form": "진짜 해쭈 생일파티 영상올라올때마다 행복해요",
          "original_form": "진짜 해쭈 생일파티 영상올라올때마다 행복해요",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000560.1.7",
          "form": "ㅋㅋㅋㅋ진짜 대리만족 중이에요ㅠㅠ 어젠가 쁘큐보이 조카 생일도 봤다규요!",
          "original_form": "ㅋㅋㅋㅋ진짜 대리만족 중이에요ㅠㅠ 어젠가 쁘큐보이 조카 생일도 봤다규요!",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000560.1.8",
          "form": "먹는것도 너무 잘먹어서 다이어트할 때도 대리만족..",
          "original_form": "먹는것도 너무 잘먹어서 다이어트할 때도 대리만족..",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000560.1.9",
          "form": "맞아욬ㅋㅋㅋㅋㅋ민준이도 넘 귀엽고 카야도 진짜 귀여워요",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋ민준이도 넘 귀엽고 카야도 진짜 귀여워요",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000560.1.10",
          "form": "진심 먹는 영상 브이로그가 제일 제 최애에요 진짜 몇번이고 계속 돌려봐요",
          "original_form": "진심 먹는 영상 브이로그가 제일 제 최애에요 진짜 몇번이고 계속 돌려봐요",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000560.1.11",
          "form": "그리고 해쭈 자체로 너무 웃겨서 너무 재밌어욬ㅋㅋㅋ",
          "original_form": "그리고 해쭈 자체로 너무 웃겨서 너무 재밌어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000560.1.12",
          "form": "ㅋㅋㅋ저만 그런게 아니었네요!",
          "original_form": "ㅋㅋㅋ저만 그런게 아니었네요!",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000560.1.13",
          "form": "룬룬쓰도 혹시 보시나용?!",
          "original_form": "룬룬쓰도 혹시 보시나용?!",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000560.1.14",
          "form": "아니요!! 룬룬쓰는 첨들어봐요",
          "original_form": "아니요!! 룬룬쓰는 첨들어봐요",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000560.1.15",
          "form": "무슨 유투버인가요??",
          "original_form": "무슨 유투버인가요??",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000560.1.16",
          "form": "일본에사 생활하는 한국유튜번데 애기도 귀엽고 영상분위기자체가 좋아요!!",
          "original_form": "일본에사 생활하는 한국유튜번데 애기도 귀엽고 영상분위기자체가 좋아요!!",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000560.1.17",
          "form": "일본에서 장보고 도시락도 만드는?",
          "original_form": "일본에서 장보고 도시락도 만드는?",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000560.1.18",
          "form": "그런 유튜버에요!",
          "original_form": "그런 유튜버에요!",
          "speaker_id": "1",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000560.1.19",
          "form": "아하 그렇군요!! 일본 브이로그구나",
          "original_form": "아하 그렇군요!! 일본 브이로그구나",
          "speaker_id": "2",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100000560.1.20",
          "form": "한번 유투브에서 찾아볼게요! 궁금해요",
          "original_form": "한번 유투브에서 찾아볼게요! 궁금해요",
          "speaker_id": "2",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100000560.1.21",
          "form": "앜ㅋㅋ 네!!",
          "original_form": "앜ㅋㅋ 네!!",
          "speaker_id": "1",
          "time": "20210819 18:02"
        }
      ]
