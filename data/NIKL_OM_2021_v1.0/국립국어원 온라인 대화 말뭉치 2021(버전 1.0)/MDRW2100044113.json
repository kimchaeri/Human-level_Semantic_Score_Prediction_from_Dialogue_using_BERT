      "utterance": [
        {
          "id": "MDRW2100044113.1.1",
          "form": "카이막 알아요!!",
          "original_form": "카이막 알아요!!",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044113.1.2",
          "form": "혹시 드셔봤나요??",
          "original_form": "혹시 드셔봤나요??",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044113.1.3",
          "form": "아 전에 그리스 터키 음식 하는 데서 먹어본 것도 같은데",
          "original_form": "아 전에 그리스 터키 음식 하는 데서 먹어본 것도 같은데",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044113.1.4",
          "form": "식전빵이랑 꾸덕한 크림? 나오는 것 맞죠?",
          "original_form": "식전빵이랑 꾸덕한 크림? 나오는 것 맞죠?",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044113.1.5",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044113.1.6",
          "form": "드셔보셨군요!!",
          "original_form": "드셔보셨군요!!",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044113.1.7",
          "form": "어 그럼 혹시 현지가서 드셨나요?",
          "original_form": "어 그럼 혹시 현지가서 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044113.1.8",
          "form": "아뇨 한국에서요ㅎㅎ",
          "original_form": "아뇨 한국에서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044113.1.9",
          "form": "현지에서 드셔봤나요?",
          "original_form": "현지에서 드셔봤나요?",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044113.1.10",
          "form": "아핳 ㅋㅋㅋ 아니요 저도 한국에서 먹어봤어요",
          "original_form": "아핳 ㅋㅋㅋ 아니요 저도 한국에서 먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044113.1.11",
          "form": "백종원 선생님이 티비에서 드시는 것 보고",
          "original_form": "백종원 선생님이 티비에서 드시는 것 보고",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044113.1.12",
          "form": "저는 차치키라고 그리스 소스가 있는데",
          "original_form": "저는 차치키라고 그리스 소스가 있는데",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044113.1.13",
          "form": "먹고 싶어서 찾아 헤메서 먹고 왔어요 ㅋㅋ",
          "original_form": "먹고 싶어서 찾아 헤메서 먹고 왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044113.1.14",
          "form": "그건 집에서 만들어 먹어요ㅋㅋ",
          "original_form": "그건 집에서 만들어 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044113.1.15",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044113.1.16",
          "form": "그릭요거트랑 오이로",
          "original_form": "그릭요거트랑 오이로",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044113.1.17",
          "form": "옹.. 주로 어떤거랑 같이 먹으면 좋은 소스인가요?",
          "original_form": "옹.. 주로 어떤거랑 같이 먹으면 좋은 소스인가요?",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044113.1.18",
          "form": "그것만 먹어도 맛있고",
          "original_form": "그것만 먹어도 맛있고",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044113.1.19",
          "form": "빵에 넣어먹거나",
          "original_form": "빵에 넣어먹거나",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044113.1.20",
          "form": "튀김음식과 먹어도 잘 어울려요",
          "original_form": "튀김음식과 먹어도 잘 어울려요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044113.1.21",
          "form": "옹.. 만능 소스인가봐요!",
          "original_form": "옹.. 만능 소스인가봐요!",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044113.1.22",
          "form": "만들기 어렵진 않나요?",
          "original_form": "만들기 어렵진 않나요?",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044113.1.23",
          "form": "네 오이채썰고 그릭요거트 올리브유 레몬즙 마늘다진 것 섞어요",
          "original_form": "네 오이채썰고 그릭요거트 올리브유 레몬즙 마늘다진 것 섞어요",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044113.1.24",
          "form": "옹.. 들어가는 재료가 엄청 많지는 않네요?",
          "original_form": "옹.. 들어가는 재료가 엄청 많지는 않네요?",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044113.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044113.1.26",
          "form": "집에 있는 것들이라",
          "original_form": "집에 있는 것들이라",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044113.1.27",
          "form": "어렵지 않게 만들 수 있어요",
          "original_form": "어렵지 않게 만들 수 있어요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        }
      ]
    }
