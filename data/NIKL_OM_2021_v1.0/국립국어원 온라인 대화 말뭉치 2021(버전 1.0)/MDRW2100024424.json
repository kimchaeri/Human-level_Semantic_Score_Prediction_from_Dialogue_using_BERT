        },
        {
          "id": "MDRW2100024424.1.5",
          "form": "name1님은요~?",
          "original_form": "&name1&님은요~?",
          "speaker_id": "2",
          "time": "20210831 08:37"
        },
        {
          "id": "MDRW2100024424.1.6",
          "form": "개강 하셨어요??",
          "original_form": "개강 하셨어요??",
          "speaker_id": "1",
          "time": "20210831 08:37"
        },
        {
          "id": "MDRW2100024424.1.7",
          "form": "전 안다녀요!",
          "original_form": "전 안다녀요!",
          "speaker_id": "1",
          "time": "20210831 08:37"
        },
        {
          "id": "MDRW2100024424.1.8",
          "form": "내일 개강입니다ㅠㅜ",
          "original_form": "내일 개강입니다ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 08:37"
        },
        {
          "id": "MDRW2100024424.1.9",
          "form": "아 졸업하신건가요?",
          "original_form": "아 졸업하신건가요?",
          "speaker_id": "2",
          "time": "20210831 08:38"
        },
        {
          "id": "MDRW2100024424.1.10",
          "form": "네 ㅎ",
          "original_form": "네 ㅎ",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024424.1.11",
          "form": "헉 내일 개강이라니",
          "original_form": "헉 내일 개강이라니",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024424.1.12",
          "form": "온라인 수업이에요?",
          "original_form": "온라인 수업이에요?",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024424.1.13",
          "form": "넵!! 9월 한달간",
          "original_form": "넵!! 9월 한달간",
          "speaker_id": "2",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024424.1.14",
          "form": "온라인으로 한다고 해요ㅠㅠㅠ",
          "original_form": "온라인으로 한다고 해요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024424.1.15",
          "form": "학교 가고 싶었는데 너무 아쉬워요ㅜ",
          "original_form": "학교 가고 싶었는데 너무 아쉬워요ㅜ",
          "speaker_id": "2",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024424.1.16",
          "form": "역시 아직 코로나 때문에 ㅠㅠ",
          "original_form": "역시 아직 코로나 때문에 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100024424.1.17",
          "form": "저번 학기도 온라인이었나요?",
          "original_form": "저번 학기도 온라인이었나요?",
          "speaker_id": "1",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100024424.1.18",
          "form": "넵!!ㅠㅠ",
          "original_form": "넵!!ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100024424.1.19",
          "form": "온라인이 이제 좀 지겨워지네요..ㅎ",
          "original_form": "온라인이 이제 좀 지겨워지네요..ㅎ",
          "speaker_id": "2",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100024424.1.20",
          "form": "학교가 그리워요ㅜㅠ",
          "original_form": "학교가 그리워요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100024424.1.21",
          "form": "name1님은 졸업하고 학교 동기들이나 선배들하고 계속 연락하시나요?",
          "original_form": "&name1&님은 졸업하고 학교 동기들이나 선배들하고 계속 연락하시나요?",
          "speaker_id": "2",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100024424.1.22",
          "form": "요즘 코로나라 동기들이랑 자주 못만나서... 그런 걱정?도 들어요ㅎㅎ",
          "original_form": "요즘 코로나라 동기들이랑 자주 못만나서... 그런 걱정?도 들어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:00"
        },
        {
          "id": "MDRW2100024424.1.23",
          "form": "네 몇명은 하고 있어요",
          "original_form": "네 몇명은 하고 있어요",
          "speaker_id": "1",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024424.1.24",
          "form": "여태 학교 한번도 안나갔어요?",
          "original_form": "여태 학교 한번도 안나갔어요?",
          "speaker_id": "1",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024424.1.25",
          "form": "일학년은 다녔어요!!",
          "original_form": "일학년은 다녔어요!!",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024424.1.26",
          "form": "근데 그 이후로는 계속...",
          "original_form": "근데 그 이후로는 계속...",
          "speaker_id": "2",
