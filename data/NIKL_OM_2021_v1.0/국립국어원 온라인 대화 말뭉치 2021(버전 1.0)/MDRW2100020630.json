      "utterance": [
        {
          "id": "MDRW2100020630.1.1",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020630.1.2",
          "form": "혹시 레드벨벳도 좋아하시나요",
          "original_form": "혹시 레드벨벳도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020630.1.3",
          "form": "레드벨벳도 좋죠",
          "original_form": "레드벨벳도 좋죠",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020630.1.4",
          "form": "입덕까진 아니여도",
          "original_form": "입덕까진 아니여도",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020630.1.5",
          "form": "영상 좀 찾아보고했었어요!",
          "original_form": "영상 좀 찾아보고했었어요!",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020630.1.6",
          "form": "레드벨벳 좋아하세요?",
          "original_form": "레드벨벳 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020630.1.7",
          "form": "아아 전 아닌데",
          "original_form": "아아 전 아닌데",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020630.1.8",
          "form": "레드벨뱃이 여덕이 많은 이유가 궁금해서요",
          "original_form": "레드벨뱃이 여덕이 많은 이유가 궁금해서요",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020630.1.9",
          "form": "마마무는 저도 좋아해서 알겠어요",
          "original_form": "마마무는 저도 좋아해서 알겠어요",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020630.1.10",
          "form": "외국학생들이 물어보는데 시원한 답을 못주겠네요",
          "original_form": "외국학생들이 물어보는데 시원한 답을 못주겠네요",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020630.1.11",
          "form": "아하! 어렵네요",
          "original_form": "아하! 어렵네요",
          "speaker_id": "2",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020630.1.12",
          "form": "음..일단 다들 성격도 털털하고",
          "original_form": "음..일단 다들 성격도 털털하고",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020630.1.13",
          "form": "노래도 좋고 춤도 잘추고",
          "original_form": "노래도 좋고 춤도 잘추고",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.14",
          "form": "근데 그건 여돌들 다 그렇지않아요?",
          "original_form": "근데 그건 여돌들 다 그렇지않아요?",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.15",
          "form": "이건 너무 평범하죠?ㅋㅋㄱ",
          "original_form": "이건 너무 평범하죠?ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.16",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.17",
          "form": "여자친구도 그렇고...",
          "original_form": "여자친구도 그렇고...",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.18",
          "form": "네 ㅠㅜ 어려워요",
          "original_form": "네 ㅠㅜ 어려워요",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.19",
          "form": "울나라에서 마마무 레벨",
          "original_form": "울나라에서 마마무 레벨",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.20",
          "form": "일케 여덕이 젤 많은데",
          "original_form": "일케 여덕이 젤 많은데",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.21",
          "form": "설명이 어렵네요",
          "original_form": "설명이 어렵네요",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.22",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020630.1.23",
          "form": "그러게요 딱히 설명하려니까 어렵네요ㅠ",
          "original_form": "그러게요 딱히 설명하려니까 어렵네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020630.1.24",
          "form": "멤버들간의 케미?ㅋㅋ",
          "original_form": "멤버들간의 케미?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020630.1.25",
          "form": "이것도 다른그룹도 비슷하겠죠?ㅋㅋㄱ",
          "original_form": "이것도 다른그룹도 비슷하겠죠?ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020630.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020630.1.27",
          "form": "역시 답이 딱 안나오네요",
          "original_form": "역시 답이 딱 안나오네요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020630.1.28",
          "form": "괜찮아요 ㅎㅎ",
          "original_form": "괜찮아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020630.1.29",
          "form": "그냥 모른다고 대답해야겠어요",
          "original_form": "그냥 모른다고 대답해야겠어요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020630.1.30",
          "form": "ㅋㅋㅋ너무 여러 요소가 있어서 콕집어서 말하긴 어려운거같아요ㅠ",
          "original_form": "ㅋㅋㅋ너무 여러 요소가 있어서 콕집어서 말하긴 어려운거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020630.1.31",
          "form": "ㅎㅎ곡을 좀 들어봐야겠어요",
          "original_form": "ㅎㅎ곡을 좀 들어봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020630.1.32",
          "form": "ㅋㅋ네!",
          "original_form": "ㅋㅋ네!",
          "speaker_id": "2",
          "time": "20210828 10:46"
        }
      ]
    }
