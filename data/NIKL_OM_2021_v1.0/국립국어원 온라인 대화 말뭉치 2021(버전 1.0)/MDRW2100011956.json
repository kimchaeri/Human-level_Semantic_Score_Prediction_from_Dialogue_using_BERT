      "utterance": [
        {
          "id": "MDRW2100011956.1.1",
          "form": "싸이월드 해본적 있어?",
          "original_form": "싸이월드 해본적 있어?",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011956.1.2",
          "form": "응 나 초등학교 때",
          "original_form": "응 나 초등학교 때",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011956.1.3",
          "form": "와 진짜 오래됬다",
          "original_form": "와 진짜 오래됬다",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011956.1.4",
          "form": "엄청 오래됐지 난 버디버디 도 했었어",
          "original_form": "엄청 오래됐지 난 버디버디 도 했었어",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011956.1.5",
          "form": "버디버디가 머야?",
          "original_form": "버디버디가 머야?",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011956.1.6",
          "form": "메신저 프로그램 서로 채팅하는 거",
          "original_form": "메신저 프로그램 서로 채팅하는 거",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011956.1.7",
          "form": "페메 같은건가?",
          "original_form": "페메 같은건가?",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011956.1.8",
          "form": "어 요즘으로 치면 그런 거",
          "original_form": "어 요즘으로 치면 그런 거",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011956.1.9",
          "form": "만약에 다시 생기면 할 생각 있어?",
          "original_form": "만약에 다시 생기면 할 생각 있어?",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011956.1.10",
          "form": "아니 요즘은 카톡 쓰니까",
          "original_form": "아니 요즘은 카톡 쓰니까",
          "speaker_id": "2",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011956.1.11",
          "form": "카톡 많이 쓰긴 하지",
          "original_form": "카톡 많이 쓰긴 하지",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011956.1.12",
          "form": "싸이월드는 그래서 여러모로 부활 안 해도 될 거 같은데",
          "original_form": "싸이월드는 그래서 여러모로 부활 안 해도 될 거 같은데",
          "speaker_id": "2",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011956.1.13",
          "form": "감성 땜에 그런거 아님?",
          "original_form": "감성 땜에 그런거 아님?",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011956.1.14",
          "form": "싸이월드 감성은 엄청 오그라드는 거야",
          "original_form": "싸이월드 감성은 엄청 오그라드는 거야",
          "speaker_id": "2",
