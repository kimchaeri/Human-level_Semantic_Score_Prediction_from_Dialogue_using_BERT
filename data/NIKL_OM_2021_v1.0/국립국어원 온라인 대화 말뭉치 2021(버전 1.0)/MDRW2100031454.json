      "utterance": [
        {
          "id": "MDRW2100031454.1.1",
          "form": "메타버스 알아? 나 최근에 해봤자나",
          "original_form": "메타버스 알아? 나 최근에 해봤자나",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031454.1.2",
          "form": "우와 어떻게??",
          "original_form": "우와 어떻게??",
          "speaker_id": "2",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031454.1.3",
          "form": "취업 설명회로 했었어",
          "original_form": "취업 설명회로 했었어",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031454.1.4",
          "form": "메타버스더라고",
          "original_form": "메타버스더라고",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031454.1.5",
          "form": "좋네 어땠어??",
          "original_form": "좋네 어땠어??",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031454.1.6",
          "form": "취업 설명회가 꾸며져있는 가상공간에서 돌아다니고 거기 운영자들도 아바타고",
          "original_form": "취업 설명회가 꾸며져있는 가상공간에서 돌아다니고 거기 운영자들도 아바타고",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031454.1.7",
          "form": "나도 아바타고",
          "original_form": "나도 아바타고",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031454.1.8",
          "form": "신기하더라",
          "original_form": "신기하더라",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031454.1.9",
          "form": "컴퓨터로 접속해서",
          "original_form": "컴퓨터로 접속해서",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031454.1.10",
          "form": "한건가??",
          "original_form": "한건가??",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031454.1.11",
          "form": "집에서?",
          "original_form": "집에서?",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100031454.1.12",
          "form": "응응 컴터루",
          "original_form": "응응 컴터루",
          "speaker_id": "1",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100031454.1.13",
          "form": "아바타꾸미기로 시작하더라?",
          "original_form": "아바타꾸미기로 시작하더라?",
          "speaker_id": "1",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100031454.1.14",
          "form": "응응 나도 해보고싶다",
          "original_form": "응응 나도 해보고싶다",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031454.1.15",
          "form": "재밌을 것 같아",
          "original_form": "재밌을 것 같아",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031454.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ신청해봐 다음 삼성 디스플레이나 그런데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ신청해봐 다음 삼성 디스플레이나 그런데",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031454.1.17",
          "form": "취업 설명회에서 아마",
          "original_form": "취업 설명회에서 아마",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031454.1.18",
          "form": "할듯!",
          "original_form": "할듯!",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031454.1.19",
          "form": "더 설명해줘 어떻게 해??",
          "original_form": "더 설명해줘 어떻게 해??",
          "speaker_id": "2",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100031454.1.20",
          "form": "키보드로 움직여서 말 거는건가??",
          "original_form": "키보드로 움직여서 말 거는건가??",
          "speaker_id": "2",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100031454.1.21",
          "form": "웅 근데말은 안걸어봤엉ㅋㅋㅋ",
          "original_form": "웅 근데말은 안걸어봤엉ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031454.1.22",
          "form": "그냥 약간 흠",
          "original_form": "그냥 약간 흠",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031454.1.23",
          "form": "퍼피레드 해봤어?",
          "original_form": "퍼피레드 해봤어?",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100031454.1.24",
          "form": "그런느낌??",
          "original_form": "그런느낌??",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100031454.1.25",
          "form": "안해봤지만 찾아봤어",
          "original_form": "안해봤지만 찾아봤어",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100031454.1.26",
          "form": "신기하다 신세대구나?",
          "original_form": "신기하다 신세대구나?",
          "speaker_id": "2",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031454.1.27",
          "form": "웅 ? 퍼피레드 나 엄청 열심히했는데",
          "original_form": "웅 ? 퍼피레드 나 엄청 열심히했는데",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031454.1.28",
          "form": "테일즈런너 광장",
          "original_form": "테일즈런너 광장",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031454.1.29",
          "form": "느낌",
          "original_form": "느낌",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031454.1.30",
          "form": "이기도하구",
          "original_form": "이기도하구",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031454.1.31",
          "form": "아아 그건 알지",
          "original_form": "아아 그건 알지",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100031454.1.32",
          "form": "그렇게보면 나도 메타버스 해봤네 ㅎㅎ",
          "original_form": "그렇게보면 나도 메타버스 해봤네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100031454.1.33",
          "form": "그게 메타버스인지는 모르겠다 맞낭?ㅋㅋㅋ",
          "original_form": "그게 메타버스인지는 모르겠다 맞낭?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031454.1.34",
          "form": "그느낌 이었다는거지",
          "original_form": "그느낌 이었다는거지",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031454.1.35",
          "form": "초창기 버전? 그렇게 말하긴하더라",
          "original_form": "초창기 버전? 그렇게 말하긴하더라",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031454.1.36",
          "form": "메타버스의 조상 쯤이라고 게임이 ㅋㅋㅋ",
          "original_form": "메타버스의 조상 쯤이라고 게임이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031454.1.37",
          "form": "오 몰랐던 정보네~",
          "original_form": "오 몰랐던 정보네~",
          "speaker_id": "1",
          "time": "20210904 22:55"
        }
      ]
    }
