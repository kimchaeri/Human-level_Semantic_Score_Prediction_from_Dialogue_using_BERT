      "utterance": [
        {
          "id": "MDRW2100023933.1.1",
          "form": "요즘 건강관리는 어떻게하세요?",
          "original_form": "요즘 건강관리는 어떻게하세요?",
          "speaker_id": "1",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023933.1.2",
          "form": "운동은 해야지 생각만 해서ㅜㅜ",
          "original_form": "운동은 해야지 생각만 해서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023933.1.3",
          "form": "잠 잘자고 밥 잘 먹고 밖에 안하는 듯 해요",
          "original_form": "잠 잘자고 밥 잘 먹고 밖에 안하는 듯 해요",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023933.1.4",
          "form": "ㅋㅋ 다들비슷하군요 저 살이 너무쪄서 충격먹었잖아요ㅜㅜ",
          "original_form": "ㅋㅋ 다들비슷하군요 저 살이 너무쪄서 충격먹었잖아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023933.1.5",
          "form": "잘자고 잘먹는게 건강에 좋긴하죠 ㅋㅋ 정신건강에도 좋고 ㅋㅋ",
          "original_form": "잘자고 잘먹는게 건강에 좋긴하죠 ㅋㅋ 정신건강에도 좋고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023933.1.6",
          "form": "나이가 들면 살이 확실히 붙는듯해요",
          "original_form": "나이가 들면 살이 확실히 붙는듯해요",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023933.1.7",
          "form": "저도 예전에는 막 신경안썼는데",
          "original_form": "저도 예전에는 막 신경안썼는데",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023933.1.8",
          "form": "여기저기 지방이",
          "original_form": "여기저기 지방이",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023933.1.9",
          "form": "헛 진짜 나잇살인가봐요ㅜㅜ",
          "original_form": "헛 진짜 나잇살인가봐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023933.1.10",
          "form": "더빼기힘들다던데",
          "original_form": "더빼기힘들다던데",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023933.1.11",
          "form": "그니까요 슬펐어요",
          "original_form": "그니까요 슬펐어요",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023933.1.12",
          "form": "두툼두툼",
          "original_form": "두툼두툼",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023933.1.13",
          "form": "자꾸특정부위만 더찌는느낌",
          "original_form": "자꾸특정부위만 더찌는느낌",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023933.1.14",
          "form": "저도 그렇습니다 중앙집중형을ㆍ",
          "original_form": "저도 그렇습니다 중앙집중형을ㆍ",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023933.1.15",
          "form": "내일 건강검진가는데 몸무게 걱정되네요ㅋ",
          "original_form": "내일 건강검진가는데 몸무게 걱정되네요ㅋ",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023933.1.16",
          "form": "내일 검진예약일이세요?",
          "original_form": "내일 검진예약일이세요?",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023933.1.17",
          "form": "일찍 쉬셔야겠어요",
          "original_form": "일찍 쉬셔야겠어요",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023933.1.18",
          "form": "중앙집중ㅋㅋㅋ 이해가되버렸어요ㅠㅜ 내일예약했어요!",
          "original_form": "중앙집중ㅋㅋㅋ 이해가되버렸어요ㅠㅜ 내일예약했어요!",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023933.1.19",
          "form": "저는 전날 괜히 떨리더라구요",
          "original_form": "저는 전날 괜히 떨리더라구요",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023933.1.20",
          "form": "8시부터 금식하래서 지금 금식중ㅜㅜ",
          "original_form": "8시부터 금식하래서 지금 금식중ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023933.1.21",
          "form": "일찍주무셔요 늦게까지 깨어있음 막 배고파지더라구요 저는ㅋㅋㅋ",
          "original_form": "일찍주무셔요 늦게까지 깨어있음 막 배고파지더라구요 저는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100023933.1.22",
          "form": "네네ㅠ 안그래도 든든히먹을까하다 몸무게걱정되서 저녁건너뛰고 바나나하나 먹었더니 벌써 배고픈 느낌이에요",
          "original_form": "네네ㅠ 안그래도 든든히먹을까하다 몸무게걱정되서 저녁건너뛰고 바나나하나 먹었더니 벌써 배고픈 느낌이에요",
          "speaker_id": "1",
          "time": "20210830 20:27"
        }
      ]
    }
  ]
