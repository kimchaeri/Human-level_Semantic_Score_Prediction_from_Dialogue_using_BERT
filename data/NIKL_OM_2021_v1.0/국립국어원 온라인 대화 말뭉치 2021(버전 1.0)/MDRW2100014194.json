      "utterance": [
        {
          "id": "MDRW2100014194.1.1",
          "form": "연애하시나요??",
          "original_form": "연애하시나요??",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014194.1.2",
          "form": "아뇨아뇨 지금은 안하고 있습니다! 연애하시나요?!",
          "original_form": "아뇨아뇨 지금은 안하고 있습니다! 연애하시나요?!",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014194.1.3",
          "form": "저두 안하고 있어요!!",
          "original_form": "저두 안하고 있어요!!",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014194.1.4",
          "form": "언제가 마지막 연애세요..?",
          "original_form": "언제가 마지막 연애세요..?",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014194.1.5",
          "form": "오오 그렇담 이상형은 어케 되세요??",
          "original_form": "오오 그렇담 이상형은 어케 되세요??",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014194.1.6",
          "form": "아 쓰읍...슬프지만",
          "original_form": "아 쓰읍...슬프지만",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014194.1.7",
          "form": "군대가고 어쩌고 저쩌고 지내다 헤어지고 난 이후로 안하고 있네요",
          "original_form": "군대가고 어쩌고 저쩌고 지내다 헤어지고 난 이후로 안하고 있네요",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014194.1.8",
          "form": "아아..그러시구나",
          "original_form": "아아..그러시구나",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014194.1.9",
          "form": "제 이상형은",
          "original_form": "제 이상형은",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014194.1.10",
          "form": "음...똑똑한사람이요!",
          "original_form": "음...똑똑한사람이요!",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014194.1.11",
          "form": "학력이 좋거나 그런것보다",
          "original_form": "학력이 좋거나 그런것보다",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014194.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.13",
          "form": "그냥 세상바라보는게 똑똑한거 있잖아요!",
          "original_form": "그냥 세상바라보는게 똑똑한거 있잖아요!",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.14",
          "form": "그런사람이 좋더라구요",
          "original_form": "그런사람이 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.15",
          "form": "오오 그거 중요하죠",
          "original_form": "오오 그거 중요하죠",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.17",
          "form": "님은 어떤 사람이",
          "original_form": "님은 어떤 사람이",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.18",
          "form": "큰일도 스무스하게 넘어가는",
          "original_form": "큰일도 스무스하게 넘어가는",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.19",
          "form": "이상형인가요??",
          "original_form": "이상형인가요??",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.20",
          "form": "저는 귀여운 사람을 좋아해요",
          "original_form": "저는 귀여운 사람을 좋아해요",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.21",
          "form": "약간 생긴게 귀여운거보다는 행동이 귀여운거요",
          "original_form": "약간 생긴게 귀여운거보다는 행동이 귀여운거요",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014194.1.22",
          "form": "아아 뭔지알죠 ㅋㅋㅋ",
          "original_form": "아아 뭔지알죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014194.1.23",
          "form": "약간 영화보면서 리액션할 때 귀엽거나",
          "original_form": "약간 영화보면서 리액션할 때 귀엽거나",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014194.1.24",
          "form": "똑똑하게 산다고 느낄때는 언제세요?",
          "original_form": "똑똑하게 산다고 느낄때는 언제세요?",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014194.1.25",
          "form": "음..그냥 뉴스같은거 꾸준히보고",
          "original_form": "음..그냥 뉴스같은거 꾸준히보고",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014194.1.26",
          "form": "뭔가 친구들 만났을때 싸보이는 대화보다는",
          "original_form": "뭔가 친구들 만났을때 싸보이는 대화보다는",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014194.1.27",
          "form": "세상이슈에 대해 이야기하는..?",
          "original_form": "세상이슈에 대해 이야기하는..?",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014194.1.28",
          "form": "그런거 옆에 듣고 있으면 재밌거든요 ㅋㅋㅋ",
          "original_form": "그런거 옆에 듣고 있으면 재밌거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014194.1.29",
          "form": "!!!!",
          "original_form": "!!!!",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014194.1.30",
          "form": "써먹어야지 나중에",
          "original_form": "써먹어야지 나중에",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014194.1.31",
          "form": "세상...일..말하는...사람..(메모",
          "original_form": "세상...일..말하는...사람..(메모",
