        },
        {
          "id": "MDRW2100020636.1.2",
          "form": "근데 다들 시험이라 긴장해서 뻣뻣하게 했던 기억이..ㅋㅋ",
          "original_form": "근데 다들 시험이라 긴장해서 뻣뻣하게 했던 기억이..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020636.1.3",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020636.1.4",
          "form": "하긴 저도 시험이라면 엄청 떨릴것같아요",
