      "utterance": [
        {
          "id": "MDRW2100046584.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20211029 17:39"
        },
        {
          "id": "MDRW2100046584.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211029 17:40"
        },
        {
          "id": "MDRW2100046584.1.3",
          "form": "할로윈 즐겨보신 적 있으세요?",
          "original_form": "할로윈 즐겨보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20211029 17:41"
        },
        {
          "id": "MDRW2100046584.1.4",
          "form": "곧 할로윈이네요 ㅎㅎ",
          "original_form": "곧 할로윈이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211029 17:41"
        },
        {
          "id": "MDRW2100046584.1.5",
          "form": "그러네요..",
          "original_form": "그러네요..",
          "speaker_id": "1",
          "time": "20211029 17:41"
        },
        {
          "id": "MDRW2100046584.1.6",
          "form": "어릴때 다른집가서 사탕 얻고 그랬었는데요 ",
          "original_form": "어릴때 다른집가서 사탕 얻고 그랬었는데요 ",
          "speaker_id": "1",
          "time": "20211029 17:42"
        },
        {
          "id": "MDRW2100046584.1.7",
          "form": "오오 제대로 즐기셨네요 ㅋㅋㅋ",
          "original_form": "오오 제대로 즐기셨네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211029 17:42"
        },
        {
          "id": "MDRW2100046584.1.8",
          "form": "요즘은 그런것도 없네용..",
          "original_form": "요즘은 그런것도 없네용..",
          "speaker_id": "1",
          "time": "20211029 17:42"
        },
        {
          "id": "MDRW2100046584.1.9",
          "form": "저는 완전히 잊고 있다가 오늘 출근길에 어떤분이 오징어게임 코스프레 한 것 보고 알았어요",
          "original_form": "저는 완전히 잊고 있다가 오늘 출근길에 어떤분이 오징어게임 코스프레 한 것 보고 알았어요",
          "speaker_id": "2",
          "time": "20211029 17:42"
        },
        {
          "id": "MDRW2100046584.1.10",
          "form": "아...아...",
          "original_form": "아...아...",
          "speaker_id": "1",
          "time": "20211029 17:42"
        },
        {
          "id": "MDRW2100046584.1.11",
          "form": "버스 탔는데",
          "original_form": "버스 탔는데",
          "speaker_id": "2",
          "time": "20211029 17:43"
        },
        {
          "id": "MDRW2100046584.1.12",
          "form": "딱 타시더라구요 ㅋㅋㅋ",
          "original_form": "딱 타시더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211029 17:43"
        },
        {
          "id": "MDRW2100046584.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211029 17:43"
        },
        {
          "id": "MDRW2100046584.1.14",
          "form": "다들 완전 쳐다보고 시선 집중됐어요 ㅋㅋㅋㅋㅋ",
          "original_form": "다들 완전 쳐다보고 시선 집중됐어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211029 17:43"
        },
        {
          "id": "MDRW2100046584.1.15",
          "form": "힠.....ㅋㅋ..ㅋ",
          "original_form": "힠.....ㅋㅋ..ㅋ",
          "speaker_id": "1",
          "time": "20211029 17:43"
        },
        {
          "id": "MDRW2100046584.1.16",
          "form": "어제 까지 시험보고 오늘좀 쉬는데 내일 서울갈려구요",
          "original_form": "어제 까지 시험보고 오늘좀 쉬는데 내일 서울갈려구요",
          "speaker_id": "1",
          "time": "20211029 17:43"
        },
        {
          "id": "MDRW2100046584.1.17",
          "form": "오 그럼 다양한 코스프레 구경하실 수 있겠네요 ㅋㅋㅋ",
          "original_form": "오 그럼 다양한 코스프레 구경하실 수 있겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211029 17:44"
        },
        {
          "id": "MDRW2100046584.1.18",
          "form": "시험망쳐서 그런 기분날지 모르겟어요;ㅋㅋ",
          "original_form": "시험망쳐서 그런 기분날지 모르겟어요;ㅋㅋ",
          "speaker_id": "1",
          "time": "20211029 17:44"
        },
        {
          "id": "MDRW2100046584.1.19",
          "form": "이태원이나 홍대 쪽 가면 엄청 많잖아요",
          "original_form": "이태원이나 홍대 쪽 가면 엄청 많잖아요",
          "speaker_id": "2",
          "time": "20211029 17:44"
        },
        {
          "id": "MDRW2100046584.1.20",
          "form": "서울 어디 가실 거예요~?",
          "original_form": "서울 어디 가실 거예요~?",
          "speaker_id": "2",
          "time": "20211029 17:45"
        },
        {
          "id": "MDRW2100046584.1.21",
          "form": "롯데월드.....갈듯해요",
          "original_form": "롯데월드.....갈듯해요",
          "speaker_id": "1",
          "time": "20211029 17:45"
        },
        {
          "id": "MDRW2100046584.1.22",
          "form": "헉 거기도 할로윈 파티 기간이겠네요 ㅋㅋㅋ",
