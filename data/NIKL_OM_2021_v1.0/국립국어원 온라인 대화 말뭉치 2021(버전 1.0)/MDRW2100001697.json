      "utterance": [
        {
          "id": "MDRW2100001697.1.1",
          "form": "운동하시는거 있으세요?",
          "original_form": "운동하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100001697.1.2",
          "form": "지금 다리 다쳐서 아무것도 못 하고 있어요ㅋㅋㅋ",
          "original_form": "지금 다리 다쳐서 아무것도 못 하고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:28"
        },
        {
          "id": "MDRW2100001697.1.3",
          "form": "헉 어쩌다가 다치셨어요",
          "original_form": "헉 어쩌다가 다치셨어요",
          "speaker_id": "1",
          "time": "20210819 18:28"
        },
        {
          "id": "MDRW2100001697.1.4",
          "form": "비가 엄청 와서 길이 미끄러웠나봐요ㅠㅜ넘어졌어요ㅜ",
          "original_form": "비가 엄청 와서 길이 미끄러웠나봐요ㅠㅜ넘어졌어요ㅜ",
          "speaker_id": "2",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001697.1.5",
          "form": "ㅜㅜ뼈는 안상하셨나요",
          "original_form": "ㅜㅜ뼈는 안상하셨나요",
          "speaker_id": "1",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100001697.1.6",
          "form": "뼈는 괜찮은데 인대가 부었다고 하더라고요ㅜ",
          "original_form": "뼈는 괜찮은데 인대가 부었다고 하더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001697.1.7",
          "form": "아이고 ㅜ 당분간 운동은 못하겠네요",
          "original_form": "아이고 ㅜ 당분간 운동은 못하겠네요",
          "speaker_id": "1",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001697.1.8",
          "form": "붕대 감아두나요?",
          "original_form": "붕대 감아두나요?",
          "speaker_id": "1",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001697.1.9",
          "form": "반깁스 중이예요ㅠㅜ",
          "original_form": "반깁스 중이예요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001697.1.10",
          "form": "레이저 치료 받는데 금액이 장난 아니더라고요ㅜ",
          "original_form": "레이저 치료 받는데 금액이 장난 아니더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001697.1.11",
          "form": "ㅜㅜㅜ깁스 나중에 또 풀면 근육다빠져서 그것도 힘들던데",
          "original_form": "ㅜㅜㅜ깁스 나중에 또 풀면 근육다빠져서 그것도 힘들던데",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001697.1.12",
          "form": "맞아요 병원도 주기적으로 가야되고 ㅜ 고생이에요",
          "original_form": "맞아요 병원도 주기적으로 가야되고 ㅜ 고생이에요",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001697.1.13",
          "form": "매일매일 다니고 있는데 너무 힘들어요ㅠㅜ",
          "original_form": "매일매일 다니고 있는데 너무 힘들어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100001697.1.14",
          "form": "진짜 어디 안 다치고 건강한 게 최고인 것 같아요",
          "original_form": "진짜 어디 안 다치고 건강한 게 최고인 것 같아요",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001697.1.15",
          "form": "맞아요 저도 최근에 손을다쳐서 반깁스한달했었거든요ㅜ",
          "original_form": "맞아요 저도 최근에 손을다쳐서 반깁스한달했었거든요ㅜ",
          "speaker_id": "1",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001697.1.16",
          "form": "삶의질 진짜 뚝뚝떨어져요",
          "original_form": "삶의질 진짜 뚝뚝떨어져요",
          "speaker_id": "1",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001697.1.17",
          "form": "진짜 너무 불편하죠ㅠㅜ저도 전에 엄지손가락 다쳐서 깁스했는데 오만게 다 불편하고 힘들더라고요ㅋㅋ",
          "original_form": "진짜 너무 불편하죠ㅠㅜ저도 전에 엄지손가락 다쳐서 깁스했는데 오만게 다 불편하고 힘들더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100001697.1.18",
          "form": "ㅜㅜ재활아직도 남았는데 병원가는거자체도 넘귀찮고",
          "original_form": "ㅜㅜ재활아직도 남았는데 병원가는거자체도 넘귀찮고",
          "speaker_id": "1",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100001697.1.19",
          "form": "그래도 재활이 진짜 중요하더라고요 귀찮아도 건강 생각해서 다녀오세요ㅜ",
          "original_form": "그래도 재활이 진짜 중요하더라고요 귀찮아도 건강 생각해서 다녀오세요ㅜ",
          "speaker_id": "2",
          "time": "20210819 18:46"
        }
      ]
    }
