      "utterance": [
        {
          "id": "MDRW2100003038.1.1",
          "form": "애들이 저녁 먹으면서 해리포터 보고 싶다네",
          "original_form": "애들이 저녁 먹으면서 해리포터 보고 싶다네",
          "speaker_id": "1",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003038.1.2",
          "form": "안돼 먹으면서 보면 많이 흘려서",
          "original_form": "안돼 먹으면서 보면 많이 흘려서",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003038.1.3",
          "form": "ㅎㅎ 그럴라나",
          "original_form": "ㅎㅎ 그럴라나",
          "speaker_id": "1",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003038.1.4",
          "form": "한두번 당했어야지 ㅎㅎ",
          "original_form": "한두번 당했어야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003038.1.5",
          "form": "빨리 먹고 같이 보자고 해",
          "original_form": "빨리 먹고 같이 보자고 해",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003038.1.6",
          "form": "ㅇㅇ 그래야겠네",
          "original_form": "ㅇㅇ 그래야겠네",
          "speaker_id": "1",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003038.1.7",
          "form": "배달은 왔어?",
          "original_form": "배달은 왔어?",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003038.1.8",
          "form": "ㅇㅇ 방금 도착했어",
          "original_form": "ㅇㅇ 방금 도착했어",
          "speaker_id": "1",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003038.1.9",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "2",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003038.1.10",
          "form": "그냥 그럭저럭",
          "original_form": "그냥 그럭저럭",
          "speaker_id": "1",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003038.1.11",
          "form": "양은 많진 않아",
          "original_form": "양은 많진 않아",
          "speaker_id": "1",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003038.1.12",
          "form": "그 윗단계를 시켰어야 되는구만",
          "original_form": "그 윗단계를 시켰어야 되는구만",
          "speaker_id": "2",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003038.1.13",
          "form": "처음 시켜봐서 우리가 알 수가 없지 ㅎㅎ",
          "original_form": "처음 시켜봐서 우리가 알 수가 없지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003038.1.14",
          "form": "ㅇㅇ 다음에 참고해서 시키면 되지",
          "original_form": "ㅇㅇ 다음에 참고해서 시키면 되지",
          "speaker_id": "2",
          "time": "20210820 17:54"
        },
        {
          "id": "MDRW2100003038.1.15",
          "form": "아무튼 애들은 먹으면서 보는 건 안된다고 전해줘",
          "original_form": "아무튼 애들은 먹으면서 보는 건 안된다고 전해줘",
          "speaker_id": "2",
          "time": "20210820 17:55"
        },
        {
          "id": "MDRW2100003038.1.16",
          "form": "ㅇㅇ 알았어요",
          "original_form": "ㅇㅇ 알았어요",
          "speaker_id": "1",
          "time": "20210820 17:55"
        },
        {
          "id": "MDRW2100003038.1.17",
          "form": "다 먹고 보자고 할께",
          "original_form": "다 먹고 보자고 할께",
          "speaker_id": "1",
          "time": "20210820 17:55"
        },
        {
          "id": "MDRW2100003038.1.18",
          "form": "오키 나 퇴근해서 빨리 갈께",
          "original_form": "오키 나 퇴근해서 빨리 갈께",
          "speaker_id": "2",
          "time": "20210820 17:55"
        },
        {
          "id": "MDRW2100003038.1.19",
          "form": "ㅇㅇ 조심해서 와요",
          "original_form": "ㅇㅇ 조심해서 와요",
          "speaker_id": "1",
          "time": "20210820 17:55"
        }
      ]
