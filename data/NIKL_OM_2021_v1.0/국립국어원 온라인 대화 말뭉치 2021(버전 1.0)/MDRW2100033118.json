      "utterance": [
        {
          "id": "MDRW2100033118.1.1",
          "form": "하도 방역대책이 많아서",
          "original_form": "하도 방역대책이 많아서",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033118.1.2",
          "form": "뭐가뭔지 잘 기억이안나네요",
          "original_form": "뭐가뭔지 잘 기억이안나네요",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033118.1.3",
          "form": "6-8인까지 허용한다하던데",
          "original_form": "6-8인까지 허용한다하던데",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033118.1.4",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033118.1.5",
          "form": "너무 중구난방이고",
          "original_form": "너무 중구난방이고",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033118.1.6",
          "form": "따로 알려주지도 않고.....",
          "original_form": "따로 알려주지도 않고.....",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033118.1.7",
          "form": "다른 나라도 이렇게 복잡할까요?",
          "original_form": "다른 나라도 이렇게 복잡할까요?",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100033118.1.8",
          "form": "단계도 외우기피곤한데",
          "original_form": "단계도 외우기피곤한데",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100033118.1.9",
          "form": "이런거까지 다 어떻게 알고 하란건지",
          "original_form": "이런거까지 다 어떻게 알고 하란건지",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033118.1.10",
          "form": "그러니깐 말이에요",
          "original_form": "그러니깐 말이에요",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033118.1.11",
          "form": "그러게요... 다른 나라는 어떠려나...",
          "original_form": "그러게요... 다른 나라는 어떠려나...",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033118.1.12",
          "form": "식구 중에 작은언니네가 미국에 살고있는데",
          "original_form": "식구 중에 작은언니네가 미국에 살고있는데",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033118.1.13",
          "form": "거기는 정말 마스크를 안쓰려고 한대요 ㅠㅠㅋㅋ",
          "original_form": "거기는 정말 마스크를 안쓰려고 한대요 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033118.1.14",
          "form": "엥 왜그러죠 답답해서그런가",
          "original_form": "엥 왜그러죠 답답해서그런가",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100033118.1.15",
          "form": "음 그것도 그런데",
          "original_form": "음 그것도 그런데",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033118.1.16",
          "form": "자유에 대한",
          "original_form": "자유에 대한",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033118.1.17",
          "form": "침해라고 생각한대요...!",
          "original_form": "침해라고 생각한대요...!",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033118.1.18",
          "form": "아 우리나라랑 가치관이 다르군요",
          "original_form": "아 우리나라랑 가치관이 다르군요",
          "speaker_id": "2",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100033118.1.19",
          "form": "네네 정말 달라요!",
          "original_form": "네네 정말 달라요!",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100033118.1.20",
          "form": "그리고 거기는 총기소지가 가능하니까",
          "original_form": "그리고 거기는 총기소지가 가능하니까",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100033118.1.21",
          "form": "얼굴을 가리는 사람을 두려워하기도 하구요ㅠ",
          "original_form": "얼굴을 가리는 사람을 두려워하기도 하구요ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100033118.1.22",
          "form": "아 범죄가 많아서 그런가봐요",
          "original_form": "아 범죄가 많아서 그런가봐요",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100033118.1.23",
          "form": "나라마다 사정이 참 다양하네요",
          "original_form": "나라마다 사정이 참 다양하네요",
          "speaker_id": "2",
          "time": "20210906 18:27"
        },
        {
          "id": "MDRW2100033118.1.24",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100033118.1.25",
          "form": "근데 코로나 때문에 정말",
          "original_form": "근데 코로나 때문에 정말",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100033118.1.26",
          "form": "너무많은 사람들이 죽었는데....",
          "original_form": "너무많은 사람들이 죽었는데....",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100033118.1.27",
          "form": "하여튼 방역 대책 때문에 자영업자들 더 힘들어졌을거예요 ㅠㅠㅠ",
          "original_form": "하여튼 방역 대책 때문에 자영업자들 더 힘들어졌을거예요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100033118.1.28",
          "form": "맞아요 정말 말도안되는 대책이라",
          "original_form": "맞아요 정말 말도안되는 대책이라",
          "speaker_id": "2",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100033118.1.29",
          "form": "제가 자영업자면 너무 화났을거같아요",
          "original_form": "제가 자영업자면 너무 화났을거같아요",
          "speaker_id": "2",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100033118.1.30",
          "form": "맞아요... 그냥 굶어죽으라는건지",
          "original_form": "맞아요... 그냥 굶어죽으라는건지",
          "speaker_id": "1",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100033118.1.31",
          "form": "그러면서 지원금에 목매달게 하는 술책 같아요ㅠ",
          "original_form": "그러면서 지원금에 목매달게 하는 술책 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:27"
        }
      ]
    }
