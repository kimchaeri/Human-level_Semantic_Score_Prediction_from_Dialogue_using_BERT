      "utterance": [
        {
          "id": "MDRW2100013954.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013954.1.2",
          "form": "안녕하세요 가족과함께 사시나요?",
          "original_form": "안녕하세요 가족과함께 사시나요?",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013954.1.3",
          "form": "네 전 가족이랑 살아요",
          "original_form": "네 전 가족이랑 살아요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013954.1.4",
          "form": "혹시 가족과 함께 사시나요?",
          "original_form": "혹시 가족과 함께 사시나요?",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013954.1.5",
          "form": "네 그래서 집안일도 분담해서 하는거 같아요 ㅋㅋ",
          "original_form": "네 그래서 집안일도 분담해서 하는거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013954.1.6",
          "form": "맞아요 같이 살면 집안일 분담이에요",
          "original_form": "맞아요 같이 살면 집안일 분담이에요",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013954.1.7",
          "form": "그래도 가족과 같이 사는게 편해요",
          "original_form": "그래도 가족과 같이 사는게 편해요",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013954.1.8",
          "form": "돈도 절약되고요 ㅋㅋ",
          "original_form": "돈도 절약되고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013954.1.9",
          "form": "그건 맞아요",
          "original_form": "그건 맞아요",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013954.1.10",
          "form": "가족구성원은 어떻게 되나요?",
          "original_form": "가족구성원은 어떻게 되나요?",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013954.1.11",
          "form": "부모님, 동생이랑 살아요",
          "original_form": "부모님, 동생이랑 살아요",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013954.1.12",
          "form": "혹시 가족구성원이 어떻게 되시나요?",
          "original_form": "혹시 가족구성원이 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013954.1.13",
          "form": "부모님하고 언니 오빠요",
          "original_form": "부모님하고 언니 오빠요",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013954.1.14",
          "form": "언니는 곧 결혼해요",
          "original_form": "언니는 곧 결혼해요",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013954.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013954.1.16",
          "form": "집안에 사람 많으면 설거지 많지 않나요?ㅜ",
          "original_form": "집안에 사람 많으면 설거지 많지 않나요?ㅜ",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013954.1.17",
          "form": "맞아요 그래서 식기세척기를 사야하나싶어요",
          "original_form": "맞아요 그래서 식기세척기를 사야하나싶어요",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013954.1.18",
          "form": "금방 더러워지고요",
          "original_form": "금방 더러워지고요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013954.1.19",
          "form": "진짜 돈들여서 식기세척기 사고싶어요ㅠ",
          "original_form": "진짜 돈들여서 식기세척기 사고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013954.1.20",
          "form": "가족많은집은 필수인듯요 ㅜ",
          "original_form": "가족많은집은 필수인듯요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013954.1.21",
          "form": "맞아요 요즘은 다산다 그러더라고요",
          "original_form": "맞아요 요즘은 다산다 그러더라고요",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013954.1.22",
          "form": "로봇청소기도 인기고요",
          "original_form": "로봇청소기도 인기고요",
          "speaker_id": "2",
          "time": "20210825 16:43"
        }
