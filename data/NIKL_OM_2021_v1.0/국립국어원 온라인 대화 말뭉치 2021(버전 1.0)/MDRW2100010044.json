          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010044.1.13",
          "form": "개당 5천원이고",
          "original_form": "개당 5천원이고",
          "speaker_id": "1",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010044.1.14",
