        },
        {
          "id": "MDRW2100046652.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 12:11"
        },
        {
          "id": "MDRW2100046652.1.3",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20211104 12:11"
        },
        {
          "id": "MDRW2100046652.1.4",
          "form": "커피 좋아합니다",
          "original_form": "커피 좋아합니다",
          "speaker_id": "1",
          "time": "20211104 12:12"
        },
        {
          "id": "MDRW2100046652.1.5",
          "form": "뜨거운거 드시나요?",
          "original_form": "뜨거운거 드시나요?",
          "speaker_id": "1",
          "time": "20211104 12:12"
        },
        {
          "id": "MDRW2100046652.1.6",
          "form": "저는 아이스 파긴 한데..!",
          "original_form": "저는 아이스 파긴 한데..!",
          "speaker_id": "2",
          "time": "20211104 12:12"
        },
        {
          "id": "MDRW2100046652.1.7",
          "form": "요즘 뜨거운 것도 가끔 먹어요 ㅋㅋ",
          "original_form": "요즘 뜨거운 것도 가끔 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 12:12"
        },
        {
          "id": "MDRW2100046652.1.8",
          "form": "추워서",
          "original_form": "추워서",
          "speaker_id": "1",
          "time": "20211104 12:12"
        },
        {
          "id": "MDRW2100046652.1.9",
          "form": "따뜻한거드세요",
          "original_form": "따뜻한거드세요",
          "speaker_id": "1",
          "time": "20211104 12:13"
        },
        {
          "id": "MDRW2100046652.1.10",
          "form": "감기걸려요",
          "original_form": "감기걸려요",
          "speaker_id": "1",
          "time": "20211104 12:13"
        },
        {
          "id": "MDRW2100046652.1.11",
          "form": "추워서 따뜻한 거 먹고싶다가도",
          "original_form": "추워서 따뜻한 거 먹고싶다가도",
          "speaker_id": "2",
          "time": "20211104 12:13"
        },
        {
          "id": "MDRW2100046652.1.12",
          "form": "시원하게 꿀꺽꿀꺽 마시고싶어서",
          "original_form": "시원하게 꿀꺽꿀꺽 마시고싶어서",
          "speaker_id": "2",
          "time": "20211104 12:13"
        },
        {
          "id": "MDRW2100046652.1.13",
          "form": "아이스라떼를 사게 되더라구요 ㅋㅋㅋ",
          "original_form": "아이스라떼를 사게 되더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 12:13"
        },
        {
          "id": "MDRW2100046652.1.14",
          "form": "얼죽아신가봐요",
          "original_form": "얼죽아신가봐요",
          "speaker_id": "1",
          "time": "20211104 12:14"
        },
        {
          "id": "MDRW2100046652.1.15",
          "form": "라떼는 시럽넣으세요?",
          "original_form": "라떼는 시럽넣으세요?",
          "speaker_id": "1",
          "time": "20211104 12:14"
        },
        {
          "id": "MDRW2100046652.1.16",
          "form": "옛날엔 강경 바닐라라떼였는데",
          "original_form": "옛날엔 강경 바닐라라떼였는데",
          "speaker_id": "2",
          "time": "20211104 12:14"
        },
        {
          "id": "MDRW2100046652.1.17",
          "form": "요즘엔 좀 어른이 돼서 시럽 안넣은 라떼 마셔요 ㅋㅋㅋ",
          "original_form": "요즘엔 좀 어른이 돼서 시럽 안넣은 라떼 마셔요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 12:14"
        },
        {
          "id": "MDRW2100046652.1.18",
          "form": "아ㅋㅋ우유최고죠",
          "original_form": "아ㅋㅋ우유최고죠",
          "speaker_id": "1",
          "time": "20211104 12:15"
        },
        {
          "id": "MDRW2100046652.1.19",
          "form": "라떼마시고싶네요",
          "original_form": "라떼마시고싶네요",
          "speaker_id": "1",
          "time": "20211104 12:15"
        },
        {
          "id": "MDRW2100046652.1.20",
          "form": "하루에 커피 몆 잔 마시세요?",
          "original_form": "하루에 커피 몆 잔 마시세요?",
          "speaker_id": "1",
          "time": "20211104 12:15"
        },
        {
          "id": "MDRW2100046652.1.21",
          "form": "라떼 넘 좋아해요",
          "original_form": "라떼 넘 좋아해요",
          "speaker_id": "2",
          "time": "20211104 12:15"
        },
        {
          "id": "MDRW2100046652.1.22",
          "form": "보통 1잔 마시고 더 마시고 싶으면 디카페인으로 마셔요!!",
          "original_form": "보통 1잔 마시고 더 마시고 싶으면 디카페인으로 마셔요!!",
          "speaker_id": "2",
          "time": "20211104 12:15"
        },
        {
          "id": "MDRW2100046652.1.23",
          "form": "하루에 커피 몇 잔 마시세요?",
          "original_form": "하루에 커피 몇 잔 마시세요?",
          "speaker_id": "2",
          "time": "20211104 12:15"
        },
        {
          "id": "MDRW2100046652.1.24",
          "form": "한잔요ㅋㅋ디카페인으로 마셔요",
          "original_form": "한잔요ㅋㅋ디카페인으로 마셔요",
          "speaker_id": "1",
          "time": "20211104 12:16"
        },
        {
          "id": "MDRW2100046652.1.25",
          "form": "카페인이 몸에 안받는 체질이셔요?",
          "original_form": "카페인이 몸에 안받는 체질이셔요?",
          "speaker_id": "2",
          "time": "20211104 12:16"
        },
        {
          "id": "MDRW2100046652.1.26",
          "form": "그건아닌데 혹시몰라서 디카페인마셔요 500원차이라...",
          "original_form": "그건아닌데 혹시몰라서 디카페인마셔요 500원차이라...",
          "speaker_id": "1",
          "time": "20211104 12:16"
        },
        {
          "id": "MDRW2100046652.1.27",
          "form": "아하~~~! 맛은 카페인 들어간게 더 맛있긴 해요!",
          "original_form": "아하~~~! 맛은 카페인 들어간게 더 맛있긴 해요!",
          "speaker_id": "2",
          "time": "20211104 12:17"
        },
        {
          "id": "MDRW2100046652.1.28",
          "form": "몸에 안 좋은 게 역시 맛있어요",
          "original_form": "몸에 안 좋은 게 역시 맛있어요",
          "speaker_id": "2",
          "time": "20211104 12:17"
        },
        {
          "id": "MDRW2100046652.1.29",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
