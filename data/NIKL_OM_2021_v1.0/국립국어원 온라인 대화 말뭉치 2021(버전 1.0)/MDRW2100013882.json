      "utterance": [
        {
          "id": "MDRW2100013882.1.1",
          "form": "좋아하시는 배달음식있으세요?!",
          "original_form": "좋아하시는 배달음식있으세요?!",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013882.1.2",
          "form": "저는 닭발이랑 치킨이요 ㅋㅋㅋㅋ",
          "original_form": "저는 닭발이랑 치킨이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013882.1.3",
          "form": "저두 치킨ㅋㅋㅋㅋㅎㅎㅎ 그리고 쌀국수도 좋아해요!",
          "original_form": "저두 치킨ㅋㅋㅋㅋㅎㅎㅎ 그리고 쌀국수도 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013882.1.4",
          "form": "마라탕 시켜보신적있나요?!",
          "original_form": "마라탕 시켜보신적있나요?!",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013882.1.5",
          "form": "마라탕은 배달 시켜서 먹은 적은 없고 가서는 먹어봣어요 ㅋㅋㅋ",
          "original_form": "마라탕은 배달 시켜서 먹은 적은 없고 가서는 먹어봣어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013882.1.6",
          "form": "저두요 ㅎㅎㅎㅎㅎ 근데 가는게 귀찮아서",
          "original_form": "저두요 ㅎㅎㅎㅎㅎ 근데 가는게 귀찮아서",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013882.1.7",
          "form": "그렇긴하져 근데 마라탕은 위생 논란 이후로",
          "original_form": "그렇긴하져 근데 마라탕은 위생 논란 이후로",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013882.1.8",
          "form": "먹기가 좀 그렇더라고요",
          "original_form": "먹기가 좀 그렇더라고요",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013882.1.9",
          "form": "배달 해보고 싶은데 맛없을까봐 걱정되서요",
          "original_form": "배달 해보고 싶은데 맛없을까봐 걱정되서요",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013882.1.10",
          "form": "아 마자요.    .....",
          "original_form": "아 마자요.    .....",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013882.1.11",
          "form": "저는 원래 고추바사삭 좋아하는데",
          "original_form": "저는 원래 고추바사삭 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013882.1.12",
          "form": "한번 포장하러 갔더니",
          "original_form": "한번 포장하러 갔더니",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013882.1.13",
          "form": "가게가 너무 더러워서 먹겠더라고요ㅠㅠ",
          "original_form": "가게가 너무 더러워서 먹겠더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013882.1.14",
          "form": "그런거 보면 진짜 먹을 맛이 떨어지져",
          "original_form": "그런거 보면 진짜 먹을 맛이 떨어지져",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013882.1.15",
          "form": "한번 가고 그 뒤로는 안가게되여",
          "original_form": "한번 가고 그 뒤로는 안가게되여",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013882.1.16",
          "form": "그러니까 덜 시켜먹게되는게 되는것같아요",
          "original_form": "그러니까 덜 시켜먹게되는게 되는것같아요",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013882.1.17",
          "form": "오을 점심은뭐드셨어요?",
          "original_form": "오을 점심은뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013882.1.18",
          "form": "오늘 아직 안 먹었는ㄷ0 벌써 4시네여",
          "original_form": "오늘 아직 안 먹었는ㄷ0 벌써 4시네여",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013882.1.19",
          "form": "저는 오늘 소고기뭇국먹었어요 ㅎㅎㅎ",
          "original_form": "저는 오늘 소고기뭇국먹었어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013882.1.20",
          "form": "그럼 첫끼 뭐드실지 계획은있으신가요?",
          "original_form": "그럼 첫끼 뭐드실지 계획은있으신가요?",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013882.1.21",
          "form": "콩국수요",
          "original_form": "콩국수요",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013882.1.22",
          "form": "오 저는 콩을 별로 안좋아하긴하는데 국수는 좋아해요ㅋㅋㅋ",
          "original_form": "오 저는 콩을 별로 안좋아하긴하는데 국수는 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013882.1.23",
          "form": "저도 콩은 별룬데 콩국수는 좋아해여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 콩은 별룬데 콩국수는 좋아해여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013882.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 앗ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 앗ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013882.1.25",
          "form": "약간 콩 모양이 살아있으면 싫은데",
          "original_form": "약간 콩 모양이 살아있으면 싫은데",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013882.1.26",
          "form": "형태가 사라지면 괜찮은거 같아여 ㅋㅋㅋㅋ",
          "original_form": "형태가 사라지면 괜찮은거 같아여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013882.1.27",
          "form": "그럼 두유도 좋아하시겠네요ㅋㅋㅋ",
          "original_form": "그럼 두유도 좋아하시겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013882.1.28",
          "form": "오 마자요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 마자요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013882.1.29",
          "form": "어릴때부터 두유 킬러거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "어릴때부터 두유 킬러거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013882.1.30",
          "form": "핰ㅋㅋㅋ 저는 안좋아해서 꾸역꾸역먹었어요",
          "original_form": "핰ㅋㅋㅋ 저는 안좋아해서 꾸역꾸역먹었어요",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013882.1.31",
          "form": "검은콩 두유 같은건 좀 더 달아서 맛있어욤 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "검은콩 두유 같은건 좀 더 달아서 맛있어욤 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013882.1.32",
          "form": "요즘 두유는 또 엄청 다양하더라고요 ㅋㅋㅋ",
          "original_form": "요즘 두유는 또 엄청 다양하더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013882.1.33",
          "form": "스벅 커스텀 좋아하시나요?",
          "original_form": "스벅 커스텀 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013882.1.34",
          "form": "아녀아녀",
          "original_form": "아녀아녀",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013882.1.35",
          "form": "아 두유 넣으실줄..!",
          "original_form": "아 두유 넣으실줄..!",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013882.1.36",
          "form": "ㅋㅋㅋㅋ 그정도는 아니에여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그정도는 아니에여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013882.1.37",
          "form": "저는 스벅 커스텀 짱좋아해서요ㅎㅎ",
          "original_form": "저는 스벅 커스텀 짱좋아해서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013882.1.38",
          "form": "에스프레소휩으로 꼭먹고 자바칩도 많이많이ㅎㅎ",
          "original_form": "에스프레소휩으로 꼭먹고 자바칩도 많이많이ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013882.1.39",
          "form": "오 저는 그냥 메뉴판에 있는그대로 먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "오 저는 그냥 메뉴판에 있는그대로 먹어요 ㅋㅋㅋㅋㅋ",
