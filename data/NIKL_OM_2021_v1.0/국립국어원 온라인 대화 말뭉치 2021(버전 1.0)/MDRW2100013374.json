      "utterance": [
        {
          "id": "MDRW2100013374.1.1",
          "form": "태풍때문에 비 많이 온다ㅠ",
          "original_form": "태풍때문에 비 많이 온다ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013374.1.2",
          "form": "남쪽 사세요?",
          "original_form": "남쪽 사세요?",
          "speaker_id": "2",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013374.1.3",
          "form": "북쪽인데 새벽부터 계속 비오고 있어요",
          "original_form": "북쪽인데 새벽부터 계속 비오고 있어요",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013374.1.4",
          "form": "아 저도 북쪽인데 저흰 비가 안오더라구요",
          "original_form": "아 저도 북쪽인데 저흰 비가 안오더라구요",
          "speaker_id": "2",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013374.1.5",
          "form": "비 구름이 이리저리 옮겨가고 있나봐요",
          "original_form": "비 구름이 이리저리 옮겨가고 있나봐요",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013374.1.6",
          "form": "습해서 힘드네요",
          "original_form": "습해서 힘드네요",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013374.1.7",
          "form": "진짜 꿉꿉해요",
          "original_form": "진짜 꿉꿉해요",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013374.1.8",
          "form": "빨래 절대 못하겠어요",
          "original_form": "빨래 절대 못하겠어요",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013374.1.9",
          "form": "맞아요 빨래 계속 밀리고",
          "original_form": "맞아요 빨래 계속 밀리고",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013374.1.10",
          "form": "일단 습해서 세탁물 나눠서 하고 있긴한데",
          "original_form": "일단 습해서 세탁물 나눠서 하고 있긴한데",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013374.1.11",
          "form": "그래도 집안이 습하니 몸이 축축 처지네요..",
          "original_form": "그래도 집안이 습하니 몸이 축축 처지네요..",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013374.1.12",
          "form": "제습기 있으신가요 저는 없는데 하나 들여야할 판이에요",
          "original_form": "제습기 있으신가요 저는 없는데 하나 들여야할 판이에요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013374.1.13",
          "form": "저도 제습기 없어요",
          "original_form": "저도 제습기 없어요",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013374.1.14",
          "form": "안타까워라",
          "original_form": "안타까워라",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013374.1.15",
          "form": "근데 제습기 틀면 더워진다는 소문도 있더라구요",
          "original_form": "근데 제습기 틀면 더워진다는 소문도 있더라구요",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013374.1.16",
          "form": "그게 사실이라면 집이 찜질방되겠네요..",
          "original_form": "그게 사실이라면 집이 찜질방되겠네요..",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013374.1.17",
          "form": "호달달",
          "original_form": "호달달",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013374.1.18",
          "form": "앜ㅋㅋㅋㅋ 진짜 더위나 습기 둘다 피할순없네요",
          "original_form": "앜ㅋㅋㅋㅋ 진짜 더위나 습기 둘다 피할순없네요",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013374.1.19",
          "form": "역시 여름은 살기가 힘드네요",
          "original_form": "역시 여름은 살기가 힘드네요",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013374.1.20",
          "form": "여름 과일 좋아하시나요",
          "original_form": "여름 과일 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013374.1.21",
          "form": "여름의 유일한 낙",
          "original_form": "여름의 유일한 낙",
          "speaker_id": "2",
          "time": "20210825 14:23"
        }
      ]
