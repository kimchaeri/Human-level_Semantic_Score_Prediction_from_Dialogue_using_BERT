        {
          "id": "MDRW2100018075.1.1",
          "form": "뭐보실건가요",
          "original_form": "뭐보실건가요",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018075.1.2",
          "form": "생각중이에요",
