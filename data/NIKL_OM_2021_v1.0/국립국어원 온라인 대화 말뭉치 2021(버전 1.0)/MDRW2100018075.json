          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018075.1.3",
          "form": "해리포터 추천이요",
          "original_form": "해리포터 추천이요",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018075.1.4",
          "form": "해리포터 다봤어요",
          "original_form": "해리포터 다봤어요",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018075.1.5",
          "form": "동물사전도 봤어요?",
          "original_form": "동물사전도 봤어요?",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018075.1.6",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018075.1.7",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018075.1.8",
          "form": "한번 찾아볼게요",
          "original_form": "한번 찾아볼게요",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018075.1.9",
          "form": "느와르좋아하세요",
          "original_form": "느와르좋아하세요",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
