      "utterance": [
        {
          "id": "MDRW2100008292.1.1",
          "form": "이런 느낌이에요",
          "original_form": "이런 느낌이에요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008292.1.2",
          "form": "군대..",
          "original_form": "군대..",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008292.1.3",
          "form": "저는 잘 몰라서 ㅎㅎ",
          "original_form": "저는 잘 몰라서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008292.1.4",
          "form": "뭐 아시는 거 있으세요?",
          "original_form": "뭐 아시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008292.1.5",
          "form": "어.. 음..",
          "original_form": "어.. 음..",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008292.1.6",
          "form": "군대..",
          "original_form": "군대..",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008292.1.7",
          "form": "주위에 군대간 사람 있으세요?",
          "original_form": "주위에 군대간 사람 있으세요?",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008292.1.8",
          "form": "음 사촌 오빠?",
          "original_form": "음 사촌 오빠?",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008292.1.9",
          "form": "친구 남친..",
          "original_form": "친구 남친..",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008292.1.10",
          "form": "그래도 요즘 군대는 연락 잘돼서",
          "original_form": "그래도 요즘 군대는 연락 잘돼서",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008292.1.11",
          "form": "그렇다더라구요",
          "original_form": "그렇다더라구요",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008292.1.12",
          "form": "다들 얘기 잘 하고 다니는것같더라고요",
          "original_form": "다들 얘기 잘 하고 다니는것같더라고요",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008292.1.13",
          "form": "핸드폰도 주고 인터넷도하고",
          "original_form": "핸드폰도 주고 인터넷도하고",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008292.1.14",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008292.1.15",
          "form": "주식하는 사람도",
          "original_form": "주식하는 사람도",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008292.1.16",
          "form": "되게 많은것같더라고요 ㅋㅋ",
          "original_form": "되게 많은것같더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008292.1.17",
          "form": "약간 가면 자기개발도하고",
          "original_form": "약간 가면 자기개발도하고",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008292.1.18",
          "form": "그런다더라구요",
          "original_form": "그런다더라구요",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008292.1.19",
          "form": "그래서 생각보다 괜찮다는 말도 하고",
          "original_form": "그래서 생각보다 괜찮다는 말도 하고",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008292.1.20",
          "form": "예전만큼 빡세지는 않은가봐요",
          "original_form": "예전만큼 빡세지는 않은가봐요",
          "speaker_id": "2",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008292.1.21",
          "form": "맞아요 그렇다고 들었어요",
          "original_form": "맞아요 그렇다고 들었어요",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008292.1.22",
          "form": "ㅎ.. 사실 저는 좀 부러워요",
          "original_form": "ㅎ.. 사실 저는 좀 부러워요",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008292.1.23",
          "form": "어떤 말인지 알거같아요",
          "original_form": "어떤 말인지 알거같아요",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008292.1.24",
          "form": "요즘 이력서 쓰는데 군대 얘기 참 많이들 적더라고요",
          "original_form": "요즘 이력서 쓰는데 군대 얘기 참 많이들 적더라고요",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008292.1.25",
          "form": "약간 사회가 허용한 자기개발시간 같은 느낌이라",
          "original_form": "약간 사회가 허용한 자기개발시간 같은 느낌이라",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008292.1.26",
          "form": "도움이 많이 되기는 하는 것 같아요",
          "original_form": "도움이 많이 되기는 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008292.1.27",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210822 14:54"
        }
      ]
    }
