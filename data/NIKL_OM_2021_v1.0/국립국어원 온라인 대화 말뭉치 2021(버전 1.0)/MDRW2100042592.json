      "utterance": [
        {
          "id": "MDRW2100042592.1.1",
          "form": "네네 아직 알아챈건ㅋㅋ",
          "original_form": "네네 아직 알아챈건ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042592.1.2",
          "form": "없다가도 생기고 그렇대요",
          "original_form": "없다가도 생기고 그렇대요",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042592.1.3",
          "form": "없다생기면 넘 슬플듯 ㅠㅠㅠㅠ",
          "original_form": "없다생기면 넘 슬플듯 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042592.1.4",
          "form": "아는 맛인데 못먹게 되면 비극이죠ㅜ",
          "original_form": "아는 맛인데 못먹게 되면 비극이죠ㅜ",
          "speaker_id": "2",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042592.1.5",
          "form": "제친구도 갑자기 게릉 먹으면 목이붓고",
          "original_form": "제친구도 갑자기 게릉 먹으면 목이붓고",
          "speaker_id": "1",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042592.1.6",
          "form": "숨이안쉬어진대요",
          "original_form": "숨이안쉬어진대요",
          "speaker_id": "1",
          "time": "20210907 00:00"
        },
        {
          "id": "MDRW2100042592.1.7",
          "form": "ㅠ 알러지없는거 큰 복이에요ㅜㅠ",
          "original_form": "ㅠ 알러지없는거 큰 복이에요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 07:00"
        },
        {
          "id": "MDRW2100042592.1.8",
          "form": "마자요... 맛잇는거편하게먹고살아야하는데ㅠ",
          "original_form": "마자요... 맛잇는거편하게먹고살아야하는데ㅠ",
          "speaker_id": "1",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042592.1.9",
          "form": "오늘도 간식을 위해 점심은 가볍게 고구마입니다",
          "original_form": "오늘도 간식을 위해 점심은 가볍게 고구마입니다",
          "speaker_id": "2",
          "time": "20210907 08:18"
        },
        {
          "id": "MDRW2100042592.1.10",
          "form": "오 간식.머드세용",
          "original_form": "오 간식.머드세용",
          "speaker_id": "1",
          "time": "20210907 08:19"
        },
        {
          "id": "MDRW2100042592.1.11",
          "form": "ㅋㅋ오늘 스벅 쿠키 먹으려고요!!! ㅋㅋ",
          "original_form": "ㅋㅋ오늘 스벅 쿠키 먹으려고요!!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042592.1.12",
          "form": "오 계획이 다 있으시근요 ㅋㅋㅋㅋ",
          "original_form": "오 계획이 다 있으시근요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042592.1.13",
          "form": "스벅쿠키는안먹어봑는데",
          "original_form": "스벅쿠키는안먹어봑는데",
          "speaker_id": "1",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042592.1.14",
          "form": "맛잇나요",
          "original_form": "맛잇나요",
          "speaker_id": "1",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042592.1.15",
          "form": "네 전 맛있는데 별로 유명하지 않더라구요",
          "original_form": "네 전 맛있는데 별로 유명하지 않더라구요",
          "speaker_id": "2",
          "time": "20210907 08:21"
        },
        {
          "id": "MDRW2100042592.1.16",
          "form": "무슨맛이에용?--",
          "original_form": "무슨맛이에용?--",
          "speaker_id": "1",
          "time": "20210907 08:21"
        },
        {
          "id": "MDRW2100042592.1.17",
          "form": "생크림카스테라보다 제 취향에는 더 맛나요",
          "original_form": "생크림카스테라보다 제 취향에는 더 맛나요",
          "speaker_id": "2",
          "time": "20210907 08:21"
        },
        {
          "id": "MDRW2100042592.1.18",
          "form": "요즘 뉴욕st 두툼한 쿠키 유햄인데",
          "original_form": "요즘 뉴욕st 두툼한 쿠키 유햄인데",
          "speaker_id": "2",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042592.1.19",
          "form": "그런 느낌처럼 달고 쫀득하고 커요",
          "original_form": "그런 느낌처럼 달고 쫀득하고 커요",
          "speaker_id": "2",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042592.1.20",
          "form": "오호...",
          "original_form": "오호...",
          "speaker_id": "1",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042592.1.21",
          "form": "가면 함 바야겟네요",
          "original_form": "가면 함 바야겟네요",
          "speaker_id": "1",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042592.1.22",
          "form": "네 바로 앞에 비치되어 있어요",
          "original_form": "네 바로 앞에 비치되어 있어요",
          "speaker_id": "2",
          "time": "20210907 08:24"
        },
        {
          "id": "MDRW2100042592.1.23",
          "form": "좋아하는 간식 있어요?",
          "original_form": "좋아하는 간식 있어요?",
