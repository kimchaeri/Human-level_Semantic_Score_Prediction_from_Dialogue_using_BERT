      "utterance": [
        {
          "id": "MDRW2100047396.1.1",
          "form": "집안일 뭘 제일 싫어하세요? ㅋㅋ",
          "original_form": "집안일 뭘 제일 싫어하세요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100047396.1.2",
          "form": "설거지죠 ㅋㅋ",
          "original_form": "설거지죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100047396.1.3",
          "form": "앗 저는 빨래개기...ㅋㅋㅋㅋㅋ",
          "original_form": "앗 저는 빨래개기...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047396.1.4",
          "form": "아 맞아요 은근 귀찮아서 밤에 해요 ㅋㅋ",
          "original_form": "아 맞아요 은근 귀찮아서 밤에 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047396.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ빨래개는게 왜이렇게 싫은지 모르겟어요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ빨래개는게 왜이렇게 싫은지 모르겟어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047396.1.6",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100047396.1.7",
          "form": "또 서랍에 넣기 가 더 싫어요",
          "original_form": "또 서랍에 넣기 가 더 싫어요",
          "speaker_id": "1",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100047396.1.8",
          "form": "맞아요 ㅋㅋㅋㅋㅋ정리정돈하는것도 일이에요",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ정리정돈하는것도 일이에요",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047396.1.9",
          "form": "전 정리정돈 꽝요",
          "original_form": "전 정리정돈 꽝요",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100047396.1.10",
          "form": "사실 집안일이 좋은게 없어요 ㅋㅋ",
          "original_form": "사실 집안일이 좋은게 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100047396.1.11",
          "form": "맞아요 ㅋㅋㅋㅋ그냥 지겹기만 해요",
          "original_form": "맞아요 ㅋㅋㅋㅋ그냥 지겹기만 해요",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100047396.1.12",
          "form": "ㅎㅎ아 주말이 젤싫어요",
          "original_form": "ㅎㅎ아 주말이 젤싫어요",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100047396.1.13",
          "form": "주말도 집안일 고고",
          "original_form": "주말도 집안일 고고",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100047396.1.14",
          "form": "주부는 주말이 없어요",
          "original_form": "주부는 주말이 없어요",
          "speaker_id": "1",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100047396.1.15",
          "form": "ㅋㅋㅋㅋㅋ저희 어머니께서도 주말 방학 제일 싫어하셧죠",
          "original_form": "ㅋㅋㅋㅋㅋ저희 어머니께서도 주말 방학 제일 싫어하셧죠",
          "speaker_id": "2",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100047396.1.16",
          "form": "네 제가 방학에 그랬죠 ㅎㅎ",
          "original_form": "네 제가 방학에 그랬죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100047396.1.17",
          "form": "방학아 사라져라",
          "original_form": "방학아 사라져라",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100047396.1.18",
          "form": "주말아 사라져라 ㅋㅋ",
          "original_form": "주말아 사라져라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100047396.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ월요일 너무 좋아하고 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ월요일 너무 좋아하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100047396.1.20",
          "form": "전 설거지 대충파요",
          "original_form": "전 설거지 대충파요",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100047396.1.21",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100047396.1.22",
          "form": "월요일 야호",
          "original_form": "월요일 야호",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100047396.1.23",
          "form": "꼼꼼파 인가요?",
          "original_form": "꼼꼼파 인가요?",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100047396.1.24",
          "form": "네 전 꼼꼼파라서",
          "original_form": "네 전 꼼꼼파라서",
          "speaker_id": "2",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100047396.1.25",
          "form": "ㅋㅋㅋㅋ집안일 빡세게 하고 널브러지는 타입...ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ집안일 빡세게 하고 널브러지는 타입...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100047396.1.26",
          "form": "어머 ㅋㅋ저 랑 반대네요",
          "original_form": "어머 ㅋㅋ저 랑 반대네요",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100047396.1.27",
          "form": "ㅋㅋ바람직해요",
          "original_form": "ㅋㅋ바람직해요",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100047396.1.28",
          "form": "전 서렁설렁해요",
          "original_form": "전 서렁설렁해요",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100047396.1.29",
          "form": "진짜 방전되서 눕겠어요",
          "original_form": "진짜 방전되서 눕겠어요",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100047396.1.30",
          "form": "전 정리정돈 안된걸 보면 스트레스받는타입이라 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 정리정돈 안된걸 보면 스트레스받는타입이라 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100047396.1.31",
          "form": "힘들어죽겟는데 꾸역꾸역 치우는 타입ㅋㅋㅋㅋ",
          "original_form": "힘들어죽겟는데 꾸역꾸역 치우는 타입ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100047396.1.32",
          "form": "어머 진짜 최고네요",
          "original_form": "어머 진짜 최고네요",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100047396.1.33",
          "form": "근데 몸이 ㅜㅜ",
          "original_form": "근데 몸이 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100047396.1.34",
          "form": "골병나요",
          "original_form": "골병나요",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100047396.1.35",
          "form": "집에만 좋아요",
          "original_form": "집에만 좋아요",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100047396.1.36",
          "form": "쉬엄쉬엄하세요",
          "original_form": "쉬엄쉬엄하세요",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100047396.1.37",
          "form": "맞아요 ㅋㅋㅋㅋㅋ정신건강을 얻고 체력을 잃어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ정신건강을 얻고 체력을 잃어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100047396.1.38",
          "form": "ㅋㅋ근데 어쩔수없죠",
          "original_form": "ㅋㅋ근데 어쩔수없죠",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100047396.1.39",
          "form": "홧팅입니다",
          "original_form": "홧팅입니다",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100047396.1.40",
          "form": "집안일 안하고 깨끗한 집에서 살고 싶은데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "집안일 안하고 깨끗한 집에서 살고 싶은데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100047396.1.41",
          "form": "아 그럼사모님",
          "original_form": "아 그럼사모님",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100047396.1.42",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100047396.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ돈이없ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ돈이없ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100047396.1.44",
          "form": "누가 해줌 좋은데",
          "original_form": "누가 해줌 좋은데",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100047396.1.45",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100047396.1.46",
          "form": "젤중요한게 없네요 ㅜㅜㅋㅋ",
          "original_form": "젤중요한게 없네요 ㅜㅜㅋㅋ",
          "speaker_id": "1",
