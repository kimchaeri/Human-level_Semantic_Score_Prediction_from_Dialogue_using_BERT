      "utterance": [
        {
          "id": "MDRW2100039171.1.1",
          "form": "배달 자주드시나요",
          "original_form": "배달 자주드시나요",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100039171.1.2",
          "form": "네 제 소울메이트죠",
          "original_form": "네 제 소울메이트죠",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100039171.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100039171.1.4",
          "form": "어우 저도 단군이래 배민 븨아피를 놓친 적이 없습니다",
          "original_form": "어우 저도 단군이래 배민 븨아피를 놓친 적이 없습니다",
          "speaker_id": "2",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100039171.1.5",
          "form": "무슨 메뉴 좋아하시나요",
          "original_form": "무슨 메뉴 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100039171.1.6",
          "form": "회!좋아합니다",
          "original_form": "회!좋아합니다",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100039171.1.7",
          "form": "님은 주로 뭐 시키세요?",
          "original_form": "님은 주로 뭐 시키세요?",
          "speaker_id": "1",
          "time": "20210903 12:30"
        },
        {
          "id": "MDRW2100039171.1.8",
          "form": "헉 날로 먹는 거 너무 좋죠 마치 인생처럼",
          "original_form": "헉 날로 먹는 거 너무 좋죠 마치 인생처럼",
          "speaker_id": "2",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100039171.1.9",
          "form": "저는 주로 치킨이나 오돌뼈를 먹어요",
          "original_form": "저는 주로 치킨이나 오돌뼈를 먹어요",
          "speaker_id": "2",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100039171.1.10",
          "form": "야식으로 딱이죠",
          "original_form": "야식으로 딱이죠",
          "speaker_id": "2",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100039171.1.11",
          "form": "ㅋㅋ 명언이네요ㅎ",
          "original_form": "ㅋㅋ 명언이네요ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100039171.1.12",
          "form": "오돌뼈는 배달 안해봤는데 잘 오나요!!!",
          "original_form": "오돌뼈는 배달 안해봤는데 잘 오나요!!!",
          "speaker_id": "1",
          "time": "20210903 12:32"
        },
        {
          "id": "MDRW2100039171.1.13",
          "form": "네 괜찮은집 찾아두면 정말 맛있더라구요",
          "original_form": "네 괜찮은집 찾아두면 정말 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 12:32"
        },
        {
          "id": "MDRW2100039171.1.14",
          "form": "매운 거 잘 드시나요?",
          "original_form": "매운 거 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210903 12:32"
        },
        {
          "id": "MDRW2100039171.1.15",
          "form": "맵고 짠거 사랑합니다ㅎㅎ",
          "original_form": "맵고 짠거 사랑합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100039171.1.16",
          "form": "님도 잘 드시나용?",
          "original_form": "님도 잘 드시나용?",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100039171.1.17",
          "form": "아뇨 저는 매운 거 잘 못먹어용",
          "original_form": "아뇨 저는 매운 거 잘 못먹어용",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100039171.1.18",
          "form": "맵찔이예여..",
          "original_form": "맵찔이예여..",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100039171.1.19",
          "form": "그치만 맛잇게 매운 건 포기할수가 없습죠",
          "original_form": "그치만 맛잇게 매운 건 포기할수가 없습죠",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100039171.1.20",
          "form": "ㅜㅇㅜ 오돌뼈 좋아하시길래ㅎ",
          "original_form": "ㅜㅇㅜ 오돌뼈 좋아하시길래ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100039171.1.21",
          "form": "진짜 맛있게 매운집이어야겠네요!!",
          "original_form": "진짜 맛있게 매운집이어야겠네요!!",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100039171.1.22",
          "form": "불닭볶음면은 가능한가요",
          "original_form": "불닭볶음면은 가능한가요",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100039171.1.23",
          "form": "헉 아니요 그건",
          "original_form": "헉 아니요 그건",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100039171.1.24",
          "form": "도전해볼 엄두도 못내봤고",
          "original_form": "도전해볼 엄두도 못내봤고",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100039171.1.25",
          "form": "까르보크림불닭 그건 얼마 전에 먹어봤어요!!",
          "original_form": "까르보크림불닭 그건 얼마 전에 먹어봤어요!!",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100039171.1.26",
          "form": "불닭볶음면 좋아하세요?",
          "original_form": "불닭볶음면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100039171.1.27",
          "form": "달아서 별로 안좋아하는데",
          "original_form": "달아서 별로 안좋아하는데",
          "speaker_id": "1",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100039171.1.28",
          "form": "까르보불닭은 좋아해요11",
          "original_form": "까르보불닭은 좋아해요11",
