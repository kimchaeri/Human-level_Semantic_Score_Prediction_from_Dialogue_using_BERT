      "utterance": [
        {
          "id": "MDRW2100020286.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020286.1.2",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020286.1.3",
          "form": "안녕하세요 직장인이에요",
          "original_form": "안녕하세요 직장인이에요",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020286.1.4",
          "form": "name1은 학생이신가요?",
          "original_form": "&name1&은 학생이신가요?",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020286.1.5",
          "form": "저도 직장인입니다!",
          "original_form": "저도 직장인입니다!",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020286.1.6",
          "form": "회사 다니다보니 학생때로 돌아가고싶어요ㅠㅠ",
          "original_form": "회사 다니다보니 학생때로 돌아가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020286.1.7",
          "form": "맞아요 학생때 너무 그립죠 ㅠㅠㅠ",
          "original_form": "맞아요 학생때 너무 그립죠 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020286.1.8",
          "form": "언제가 제일 그리우세요?",
          "original_form": "언제가 제일 그리우세요?",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020286.1.9",
          "form": "저 고등학교때요!",
          "original_form": "저 고등학교때요!",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020286.1.10",
          "form": "힘들었지만 그래도 그시절이 젤 재밌었어요",
          "original_form": "힘들었지만 그래도 그시절이 젤 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020286.1.11",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020286.1.12",
          "form": "저도 고등학교시절 너무 그리워요",
          "original_form": "저도 고등학교시절 너무 그리워요",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020286.1.13",
          "form": "그땐 진짜 공부하기 싫었는데",
          "original_form": "그땐 진짜 공부하기 싫었는데",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020286.1.14",
          "form": "아무것도 모르고 공부할때가 젤 좋은거였어요ㅠㅠ",
          "original_form": "아무것도 모르고 공부할때가 젤 좋은거였어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020286.1.15",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020286.1.16",
          "form": "친구들끼리 별 내용없는 수다떨고 ㅋㅋㅋㅋ",
          "original_form": "친구들끼리 별 내용없는 수다떨고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020286.1.17",
          "form": "헐맞아요! 야자시간조차도 그리운거있죠ㅋㅋ큐ㅠㅠ",
          "original_form": "헐맞아요! 야자시간조차도 그리운거있죠ㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020286.1.18",
          "form": "머리카락 사이로 이어폰 꽂고 몰래 딴짓하고 ㅋㅋㅋㅋㅋ",
          "original_form": "머리카락 사이로 이어폰 꽂고 몰래 딴짓하고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020286.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ라디오 몰래 듣고 소설도 읽고요ㅎㅎ..",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ라디오 몰래 듣고 소설도 읽고요ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020286.1.20",
          "form": "다운받은 예능도 몰래보고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다운받은 예능도 몰래보고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020286.1.21",
          "form": "ㅋㅋㅋㅋㅋ동년배이신듯하네요",
