      "utterance": [
        {
          "id": "MDRW2100005141.1.1",
          "form": "혹시 동물 키우고싶은 마음 있어?",
          "original_form": "혹시 동물 키우고싶은 마음 있어?",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005141.1.2",
          "form": "아니 아직..ㅠ 내가 생명을 책임진다는게 좀 두려워",
          "original_form": "아니 아직..ㅠ 내가 생명을 책임진다는게 좀 두려워",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005141.1.3",
          "form": "그건 맞지맞지",
          "original_form": "그건 맞지맞지",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005141.1.4",
          "form": "넌 지금 name3랑 같이 살고있자나",
          "original_form": "넌 지금 &name3&랑 같이 살고있자나",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005141.1.5",
          "form": "어떻게 만났어?",
          "original_form": "어떻게 만났어?",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.6",
          "form": "name3랑",
          "original_form": "&name3&랑",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.7",
          "form": "맞아 우리집 강아지 name3",
          "original_form": "맞아 우리집 강아지 &name3&",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.8",
          "form": "나는 어렸을 때 펫샵에서 만났어",
          "original_form": "나는 어렸을 때 펫샵에서 만났어",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.9",
          "form": "그땐 쪼끄만 아기강아지였는데",
          "original_form": "그땐 쪼끄만 아기강아지였는데",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.10",
          "form": "아항아항",
          "original_form": "아항아항",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.11",
          "form": "벌써 12살이네",
          "original_form": "벌써 12살이네",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.12",
          "form": "name3지금도아기야",
          "original_form": "&name3&지금도아기야",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.13",
          "form": "아직 아긔야",
          "original_form": "아직 아긔야",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.14",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005141.1.15",
          "form": "name3 안고 있으면 따듯해?",
          "original_form": "&name3& 안고 있으면 따듯해?",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005141.1.16",
          "form": "포근포근말랑말랑해?",
          "original_form": "포근포근말랑말랑해?",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005141.1.17",
          "form": "name3 안고있으면",
          "original_form": "&name3& 안고있으면",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005141.1.18",
          "form": "더워",
          "original_form": "더워",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005141.1.19",
          "form": "ㅋㅋㅋㅋㅋ아그정도구낰ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아그정도구낰ㅋ",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005141.1.20",
          "form": "여름엔 뜨거워",
          "original_form": "여름엔 뜨거워",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005141.1.21",
          "form": "겨울엔 안고있으면 따시겠다",
          "original_form": "겨울엔 안고있으면 따시겠다",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005141.1.22",
          "form": "겨울에도 더운건아니짘ㅋ?",
          "original_form": "겨울에도 더운건아니짘ㅋ?",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005141.1.23",
          "form": "겨울에 이불 안에서 name3 안고 있으면 그저 천국",
          "original_form": "겨울에 이불 안에서 &name3& 안고 있으면 그저 천국",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005141.1.24",
          "form": "히힝 name3 넘귀요워",
          "original_form": "히힝 &name3& 넘귀요워",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005141.1.25",
          "form": "요즘 name3한테 빙글도는 훈련 시키는 중",
          "original_form": "요즘 &name3&한테 빙글도는 훈련 시키는 중",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005141.1.26",
          "form": "왴ㅋㅋ 잘따라와?",
          "original_form": "왴ㅋㅋ 잘따라와?",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005141.1.27",
          "form": "음 아직 잘 모르나봐",
          "original_form": "음 아직 잘 모르나봐",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005141.1.28",
          "form": "name3 훈련시키고 name3가 잘 하면 너무 기뻐",
          "original_form": "&name3& 훈련시키고 &name3&가 잘 하면 너무 기뻐",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005141.1.29",
          "form": "간식 와방많이줘",
          "original_form": "간식 와방많이줘",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005141.1.30",
          "form": "근데 빙글 도는 건 왜 가르치는거야",
          "original_form": "근데 빙글 도는 건 왜 가르치는거야",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005141.1.31",
          "form": "간식 많이주면 사료를 안 먹어ㅠㅠ 하지만 간식을 줄 수밖에 없지",
          "original_form": "간식 많이주면 사료를 안 먹어ㅠㅠ 하지만 간식을 줄 수밖에 없지",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005141.1.32",
          "form": "재밌잖아 앉아 엎드려 손 돌아",
          "original_form": "재밌잖아 앉아 엎드려 손 돌아",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005141.1.33",
          "form": "앉아 엎드려 손도 하고 코 하면 코도 준다",
          "original_form": "앉아 엎드려 손도 하고 코 하면 코도 준다",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005141.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋname3.. 올해초에 산책할때보고 못봐따",
          "original_form": "ㅋㅋㅋㅋㅋㅋ&name3&.. 올해초에 산책할때보고 못봐따",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005141.1.35",
          "form": "헐 코!! 그거 어어어엄청 귀엽던데",
          "original_form": "헐 코!! 그거 어어어엄청 귀엽던데",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005141.1.36",
          "form": "나중에 또 만나 산책 또 해",
          "original_form": "나중에 또 만나 산책 또 해",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005141.1.37",
