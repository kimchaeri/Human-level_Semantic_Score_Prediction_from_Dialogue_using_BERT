      "utterance": [
        {
          "id": "MDRW2100025012.1.1",
          "form": "어떤 계절을 좋아하시나요?",
          "original_form": "어떤 계절을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025012.1.2",
          "form": "가을이요",
          "original_form": "가을이요",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025012.1.3",
          "form": "단풍드는게 너무 예쁘죠?ㅎ",
          "original_form": "단풍드는게 너무 예쁘죠?ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025012.1.4",
          "form": "네, 그리고 하늘도 너무 예뻐요",
          "original_form": "네, 그리고 하늘도 너무 예뻐요",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025012.1.5",
          "form": "공기도 싱그럽고",
          "original_form": "공기도 싱그럽고",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025012.1.6",
          "form": "가을 공기냄새는 정말 좋아요ㅎ",
          "original_form": "가을 공기냄새는 정말 좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025012.1.7",
          "form": "네. 마스크 쓰고 다녀야한다는게 안타까울 만큼",
          "original_form": "네. 마스크 쓰고 다녀야한다는게 안타까울 만큼",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025012.1.8",
          "form": "가을은 새벽공기도 참 맑은 느낌이에요ㅎㅎㅅ",
          "original_form": "가을은 새벽공기도 참 맑은 느낌이에요ㅎㅎㅅ",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025012.1.9",
          "form": "ㅎㅎ 새벽 공기는 맡아본 기억이 많이 없네요 ㅋㅋ",
          "original_form": "ㅎㅎ 새벽 공기는 맡아본 기억이 많이 없네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025012.1.10",
          "form": "아침잠이 많아서리",
          "original_form": "아침잠이 많아서리",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025012.1.11",
          "form": "ㅎㅎㅎ 저녁공기도 좋아요ㅎㅎ",
          "original_form": "ㅎㅎㅎ 저녁공기도 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025012.1.12",
          "form": "그쵸. 어쩐지 출퇴근길이 상쾌해지는 날씨에요",
          "original_form": "그쵸. 어쩐지 출퇴근길이 상쾌해지는 날씨에요",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025012.1.13",
          "form": "낮은 아직 좀 덥지만 점점 시원해지고 있네요ㅎ",
          "original_form": "낮은 아직 좀 덥지만 점점 시원해지고 있네요ㅎ",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025012.1.14",
          "form": "오늘은 외려 추울정도였어요",
          "original_form": "오늘은 외려 추울정도였어요",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100025012.1.15",
          "form": "저녁은 창문을 닫고 자야겠더라구요ㅎㅎ",
          "original_form": "저녁은 창문을 닫고 자야겠더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 23:27"
        }
      ]
