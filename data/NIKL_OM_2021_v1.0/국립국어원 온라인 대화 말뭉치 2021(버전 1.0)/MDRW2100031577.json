      "utterance": [
        {
          "id": "MDRW2100031577.1.1",
          "form": "오늘 날씨 좋더라구요",
          "original_form": "오늘 날씨 좋더라구요",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100031577.1.2",
          "form": "오늘 날씨 완전 가을이었죠!",
          "original_form": "오늘 날씨 완전 가을이었죠!",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031577.1.3",
          "form": "네 약간 더운감이 아직 남았지만 가을 같았어요",
          "original_form": "네 약간 더운감이 아직 남았지만 가을 같았어요",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031577.1.4",
          "form": "일단 바람이 너무 시원하더라고요",
          "original_form": "일단 바람이 너무 시원하더라고요",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031577.1.5",
          "form": "가을옷좀 사야하나봐요ㅎ",
          "original_form": "가을옷좀 사야하나봐요ㅎ",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031577.1.6",
          "form": "반팔에다가",
          "original_form": "반팔에다가",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031577.1.7",
          "form": "가디건필수예요",
          "original_form": "가디건필수예요",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031577.1.8",
          "form": "맞아요 이젠 반팔만 입으면 춥더라고요",
          "original_form": "맞아요 이젠 반팔만 입으면 춥더라고요",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100031577.1.9",
          "form": "시간이 너무 빨리가는거 같아요ㅜ",
          "original_form": "시간이 너무 빨리가는거 같아요ㅜ",
          "speaker_id": "1",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100031577.1.10",
          "form": "맞아요 이러다가 또 겨을 오겠죠",
          "original_form": "맞아요 이러다가 또 겨을 오겠죠",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100031577.1.11",
          "form": "겨울이 좋으세요 여름이 좋으세요?",
          "original_form": "겨울이 좋으세요 여름이 좋으세요?",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031577.1.12",
          "form": "저는 겨울이요",
          "original_form": "저는 겨울이요",
          "speaker_id": "2",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100031577.1.13",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100031577.1.14",
          "form": "저는 여름이었는데 점점 겨울로 바뀌고있어요",
          "original_form": "저는 여름이었는데 점점 겨울로 바뀌고있어요",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100031577.1.15",
          "form": "날씨가 진짜 점점 더 더워져서 ㅜ",
          "original_form": "날씨가 진짜 점점 더 더워져서 ㅜ",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100031577.1.16",
          "form": "여름에 더우면 얇게 입지가 이젠 안되더라고요",
          "original_form": "여름에 더우면 얇게 입지가 이젠 안되더라고요",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031577.1.17",
          "form": "겨울엔 차라리 껴입을 수라도 있지",
          "original_form": "겨울엔 차라리 껴입을 수라도 있지",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031577.1.18",
          "form": "맞아요 회사에 핫팬츠입을수도없고",
          "original_form": "맞아요 회사에 핫팬츠입을수도없고",
          "speaker_id": "1",
          "time": "20210904 23:13"
        }
      ]
