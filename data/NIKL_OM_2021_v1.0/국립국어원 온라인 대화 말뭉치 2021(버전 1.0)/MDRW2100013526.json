      "utterance": [
        {
          "id": "MDRW2100013526.1.1",
          "form": "한달에 한두번은 먹는것같아요",
          "original_form": "한달에 한두번은 먹는것같아요",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013526.1.2",
          "form": "치킨 맛있지만 너무 비싼 그대",
          "original_form": "치킨 맛있지만 너무 비싼 그대",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013526.1.3",
          "form": "맞아요... 저도 한달에 한 두번 정도 먹어요",
          "original_form": "맞아요... 저도 한달에 한 두번 정도 먹어요",
          "speaker_id": "1",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013526.1.4",
          "form": "주로 어떤 거 드시나용?",
          "original_form": "주로 어떤 거 드시나용?",
          "speaker_id": "1",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013526.1.5",
          "form": "저는 최근에 청년치킨에서 시켜먹어요",
          "original_form": "저는 최근에 청년치킨에서 시켜먹어요",
          "speaker_id": "2",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013526.1.6",
          "form": "양념치킨이 옛날 동네에서 먹던거랑 그나마 비슷하더라고요",
          "original_form": "양념치킨이 옛날 동네에서 먹던거랑 그나마 비슷하더라고요",
          "speaker_id": "2",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013526.1.7",
          "form": "오 청년치킨이라는 곳도 생겼군요? 저도 먹어보고 싶네요",
          "original_form": "오 청년치킨이라는 곳도 생겼군요? 저도 먹어보고 싶네요",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013526.1.8",
          "form": "양념이 듬뿍있어서",
          "original_form": "양념이 듬뿍있어서",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013526.1.9",
          "form": "반반시켜서 후라이드 찍어먹어도 좋아요",
          "original_form": "반반시켜서 후라이드 찍어먹어도 좋아요",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013526.1.10",
          "form": "점바점은...아니겠죠? ㅎㅎ",
          "original_form": "점바점은...아니겠죠? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013526.1.11",
          "form": "ㅋㅋㅋㅋ 양념 치킨 좋아하시나봐요!",
          "original_form": "ㅋㅋㅋㅋ 양념 치킨 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013526.1.12",
          "form": "전 후라이드나 간장 주로 먹거든요ㅋㅋㅋ",
          "original_form": "전 후라이드나 간장 주로 먹거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013526.1.13",
          "form": "전 베이직한 기본메뉴가 좋아서 주로 후라이드나 양념으로 먹어요",
          "original_form": "전 베이직한 기본메뉴가 좋아서 주로 후라이드나 양념으로 먹어요",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013526.1.14",
          "form": "간장치킨은 교촌 허니콤보?",
          "original_form": "간장치킨은 교촌 허니콤보?",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013526.1.15",
          "form": "아니용! 전 사쿠사쿠 간장치킨을 제일 많이 시켜요ㅎㅎ",
          "original_form": "아니용! 전 사쿠사쿠 간장치킨을 제일 많이 시켜요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013526.1.16",
          "form": "사쿠사쿠라는곳 처음들어봐요",
          "original_form": "사쿠사쿠라는곳 처음들어봐요",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013526.1.17",
          "form": "아 약간 호프집 같은 느낌이어서 그런가봐요ㅋㅋㅋㅋㅋ 근데 가격도 좋고 맛있어요",
          "original_form": "아 약간 호프집 같은 느낌이어서 그런가봐요ㅋㅋㅋㅋㅋ 근데 가격도 좋고 맛있어요",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013526.1.18",
          "form": "주변에 그런곳 있으면 좋죠",
          "original_form": "주변에 그런곳 있으면 좋죠",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013526.1.19",
          "form": "맞아요ㅎㅎ 혹시 길가다 보이시면 꼭 드셔보셔요 진짜 맛있어요!",
          "original_form": "맞아요ㅎㅎ 혹시 길가다 보이시면 꼭 드셔보셔요 진짜 맛있어요!",
          "speaker_id": "1",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013526.1.20",
          "form": "약간 체인이긴 한데 단골버프 있어서 소스도 많이 주시더라구욬ㅋㅋ",
          "original_form": "약간 체인이긴 한데 단골버프 있어서 소스도 많이 주시더라구욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013526.1.21",
          "form": "입이 심심해지네요",
          "original_form": "입이 심심해지네요",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100013526.1.22",
          "form": "먹는 이야기하니 뭔가 출출해요",
          "original_form": "먹는 이야기하니 뭔가 출출해요",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013526.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:21"
        }
      ]
    }
