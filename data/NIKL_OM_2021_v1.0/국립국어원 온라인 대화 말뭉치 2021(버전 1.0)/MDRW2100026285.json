      "utterance": [
        {
          "id": "MDRW2100026285.1.1",
          "form": "name2 점심 드셨나요",
          "original_form": "&name2& 점심 드셨나요",
          "speaker_id": "1",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100026285.1.2",
          "form": "전 곤드레밥에 사과먹고 있어요",
          "original_form": "전 곤드레밥에 사과먹고 있어요",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100026285.1.3",
          "form": "아침은 먹었고 점심은 아직이요",
          "original_form": "아침은 먹었고 점심은 아직이요",
          "speaker_id": "2",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100026285.1.4",
          "form": "맛있는 거 드셨네요",
          "original_form": "맛있는 거 드셨네요",
          "speaker_id": "2",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100026285.1.5",
          "form": "아 짐 창원인데 제주산 곤드레를 드렸더니",
          "original_form": "아 짐 창원인데 제주산 곤드레를 드렸더니",
          "speaker_id": "1",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100026285.1.6",
          "form": "점심으로 나왔네요",
          "original_form": "점심으로 나왔네요",
          "speaker_id": "1",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100026285.1.7",
          "form": "창원에서는 어떤 음식이 유명해요",
          "original_form": "창원에서는 어떤 음식이 유명해요",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100026285.1.8",
          "form": "아침은 생선이랑 먹었어요",
          "original_form": "아침은 생선이랑 먹었어요",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100026285.1.9",
          "form": "창원은 마산창원진해가 통합된 곳이라",
          "original_form": "창원은 마산창원진해가 통합된 곳이라",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100026285.1.10",
          "form": "아구찜이 생각나네요",
          "original_form": "아구찜이 생각나네요",
          "speaker_id": "2",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100026285.1.11",
          "form": "해물찜 아님 복국?",
          "original_form": "해물찜 아님 복국?",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100026285.1.12",
          "form": "네 근데 아귀찜 맛집은 다 동네에 숨어있어요",
          "original_form": "네 근데 아귀찜 맛집은 다 동네에 숨어있어요",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100026285.1.13",
          "form": "복국 맛있겠네요 ㅎㅎ",
          "original_form": "복국 맛있겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100026285.1.14",
          "form": "네 지리로여",
          "original_form": "네 지리로여",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100026285.1.15",
          "form": "가신 김에 아귀찜 드셔야죠",
          "original_form": "가신 김에 아귀찜 드셔야죠",
          "speaker_id": "2",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100026285.1.16",
          "form": "복국은 한 번도 안 먹어 봤어요",
          "original_form": "복국은 한 번도 안 먹어 봤어요",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100026285.1.17",
          "form": "한상차림이라고 약간 다찌스탈로 나오는 곳 가서",
          "original_form": "한상차림이라고 약간 다찌스탈로 나오는 곳 가서",
          "speaker_id": "1",
