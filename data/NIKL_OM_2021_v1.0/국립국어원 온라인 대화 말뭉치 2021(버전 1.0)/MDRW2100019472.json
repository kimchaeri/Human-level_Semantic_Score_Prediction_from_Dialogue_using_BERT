      "utterance": [
        {
          "id": "MDRW2100019472.1.1",
          "form": "안녕하세요 저녁드셨나요",
          "original_form": "안녕하세요 저녁드셨나요",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019472.1.2",
          "form": "아뇨 아직요 name1님은요?",
          "original_form": "아뇨 아직요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019472.1.3",
          "form": "방금 막 먹고 왔어요",
          "original_form": "방금 막 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019472.1.4",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019472.1.5",
          "form": "된장짜글이 사와서 밥비벼 먹었어요",
          "original_form": "된장짜글이 사와서 밥비벼 먹었어요",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019472.1.6",
          "form": "name2님은 뭐 드실거예요?",
          "original_form": "&name2&님은 뭐 드실거예요?",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019472.1.7",
          "form": "우와 진짜 맛있겠다 전 방금 과자 한봉지를 털어버려서...ㅋㅋㅋㅋ 먹을지 안먹을지 모르겠어요",
          "original_form": "우와 진짜 맛있겠다 전 방금 과자 한봉지를 털어버려서...ㅋㅋㅋㅋ 먹을지 안먹을지 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019472.1.8",
          "form": "김치짜글이는 들어봤는데 된장짜글이는 처음 들어봐요!",
          "original_form": "김치짜글이는 들어봤는데 된장짜글이는 처음 들어봐요!",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019472.1.9",
          "form": "제가 김치를 그닥 안좋아해서요",
          "original_form": "제가 김치를 그닥 안좋아해서요",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019472.1.10",
          "form": "다른 맛집으로 찾아갔다가 짜글이에 맛붙였어요",
          "original_form": "다른 맛집으로 찾아갔다가 짜글이에 맛붙였어요",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019472.1.11",
          "form": "오오 짜글이가 아주 맛있나보네요",
          "original_form": "오오 짜글이가 아주 맛있나보네요",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019472.1.12",
          "form": "재료는 뭐가 들어가요?",
          "original_form": "재료는 뭐가 들어가요?",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019472.1.13",
          "form": "두부, 호박, 팽이버섯, 무, 양파, 된장 이렇게 들어가요",
          "original_form": "두부, 호박, 팽이버섯, 무, 양파, 된장 이렇게 들어가요",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019472.1.14",
          "form": "재료는 일반 된장찌개랑 같네요!",
          "original_form": "재료는 일반 된장찌개랑 같네요!",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019472.1.15",
          "form": "name2님 말 듣고보니까 진짜 국물만 적게 해서 만든거네요",
          "original_form": "&name2&님 말 듣고보니까 진짜 국물만 적게 해서 만든거네요",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019472.1.16",
          "form": "약간 강된장같은 맛 일까요?",
          "original_form": "약간 강된장같은 맛 일까요?",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019472.1.17",
          "form": "비슷할거 같아요",
          "original_form": "비슷할거 같아요",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019472.1.18",
          "form": "대신 짜글이는 국물이 조금이라도 있다는게 다르네요",
          "original_form": "대신 짜글이는 국물이 조금이라도 있다는게 다르네요",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019472.1.19",
          "form": "오 궁금하네요~",
          "original_form": "오 궁금하네요~",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019472.1.20",
          "form": "낮에는 뭐 드셨나요?",
          "original_form": "낮에는 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019472.1.21",
          "form": "전 낙지볶음밥 먹었어요",
          "original_form": "전 낙지볶음밥 먹었어요",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019472.1.22",
          "form": "낮에는 비비고 곰탕 해서 먹었어요",
          "original_form": "낮에는 비비고 곰탕 해서 먹었어요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019472.1.23",
          "form": "낙지볶음밥 맛있었겠어요",
          "original_form": "낙지볶음밥 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019472.1.24",
          "form": "비비고 곰탕 완전 최고",
          "original_form": "비비고 곰탕 완전 최고",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019472.1.25",
          "form": "비비고 천재",
          "original_form": "비비고 천재",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019472.1.26",
          "form": "그쵸?ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019472.1.27",
          "form": "낚지볶음밥이 초라해졌어요",
          "original_form": "낚지볶음밥이 초라해졌어요",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019472.1.28",
          "form": "곰탕이 더 맛있어요",
          "original_form": "곰탕이 더 맛있어요",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019472.1.29",
          "form": "엌ㅋㅋ 그정도인가요 낚지볶음밥이 밀리다니요",
          "original_form": "엌ㅋㅋ 그정도인가요 낚지볶음밥이 밀리다니요",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019472.1.30",
          "form": "얼린낙지 맛없어요..ㅠ",
          "original_form": "얼린낙지 맛없어요..ㅠ",
          "speaker_id": "2",
