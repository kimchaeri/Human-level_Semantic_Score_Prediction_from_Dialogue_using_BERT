      "utterance": [
        {
          "id": "MDRW2100012212.1.1",
          "form": "소고기 파 콩나물 무",
          "original_form": "소고기 파 콩나물 무",
          "speaker_id": "2",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012212.1.2",
          "form": "파기름 내?",
          "original_form": "파기름 내?",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012212.1.3",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100012212.1.4",
          "form": "소고기\n무\n참기름, 국간장, 고춧가루 넣고 오랫동안 볶아야해",
          "original_form": "소고기\n무\n참기름, 국간장, 고춧가루 넣고 오랫동안 볶아야해",
          "speaker_id": "2",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100012212.1.5",
          "form": "파기름 안내는구나",
          "original_form": "파기름 안내는구나",
          "speaker_id": "1",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012212.1.6",
          "form": "자신있는 요리 뭐야?",
          "original_form": "자신있는 요리 뭐야?",
          "speaker_id": "1",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012212.1.7",
          "form": "오래볶음 맛있어",
          "original_form": "오래볶음 맛있어",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012212.1.8",
          "form": "소고기무국",
          "original_form": "소고기무국",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012212.1.9",
          "form": "스타벅스가 좋아?",
          "original_form": "스타벅스가 좋아?",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012212.1.10",
          "form": "맛있는 음료가 뭐야",
          "original_form": "맛있는 음료가 뭐야",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012212.1.11",
          "form": "난 커피 못 먹어",
          "original_form": "난 커피 못 먹어",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012212.1.12",
          "form": "허니자몽블랙티",
          "original_form": "허니자몽블랙티",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012212.1.13",
          "form": "아이스??",
          "original_form": "아이스??",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012212.1.14",
          "form": "담에...쿨라임에이드 인가..그거 마셔봐",
          "original_form": "담에...쿨라임에이드 인가..그거 마셔봐",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012212.1.15",
          "form": "그거 카페인있다 아니야?",
          "original_form": "그거 카페인있다 아니야?",
          "speaker_id": "1",
          "time": "20210824 20:29"
        },
        {
          "id": "MDRW2100012212.1.16",
          "form": "없을텐데..",
          "original_form": "없을텐데..",
          "speaker_id": "2",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012212.1.17",
          "form": "민초 좋아해?",
          "original_form": "민초 좋아해?",
          "speaker_id": "2",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012212.1.18",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012212.1.19",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012212.1.20",
          "form": "난 민초 덕후야",
          "original_form": "난 민초 덕후야",
          "speaker_id": "2",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012212.1.21",
          "form": "민트향을 안좋아해?",
          "original_form": "민트향을 안좋아해?",
          "speaker_id": "2",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012212.1.22",
          "form": "진짜??대박",
          "original_form": "진짜??대박",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012212.1.23",
          "form": "몰랐네",
          "original_form": "몰랐네",
          "speaker_id": "1",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100012212.1.24",
          "form": "그럼 스타벅스 쿨라임은 별로일거야",
          "original_form": "그럼 스타벅스 쿨라임은 별로일거야",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012212.1.25",
          "form": "난 레몬민트를 좋아해",
          "original_form": "난 레몬민트를 좋아해",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
