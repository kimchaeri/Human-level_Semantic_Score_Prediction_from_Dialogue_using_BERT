      "utterance": [
        {
          "id": "MDRW2100044606.1.1",
          "form": "운동 하세요?",
          "original_form": "운동 하세요?",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044606.1.2",
          "form": "숨쉬기 운동요 먹짱이님은요?",
          "original_form": "숨쉬기 운동요 먹짱이님은요?",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044606.1.3",
          "form": "ㅋㅋㅋㅋ 전 헬스요",
          "original_form": "ㅋㅋㅋㅋ 전 헬스요",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044606.1.4",
          "form": "다이어트 안하세요?",
          "original_form": "다이어트 안하세요?",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044606.1.5",
          "form": "다이어트는 식이 위주로 하고 홈트 하긴 한데 잘 안해요 헬스 얼마나하셨어요?",
          "original_form": "다이어트는 식이 위주로 하고 홈트 하긴 한데 잘 안해요 헬스 얼마나하셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044606.1.6",
          "form": "ㅋㅋ 3년이요",
          "original_form": "ㅋㅋ 3년이요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044606.1.7",
          "form": "근대 1주일에 한시간이요",
          "original_form": "근대 1주일에 한시간이요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044606.1.8",
          "form": "오 대박 그래도 꾸준히 하시네요",
          "original_form": "오 대박 그래도 꾸준히 하시네요",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044606.1.9",
          "form": "ㅋㅋㅋㅋ 네...안하면 죽을거같아서",
          "original_form": "ㅋㅋㅋㅋ 네...안하면 죽을거같아서",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044606.1.10",
          "form": "영양제 드세여?",
          "original_form": "영양제 드세여?",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044606.1.11",
          "form": "유산균만 먹어요 님은 챙기세요?",
          "original_form": "유산균만 먹어요 님은 챙기세요?",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044606.1.12",
          "form": "저두요 ㅋㅋ 유산균 효과있죠,",
          "original_form": "저두요 ㅋㅋ 유산균 효과있죠,",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044606.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044606.1.14",
          "form": "여성유산균인데 엄마 잔소리때문에 먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "여성유산균인데 엄마 잔소리때문에 먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044606.1.15",
          "form": "아 ㅋㅋㅋㅋ 비타민은 드시라고 안하나요",
          "original_form": "아 ㅋㅋㅋㅋ 비타민은 드시라고 안하나요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044606.1.16",
          "form": "종합 비타민은 지금 없는데 있을때 말씀하세요",
          "original_form": "종합 비타민은 지금 없는데 있을때 말씀하세요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044606.1.17",
          "form": "비타민은 귤맛나서 먹을만해요",
          "original_form": "비타민은 귤맛나서 먹을만해요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044606.1.18",
          "form": "ㅋㅋㅋㅋ 마식는 비타민 드시는군요",
          "original_form": "ㅋㅋㅋㅋ 마식는 비타민 드시는군요",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044606.1.19",
          "form": "알약인데 오렌지향? 귤향 나거든요",
          "original_form": "알약인데 오렌지향? 귤향 나거든요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044606.1.20",
          "form": "님은 더 챙기는거있나요?",
          "original_form": "님은 더 챙기는거있나요?",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044606.1.21",
          "form": "ㅋㅋㅋㅋ 저도 비타민 유산균만요",
          "original_form": "ㅋㅋㅋㅋ 저도 비타민 유산균만요",
