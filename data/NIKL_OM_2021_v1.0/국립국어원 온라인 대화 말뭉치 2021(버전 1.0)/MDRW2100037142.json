      "utterance": [
        {
          "id": "MDRW2100037142.1.1",
          "form": "일 하시나요?",
          "original_form": "일 하시나요?",
          "speaker_id": "2",
          "time": "20210831 20:34"
        },
        {
          "id": "MDRW2100037142.1.2",
          "form": "네네~",
          "original_form": "네네~",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037142.1.3",
          "form": "만족하며 다니시는 거죠?",
          "original_form": "만족하며 다니시는 거죠?",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037142.1.4",
          "form": "네 괜참아요",
          "original_form": "네 괜참아요",
          "speaker_id": "1",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037142.1.5",
          "form": "오~ 전 매년 스트레스인데요ㅠ",
          "original_form": "오~ 전 매년 스트레스인데요ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037142.1.6",
          "form": "헐 매년요?",
          "original_form": "헐 매년요?",
          "speaker_id": "1",
          "time": "20210831 23:23"
        },
        {
          "id": "MDRW2100037142.1.7",
          "form": "네 ㅠ 오늘도 일이 너무 많았어요 ㅠ",
          "original_form": "네 ㅠ 오늘도 일이 너무 많았어요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:23"
        },
        {
          "id": "MDRW2100037142.1.8",
          "form": "에공 토닥토닥",
          "original_form": "에공 토닥토닥",
          "speaker_id": "1",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037142.1.9",
          "form": "정말 집에오면 말그대로 녹초가 되요 ㅠ",
          "original_form": "정말 집에오면 말그대로 녹초가 되요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037142.1.10",
          "form": "일양때문에 스트레스인거에뇨?",
          "original_form": "일양때문에 스트레스인거에뇨?",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037142.1.11",
          "form": "일양도 그렇고 민원이 너무 많아요 말도 안되는 민원요",
          "original_form": "일양도 그렇고 민원이 너무 많아요 말도 안되는 민원요",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037142.1.12",
          "form": "ㅠㅜ토닥이네요",
          "original_form": "ㅠㅜ토닥이네요",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037142.1.13",
          "form": "그래도 이번이 옮겨온 지점 상사는 상식적이라 좀 나아여",
          "original_form": "그래도 이번이 옮겨온 지점 상사는 상식적이라 좀 나아여",
