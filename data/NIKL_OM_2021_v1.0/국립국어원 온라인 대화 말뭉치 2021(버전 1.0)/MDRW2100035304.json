      "utterance": [
        {
          "id": "MDRW2100035304.1.1",
          "form": "오 완전",
          "original_form": "오 완전",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100035304.1.2",
          "form": "맛있어보여요",
          "original_form": "맛있어보여요",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100035304.1.3",
          "form": "즉석떡볶이",
          "original_form": "즉석떡볶이",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100035304.1.4",
          "form": "저희동네는 아딸이었는데",
          "original_form": "저희동네는 아딸이었는데",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100035304.1.5",
          "form": "이름이 감탄인가 뭐로 바뀌었죠",
          "original_form": "이름이 감탄인가 뭐로 바뀌었죠",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100035304.1.6",
          "form": "또보겠지 맛있어요 즉성떡볶이에 버터갈릭 감튀를 추가한 맛",
          "original_form": "또보겠지 맛있어요 즉성떡볶이에 버터갈릭 감튀를 추가한 맛",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035304.1.7",
          "form": "아딸떡볶이는 상표권분쟁때문에 그래요",
          "original_form": "아딸떡볶이는 상표권분쟁때문에 그래요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035304.1.8",
          "form": "부부가 만든 사업인데 이혼하면서",
          "original_form": "부부가 만든 사업인데 이혼하면서",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035304.1.9",
          "form": "엥 헐",
          "original_form": "엥 헐",
          "speaker_id": "2",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035304.1.10",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "2",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035304.1.11",
          "form": "이게 ㅋㅋ사업 꼼수인데",
          "original_form": "이게 ㅋㅋ사업 꼼수인데",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100035304.1.12",
          "form": "그래서 상표가",
          "original_form": "그래서 상표가",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100035304.1.13",
          "form": "상표권을 가족이 가지고 가족에게 상표권 사용권 주는 식으로 많이들 돈을 삥땅쳐요",
          "original_form": "상표권을 가족이 가지고 가족에게 상표권 사용권 주는 식으로 많이들 돈을 삥땅쳐요",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100035304.1.14",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100035304.1.15",
          "form": "파바 회장 부인이",
          "original_form": "파바 회장 부인이",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100035304.1.16",
          "form": "뭔가 동심파괴느낌이에요",
          "original_form": "뭔가 동심파괴느낌이에요",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100035304.1.17",
          "form": "파리바케트 상표권을 가지고 있어요",
          "original_form": "파리바케트 상표권을 가지고 있어요",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100035304.1.18",
          "form": "그래서 파바 회사가 일년에 몇십억인가를 회장 아내에게 줘요 이게 가맹점주들 돈뜯어서 주는거죠",
          "original_form": "그래서 파바 회사가 일년에 몇십억인가를 회장 아내에게 줘요 이게 가맹점주들 돈뜯어서 주는거죠",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100035304.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100035304.1.20",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100035304.1.21",
          "form": "와 굉장히 묘해요",
          "original_form": "와 굉장히 묘해요",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100035304.1.22",
          "form": "그래서 아딸떡볶이도 그런식으로 운영했는데",
          "original_form": "그래서 아딸떡볶이도 그런식으로 운영했는데",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100035304.1.23",
          "form": "아내랑 이혼을 해버리니깐ㅋㅋ 아빠와딸 떡볶이도 상표권이 전아내에게가벌니거죠",
          "original_form": "아내랑 이혼을 해버리니깐ㅋㅋ 아빠와딸 떡볶이도 상표권이 전아내에게가벌니거죠",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035304.1.24",
          "form": "굉장히 묘하고 흥미롭네요",
          "original_form": "굉장히 묘하고 흥미롭네요",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035304.1.25",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035304.1.26",
          "form": "이혼한 아내에게 돈을 주느니 개명한 브랜드",
          "original_form": "이혼한 아내에게 돈을 주느니 개명한 브랜드",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035304.1.27",
          "form": "사업하며 같이 힘들었을텐데 적당히 좀 주지",
          "original_form": "사업하며 같이 힘들었을텐데 적당히 좀 주지",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035304.1.28",
          "form": "뭔가 엄청 흥미로워요",
          "original_form": "뭔가 엄청 흥미로워요",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035304.1.29",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035304.1.30",
          "form": "뭔가 추하네요",
          "original_form": "뭔가 추하네요",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035304.1.31",
          "form": "좋아하는 떡볶이에",
          "original_form": "좋아하는 떡볶이에",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035304.1.32",
          "form": "좋아하는 돈얘기가 얽히니 당연히 재미있죠 호호",
          "original_form": "좋아하는 돈얘기가 얽히니 당연히 재미있죠 호호",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035304.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035304.1.34",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035304.1.35",
          "form": "역시 돈이 끼어있으면 재밌어요",
          "original_form": "역시 돈이 끼어있으면 재밌어요",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035304.1.36",
          "form": "네 제가 좋아하는 음식점도 누가 거기 보증금을 ㅡ얘기하면 귀가 활짝",
          "original_form": "네 제가 좋아하는 음식점도 누가 거기 보증금을 ㅡ얘기하면 귀가 활짝",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100035304.1.37",
          "form": "로제 떡볶이도 누가 원조인가 이런걸로 말이 많던데",
          "original_form": "로제 떡볶이도 누가 원조인가 이런걸로 말이 많던데",
          "speaker_id": "1",
          "time": "20210829 14:24"
        }
      ]
    }
