      "utterance": [
        {
          "id": "MDRW2100035993.1.1",
          "form": "장르가 뭐에용?",
          "original_form": "장르가 뭐에용?",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100035993.1.2",
          "form": "네네!! 이번에 시즌4 나왔어요!!",
          "original_form": "네네!! 이번에 시즌4 나왔어요!!",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100035993.1.3",
          "form": "시즌4까지 나왔구나!",
          "original_form": "시즌4까지 나왔구나!",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100035993.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100035993.1.5",
          "form": "장르를 뭐라고해야하지",
          "original_form": "장르를 뭐라고해야하지",
          "speaker_id": "2",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035993.1.6",
          "form": "우선 주인공은 자폐있는 학생인데",
          "original_form": "우선 주인공은 자폐있는 학생인데",
          "speaker_id": "2",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035993.1.7",
          "form": "너무 무겁지않고 재밌어요!!!",
          "original_form": "너무 무겁지않고 재밌어요!!!",
          "speaker_id": "2",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035993.1.8",
          "form": "아하 학원물인가여??",
          "original_form": "아하 학원물인가여??",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035993.1.9",
          "form": "그런거 넘 좋아해요ㅋㅋㅋ",
          "original_form": "그런거 넘 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035993.1.10",
          "form": "알고보면 가족들도 다 하나씩 별난 부분이 있고..?!?!",
          "original_form": "알고보면 가족들도 다 하나씩 별난 부분이 있고..?!?!",
          "speaker_id": "2",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035993.1.11",
          "form": "가족물이라고 해야겠네여 ㅋㅋㅋ",
          "original_form": "가족물이라고 해야겠네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035993.1.12",
          "form": "오오 가끔 터지는거 소소한거 좋아해요ㅋㅋㅋ",
          "original_form": "오오 가끔 터지는거 소소한거 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035993.1.13",
          "form": "넷플릭스에서 가장 많이 본 드라마가 기묘한이야기라던데",
          "original_form": "넷플릭스에서 가장 많이 본 드라마가 기묘한이야기라던데",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035993.1.14",
          "form": "이것도 킵해뒀어요ㅋㅋㅋ",
          "original_form": "이것도 킵해뒀어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035993.1.15",
          "form": "저도 그거 하도유명해서",
          "original_form": "저도 그거 하도유명해서",
          "speaker_id": "2",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035993.1.16",
          "form": "넷플깔자마자 본게 그거에요 ㅋㅋㅋㅋ",
          "original_form": "넷플깔자마자 본게 그거에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035993.1.17",
          "form": "1화가 장벽이라고",
          "original_form": "1화가 장벽이라고",
          "speaker_id": "1",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100035993.1.18",
          "form": "1화만 참고보라고 하던데",
          "original_form": "1화만 참고보라고 하던데",
          "speaker_id": "1",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100035993.1.19",
          "form": "재밌나여??",
          "original_form": "재밌나여??",
          "speaker_id": "1",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100035993.1.20",
          "form": "저는 보다보면 약간 허술하다 싶은 부분들 좀 있긴했는뎈ㅋㅋㅋ",
          "original_form": "저는 보다보면 약간 허술하다 싶은 부분들 좀 있긴했는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100035993.1.21",
          "form": "볼만해요 ㅋㅋㅋㅋㅋ",
          "original_form": "볼만해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100035993.1.22",
          "form": "막 강추!까지는 아니시군여??",
          "original_form": "막 강추!까지는 아니시군여??",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100035993.1.23",
          "form": "미국에서 인기많아서 프렌즈 다음으로? 프렌즈를 제치고? 굿즈도 많이팔린더고 들었어여ㅋㅋㅋ",
          "original_form": "미국에서 인기많아서 프렌즈 다음으로? 프렌즈를 제치고? 굿즈도 많이팔린더고 들었어여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100035993.1.24",
          "form": "헐 그정도구나ㅋㅋㅋㅋㅋ",
          "original_form": "헐 그정도구나ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100035993.1.25",
          "form": "생각보다 엄청 많네요ㅋㅋㅋ",
