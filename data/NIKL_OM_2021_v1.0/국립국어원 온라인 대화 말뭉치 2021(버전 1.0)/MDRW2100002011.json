      "utterance": [
        {
          "id": "MDRW2100002011.1.1",
          "form": "요즘 노래 핫한게 뭐야?",
          "original_form": "요즘 노래 핫한게 뭐야?",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002011.1.2",
          "form": "노래??  아 악동뮤지션 낙하 좋드라",
          "original_form": "노래??  아 악동뮤지션 낙하 좋드라",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100002011.1.3",
          "form": "신곡 나왔구나",
          "original_form": "신곡 나왔구나",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002011.1.4",
          "form": "아이유랑 콜라보 했던데 좋드라고",
          "original_form": "아이유랑 콜라보 했던데 좋드라고",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002011.1.5",
          "form": "아이유랑 오~장난이 아니네",
          "original_form": "아이유랑 오~장난이 아니네",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002011.1.6",
          "form": "둘이 친하단다. 악동의 남자애 곡도 잘 만드니깐",
          "original_form": "둘이 친하단다. 악동의 남자애 곡도 잘 만드니깐",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002011.1.7",
          "form": "오빠가 작곡작사하나?",
          "original_form": "오빠가 작곡작사하나?",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002011.1.8",
          "form": "대부분 오빠가 만들어.  음원수익도 장난 아님",
          "original_form": "대부분 오빠가 만들어.  음원수익도 장난 아님",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002011.1.9",
          "form": "아 동생은 메이크업하고",
          "original_form": "아 동생은 메이크업하고",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002011.1.10",
          "form": "동생이 메이크업 해?",
          "original_form": "동생이 메이크업 해?",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002011.1.11",
          "form": "유튜브하잖아 메이크업으로",
          "original_form": "유튜브하잖아 메이크업으로",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002011.1.12",
          "form": "그래?  노래도 잘하고 메이크업으로 돈도 버네ㅋ",
          "original_form": "그래?  노래도 잘하고 메이크업으로 돈도 버네ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100002011.1.13",
          "form": "남매가 소비는 안하는듯",
          "original_form": "남매가 소비는 안하는듯",
          "speaker_id": "2",
          "time": "20210819 22:54"
