      "utterance": [
        {
          "id": "MDRW2100041469.1.1",
          "form": "너 에그 슬럿 먹어봤니",
          "original_form": "너 에그 슬럿 먹어봤니",
          "speaker_id": "2",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041469.1.2",
          "form": "들어왔을 때 난리였지",
          "original_form": "들어왔을 때 난리였지",
          "speaker_id": "1",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041469.1.3",
          "form": "맞아 집 근처에 있는 곳에 들어왔잖아",
          "original_form": "맞아 집 근처에 있는 곳에 들어왔잖아",
          "speaker_id": "2",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041469.1.4",
          "form": "근데 한 번도 안 가봤어",
          "original_form": "근데 한 번도 안 가봤어",
          "speaker_id": "1",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041469.1.5",
          "form": "나는 오늘 한번 배달시켜 먹어서",
          "original_form": "나는 오늘 한번 배달시켜 먹어서",
          "speaker_id": "2",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041469.1.6",
          "form": "어땠어",
          "original_form": "어땠어",
          "speaker_id": "1",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041469.1.7",
          "form": "일단 거긴 계란 버거가 유명해",
          "original_form": "일단 거긴 계란 버거가 유명해",
          "speaker_id": "2",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100041469.1.8",
          "form": "무항생제의 단계를 쓴다던데",
          "original_form": "무항생제의 단계를 쓴다던데",
          "speaker_id": "1",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100041469.1.9",
          "form": "맞아 그래서 그런지 무척 건강한 느낌이야",
          "original_form": "맞아 그래서 그런지 무척 건강한 느낌이야",
          "speaker_id": "2",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100041469.1.10",
          "form": "리뷰 이벤트도 있었니",
          "original_form": "리뷰 이벤트도 있었니",
          "speaker_id": "1",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100041469.1.11",
          "form": "배달의 리뷰 이벤트는 필수지",
          "original_form": "배달의 리뷰 이벤트는 필수지",
          "speaker_id": "2",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100041469.1.12",
          "form": "맛있었나 보다",
          "original_form": "맛있었나 보다",
          "speaker_id": "1",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100041469.1.13",
          "form": "내가 먹은 햄버거 중 제일 맛있었어",
          "original_form": "내가 먹은 햄버거 중 제일 맛있었어",
          "speaker_id": "2",
          "time": "20210906 00:52"
