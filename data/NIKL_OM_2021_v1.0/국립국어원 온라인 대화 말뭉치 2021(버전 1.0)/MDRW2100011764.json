      "utterance": [
        {
          "id": "MDRW2100011764.1.1",
          "form": "전 19년부터봤는데",
          "original_form": "전 19년부터봤는데",
          "speaker_id": "2",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011764.1.2",
          "form": "최근엔 아무래도 컴눈명시리즈가 재밋었던거같아요",
          "original_form": "최근엔 아무래도 컴눈명시리즈가 재밋었던거같아요",
          "speaker_id": "2",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011764.1.3",
          "form": "아 저도요 ㅋㅋㅋ애프터스쿨 나올때 감동받앗어요",
          "original_form": "아 저도요 ㅋㅋㅋ애프터스쿨 나올때 감동받앗어요",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011764.1.4",
          "form": "저도요.. 다시 모인거 너무 감동이었고 무대도 너무멋졌어요",
          "original_form": "저도요.. 다시 모인거 너무 감동이었고 무대도 너무멋졌어요",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011764.1.5",
          "form": "ㅜㅜ 진짜 열정이 사그라들지않은게 너무멋졌어요",
          "original_form": "ㅜㅜ 진짜 열정이 사그라들지않은게 너무멋졌어요",
          "speaker_id": "1",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011764.1.6",
          "form": "bang 최근에 다시 들었어요 당시엔 안들었는데 ㅋㅋㅋ",
          "original_form": "bang 최근에 다시 들었어요 당시엔 안들었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:59"
        },
        {
          "id": "MDRW2100011764.1.7",
          "form": "저도 컴눈명하고나서",
          "original_form": "저도 컴눈명하고나서",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011764.1.8",
          "form": "애프터스쿨이랑 나인뮤지스 노래들 재생목록에 쫙 넣었어요ㅋㅋㅋㅋ",
          "original_form": "애프터스쿨이랑 나인뮤지스 노래들 재생목록에 쫙 넣었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011764.1.9",
          "form": "앜ㅋㅋㅋㅋ 맞아요 나인뮤지스 ㅜㅜ",
          "original_form": "앜ㅋㅋㅋㅋ 맞아요 나인뮤지스 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011764.1.10",
          "form": "최애곡 있으세요?",
          "original_form": "최애곡 있으세요?",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011764.1.11",
          "form": "하 많죠...",
          "original_form": "하 많죠...",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011764.1.12",
          "form": "돌스 드라마 와일드 티켓 뉴스..",
          "original_form": "돌스 드라마 와일드 티켓 뉴스..",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011764.1.13",
          "form": "좋은노래 너무많아요진짜",
          "original_form": "좋은노래 너무많아요진짜",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011764.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아 늦덕되셔서 어케요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아 늦덕되셔서 어케요",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011764.1.15",
          "form": "나뮤 컴백 시위해야겟아ㅓ요",
          "original_form": "나뮤 컴백 시위해야겟아ㅓ요",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011764.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ그러게요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러게요...",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011764.1.17",
          "form": "활동할때 빛 못본 여돌들 더더 잘됐으면좋겠네요",
          "original_form": "활동할때 빛 못본 여돌들 더더 잘됐으면좋겠네요",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011764.1.18",
          "form": "맞아요 달샤벳이나 레인보우 같은 ㅜㅜ",
          "original_form": "맞아요 달샤벳이나 레인보우 같은 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011764.1.19",
          "form": "맞아요 티아라도그렇고....ㅠ",
          "original_form": "맞아요 티아라도그렇고....ㅠ",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100011764.1.20",
