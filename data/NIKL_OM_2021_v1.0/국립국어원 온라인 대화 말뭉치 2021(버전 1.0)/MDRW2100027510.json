      "utterance": [
        {
          "id": "MDRW2100027510.1.1",
          "form": "길냥이들만 안하구 급하게 임보 필요한 친규들이요 ㅎㅎㅎㅎ",
          "original_form": "길냥이들만 안하구 급하게 임보 필요한 친규들이요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027510.1.2",
          "form": "버려진 친구들이라던가...!",
          "original_form": "버려진 친구들이라던가...!",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027510.1.3",
          "form": "그런 식의 여러방면이군요",
          "original_form": "그런 식의 여러방면이군요",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027510.1.4",
          "form": "네 맞아요 ㅎㅎㅎ",
          "original_form": "네 맞아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027510.1.5",
          "form": "참 예쁜 아이들인데 안타까운 사연이 너무 많아요ㅜㅜ",
          "original_form": "참 예쁜 아이들인데 안타까운 사연이 너무 많아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027510.1.6",
          "form": "ㅇㅅㅠ 맞아요 너무 쉽게 사서 그런건지 너무 쉽게 버리는 것 같아요ㅠㅠ",
          "original_form": "ㅇㅅㅠ 맞아요 너무 쉽게 사서 그런건지 너무 쉽게 버리는 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027510.1.7",
          "form": "그러니까요ㅜㅜ 사지말고 다들 입양했으면",
          "original_form": "그러니까요ㅜㅜ 사지말고 다들 입양했으면",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027510.1.8",
          "form": "저는 전에 병아리 길렀었어요",
          "original_form": "저는 전에 병아리 길렀었어요",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027510.1.9",
          "form": "새상에 엄청 귀여웠겠어요",
          "original_form": "새상에 엄청 귀여웠겠어요",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027510.1.10",
          "form": "날짜로는 오래 길렀어요 백일 넘었으니까요 ㅎㅎ",
          "original_form": "날짜로는 오래 길렀어요 백일 넘었으니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027510.1.11",
          "form": "우와 진짜 오래네요!",
          "original_form": "우와 진짜 오래네요!",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027510.1.12",
          "form": "동물이랑 같이 사는건 진짜 좋은 것 같아요 ㅎㅎㅎㅎ",
          "original_form": "동물이랑 같이 사는건 진짜 좋은 것 같아요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027510.1.13",
          "form": "그쵸그쵸 근데 제가 키운 병아리는 아파서 닭도 못 됐어요ㅠㅠ",
          "original_form": "그쵸그쵸 근데 제가 키운 병아리는 아파서 닭도 못 됐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027510.1.14",
          "form": "아이고ㅠㅠ 무럭무럭 컸어야하는데 말이에요ㅜㅜㅜ",
          "original_form": "아이고ㅠㅠ 무럭무럭 컸어야하는데 말이에요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027510.1.15",
          "form": "날개깃만 털갈이 했어요ㅠㅠ",
          "original_form": "날개깃만 털갈이 했어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027510.1.16",
