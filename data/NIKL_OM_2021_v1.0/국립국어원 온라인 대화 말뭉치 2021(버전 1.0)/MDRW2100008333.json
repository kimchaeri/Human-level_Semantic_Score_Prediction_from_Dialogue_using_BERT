      "utterance": [
        {
          "id": "MDRW2100008333.1.1",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008333.1.2",
          "form": "직접보진않고 인터넷으로 접했어요",
          "original_form": "직접보진않고 인터넷으로 접했어요",
          "speaker_id": "2",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100008333.1.3",
          "form": "주먹쿵야라든지 ㅋㅋㅋ",
          "original_form": "주먹쿵야라든지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100008333.1.4",
          "form": "아아 맞아요 사람들 드립이 되게재밌더라구요 !",
          "original_form": "아아 맞아요 사람들 드립이 되게재밌더라구요 !",
          "speaker_id": "1",
          "time": "20210822 15:18"
        },
        {
          "id": "MDRW2100008333.1.5",
          "form": "저는 배구봤는데 너무 재밌게 봤어요",
          "original_form": "저는 배구봤는데 너무 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210822 15:18"
        },
        {
          "id": "MDRW2100008333.1.6",
          "form": "앗 배구도",
          "original_form": "앗 배구도",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008333.1.7",
          "form": "연경언니 넘 멋져요..(사실 배구경기 안봄",
          "original_form": "연경언니 넘 멋져요..(사실 배구경기 안봄",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008333.1.8",
          "form": "진짜 너무 잘하고 멋있더라구요",
          "original_form": "진짜 너무 잘하고 멋있더라구요",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008333.1.9",
          "form": "다른 선수들도 잘해서 배구에 빠져버렸어요",
          "original_form": "다른 선수들도 잘해서 배구에 빠져버렸어요",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008333.1.10",
          "form": "마자여 사실 저는 알못이라 연경언니만 아는데",
          "original_form": "마자여 사실 저는 알못이라 연경언니만 아는데",
          "speaker_id": "2",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008333.1.11",
          "form": "190이 넘는담서요 완전 짱이야",
          "original_form": "190이 넘는담서요 완전 짱이야",
          "speaker_id": "2",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008333.1.12",
          "form": "완전 멋져요",
          "original_form": "완전 멋져요",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008333.1.13",
          "form": "혹시 양궁 경기도 잠깐 보셨나요?",
          "original_form": "혹시 양궁 경기도 잠깐 보셨나요?",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008333.1.14",
          "form": "저는 짤만봤어요",
          "original_form": "저는 짤만봤어요",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100008333.1.15",
          "form": "근육끊어진 아재가 쏘고",
          "original_form": "근육끊어진 아재가 쏘고",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100008333.1.16",
          "form": "끝 하고 입모양말하는것도 보고",
          "original_form": "끝 하고 입모양말하는것도 보고",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100008333.1.17",
          "form": "맞아요 양궁 다 엄청 잘하시더라구요!",
          "original_form": "맞아요 양궁 다 엄청 잘하시더라구요!",
          "speaker_id": "1",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100008333.1.18",
          "form": "먼거리인데도 10점을 되게 잘쏘셔서 놀랐어요",
          "original_form": "먼거리인데도 10점을 되게 잘쏘셔서 놀랐어요",
          "speaker_id": "1",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100008333.1.19",
          "form": "진짜 보면 국뽕이 찰 만 해요",
          "original_form": "진짜 보면 국뽕이 찰 만 해요",
          "speaker_id": "2",
          "time": "20210823 20:28"
        },
        {
          "id": "MDRW2100008333.1.20",
          "form": "그러니까요.. 양궁은 한국이 진짜 잘해요!",
          "original_form": "그러니까요.. 양궁은 한국이 진짜 잘해요!",
          "speaker_id": "1",
          "time": "20210823 20:30"
        },
        {
          "id": "MDRW2100008333.1.21",
          "form": "외국애들이 견제 엄청 하잖아요",
          "original_form": "외국애들이 견제 엄청 하잖아요",
          "speaker_id": "2",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100008333.1.22",
          "form": "제덕쿵야 화이팅도 그거 의식한 전략이라네여",
          "original_form": "제덕쿵야 화이팅도 그거 의식한 전략이라네여",
          "speaker_id": "2",
          "time": "20210823 20:36"
        },
        {
          "id": "MDRW2100008333.1.23",
          "form": "오 그건 몰랐네요! 양궁 할때 항상 조용한 분위기였던 것 같은데",
          "original_form": "오 그건 몰랐네요! 양궁 할때 항상 조용한 분위기였던 것 같은데",
          "speaker_id": "1",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100008333.1.24",
          "form": "이번에는 파이팅 구호되치면서 하니까 뭔가 더 재미나게 보게 되더라구요ㅋㅋ",
          "original_form": "이번에는 파이팅 구호되치면서 하니까 뭔가 더 재미나게 보게 되더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100008333.1.25",
          "form": "애기가 그러면 을매나 귀엽게요 ㅋㅋㅋㅋ",
          "original_form": "애기가 그러면 을매나 귀엽게요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:03"
        }
      ]
    }
