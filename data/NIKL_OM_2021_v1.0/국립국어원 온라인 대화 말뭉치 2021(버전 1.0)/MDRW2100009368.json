      "utterance": [
        {
          "id": "MDRW2100009368.1.1",
          "form": "영화좋아하세요?",
          "original_form": "영화좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009368.1.2",
          "form": "네 엄청 좋아해요!",
          "original_form": "네 엄청 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009368.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009368.1.4",
          "form": "개인적으로",
          "original_form": "개인적으로",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009368.1.5",
          "form": "여러 장르 모두 챙겨본거같아요!",
          "original_form": "여러 장르 모두 챙겨본거같아요!",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009368.1.6",
          "form": "전 로코요",
          "original_form": "전 로코요",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009368.1.7",
          "form": "로맨스랑 코미디 말하시는건가용?",
          "original_form": "로맨스랑 코미디 말하시는건가용?",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009368.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009368.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009368.1.10",
          "form": "둘다 재밌죠",
          "original_form": "둘다 재밌죠",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009368.1.11",
          "form": "저도 개인적으로 코미디 엄청 좋아하는데",
          "original_form": "저도 개인적으로 코미디 엄청 좋아하는데",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009368.1.12",
          "form": "스트레스풀고",
          "original_form": "스트레스풀고",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009368.1.13",
          "form": "맞아요 스트레스도 풀리고",
          "original_form": "맞아요 스트레스도 풀리고",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009368.1.14",
          "form": "웃음도 잔뜩 나와서 좋더라구요!",
          "original_form": "웃음도 잔뜩 나와서 좋더라구요!",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009368.1.15",
          "form": "그게 아니면 웃을일이 없어요",
          "original_form": "그게 아니면 웃을일이 없어요",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009368.1.16",
          "form": "ㅠㅠ 그랴도",
          "original_form": "ㅠㅠ 그랴도",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009368.1.17",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009368.1.18",
          "form": "코미디영화라도 보면서",
          "original_form": "코미디영화라도 보면서",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009368.1.19",
          "form": "웃으시면 되죠!",
          "original_form": "웃으시면 되죠!",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009368.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
