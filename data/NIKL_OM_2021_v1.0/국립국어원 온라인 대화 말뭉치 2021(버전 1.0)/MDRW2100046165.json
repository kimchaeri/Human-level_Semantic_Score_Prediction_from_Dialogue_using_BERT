      "utterance": [
        {
          "id": "MDRW2100046165.1.1",
          "form": "더마켓 사셨어요?",
          "original_form": "더마켓 사셨어요?",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.3",
          "form": "쿠폰줘요",
          "original_form": "쿠폰줘요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.4",
          "form": "쿠폰이 45퍼나오면사려구요",
          "original_form": "쿠폰이 45퍼나오면사려구요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.5",
          "form": "어제 43나와서 그냥 주문",
          "original_form": "어제 43나와서 그냥 주문",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.6",
          "form": "45퍼나왔을땐 담주 토스",
          "original_form": "45퍼나왔을땐 담주 토스",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.7",
          "form": "나올까봐 미루고나니",
          "original_form": "나올까봐 미루고나니",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.8",
          "form": "저번달 주문했다고 5프로 중복주더라구요",
          "original_form": "저번달 주문했다고 5프로 중복주더라구요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.9",
          "form": "44퍼나오고 40퍼나오고 ㅋ",
          "original_form": "44퍼나오고 40퍼나오고 ㅋ",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046165.1.10",
          "form": "44퍼라도 사시지",
          "original_form": "44퍼라도 사시지",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046165.1.11",
          "form": "45퍼는 5만원이상이더라구요ㅋ",
          "original_form": "45퍼는 5만원이상이더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046165.1.12",
          "form": "그럴껄그랬나봐요",
          "original_form": "그럴껄그랬나봐요",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046165.1.13",
          "form": "5만원 넘기는것 쉽긴하지만",
          "original_form": "5만원 넘기는것 쉽긴하지만",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046165.1.14",
          "form": "저는 이것저것담아서 9만얼마던데요",
          "original_form": "저는 이것저것담아서 9만얼마던데요",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046165.1.15",
          "form": "할인받아서 5만윈",
          "original_form": "할인받아서 5만윈",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046165.1.16",
          "form": "헐ㅋ 뭘담으셨길래ㅋ",
          "original_form": "헐ㅋ 뭘담으셨길래ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046165.1.17",
          "form": "전 죽으로 왕창샀어요",
          "original_form": "전 죽으로 왕창샀어요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046165.1.18",
          "form": "애들 미니돈까스 등등",
          "original_form": "애들 미니돈까스 등등",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046165.1.19",
          "form": "냉동식품으로 쫘악",
          "original_form": "냉동식품으로 쫘악",
