      "utterance": [
        {
          "id": "MDRW2100028029.1.1",
          "form": "최근에 어디 여행 가신 곳 있으세요?",
          "original_form": "최근에 어디 여행 가신 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100028029.1.2",
          "form": "최근에는 못 갔어요",
          "original_form": "최근에는 못 갔어요",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100028029.1.3",
          "form": "확진자 줄어들면 호캉스 가보고 싶네요.",
          "original_form": "확진자 줄어들면 호캉스 가보고 싶네요.",
          "speaker_id": "1",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028029.1.4",
          "form": "우와 저도요!!!!!!!! 호캉스 너무 해보고 싶어요...",
          "original_form": "우와 저도요!!!!!!!! 호캉스 너무 해보고 싶어요...",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028029.1.5",
          "form": "침대에만 누워있고 싶어요...",
          "original_form": "침대에만 누워있고 싶어요...",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028029.1.6",
          "form": "집이 아닌.",
          "original_form": "집이 아닌.",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028029.1.7",
          "form": "맞아요ㅋㅋ 호텔 음식도 맛있다고 하구...",
          "original_form": "맞아요ㅋㅋ 호텔 음식도 맛있다고 하구...",
          "speaker_id": "1",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100028029.1.8",
          "form": "호텔 음식 궁금하네욥",
          "original_form": "호텔 음식 궁금하네욥",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100028029.1.9",
          "form": "룸서비스 라면도 비싸다고 하더라구요ㅋㅋ",
          "original_form": "룸서비스 라면도 비싸다고 하더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028029.1.10",
          "form": "엄청 맛있긴하다는데ㅋㅋ 해물도 많이 넣고요",
          "original_form": "엄청 맛있긴하다는데ㅋㅋ 해물도 많이 넣고요",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028029.1.11",
          "form": "아 라면도 있어요?ㅋㅋㅋㅋㅋㅋ 한... 6천원 하려나... 호캉스도 뭔가 바다 보이는 곳으로 가구 싶네요",
          "original_form": "아 라면도 있어요?ㅋㅋㅋㅋㅋㅋ 한... 6천원 하려나... 호캉스도 뭔가 바다 보이는 곳으로 가구 싶네요",
          "speaker_id": "2",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028029.1.12",
          "form": "오 맞아요 저 부산 가보고싶어요",
          "original_form": "오 맞아요 저 부산 가보고싶어요",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028029.1.13",
          "form": "바다만 보러 가지 따로 여행을 가본적이 없어요",
          "original_form": "바다만 보러 가지 따로 여행을 가본적이 없어요",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028029.1.14",
          "form": "헉 진짜요!",
          "original_form": "헉 진짜요!",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028029.1.15",
          "form": "그냥 해운대만 보고 돌아왔던 기억밖에 없네요",
          "original_form": "그냥 해운대만 보고 돌아왔던 기억밖에 없네요",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028029.1.16",
          "form": "친척집이 있어서 매번 거기서 자고온 기억만 있어요",
          "original_form": "친척집이 있어서 매번 거기서 자고온 기억만 있어요",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028029.1.17",
          "form": "헉 뭔가 그냥... 친척집만 가는 느낌으로 가시는군요!",
          "original_form": "헉 뭔가 그냥... 친척집만 가는 느낌으로 가시는군요!",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028029.1.18",
          "form": "먹방투어 함 해보고 싶어요 부간",
          "original_form": "먹방투어 함 해보고 싶어요 부간",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028029.1.19",
          "form": "산",
          "original_form": "산",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028029.1.20",
          "form": "맞아요 맛있는거 많다고만 들었지 회말곤 모르겠네요ㅋㅋ",
          "original_form": "맞아요 맛있는거 많다고만 들었지 회말곤 모르겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028029.1.21",
          "form": "그러게요 그리고 어묵도! 거기 시장 한 번 가보고 싶어요",
          "original_form": "그러게요 그리고 어묵도! 거기 시장 한 번 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028029.1.22",
          "form": "시장은 먹을거 천지일거 같은데요ㅋㅋㅋ",
          "original_form": "시장은 먹을거 천지일거 같은데요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028029.1.23",
          "form": "이것이 음식 여행...",
          "original_form": "이것이 음식 여행...",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028029.1.24",
          "form": "너무 먹으러 놀러 다니고 싶어요 ㅋㅋ",
          "original_form": "너무 먹으러 놀러 다니고 싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028029.1.25",
          "form": "먹는게 반이긴 하죠ㅋㅋㅋ",
          "original_form": "먹는게 반이긴 하죠ㅋㅋㅋ",
          "speaker_id": "1",
