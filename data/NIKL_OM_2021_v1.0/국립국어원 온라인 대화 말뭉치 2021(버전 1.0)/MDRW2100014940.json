      "utterance": [
        {
          "id": "MDRW2100014940.1.1",
          "form": "저도 연안 식당 가봤는데 확실히 엄지네가 더 맛있어요",
          "original_form": "저도 연안 식당 가봤는데 확실히 엄지네가 더 맛있어요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014940.1.2",
          "form": "그쵸 연안식당 기름에 자부심이 대단하길래 그것도 기대했는데.",
          "original_form": "그쵸 연안식당 기름에 자부심이 대단하길래 그것도 기대했는데.",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014940.1.3",
          "form": "향도 고소함도 기대만 못하더라고요",
          "original_form": "향도 고소함도 기대만 못하더라고요",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014940.1.4",
          "form": "담에 강릉가면 엄지네 꼭 가야겠어요",
          "original_form": "담에 강릉가면 엄지네 꼭 가야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014940.1.5",
          "form": "가격은 엄지네가 좀더 비싸긴 했어요ㅠㅠ 아무래도 관광지라서 그런지...",
          "original_form": "가격은 엄지네가 좀더 비싸긴 했어요ㅠㅠ 아무래도 관광지라서 그런지...",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014940.1.6",
          "form": "혹시 가시게되면 육사시미도 드셔보세요!!",
          "original_form": "혹시 가시게되면 육사시미도 드셔보세요!!",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014940.1.7",
          "form": "육사시미도 판다니 신기한 조합이네요",
          "original_form": "육사시미도 판다니 신기한 조합이네요",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014940.1.8",
          "form": "솔직히 꼬막보다 이게 더 기대돼요ㅎ",
          "original_form": "솔직히 꼬막보다 이게 더 기대돼요ㅎ",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014940.1.9",
          "form": "제가 완전 육식파라",
          "original_form": "제가 완전 육식파라",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014940.1.10",
          "form": "아 그러네요ㅋㅋㅋㅋ 사실 추천메뉴라길래 먹었는데",
          "original_form": "아 그러네요ㅋㅋㅋㅋ 사실 추천메뉴라길래 먹었는데",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014940.1.11",
          "form": "그러고보니 조합이 신기한 조합이네요",
          "original_form": "그러고보니 조합이 신기한 조합이네요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014940.1.12",
          "form": "인터넷 검색해보니 비주얼이 너무 훌륭하네요",
          "original_form": "인터넷 검색해보니 비주얼이 너무 훌륭하네요",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014940.1.13",
          "form": "육사시미 가격도 생각보다 착하고",
          "original_form": "육사시미 가격도 생각보다 착하고",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014940.1.14",
          "form": "이거 먹으러 강릉 여행가고 싶네요",
          "original_form": "이거 먹으러 강릉 여행가고 싶네요",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014940.1.15",
          "form": "맛집 찾아다니는 여행 너무 좋죠ㅠ",
          "original_form": "맛집 찾아다니는 여행 너무 좋죠ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014940.1.16",
          "form": "사실 엄지네도 서울이나 여러 지점이 있긴하더라고요",
          "original_form": "사실 엄지네도 서울이나 여러 지점이 있긴하더라고요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014940.1.17",
          "form": "헉 서울에도 있나요!",
          "original_form": "헉 서울에도 있나요!",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014940.1.18",
          "form": "전 강릉에만 있는줄 알았어요",
          "original_form": "전 강릉에만 있는줄 알았어요",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014940.1.19",
          "form": "저도 있다는 말만 듣고 가보진 않아서요",
          "original_form": "저도 있다는 말만 듣고 가보진 않아서요",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014940.1.20",
          "form": "사실 강릉이 꼬막 산지는 아니니까요 ㅋㅋㅋㅋ",
          "original_form": "사실 강릉이 꼬막 산지는 아니니까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014940.1.21",
          "form": "아 그렇지요ㅎㅎ 듣고보니ㅎ",
          "original_form": "아 그렇지요ㅎㅎ 듣고보니ㅎ",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014940.1.22",
          "form": "검색하니 현백에 있네요",
          "original_form": "검색하니 현백에 있네요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014940.1.23",
          "form": "백화점은 왠지 그느낌이 안나는데",
          "original_form": "백화점은 왠지 그느낌이 안나는데",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014940.1.24",
          "form": "그래도 강릉 못가도 아쉬움 달랠수 있겠어요",
          "original_form": "그래도 강릉 못가도 아쉬움 달랠수 있겠어요",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014940.1.25",
          "form": "서울에 있는지점도 맛집이었으면 좋겠네요",
          "original_form": "서울에 있는지점도 맛집이었으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014940.1.26",
          "form": "그러길 바라요. 그럴 거예요!!",
          "original_form": "그러길 바라요. 그럴 거예요!!",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014940.1.27",
          "form": "내일은 꿩 닭 느낌으로 근처 연안식당이라도 가야겠어요",
          "original_form": "내일은 꿩 닭 느낌으로 근처 연안식당이라도 가야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:38"
        }
      ]
