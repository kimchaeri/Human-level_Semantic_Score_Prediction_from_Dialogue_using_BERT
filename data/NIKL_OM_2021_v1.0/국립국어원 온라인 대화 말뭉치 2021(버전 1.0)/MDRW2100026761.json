        },
        {
          "id": "MDRW2100026761.1.2",
          "form": "유통기한 얼마 안남은 음싣들",
          "original_form": "유통기한 얼마 안남은 음싣들",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026761.1.3",
          "form": "엄청 싸게파는 샵들이있는데",
          "original_form": "엄청 싸게파는 샵들이있는데",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026761.1.4",
          "form": "재고처리가 목적이라서 배송비도 다 무료고",
          "original_form": "재고처리가 목적이라서 배송비도 다 무료고",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026761.1.5",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.6",
          "form": "나 그런거",
          "original_form": "나 그런거",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.7",
          "form": "진짜 금방 먹을 수 있어서",
          "original_form": "진짜 금방 먹을 수 있어서",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.8",
          "form": "막 70 80프로 할인하고 이렇거든",
          "original_form": "막 70 80프로 할인하고 이렇거든",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.9",
          "form": "상관 없는데",
          "original_form": "상관 없는데",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.11",
          "form": "그래서 나도",
          "original_form": "그래서 나도",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.12",
          "form": "엄마 일할때 간식겸으로",
          "original_form": "엄마 일할때 간식겸으로",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.13",
          "form": "처음 사봤는데",
          "original_form": "처음 사봤는데",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.14",
          "form": "완전좋음",
          "original_form": "완전좋음",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026761.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026761.1.16",
          "form": "미주라 통밀 어쩌구 빵",
