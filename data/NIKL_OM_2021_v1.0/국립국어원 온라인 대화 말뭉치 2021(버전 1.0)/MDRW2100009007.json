      "utterance": [
        {
          "id": "MDRW2100009007.1.1",
          "form": "안녕하세요 저녁드셨나요",
          "original_form": "안녕하세요 저녁드셨나요",
          "speaker_id": "1",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100009007.1.2",
          "form": "넹넹 먹었어요!!",
          "original_form": "넹넹 먹었어요!!",
          "speaker_id": "2",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009007.1.3",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009007.1.4",
          "form": "저는 물냉면 먹었어요!",
          "original_form": "저는 물냉면 먹었어요!",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009007.1.5",
          "form": "전 치킨 먹엇어용",
          "original_form": "전 치킨 먹엇어용",
          "speaker_id": "2",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009007.1.6",
          "form": "더울땐 물냉이 짱이죠",
          "original_form": "더울땐 물냉이 짱이죠",
          "speaker_id": "2",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009007.1.7",
          "form": "헐 무슨치킨 드셧나용",
          "original_form": "헐 무슨치킨 드셧나용",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009007.1.8",
          "form": "저도 치킨 진짜 좋아해요",
          "original_form": "저도 치킨 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009007.1.9",
          "form": "저 노랑통닭에서 깐풍치킨 먹었는데 진짜 맛있어요!!",
          "original_form": "저 노랑통닭에서 깐풍치킨 먹었는데 진짜 맛있어요!!",
          "speaker_id": "2",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009007.1.10",
          "form": "오 노랑 통닭은 안 먹어봐서 궁금하네요",
          "original_form": "오 노랑 통닭은 안 먹어봐서 궁금하네요",
          "speaker_id": "1",
          "time": "20210823 01:15"
        },
        {
          "id": "MDRW2100009007.1.11",
          "form": "저는 요즘 푸라닭에 꽂혔어요..",
          "original_form": "저는 요즘 푸라닭에 꽂혔어요..",
          "speaker_id": "1",
          "time": "20210823 01:15"
        },
        {
          "id": "MDRW2100009007.1.12",
          "form": "투움바치킨 맛있더라고요",
          "original_form": "투움바치킨 맛있더라고요",
          "speaker_id": "1",
          "time": "20210823 01:15"
        },
        {
          "id": "MDRW2100009007.1.13",
          "form": "오 저도 먹어봤는데 제가 크림류 좋아해서 그런지",
          "original_form": "오 저도 먹어봤는데 제가 크림류 좋아해서 그런지",
          "speaker_id": "2",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009007.1.14",
          "form": "되게 맛있더라구요!!",
          "original_form": "되게 맛있더라구요!!",
          "speaker_id": "2",
          "time": "20210823 01:24"
        },
        {
          "id": "MDRW2100009007.1.15",
          "form": "저도 평소에 느끼한거 좋아해서 맛있게 먹었어요!! 완전제취향 ",
          "original_form": "저도 평소에 느끼한거 좋아해서 맛있게 먹었어요!! 완전제취향 ",
          "speaker_id": "1",
          "time": "20210823 01:26"
        },
        {
          "id": "MDRW2100009007.1.16",
          "form": "좋아하는 음식 있으신가요??",
          "original_form": "좋아하는 음식 있으신가요??",
          "speaker_id": "1",
          "time": "20210823 01:27"
        },
        {
          "id": "MDRW2100009007.1.17",
          "form": "저도 느끼한거 완전좋아해요!!",
          "original_form": "저도 느끼한거 완전좋아해요!!",
          "speaker_id": "2",
          "time": "20210823 08:16"
        },
        {
          "id": "MDRW2100009007.1.18",
          "form": "크림파스타도 좋아하구요",
          "original_form": "크림파스타도 좋아하구요",
          "speaker_id": "2",
          "time": "20210823 08:16"
        },
        {
          "id": "MDRW2100009007.1.19",
          "form": "헉 저도 까르보나라 같은거 좋아해요 ㅎㅎ",
          "original_form": "헉 저도 까르보나라 같은거 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 08:53"
        },
        {
          "id": "MDRW2100009007.1.20",
          "form": "요즘 로제떡볶이가 유행이던데 드셔보셨나요?",
          "original_form": "요즘 로제떡볶이가 유행이던데 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210823 08:53"
        },
        {
          "id": "MDRW2100009007.1.21",
          "form": "오 로제떡볶이 완전 좋아해요!!",
          "original_form": "오 로제떡볶이 완전 좋아해요!!",
          "speaker_id": "2",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100009007.1.22",
          "form": "어디거 드셔보셨어요?",
          "original_form": "어디거 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009007.1.23",
          "form": "저 엽떡,신전,태리욤 ㅋㅋ",
          "original_form": "저 엽떡,신전,태리욤 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009007.1.24",
          "form": "저는 삼첩분식이랑 배떡 먹어봤는데 삼첩분식은 별로였고 배떡은 맛있었어요!",
          "original_form": "저는 삼첩분식이랑 배떡 먹어봤는데 삼첩분식은 별로였고 배떡은 맛있었어요!",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009007.1.25",
          "form": "태리는 뭔가용",
          "original_form": "태리는 뭔가용",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009007.1.26",
          "form": "아 저도 삼첩먹어봣어요 삼첩은 기본이 더낫더라구요",
          "original_form": "아 저도 삼첩먹어봣어요 삼첩은 기본이 더낫더라구요",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009007.1.27",
          "form": "태리로제떡볶이라는 가게가 잇는데",
          "original_form": "태리로제떡볶이라는 가게가 잇는데",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009007.1.28",
          "form": "주변에 잇으면 드셔보세요!!",
          "original_form": "주변에 잇으면 드셔보세요!!",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009007.1.29",
          "form": "맛잇어요",
          "original_form": "맛잇어요",
          "speaker_id": "2",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100009007.1.30",
          "form": "감사합니당 저는 첨들어보내요",
          "original_form": "감사합니당 저는 첨들어보내요",
          "speaker_id": "1",
