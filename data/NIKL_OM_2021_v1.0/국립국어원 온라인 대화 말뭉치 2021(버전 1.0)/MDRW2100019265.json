      "utterance": [
        {
          "id": "MDRW2100019265.1.1",
          "form": "좋아하시는 음식이 무엇인가용?",
          "original_form": "좋아하시는 음식이 무엇인가용?",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019265.1.2",
          "form": "정말 많아서 고를 수 없는데 요즘은 치킨이요",
          "original_form": "정말 많아서 고를 수 없는데 요즘은 치킨이요",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019265.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019265.1.4",
          "form": "전 마라탕이요",
          "original_form": "전 마라탕이요",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019265.1.5",
          "form": "앗 떡볶이도 좋아요",
          "original_form": "앗 떡볶이도 좋아요",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019265.1.6",
          "form": "마라탕 좋아하시나요?",
          "original_form": "마라탕 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019265.1.7",
          "form": "마라탕 아직 한번도 안먹어봤어요..! 무슨 맛인가요",
          "original_form": "마라탕 아직 한번도 안먹어봤어요..! 무슨 맛인가요",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019265.1.8",
          "form": "음 매콤한데 고추가루 느낌의 매콤은 아니고...",
          "original_form": "음 매콤한데 고추가루 느낌의 매콤은 아니고...",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019265.1.9",
          "form": "시원하게 매운데 얼얼 느낌의 매콤?",
          "original_form": "시원하게 매운데 얼얼 느낌의 매콤?",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019265.1.10",
          "form": "페퍼론치노 같은 매운 맛일까요?",
          "original_form": "페퍼론치노 같은 매운 맛일까요?",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019265.1.11",
          "form": "음 페퍼론치노 맛이 뭔지 잘 모르겠어요ㅠㅎㅎ",
          "original_form": "음 페퍼론치노 맛이 뭔지 잘 모르겠어요ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019265.1.12",
          "form": "근데 향신료지만 고수같이 엄청 특이한 향신료는 아닌 느낌이에요",
          "original_form": "근데 향신료지만 고수같이 엄청 특이한 향신료는 아닌 느낌이에요",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019265.1.13",
          "form": "궁금하긴해요!! name1님은 매운거 잘드시나봐요?!",
          "original_form": "궁금하긴해요!! &name1&님은 매운거 잘드시나봐요?!",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019265.1.14",
          "form": "사실 전 맵찔이에요ㅋㅋㅋㅋㅋ",
          "original_form": "사실 전 맵찔이에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019265.1.15",
          "form": "근데 마라탕에는 매움 단계가 있어서",
          "original_form": "근데 마라탕에는 매움 단계가 있어서",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019265.1.16",
          "form": "0단계는 아예 안매운 백탕이구 1단계부터 매워져요",
          "original_form": "0단계는 아예 안매운 백탕이구 1단계부터 매워져요",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019265.1.17",
          "form": "저는 1단계에서만 먹어요ㅎㅎ",
          "original_form": "저는 1단계에서만 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019265.1.18",
          "form": "저도 맵찔이라서 ㅎㅎ 엽기떡볶이도 못먹는 맵찔이에요 ㅠㅠ 좋아하지만 못먹는 ㅎㅎ",
          "original_form": "저도 맵찔이라서 ㅎㅎ 엽기떡볶이도 못먹는 맵찔이에요 ㅠㅠ 좋아하지만 못먹는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019265.1.19",
          "form": "다음에 1단계로 도전해봐야겠어요",
          "original_form": "다음에 1단계로 도전해봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
