      "utterance": [
        {
          "id": "MDRW2100033614.1.1",
          "form": "같이살때도 그냥저냥이였는데",
          "original_form": "같이살때도 그냥저냥이였는데",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100033614.1.2",
          "form": "나오고나서 진짜 필요할때만 해요",
          "original_form": "나오고나서 진짜 필요할때만 해요",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100033614.1.3",
          "form": "그래도 가족이 젤 인거같아요 ㅎ",
          "original_form": "그래도 가족이 젤 인거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100033614.1.4",
          "form": "결국은 가족밖에 없죠",
          "original_form": "결국은 가족밖에 없죠",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100033614.1.5",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100033614.1.6",
          "form": "저는 지금 연년생 처음엔 힘들었거든요",
          "original_form": "저는 지금 연년생 처음엔 힘들었거든요",
          "speaker_id": "2",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100033614.1.7",
          "form": "친구들이야 그때뿐이고 다늙고 나면 가족밖에",
          "original_form": "친구들이야 그때뿐이고 다늙고 나면 가족밖에",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100033614.1.8",
          "form": "후회도하고...",
          "original_form": "후회도하고...",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100033614.1.9",
          "form": "근데 지금도 보면 둘이서 잘놀고",
          "original_form": "근데 지금도 보면 둘이서 잘놀고",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100033614.1.10",
          "form": "유치원에서도 서로 챙겨주고하니",
          "original_form": "유치원에서도 서로 챙겨주고하니",
          "speaker_id": "2",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100033614.1.11",
          "form": "혼자보다 둘이 좋구나싶어요 ㅎ",
          "original_form": "혼자보다 둘이 좋구나싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033614.1.12",
          "form": "제생각도 자녀는 둘이 좋은듯요",
          "original_form": "제생각도 자녀는 둘이 좋은듯요",
          "speaker_id": "1",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033614.1.13",
          "form": "부모입장에선 딸하나 아들하나",
          "original_form": "부모입장에선 딸하나 아들하나",
          "speaker_id": "1",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033614.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033614.1.15",
          "form": "애들입장에선 같은성별이 좋더라구요",
          "original_form": "애들입장에선 같은성별이 좋더라구요",
          "speaker_id": "1",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033614.1.16",
          "form": "딸하나 아들하나는 부모입장이예요",
          "original_form": "딸하나 아들하나는 부모입장이예요",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100033614.1.17",
          "form": "애도 없는데 애있는사람같네요ㅋ",
          "original_form": "애도 없는데 애있는사람같네요ㅋ",
          "speaker_id": "1",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100033614.1.18",
          "form": "저도 그렇게 생각했어요",
          "original_form": "저도 그렇게 생각했어요",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100033614.1.19",
          "form": "그래서 둘째생겼을때 딸이였으면 했죠",
          "original_form": "그래서 둘째생겼을때 딸이였으면 했죠",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100033614.1.20",
          "form": "시엄니는 싫어하셨겠지만요 ㅎㅎ",
          "original_form": "시엄니는 싫어하셨겠지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100033614.1.21",
          "form": "근데 확실히 시부모님은 아들운 원하시는듯",
          "original_form": "근데 확실히 시부모님은 아들운 원하시는듯",
          "speaker_id": "1",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100033614.1.22",
          "form": "옛날분은 옛날분ㅎ",
          "original_form": "옛날분은 옛날분ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100033614.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100033614.1.24",
          "form": "안그러시는듯 하시면서도 티가나요 ㅎ",
          "original_form": "안그러시는듯 하시면서도 티가나요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:16"
        }
      ]
