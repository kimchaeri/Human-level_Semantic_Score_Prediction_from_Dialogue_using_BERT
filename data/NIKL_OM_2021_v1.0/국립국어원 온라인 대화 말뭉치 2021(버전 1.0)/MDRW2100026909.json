      "utterance": [
        {
          "id": "MDRW2100026909.1.1",
          "form": "곰표맥주드셔보셨나요",
          "original_form": "곰표맥주드셔보셨나요",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026909.1.2",
          "form": "품절이라 아직 영접하지못했습니다",
          "original_form": "품절이라 아직 영접하지못했습니다",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026909.1.3",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026909.1.4",
          "form": "전먹어봤는데 맛있어요",
          "original_form": "전먹어봤는데 맛있어요",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026909.1.5",
          "form": "밀맥주에요",
          "original_form": "밀맥주에요",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026909.1.6",
          "form": "콜라보는 평이 좋아요",
          "original_form": "콜라보는 평이 좋아요",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026909.1.7",
          "form": "말표흑맥주 드셔보셨나요",
          "original_form": "말표흑맥주 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026909.1.8",
          "form": "두꺼비 피규어도 탐납니다",
          "original_form": "두꺼비 피규어도 탐납니다",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100026909.1.9",
          "form": "진로 두꺼비 그려진 에코백 있어요",
          "original_form": "진로 두꺼비 그려진 에코백 있어요",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.10",
          "form": "저는 외딴곳이라 신문맹혜택을 못받습니다",
          "original_form": "저는 외딴곳이라 신문맹혜택을 못받습니다",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.11",
          "form": "오 득템하셨군요",
          "original_form": "오 득템하셨군요",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.12",
          "form": "말표흑맥 노맛입니다",
          "original_form": "말표흑맥 노맛입니다",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.13",
          "form": "돈아끼세요",
          "original_form": "돈아끼세요",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.14",
          "form": "검은 맥주겠군요",
          "original_form": "검은 맥주겠군요",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.15",
          "form": "곰표 치킨너겟 보셨나요",
          "original_form": "곰표 치킨너겟 보셨나요",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.16",
          "form": "너겟모양이 곰표 곰돌이입니다",
          "original_form": "너겟모양이 곰표 곰돌이입니다",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.17",
          "form": "곰표는 밀가루밖에 알지모답니다",
          "original_form": "곰표는 밀가루밖에 알지모답니다",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026909.1.18",
          "form": "오 깜찍하군요 저처럼",
          "original_form": "오 깜찍하군요 저처럼",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100026909.1.19",
          "form": "귀여워서 먹고싶지만 아파네요",
          "original_form": "귀여워서 먹고싶지만 아파네요",
