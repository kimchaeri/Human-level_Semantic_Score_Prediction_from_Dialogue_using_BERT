      "utterance": [
        {
          "id": "MDRW2100022679.1.1",
          "form": "회를 잘 드세요?",
          "original_form": "회를 잘 드세요?",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.2",
          "form": "회 잘 먹는 편이에요",
          "original_form": "회 잘 먹는 편이에요",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.3",
          "form": "이번 겨울 기다리고 있어요 ㅎㅎ",
          "original_form": "이번 겨울 기다리고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.4",
          "form": "오 겨울에 제철인게 잇나봐요",
          "original_form": "오 겨울에 제철인게 잇나봐요",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.5",
          "form": "네 혹시 알고 있는 거 있어요?ㅎ",
          "original_form": "네 혹시 알고 있는 거 있어요?ㅎ",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.6",
          "form": "저는 잘 모르긴해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 잘 모르긴해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.7",
          "form": "방어는 겨울아니죠?",
          "original_form": "방어는 겨울아니죠?",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.8",
          "form": "어 방어 맞아요 ㅋㅋㅋ",
          "original_form": "어 방어 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.9",
          "form": "정답입니다~",
          "original_form": "정답입니다~",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022679.1.10",
          "form": "아 겨울이구나 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 겨울이구나 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.11",
          "form": "한 1월 2월이 완전 제철!!",
          "original_form": "한 1월 2월이 완전 제철!!",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.12",
          "form": "아니그럼 벌써 겨울다가오는거네요",
          "original_form": "아니그럼 벌써 겨울다가오는거네요",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.13",
          "form": "얼마전에 먹은느낌인데",
          "original_form": "얼마전에 먹은느낌인데",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.14",
          "form": "시간빨라요 ㅠ",
          "original_form": "시간빨라요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.15",
          "form": "이제 가을 시작했지만 난 겨울을 기다린다!",
          "original_form": "이제 가을 시작했지만 난 겨울을 기다린다!",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.16",
          "form": "아 ㅋㅋㅋㅋㅋㅋ그렇군요",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ그렇군요",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.17",
          "form": "근데 방어 너무 비싸요 ㅜㅜ",
          "original_form": "근데 방어 너무 비싸요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.18",
          "form": "방어 맛잇더라구요 몬가 쫀득하고",
          "original_form": "방어 맛잇더라구요 몬가 쫀득하고",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.19",
          "form": "얘도 양식 되면 좋겠어요",
          "original_form": "얘도 양식 되면 좋겠어요",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.20",
          "form": "오 양식이어니군요",
          "original_form": "오 양식이어니군요",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.21",
          "form": "그러면 맛이 달라지지않을ㄴ가요",
          "original_form": "그러면 맛이 달라지지않을ㄴ가요",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.22",
          "form": "얘 양식이였어요?",
          "original_form": "얘 양식이였어요?",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.23",
          "form": "아뇨 저 잘몰라요 ㅋㅋㅋ",
          "original_form": "아뇨 저 잘몰라요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022679.1.24",
          "form": "괜찮아요 광어 우럭도 원래 고급어종이고 양식 하기 쉽지 않은데 우리나라 성공한거래요",
          "original_form": "괜찮아요 광어 우럭도 원래 고급어종이고 양식 하기 쉽지 않은데 우리나라 성공한거래요",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.25",
          "form": "오오오 싱기하다",
          "original_form": "오오오 싱기하다",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.26",
          "form": "광어 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "광어 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.27",
          "form": "그래서 지금 회 저렴하게 먹을 수 있는거래요",
          "original_form": "그래서 지금 회 저렴하게 먹을 수 있는거래요",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.28",
          "form": "저도요 ㅠ 광어가 납작한 얘 맞죠?",
          "original_form": "저도요 ㅠ 광어가 납작한 얘 맞죠?",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.29",
          "form": "아 우오 그런거군요",
          "original_form": "아 우오 그런거군요",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.30",
          "form": "ㅋㅋㅋ광이 한자로 미칠광인 줄 알았는데",
          "original_form": "ㅋㅋㅋ광이 한자로 미칠광인 줄 알았는데",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.31",
          "form": "아니래요",
          "original_form": "아니래요",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.32",
          "form": "아마두요..? 저는 먹을때만봐서ㅋㅋㅋㅋ",
          "original_form": "아마두요..? 저는 먹을때만봐서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.33",
          "form": "아닠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022679.1.34",
          "form": "ㅋㅋㅋㅋ넓을 광이래요 알고 계셨어요..?",
          "original_form": "ㅋㅋㅋㅋ넓을 광이래요 알고 계셨어요..?",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022679.1.35",
          "form": "미칠광인게 드물지않아요?",
          "original_form": "미칠광인게 드물지않아요?",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022679.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022679.1.37",
          "form": "ㅋㅋㅋㅋ저 처음 알았어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ저 처음 알았어요 ㅋㅋㅋㅋㅋㅋㅋ",
