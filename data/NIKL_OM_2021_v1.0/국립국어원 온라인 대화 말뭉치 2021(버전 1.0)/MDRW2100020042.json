        },
        {
          "id": "MDRW2100020042.1.2",
          "form": "네 최근엔 마루밑 아리에티봤어요",
          "original_form": "네 최근엔 마루밑 아리에티봤어요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020042.1.3",
          "form": "애니네요",
          "original_form": "애니네요",
          "speaker_id": "1",
          "time": "20210828 07:38"
        },
        {
          "id": "MDRW2100020042.1.4",
          "form": "네 최근에 보신거는요?",
          "original_form": "네 최근에 보신거는요?",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020042.1.5",
          "form": "모가디슈 볼려구요",
          "original_form": "모가디슈 볼려구요",
          "speaker_id": "1",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100020042.1.6",
          "form": "우와모가디슈궁금해요 저는 샹치",
          "original_form": "우와모가디슈궁금해요 저는 샹치",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100020042.1.7",
          "form": "것도 잼나겠다요",
          "original_form": "것도 잼나겠다요",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100020042.1.8",
          "form": "또 그 윤아 박정민 나오는 기차역? 이있던데",
          "original_form": "또 그 윤아 박정민 나오는 기차역? 이있던데",
