      "utterance": [
        {
          "id": "MDRW2100002380.1.1",
          "form": "티비 자주 보세요?",
          "original_form": "티비 자주 보세요?",
          "speaker_id": "1",
          "time": "20210820 10:54"
        },
        {
          "id": "MDRW2100002380.1.2",
          "form": "잘안봐요",
          "original_form": "잘안봐요",
          "speaker_id": "2",
          "time": "20210820 10:55"
        },
        {
          "id": "MDRW2100002380.1.3",
          "form": "유트브를 자주봅니다",
          "original_form": "유트브를 자주봅니다",
          "speaker_id": "2",
          "time": "20210820 10:55"
        },
        {
          "id": "MDRW2100002380.1.4",
          "form": "아 저는 넷플릭스를 자주 봐요",
          "original_form": "아 저는 넷플릭스를 자주 봐요",
          "speaker_id": "1",
          "time": "20210820 10:56"
        },
        {
          "id": "MDRW2100002380.1.5",
          "form": "넷플릭스에서 뭐가 재밌나요",
          "original_form": "넷플릭스에서 뭐가 재밌나요",
          "speaker_id": "2",
          "time": "20210820 10:57"
        },
        {
          "id": "MDRW2100002380.1.6",
          "form": "최근에는 낙원의 밤 봤는데 흥미롭더라구요",
          "original_form": "최근에는 낙원의 밤 봤는데 흥미롭더라구요",
          "speaker_id": "1",
          "time": "20210820 10:57"
        },
        {
          "id": "MDRW2100002380.1.7",
          "form": "전여빈 배우 나와요 아세요?",
          "original_form": "전여빈 배우 나와요 아세요?",
          "speaker_id": "1",
          "time": "20210820 10:57"
        },
        {
          "id": "MDRW2100002380.1.8",
          "form": "네 전여빈배우 알죠!  멜로가체질 빈센조^ㄲ",
          "original_form": "네 전여빈배우 알죠!  멜로가체질 빈센조^ㄲ",
          "speaker_id": "2",
          "time": "20210820 10:58"
        },
        {
          "id": "MDRW2100002380.1.9",
          "form": "네! 낙원에 밤에서는 또 새롭게 나와요",
          "original_form": "네! 낙원에 밤에서는 또 새롭게 나와요",
          "speaker_id": "1",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002380.1.10",
          "form": "범죄영화군요!",
          "original_form": "범죄영화군요!",
          "speaker_id": "2",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002380.1.11",
          "form": "액션까진 아닌데 보는 재미가 있어서!",
          "original_form": "액션까진 아닌데 보는 재미가 있어서!",
          "speaker_id": "1",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002380.1.12",
          "form": "저는 추천드려요!ㅎ.ㅎ",
          "original_form": "저는 추천드려요!ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002380.1.13",
          "form": "네 다음에 기회가 된다면 꼭 볼게요 추천감사해요^^",
          "original_form": "네 다음에 기회가 된다면 꼭 볼게요 추천감사해요^^",
          "speaker_id": "2",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002380.1.14",
          "form": "드라마는 안보시나요?ㅎㅎ",
          "original_form": "드라마는 안보시나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 10:59"
        },
        {
          "id": "MDRW2100002380.1.15",
          "form": "종종 봐요! 어제는 슬기로운 의사생활을 봤네요~",
          "original_form": "종종 봐요! 어제는 슬기로운 의사생활을 봤네요~",
          "speaker_id": "1",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002380.1.16",
          "form": "드라마 보시나요?",
          "original_form": "드라마 보시나요?",
          "speaker_id": "1",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002380.1.17",
          "form": "오 슬의생 재밌다면서요!",
          "original_form": "오 슬의생 재밌다면서요!",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002380.1.18",
          "form": "전요즘  너는,나의봄  보고있어요^^",
          "original_form": "전요즘  너는,나의봄  보고있어요^^",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002380.1.19",
          "form": "김동욱 서현진 주연의 드라마에요~ㅎㅎ",
          "original_form": "김동욱 서현진 주연의 드라마에요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002380.1.20",
          "form": "아! 그거 짤?로만 본적있어요",
          "original_form": "아! 그거 짤?로만 본적있어요",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002380.1.21",
          "form": "서현진 연기 잘한다고 재밌다고 하더라구요",
          "original_form": "서현진 연기 잘한다고 재밌다고 하더라구요",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002380.1.22",
          "form": "제가 좋아하는 배우들이라  너무 재밌게 보고있어요^^",
          "original_form": "제가 좋아하는 배우들이라  너무 재밌게 보고있어요^^",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002380.1.23",
          "form": "맞아요! 연기력도 최고ㅎㅎ",
          "original_form": "맞아요! 연기력도 최고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002380.1.24",
          "form": "ㅎ.ㅎ 한번 봐야겠네요 저는 몰아보는거 좋아해서",
          "original_form": "ㅎ.ㅎ 한번 봐야겠네요 저는 몰아보는거 좋아해서",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002380.1.25",
          "form": "저도 몰아보는거 좋아해서  이제 시작한지얼마 안됐어욯ㅋㅋ",
          "original_form": "저도 몰아보는거 좋아해서  이제 시작한지얼마 안됐어욯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002380.1.26",
          "form": "드라마 보면 쭉 보는게 몰입도가 안깨지고 좋더라구요!",
          "original_form": "드라마 보면 쭉 보는게 몰입도가 안깨지고 좋더라구요!",
          "speaker_id": "1",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002380.1.27",
          "form": "16부작이고 14회까지 방송했답니다 ^^",
          "original_form": "16부작이고 14회까지 방송했답니다 ^^",
