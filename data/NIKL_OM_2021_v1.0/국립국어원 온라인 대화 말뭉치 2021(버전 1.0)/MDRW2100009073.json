      "utterance": [
        {
          "id": "MDRW2100009073.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009073.1.2",
          "form": "떡볶이요",
          "original_form": "떡볶이요",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009073.1.3",
          "form": "전 요즘 로제떡볶이가 너무 맛있어요",
          "original_form": "전 요즘 로제떡볶이가 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009073.1.4",
          "form": "한번도 안먹어봤네요 ㅜ",
          "original_form": "한번도 안먹어봤네요 ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009073.1.5",
          "form": "전 원래 떡볶이 별로 안 좋아하는데 로제는 좋아요",
          "original_form": "전 원래 떡볶이 별로 안 좋아하는데 로제는 좋아요",
          "speaker_id": "1",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100009073.1.6",
          "form": "역시 유명한건 이유가 있네요",
          "original_form": "역시 유명한건 이유가 있네요",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009073.1.7",
          "form": "어디 떡볶이 좋아하세요?",
          "original_form": "어디 떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009073.1.8",
          "form": "저는 아무거나 다좋아해요 동네거 쉽게 접하는걸로 먹어요",
          "original_form": "저는 아무거나 다좋아해요 동네거 쉽게 접하는걸로 먹어요",
          "speaker_id": "2",
          "time": "20210823 07:27"
        },
        {
          "id": "MDRW2100009073.1.9",
          "form": "전 아직도 초등학교 앞 떡볶이가 가장 맛있어요",
          "original_form": "전 아직도 초등학교 앞 떡볶이가 가장 맛있어요",
          "speaker_id": "1",
          "time": "20210823 10:41"
        },
        {
          "id": "MDRW2100009073.1.10",
          "form": "맛아요 그맛이 안나네요 ㅜ",
          "original_form": "맛아요 그맛이 안나네요 ㅜ",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100009073.1.11",
          "form": "며칠전에 정씨함박 갔는데 거기 떡볶이가 딱 그 맛이었어요!",
          "original_form": "며칠전에 정씨함박 갔는데 거기 떡볶이가 딱 그 맛이었어요!",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100009073.1.12",
          "form": "컵떡볶이가 최고였는데 ㅎ",
          "original_form": "컵떡볶이가 최고였는데 ㅎ",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100009073.1.13",
          "form": "한입만먹어도",
          "original_form": "한입만먹어도",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100009073.1.14",
          "form": "맞아요ㅋㅋ컵슬러쉬도 같이 먹어줘야해요",
          "original_form": "맞아요ㅋㅋ컵슬러쉬도 같이 먹어줘야해요",
          "speaker_id": "1",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100009073.1.15",
          "form": "꺄 ㅋ전 섞어먹는걸 좋아했어요 포도 오렌지",
          "original_form": "꺄 ㅋ전 섞어먹는걸 좋아했어요 포도 오렌지",
          "speaker_id": "2",
          "time": "20210823 16:59"
        },
        {
          "id": "MDRW2100009073.1.16",
          "form": "맞아요ㅋㅋㅋ섞어먹는 건 다 똑같군요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ섞어먹는 건 다 똑같군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:09"
        }
