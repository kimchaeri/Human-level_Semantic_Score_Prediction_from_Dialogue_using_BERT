      "utterance": [
        {
          "id": "MDRW2100014466.1.1",
          "form": "애블럭사러간적있는데요",
          "original_form": "애블럭사러간적있는데요",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014466.1.2",
          "form": "아들가방 쿠폰하고 출석해서 싸게 샀어요",
          "original_form": "아들가방 쿠폰하고 출석해서 싸게 샀어요",
          "speaker_id": "2",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014466.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014466.1.4",
          "form": "뽀로로 블럭 사러갔는데",
          "original_form": "뽀로로 블럭 사러갔는데",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.5",
          "form": "블럭 저 일일이 닦아서 팔다 멘붕이 ㅋㅋ",
          "original_form": "블럭 저 일일이 닦아서 팔다 멘붕이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.7",
          "form": "그분이 주소를 잘못알려주셔서",
          "original_form": "그분이 주소를 잘못알려주셔서",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.8",
          "form": "앙 아가는 뽀로로죠",
          "original_form": "앙 아가는 뽀로로죠",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.10",
          "form": "이사가기 전주소로 갔었어요 ㅎ",
          "original_form": "이사가기 전주소로 갔었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.11",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.12",
          "form": "그건 아니죠",
          "original_form": "그건 아니죠",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.13",
          "form": "그럼 그분이 오셔야죠",
          "original_form": "그럼 그분이 오셔야죠",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014466.1.14",
          "form": "다행이 한블럭 위에? 5분정도",
          "original_form": "다행이 한블럭 위에? 5분정도",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.15",
          "form": "오다행이더",
          "original_form": "오다행이더",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.16",
          "form": "거리라 갔더니",
          "original_form": "거리라 갔더니",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.17",
          "form": "운좋았어요 ㅎㅎ",
          "original_form": "운좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.19",
          "form": "그분이 오시지",
          "original_form": "그분이 오시지",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.20",
          "form": "좀 매너가 없네요",
          "original_form": "좀 매너가 없네요",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.21",
          "form": "죄송하다고 사려고 했던거말고 토마스블럭한통\r\n몰펀까지",
          "original_form": "죄송하다고 사려고 했던거말고 토마스블럭한통\r\n몰펀까지",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.22",
          "form": "다 주시더라구요 ㅋ",
          "original_form": "다 주시더라구요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.23",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014466.1.24",
          "form": "경우가 바르시네요 ㅋㅋ",
          "original_form": "경우가 바르시네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014466.1.25",
          "form": "그나마 좀 기분이 풀렸었어요 ㅎ",
          "original_form": "그나마 좀 기분이 풀렸었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014466.1.26",
          "form": "이래서 끝까지 듣고봐야 하네요 ㅋㅋ",
          "original_form": "이래서 끝까지 듣고봐야 하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014466.1.27",
          "form": "완전 대박요",
          "original_form": "완전 대박요",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014466.1.28",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014466.1.29",
          "form": "다행이예요 중고거래 진짜 어려워요",
          "original_form": "다행이예요 중고거래 진짜 어려워요",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014466.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014466.1.31",
