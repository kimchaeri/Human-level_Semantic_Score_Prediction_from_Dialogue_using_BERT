      "utterance": [
        {
          "id": "MDRW2100022328.1.1",
          "form": "피자헛이 진짜 가까이에 있거든요",
          "original_form": "피자헛이 진짜 가까이에 있거든요",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.2",
          "form": "피자헛 맛있죠",
          "original_form": "피자헛 맛있죠",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.3",
          "form": "바로 옆건물이라 1분거리에 있어요 ㅋㅋㅋ",
          "original_form": "바로 옆건물이라 1분거리에 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.4",
          "form": "거기 샐러드바 자주먹었는데",
          "original_form": "거기 샐러드바 자주먹었는데",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.5",
          "form": "어머 샐러드바도 있었나?",
          "original_form": "어머 샐러드바도 있었나?",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.6",
          "form": "없으셨어요?",
          "original_form": "없으셨어요?",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.7",
          "form": "아 여기 지점은 포장/배달전문점이라서 ㅜㅜ",
          "original_form": "아 여기 지점은 포장/배달전문점이라서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.8",
          "form": "아.. 안타깝",
          "original_form": "아.. 안타깝",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.9",
          "form": "샐러드바는 미피가 최고였던 것 같은데",
          "original_form": "샐러드바는 미피가 최고였던 것 같은데",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.10",
          "form": "혹시 피자대축제 이런거하면 먹으셨나요",
          "original_form": "혹시 피자대축제 이런거하면 먹으셨나요",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.11",
          "form": "미피 한번 난리나고 거의 불매운동급이던데요 ㅠㅠ",
          "original_form": "미피 한번 난리나고 거의 불매운동급이던데요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100022328.1.12",
          "form": "피자대축제 들어본 거 같은데 그게 뭐에요?",
          "original_form": "피자대축제 들어본 거 같은데 그게 뭐에요?",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.13",
          "form": "피자헛에서",
          "original_form": "피자헛에서",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.14",
          "form": "피자대축제라고",
          "original_form": "피자대축제라고",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.15",
          "form": "뭐 할인 해주나?",
          "original_form": "뭐 할인 해주나?",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.16",
          "form": "피자 뷔페식으로",
          "original_form": "피자 뷔페식으로",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.17",
          "form": "아~ 그거 지금도 해요?",
          "original_form": "아~ 그거 지금도 해요?",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.18",
          "form": "일정금액내면 무제한 섭취인데",
          "original_form": "일정금액내면 무제한 섭취인데",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.19",
          "form": "안할걸요",
          "original_form": "안할걸요",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.20",
          "form": "지금도 하면 저 먹고싶어요 ㅠㅠㅠ",
          "original_form": "지금도 하면 저 먹고싶어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.21",
          "form": "그때 얼마였어요? 되게 저렴했을 거 같아요",
          "original_form": "그때 얼마였어요? 되게 저렴했을 거 같아요",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022328.1.22",
          "form": "그거 한 5년전엔가 했던거같은데..",
          "original_form": "그거 한 5년전엔가 했던거같은데..",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022328.1.23",
          "form": "점심9900  그외는 13900인가",
          "original_form": "점심9900  그외는 13900인가",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022328.1.24",
          "form": "그거 비슷하게 미피에서 지금도 하고 있어요",
          "original_form": "그거 비슷하게 미피에서 지금도 하고 있어요",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022328.1.25",
          "form": "어 가격도 딱 그정도 ㅋㅋㅋ",
          "original_form": "어 가격도 딱 그정도 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022328.1.26",
          "form": "지금요?",
          "original_form": "지금요?",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022328.1.27",
          "form": "대박이다",
          "original_form": "대박이다",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022328.1.28",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022328.1.29",
          "form": "신림점에도 있어요",
          "original_form": "신림점에도 있어요",
          "speaker_id": "2",
          "time": "20210829 12:40"
        }
      ]
    }
  ]
