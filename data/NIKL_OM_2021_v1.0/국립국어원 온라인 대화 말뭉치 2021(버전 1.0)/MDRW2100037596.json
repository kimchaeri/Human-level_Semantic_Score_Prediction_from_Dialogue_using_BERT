      "utterance": [
        {
          "id": "MDRW2100037596.1.1",
          "form": "ㅋㅋ 아 진짜요?",
          "original_form": "ㅋㅋ 아 진짜요?",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100037596.1.2",
          "form": "원래도 유명했군요ㅋㅋ",
          "original_form": "원래도 유명했군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100037596.1.3",
          "form": "거기 볶음밥이 있는데",
          "original_form": "거기 볶음밥이 있는데",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100037596.1.4",
          "form": "양볶음밥? 그거라서 톡톡 터저요 먹을때",
          "original_form": "양볶음밥? 그거라서 톡톡 터저요 먹을때",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100037596.1.5",
          "form": "헉.. 날치알",
          "original_form": "헉.. 날치알",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100037596.1.6",
          "form": "이런게 들어가나요ㅋㅋ",
          "original_form": "이런게 들어가나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100037596.1.7",
          "form": "날치알도 따로 있었던거같아요!",
          "original_form": "날치알도 따로 있었던거같아요!",
          "speaker_id": "1",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100037596.1.8",
          "form": "ㅠ얘기하니까 먹고싶네요",
          "original_form": "ㅠ얘기하니까 먹고싶네요",
          "speaker_id": "1",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100037596.1.9",
          "form": "ㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100037596.1.10",
          "form": "저됴 먹고싶어요",
          "original_form": "저됴 먹고싶어요",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100037596.1.11",
          "form": "전 약간 바삭바삭하게 구워진게 좋아요",
          "original_form": "전 약간 바삭바삭하게 구워진게 좋아요",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100037596.1.12",
          "form": "대창..",
          "original_form": "대창..",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100037596.1.13",
          "form": "전 대창 좋은데..",
          "original_form": "전 대창 좋은데..",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037596.1.14",
          "form": "여러번 먹으면 물리더라구요!!",
          "original_form": "여러번 먹으면 물리더라구요!!",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037596.1.15",
          "form": "맞아요ㅋㅋㅋ단독보단",
          "original_form": "맞아요ㅋㅋㅋ단독보단",
          "speaker_id": "1",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100037596.1.16",
          "form": "모듬으로 같이 나오는ㅎㅎ",
          "original_form": "모듬으로 같이 나오는ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100037596.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210902 11:50"
        },
        {
          "id": "MDRW2100037596.1.18",
          "form": "막창두요 ㅠㅠㅠ",
          "original_form": "막창두요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:50"
        },
        {
          "id": "MDRW2100037596.1.19",
          "form": "헐 저도 막창 완전 좋아해요",
          "original_form": "헐 저도 막창 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100037596.1.20",
          "form": "쫄깃..",
