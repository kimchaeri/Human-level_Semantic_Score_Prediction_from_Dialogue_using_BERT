      "utterance": [
        {
          "id": "MDRW2100030324.1.1",
          "form": "혼자 우수에 차는 거 아시죠",
          "original_form": "혼자 우수에 차는 거 아시죠",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030324.1.2",
          "form": "하...... 시마 과거 동료 오해 풀어줄 때도",
          "original_form": "하...... 시마 과거 동료 오해 풀어줄 때도",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030324.1.3",
          "form": "펑펑 울고",
          "original_form": "펑펑 울고",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030324.1.4",
          "form": "아 저 그 이부키 ㅠㅠ ! 스승님 잡혀갈때",
          "original_form": "아 저 그 이부키 ㅠㅠ ! 스승님 잡혀갈때",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030324.1.5",
          "form": "꺽꺽 울었어요",
          "original_form": "꺽꺽 울었어요",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030324.1.6",
          "form": "ㅜㅠㅠㅠㅠ 헉 진짜 저도 그편 완전 울었어요",
          "original_form": "ㅜㅠㅠㅠㅠ 헉 진짜 저도 그편 완전 울었어요",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030324.1.7",
          "form": "가마상. .. . ..",
          "original_form": "가마상. .. . ..",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030324.1.8",
          "form": "사실 초반부분이 좀 ...재미없엇어서....ㅎ....... 대충봤더니 뒤에가서 엥?? 이래가지고 몇번다시봤는데...",
          "original_form": "사실 초반부분이 좀 ...재미없엇어서....ㅎ....... 대충봤더니 뒤에가서 엥?? 이래가지고 몇번다시봤는데...",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030324.1.9",
          "form": "가마상......ㅜㅠㅠㅠㅠㅠ",
          "original_form": "가마상......ㅜㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030324.1.10",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030324.1.11",
          "form": "아 그니까요!! 저도 첨에 에 =.= 노잼 이러고 보다가",
          "original_form": "아 그니까요!! 저도 첨에 에 =.= 노잼 이러고 보다가",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030324.1.12",
          "form": "아내 죽었다는 이야기부터...",
          "original_form": "아내 죽었다는 이야기부터...",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030324.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030324.1.14",
          "form": "걍 베개에 얼굴 박ㄱ고 울고",
          "original_form": "걍 베개에 얼굴 박ㄱ고 울고",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030324.1.15",
          "form": "진짜...",
          "original_form": "진짜...",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030324.1.16",
          "form": "저 그 이부키한테",
          "original_form": "저 그 이부키한테",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030324.1.17",
          "form": "담날 커버 빨았어요",
          "original_form": "담날 커버 빨았어요",
          "speaker_id": "1",
          "time": "20210903 23:48"
