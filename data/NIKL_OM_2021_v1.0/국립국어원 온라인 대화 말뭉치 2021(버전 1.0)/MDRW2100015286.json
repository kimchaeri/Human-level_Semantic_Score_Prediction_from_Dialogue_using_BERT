      "utterance": [
        {
          "id": "MDRW2100015286.1.1",
          "form": "빙수 배달 시켜보셨어요?",
          "original_form": "빙수 배달 시켜보셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015286.1.2",
          "form": "갑자기 설빙 땡기네요~",
          "original_form": "갑자기 설빙 땡기네요~",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015286.1.3",
          "form": "설빙 시.키면 안녹고",
          "original_form": "설빙 시.키면 안녹고",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015286.1.4",
          "form": "잘오나요?!!!!!",
          "original_form": "잘오나요?!!!!!",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015286.1.5",
          "form": "네 안녹고 잘오긴하는데..",
          "original_form": "네 안녹고 잘오긴하는데..",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015286.1.6",
          "form": "용기에 담겨와서 그런지 매장보다 양은 적은느낌이에요 ㅋㅋㅋ",
          "original_form": "용기에 담겨와서 그런지 매장보다 양은 적은느낌이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015286.1.7",
          "form": "그렇군요ㅋㅋㅋ",
          "original_form": "그렇군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015286.1.8",
          "form": "설빙 뭐좋아하세요?",
          "original_form": "설빙 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015286.1.9",
          "form": "요거통통메론이요!ㅋㅋ",
          "original_form": "요거통통메론이요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015286.1.10",
          "form": "가면 그거만 먹어요..",
          "original_form": "가면 그거만 먹어요..",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015286.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015286.1.12",
          "form": "저랑같으시네요?!",
          "original_form": "저랑같으시네요?!",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015286.1.13",
          "form": "여름에만 나와서ㅠㅠㅋㅋㅋㅋㅋ여름에만 가요 설빙",
          "original_form": "여름에만 나와서ㅠㅠㅋㅋㅋㅋㅋ여름에만 가요 설빙",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015286.1.14",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100015286.1.15",
          "form": "그거여름에만 나오나요?ㅋㅋㅋ",
          "original_form": "그거여름에만 나오나요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100015286.1.16",
          "form": "메론이 계절과일이라 그런가 여름에만 하더라구요ㅋㅋㅋ",
          "original_form": "메론이 계절과일이라 그런가 여름에만 하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100015286.1.17",
          "form": "인절미빙수는",
          "original_form": "인절미빙수는",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100015286.1.18",
          "form": "별로인가요?",
          "original_form": "별로인가요?",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100015286.1.19",
          "form": "인절미도 꼬숩고 맛있죠ㅋㅋㅋ 근데 메론이 최강자.....",
          "original_form": "인절미도 꼬숩고 맛있죠ㅋㅋㅋ 근데 메론이 최강자.....",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100015286.1.20",
          "form": "인절미도 좋아하세요?",
          "original_form": "인절미도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100015286.1.21",
          "form": "ㅋㅋㅋ 사실인절미는",
          "original_form": "ㅋㅋㅋ 사실인절미는",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100015286.1.22",
          "form": "그닥좋아하지않는답니다",
          "original_form": "그닥좋아하지않는답니다",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100015286.1.23",
          "form": "초딩입맛이라서요",
          "original_form": "초딩입맛이라서요",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100015286.1.24",
          "form": "제친구들은 치즈큐브 들어간 치즈빙수 좋아하던데ㅋㅋ",
