      "utterance": [
        {
          "id": "MDRW2100027880.1.1",
          "form": "난 내일 공강임.",
          "original_form": "난 내일 공강임.",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027880.1.2",
          "form": "부럽지",
          "original_form": "부럽지",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027880.1.3",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027880.1.4",
          "form": "나도 자체공강 해야지.",
          "original_form": "나도 자체공강 해야지.",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027880.1.5",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027880.1.6",
          "form": "님 수업 있는 것 같아서 오전에 놀러간다고 한 건데",
          "original_form": "님 수업 있는 것 같아서 오전에 놀러간다고 한 건데",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027880.1.7",
          "form": "아니 내일 오후 수업 있는데",
          "original_form": "아니 내일 오후 수업 있는데",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027880.1.8",
          "form": "녹강인지 실강인지",
          "original_form": "녹강인지 실강인지",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027880.1.9",
          "form": "말을 안 해주심",
          "original_form": "말을 안 해주심",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027880.1.10",
          "form": "그래서 자체공강을 하시겠다?",
          "original_form": "그래서 자체공강을 하시겠다?",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027880.1.11",
          "form": "어차피 휴학할 건데.",
          "original_form": "어차피 휴학할 건데.",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027880.1.12",
          "form": "웃기네",
          "original_form": "웃기네",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027880.1.13",
          "form": "하... 내일 공강하는 나 vs 휴학하는 님",
          "original_form": "하... 내일 공강하는 나 vs 휴학하는 님",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.14",
          "form": "그치만 오늘 하루종일 수강신청 페이지",
          "original_form": "그치만 오늘 하루종일 수강신청 페이지",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.15",
          "form": "님 압승",
          "original_form": "님 압승",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.16",
          "form": "새로고침 하고 있는데",
          "original_form": "새로고침 하고 있는데",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.17",
          "form": "자리가 한자리도 안 나",
          "original_form": "자리가 한자리도 안 나",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.18",
          "form": "그럴수가...",
          "original_form": "그럴수가...",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.19",
          "form": "얌전히 휴학하겠습니다",
          "original_form": "얌전히 휴학하겠습니다",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.20",
          "form": "에타에서 교환 같은 거 못 구하나",
          "original_form": "에타에서 교환 같은 거 못 구하나",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.21",
          "form": "양도나...",
          "original_form": "양도나...",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027880.1.22",
          "form": "그런 거... 없어",
          "original_form": "그런 거... 없어",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027880.1.23",
          "form": "헐... 진짜?",
          "original_form": "헐... 진짜?",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027880.1.24",
          "form": "우리 학교 방식으론 너무 어렵기도 하고...",
          "original_form": "우리 학교 방식으론 너무 어렵기도 하고...",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027880.1.25",
          "form": "옛날엔 가능했는데",
          "original_form": "옛날엔 가능했는데",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027880.1.26",
          "form": "방식 바뀌고 거의 없어진듯",
          "original_form": "방식 바뀌고 거의 없어진듯",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027880.1.27",
          "form": "충격...",
          "original_form": "충격...",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027880.1.28",
          "form": "그 방식도 거지같던데",
          "original_form": "그 방식도 거지같던데",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027880.1.29",
          "form": "진짜 싫어...",
          "original_form": "진짜 싫어...",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.30",
          "form": "수업을 들을 수가 없구만",
          "original_form": "수업을 들을 수가 없구만",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.31",
          "form": "제발 다시 돌려놔...",
          "original_form": "제발 다시 돌려놔...",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.32",
          "form": "졸업하게 해줘...",
          "original_form": "졸업하게 해줘...",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.33",
          "form": "대체 누가 만든 방식이란말임",
          "original_form": "대체 누가 만든 방식이란말임",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.34",
          "form": "발상 압수",
          "original_form": "발상 압수",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.35",
          "form": "ㄹㅇ 내말이",
          "original_form": "ㄹㅇ 내말이",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.36",
          "form": "수강신청 방식 바꾸지 말고",
          "original_form": "수강신청 방식 바꾸지 말고",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.37",
          "form": "분반이나 늘려주지",
          "original_form": "분반이나 늘려주지",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027880.1.38",
          "form": "거지갓아.",
          "original_form": "거지갓아.",
          "speaker_id": "2",
          "time": "20210902 15:57"
        }
      ]
