          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046016.1.9",
          "form": "은행이싫어",
          "original_form": "은행이싫어",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046016.1.10",
          "form": "설마 냄새 때문에",
