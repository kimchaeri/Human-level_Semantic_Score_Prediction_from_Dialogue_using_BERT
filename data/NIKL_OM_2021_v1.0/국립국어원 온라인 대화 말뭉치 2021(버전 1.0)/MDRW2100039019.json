      "utterance": [
        {
          "id": "MDRW2100039019.1.1",
          "form": "요즘 시원하지않나요",
          "original_form": "요즘 시원하지않나요",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100039019.1.2",
          "form": "네 지금 창문 좀 열어놨는데 선선해서 좋네요ㅋㅋ",
          "original_form": "네 지금 창문 좀 열어놨는데 선선해서 좋네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100039019.1.3",
          "form": "ㅋㅋ 비가 담주면 좀 덜오겟죠?",
          "original_form": "ㅋㅋ 비가 담주면 좀 덜오겟죠?",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100039019.1.4",
          "form": "덜왓으면 좋겠어요ㅠ 우산 챙기기 무겁네요ㅠ",
          "original_form": "덜왓으면 좋겠어요ㅠ 우산 챙기기 무겁네요ㅠ",
          "speaker_id": "2",
          "time": "20210903 01:43"
        },
        {
          "id": "MDRW2100039019.1.5",
          "form": "그죠 귀찮아요ㅠㅠ",
          "original_form": "그죠 귀찮아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 01:45"
        },
        {
          "id": "MDRW2100039019.1.6",
          "form": "언제올지도몰겟고",
          "original_form": "언제올지도몰겟고",
          "speaker_id": "1",
          "time": "20210903 01:46"
        },
        {
          "id": "MDRW2100039019.1.7",
          "form": "작년에도 이시기때 비가 자꾸 왓던가요?",
          "original_form": "작년에도 이시기때 비가 자꾸 왓던가요?",
          "speaker_id": "2",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100039019.1.8",
          "form": "기억이안나네요..아닌거같은뎅...",
          "original_form": "기억이안나네요..아닌거같은뎅...",
          "speaker_id": "1",
          "time": "20210903 02:05"
        },
        {
          "id": "MDRW2100039019.1.9",
          "form": "저도 아닌거같은데 이번해엔 천둥도 막 치고 이상하긴 한거같아요",
          "original_form": "저도 아닌거같은데 이번해엔 천둥도 막 치고 이상하긴 한거같아요",
          "speaker_id": "2",
          "time": "20210903 02:07"
        },
        {
          "id": "MDRW2100039019.1.10",
          "form": "긍까요... 여름엔 비가 덜온것같고...",
          "original_form": "긍까요... 여름엔 비가 덜온것같고...",
          "speaker_id": "1",
          "time": "20210903 02:14"
        },
        {
          "id": "MDRW2100039019.1.11",
          "form": "네 이번엔 태풍이 별로 안온거 같네요",
          "original_form": "네 이번엔 태풍이 별로 안온거 같네요",
          "speaker_id": "2",
          "time": "20210903 02:21"
        },
        {
          "id": "MDRW2100039019.1.12",
          "form": "대신 비가 늦게오는건강",
          "original_form": "대신 비가 늦게오는건강",
          "speaker_id": "1",
          "time": "20210903 02:23"
        },
        {
          "id": "MDRW2100039019.1.13",
          "form": "그래도 지금 시원해서조아여 습도만 좀 덜하면",
          "original_form": "그래도 지금 시원해서조아여 습도만 좀 덜하면",
          "speaker_id": "1",
          "time": "20210903 02:23"
        },
        {
          "id": "MDRW2100039019.1.14",
          "form": "그쵸 저도 아침에 습도 너무 높아서 깼어요ㅠ",
          "original_form": "그쵸 저도 아침에 습도 너무 높아서 깼어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 02:27"
        },
        {
          "id": "MDRW2100039019.1.15",
          "form": "이정도 기온에 습도높은 기분 자주 못느껴서 이상해요",
          "original_form": "이정도 기온에 습도높은 기분 자주 못느껴서 이상해요",
          "speaker_id": "1",
          "time": "20210903 02:29"
        }
      ]
