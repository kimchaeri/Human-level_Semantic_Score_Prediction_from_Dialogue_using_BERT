      "utterance": [
        {
          "id": "MDRW2100030918.1.1",
          "form": "점심드셧어요?",
          "original_form": "점심드셧어요?",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030918.1.2",
          "form": "아 답장이 늦었네요 집안일 하느라 ㅠ",
          "original_form": "아 답장이 늦었네요 집안일 하느라 ㅠ",
          "speaker_id": "2",
          "time": "20210905 06:47"
        },
        {
          "id": "MDRW2100030918.1.3",
          "form": "아기 밥줘야될것같아요",
          "original_form": "아기 밥줘야될것같아요",
          "speaker_id": "2",
          "time": "20210905 06:48"
        },
        {
          "id": "MDRW2100030918.1.4",
          "form": "ㅋㅋㅋ 아기 밥 뭐먹나요",
          "original_form": "ㅋㅋㅋ 아기 밥 뭐먹나요",
          "speaker_id": "1",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100030918.1.5",
          "form": "오늘 처가댁 다녀와서 좀 바빳네요 ㅠ",
          "original_form": "오늘 처가댁 다녀와서 좀 바빳네요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100030918.1.6",
          "form": "아기 간 하지 않은 밥먹어요 ㅎㅎ",
          "original_form": "아기 간 하지 않은 밥먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100030918.1.7",
          "form": "마니 먹어유",
          "original_form": "마니 먹어유",
          "speaker_id": "2",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100030918.1.8",
          "form": "지금 감자밥 먹이고 있습니다",
          "original_form": "지금 감자밥 먹이고 있습니다",
          "speaker_id": "2",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100030918.1.9",
          "form": "감자밥은 먼가요 감자 으갠서요? ㅋㅋ",
          "original_form": "감자밥은 먼가요 감자 으갠서요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100030918.1.10",
          "form": "밥 할 때 같이 넣었습니다",
          "original_form": "밥 할 때 같이 넣었습니다",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100030918.1.11",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100030918.1.12",
          "form": "감자가 좋아서 그런지",
          "original_form": "감자가 좋아서 그런지",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100030918.1.13",
          "form": "간 안해도 맛나요",
          "original_form": "간 안해도 맛나요",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100030918.1.14",
          "form": "추천!",
          "original_form": "추천!",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100030918.1.15",
          "form": "오신기하다...강원도감잔가...",
          "original_form": "오신기하다...강원도감잔가...",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100030918.1.16",
          "form": "그런가봅니다 ㅋㅋㅋㅋ",
          "original_form": "그런가봅니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100030918.1.17",
          "form": "야식 드실건가요?",
          "original_form": "야식 드실건가요?",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100030918.1.18",
          "form": "아뇨 ㅋㅋ 참아야죠 .. 그냥 포도몇개 주워먹었슴다 야식 드시나요",
          "original_form": "아뇨 ㅋㅋ 참아야죠 .. 그냥 포도몇개 주워먹었슴다 야식 드시나요",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100030918.1.19",
          "form": "자두 한알 먹구요",
          "original_form": "자두 한알 먹구요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100030918.1.20",
          "form": "요거트하나 ?",
          "original_form": "요거트하나 ?",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100030918.1.21",
          "form": "요래 해써요 ㅋㅋ",
          "original_form": "요래 해써요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100030918.1.22",
          "form": "자두 어제먹었는데",
          "original_form": "자두 어제먹었는데",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100030918.1.23",
          "form": "왕자두",
          "original_form": "왕자두",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100030918.1.24",
          "form": "왕자두 존맛탱",
          "original_form": "왕자두 존맛탱",
          "speaker_id": "2",
          "time": "20210906 10:23"
        },
        {
          "id": "MDRW2100030918.1.25",
          "form": "상큼",
          "original_form": "상큼",
          "speaker_id": "2",
          "time": "20210906 10:23"
        },
        {
          "id": "MDRW2100030918.1.26",
          "form": "새콤",
          "original_form": "새콤",
          "speaker_id": "2",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100030918.1.27",
          "form": "혀 아래에 침고여유",
          "original_form": "혀 아래에 침고여유",
          "speaker_id": "2",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100030918.1.28",
          "form": "달콤한 자두 또 사야징",
          "original_form": "달콤한 자두 또 사야징",
          "speaker_id": "2",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100030918.1.29",
          "form": "자두먹고싶다 ㅎ.ㅎ",
          "original_form": "자두먹고싶다 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100030918.1.30",
          "form": "점심메뉴머엿나요",
          "original_form": "점심메뉴머엿나요",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100030918.1.31",
          "form": "아 비가 약간와서",
          "original_form": "아 비가 약간와서",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100030918.1.32",
          "form": "고소한 해물파전",
          "original_form": "고소한 해물파전",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100030918.1.33",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100030918.1.34",
          "form": "김치 칼제비를 먹었습니다",
          "original_form": "김치 칼제비를 먹었습니다",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100030918.1.35",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100030918.1.36",
          "form": "칼제비  죽이네요",
          "original_form": "칼제비  죽이네요",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100030918.1.37",
          "form": "조아하는데 파는데가 별루 없다능",
          "original_form": "조아하는데 파는데가 별루 없다능",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100030918.1.38",
          "form": "아 동네가 어딘가요?",
          "original_form": "아 동네가 어딘가요?",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100030918.1.39",
          "form": "대전에는 칼국수가 유명하다고 하던데 나중에 기회되면 한번 꼭가보세유",
          "original_form": "대전에는 칼국수가 유명하다고 하던데 나중에 기회되면 한번 꼭가보세유",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100030918.1.40",
          "form": "대전인데! 근데 수제비는 별루 없어욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "대전인데! 근데 수제비는 별루 없어욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100030918.1.41",
          "form": "아 대전?",
          "original_form": "아 대전?",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100030918.1.42",
          "form": "아 대전사시는군요 ㅋㅋㅋㄱㅋㅋ",
          "original_form": "아 대전사시는군요 ㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100030918.1.43",
          "form": "수제비가 엄따니 띠로리",
          "original_form": "수제비가 엄따니 띠로리",
          "speaker_id": "2",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100030918.1.44",
          "form": "거의 안보여요 ㅋㅋㅋ 수제비 더 조아하는데",
          "original_form": "거의 안보여요 ㅋㅋㅋ 수제비 더 조아하는데",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100030918.1.45",
          "form": "아\r\n글쿤요",
          "original_form": "아\r\n글쿤요",
          "speaker_id": "2",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100030918.1.46",
          "form": "대전 축구하러 갓섯는데 ㅋㅋ",
          "original_form": "대전 축구하러 갓섯는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100030918.1.47",
          "form": "4개 과기원 축구대회있어서",
          "original_form": "4개 과기원 축구대회있어서",
          "speaker_id": "2",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100030918.1.48",
          "form": "카이스트 가서 한겜 찬 기억이있네요 ㅋㅋ",
          "original_form": "카이스트 가서 한겜 찬 기억이있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100030918.1.49",
          "form": "대전은 이마트가서 김박사 그럼 최소 3명이 돌아본다고 하는곳 이죠?",
          "original_form": "대전은 이마트가서 김박사 그럼 최소 3명이 돌아본다고 하는곳 이죠?",
          "speaker_id": "2",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100030918.1.50",
          "form": "앜ㅋㅋㅋㅋㅋ그러수도잇겟네요  축구잘하시나바요",
          "original_form": "앜ㅋㅋㅋㅋㅋ그러수도잇겟네요  축구잘하시나바요",
          "speaker_id": "1",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100030918.1.51",
          "form": "아녀 사기 진작용으로 뽑혓습니다",
          "original_form": "아녀 사기 진작용으로 뽑혓습니다",
          "speaker_id": "2",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100030918.1.52",
          "form": "제가 화이팅을 잘하거든요 ㅋㅋ",
          "original_form": "제가 화이팅을 잘하거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100030918.1.53",
          "form": "ㅋㅋㅋ제덕롤인가요",
          "original_form": "ㅋㅋㅋ제덕롤인가요",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100030918.1.54",
          "form": "제덕롤이면 대박!",
          "original_form": "제덕롤이면 대박!",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100030918.1.55",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:38"
        }
      ]
    }
