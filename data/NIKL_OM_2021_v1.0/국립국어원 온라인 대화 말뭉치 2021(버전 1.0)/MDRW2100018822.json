      "utterance": [
        {
          "id": "MDRW2100018822.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018822.1.2",
          "form": "안녕하세요 ㅎㅎ 식사 하셨나요?",
          "original_form": "안녕하세요 ㅎㅎ 식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018822.1.3",
          "form": "네 오늘 육개장 먹었어요~ㅎㅎ",
          "original_form": "네 오늘 육개장 먹었어요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018822.1.4",
          "form": "맛있겠네요 ㅎㅎ 국 좋아하시나요?",
          "original_form": "맛있겠네요 ㅎㅎ 국 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018822.1.5",
          "form": "네! 국밥 너무좋아해요~",
          "original_form": "네! 국밥 너무좋아해요~",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018822.1.6",
          "form": "무슨 국을 제일 좋아하세요?",
          "original_form": "무슨 국을 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018822.1.7",
          "form": "저는 미역국밥 돼지국밥 두가지 좋아해요~ 한개못픽하겠어요 ㅗ",
          "original_form": "저는 미역국밥 돼지국밥 두가지 좋아해요~ 한개못픽하겠어요 ㅗ",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018822.1.8",
          "form": "어머 오타죄송해요 ㅠㅠ",
          "original_form": "어머 오타죄송해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018822.1.9",
          "form": "괜찮아요ㅎㅎ 앗 그럼 미역국엔 뭐들어간거 좋아하시나요? 소고기? 해물?",
          "original_form": "괜찮아요ㅎㅎ 앗 그럼 미역국엔 뭐들어간거 좋아하시나요? 소고기? 해물?",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018822.1.10",
          "form": "저는 무조건 소고기요~",
          "original_form": "저는 무조건 소고기요~",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018822.1.11",
          "form": "해물그닥안좋아해서요 ㅋㅋ",
          "original_form": "해물그닥안좋아해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018822.1.12",
          "form": "전 요상하게 국엔 해물 들어간게 좋더라구요ㅋㅋㅋ 평소엔 잘 안먹는데 국에 들어가면 좋아해요ㅋㅋㅋ 돼지국밥도 그렇고 국에 고기들어간거 좋아하시나요?",
          "original_form": "전 요상하게 국엔 해물 들어간게 좋더라구요ㅋㅋㅋ 평소엔 잘 안먹는데 국에 들어가면 좋아해요ㅋㅋㅋ 돼지국밥도 그렇고 국에 고기들어간거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018822.1.13",
          "form": "해물들어가면 국물깊이가 달라지긴 하죠~",
          "original_form": "해물들어가면 국물깊이가 달라지긴 하죠~",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018822.1.14",
          "form": "맞아요 고기베이스 육수 참 좋아합니당",
          "original_form": "맞아요 고기베이스 육수 참 좋아합니당",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018822.1.15",
          "form": "고기 있고 없고도 차이 많이 나죠 ㅎㅎ 전에 쌀국수 먹는데 걍 가루로 된 스프 넣다가 육수 넣어봤는데 맛이 확 다르더라구요 ㅋㅋㅋㅋ 고기 필수!",
          "original_form": "고기 있고 없고도 차이 많이 나죠 ㅎㅎ 전에 쌀국수 먹는데 걍 가루로 된 스프 넣다가 육수 넣어봤는데 맛이 확 다르더라구요 ㅋㅋㅋㅋ 고기 필수!",
          "speaker_id": "2",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018822.1.16",
          "form": "맞아요~ 쌀국수는 무조건 고기!!",
          "original_form": "맞아요~ 쌀국수는 무조건 고기!!",
          "speaker_id": "1",
          "time": "20210827 12:57"
        }
      ]
