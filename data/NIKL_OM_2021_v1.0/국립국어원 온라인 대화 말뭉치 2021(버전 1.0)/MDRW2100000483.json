      "utterance": [
        {
          "id": "MDRW2100000483.1.1",
          "form": "싸이월드 해보신 적 있으세요?",
          "original_form": "싸이월드 해보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000483.1.2",
          "form": "싸이월드!!! 예전에했죠",
          "original_form": "싸이월드!!! 예전에했죠",
          "speaker_id": "2",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000483.1.3",
          "form": "ㅋㅋ요번에 부활했다는데 아이디랑 비밀번호 찾으셨어요?ㅋㅋㅋ",
          "original_form": "ㅋㅋ요번에 부활했다는데 아이디랑 비밀번호 찾으셨어요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000483.1.4",
          "form": "아니요ㅠㅠ흑역사라 걍 묻어두고싶오요",
          "original_form": "아니요ㅠㅠ흑역사라 걍 묻어두고싶오요",
          "speaker_id": "2",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000483.1.5",
          "form": "저도 흑역사가 걱정되긴 하는데 그래도 다시 해보고 싶어서 찾는 거 신청해뒀었어요ㅋㅋㅋ",
          "original_form": "저도 흑역사가 걱정되긴 하는데 그래도 다시 해보고 싶어서 찾는 거 신청해뒀었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000483.1.6",
          "form": "아 진ㅋ자 싸이월드 언제 없어지는거람..",
          "original_form": "아 진ㅋ자 싸이월드 언제 없어지는거람..",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000483.1.7",
          "form": "안없어질것같은데ㅋㅋㅋㅋ",
          "original_form": "안없어질것같은데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000483.1.8",
          "form": "걍 아이디 찾아서 다 삭제하는 게 더 마음 편할 듯ㅋㅋ",
          "original_form": "걍 아이디 찾아서 다 삭제하는 게 더 마음 편할 듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000483.1.9",
          "form": "개인정보 보관기간 이미 지난거 아닌지?",
          "original_form": "개인정보 보관기간 이미 지난거 아닌지?",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000483.1.10",
          "form": "아 그런가? 그럼 걍 새로 시작하면 되겠네ㅋㅋ",
          "original_form": "아 그런가? 그럼 걍 새로 시작하면 되겠네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000483.1.11",
          "form": "싸이월드 너무",
          "original_form": "싸이월드 너무",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000483.1.12",
          "form": "맨날 부활하고 폐지하고",
          "original_form": "맨날 부활하고 폐지하고",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000483.1.13",
          "form": "반복되어서 짜증남",
          "original_form": "반복되어서 짜증남",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000483.1.14",
          "form": "그건 그래ㅋㅋㅋ그래도 막상 해보면 옛날 생각나서 재밌을 듯ㅋㅋㅋ",
          "original_form": "그건 그래ㅋㅋㅋ그래도 막상 해보면 옛날 생각나서 재밌을 듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000483.1.15",
          "form": "미니홈피 그 미니룸 꾸미는건",
          "original_form": "미니홈피 그 미니룸 꾸미는건",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000483.1.16",
          "form": "재밌어",
          "original_form": "재밌어",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000483.1.17",
          "form": "맞아ㅋㅋㅋㅋ아직도 도토리로 브금 사야하려나?ㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋㅋ아직도 도토리로 브금 사야하려나?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000483.1.18",
