      "utterance": [
        {
          "id": "MDRW2100012096.1.1",
          "form": "네 방금 먹었어요",
          "original_form": "네 방금 먹었어요",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012096.1.2",
          "form": "여름 님도 드셨나요?",
          "original_form": "여름 님도 드셨나요?",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012096.1.3",
          "form": "저는 아직 저녁안먹었어요ㅜ",
          "original_form": "저는 아직 저녁안먹었어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012096.1.4",
          "form": "배가 글케 고프진않는데 떡볶이먹을까 생각중이에요",
          "original_form": "배가 글케 고프진않는데 떡볶이먹을까 생각중이에요",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012096.1.5",
          "form": "떡볶이좋아하세요?",
          "original_form": "떡볶이좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012096.1.6",
          "form": "네 완전 좋아해요!",
          "original_form": "네 완전 좋아해요!",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100012096.1.7",
          "form": "어떤 떡볶이 좋아하세요?",
          "original_form": "어떤 떡볶이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100012096.1.8",
          "form": "전 분식점떡볶이스타일이랑 로제좋아해요",
          "original_form": "전 분식점떡볶이스타일이랑 로제좋아해요",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100012096.1.9",
          "form": "고고님은요",
          "original_form": "고고님은요",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100012096.1.10",
          "form": "오 저도 분식점스타일 좋아해요",
          "original_form": "오 저도 분식점스타일 좋아해요",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012096.1.11",
          "form": "딱히 안 좋아하는 스타일은 없는 것 같아요ㅋㅋㅋ",
          "original_form": "딱히 안 좋아하는 스타일은 없는 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012096.1.12",
          "form": "아하 그럼떡볶이먹을때 먹는조합있나요",
          "original_form": "아하 그럼떡볶이먹을때 먹는조합있나요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012096.1.13",
          "form": "떡볶이 토핑 중에서 중국당면 좋아해요 너무 맛있어요",
          "original_form": "떡볶이 토핑 중에서 중국당면 좋아해요 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012096.1.14",
          "form": "여름님은요?",
          "original_form": "여름님은요?",
          "speaker_id": "2",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012096.1.15",
          "form": "저도 중국당면 좋아해요ㅎㅎ 전 떡볶이먹을때 야채랑오뎅더좋아하는편이라",
          "original_form": "저도 중국당면 좋아해요ㅎㅎ 전 떡볶이먹을때 야채랑오뎅더좋아하는편이라",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012096.1.16",
          "form": "그리고 튀김이랑 순대꼭 같이먹어요 ㅎㅎ",
          "original_form": "그리고 튀김이랑 순대꼭 같이먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012096.1.17",
          "form": "떡튀순 공식이죠ㅋㅋ",
          "original_form": "떡튀순 공식이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012096.1.18",
          "form": "요즘 프랜차이즈 떡볶이는 너무 비싸서 다른 사이드메뉴를 못 시키겠어요",
          "original_form": "요즘 프랜차이즈 떡볶이는 너무 비싸서 다른 사이드메뉴를 못 시키겠어요",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012096.1.19",
          "form": "맞아요ㅜ 솔직히 그정도가격에 그정도 양 안먹는데",
          "original_form": "맞아요ㅜ 솔직히 그정도가격에 그정도 양 안먹는데",
          "speaker_id": "1",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012096.1.20",
          "form": "엽떡이 첫단추였을까요..",
          "original_form": "엽떡이 첫단추였을까요..",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012096.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012096.1.22",
          "form": "다같이 손잡고 가격 좀 내렸으면 좋겠어요",
          "original_form": "다같이 손잡고 가격 좀 내렸으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012096.1.23",
          "form": "ㅜㅜ그니까요 양도 솔직히 넘많아요",
          "original_form": "ㅜㅜ그니까요 양도 솔직히 넘많아요",
          "speaker_id": "1",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012096.1.24",
          "form": "맨날 시키면 남고",
          "original_form": "맨날 시키면 남고",
          "speaker_id": "1",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012096.1.25",
          "form": "그쵸 항상 떡볶이 시켜먹으려면 친구랑 폭식해야 돼요ㅋㅋㅋ",
          "original_form": "그쵸 항상 떡볶이 시켜먹으려면 친구랑 폭식해야 돼요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:37"
        }
      ]
    }
