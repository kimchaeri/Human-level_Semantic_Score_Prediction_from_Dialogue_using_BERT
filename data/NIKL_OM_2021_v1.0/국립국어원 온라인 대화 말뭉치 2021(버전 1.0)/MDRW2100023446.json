      "utterance": [
        {
          "id": "MDRW2100023446.1.1",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "2",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.2",
          "form": "오늘 내일 적립 4프로 더 해줘",
          "original_form": "오늘 내일 적립 4프로 더 해줘",
          "speaker_id": "1",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.3",
          "form": "나 네이버 멤버십 가입 안햇는데",
          "original_form": "나 네이버 멤버십 가입 안햇는데",
          "speaker_id": "2",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.4",
          "form": "지를 게 있다면 지금 질러야한다는 뜻이지",
          "original_form": "지를 게 있다면 지금 질러야한다는 뜻이지",
          "speaker_id": "1",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.5",
          "form": "가입 안해도 4프로 적립 더 해주는건 해줄걸?",
          "original_form": "가입 안해도 4프로 적립 더 해주는건 해줄걸?",
          "speaker_id": "1",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.6",
          "form": "그리고 나 어제랑 어제아래 지름신돌아서 살거다삿는데",
          "original_form": "그리고 나 어제랑 어제아래 지름신돌아서 살거다삿는데",
          "speaker_id": "2",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.7",
          "form": "심지어샤인머스켓도",
          "original_form": "심지어샤인머스켓도",
          "speaker_id": "2",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.8",
          "form": "네이버에서삿는데",
          "original_form": "네이버에서삿는데",
          "speaker_id": "2",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.9",
          "form": "금요일에",
          "original_form": "금요일에",
          "speaker_id": "2",
          "time": "20210830 03:10"
        },
        {
          "id": "MDRW2100023446.1.10",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023446.1.11",
          "form": "아직 발송전이면",
          "original_form": "아직 발송전이면",
          "speaker_id": "1",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023446.1.12",
          "form": "빨리 취소하고 다시 구매하면 되지않을까",
          "original_form": "빨리 취소하고 다시 구매하면 되지않을까",
          "speaker_id": "1",
          "time": "20210830 03:11"
        },
        {
          "id": "MDRW2100023446.1.13",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023446.1.14",
          "form": "그렇게까지...하고싶지않아",
          "original_form": "그렇게까지...하고싶지않아",
          "speaker_id": "2",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023446.1.15",
          "form": "다시 보니까 가입을 해야 더 적립해주는거같애",
          "original_form": "다시 보니까 가입을 해야 더 적립해주는거같애",
          "speaker_id": "1",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023446.1.16",
          "form": "링크는 톡으로 보냈어",
          "original_form": "링크는 톡으로 보냈어",
          "speaker_id": "1",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023446.1.17",
          "form": "그치?내말이맞다니까",
          "original_form": "그치?내말이맞다니까",
          "speaker_id": "2",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023446.1.18",
          "form": "맞네 니말이",
          "original_form": "맞네 니말이",
          "speaker_id": "1",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023446.1.19",
          "form": "사실 가입비가 있어서",
          "original_form": "사실 가입비가 있어서",
          "speaker_id": "1",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023446.1.20",
          "form": "멤버쉽 혜택이 그렇게 센지 모르겟긴해 나도",
          "original_form": "멤버쉽 혜택이 그렇게 센지 모르겟긴해 나도",
          "speaker_id": "1",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100023446.1.21",
          "form": "ㅋㅋ맞어 물건하나 취소하고다시사기엔...",
          "original_form": "ㅋㅋ맞어 물건하나 취소하고다시사기엔...",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.22",
          "form": "그래도ㄱㅅ",
          "original_form": "그래도ㄱㅅ",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.23",
          "form": "최근에 뭐 뭐 샀너",
          "original_form": "최근에 뭐 뭐 샀너",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.24",
          "form": "책이나 좀 사야지",
          "original_form": "책이나 좀 사야지",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.25",
          "form": "나한테 추천해줘",
          "original_form": "나한테 추천해줘",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.26",
          "form": "샤인머스커시",
          "original_form": "샤인머스커시",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.27",
          "form": "태어나서처음먹어봄",
          "original_form": "태어나서처음먹어봄",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.28",
          "form": "샤인머스켓 지금이 한참 나올때야?",
          "original_form": "샤인머스켓 지금이 한참 나올때야?",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.29",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.30",
          "form": "남들처럼부유하지못한샤인머스케환경이라",
          "original_form": "남들처럼부유하지못한샤인머스케환경이라",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.31",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.32",
          "form": "야 근데 샤인머스켓",
          "original_form": "야 근데 샤인머스켓",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.33",
          "form": "태어나서처음사봣기때문이야",
          "original_form": "태어나서처음사봣기때문이야",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.34",
          "form": "당근마켓에서도 한번씩 팔더라",
          "original_form": "당근마켓에서도 한번씩 팔더라",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.35",
          "form": "난 당근에서 한번 사봤어",
          "original_form": "난 당근에서 한번 사봤어",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.36",
          "form": "난 ㄷ ㅓ 가난하기때문에",
          "original_form": "난 ㄷ ㅓ 가난하기때문에",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.37",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "2",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.38",
          "form": "그 상품성 없는거",
          "original_form": "그 상품성 없는거",
          "speaker_id": "1",
          "time": "20210830 03:13"
        },
        {
          "id": "MDRW2100023446.1.39",
          "form": "가난배틀은좋지않다고봐",
          "original_form": "가난배틀은좋지않다고봐",
          "speaker_id": "2",
          "time": "20210830 03:14"
        },
        {
          "id": "MDRW2100023446.1.40",
          "form": "가정용으로 싸게판대서",
          "original_form": "가정용으로 싸게판대서",
          "speaker_id": "1",
          "time": "20210830 03:14"
        },
        {
          "id": "MDRW2100023446.1.41",
          "form": "그런걸 사먹어봤어",
          "original_form": "그런걸 사먹어봤어",
          "speaker_id": "1",
          "time": "20210830 03:14"
        },
        {
          "id": "MDRW2100023446.1.42",
          "form": "나는 이번에 샤인머스켓 3-4구짜린데",
          "original_form": "나는 이번에 샤인머스켓 3-4구짜린데",
          "speaker_id": "2",
          "time": "20210830 03:14"
        },
        {
          "id": "MDRW2100023446.1.43",
          "form": "그건그래... 배틀 시동걸어서 미안해...",
          "original_form": "그건그래... 배틀 시동걸어서 미안해...",
          "speaker_id": "1",
          "time": "20210830 03:14"
        },
        {
          "id": "MDRW2100023446.1.44",
          "form": "할아버지댁에 직접구운휘낭시에랑 가져가서",
          "original_form": "할아버지댁에 직접구운휘낭시에랑 가져가서",
          "speaker_id": "2",
          "time": "20210830 03:14"
        },
        {
          "id": "MDRW2100023446.1.45",
          "form": "가성비 귀여운손녀 하려고",
          "original_form": "가성비 귀여운손녀 하려고",
          "speaker_id": "2",
          "time": "20210830 03:14"
        },
        {
          "id": "MDRW2100023446.1.46",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210830 03:14"
        }
      ]
    }
  ]
