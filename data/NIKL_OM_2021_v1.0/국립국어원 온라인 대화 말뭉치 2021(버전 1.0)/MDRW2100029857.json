        {
          "id": "MDRW2100029857.1.2",
          "form": "참;;",
          "original_form": "참;;",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100029857.1.3",
          "form": "애당초 말이 스트레스 받는 환경일텐데",
          "original_form": "애당초 말이 스트레스 받는 환경일텐데",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100029857.1.4",
          "form": "그러니까요 적의 말을 뺏는다는 설정이라 무작위로",
          "original_form": "그러니까요 적의 말을 뺏는다는 설정이라 무작위로",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100029857.1.5",
          "form": "아무 말이나 받아서 잘 다루고 교감하는 것도 능력일텐데",
          "original_form": "아무 말이나 받아서 잘 다루고 교감하는 것도 능력일텐데",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029857.1.6",
          "form": "선수도 코치도 자격미달이에요",
          "original_form": "선수도 코치도 자격미달이에요",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029857.1.7",
          "form": "나쁜사람들ㅜㅜ",
          "original_form": "나쁜사람들ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029857.1.8",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029857.1.9",
          "form": "아예 아마추어면",
          "original_form": "아예 아마추어면",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029857.1.10",
          "form": "모를까",
          "original_form": "모를까",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029857.1.11",
          "form": "마자용",
          "original_form": "마자용",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029857.1.12",
          "form": "올림픽에 나올 정도인 사람인데",
          "original_form": "올림픽에 나올 정도인 사람인데",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100029857.1.13",
          "form": "그니깐요",
          "original_form": "그니깐요",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029857.1.14",
          "form": "국가를 대표해 온 사람들이",
          "original_form": "국가를 대표해 온 사람들이",
          "speaker_id": "2",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100029857.1.15",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100029857.1.16",
          "form": "확실히 생물이 있는 스포츠는",
          "original_form": "확실히 생물이 있는 스포츠는",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029857.1.17",
          "form": "유대감을 보여주는 쪽으로만 했으면",
          "original_form": "유대감을 보여주는 쪽으로만 했으면",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029857.1.18",
          "form": "좋을 거 같은데",
          "original_form": "좋을 거 같은데",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029857.1.19",
          "form": "그렇죠 안타깝고 아쉬웠어요",
          "original_form": "그렇죠 안타깝고 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029857.1.20",
          "form": "뭔가 철인 3종 처럼",
          "original_form": "뭔가 철인 3종 처럼",
          "speaker_id": "1",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029857.1.21",
          "form": "그 자체로 의미 있는 스포츠니",
          "original_form": "그 자체로 의미 있는 스포츠니",
          "speaker_id": "1",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029857.1.22",
          "form": "없애긴 힘들겠지만...",
          "original_form": "없애긴 힘들겠지만...",
          "speaker_id": "1",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029857.1.23",
          "form": "맞아요 쿠베르탱이 직접 창시 했다니 절대 없어지진 않겠지만",
          "original_form": "맞아요 쿠베르탱이 직접 창시 했다니 절대 없어지진 않겠지만",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029857.1.24",
          "form": "적어도 말을 학대하는 건 분명히 금지해야 할 거 같아요",
          "original_form": "적어도 말을 학대하는 건 분명히 금지해야 할 거 같아요",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029857.1.25",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "1",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029857.1.26",
          "form": "근대 5종 말고 또",
          "original_form": "근대 5종 말고 또",
          "speaker_id": "1",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029857.1.27",
          "form": "뭐 보셨다고 하셨죠",
          "original_form": "뭐 보셨다고 하셨죠",
          "speaker_id": "1",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029857.1.28",
          "form": "저 트램펄린 이요",
          "original_form": "저 트램펄린 이요",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029857.1.29",
          "form": "리듬체조 너무 멋지던데",
          "original_form": "리듬체조 너무 멋지던데",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029857.1.30",
          "form": "기계체조도 그렇고",
          "original_form": "기계체조도 그렇고",
          "speaker_id": "2",
