      "utterance": [
        {
          "id": "MDRW2100029013.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ군함도 어설프게 건드린건 류승완이 잘못하긴 했지...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ군함도 어설프게 건드린건 류승완이 잘못하긴 했지...",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029013.1.2",
          "form": "아니 근데그땐 배우감독다 넹글해서",
          "original_form": "아니 근데그땐 배우감독다 넹글해서",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029013.1.3",
          "form": "말도안되는소리로 입을 놀리고...",
          "original_form": "말도안되는소리로 입을 놀리고...",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029013.1.4",
          "form": "ㅇㅇㅇㅇ......",
          "original_form": "ㅇㅇㅇㅇ......",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029013.1.5",
          "form": "안그래도 불 났는데",
          "original_form": "안그래도 불 났는데",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029013.1.6",
          "form": "기름 들이부었지....",
          "original_form": "기름 들이부었지....",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029013.1.7",
          "form": "다른것도 아니고 일제강점기 관련해서 경솔하게 구는 건",
          "original_form": "다른것도 아니고 일제강점기 관련해서 경솔하게 구는 건",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029013.1.8",
          "form": "1만 욕먹을 거 10 먹잖아요",
          "original_form": "1만 욕먹을 거 10 먹잖아요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029013.1.9",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029013.1.10",
          "form": "아니 모두",
          "original_form": "아니 모두",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029013.1.11",
          "form": "욱일기자르는거보고",
          "original_form": "욱일기자르는거보고",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029013.1.12",
          "form": "각시탈의그것을생각하고",
          "original_form": "각시탈의그것을생각하고",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029013.1.13",
          "form": "뽕에찼는데",
          "original_form": "뽕에찼는데",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029013.1.14",
          "form": "별거아닌씬이었다고",
          "original_form": "별거아닌씬이었다고",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029013.1.15",
          "form": "그니깤ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니깤ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029013.1.16",
          "form": "솔직히 홍보할 때",
          "original_form": "솔직히 홍보할 때",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029013.1.17",
          "form": "그 부분 내보낸거",
          "original_form": "그 부분 내보낸거",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029013.1.18",
          "form": "노린거 아닐리가 없는데",
          "original_form": "노린거 아닐리가 없는데",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029013.1.19",
          "form": "정작 뚜껑 열어보니ㅋㅋㅋㅋㅋㅋㅋ....",
          "original_form": "정작 뚜껑 열어보니ㅋㅋㅋㅋㅋㅋㅋ....",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029013.1.20",
          "form": "ㅇㅇㅇㅇ 완전",
          "original_form": "ㅇㅇㅇㅇ 완전",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029013.1.21",
          "form": "그냥 천이필요해서자른거더만",
          "original_form": "그냥 천이필요해서자른거더만",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029013.1.22",
          "form": "악마의편집무엇",
          "original_form": "악마의편집무엇",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029013.1.23",
          "form": "홍보라도 국뽕st로 하지 말았어야지",
          "original_form": "홍보라도 국뽕st로 하지 말았어야지",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029013.1.24",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029013.1.25",
          "form": "아 애국심강요는엄청했다던데",
          "original_form": "아 애국심강요는엄청했다던데",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029013.1.26",
          "form": "이상한데서",
          "original_form": "이상한데서",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029013.1.27",
          "form": "?????",
          "original_form": "?????",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029013.1.28",
          "form": "애국심을 강요했는데 그런 결과물이.. 나오나...?",
          "original_form": "애국심을 강요했는데 그런 결과물이.. 나오나...?",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029013.1.29",
          "form": "이해를잘못하겠어",
          "original_form": "이해를잘못하겠어",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029013.1.30",
          "form": "개과한애국심강요 눈물강요신파",
          "original_form": "개과한애국심강요 눈물강요신파",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029013.1.31",
          "form": "이랬다는데",
          "original_form": "이랬다는데",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029013.1.32",
          "form": "거기조선인들나쁜놈으로나오잖아",
          "original_form": "거기조선인들나쁜놈으로나오잖아",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029013.1.33",
          "form": "ㅇㅇㅇ나쁜 조선인 착한 일본인 다 있었다",
          "original_form": "ㅇㅇㅇ나쁜 조선인 착한 일본인 다 있었다",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029013.1.34",
          "form": "뭐 그런 식으로",
          "original_form": "뭐 그런 식으로",
