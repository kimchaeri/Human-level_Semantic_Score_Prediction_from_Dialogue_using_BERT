      "utterance": [
        {
          "id": "MDRW2100011108.1.1",
          "form": "여행 좋아하시나여?",
          "original_form": "여행 좋아하시나여?",
          "speaker_id": "1",
          "time": "20210824 01:02"
        },
        {
          "id": "MDRW2100011108.1.2",
          "form": "많이 가진 않는데 혼자 돌아다니는 건 좋아해요",
          "original_form": "많이 가진 않는데 혼자 돌아다니는 건 좋아해요",
          "speaker_id": "2",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100011108.1.3",
          "form": "오 최근에 어디 가보셨나요",
          "original_form": "오 최근에 어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100011108.1.4",
          "form": "최근은 아닌데 몇달전에 진주랑 부산 갔었어요",
          "original_form": "최근은 아닌데 몇달전에 진주랑 부산 갔었어요",
          "speaker_id": "2",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011108.1.5",
          "form": "오 진주도 볼게 많나요?",
          "original_form": "오 진주도 볼게 많나요?",
          "speaker_id": "1",
          "time": "20210824 01:10"
        },
        {
          "id": "MDRW2100011108.1.6",
          "form": "아뇨 그 쪽에 친구가 있어서 잠깐 보러갔던 거예요",
          "original_form": "아뇨 그 쪽에 친구가 있어서 잠깐 보러갔던 거예요",
          "speaker_id": "2",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011108.1.7",
          "form": "볼 건 딱히 없었어요",
          "original_form": "볼 건 딱히 없었어요",
          "speaker_id": "2",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011108.1.8",
          "form": "부산에는 어디가 가볼만할까요?",
          "original_form": "부산에는 어디가 가볼만할까요?",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011108.1.9",
          "form": "보통 바다보러 많이 가는 것 같아요 광안리나 해운대",
          "original_form": "보통 바다보러 많이 가는 것 같아요 광안리나 해운대",
          "speaker_id": "2",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100011108.1.10",
          "form": "근데 뭐 보러 가는 것보다는 먹는 걸 추천드려요 밀면이나 돼지국밥 같은 거요",
          "original_form": "근데 뭐 보러 가는 것보다는 먹는 걸 추천드려요 밀면이나 돼지국밥 같은 거요",
          "speaker_id": "2",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100011108.1.11",
          "form": "낙곱새가 땡기네요ㅎㅎ 돼지국밥도",
          "original_form": "낙곱새가 땡기네요ㅎㅎ 돼지국밥도",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100011108.1.12",
          "form": "오 낙곱새도 맛있겠네요",
          "original_form": "오 낙곱새도 맛있겠네요",
          "speaker_id": "2",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100011108.1.13",
          "form": "부산 가서 다른 거 드시고 싶은 거 더 있으신가요?",
          "original_form": "부산 가서 다른 거 드시고 싶은 거 더 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100011108.1.14",
          "form": "밀면도 먹고싶네요~",
          "original_form": "밀면도 먹고싶네요~",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100011108.1.15",
          "form": "다음에 부산 오시면 한 번 드셔보세요",
          "original_form": "다음에 부산 오시면 한 번 드셔보세요",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100011108.1.16",
          "form": "부산밀면 메모해야겠네요",
          "original_form": "부산밀면 메모해야겠네요",
