          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012075.1.13",
          "form": "발열이 너무 심해요;;",
          "original_form": "발열이 너무 심해요;;",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012075.1.14",
