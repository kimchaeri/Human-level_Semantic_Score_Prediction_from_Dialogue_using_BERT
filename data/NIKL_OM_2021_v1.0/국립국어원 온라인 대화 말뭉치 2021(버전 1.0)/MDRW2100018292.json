      "utterance": [
        {
          "id": "MDRW2100018292.1.1",
          "form": "맛집 많이 아세요?",
          "original_form": "맛집 많이 아세요?",
          "speaker_id": "1",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100018292.1.2",
          "form": "많이는 모르지만 좋아는 해요",
          "original_form": "많이는 모르지만 좋아는 해요",
          "speaker_id": "2",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100018292.1.3",
          "form": "고기리막국수 아시나요?",
          "original_form": "고기리막국수 아시나요?",
          "speaker_id": "2",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100018292.1.4",
          "form": "고기리막국수가 뭐예요?",
          "original_form": "고기리막국수가 뭐예요?",
          "speaker_id": "1",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100018292.1.5",
          "form": "유명한 맛집인데 오뚜기에서 제품 만들어서 팔더라구요",
          "original_form": "유명한 맛집인데 오뚜기에서 제품 만들어서 팔더라구요",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100018292.1.6",
          "form": "용인 고기리에 있는 막국수집이에요",
          "original_form": "용인 고기리에 있는 막국수집이에요",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100018292.1.7",
          "form": "아ㅋ 용인에 가본적이 없어요ㅠ",
          "original_form": "아ㅋ 용인에 가본적이 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100018292.1.8",
          "form": "아 실례지만 어디사시나요?",
          "original_form": "아 실례지만 어디사시나요?",
          "speaker_id": "2",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100018292.1.9",
          "form": "저는 부산에 살아요ㅋ",
          "original_form": "저는 부산에 살아요ㅋ",
          "speaker_id": "1",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100018292.1.10",
          "form": "우왕 저 부산에 할매등대집인가",
          "original_form": "우왕 저 부산에 할매등대집인가",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100018292.1.11",
          "form": "장어먹으러 가봤어요",
          "original_form": "장어먹으러 가봤어요",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100018292.1.12",
          "form": "최자로드에서 나온곳이에요",
          "original_form": "최자로드에서 나온곳이에요",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100018292.1.13",
          "form": "오히려 외지분들이 부산맛집을 더 잘 아시더라구요",
          "original_form": "오히려 외지분들이 부산맛집을 더 잘 아시더라구요",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100018292.1.14",
          "form": "부산 찐맛집은 어딘가요",
          "original_form": "부산 찐맛집은 어딘가요",
          "speaker_id": "2",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100018292.1.15",
          "form": "관광객들은 검색해서갈수밖에 없어요ㅜ",
          "original_form": "관광객들은 검색해서갈수밖에 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100018292.1.16",
          "form": "찐맛집요?  솔직히 전 맛집 잘 몰라요ㅠ",
          "original_form": "찐맛집요?  솔직히 전 맛집 잘 몰라요ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100018292.1.17",
          "form": "아 넵ㅎ 이하정간장게장은 별로 였고",
          "original_form": "아 넵ㅎ 이하정간장게장은 별로 였고",
          "speaker_id": "2",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100018292.1.18",
          "form": "암소한우인가??거긴비싸서 못멋먹은 기억이 있네요ㅋㅋ",
          "original_form": "암소한우인가??거긴비싸서 못멋먹은 기억이 있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100018292.1.19",
          "form": "암소한우 맛있긴 했어요. 예전에 가봤어요ㅋ",
          "original_form": "암소한우 맛있긴 했어요. 예전에 가봤어요ㅋ",
          "speaker_id": "1",
          "time": "20210827 17:38"
        }
