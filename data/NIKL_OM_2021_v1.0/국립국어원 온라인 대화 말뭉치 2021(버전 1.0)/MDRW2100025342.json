      "utterance": [
        {
          "id": "MDRW2100025342.1.1",
          "form": "아침먹었어??",
          "original_form": "아침먹었어??",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025342.1.2",
          "form": "타코파니니 우유ㅎ",
          "original_form": "타코파니니 우유ㅎ",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100025342.1.3",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100025342.1.4",
          "form": "타코파니니?",
          "original_form": "타코파니니?",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025342.1.5",
          "form": "신기한 조합이네 ㅋㅋ 타코는 멕시코음식 파니니는 이태리음식 ㅋㅋ",
          "original_form": "신기한 조합이네 ㅋㅋ 타코는 멕시코음식 파니니는 이태리음식 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025342.1.6",
          "form": "ㅎㅅ ㅎ속 내용물이 타코\r\n파니니는 빵형태ㅎ",
          "original_form": "ㅎㅅ ㅎ속 내용물이 타코\r\n파니니는 빵형태ㅎ",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025342.1.7",
          "form": "살 매콤한듯아닌듯 맛 괜찮더라고ㅎ",
          "original_form": "살 매콤한듯아닌듯 맛 괜찮더라고ㅎ",
          "speaker_id": "2",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025342.1.8",
          "form": "ㅇㅇ내용물 타코 ㅋㅋ괜찮을것 같은데 완전퓨전 ㅋㅋ",
          "original_form": "ㅇㅇ내용물 타코 ㅋㅋ괜찮을것 같은데 완전퓨전 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100025342.1.9",
          "form": "ㅋㅋㅋ퓨전이 괜찮더라\r\n현지화음식ㅋㅋ",
          "original_form": "ㅋㅋㅋ퓨전이 괜찮더라\r\n현지화음식ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025342.1.10",
          "form": "ㅇㅇ 한국입맛에 맞게",
          "original_form": "ㅇㅇ 한국입맛에 맞게",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025342.1.11",
          "form": "점심먹었나?",
          "original_form": "점심먹었나?",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025342.1.12",
          "form": "응 고사리+삼겹+김치 먹었어",
          "original_form": "응 고사리+삼겹+김치 먹었어",
          "speaker_id": "2",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025342.1.13",
          "form": "와우 점심도 맛있는거 먹었네 삼겹살이랑 고사리 조합 너무 좋지",
          "original_form": "와우 점심도 맛있는거 먹었네 삼겹살이랑 고사리 조합 너무 좋지",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025342.1.14",
          "form": "그치 고사리 좋아하는데\r\n여긴 기름범벅이라 고사리 맛이 안나더라",
          "original_form": "그치 고사리 좋아하는데\r\n여긴 기름범벅이라 고사리 맛이 안나더라",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100025342.1.15",
          "form": "아쉽ㅜㅜ",
          "original_form": "아쉽ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100025342.1.16",
          "form": "삼겹살 기름에 굽는게 맛잇다 그래서 그 기름에 구웠나봐ㅜ",
          "original_form": "삼겹살 기름에 굽는게 맛잇다 그래서 그 기름에 구웠나봐ㅜ",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025342.1.17",
          "form": "난 저녁먹었다 동그랑땡 부쳐서 김이랑 김치랑",
          "original_form": "난 저녁먹었다 동그랑땡 부쳐서 김이랑 김치랑",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025342.1.18",
          "form": "철판 그릇에 데워?주는데\r\n철판에 눌러붙지말라고 기름을 한그 부어서 데워주니까\r\n기름이 많아ㅎㅎ",
          "original_form": "철판 그릇에 데워?주는데\r\n철판에 눌러붙지말라고 기름을 한그 부어서 데워주니까\r\n기름이 많아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025342.1.19",
          "form": "시간이 훅 갔네\r\n5시 넘었어",
          "original_form": "시간이 훅 갔네\r\n5시 넘었어",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025342.1.20",
          "form": "동그랑땡 오랜만에 듣는다ㅎ",
          "original_form": "동그랑땡 오랜만에 듣는다ㅎ",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025342.1.21",
          "form": "아 그럼 느끼하겠다 ㅜㅜ",
          "original_form": "아 그럼 느끼하겠다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025342.1.22",
          "form": "동그랑땡 요즘 가끔 먹어 반찬하기 좋더라구",
          "original_form": "동그랑땡 요즘 가끔 먹어 반찬하기 좋더라구",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025342.1.23",
          "form": "저녁 정해졌다ㅋ",
          "original_form": "저녁 정해졌다ㅋ",
          "speaker_id": "2",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100025342.1.24",
