      "utterance": [
        {
          "id": "MDRW2100045365.1.1",
          "form": "사람 애기만 하다는",
          "original_form": "사람 애기만 하다는",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045365.1.2",
          "form": "모든 정성이 필요하네요",
          "original_form": "모든 정성이 필요하네요",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045365.1.3",
          "form": "다이어트 사료 먹이는데",
          "original_form": "다이어트 사료 먹이는데",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045365.1.4",
          "form": "살쪘나 봐요",
          "original_form": "살쪘나 봐요",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045365.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045365.1.6",
          "form": "근데 변비가 있어요",
          "original_form": "근데 변비가 있어요",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045365.1.7",
          "form": "고양이도 사람이랑 같네요",
          "original_form": "고양이도 사람이랑 같네요",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045365.1.8",
          "form": "물을 많이 먹여야 한다는데",
          "original_form": "물을 많이 먹여야 한다는데",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045365.1.9",
          "form": "쉽지 않겠네요",
          "original_form": "쉽지 않겠네요",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045365.1.10",
          "form": "그래서 고기랑 섞어 줘요",
          "original_form": "그래서 고기랑 섞어 줘요",
          "speaker_id": "2",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045365.1.11",
          "form": "그래야겠네요",
          "original_form": "그래야겠네요",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045365.1.12",
          "form": "매일 누기는 해서 심하진 않는듯요",
          "original_form": "매일 누기는 해서 심하진 않는듯요",
          "speaker_id": "2",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045365.1.13",
          "form": "그럼 괜찮을걸요",
          "original_form": "그럼 괜찮을걸요",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045365.1.14",
          "form": "제일 좋아하는 간식이 무엇인가요",
          "original_form": "제일 좋아하는 간식이 무엇인가요",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045365.1.15",
          "form": "고기는 다 좋아하더라고요",
          "original_form": "고기는 다 좋아하더라고요",
          "speaker_id": "2",
          "time": "20210908 02:10"
