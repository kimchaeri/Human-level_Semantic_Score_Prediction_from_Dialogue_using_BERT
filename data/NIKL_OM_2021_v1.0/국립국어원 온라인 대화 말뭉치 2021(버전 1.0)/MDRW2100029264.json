      "utterance": [
        {
          "id": "MDRW2100029264.1.1",
          "form": "네 저 눈좋아해요!",
          "original_form": "네 저 눈좋아해요!",
          "speaker_id": "1",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029264.1.2",
          "form": "근데 저는 뭐 많이 보고 자라와서",
          "original_form": "근데 저는 뭐 많이 보고 자라와서",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029264.1.3",
          "form": "괜찮지만",
          "original_form": "괜찮지만",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029264.1.4",
          "form": "아이들이 못보는게 너무 아쉬워요ㅜ",
          "original_form": "아이들이 못보는게 너무 아쉬워요ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029264.1.5",
          "form": "눈사람한번 못만들어보고..흑흑",
          "original_form": "눈사람한번 못만들어보고..흑흑",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029264.1.6",
          "form": "앗 그러네요 어른들은 눈이 악마의 똥가루같은 거지만",
          "original_form": "앗 그러네요 어른들은 눈이 악마의 똥가루같은 거지만",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029264.1.7",
          "form": "아이들은 아니죠ㅠㅠ",
          "original_form": "아이들은 아니죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100029264.1.8",
          "form": "악마의똥가루ㅋㅋㅋㅋㅋㅋㅋㅋㅋ  너무웃긴표현이네요 ㅋㄱㅋ",
          "original_form": "악마의똥가루ㅋㅋㅋㅋㅋㅋㅋㅋㅋ  너무웃긴표현이네요 ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029264.1.9",
          "form": "거제는  거의 조선업이거든요",
          "original_form": "거제는  거의 조선업이거든요",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029264.1.10",
          "form": "그래서 눈이오면",
          "original_form": "그래서 눈이오면",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029264.1.11",
          "form": "일이 올스탑이에요 ㅋㅋㅋ 그냥 못하는거!ㅋㄱㅋ",
          "original_form": "일이 올스탑이에요 ㅋㅋㅋ 그냥 못하는거!ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029264.1.12",
          "form": "헐 그래요???",
          "original_form": "헐 그래요???",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029264.1.13",
          "form": "근데 뭐 그런날이없네요~ 눈이안오니까요^^;",
          "original_form": "근데 뭐 그런날이없네요~ 눈이안오니까요^^;",
          "speaker_id": "1",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029264.1.14",
          "form": "혹시 비가 와도 그런가요???",
          "original_form": "혹시 비가 와도 그런가요???",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029264.1.15",
          "form": "전국적으로 눈이..라고하지만  전국에서 저희만 빠진다는요",
          "original_form": "전국적으로 눈이..라고하지만  전국에서 저희만 빠진다는요",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.16",
          "form": "아니요 ㅎㅎ;ㅠ",
          "original_form": "아니요 ㅎㅎ;ㅠ",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.17",
          "form": "아이곸ㅋ쿠ㅜㅜㅠ",
          "original_form": "아이곸ㅋ쿠ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.18",
          "form": "비는 상관없어요",
          "original_form": "비는 상관없어요",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.19",
          "form": "다른나라 같겠어요",
          "original_form": "다른나라 같겠어요",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.21",
          "form": "좀 신기하네요 같은 물인데요",
          "original_form": "좀 신기하네요 같은 물인데요",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.22",
          "form": "태풍이 영향이있죠 ㅎㅎ",
          "original_form": "태풍이 영향이있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.23",
          "form": "태풍 오면 올스탑인가요???",
          "original_form": "태풍 오면 올스탑인가요???",
          "speaker_id": "2",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.24",
          "form": "오웅 너무 쌀쌀해서 지금 문을 닫고자야하나 말아야하나 엄청 고민되네요 ㅋㅋㅋ",
          "original_form": "오웅 너무 쌀쌀해서 지금 문을 닫고자야하나 말아야하나 엄청 고민되네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.25",
          "form": "네네 ㅋㅋ",
          "original_form": "네네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029264.1.26",
          "form": "태풍에따라서 다르지만요 ㅋㅋ",
          "original_form": "태풍에따라서 다르지만요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029264.1.27",
          "form": "뭐 이와중에 나가는사람도 분명 있겠쭄ㅋㅋ?",
          "original_form": "뭐 이와중에 나가는사람도 분명 있겠쭄ㅋㅋ?",
          "speaker_id": "1",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029264.1.28",
          "form": "코로나 때문에 셧다운이 몇번 됐었죠ㅠㅠ",
          "original_form": "코로나 때문에 셧다운이 몇번 됐었죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029264.1.29",
          "form": "와ㅠㅠㅠㅠ 태풍이 오는데 나오시는거면 진짜 너무 대단하시네요",
          "original_form": "와ㅠㅠㅠㅠ 태풍이 오는데 나오시는거면 진짜 너무 대단하시네요",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029264.1.30",
          "form": "아....코로나....그렇겠네요",
          "original_form": "아....코로나....그렇겠네요",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029264.1.31",
          "form": "오우 바람 너무 무서워요",
          "original_form": "오우 바람 너무 무서워요",
          "speaker_id": "1",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029264.1.32",
          "form": "지난주였나요 ~ 태풍올때",
          "original_form": "지난주였나요 ~ 태풍올때",
          "speaker_id": "1",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029264.1.33",
          "form": "아침 기온은 보통 어떤 편이예요?",
          "original_form": "아침 기온은 보통 어떤 편이예요?",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029264.1.34",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029264.1.35",
          "form": "거제에 천둥번개가 엄청엄청 심하게 쳤어요!",
          "original_form": "거제에 천둥번개가 엄청엄청 심하게 쳤어요!",
          "speaker_id": "1",
          "time": "20210903 00:56"
        },
        {
          "id": "MDRW2100029264.1.36",
          "form": "아!!",
          "original_form": "아!!",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029264.1.37",
          "form": "지금 20도정도네요^^",
          "original_form": "지금 20도정도네요^^",
          "speaker_id": "1",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029264.1.38",
          "form": "막 하늘 무너질 정도로요??",
          "original_form": "막 하늘 무너질 정도로요??",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029264.1.39",
          "form": "문 닫고 주무시는게 나으시겠네요",
          "original_form": "문 닫고 주무시는게 나으시겠네요",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029264.1.40",
          "form": "기온이 정말 가을날씨네요...",
          "original_form": "기온이 정말 가을날씨네요...",
          "speaker_id": "2",
          "time": "20210903 00:57"
        },
        {
          "id": "MDRW2100029264.1.41",
          "form": "그래야겠어요ㅠ 남편은 시원하겠지만 애기들이 감기걸릴까봐 걱정이네요",
          "original_form": "그래야겠어요ㅠ 남편은 시원하겠지만 애기들이 감기걸릴까봐 걱정이네요",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029264.1.42",
          "form": "전 에어컨켜고 이불덥고 자는 사람이라 딱좋은데",
          "original_form": "전 에어컨켜고 이불덥고 자는 사람이라 딱좋은데",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029264.1.43",
          "form": "꼬맹이둘은 이불을 안덥거든요ㅜ",
          "original_form": "꼬맹이둘은 이불을 안덥거든요ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:58"
        },
        {
          "id": "MDRW2100029264.1.44",
          "form": "앗 꼬맹이들 있으면 진짜 그러네요",
          "original_form": "앗 꼬맹이들 있으면 진짜 그러네요",
          "speaker_id": "2",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029264.1.45",
          "form": "환절기라 애들 감기 잘 걸리더라구요ㅠㅠㅠ",
          "original_form": "환절기라 애들 감기 잘 걸리더라구요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029264.1.46",
          "form": "유치원꼬맹이들은 이불을 주면 발로 차고 그냥자네요?ㅜㅜ",
          "original_form": "유치원꼬맹이들은 이불을 주면 발로 차고 그냥자네요?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029264.1.47",
          "form": "맞아요ㅠ 일교차커지면서",
          "original_form": "맞아요ㅠ 일교차커지면서",
          "speaker_id": "1",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029264.1.48",
          "form": "조심해야하죠 ~~",
          "original_form": "조심해야하죠 ~~",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
