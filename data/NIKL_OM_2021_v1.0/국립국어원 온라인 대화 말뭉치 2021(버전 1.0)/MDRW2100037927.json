      "utterance": [
        {
          "id": "MDRW2100037927.1.1",
          "form": "경우의 수에서 주연",
          "original_form": "경우의 수에서 주연",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100037927.1.2",
          "form": "아 누군지 알겠어요ㅋ",
          "original_form": "아 누군지 알겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100037927.1.3",
          "form": "거의 주연이네 다",
          "original_form": "거의 주연이네 다",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037927.1.4",
          "form": "옹성우랑 나온 드라마",
          "original_form": "옹성우랑 나온 드라마",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037927.1.5",
          "form": "연기가 출중 하지는 않던데요",
          "original_form": "연기가 출중 하지는 않던데요",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037927.1.6",
          "form": "응 비트원이라는 단막극은 완전 치정 멜로드라마",
          "original_form": "응 비트원이라는 단막극은 완전 치정 멜로드라마",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037927.1.7",
          "form": "또 치정?  요새 치정 아니면 안되나요",
          "original_form": "또 치정?  요새 치정 아니면 안되나요",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037927.1.8",
          "form": "쌍둥이 형제가 한여자를 사이에 두구 벌어지는 내용",
          "original_form": "쌍둥이 형제가 한여자를 사이에 두구 벌어지는 내용",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037927.1.9",
          "form": "완전 막장의 냄새가",
          "original_form": "완전 막장의 냄새가",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037927.1.10",
          "form": "치정 아니면 살인범죄네",
          "original_form": "치정 아니면 살인범죄네",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037927.1.11",
          "form": "꼭 형제들이 같은 여자 좋아하대요",
          "original_form": "꼭 형제들이 같은 여자 좋아하대요",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037927.1.12",
          "form": "성유빈 남주가 1인2역인갑다",
          "original_form": "성유빈 남주가 1인2역인갑다",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037927.1.13",
          "form": "성유빈이 누구죠?",
          "original_form": "성유빈이 누구죠?",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037927.1.14",
          "form": "영화 완득이에서 나왔나봐",
          "original_form": "영화 완득이에서 나왔나봐",
