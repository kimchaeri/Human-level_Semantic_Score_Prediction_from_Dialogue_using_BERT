      "utterance": [
        {
          "id": "MDRW2100016255.1.1",
          "form": "와 좋네요",
          "original_form": "와 좋네요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016255.1.2",
          "form": "ㅋㅋㅋ원우얘기 더 해바",
          "original_form": "ㅋㅋㅋ원우얘기 더 해바",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016255.1.3",
          "form": "나 걔 얼굴 밖에 몰라",
          "original_form": "나 걔 얼굴 밖에 몰라",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016255.1.4",
          "form": "심지어 얼굴도",
          "original_form": "심지어 얼굴도",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016255.1.5",
          "form": "세븐틴 애들 사이에 잇으면",
          "original_form": "세븐틴 애들 사이에 잇으면",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016255.1.6",
          "form": "못 찾을 때 완전 많음",
          "original_form": "못 찾을 때 완전 많음",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016255.1.7",
          "form": "안경 안써서?",
          "original_form": "안경 안써서?",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016255.1.8",
          "form": "너무 잘 아는데?",
          "original_form": "너무 잘 아는데?",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016255.1.9",
          "form": "ㅋㅋㅋㅋ활동할땐 안쓰나바",
          "original_form": "ㅋㅋㅋㅋ활동할땐 안쓰나바",
          "speaker_id": "1",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016255.1.10",
          "form": "하지만 안경 쓴게 잘 생겼는걸",
          "original_form": "하지만 안경 쓴게 잘 생겼는걸",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016255.1.11",
          "form": "고잉세븐틴 때는 많이 쓰고",
          "original_form": "고잉세븐틴 때는 많이 쓰고",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016255.1.12",
          "form": "활동 때는",
          "original_form": "활동 때는",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016255.1.13",
          "form": "잘 안쓰는듯",
          "original_form": "잘 안쓰는듯",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016255.1.14",
          "form": "자체 예능을 많이 찍어야겠네",
          "original_form": "자체 예능을 많이 찍어야겠네",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016255.1.15",
          "form": "난 근데 멋진 사진으로도 보고싶으니까",
          "original_form": "난 근데 멋진 사진으로도 보고싶으니까",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016255.1.16",
          "form": "우리나라가 안경에 관대해져야만",
          "original_form": "우리나라가 안경에 관대해져야만",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016255.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016255.1.18",
          "form": "안경 화보 가쟈",
          "original_form": "안경 화보 가쟈",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016255.1.19",
          "form": "엥간하면 렌즈 끼자너",
          "original_form": "엥간하면 렌즈 끼자너",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016255.1.20",
          "form": "연예인들 안경 잘 안써",
          "original_form": "연예인들 안경 잘 안써",
          "speaker_id": "1",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100016255.1.21",
          "form": "노렌즈 노오",
          "original_form": "노렌즈 노오",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016255.1.22",
          "form": "쟨 안경이어야해",
          "original_form": "쟨 안경이어야해",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016255.1.23",
          "form": "소속사에 메일 보내바",
          "original_form": "소속사에 메일 보내바",
          "speaker_id": "1",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100016255.1.24",
          "form": "안경을 달라 안경 (시위",
          "original_form": "안경을 달라 안경 (시위",
          "speaker_id": "2",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100016255.1.25",
          "form": "(1인시위",
          "original_form": "(1인시위",
          "speaker_id": "2",
          "time": "20210826 11:30"
        }
      ]
