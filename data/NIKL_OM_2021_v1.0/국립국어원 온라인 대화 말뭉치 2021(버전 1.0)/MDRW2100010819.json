      "utterance": [
        {
          "id": "MDRW2100010819.1.1",
          "form": "난 요즘 중고거래로",
          "original_form": "난 요즘 중고거래로",
          "speaker_id": "1",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010819.1.2",
          "form": "당근마켓을 많이해",
          "original_form": "당근마켓을 많이해",
          "speaker_id": "1",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010819.1.3",
          "form": "저는 중고나라를 주로 이용해요",
          "original_form": "저는 중고나라를 주로 이용해요",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010819.1.4",
          "form": "주로 뭘 사시나요",
          "original_form": "주로 뭘 사시나요",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010819.1.5",
          "form": "작은 가전제품들 많이 사",
          "original_form": "작은 가전제품들 많이 사",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010819.1.6",
          "form": "새 제품 싸게 팔아서 좋아",
          "original_form": "새 제품 싸게 팔아서 좋아",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010819.1.7",
          "form": "name2는 뭐 사??",
          "original_form": "&name2&는 뭐 사??",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010819.1.8",
          "form": "와 당근이 좋다고 들었는데 저도 앱 깔아야겠네요",
          "original_form": "와 당근이 좋다고 들었는데 저도 앱 깔아야겠네요",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010819.1.9",
          "form": "전 소소한 옷가지 구두 이런 거 사요",
          "original_form": "전 소소한 옷가지 구두 이런 거 사요",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010819.1.10",
          "form": "오! 당근 한번 써봐 중고나라보다 조금 더 편한것 같아",
          "original_form": "오! 당근 한번 써봐 중고나라보다 조금 더 편한것 같아",
          "speaker_id": "1",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010819.1.11",
          "form": "가장최근에 산 물건이 뭔가요",
          "original_form": "가장최근에 산 물건이 뭔가요",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010819.1.12",
          "form": "ㅎㅎ 당근은 직거래지요",
          "original_form": "ㅎㅎ 당근은 직거래지요",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010819.1.13",
          "form": "최근에는 온열안대 샀어",
          "original_form": "최근에는 온열안대 샀어",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010819.1.14",
          "form": "오오 온열안대 좋은가요?",
          "original_form": "오오 온열안대 좋은가요?",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010819.1.15",
          "form": "꿀잠잘듯요",
          "original_form": "꿀잠잘듯요",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010819.1.16",
          "form": "시력이 안좋은편이라 샀는데 꽤 좋은거같아",
          "original_form": "시력이 안좋은편이라 샀는데 꽤 좋은거같아",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010819.1.17",
          "form": "사길 잘한듯! 만원에 샀거든ㅋㅋㅋ",
          "original_form": "사길 잘한듯! 만원에 샀거든ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010819.1.18",
          "form": "헐! 잘 샀네요!",
          "original_form": "헐! 잘 샀네요!",
          "speaker_id": "2",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010819.1.19",
          "form": "잘 때 쓰시는 거죠?",
          "original_form": "잘 때 쓰시는 거죠?",
          "speaker_id": "2",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010819.1.20",
          "form": "저도 혹하네요",
          "original_form": "저도 혹하네요",
          "speaker_id": "2",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010819.1.21",
          "form": "응 자기 전에 주로 써! 아무개도 기회되면 하나 구매해봐",
          "original_form": "응 자기 전에 주로 써! 아무개도 기회되면 하나 구매해봐",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010819.1.22",
          "form": "요즘 액정볼일이 많잖아ㅎㅎ 피로가 잘풀려",
          "original_form": "요즘 액정볼일이 많잖아ㅎㅎ 피로가 잘풀려",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010819.1.23",
          "form": "네네. 요새 불면증이 있는데 하나 구입해봐야겠어요",
          "original_form": "네네. 요새 불면증이 있는데 하나 구입해봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010819.1.24",
          "form": "추천 감사드려요",
          "original_form": "추천 감사드려요",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010819.1.25",
          "form": "ㅎㅎㅎ 도움이 되었다니 좋다",
          "original_form": "ㅎㅎㅎ 도움이 되었다니 좋다",
          "speaker_id": "1",
          "time": "20210823 22:18"
        }
      ]
    }
  ]
