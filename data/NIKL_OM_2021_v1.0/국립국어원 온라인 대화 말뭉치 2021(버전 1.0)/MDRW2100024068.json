      "utterance": [
        {
          "id": "MDRW2100024068.1.1",
          "form": "점심먹었나요?",
          "original_form": "점심먹었나요?",
          "speaker_id": "2",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100024068.1.2",
          "form": "오늘도 점심 구내식당 먹었어요 ㅎㅎ",
          "original_form": "오늘도 점심 구내식당 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024068.1.3",
          "form": "저녁 뭐드실건가요~?",
          "original_form": "저녁 뭐드실건가요~?",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024068.1.4",
          "form": "저는 스벅에서 먹을려구요",
          "original_form": "저는 스벅에서 먹을려구요",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024068.1.5",
          "form": "스벅에서 어떤 메뉴 드셨나요~,",
          "original_form": "스벅에서 어떤 메뉴 드셨나요~,",
          "speaker_id": "1",
          "time": "20210901 00:33"
        },
        {
          "id": "MDRW2100024068.1.6",
          "form": "카라멜마끼야또먹었어요",
          "original_form": "카라멜마끼야또먹었어요",
          "speaker_id": "2",
          "time": "20210901 05:58"
        },
        {
          "id": "MDRW2100024068.1.7",
          "form": "스타벅스에 바닐라더블샷? 그메뉴 맛나다던데 드셔보셨나용",
          "original_form": "스타벅스에 바닐라더블샷? 그메뉴 맛나다던데 드셔보셨나용",
          "speaker_id": "1",
          "time": "20210901 08:33"
        },
        {
          "id": "MDRW2100024068.1.8",
          "form": "먹어본것같아요",
          "original_form": "먹어본것같아요",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100024068.1.9",
          "form": "맛있으셨나요?",
          "original_form": "맛있으셨나요?",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100024068.1.10",
          "form": "네 괜찮았어요",
          "original_form": "네 괜찮았어요",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100024068.1.11",
          "form": "오늘  저녁은 뭐드시나요?",
          "original_form": "오늘  저녁은 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100024068.1.12",
          "form": "삼겹살먹고싶어요",
          "original_form": "삼겹살먹고싶어요",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100024068.1.13",
          "form": "후식으로배스킨먹으면 금상첨화",
          "original_form": "후식으로배스킨먹으면 금상첨화",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100024068.1.14",
          "form": "악 저 삼겹살 킬런데 ㅋㅋ",
          "original_form": "악 저 삼겹살 킬런데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100024068.1.15",
          "form": "외식하실건가요?~",
          "original_form": "외식하실건가요?~",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100024068.1.16",
          "form": "외식은 희망사항이에요ㅜㅜ",
          "original_form": "외식은 희망사항이에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100024068.1.17",
          "form": "머먹지....고기가땡기는데",
          "original_form": "머먹지....고기가땡기는데",
