      "utterance": [
        {
          "id": "MDRW2100007422.1.1",
          "form": "혹시 카페투어도 하시나요",
          "original_form": "혹시 카페투어도 하시나요",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007422.1.2",
          "form": "시간 날 때 가는 편이에요",
          "original_form": "시간 날 때 가는 편이에요",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007422.1.3",
          "form": "어디 카페좋아하세요?",
          "original_form": "어디 카페좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007422.1.4",
          "form": "개인카페 자주가시려나",
          "original_form": "개인카페 자주가시려나",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007422.1.5",
          "form": "어엇 네네 개인카페 자주가고",
          "original_form": "어엇 네네 개인카페 자주가고",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007422.1.6",
          "form": "저는 카페 뭔가 디저트 먹으러 가는 것 같아요",
          "original_form": "저는 카페 뭔가 디저트 먹으러 가는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007422.1.7",
          "form": "디저트면 어떤거요 케이크?",
          "original_form": "디저트면 어떤거요 케이크?",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.8",
          "form": "크로플이랑",
          "original_form": "크로플이랑",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.9",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.10",
          "form": "빵류들 먹으러 다뇨요",
          "original_form": "빵류들 먹으러 다뇨요",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.11",
          "form": "근데 그 뷰가 좋은 곳은",
          "original_form": "근데 그 뷰가 좋은 곳은",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.12",
          "form": "맛이없더라고요",
          "original_form": "맛이없더라고요",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.15",
          "form": "동탄에 한옥 유명한 카페 갔었는데",
          "original_form": "동탄에 한옥 유명한 카페 갔었는데",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.16",
          "form": "맛없더라고요",
          "original_form": "맛없더라고요",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.17",
          "form": "뭔가 음식보다 인테리어나 뷰에 초점을 맞춘느낌",
          "original_form": "뭔가 음식보다 인테리어나 뷰에 초점을 맞춘느낌",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.18",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.19",
          "form": "음식도 이쁘기만하고",
          "original_form": "음식도 이쁘기만하고",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.20",
          "form": "그런곳은 두 번안가게 되더라구요",
          "original_form": "그런곳은 두 번안가게 되더라구요",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007422.1.21",
          "form": "맞아요 사진 한 번이면 족하다",
          "original_form": "맞아요 사진 한 번이면 족하다",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007422.1.22",
          "form": "실속없는 편ㅎ..",
          "original_form": "실속없는 편ㅎ..",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007422.1.23",
          "form": "00다방 이렇게 되어있는 곳중에서",
          "original_form": "00다방 이렇게 되어있는 곳중에서",
          "speaker_id": "2",
          "time": "20210822 02:05"
        }
