      "utterance": [
        {
          "id": "MDRW2100018036.1.1",
          "form": "아니요 제주도 못가본것도 너무 오래됐어요 ㅠㅠ",
          "original_form": "아니요 제주도 못가본것도 너무 오래됐어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018036.1.2",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100018036.1.3",
          "form": "저는 원래 아침 잘 안챙겨먹는데 오늘은 먹었어요",
          "original_form": "저는 원래 아침 잘 안챙겨먹는데 오늘은 먹었어요",
          "speaker_id": "1",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100018036.1.4",
          "form": "넹넹 계란 두알 먹어요 매일 ㅋㅋ",
          "original_form": "넹넹 계란 두알 먹어요 매일 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100018036.1.5",
          "form": "오 오늘은 일찍 일어나셨나봐요",
          "original_form": "오 오늘은 일찍 일어나셨나봐요",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100018036.1.6",
          "form": "뭔가 잠이 잘 안오더라고요ㅎ",
          "original_form": "뭔가 잠이 잘 안오더라고요ㅎ",
          "speaker_id": "1",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100018036.1.7",
          "form": "삶은계란이요?",
          "original_form": "삶은계란이요?",
          "speaker_id": "1",
          "time": "20210831 09:28"
        },
        {
          "id": "MDRW2100018036.1.8",
          "form": "넹 삶은 계란!!",
          "original_form": "넹 삶은 계란!!",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100018036.1.9",
          "form": "뭐 드셨어요",
          "original_form": "뭐 드셨어요",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100018036.1.10",
          "form": "계란후라이 2개해서",
          "original_form": "계란후라이 2개해서",
          "speaker_id": "1",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100018036.1.11",
          "form": "버터간장밥 볶아먹었어요 ㅜㅜ꿀맛",
          "original_form": "버터간장밥 볶아먹었어요 ㅜㅜ꿀맛",
          "speaker_id": "1",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100018036.1.12",
          "form": "헐 진짜 맛있는거 드셨네요 대박",
          "original_form": "헐 진짜 맛있는거 드셨네요 대박",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100018036.1.13",
          "form": "오늘은 어제 먹다남은 찜닭이 있어서",
          "original_form": "오늘은 어제 먹다남은 찜닭이 있어서",
          "speaker_id": "1",
          "time": "20210901 06:15"
        },
        {
          "id": "MDRW2100018036.1.14",
          "form": "그거로 볶아먹으려구요ㅋㅋㅋㅋ",
          "original_form": "그거로 볶아먹으려구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 06:15"
        },
        {
          "id": "MDRW2100018036.1.15",
          "form": "오늘도 삶은계란 드시겠네요!",
          "original_form": "오늘도 삶은계란 드시겠네요!",
          "speaker_id": "1",
          "time": "20210901 06:16"
        },
        {
          "id": "MDRW2100018036.1.16",
          "form": "넹 몇개월짼데 별로 안 질리네요ㅋㅋㅋ",
          "original_form": "넹 몇개월짼데 별로 안 질리네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 06:33"
        },
        {
          "id": "MDRW2100018036.1.17",
          "form": "찜닭은 간장 베이스 드셨나용",
          "original_form": "찜닭은 간장 베이스 드셨나용",
          "speaker_id": "2",
          "time": "20210901 06:33"
        },
        {
          "id": "MDRW2100018036.1.18",
          "form": "네네 찜닭은 간장이 진리죠",
          "original_form": "네네 찜닭은 간장이 진리죠",
          "speaker_id": "1",
          "time": "20210901 06:34"
        },
        {
          "id": "MDRW2100018036.1.19",
          "form": "그냥 계란만 드세요?!",
          "original_form": "그냥 계란만 드세요?!",
          "speaker_id": "1",
          "time": "20210901 06:34"
        },
        {
          "id": "MDRW2100018036.1.20",
          "form": "넹넨 계란만 ㅋㅋㅋㅋ",
          "original_form": "넹넨 계란만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 06:39"
        },
        {
          "id": "MDRW2100018036.1.21",
          "form": "식단 조절하시는거예요?!",
          "original_form": "식단 조절하시는거예요?!",
          "speaker_id": "1",
          "time": "20210901 06:41"
        },
        {
          "id": "MDRW2100018036.1.22",
          "form": "ㅠㅠ계란은 반숙후라이해서 노른자 탁 흘려먹는게",
          "original_form": "ㅠㅠ계란은 반숙후라이해서 노른자 탁 흘려먹는게",
          "speaker_id": "1",
          "time": "20210901 06:41"
        },
        {
          "id": "MDRW2100018036.1.23",
          "form": "맛있는데ㅜㅜ",
          "original_form": "맛있는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 06:41"
        },
        {
          "id": "MDRW2100018036.1.24",
          "form": "아니요 아침엔 밥먹으면 뭔가 배가 아파서 ㅌㅋㅋ",
          "original_form": "아니요 아침엔 밥먹으면 뭔가 배가 아파서 ㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 06:41"
        }
      ]
