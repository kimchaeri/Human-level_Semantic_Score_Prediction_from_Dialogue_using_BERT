      "utterance": [
        {
          "id": "MDRW2100047717.1.1",
          "form": "오늘 청소 하시나요?",
          "original_form": "오늘 청소 하시나요?",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100047717.1.2",
          "form": "네 해야되요",
          "original_form": "네 해야되요",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100047717.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100047717.1.4",
          "form": "오늘 금요일이라 ㅎ",
          "original_form": "오늘 금요일이라 ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100047717.1.5",
          "form": "ㅎㅎ오늘 하고 내일은 늦게까지 쉬려구요",
          "original_form": "ㅎㅎ오늘 하고 내일은 늦게까지 쉬려구요",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100047717.1.6",
          "form": "저도 오늘하고 주말은..",
          "original_form": "저도 오늘하고 주말은..",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100047717.1.7",
          "form": "그냥 설거지 이런거만 하려고용",
          "original_form": "그냥 설거지 이런거만 하려고용",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100047717.1.8",
          "form": "주말까지 집안일에 치이면 못 살거 같아요ㅋㅋ",
          "original_form": "주말까지 집안일에 치이면 못 살거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100047717.1.9",
          "form": "ㅠ 주말이라도 쉬어야죠",
          "original_form": "ㅠ 주말이라도 쉬어야죠",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100047717.1.10",
          "form": "맞아요ㅠ 설거지도 덜 나오게 사먹어야겠어요",
          "original_form": "맞아요ㅠ 설거지도 덜 나오게 사먹어야겠어요",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100047717.1.11",
          "form": "ㅋㅋ ㅋㅋㅋㅋㅋㅋ 너무 하기 싫은게",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋㅋㅋ 너무 하기 싫은게",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100047717.1.12",
          "form": "보이네요!!!",
          "original_form": "보이네요!!!",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100047717.1.13",
          "form": "ㅋㅋㅋㅋㅋ덜 하고 살 수 있었으면 좋겠어요",
          "original_form": "ㅋㅋㅋㅋㅋ덜 하고 살 수 있었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100047717.1.14",
          "form": "ㅎ 로봇청소기 말고도",
          "original_form": "ㅎ 로봇청소기 말고도",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100047717.1.15",
          "form": "청소하는 기계가 나오면 좋겠어요",
          "original_form": "청소하는 기계가 나오면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100047717.1.16",
          "form": "맞아요 청소하는거 넘 귀찮아요",
          "original_form": "맞아요 청소하는거 넘 귀찮아요",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100047717.1.17",
          "form": "ㅋㅋㅎ 발명해줬으면요 ㅠ",
          "original_form": "ㅋㅋㅎ 발명해줬으면요 ㅠ",
