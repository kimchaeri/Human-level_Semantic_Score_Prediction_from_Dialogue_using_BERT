      "utterance": [
        {
          "id": "MDRW2100004196.1.1",
          "form": "요즘 날씨가 선선하니 좋네요",
          "original_form": "요즘 날씨가 선선하니 좋네요",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004196.1.2",
          "form": "네 저녁 아침 선풍기 없이 자고 좋아요",
          "original_form": "네 저녁 아침 선풍기 없이 자고 좋아요",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004196.1.3",
          "form": "역시 조상님이 나눈 절기가 맞아서 신기해요",
          "original_form": "역시 조상님이 나눈 절기가 맞아서 신기해요",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004196.1.4",
          "form": "저는 열이 많아서 아직은 미풍으로 켜둬요",
          "original_form": "저는 열이 많아서 아직은 미풍으로 켜둬요",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004196.1.5",
          "form": "그렇죠 근데 여름이 점점 습해져요",
          "original_form": "그렇죠 근데 여름이 점점 습해져요",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004196.1.6",
          "form": "저도 낮은 미풍요",
          "original_form": "저도 낮은 미풍요",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004196.1.7",
          "form": "네 맞아요 동남아처럼 습해지네요 스콜 현상도 있구요",
          "original_form": "네 맞아요 동남아처럼 습해지네요 스콜 현상도 있구요",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004196.1.8",
          "form": "네 그래서 하루종일 더운데 습도가 높았어요",
          "original_form": "네 그래서 하루종일 더운데 습도가 높았어요",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004196.1.9",
          "form": "습도 때문에 여름에 에어컨 풀가동했어요",
          "original_form": "습도 때문에 여름에 에어컨 풀가동했어요",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004196.1.10",
          "form": "제습대용으로요",
          "original_form": "제습대용으로요",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004196.1.11",
          "form": "전 추위 엄청타요",
          "original_form": "전 추위 엄청타요",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004196.1.12",
          "form": "벌써 겨울 무서워요",
          "original_form": "벌써 겨울 무서워요",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004196.1.13",
          "form": "이번에 전기세 꽤 나왔어요 날씨때문에 계속 트니깐요",
          "original_form": "이번에 전기세 꽤 나왔어요 날씨때문에 계속 트니깐요",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004196.1.14",
          "form": "이번 겨울도 엄청 추울것같아요",
          "original_form": "이번 겨울도 엄청 추울것같아요",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004196.1.15",
          "form": "안돼요 무서워요 전기세 폭탄",
          "original_form": "안돼요 무서워요 전기세 폭탄",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004196.1.16",
          "form": "겨울에 추워서 난방비 작년에 못돈 나갔네요",
          "original_form": "겨울에 추워서 난방비 작년에 못돈 나갔네요",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004196.1.17",
          "form": "저희집은 더위를 잘 타서 에어컨비 많이났네요",
          "original_form": "저희집은 더위를 잘 타서 에어컨비 많이났네요",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004196.1.18",
          "form": "저 아직 고지서 못받았어요 ㅜㅜ떨리네요",
          "original_form": "저 아직 고지서 못받았어요 ㅜㅜ떨리네요",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004196.1.19",
          "form": "겨울은 두꺼운 이불이면 되는데 여름은 감당 안되니깐요",
          "original_form": "겨울은 두꺼운 이불이면 되는데 여름은 감당 안되니깐요",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004196.1.20",
          "form": "그렇죠 무조건 에어컨 켜야 해요",
          "original_form": "그렇죠 무조건 에어컨 켜야 해요",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004196.1.21",
          "form": "겨울은 보일러비 ㄷㄷㄷ",
          "original_form": "겨울은 보일러비 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210821 15:36"
        }
      ]
