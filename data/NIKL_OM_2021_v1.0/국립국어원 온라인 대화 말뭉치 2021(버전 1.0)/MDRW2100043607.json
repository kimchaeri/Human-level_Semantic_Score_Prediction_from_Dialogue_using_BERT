          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043607.1.5",
          "form": "ㅜ아녀",
          "original_form": "ㅜ아녀",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043607.1.6",
          "form": "연예인좋아하는분있나요",
