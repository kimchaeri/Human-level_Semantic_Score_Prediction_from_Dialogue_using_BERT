        {
          "id": "MDRW2100005211.1.1",
          "form": "팀플 없었으면 좋겠다",
          "original_form": "팀플 없었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005211.1.2",
          "form": "제발",
          "original_form": "제발",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005211.1.3",
          "form": "사회악이야",
          "original_form": "사회악이야",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005211.1.4",
          "form": "원수 만나기 싫어",
          "original_form": "원수 만나기 싫어",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005211.1.5",
          "form": "절대 안 되지",
          "original_form": "절대 안 되지",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005211.1.6",
          "form": "공지도 빨리 주셨으면 좋겠다",
          "original_form": "공지도 빨리 주셨으면 좋겠다",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005211.1.7",
          "form": "융사는 팀플 있잖아",
          "original_form": "융사는 팀플 있잖아",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005211.1.8",
          "form": "최악이야",
          "original_form": "최악이야",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005211.1.9",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005211.1.10",
          "form": "오노~",
          "original_form": "오노~",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005211.1.11",
          "form": "하하하! 죽겠어요.",
          "original_form": "하하하! 죽겠어요.",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005211.1.12",
          "form": "우리 교수님은 따로 없었음",
          "original_form": "우리 교수님은 따로 없었음",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005211.1.13",
          "form": "오엥",
          "original_form": "오엥",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005211.1.14",
          "form": "어떤 교수님이었어?",
          "original_form": "어떤 교수님이었어?",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005211.1.15",
          "form": "name3교수님",
          "original_form": "&name3&교수님",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005211.1.16",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005211.1.17",
          "form": "부럼다",
          "original_form": "부럼다",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005211.1.18",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005211.1.19",
          "form": "마지막에 짝이랑 리포트 점검해주는 거 말고는 없었음",
