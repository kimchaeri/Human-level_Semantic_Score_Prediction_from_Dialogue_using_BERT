      "utterance": [
        {
          "id": "MDRW2100024292.1.1",
          "form": "아들이랑 자주놀러가요?",
          "original_form": "아들이랑 자주놀러가요?",
          "speaker_id": "2",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100024292.1.2",
          "form": "네네  ㅎㅎ",
          "original_form": "네네  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024292.1.3",
          "form": "둘이 잘다녀요",
          "original_form": "둘이 잘다녀요",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024292.1.4",
          "form": "어디어디가요?",
          "original_form": "어디어디가요?",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024292.1.5",
          "form": "코로나 전에는  예쁜 카페 다니는 ㅎㅎ 취미였는데",
          "original_form": "코로나 전에는  예쁜 카페 다니는 ㅎㅎ 취미였는데",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024292.1.6",
          "form": "추천좀해주세요ㅎㅎ",
          "original_form": "추천좀해주세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024292.1.7",
          "form": "아아ㅎㅎ 까페ㅎ 아이랑 대화가통하니깐ㅎ 잼겟다",
          "original_form": "아아ㅎㅎ 까페ㅎ 아이랑 대화가통하니깐ㅎ 잼겟다",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024292.1.8",
          "form": "보통 삽교호  나 제부도  가구요",
          "original_form": "보통 삽교호  나 제부도  가구요",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024292.1.9",
          "form": "카페는 북카페 잘가요",
          "original_form": "카페는 북카페 잘가요",
          "speaker_id": "1",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024292.1.10",
          "form": "맛있는메뉴도 먹고  책도 읽고",
          "original_form": "맛있는메뉴도 먹고  책도 읽고",
          "speaker_id": "1",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024292.1.11",
          "form": "자연 경관을 많이 보러가시나보네요ㅎㅎ",
          "original_form": "자연 경관을 많이 보러가시나보네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.12",
          "form": "도서관 데이트 많이 하구요 ㅎㅎ",
          "original_form": "도서관 데이트 많이 하구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.13",
          "form": "그도 그럴것이 코로나라ㅎ",
          "original_form": "그도 그럴것이 코로나라ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.14",
          "form": "우왐ㅎ 북까페 좋네요 ㅎㅎ",
          "original_form": "우왐ㅎ 북까페 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.15",
          "form": "도서관도ㅎㅎ",
          "original_form": "도서관도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.16",
          "form": "제부도는 ㅋㅋ 모래파러 가요 ㅋ",
          "original_form": "제부도는 ㅋㅋ 모래파러 가요 ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.17",
          "form": "앙아아 ㅎㅎㅎ",
          "original_form": "앙아아 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.18",
          "form": "삽교호는  갈매기 밥주러 ㅋ",
          "original_form": "삽교호는  갈매기 밥주러 ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.19",
          "form": "저희 애도 모래좋아하는데ㅠ",
          "original_form": "저희 애도 모래좋아하는데ㅠ",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024292.1.20",
          "form": "맨날 아파트에선 안된다고만하니ㅎ",
          "original_form": "맨날 아파트에선 안된다고만하니ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:20"
        },
        {
          "id": "MDRW2100024292.1.21",
          "form": "이번주말에는 모래놀이를 맘껏할수있는데로 데리고가야겟어요ㅎ",
          "original_form": "이번주말에는 모래놀이를 맘껏할수있는데로 데리고가야겟어요ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:20"
        }
      ]
