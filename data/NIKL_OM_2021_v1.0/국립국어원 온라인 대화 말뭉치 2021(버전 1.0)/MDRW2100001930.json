          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001930.1.5",
          "form": "샀어요!",
          "original_form": "샀어요!",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001930.1.6",
          "form": "우와 과자 뭐 사셨어요?",
          "original_form": "우와 과자 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001930.1.7",
          "form": "허니버터 아몬드랑 마가렛트 그리구 오예스",
          "original_form": "허니버터 아몬드랑 마가렛트 그리구 오예스",
          "speaker_id": "2",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001930.1.8",
          "form": "샀어요!!",
          "original_form": "샀어요!!",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001930.1.9",
          "form": "군것질 좋아해서 ㅎㅎ",
          "original_form": "군것질 좋아해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001930.1.10",
          "form": "허 허니버터 아몬드 맛있겠어요,,",
          "original_form": "허 허니버터 아몬드 맛있겠어요,,",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001930.1.11",
          "form": "그거 매력 넘치는 맛이에요...",
          "original_form": "그거 매력 넘치는 맛이에요...",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001930.1.12",
          "form": "아아 그러니까 먹 ㅡ고싶네요",
          "original_form": "아아 그러니까 먹 ㅡ고싶네요",
