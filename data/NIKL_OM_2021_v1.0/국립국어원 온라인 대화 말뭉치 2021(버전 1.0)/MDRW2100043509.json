          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043509.1.4",
          "form": "웅웅 ㅎㅎ 과자같은거보다 훨씬 낫지",
          "original_form": "웅웅 ㅎㅎ 과자같은거보다 훨씬 낫지",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043509.1.5",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043509.1.6",
          "form": "하나만 샀어용?",
          "original_form": "하나만 샀어용?",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043509.1.7",
          "form": "세일하나보네",
          "original_form": "세일하나보네",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043509.1.8",
          "form": "한 개만 샀다",
          "original_form": "한 개만 샀다",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
