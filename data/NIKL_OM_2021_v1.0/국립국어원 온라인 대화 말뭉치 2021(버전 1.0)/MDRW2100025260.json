      "utterance": [
        {
          "id": "MDRW2100025260.1.1",
          "form": "신전은 매워야 맛이 있는터인데",
          "original_form": "신전은 매워야 맛이 있는터인데",
          "speaker_id": "2",
          "time": "20210901 03:29"
        },
        {
          "id": "MDRW2100025260.1.2",
          "form": "네 3단계요 ㅋㅋ",
          "original_form": "네 3단계요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:29"
        },
        {
          "id": "MDRW2100025260.1.3",
          "form": "와 그건 그냥 고통받는느낌이던데 ㅋㅋㅋㅋ",
          "original_form": "와 그건 그냥 고통받는느낌이던데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:29"
        },
        {
          "id": "MDRW2100025260.1.4",
          "form": "근데 좀 지점마다 달라서",
          "original_form": "근데 좀 지점마다 달라서",
          "speaker_id": "2",
          "time": "20210901 03:30"
        },
        {
          "id": "MDRW2100025260.1.5",
          "form": "원래 살던데는 진짜 고통이었는데",
          "original_form": "원래 살던데는 진짜 고통이었는데",
          "speaker_id": "2",
          "time": "20210901 03:30"
        },
        {
          "id": "MDRW2100025260.1.6",
          "form": "이게 음식인가 고문인가 했어욬ㅋㅋ",
          "original_form": "이게 음식인가 고문인가 했어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:30"
        },
        {
          "id": "MDRW2100025260.1.7",
          "form": "지금은 2.5",
          "original_form": "지금은 2.5",
          "speaker_id": "2",
          "time": "20210901 03:30"
        },
        {
          "id": "MDRW2100025260.1.8",
          "form": "느낌이 ㅋㅋ",
          "original_form": "느낌이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:30"
        },
        {
          "id": "MDRW2100025260.1.9",
          "form": "ㅋㅋㅋㅋ 2·5하니까 마라탕 생각나요.... 왜죠",
          "original_form": "ㅋㅋㅋㅋ 2·5하니까 마라탕 생각나요.... 왜죠",
          "speaker_id": "1",
          "time": "20210901 03:30"
        },
        {
          "id": "MDRW2100025260.1.10",
          "form": "마라탕 드시나요",
          "original_form": "마라탕 드시나요",
          "speaker_id": "1",
          "time": "20210901 03:30"
        },
        {
          "id": "MDRW2100025260.1.11",
          "form": "아 저는 마라탕을 먹지 않습니다....",
          "original_form": "아 저는 마라탕을 먹지 않습니다....",
          "speaker_id": "2",
          "time": "20210901 03:31"
        },
        {
          "id": "MDRW2100025260.1.12",
          "form": "앗 이유가뭔가요",
          "original_form": "앗 이유가뭔가요",
          "speaker_id": "1",
          "time": "20210901 03:31"
        },
        {
          "id": "MDRW2100025260.1.13",
          "form": "더 이상 함께 할 수 없군요",
          "original_form": "더 이상 함께 할 수 없군요",
          "speaker_id": "2",
          "time": "20210901 03:31"
        },
        {
          "id": "MDRW2100025260.1.14",
          "form": "그 향이 싫어서 ㅋㅋㅋㅋ",
          "original_form": "그 향이 싫어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:31"
        },
        {
          "id": "MDRW2100025260.1.15",
          "form": "중국에 좋지않은 감정이 있으신가했어요",
          "original_form": "중국에 좋지않은 감정이 있으신가했어요",
          "speaker_id": "1",
          "time": "20210901 03:31"
        },
        {
          "id": "MDRW2100025260.1.16",
          "form": "다른건 괜찮은데 마라향이ㅋㅋ",
          "original_form": "다른건 괜찮은데 마라향이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:31"
        },
        {
          "id": "MDRW2100025260.1.17",
          "form": "고수도 못먹습니다",
          "original_form": "고수도 못먹습니다",
          "speaker_id": "2",
          "time": "20210901 03:31"
        },
        {
          "id": "MDRW2100025260.1.18",
          "form": "ㅋㅋㅋ 혹시 방아잎 아세요?",
          "original_form": "ㅋㅋㅋ 혹시 방아잎 아세요?",
          "speaker_id": "1",
          "time": "20210901 03:32"
        },
        {
          "id": "MDRW2100025260.1.19",
          "form": "그게 뭐죠",
          "original_form": "그게 뭐죠",
          "speaker_id": "2",
          "time": "20210901 03:32"
        },
        {
          "id": "MDRW2100025260.1.20",
          "form": "전 아랫지방 사는데 여기는 추어탕이나 그런거 할때 그 방아잎을 넣는데",
          "original_form": "전 아랫지방 사는데 여기는 추어탕이나 그런거 할때 그 방아잎을 넣는데",
          "speaker_id": "1",
          "time": "20210901 03:32"
        },
        {
          "id": "MDRW2100025260.1.21",
          "form": "향이 좀 고수처럼 세서 못먹는분많아요",
          "original_form": "향이 좀 고수처럼 세서 못먹는분많아요",
          "speaker_id": "1",
          "time": "20210901 03:32"
        },
        {
          "id": "MDRW2100025260.1.22",
          "form": "어우 향 센거 못먹어요",
          "original_form": "어우 향 센거 못먹어요",
          "speaker_id": "2",
          "time": "20210901 03:33"
        },
        {
          "id": "MDRW2100025260.1.23",
          "form": "ㅠ.ㅠ",
          "original_form": "ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210901 03:33"
        },
        {
          "id": "MDRW2100025260.1.24",
          "form": "그럼 산초가루도 못드시겠네요?",
          "original_form": "그럼 산초가루도 못드시겠네요?",
          "speaker_id": "1",
          "time": "20210901 03:33"
        }
      ]
    }
