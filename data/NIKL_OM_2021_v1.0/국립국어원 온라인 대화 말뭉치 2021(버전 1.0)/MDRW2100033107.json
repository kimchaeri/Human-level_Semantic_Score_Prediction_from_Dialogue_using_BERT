      "utterance": [
        {
          "id": "MDRW2100033107.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100033107.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033107.1.3",
          "form": "아 저 안그래도 폰바꾸려고 하는데 플립 참 예뻐요",
          "original_form": "아 저 안그래도 폰바꾸려고 하는데 플립 참 예뻐요",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033107.1.4",
          "form": "헉 플립으로 바꾸려구요?",
          "original_form": "헉 플립으로 바꾸려구요?",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033107.1.5",
          "form": "무슨 색으로요?",
          "original_form": "무슨 색으로요?",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033107.1.6",
          "form": "아뇨...아이폰으로 갈아탈 계획입니다",
          "original_form": "아뇨...아이폰으로 갈아탈 계획입니다",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033107.1.7",
          "form": "플립은 그 예약판매? 놓치니까 사기가 아깝더라구요",
          "original_form": "플립은 그 예약판매? 놓치니까 사기가 아깝더라구요",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033107.1.8",
          "form": "근데 그린이랑 라벤더가 너무 예뻐요",
          "original_form": "근데 그린이랑 라벤더가 너무 예뻐요",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033107.1.9",
          "form": "아 ㅋㅋㅋㅋㅋㅋ 그렇죠",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ 그렇죠",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033107.1.10",
          "form": "실물 보셨어요?",
          "original_form": "실물 보셨어요?",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033107.1.11",
          "form": "아니여 ㅋㅋㅋ 친구가 사진보내준거만 봣어요",
          "original_form": "아니여 ㅋㅋㅋ 친구가 사진보내준거만 봣어요",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100033107.1.12",
          "form": "근데 이번에 다른친구가 갤워치 그린사서 그린색은 같겠거니 하고 잇거든요",
          "original_form": "근데 이번에 다른친구가 갤워치 그린사서 그린색은 같겠거니 하고 잇거든요",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100033107.1.13",
          "form": "제가 좋아하는 고급진 그린이라 ㅠㅠ 넘예뻐요",
          "original_form": "제가 좋아하는 고급진 그린이라 ㅠㅠ 넘예뻐요",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100033107.1.14",
          "form": "아 저도 그 그린 좋아해요",
          "original_form": "아 저도 그 그린 좋아해요",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100033107.1.15",
          "form": "색상이 진짜 예뻐오",
          "original_form": "색상이 진짜 예뻐오",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100033107.1.16",
          "form": "실물이 어떨지는 모르겠지만ㅋㅋㅋㅋㅋㅜ",
          "original_form": "실물이 어떨지는 모르겠지만ㅋㅋㅋㅋㅋㅜ",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100033107.1.17",
          "form": "하태하태",
          "original_form": "하태하태",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033107.1.18",
          "form": "폰 바꾸실 계획은 없으세요?",
          "original_form": "폰 바꾸실 계획은 없으세요?",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033107.1.19",
          "form": "저 바꾸고싶은데",
          "original_form": "저 바꾸고싶은데",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033107.1.20",
          "form": "최근에 바꾼거라서요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "최근에 바꾼거라서요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033107.1.21",
          "form": "아이고 ㅋㅋㅋㅋ 뭘로 바꾸셨어요?",
          "original_form": "아이고 ㅋㅋㅋㅋ 뭘로 바꾸셨어요?",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100033107.1.22",
          "form": "저 갤럭시s21 이건가요?",
          "original_form": "저 갤럭시s21 이건가요?",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100033107.1.23",
          "form": "몇달전에 바꿨어요ㅋㅋㅋㅋㅋ",
          "original_form": "몇달전에 바꿨어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100033107.1.24",
          "form": "오! 그거 요즘 가격 많이 떨어졌던데",
          "original_form": "오! 그거 요즘 가격 많이 떨어졌던데",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033107.1.25",
          "form": "어때요? 좋아요? 새폰이즈 뭔들이긴 하지만",
          "original_form": "어때요? 좋아요? 새폰이즈 뭔들이긴 하지만",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033107.1.26",
          "form": "무슨색이에요?",
          "original_form": "무슨색이에요?",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033107.1.27",
