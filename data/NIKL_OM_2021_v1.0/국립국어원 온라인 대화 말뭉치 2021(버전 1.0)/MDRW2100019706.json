      "utterance": [
        {
          "id": "MDRW2100019706.1.1",
          "form": "음악 듣는거 좋아해요?",
          "original_form": "음악 듣는거 좋아해요?",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019706.1.2",
          "form": "네네 유튜브 추천음악으로 ㅋㅋㅋ",
          "original_form": "네네 유튜브 추천음악으로 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019706.1.3",
          "form": "오 그런게 있어요?",
          "original_form": "오 그런게 있어요?",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100019706.1.4",
          "form": "막 중간에 갑자기 광고나오고 그러진 않아요..?",
          "original_form": "막 중간에 갑자기 광고나오고 그러진 않아요..?",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100019706.1.5",
          "form": "사실 유튜브 프리미엄이라서",
          "original_form": "사실 유튜브 프리미엄이라서",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019706.1.6",
          "form": "인공지능 스피커 주더라구요",
          "original_form": "인공지능 스피커 주더라구요",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019706.1.7",
          "form": "그래서 구글 노래틀어줘~하면 암거나 틀어줘요",
          "original_form": "그래서 구글 노래틀어줘~하면 암거나 틀어줘요",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019706.1.8",
          "form": "헐 프리미엄 구독하면 스피커를 줘요?",
          "original_form": "헐 프리미엄 구독하면 스피커를 줘요?",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019706.1.9",
          "form": "그것도 인공지능 스피커를요?",
          "original_form": "그것도 인공지능 스피커를요?",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019706.1.10",
          "form": "네 언제였지? 갑자기 프리미엄 구독자들은 신청하면 무료로 스피커 배송해준대서",
          "original_form": "네 언제였지? 갑자기 프리미엄 구독자들은 신청하면 무료로 스피커 배송해준대서",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019706.1.11",
          "form": "역시 대기업은 다르다 하며 감탄했어요",
          "original_form": "역시 대기업은 다르다 하며 감탄했어요",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019706.1.12",
          "form": "한시적인 이벤트였어요",
          "original_form": "한시적인 이벤트였어요",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019706.1.13",
          "form": "헐 ㅜㅜ 전 왜 전혀 몰랐죠... 아깝다.",
          "original_form": "헐 ㅜㅜ 전 왜 전혀 몰랐죠... 아깝다.",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019706.1.14",
          "form": "그럼 노래는 유튜브에서만 들어요?ㅎㅎ",
          "original_form": "그럼 노래는 유튜브에서만 들어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019706.1.15",
          "form": "네ㅋㅋㅋ 근데 유튜브러 음악듣는게 멜론 이런데보다는 불편해요",
          "original_form": "네ㅋㅋㅋ 근데 유튜브러 음악듣는게 멜론 이런데보다는 불편해요",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019706.1.16",
          "form": "가사 보고싶은데 가사 하나도 안뜨고",
          "original_form": "가사 보고싶은데 가사 하나도 안뜨고",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019706.1.17",
          "form": "월드탑텐 이런거 틀면 대부분 인도음악이ㅋㅋㅋㅋㅋ",
          "original_form": "월드탑텐 이런거 틀면 대부분 인도음악이ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019706.1.18",
          "form": "아 인도음악 ㅋㅋㅋ진짜 웃기네요",
          "original_form": "아 인도음악 ㅋㅋㅋ진짜 웃기네요",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019706.1.19",
          "form": "인도가 인구가 많아서 탑텐에 뜨나봐욬ㅋㅋㅋㅋ",
          "original_form": "인도가 인구가 많아서 탑텐에 뜨나봐욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019706.1.20",
          "form": "네 진짜 드라이브한다고 차에서 틀면",
          "original_form": "네 진짜 드라이브한다고 차에서 틀면",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019706.1.21",
          "form": "인도음악이 울려퍼져서",
          "original_form": "인도음악이 울려퍼져서",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019706.1.22",
          "form": "뭔가 원하는 분위기가 아니고ㅋㅋㅋ",
          "original_form": "뭔가 원하는 분위기가 아니고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019706.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ유튜브에 음악채널이 따로 있는거에요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ유튜브에 음악채널이 따로 있는거에요?",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019706.1.24",
          "form": "아니면 유튜버가 따로 올린 영상 트는거에요?",
          "original_form": "아니면 유튜버가 따로 올린 영상 트는거에요?",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019706.1.25",
          "form": "유튜브 뮤직 서비스가 있어요",
          "original_form": "유튜브 뮤직 서비스가 있어요",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019706.1.26",
          "form": "그런것도 프리미엄 서비스 안에 포함이에요?",
          "original_form": "그런것도 프리미엄 서비스 안에 포함이에요?",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019706.1.27",
          "form": "구글이 컨텐츠 사업 하려고 구글 뮤직같은 음원 서비스 만들었는데 구냥 유튜브 컨텐츠로 연계한거 같어요",
          "original_form": "구글이 컨텐츠 사업 하려고 구글 뮤직같은 음원 서비스 만들었는데 구냥 유튜브 컨텐츠로 연계한거 같어요",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019706.1.28",
          "form": "그거 진짜 잘한 선택인 것 같아요 ㅎㅎ",
          "original_form": "그거 진짜 잘한 선택인 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:09"
        }
      ]
