      "utterance": [
        {
          "id": "MDRW2100047738.1.1",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100047738.1.2",
          "form": "저 게임 거의 안하는데 삘 받을때마다 다운받아서 해요",
          "original_form": "저 게임 거의 안하는데 삘 받을때마다 다운받아서 해요",
          "speaker_id": "2",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100047738.1.3",
          "form": "주로 어떤 게임 하세요?",
          "original_form": "주로 어떤 게임 하세요?",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100047738.1.4",
          "form": "핸드폰 게임만 해요",
          "original_form": "핸드폰 게임만 해요",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100047738.1.5",
          "form": "탈출게임은 여러가지 하고, 마피아 42도 좋아해요",
          "original_form": "탈출게임은 여러가지 하고, 마피아 42도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100047738.1.6",
          "form": "가끔 하기에는 핸드폰 게임이 좋은 거 같아요",
          "original_form": "가끔 하기에는 핸드폰 게임이 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100047738.1.7",
          "form": "아 저도 탈출 게임 좋아해요!",
          "original_form": "아 저도 탈출 게임 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100047738.1.8",
          "form": "탈출게임 정말 재밌어요",
          "original_form": "탈출게임 정말 재밌어요",
          "speaker_id": "2",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100047738.1.9",
          "form": "싸우는 게임 같은건 실력이 안돼서 못하는데 탈출게임은 시간 많이 쓰면서 할 수 있고요 ㅎㅎ",
          "original_form": "싸우는 게임 같은건 실력이 안돼서 못하는데 탈출게임은 시간 많이 쓰면서 할 수 있고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100047738.1.10",
          "form": "맞아요! 이것저것 퍼즐 풀고 추리하는게 은근 재밌어요~",
          "original_form": "맞아요! 이것저것 퍼즐 풀고 추리하는게 은근 재밌어요~",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100047738.1.11",
          "form": "마피아 42 좋아하시면 어몽어스도 해보셨나요?",
          "original_form": "마피아 42 좋아하시면 어몽어스도 해보셨나요?",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100047738.1.12",
          "form": "어몽어스 인기있을때 해봤는데 스피드있게 조작해야하는게 너무 어렵더라고요",
          "original_form": "어몽어스 인기있을때 해봤는데 스피드있게 조작해야하는게 너무 어렵더라고요",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100047738.1.13",
          "form": "맞아요ㅠㅠ 저도 어몽어스보다는 마피아42가 더 재밌었어요",
          "original_form": "맞아요ㅠㅠ 저도 어몽어스보다는 마피아42가 더 재밌었어요",
          "speaker_id": "1",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100047738.1.14",
          "form": "방탈출 카페 가본 적 있으신가요?",
          "original_form": "방탈출 카페 가본 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100047738.1.15",
          "form": "방탈출카페 꼭 가보고 싶어요!!! 제가 사는덴 없거든요",
          "original_form": "방탈출카페 꼭 가보고 싶어요!!! 제가 사는덴 없거든요",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100047738.1.16",
          "form": "시외버스타고 나가야 갈 수 있는곳이에요 ㅋㅋ",
          "original_form": "시외버스타고 나가야 갈 수 있는곳이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100047738.1.17",
          "form": "저도 아직 한번도 안 해봤는데 재밌을 거 같아요",
          "original_form": "저도 아직 한번도 안 해봤는데 재밌을 거 같아요",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100047738.1.18",
          "form": "맞아요 해본다면 공포 도전하고 싶네요",
          "original_form": "맞아요 해본다면 공포 도전하고 싶네요",
          "speaker_id": "2",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100047738.1.19",
          "form": "공포 테마! 너무 재밌을 거 같아요",
          "original_form": "공포 테마! 너무 재밌을 거 같아요",
          "speaker_id": "1",
          "time": "20210827 11:57"
        }
      ]
