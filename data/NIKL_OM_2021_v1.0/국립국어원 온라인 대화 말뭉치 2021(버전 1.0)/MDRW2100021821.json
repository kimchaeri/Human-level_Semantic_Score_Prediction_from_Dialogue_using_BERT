      "utterance": [
        {
          "id": "MDRW2100021821.1.1",
          "form": "좋지 ㅎㅎ 근데 라이스페이퍼랑 소스가 없는데?",
          "original_form": "좋지 ㅎㅎ 근데 라이스페이퍼랑 소스가 없는데?",
          "speaker_id": "2",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100021821.1.2",
          "form": "사면 되지 뭐 ㅎㅎ",
          "original_form": "사면 되지 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100021821.1.3",
          "form": "들어가는 재료야 집에 있는 채소 쓰면 되니까",
          "original_form": "들어가는 재료야 집에 있는 채소 쓰면 되니까",
          "speaker_id": "2",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100021821.1.4",
          "form": "참깨소스랑 스윗칠리소스 슈퍼 가면 파니까 그거 쓰공",
          "original_form": "참깨소스랑 스윗칠리소스 슈퍼 가면 파니까 그거 쓰공",
          "speaker_id": "1",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100021821.1.5",
          "form": "ㅎㅎ 그러면 되겠다",
          "original_form": "ㅎㅎ 그러면 되겠다",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100021821.1.6",
          "form": "특별한 재료 없어도 할 수 있으니 좋은 것 같아",
          "original_form": "특별한 재료 없어도 할 수 있으니 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100021821.1.7",
          "form": "양파 당근 파프리카 깻잎 양배추 정도만 넣어도 맛있을거야",
          "original_form": "양파 당근 파프리카 깻잎 양배추 정도만 넣어도 맛있을거야",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021821.1.8",
          "form": "집에 맛살도 있으니 좀 썰어서 넣어야겠다",
          "original_form": "집에 맛살도 있으니 좀 썰어서 넣어야겠다",
          "speaker_id": "1",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021821.1.9",
          "form": "오리고기도 좀 넣고 ㅎㅎ",
          "original_form": "오리고기도 좀 넣고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021821.1.10",
          "form": "얘기하다보니 엄청 많이 들어가네 ㅋ",
          "original_form": "얘기하다보니 엄청 많이 들어가네 ㅋ",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021821.1.11",
          "form": "건강에 너무 좋겠네",
          "original_form": "건강에 너무 좋겠네",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021821.1.12",
          "form": "싸먹는 재미도 있고",
          "original_form": "싸먹는 재미도 있고",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021821.1.13",
          "form": "애들도 좋아하겠다 ㅎㅎ",
          "original_form": "애들도 좋아하겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021821.1.14",
          "form": "라이스페이퍼 자체를 신기해할듯",
          "original_form": "라이스페이퍼 자체를 신기해할듯",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021821.1.15",
          "form": "아침에 샐러드 먹으니까 속이 가볍다",
          "original_form": "아침에 샐러드 먹으니까 속이 가볍다",
