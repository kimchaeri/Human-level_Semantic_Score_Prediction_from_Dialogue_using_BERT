      "utterance": [
        {
          "id": "MDRW2100007145.1.1",
          "form": "있지 술을 알코올 알러지 때문에 못먹는거면",
          "original_form": "있지 술을 알코올 알러지 때문에 못먹는거면",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007145.1.2",
          "form": "조리과정에서 알코올 날리는 음식도 못먹는거야?",
          "original_form": "조리과정에서 알코올 날리는 음식도 못먹는거야?",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007145.1.3",
          "form": "바지락 술찜같은거말야",
          "original_form": "바지락 술찜같은거말야",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007145.1.4",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007145.1.5",
          "form": "먹을수있지않을까 싶긴한데",
          "original_form": "먹을수있지않을까 싶긴한데",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007145.1.6",
          "form": "굳이 그런 모험을",
          "original_form": "굳이 그런 모험을",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007145.1.7",
          "form": "안해봐서 모르겠어요",
          "original_form": "안해봐서 모르겠어요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007145.1.8",
          "form": "알콜 다 날린거면 상관없지않을까요??",
          "original_form": "알콜 다 날린거면 상관없지않을까요??",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007145.1.9",
          "form": "알콜이 문제인거니까...",
          "original_form": "알콜이 문제인거니까...",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007145.1.10",
          "form": "음 그러면 미림같은 조미료도 조심해야겠네",
          "original_form": "음 그러면 미림같은 조미료도 조심해야겠네",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007145.1.11",
          "form": "요즘 레시피에는 많이들 들어가는데",
          "original_form": "요즘 레시피에는 많이들 들어가는데",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.12",
          "form": "그것도 술이니까",
          "original_form": "그것도 술이니까",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.13",
          "form": "잉 그래요?",
          "original_form": "잉 그래요?",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.14",
          "form": "근데 그런건 상관없을걸요 ...?",
          "original_form": "근데 그런건 상관없을걸요 ...?",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.15",
          "form": "응응 미림는 일본주야",
          "original_form": "응응 미림는 일본주야",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.16",
          "form": "그 왜 와인이 들어갔다는",
          "original_form": "그 왜 와인이 들어갔다는",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.17",
          "form": "스테이크 소스 이런거엔",
          "original_form": "스테이크 소스 이런거엔",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.18",
          "form": "상관없었어요 가열된 그런거",
          "original_form": "상관없었어요 가열된 그런거",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.19",
          "form": "미성년자들도 먹을수있게",
          "original_form": "미성년자들도 먹을수있게",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.20",
          "form": "알콜 다날아간거면",
          "original_form": "알콜 다날아간거면",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.21",
          "form": "먹을수있을듯",
          "original_form": "먹을수있을듯",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007145.1.22",
          "form": "그런건 가열중에 증발하니까 괜찮을텐데",
          "original_form": "그런건 가열중에 증발하니까 괜찮을텐데",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007145.1.23",
          "form": "생으로 먹는 음식 있으면 조심해야겠다",
          "original_form": "생으로 먹는 음식 있으면 조심해야겠다",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007145.1.24",
          "form": "미림 요즘 별별곳에 다 들어가니까",
          "original_form": "미림 요즘 별별곳에 다 들어가니까",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007145.1.25",
          "form": "어...진짜요??",
          "original_form": "어...진짜요??",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007145.1.26",
          "form": "그런거 생각안하고 살아왔는데",
          "original_form": "그런거 생각안하고 살아왔는데",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007145.1.27",
          "form": "근데 알코올성분 안날린거면",
          "original_form": "근데 알코올성분 안날린거면",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007145.1.28",
          "form": "신분증 봐야되는거 아녜요..?",
          "original_form": "신분증 봐야되는거 아녜요..?",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007145.1.29",
          "form": "미성년자도 그거 먹을 수 있나요...?",
          "original_form": "미성년자도 그거 먹을 수 있나요...?",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007145.1.30",
          "form": "찜찜",
          "original_form": "찜찜",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007145.1.31",
          "form": "사는거 자체에는 신분증이 필요하다나봐",
          "original_form": "사는거 자체에는 신분증이 필요하다나봐",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007145.1.32",
          "form": "낮아도 알코올 도수 14%는 되나본데",
          "original_form": "낮아도 알코올 도수 14%는 되나본데",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007145.1.33",
          "form": "아 그럼 됐어요",
          "original_form": "아 그럼 됐어요",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.34",
          "form": "저 신분증요구하는 음식은",
          "original_form": "저 신분증요구하는 음식은",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.35",
          "form": "먹은적없어서",
          "original_form": "먹은적없어서",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그렇게 해주면 거를 수 있어서 다행이죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그렇게 해주면 거를 수 있어서 다행이죠",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.37",
          "form": "푹 익혀나오는 음식이면 괜찮긴 하겠다",
          "original_form": "푹 익혀나오는 음식이면 괜찮긴 하겠다",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.38",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.39",
          "form": "알러지 있으면 많이 불편하지",
          "original_form": "알러지 있으면 많이 불편하지",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.40",
          "form": "복숭아철에 복숭아 못먹는",
          "original_form": "복숭아철에 복숭아 못먹는",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.41",
          "form": "인간이 말함",
          "original_form": "인간이 말함",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.42",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007145.1.43",
          "form": "복숭아 맛있는데",
          "original_form": "복숭아 맛있는데",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007145.1.44",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007145.1.45",
          "form": "과육 자체는 괜찮은데 솜털만 닿으면",
          "original_form": "과육 자체는 괜찮은데 솜털만 닿으면",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007145.1.46",
          "form": "아 과육이 괜찮은거면..",
          "original_form": "아 과육이 괜찮은거면..",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007145.1.47",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007145.1.48",
          "form": "가렵고 따가워서 안먹어",
          "original_form": "가렵고 따가워서 안먹어",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007145.1.49",
          "form": "전 복숭아 껍질째 먹는데",
          "original_form": "전 복숭아 껍질째 먹는데",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007145.1.50",
          "form": "또르르...",
          "original_form": "또르르...",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007145.1.51",
          "form": "먹는거에 알러지있는게",
          "original_form": "먹는거에 알러지있는게",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007145.1.52",
          "form": "제일 싫어요 ㅠㅠ",
          "original_form": "제일 싫어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007145.1.53",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007145.1.54",
          "form": "내 친구중에는 계란 알러지가 있는 친구도 있는데",
          "original_form": "내 친구중에는 계란 알러지가 있는 친구도 있는데",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007145.1.55",
          "form": "먹을 수 있는 음식이 절반으로 줄어들더라",
          "original_form": "먹을 수 있는 음식이 절반으로 줄어들더라",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007145.1.56",
          "form": "계란알러지는",
          "original_form": "계란알러지는",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007145.1.57",
          "form": "특히....",
          "original_form": "특히....",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007145.1.58",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007145.1.59",
          "form": "다른건 참아도",
          "original_form": "다른건 참아도",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007145.1.60",
          "form": "먹는거에 제한이 생기면",
          "original_form": "먹는거에 제한이 생기면",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
