      "utterance": [
        {
          "id": "MDRW2100041502.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 07:53"
        },
        {
          "id": "MDRW2100041502.1.2",
          "form": "자주 보러가는 편이에요",
          "original_form": "자주 보러가는 편이에요",
          "speaker_id": "2",
          "time": "20210906 07:54"
        },
        {
          "id": "MDRW2100041502.1.3",
          "form": "아 극장에 자주 보러 가세요? 그럼 혹시 가장 최근에 보신 영화 제목 물어봐도 될까요?",
          "original_form": "아 극장에 자주 보러 가세요? 그럼 혹시 가장 최근에 보신 영화 제목 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210906 07:54"
        },
        {
          "id": "MDRW2100041502.1.4",
          "form": "최근에 3주 연속으로 봤어요ㅋㅋㅋ",
          "original_form": "최근에 3주 연속으로 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 08:03"
        },
        {
          "id": "MDRW2100041502.1.5",
          "form": "싱크홀 인질 올드요",
          "original_form": "싱크홀 인질 올드요",
          "speaker_id": "2",
          "time": "20210906 08:03"
        },
        {
          "id": "MDRW2100041502.1.6",
          "form": "와 다 진짜 보고 싶었던 건데 어떠셨어요??",
          "original_form": "와 다 진짜 보고 싶었던 건데 어떠셨어요??",
          "speaker_id": "1",
          "time": "20210906 08:04"
        },
        {
          "id": "MDRW2100041502.1.7",
          "form": "개인적으로는 싱크홀이 제일 나았어요",
          "original_form": "개인적으로는 싱크홀이 제일 나았어요",
          "speaker_id": "2",
          "time": "20210906 08:05"
        },
        {
          "id": "MDRW2100041502.1.8",
          "form": "코미디를 좋아해서요",
          "original_form": "코미디를 좋아해서요",
          "speaker_id": "2",
          "time": "20210906 08:05"
        },
        {
          "id": "MDRW2100041502.1.9",
          "form": "아 싱크홀 코미디 요소가 강한가봐요 재난 영화인 줄 알았는데",
          "original_form": "아 싱크홀 코미디 요소가 강한가봐요 재난 영화인 줄 알았는데",
          "speaker_id": "1",
          "time": "20210906 08:05"
        },
        {
          "id": "MDRW2100041502.1.10",
          "form": "재난이긴 한데 베이스로 코미디를 깔고 가요!",
          "original_form": "재난이긴 한데 베이스로 코미디를 깔고 가요!",
          "speaker_id": "2",
          "time": "20210906 08:06"
        },
        {
          "id": "MDRW2100041502.1.11",
          "form": "헉 재밌겠다",
          "original_form": "헉 재밌겠다",
          "speaker_id": "1",
          "time": "20210906 08:07"
        },
        {
          "id": "MDRW2100041502.1.12",
          "form": "뮤지컬이나 연극은 종종 보세요?",
          "original_form": "뮤지컬이나 연극은 종종 보세요?",
          "speaker_id": "1",
          "time": "20210906 08:07"
        },
        {
          "id": "MDRW2100041502.1.13",
          "form": "돈내고 본 적은 없어요ㅎㅎ 이제 슬슬 한번 보고 싶네요",
          "original_form": "돈내고 본 적은 없어요ㅎㅎ 이제 슬슬 한번 보고 싶네요",
          "speaker_id": "2",
          "time": "20210906 08:07"
        },
        {
          "id": "MDRW2100041502.1.14",
          "form": "다람님은요?",
          "original_form": "다람님은요?",
          "speaker_id": "2",
          "time": "20210906 08:07"
        },
        {
          "id": "MDRW2100041502.1.15",
          "form": "저는 연극 한 번 본 적 있는데 인형의 집이었던 것 같아요 제목이",
          "original_form": "저는 연극 한 번 본 적 있는데 인형의 집이었던 것 같아요 제목이",
          "speaker_id": "1",
          "time": "20210906 08:08"
        },
        {
          "id": "MDRW2100041502.1.16",
          "form": "오, 그 유명한 소설인가요",
          "original_form": "오, 그 유명한 소설인가요",
          "speaker_id": "2",
          "time": "20210906 08:09"
        },
        {
          "id": "MDRW2100041502.1.17",
          "form": "아 아시는구나 네네 친구랑 같이 보러 갔는데 좋더라구요",
          "original_form": "아 아시는구나 네네 친구랑 같이 보러 갔는데 좋더라구요",
          "speaker_id": "1",
          "time": "20210906 08:10"
        },
        {
          "id": "MDRW2100041502.1.18",
          "form": "소극장이었나요?",
          "original_form": "소극장이었나요?",
          "speaker_id": "2",
          "time": "20210906 08:10"
        },
        {
          "id": "MDRW2100041502.1.19",
          "form": "네 소극장이었던 것 같아요 사실 코로나 전에 봤었던 거라 기억은 잘 안 나지만",
          "original_form": "네 소극장이었던 것 같아요 사실 코로나 전에 봤었던 거라 기억은 잘 안 나지만",
          "speaker_id": "1",
          "time": "20210906 08:13"
        },
        {
          "id": "MDRW2100041502.1.20",
          "form": "아하, 좀 지난 얘기군요",
          "original_form": "아하, 좀 지난 얘기군요",
          "speaker_id": "2",
          "time": "20210906 08:14"
        },
        {
          "id": "MDRW2100041502.1.21",
          "form": "옙",
          "original_form": "옙",
          "speaker_id": "2",
          "time": "20210906 08:14"
        }
      ]
    }
