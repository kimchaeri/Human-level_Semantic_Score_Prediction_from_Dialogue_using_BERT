      "utterance": [
        {
          "id": "MDRW2100007253.1.1",
          "form": "안녕허세요",
          "original_form": "안녕허세요",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007253.1.2",
          "form": "안녕하세요 :)",
          "original_form": "안녕하세요 :)",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007253.1.3",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007253.1.4",
          "form": "그냥 그래요 ㅎㅎ",
          "original_form": "그냥 그래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007253.1.5",
          "form": "중고거래 재밌어요",
          "original_form": "중고거래 재밌어요",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007253.1.6",
          "form": "오 당근마켓? 중고나라?",
          "original_form": "오 당근마켓? 중고나라?",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007253.1.7",
          "form": "당근이요ㅎㅎ",
          "original_form": "당근이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007253.1.8",
          "form": "중고는 좀 무서워요",
          "original_form": "중고는 좀 무서워요",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007253.1.9",
          "form": "맞아요 정신차리면 통장 털려있을 것 같아요",
          "original_form": "맞아요 정신차리면 통장 털려있을 것 같아요",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007253.1.10",
          "form": "최근에 어떤거 구입하셨어요?",
          "original_form": "최근에 어떤거 구입하셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007253.1.11",
          "form": "아 팔았습니다",
          "original_form": "아 팔았습니다",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007253.1.12",
          "form": "전자렌지!!!!",
          "original_form": "전자렌지!!!!",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007253.1.13",
          "form": "우와!",
          "original_form": "우와!",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007253.1.14",
          "form": "전 사보기만 했었어서!!ㅋㅋㅋ 실제로 만나서 거래하셨어요?",
          "original_form": "전 사보기만 했었어서!!ㅋㅋㅋ 실제로 만나서 거래하셨어요?",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007253.1.15",
          "form": "네 만나서 드리고 돈 받았습니다ㅋㅋ 뭐 사셨어요?",
          "original_form": "네 만나서 드리고 돈 받았습니다ㅋㅋ 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007253.1.16",
          "form": "저는 소소하게 절판된 해리포터 책을...ㅎㅎ",
          "original_form": "저는 소소하게 절판된 해리포터 책을...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007253.1.17",
          "form": "지금은 플스를 건져볼까 기웃기웃...",
          "original_form": "지금은 플스를 건져볼까 기웃기웃...",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007253.1.18",
          "form": "오 절판된거면 가격이 꽤 나갈거 같은데",
          "original_form": "오 절판된거면 가격이 꽤 나갈거 같은데",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007253.1.19",
          "form": "근데 워낙에 많이 팔린 책이라 원가보다는 훨씬 저렴해요",
          "original_form": "근데 워낙에 많이 팔린 책이라 원가보다는 훨씬 저렴해요",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007253.1.20",
          "form": "아 그런가요? 그러면 아깝네요",
          "original_form": "아 그런가요? 그러면 아깝네요",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007253.1.21",
          "form": "그래도 득템해서 행복했어요 저도 갈무리해서 한번 팔아봐야겠어요ㅎㅎ",
          "original_form": "그래도 득템해서 행복했어요 저도 갈무리해서 한번 팔아봐야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:21"
        }
      ]
