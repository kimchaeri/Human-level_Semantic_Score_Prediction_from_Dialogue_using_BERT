      "utterance": [
        {
          "id": "MDRW2100037388.1.1",
          "form": "생일에 먹고 싶은 음식 뭐가 있으세요?",
          "original_form": "생일에 먹고 싶은 음식 뭐가 있으세요?",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037388.1.2",
          "form": "저는 엄마 집밥이요 ㅎㅎ",
          "original_form": "저는 엄마 집밥이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037388.1.3",
          "form": "부모님이랑 같이 사시나요?",
          "original_form": "부모님이랑 같이 사시나요?",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037388.1.4",
          "form": "아뇨 못살아서 더글츄",
          "original_form": "아뇨 못살아서 더글츄",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037388.1.5",
          "form": "사먹는 음식은 정성없음이 티나는 거 같아요",
          "original_form": "사먹는 음식은 정성없음이 티나는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037388.1.6",
          "form": "어쩔수없는거같기도요...",
          "original_form": "어쩔수없는거같기도요...",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037388.1.7",
          "form": "코로나때문에 ㅜㅜ",
          "original_form": "코로나때문에 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037388.1.8",
          "form": "밀키트로 음식 만들어 보셨어요?",
          "original_form": "밀키트로 음식 만들어 보셨어요?",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037388.1.9",
          "form": "네 요즘은 잘나오더라구요",
          "original_form": "네 요즘은 잘나오더라구요",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037388.1.10",
          "form": "요리하려면 번거로운데 사먹기는 비싸고 혼자 가면 음식점에서 구박 하니까",
          "original_form": "요리하려면 번거로운데 사먹기는 비싸고 혼자 가면 음식점에서 구박 하니까",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037388.1.11",
          "form": "밀키트가 괜찮은 선택지 같더라고요",
          "original_form": "밀키트가 괜찮은 선택지 같더라고요",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037388.1.12",
          "form": "딱 좋아요 ㅎㅎ",
          "original_form": "딱 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037388.1.13",
          "form": "그리고 채소를  한꺼번에 사서 버렷어요 많이",
          "original_form": "그리고 채소를  한꺼번에 사서 버렷어요 많이",
          "speaker_id": "1",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100037388.1.14",
          "form": "다못먹고 물르구",
          "original_form": "다못먹고 물르구",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100037388.1.15",
          "form": "밀키트는 그걸 잔잔하게 썰어서 나눠 주니까 좋아요",
          "original_form": "밀키트는 그걸 잔잔하게 썰어서 나눠 주니까 좋아요",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037388.1.16",
          "form": "빙고뇨",
