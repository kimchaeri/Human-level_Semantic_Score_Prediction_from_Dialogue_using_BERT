      "utterance": [
        {
          "id": "MDRW2100010851.1.1",
          "form": "요즘 운동하시는거잇으세요?",
          "original_form": "요즘 운동하시는거잇으세요?",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010851.1.2",
          "form": "운동..!",
          "original_form": "운동..!",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010851.1.3",
          "form": "아뇨 제가 사실 몇 달 전 눈 수술을 해서 의사선생님이 격한 운동은 안 된다고 하시더라고요",
          "original_form": "아뇨 제가 사실 몇 달 전 눈 수술을 해서 의사선생님이 격한 운동은 안 된다고 하시더라고요",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010851.1.4",
          "form": "구래서 그냥 걷기정도만 하고있는데",
          "original_form": "구래서 그냥 걷기정도만 하고있는데",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010851.1.5",
          "form": "수술 전에는 주짓수 진짜 재밌게 다녔었어요",
          "original_form": "수술 전에는 주짓수 진짜 재밌게 다녔었어요",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010851.1.6",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010851.1.7",
          "form": "주짓수 어떤가요 얼마나 하셧어요?",
          "original_form": "주짓수 어떤가요 얼마나 하셧어요?",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010851.1.8",
          "form": "저 너무 배워보고싶엇는ㄷ0",
          "original_form": "저 너무 배워보고싶엇는ㄷ0",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010851.1.9",
          "form": "얼마 안 다녀서 좀 민망쓰하네요",
          "original_form": "얼마 안 다녀서 좀 민망쓰하네요",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010851.1.10",
          "form": "4개월 다녔었어용",
          "original_form": "4개월 다녔었어용",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010851.1.11",
          "form": "진짜 엄청 정신없고 힘들고 멍이 막 들거든요???",
          "original_form": "진짜 엄청 정신없고 힘들고 멍이 막 들거든요???",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.12",
          "form": "사개월이면 꽤 오래다니셧네요",
          "original_form": "사개월이면 꽤 오래다니셧네요",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.13",
          "form": "땀이 턱을 타고 흘러요",
          "original_form": "땀이 턱을 타고 흘러요",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.14",
          "form": "헐 그렇구나",
          "original_form": "헐 그렇구나",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.15",
          "form": "근데 너무 재밌어요",
          "original_form": "근데 너무 재밌어요",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.16",
          "form": "근대 막 엄청 뛰어다니고 이런건 아닌ㄷ0",
          "original_form": "근대 막 엄청 뛰어다니고 이런건 아닌ㄷ0",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.17",
          "form": "아픈줄도 모르고 해요",
          "original_form": "아픈줄도 모르고 해요",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.18",
          "form": "땀이 많이 나나봐요",
          "original_form": "땀이 많이 나나봐요",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.19",
          "form": "그 스파링 할 때 땀이 엄청 나요",
          "original_form": "그 스파링 할 때 땀이 엄청 나요",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010851.1.20",
          "form": "와 진짜 재밌겠다..",
          "original_form": "와 진짜 재밌겠다..",
          "speaker_id": "1",
          "time": "20210823 22:32"
        }
      ]
