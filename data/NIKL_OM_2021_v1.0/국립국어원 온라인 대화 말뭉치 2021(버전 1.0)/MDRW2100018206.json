      "utterance": [
        {
          "id": "MDRW2100018206.1.1",
          "form": "아 근데 저 해주고싶은 말 있어요",
          "original_form": "아 근데 저 해주고싶은 말 있어요",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018206.1.2",
          "form": "결혼하면 이제 집 구하자나여",
          "original_form": "결혼하면 이제 집 구하자나여",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018206.1.3",
          "form": "그 직전달에 원천징수 수입을 최대한 줄이세여....",
          "original_form": "그 직전달에 원천징수 수입을 최대한 줄이세여....",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018206.1.4",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018206.1.5",
          "form": "^^ 큰코다쳤어요",
          "original_form": "^^ 큰코다쳤어요",
          "speaker_id": "1",
          "time": "20210827 09:15"
        },
        {
          "id": "MDRW2100018206.1.6",
          "form": "헐 저 암것도 몰라요",
          "original_form": "헐 저 암것도 몰라요",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018206.1.7",
          "form": "원천징수 수입 높으면 어케돼여..?",
          "original_form": "원천징수 수입 높으면 어케돼여..?",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018206.1.8",
          "form": "매매도 보금자리론이 가능하고",
          "original_form": "매매도 보금자리론이 가능하고",
          "speaker_id": "1",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018206.1.9",
          "form": "물론 이건 여러 조건이 더 있지만 주택 가격이라덩가...",
          "original_form": "물론 이건 여러 조건이 더 있지만 주택 가격이라덩가...",
          "speaker_id": "1",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018206.1.10",
          "form": "전세 가더라도",
          "original_form": "전세 가더라도",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.11",
          "form": "신혼전세대출이 되는데",
          "original_form": "신혼전세대출이 되는데",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.12",
          "form": "모든 것에 소득제한이 따라요",
          "original_form": "모든 것에 소득제한이 따라요",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.13",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.14",
          "form": "아이고이런",
          "original_form": "아이고이런",
          "speaker_id": "2",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.15",
          "form": "저는 남친이랑 둘다 공무원인데",
          "original_form": "저는 남친이랑 둘다 공무원인데",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.16",
          "form": "우와 멋져요",
          "original_form": "우와 멋져요",
          "speaker_id": "2",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.17",
          "form": "연금이랑 공제회때문에",
          "original_form": "연금이랑 공제회때문에",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.18",
          "form": "실수령액은 둘다 200초라서.. 빠듯한데",
          "original_form": "실수령액은 둘다 200초라서.. 빠듯한데",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.19",
          "form": "세전이 둘 합쳐서 7천인가 넘겨가지구",
          "original_form": "세전이 둘 합쳐서 7천인가 넘겨가지구",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.20",
          "form": "혜택을 못받거든요",
          "original_form": "혜택을 못받거든요",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018206.1.21",
          "form": "아니여... 저 다시 돌아가면 공뭥 안햇을거에여",
          "original_form": "아니여... 저 다시 돌아가면 공뭥 안햇을거에여",
          "speaker_id": "1",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018206.1.22",
          "form": "ㄹㅇ... 가성비 떨어져요 흑",
          "original_form": "ㄹㅇ... 가성비 떨어져요 흑",
          "speaker_id": "1",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018206.1.23",
          "form": "우와 그래도 공뭔경험 살려서 책이라도 내주세요",
          "original_form": "우와 그래도 공뭔경험 살려서 책이라도 내주세요",
          "speaker_id": "2",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018206.1.24",
          "form": "헷",
          "original_form": "헷",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018206.1.25",
          "form": "아 겸직 금지라 안되나",
          "original_form": "아 겸직 금지라 안되나",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018206.1.26",
          "form": "책은 되는 거 같아서",
          "original_form": "책은 되는 거 같아서",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018206.1.27",
          "form": "크몽에 전자책 도전해보려구요 연말에!",
          "original_form": "크몽에 전자책 도전해보려구요 연말에!",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018206.1.28",
          "form": "제가 좀 소수직렬이거듬요",
          "original_form": "제가 좀 소수직렬이거듬요",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018206.1.29",
          "form": "와아아아아",
          "original_form": "와아아아아",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018206.1.30",
          "form": "제가 홍보할게요",
          "original_form": "제가 홍보할게요",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018206.1.31",
          "form": "너무멋지시다",
          "original_form": "너무멋지시다",
          "speaker_id": "2",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018206.1.32",
          "form": "우와 너무 감사하져,,다흐흑",
          "original_form": "우와 너무 감사하져,,다흐흑",
          "speaker_id": "1",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018206.1.33",
          "form": "맡겨만주시라구요~!",
          "original_form": "맡겨만주시라구요~!",
          "speaker_id": "2",
          "time": "20210827 09:30"
        },
        {
          "id": "MDRW2100018206.1.34",
          "form": "우린 서로이웃이니까특혜특혜",
          "original_form": "우린 서로이웃이니까특혜특혜",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018206.1.35",
          "form": "name2 블로그보면",
          "original_form": "&name2& 블로그보면",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018206.1.36",
          "form": "ㄹㅇ 성공하실듯... 재테크로",
          "original_form": "ㄹㅇ 성공하실듯... 재테크로",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018206.1.37",
          "form": "엄청 대단해여...",
          "original_form": "엄청 대단해여...",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018206.1.38",
          "form": "콘텐츠도 다양하규",
          "original_form": "콘텐츠도 다양하규",
          "speaker_id": "1",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018206.1.39",
          "form": "특혜 인정임돠 완전 빠방하게 공뭔준비생들한테 뿌릴게요",
          "original_form": "특혜 인정임돠 완전 빠방하게 공뭔준비생들한테 뿌릴게요",
          "speaker_id": "2",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018206.1.40",
          "form": "에에에에",
          "original_form": "에에에에",
          "speaker_id": "2",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018206.1.41",
          "form": "감사해요!",
          "original_form": "감사해요!",
          "speaker_id": "2",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018206.1.42",
