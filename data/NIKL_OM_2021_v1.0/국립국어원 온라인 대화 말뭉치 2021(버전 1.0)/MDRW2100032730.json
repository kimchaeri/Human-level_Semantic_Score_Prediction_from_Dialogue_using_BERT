      "utterance": [
        {
          "id": "MDRW2100032730.1.1",
          "form": "당시엔 해결안되조",
          "original_form": "당시엔 해결안되조",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032730.1.2",
          "form": "성숙을 말한거였어요",
          "original_form": "성숙을 말한거였어요",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032730.1.3",
          "form": "아 맞아요ㅋㅋㅋㅋ그 뚯인가 살짝 생각했다가ㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋㅋ그 뚯인가 살짝 생각했다가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032730.1.4",
          "form": "혼자 분했던 생각이 나서ㅋㅋㅋ급울컥했네요",
          "original_form": "혼자 분했던 생각이 나서ㅋㅋㅋ급울컥했네요",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032730.1.5",
          "form": "ㅋㅋㅋ그럴수있조 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그럴수있조 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100032730.1.6",
          "form": "저 제일 첫 연애가 엄청 기억 남는 연애였는데, 헤어지고 나서 같이 공부하던 자리, 같이 다니던 길 이런데 혼자 걸어다니면서 울고 혼자 마음 정리하고 했던게 지금와서 생가하면 너무 귀여워요ㅋㅋ그땐 진짜 너무 슬펐는데",
          "original_form": "저 제일 첫 연애가 엄청 기억 남는 연애였는데, 헤어지고 나서 같이 공부하던 자리, 같이 다니던 길 이런데 혼자 걸어다니면서 울고 혼자 마음 정리하고 했던게 지금와서 생가하면 너무 귀여워요ㅋㅋ그땐 진짜 너무 슬펐는데",
          "speaker_id": "2",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100032730.1.7",
          "form": "처음이 어쩔수없이 강렬한 걸까요 ㅋㅋㅋ",
          "original_form": "처음이 어쩔수없이 강렬한 걸까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032730.1.8",
          "form": "이젠 헤어지면 맘정리 단칼에 하나요?",
          "original_form": "이젠 헤어지면 맘정리 단칼에 하나요?",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032730.1.9",
          "form": "그 이후로는 거의 단칼이았던거 같아요ㅋㅋㅋ그냥 종종 생각나는 정도?",
          "original_form": "그 이후로는 거의 단칼이았던거 같아요ㅋㅋㅋ그냥 종종 생각나는 정도?",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032730.1.10",
          "form": "그냥 인사정도 하고 지내는 애들도 있고ㅋㅋ",
          "original_form": "그냥 인사정도 하고 지내는 애들도 있고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032730.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ역시뭐든지 처음이어렵나봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ역시뭐든지 처음이어렵나봐요",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032730.1.12",
          "form": "기억에 남는 연애 있으신가요ㅎㅎ",
          "original_form": "기억에 남는 연애 있으신가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032730.1.13",
          "form": "있죠 ㅋㅋㅋㅋㅋㅋㅋ어떻게 없겠습니까 ㅎㅎㅎ",
          "original_form": "있죠 ㅋㅋㅋㅋㅋㅋㅋ어떻게 없겠습니까 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032730.1.14",
          "form": "궁금해요 ㅋㅋㅋ하나만 청해들어도 되나용ㅋㅋㅋ",
          "original_form": "궁금해요 ㅋㅋㅋ하나만 청해들어도 되나용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032730.1.15",
          "form": "ㅋㅋㅋ청할게없어요",
          "original_form": "ㅋㅋㅋ청할게없어요",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032730.1.16",
          "form": "하나뿐이라",
          "original_form": "하나뿐이라",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032730.1.17",
          "form": "아주 오랜기간 사겼고",
          "original_form": "아주 오랜기간 사겼고",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032730.1.18",
          "form": "단칼에 헤어짐을 선고받아서 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "단칼에 헤어짐을 선고받아서 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032730.1.19",
          "form": "아.. 허무하단 말씀 그래서 하신거구나",
          "original_form": "아.. 허무하단 말씀 그래서 하신거구나",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032730.1.20",
          "form": "ㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋ그쵸",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032730.1.21",
          "form": "음 20대 청춘 같이 보냈을때는 헤어지면 그 시간들이 더 아쉽고 허무하고 아까운 느낌이기는해요",
          "original_form": "음 20대 청춘 같이 보냈을때는 헤어지면 그 시간들이 더 아쉽고 허무하고 아까운 느낌이기는해요",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032730.1.22",
          "form": "ㅋㅋㅋㅋ음",
          "original_form": "ㅋㅋㅋㅋ음",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032730.1.23",
          "form": "아깝지는 않은데",
          "original_form": "아깝지는 않은데",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032730.1.24",
          "form": "항상 어떤 일이든 사람이든",
          "original_form": "항상 어떤 일이든 사람이든",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032730.1.25",
          "form": "마무리가 중요하잖아요?",
          "original_form": "마무리가 중요하잖아요?",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032730.1.26",
          "form": "마무리가 정말 중요하죠 그럼요",
          "original_form": "마무리가 정말 중요하죠 그럼요",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032730.1.27",
          "form": "나는 정말 이 사람이 좋고 괜찮다고 생각해서 오래 만난건데",
          "original_form": "나는 정말 이 사람이 좋고 괜찮다고 생각해서 오래 만난건데",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032730.1.28",
          "form": "끝이 정말.. 내가 알던 사람이 맞아?",
          "original_form": "끝이 정말.. 내가 알던 사람이 맞아?",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.29",
          "form": "라는 생각이 드니까",
          "original_form": "라는 생각이 드니까",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.30",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.31",
          "form": "그것도 지나보니",
          "original_form": "그것도 지나보니",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.32",
          "form": "이유는 얘기해주던가요..?",
          "original_form": "이유는 얘기해주던가요..?",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.33",
          "form": "원래 그런 사람이었는데",
          "original_form": "원래 그런 사람이었는데",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.34",
          "form": "콩깍지씌여서",
          "original_form": "콩깍지씌여서",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.35",
          "form": "못본거더라구요",
          "original_form": "못본거더라구요",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.36",
          "form": "뭐 그런 점들이 허무했어요",
          "original_form": "뭐 그런 점들이 허무했어요",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032730.1.37",
          "form": "네 이야기해줬죠 ㅋㅋ",
          "original_form": "네 이야기해줬죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032730.1.38",
          "form": "콩깍지라는 말이 정말 예전엔 장난스럽게 느껴졌는데",
          "original_form": "콩깍지라는 말이 정말 예전엔 장난스럽게 느껴졌는데",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032730.1.39",
          "form": "한 발자국만 떨어져서 3자 입장에서 보면 너무 당연한걸 못보게 만드는게 결국는 콩깍지라",
          "original_form": "한 발자국만 떨어져서 3자 입장에서 보면 너무 당연한걸 못보게 만드는게 결국는 콩깍지라",
          "speaker_id": "2",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032730.1.40",
          "form": "근데 그만큼.. 사랑할 수 있는건",
          "original_form": "근데 그만큼.. 사랑할 수 있는건",
          "speaker_id": "1",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032730.1.41",
          "form": "근데 연애 하면서 이성적이기가 쉽나요 감성적이고 감정적이기 쉽지",
          "original_form": "근데 연애 하면서 이성적이기가 쉽나요 감성적이고 감정적이기 쉽지",
          "speaker_id": "2",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032730.1.42",
          "form": "오직 그 때만 가능한거잖아요",
          "original_form": "오직 그 때만 가능한거잖아요",
          "speaker_id": "1",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032730.1.43",
          "form": "이젠 그만틈",
          "original_form": "이젠 그만틈",
          "speaker_id": "1",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032730.1.44",
          "form": "열정적이진 못하겠조",
          "original_form": "열정적이진 못하겠조",
          "speaker_id": "1",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032730.1.45",
          "form": "그 꽁깍지도",
          "original_form": "그 꽁깍지도",
          "speaker_id": "1",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100032730.1.46",
          "form": "이젠 더 이상 그만큼 씌이지는 못하겠죠",
          "original_form": "이젠 더 이상 그만큼 씌이지는 못하겠죠",
          "speaker_id": "1",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100032730.1.47",
          "form": "어떤 사람이냐 따라 다르지 않을까요?",
          "original_form": "어떤 사람이냐 따라 다르지 않을까요?",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032730.1.48",
          "form": "앞선 연애에서 내가 데였던 부분은 콩깍지 없이 보려고 노력하겠지만",
          "original_form": "앞선 연애에서 내가 데였던 부분은 콩깍지 없이 보려고 노력하겠지만",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032730.1.49",
          "form": "음 전 아닐거같아요.",
          "original_form": "음 전 아닐거같아요.",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100032730.1.50",
          "form": "또 아닌 부분은 유연하게 받아들이다보면 ㅎㅎ저는 좀 연애때마다 중요하게 본게 달랐거든요",
          "original_form": "또 아닌 부분은 유연하게 받아들이다보면 ㅎㅎ저는 좀 연애때마다 중요하게 본게 달랐거든요",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032730.1.51",
          "form": "상대의 장점 위주로? 그래서 콩깍지 잘못씌였다 헤어지고 그랬었어요",
          "original_form": "상대의 장점 위주로? 그래서 콩깍지 잘못씌였다 헤어지고 그랬었어요",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032730.1.52",
          "form": "음 ㅋㅋㅋㅋㅋ 뭐.. 안해본걸 확신할 순럾죠",
          "original_form": "음 ㅋㅋㅋㅋㅋ 뭐.. 안해본걸 확신할 순럾죠",
