      "utterance": [
        {
          "id": "MDRW2100016085.1.1",
          "form": "여행가고싶어요",
          "original_form": "여행가고싶어요",
          "speaker_id": "2",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016085.1.2",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100016085.1.3",
          "form": "가깝게 부산여행 다녀올까 생각중이에요",
          "original_form": "가깝게 부산여행 다녀올까 생각중이에요",
          "speaker_id": "2",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100016085.1.4",
          "form": "부산좋죠!",
          "original_form": "부산좋죠!",
          "speaker_id": "1",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100016085.1.5",
          "form": "바다에 넘 가고싶네요",
          "original_form": "바다에 넘 가고싶네요",
          "speaker_id": "2",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100016085.1.6",
          "form": "전 산이나 계곡이요",
          "original_form": "전 산이나 계곡이요",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100016085.1.7",
          "form": "이번에 어디 다녀오신 데 있으신가요?",
          "original_form": "이번에 어디 다녀오신 데 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100016085.1.8",
          "form": "아니요ㅜㅜ",
          "original_form": "아니요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100016085.1.9",
          "form": "전 계곡 다녀왔어요",
          "original_form": "전 계곡 다녀왔어요",
          "speaker_id": "2",
          "time": "20210826 10:38"
        },
        {
          "id": "MDRW2100016085.1.10",
          "form": "여름에 어디돌아다니는 걸 안좋아해요",
          "original_form": "여름에 어디돌아다니는 걸 안좋아해요",
          "speaker_id": "1",
          "time": "20210826 10:38"
        },
        {
          "id": "MDRW2100016085.1.11",
          "form": "이제 가을이니까 여행 좀 다니려구요",
          "original_form": "이제 가을이니까 여행 좀 다니려구요",
          "speaker_id": "2",
          "time": "20210826 10:38"
        },
        {
          "id": "MDRW2100016085.1.12",
          "form": "조심하세요",
          "original_form": "조심하세요",
          "speaker_id": "1",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100016085.1.13",
          "form": "네ㅜㅜ 경주도 가고싶어요",
          "original_form": "네ㅜㅜ 경주도 가고싶어요",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100016085.1.14",
          "form": "경주 진짜 가을에  꼭 가보세요",
          "original_form": "경주 진짜 가을에  꼭 가보세요",
          "speaker_id": "1",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100016085.1.15",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "1",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100016085.1.16",
          "form": "저 부산 여행가요",
          "original_form": "저 부산 여행가요",
          "speaker_id": "2",
