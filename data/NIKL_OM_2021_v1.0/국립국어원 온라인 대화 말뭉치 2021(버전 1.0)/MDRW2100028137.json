        {
          "id": "MDRW2100028137.1.1",
          "form": "대학생활은어때여",
          "original_form": "대학생활은어때여",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028137.1.2",
          "form": "몇번 나가지도 못하고 온강 듣고 있어요",
          "original_form": "몇번 나가지도 못하고 온강 듣고 있어요",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028137.1.3",
          "form": "이게 대학인지 인강인지...",
          "original_form": "이게 대학인지 인강인지...",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028137.1.4",
          "form": "대학 몇 번이나 간 거에요?",
          "original_form": "대학 몇 번이나 간 거에요?",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028137.1.5",
          "form": "그 몇 번 안 가고 씨씨를..",
          "original_form": "그 몇 번 안 가고 씨씨를..",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028137.1.6",
          "form": "씨씨 아니에요ㅋㅋ",
          "original_form": "씨씨 아니에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028137.1.7",
          "form": "헐 글쿤요. 학교는 몇번이나 간 거래요",
          "original_form": "헐 글쿤요. 학교는 몇번이나 간 거래요",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028137.1.8",
          "form": "시험볼 때랑 실습 몇번 제외하고는 거의 안 갔어요",
          "original_form": "시험볼 때랑 실습 몇번 제외하고는 거의 안 갔어요",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028137.1.9",
          "form": "엇 실습도 해요?",
          "original_form": "엇 실습도 해요?",
          "speaker_id": "1",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028137.1.10",
          "form": "실습이라고 해봤자 컴퓨터 앞에 앉아있는거예요ㅋㅋ",
          "original_form": "실습이라고 해봤자 컴퓨터 앞에 앉아있는거예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100028137.1.11",
          "form": "오잉 컴퓨터관련학과신가바요",
          "original_form": "오잉 컴퓨터관련학과신가바요",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028137.1.12",
          "form": "디자인하고 있어요",
          "original_form": "디자인하고 있어요",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028137.1.13",
          "form": "우와 짱멋져요",
          "original_form": "우와 짱멋져요",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028137.1.14",
          "form": "별거 없어요...ㅎㅎ",
          "original_form": "별거 없어요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028137.1.15",
          "form": "전공 있는 건 좋은거죠",
          "original_form": "전공 있는 건 좋은거죠",
          "speaker_id": "1",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028137.1.16",
          "form": "딱히 좋은 전공이라고 생각해본적이 없어서요",
          "original_form": "딱히 좋은 전공이라고 생각해본적이 없어서요",
