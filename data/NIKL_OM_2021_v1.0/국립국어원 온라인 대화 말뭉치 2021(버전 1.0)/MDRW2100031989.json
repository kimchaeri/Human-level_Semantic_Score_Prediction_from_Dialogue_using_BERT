      "utterance": [
        {
          "id": "MDRW2100031989.1.1",
          "form": "커도 흔들리면 ㅋㅋ",
          "original_form": "커도 흔들리면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031989.1.2",
          "form": "파도 따라 ㅎㅎ",
          "original_form": "파도 따라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100031989.1.3",
          "form": "헐헐 커도 파도흔들리면 멀미 심하군요 ㅠㅠㅠㅠ",
          "original_form": "헐헐 커도 파도흔들리면 멀미 심하군요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031989.1.4",
          "form": "울릉도안에 숙소는 많이 있나요?!",
          "original_form": "울릉도안에 숙소는 많이 있나요?!",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031989.1.5",
          "form": "있긴한데",
          "original_form": "있긴한데",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031989.1.6",
          "form": "육지",
          "original_form": "육지",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031989.1.7",
          "form": "보다",
          "original_form": "보다",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031989.1.8",
          "form": "컨디션이 안좋아요",
          "original_form": "컨디션이 안좋아요",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100031989.1.9",
          "form": "하긴 그럴것 같긴해요 ㅠㅠ 울릉도는 제주도보다 훠어얼씬 작죠? 하루나 이틀이면 다 구경가능한가요?",
          "original_form": "하긴 그럴것 같긴해요 ㅠㅠ 울릉도는 제주도보다 훠어얼씬 작죠? 하루나 이틀이면 다 구경가능한가요?",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100031989.1.10",
          "form": "제주보단 작죠 ㅎㅎ 가능은 한데",
          "original_form": "제주보단 작죠 ㅎㅎ 가능은 한데",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031989.1.11",
          "form": "조금 빡빡해서",
          "original_form": "조금 빡빡해서",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031989.1.12",
          "form": "3일정도가 넉넉할듯요",
          "original_form": "3일정도가 넉넉할듯요",
          "speaker_id": "1",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031989.1.13",
          "form": "울릉도에선 어떤것들 드셨어요? ㅋㅋ맛있는거 많았나요?",
          "original_form": "울릉도에선 어떤것들 드셨어요? ㅋㅋ맛있는거 많았나요?",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031989.1.14",
          "form": "뭐 호박엿, 따개비밥? 먹긴 먹는데 ㅋㅋ 물가가 비싸요",
          "original_form": "뭐 호박엿, 따개비밥? 먹긴 먹는데 ㅋㅋ 물가가 비싸요",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031989.1.15",
          "form": "name2은 어디 다녀오셨나요",
          "original_form": "&name2&은 어디 다녀오셨나요",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031989.1.16",
          "form": "따개비밥은 어떤거에요?",
          "original_form": "따개비밥은 어떤거에요?",
          "speaker_id": "2",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031989.1.17",
          "form": "저는 최근에는 코로나때매 여행을 거의 못갔어요ㅠ",
          "original_form": "저는 최근에는 코로나때매 여행을 거의 못갔어요ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031989.1.18",
          "form": "따개비따서 밥에 ㅋㅋ",
          "original_form": "따개비따서 밥에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031989.1.19",
          "form": "저도 많이 줄이긴 했는데",
          "original_form": "저도 많이 줄이긴 했는데",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031989.1.20",
          "form": "해외가고 싶네요 ㅋㅋ",
          "original_form": "해외가고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031989.1.21",
          "form": "휴 맞아요 해외가고싶어요 ㅠ",
          "original_form": "휴 맞아요 해외가고싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031989.1.22",
          "form": "이번 추석연휴가 진짜",
          "original_form": "이번 추석연휴가 진짜",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031989.1.23",
          "form": "해외가기 좋은 긴 연휴인데ㅠ 아쉽네요",
          "original_form": "해외가기 좋은 긴 연휴인데ㅠ 아쉽네요",
          "speaker_id": "2",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031989.1.24",
          "form": "마지막 해외",
          "original_form": "마지막 해외",
          "speaker_id": "1",
          "time": "20210905 10:41"
        },
        {
          "id": "MDRW2100031989.1.25",
          "form": "언제신가요",
          "original_form": "언제신가요",
          "speaker_id": "1",
          "time": "20210905 10:41"
        },
        {
          "id": "MDRW2100031989.1.26",
          "form": "흐음 ... 2019년? 여름이네요 ㅠㅠ 일본이요",
          "original_form": "흐음 ... 2019년? 여름이네요 ㅠㅠ 일본이요",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031989.1.27",
          "form": "저는 19년 10월인가....ㅎㅎ",
          "original_form": "저는 19년 10월인가....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100031989.1.28",
          "form": "20년 초에 브루나이 가려다가",
          "original_form": "20년 초에 브루나이 가려다가",
          "speaker_id": "1",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031989.1.29",
          "form": "에이 특가 아니네 안가 하고",
          "original_form": "에이 특가 아니네 안가 하고",
          "speaker_id": "1",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031989.1.30",
          "form": "안했는데ㅠ",
          "original_form": "안했는데ㅠ",
          "speaker_id": "1",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100031989.1.31",
          "form": "저도 20년 여름에 미국 3주 여행가려고 19년말부터 20년 초까지 휴가도 안쓰고 풀 근무 했었는데ㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "저도 20년 여름에 미국 3주 여행가려고 19년말부터 20년 초까지 휴가도 안쓰고 풀 근무 했었는데ㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
