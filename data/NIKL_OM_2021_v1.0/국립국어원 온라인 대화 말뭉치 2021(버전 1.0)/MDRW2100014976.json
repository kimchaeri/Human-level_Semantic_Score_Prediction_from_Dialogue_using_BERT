      "utterance": [
        {
          "id": "MDRW2100014976.1.1",
          "form": "마지막으로 여행가신게 언제인가요?",
          "original_form": "마지막으로 여행가신게 언제인가요?",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014976.1.2",
          "form": "코로나 때문에 오래됐어요 ㅠㅠ",
          "original_form": "코로나 때문에 오래됐어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014976.1.3",
          "form": "여행 정말 좋아하는데ㅠㅠ",
          "original_form": "여행 정말 좋아하는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014976.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014976.1.5",
          "form": "그래도 마지막이 할머니모시고 일본 간거라 좀 위안이요",
          "original_form": "그래도 마지막이 할머니모시고 일본 간거라 좀 위안이요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014976.1.6",
          "form": "와 할머니께서 좋아하셨겠어요",
          "original_form": "와 할머니께서 좋아하셨겠어요",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014976.1.7",
          "form": "일본 어디다녀오셧어요?",
          "original_form": "일본 어디다녀오셧어요?",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014976.1.8",
          "form": "도쿄쪽 갔다왔어요",
          "original_form": "도쿄쪽 갔다왔어요",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014976.1.9",
          "form": "내가 아는 곳으로만",
          "original_form": "내가 아는 곳으로만",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014976.1.10",
          "form": "도쿄 한번도 안가봤어요 ㅎㅎ",
          "original_form": "도쿄 한번도 안가봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014976.1.11",
          "form": "서울하고 비슷해요",
          "original_form": "서울하고 비슷해요",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014976.1.12",
          "form": "한국사람도 많고",
          "original_form": "한국사람도 많고",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014976.1.13",
          "form": "맛있는 디저트 얘기도 많이들엇어요 ㅋㅋㅋ",
          "original_form": "맛있는 디저트 얘기도 많이들엇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014976.1.14",
          "form": "네 전 특히 백화점지하를 사랑합니다",
          "original_form": "네 전 특히 백화점지하를 사랑합니다",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014976.1.15",
          "form": "ㅋㅋㅋㅋㅋ어느나라든 백화점지하가 맛있는건 공통인가요",
          "original_form": "ㅋㅋㅋㅋㅋ어느나라든 백화점지하가 맛있는건 공통인가요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014976.1.16",
          "form": "유명한가게 팝업도 다있고",
          "original_form": "유명한가게 팝업도 다있고",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014976.1.17",
          "form": "고래고기 먹은게 좀 기억에 남네요",
          "original_form": "고래고기 먹은게 좀 기억에 남네요",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014976.1.18",
          "form": "고래고기요?? 우와 신기",
          "original_form": "고래고기요?? 우와 신기",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014976.1.19",
          "form": "안먹어봐서 어떤맛인지 궁금해요 ㅋㅋ",
          "original_form": "안먹어봐서 어떤맛인지 궁금해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014976.1.20",
          "form": "익은 꼬치여서 참치스테이크 같았어요",
          "original_form": "익은 꼬치여서 참치스테이크 같았어요",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014976.1.21",
          "form": "냄새를 감추려한건지 좀 간이 쌔더라구요",
          "original_form": "냄새를 감추려한건지 좀 간이 쌔더라구요",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014976.1.22",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014976.1.23",
          "form": "참치스테이크 같으면 맛잇을것같아요",
          "original_form": "참치스테이크 같으면 맛잇을것같아요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014976.1.24",
          "form": "나중에 먹어볼일잇으면 먹어봐야겟어요 ㅋㅋㅋㅋ",
          "original_form": "나중에 먹어볼일잇으면 먹어봐야겟어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014976.1.25",
          "form": "추천할맛은 아니예요",
          "original_form": "추천할맛은 아니예요",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014976.1.26",
          "form": "규탄이라고 소혀는 진짜 강추",
          "original_form": "규탄이라고 소혀는 진짜 강추",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014976.1.27",
          "form": "규탄은 먹어봣어요 그냥 녹던데요ㅠㅠ",
          "original_form": "규탄은 먹어봣어요 그냥 녹던데요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014976.1.28",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014976.1.29",
          "form": "한국엔 많이없어서 아쉽",
          "original_form": "한국엔 많이없어서 아쉽",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014976.1.30",
          "form": "너무 맛있었음...ㅠㅠ",
          "original_form": "너무 맛있었음...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014976.1.31",
          "form": "이태원에서 한곳 봤어요",
          "original_form": "이태원에서 한곳 봤어요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014976.1.32",
          "form": "레몬이랑도 너무 잘어울리고ㅠㅠ",
          "original_form": "레몬이랑도 너무 잘어울리고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014976.1.33",
          "form": "파랑 소스랑",
          "original_form": "파랑 소스랑",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014976.1.34",
