      "utterance": [
        {
          "id": "MDRW2100022007.1.1",
          "form": "맨날 저는 불평불만이라ㅎㅎ",
          "original_form": "맨날 저는 불평불만이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:49"
        },
        {
          "id": "MDRW2100022007.1.2",
          "form": "솔직히 일하는거는 고충은 없어요ㅎㅎ",
          "original_form": "솔직히 일하는거는 고충은 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100022007.1.3",
          "form": "내일하고 내가 열씸히하면 된다고 생각하기때문에ㅎㅎ",
          "original_form": "내일하고 내가 열씸히하면 된다고 생각하기때문에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100022007.1.4",
          "form": "저는 좀 그런갑다 하고 넘기는 스탈이라 ㅋ",
          "original_form": "저는 좀 그런갑다 하고 넘기는 스탈이라 ㅋ",
          "speaker_id": "1",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100022007.1.5",
          "form": "그런데 이런게 문제에요ㅎ",
          "original_form": "그런데 이런게 문제에요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100022007.1.6",
          "form": "자기가 보고하려고",
          "original_form": "자기가 보고하려고",
          "speaker_id": "2",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100022007.1.7",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100022007.1.8",
          "form": "나한테",
          "original_form": "나한테",
          "speaker_id": "2",
          "time": "20210829 00:51"
        },
        {
          "id": "MDRW2100022007.1.9",
          "form": "시키는거요ㅎㅎㅎ",
          "original_form": "시키는거요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:51"
        },
        {
          "id": "MDRW2100022007.1.10",
          "form": "헐 최악!!",
          "original_form": "헐 최악!!",
          "speaker_id": "1",
          "time": "20210829 00:51"
        },
        {
          "id": "MDRW2100022007.1.11",
          "form": "업무적으로 뭘 필요해서가 아니라",
          "original_form": "업무적으로 뭘 필요해서가 아니라",
          "speaker_id": "2",
          "time": "20210829 00:51"
        },
        {
          "id": "MDRW2100022007.1.12",
          "form": "보고용인거죠ㅎㅎ",
          "original_form": "보고용인거죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:51"
        },
        {
          "id": "MDRW2100022007.1.13",
          "form": "근데 저는 그럼 대충 만들수있을까요?ㅎㅎㅎ 대표님 보고 사항인데ㅎ",
          "original_form": "근데 저는 그럼 대충 만들수있을까요?ㅎㅎㅎ 대표님 보고 사항인데ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:51"
        },
        {
          "id": "MDRW2100022007.1.14",
          "form": "없죠 ㅠㅠ",
          "original_form": "없죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100022007.1.15",
          "form": "글구 성격 들음비대로라면",
          "original_form": "글구 성격 들음비대로라면",
          "speaker_id": "1",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100022007.1.16",
          "form": "완벽라게 하실라고 ㄷㄷㄷ",
          "original_form": "완벽라게 하실라고 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100022007.1.17",
          "form": "제가 제일싫어하는게",
          "original_form": "제가 제일싫어하는게",
          "speaker_id": "2",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100022007.1.18",
          "form": "다시해와 에요ㅎㅎㅎㅎㄹ",
          "original_form": "다시해와 에요ㅎㅎㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100022007.1.19",
          "form": "예전에 그랬는데ㅎㅎ 지금은 음...",
          "original_form": "예전에 그랬는데ㅎㅎ 지금은 음...",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022007.1.20",
          "form": "ㅡㅁ ㅡ 그건 누구나ㅜㅜㅋ",
          "original_form": "ㅡㅁ ㅡ 그건 누구나ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022007.1.21",
          "form": "호박엿 먹어라 하면서",
          "original_form": "호박엿 먹어라 하면서",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022007.1.22",
          "form": "페이지수 일부러 늘려요ㄹㅎㅎㅎㅎㄹㄹㄹ",
          "original_form": "페이지수 일부러 늘려요ㄹㅎㅎㅎㅎㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022007.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022007.1.24",
          "form": "오늘도 100페이지 넘기고ㅎㄹㄹ",
          "original_form": "오늘도 100페이지 넘기고ㅎㄹㄹ",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022007.1.25",
          "form": "다른자료들도ㅎ 자세히보면 아에 이해도 안되게 어렵게 만들고",
          "original_form": "다른자료들도ㅎ 자세히보면 아에 이해도 안되게 어렵게 만들고",
          "speaker_id": "2",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100022007.1.26",
          "form": "고생했어요",
          "original_form": "고생했어요",
          "speaker_id": "1",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100022007.1.27",
          "form": "에이 고생이라기보단ㅎ 재밌어요ㅎ",
          "original_form": "에이 고생이라기보단ㅎ 재밌어요ㅎ",
