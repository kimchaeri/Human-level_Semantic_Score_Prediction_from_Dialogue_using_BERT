      "utterance": [
        {
          "id": "MDRW2100042788.1.1",
          "form": "전 비행기 시간이 남아서 까페갈건데",
          "original_form": "전 비행기 시간이 남아서 까페갈건데",
          "speaker_id": "1",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100042788.1.2",
          "form": "까페가심 주로 뭐 시키세요",
          "original_form": "까페가심 주로 뭐 시키세요",
          "speaker_id": "1",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100042788.1.3",
          "form": "혼자계시면 밥은거의안해드시겠네요",
          "original_form": "혼자계시면 밥은거의안해드시겠네요",
          "speaker_id": "2",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100042788.1.4",
          "form": "저는 라떼요",
          "original_form": "저는 라떼요",
          "speaker_id": "2",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100042788.1.5",
          "form": "한끼 해먹고",
          "original_form": "한끼 해먹고",
          "speaker_id": "1",
          "time": "20210907 09:08"
        },
        {
          "id": "MDRW2100042788.1.6",
          "form": "한끼 사먹고 그래요",
          "original_form": "한끼 사먹고 그래요",
          "speaker_id": "1",
          "time": "20210907 09:08"
        },
        {
          "id": "MDRW2100042788.1.7",
          "form": "혼자있음그럴것같아요",
          "original_form": "혼자있음그럴것같아요",
          "speaker_id": "2",
          "time": "20210907 09:08"
        },
        {
          "id": "MDRW2100042788.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 09:08"
        },
        {
          "id": "MDRW2100042788.1.9",
          "form": "가성비도 안좋아여 혼자서먹음",
          "original_form": "가성비도 안좋아여 혼자서먹음",
          "speaker_id": "1",
          "time": "20210907 09:08"
        },
        {
          "id": "MDRW2100042788.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 09:08"
        },
        {
          "id": "MDRW2100042788.1.11",
          "form": "사서하는것보다 한끼사먹는게 나을듯해요",
          "original_form": "사서하는것보다 한끼사먹는게 나을듯해요",
          "speaker_id": "2",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.12",
          "form": "저도 가서 아메리카노 마셔야겠어요",
          "original_form": "저도 가서 아메리카노 마셔야겠어요",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.14",
          "form": "날씨 흐리고 커피마시기 딱이네요",
          "original_form": "날씨 흐리고 커피마시기 딱이네요",
          "speaker_id": "2",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.15",
          "form": "점심은 뭐 드실 예정이세요",
          "original_form": "점심은 뭐 드실 예정이세요",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.17",
          "form": "전 아침 호밀빵 과일 요거트 먹었어요",
          "original_form": "전 아침 호밀빵 과일 요거트 먹었어요",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.18",
          "form": "저도 오후에일나가서 간단히",
          "original_form": "저도 오후에일나가서 간단히",
          "speaker_id": "2",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042788.1.20",
          "form": "저도 프리랜서라",
          "original_form": "저도 프리랜서라",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042788.1.21",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042788.1.22",
          "form": "집에있는거 간단히먹고..",
          "original_form": "집에있는거 간단히먹고..",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042788.1.23",
          "form": "전 회시나가서 일하는게 좋아여",
          "original_form": "전 회시나가서 일하는게 좋아여",
          "speaker_id": "1",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042788.1.24",
          "form": "저두요..",
          "original_form": "저두요..",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042788.1.25",
          "form": "작년에 너무 많이해서 번아웃와서",
          "original_form": "작년에 너무 많이해서 번아웃와서",
          "speaker_id": "1",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042788.1.26",
          "form": "회사다니고싶어요..",
          "original_form": "회사다니고싶어요..",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042788.1.27",
          "form": "잠깐 프리랜서로",
          "original_form": "잠깐 프리랜서로",
          "speaker_id": "1",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042788.1.28",
          "form": "회사가면 같이 맛집투어하고",
          "original_form": "회사가면 같이 맛집투어하고",
          "speaker_id": "1",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042788.1.29",
          "form": "ㅋㅋ",
