      "utterance": [
        {
          "id": "MDRW2100034231.1.1",
          "form": "10시가",
          "original_form": "10시가",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100034231.1.2",
          "form": "다사 오네요 ㅋㅋ",
          "original_form": "다사 오네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100034231.1.3",
          "form": "펜트하우스 ㅋㅋ",
          "original_form": "펜트하우스 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100034231.1.4",
          "form": "달립니다",
          "original_form": "달립니다",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100034231.1.5",
          "form": "시작했으니 욕하며 봐요 ㅎㅎ",
          "original_form": "시작했으니 욕하며 봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100034231.1.6",
          "form": "크으 처음부터 보셨나봐요",
          "original_form": "크으 처음부터 보셨나봐요",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100034231.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100034231.1.8",
          "form": "저도 첨엔 대충보다 ㅋㅋ점점 시작한거 제시간에 봐요",
          "original_form": "저도 첨엔 대충보다 ㅋㅋ점점 시작한거 제시간에 봐요",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100034231.1.9",
          "form": "안보세요?",
          "original_form": "안보세요?",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100034231.1.10",
          "form": "막장이 보며 욕해도 또봐요",
          "original_form": "막장이 보며 욕해도 또봐요",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100034231.1.11",
          "form": "ㅋㅋ저는 보려했는데 1부 끝나고 알았어요",
          "original_form": "ㅋㅋ저는 보려했는데 1부 끝나고 알았어요",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100034231.1.12",
          "form": "지금은 주에 1회한다더라구요",
          "original_form": "지금은 주에 1회한다더라구요",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100034231.1.13",
          "form": "네 그렇게 해요",
          "original_form": "네 그렇게 해요",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100034231.1.14",
          "form": "뭐 워낙 죽고 살아나서 뒤죽박죽 이예요",
          "original_form": "뭐 워낙 죽고 살아나서 뒤죽박죽 이예요",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100034231.1.15",
          "form": "어찌 될지 막 욕하며 추리해여",
          "original_form": "어찌 될지 막 욕하며 추리해여",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100034231.1.16",
          "form": "오늘 뭐보세요?",
          "original_form": "오늘 뭐보세요?",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100034231.1.17",
          "form": "딱히 챙겨보는건 없어요",
          "original_form": "딱히 챙겨보는건 없어요",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100034231.1.18",
          "form": "보이스 봤었는데 끝나서 아쉬워요",
          "original_form": "보이스 봤었는데 끝나서 아쉬워요",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100034231.1.19",
          "form": "전 1.23",
          "original_form": "전 1.23",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100034231.1.20",
          "form": "넘 잼나게 봤어요",
          "original_form": "넘 잼나게 봤어요",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100034231.1.21",
          "form": "보이스 쭉 시작부터 봤는데 4는 조금보다 못봤어요",
          "original_form": "보이스 쭉 시작부터 봤는데 4는 조금보다 못봤어요",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100034231.1.22",
          "form": "앗 저는 4만 봤어요",
          "original_form": "앗 저는 4만 봤어요",
          "speaker_id": "1",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100034231.1.23",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100034231.1.24",
          "form": "1.2.3 보세요",
          "original_form": "1.2.3 보세요",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100034231.1.25",
          "form": "갈수록 재밌러요",
          "original_form": "갈수록 재밌러요",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100034231.1.26",
          "form": "요편들 진짜 잼나요",
          "original_form": "요편들 진짜 잼나요",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100034231.1.27",
          "form": "아항 봐야지하는데 볼게 쏟아지니 늦어져요ㅠ",
          "original_form": "아항 봐야지하는데 볼게 쏟아지니 늦어져요ㅠ",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100034231.1.28",
          "form": "맞아요 시간되면 보는거죠",
          "original_form": "맞아요 시간되면 보는거죠",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034231.1.29",
          "form": "맞아욥ㅋㅋ",
          "original_form": "맞아욥ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034231.1.30",
          "form": "전 지금 유튜브 2ne1 컴백홈 들어요 ㅋㅋ",
          "original_form": "전 지금 유튜브 2ne1 컴백홈 들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034231.1.31",
          "form": "외국드라마도 재밌는게 많아서욥",
          "original_form": "외국드라마도 재밌는게 많아서욥",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034231.1.32",
          "form": "오ㅋㅋ 반갑네용",
          "original_form": "오ㅋㅋ 반갑네용",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034231.1.33",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034231.1.34",
          "form": "맞아요 시리즈 잼나죠",
          "original_form": "맞아요 시리즈 잼나죠",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100034231.1.35",
          "form": "뭐보세요 ?",
          "original_form": "뭐보세요 ?",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100034231.1.36",
          "form": "저는 최근에 더오피스 미드 봤어요",
          "original_form": "저는 최근에 더오피스 미드 봤어요",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034231.1.37",
          "form": "오 회사에서 일러나는 일인가요?",
          "original_form": "오 회사에서 일러나는 일인가요?",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034231.1.38",
          "form": "전 위쳐",
          "original_form": "전 위쳐",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034231.1.39",
          "form": "판타지 좋아해요",
          "original_form": "판타지 좋아해요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034231.1.40",
          "form": "넹ㅋㅋ 너무 현실적이에요",
          "original_form": "넹ㅋㅋ 너무 현실적이에요",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034231.1.41",
          "form": "오ㅋㅋ 판타지도 좋죵",
          "original_form": "오ㅋㅋ 판타지도 좋죵",
