      "utterance": [
        {
          "id": "MDRW2100021775.1.1",
          "form": "저녁뭐드셨어요",
          "original_form": "저녁뭐드셨어요",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021775.1.2",
          "form": "닭볶음탕 먹었어요 name1님은요?",
          "original_form": "닭볶음탕 먹었어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021775.1.3",
          "form": "저는 피자요",
          "original_form": "저는 피자요",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021775.1.4",
          "form": "어떤 피자 드셨나요?",
          "original_form": "어떤 피자 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100021775.1.5",
          "form": "청년피자먹었어용 닭볶음탕 궁금해요",
          "original_form": "청년피자먹었어용 닭볶음탕 궁금해요",
          "speaker_id": "1",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100021775.1.6",
          "form": "엄마가 손이 커서 며칠째 먹고 있어요 ㅋㅋㅋ",
          "original_form": "엄마가 손이 커서 며칠째 먹고 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100021775.1.7",
          "form": "어떤 피자 먹었나요",
          "original_form": "어떤 피자 먹었나요",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100021775.1.8",
          "form": "에그콘 피자요",
          "original_form": "에그콘 피자요",
          "speaker_id": "1",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100021775.1.9",
          "form": "와 어머니 요리실력이 좋으시다!",
          "original_form": "와 어머니 요리실력이 좋으시다!",
          "speaker_id": "1",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100021775.1.10",
          "form": "처음 들어 보는 피자네요",
          "original_form": "처음 들어 보는 피자네요",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100021775.1.11",
          "form": "손만 크신거예요 전 닭볶음탕 속 양념 잘 스며든 감자 좋아해요",
          "original_form": "손만 크신거예요 전 닭볶음탕 속 양념 잘 스며든 감자 좋아해요",
          "speaker_id": "2",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100021775.1.12",
          "form": "앜ㅋㅋㅋㅋ오 감자",
          "original_form": "앜ㅋㅋㅋㅋ오 감자",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100021775.1.13",
          "form": "저두요 사리같은거",
          "original_form": "저두요 사리같은거",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100021775.1.14",
          "form": "요즘은 칼칼한 음식이 좋더라구요",
          "original_form": "요즘은 칼칼한 음식이 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100021775.1.15",
          "form": "저도그래요",
          "original_form": "저도그래요",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100021775.1.16",
          "form": "배가 든든해져서 좋아요",
          "original_form": "배가 든든해져서 좋아요",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100021775.1.17",
          "form": "저녁엔 장을 봐왔어요 국거리고기, 볶음면",
          "original_form": "저녁엔 장을 봐왔어요 국거리고기, 볶음면",
          "speaker_id": "1",
          "time": "20210829 18:24"
        }
