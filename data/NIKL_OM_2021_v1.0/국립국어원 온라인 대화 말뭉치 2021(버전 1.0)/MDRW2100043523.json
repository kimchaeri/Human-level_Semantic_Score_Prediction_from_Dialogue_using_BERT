      "utterance": [
        {
          "id": "MDRW2100043523.1.1",
          "form": "내일은 뭐먹지",
          "original_form": "내일은 뭐먹지",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043523.1.2",
          "form": "요거트 왔으니 차치키도 만들어야겠다 조만간",
          "original_form": "요거트 왔으니 차치키도 만들어야겠다 조만간",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043523.1.3",
          "form": "응 내일 오후에 오이사서",
          "original_form": "응 내일 오후에 오이사서",
          "speaker_id": "2",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043523.1.4",
          "form": "만들어야쥐",
          "original_form": "만들어야쥐",
          "speaker_id": "2",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043523.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043523.1.6",
          "form": "차치기 만들어서",
          "original_form": "차치기 만들어서",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043523.1.7",
          "form": "고 추바 사삭",
          "original_form": "고 추바 사삭",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043523.1.8",
          "form": "이랑 먹자ㅋㅋ",
          "original_form": "이랑 먹자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043523.1.9",
          "form": "아빠가 고 추양념치킨이랑 드실듯",
          "original_form": "아빠가 고 추양념치킨이랑 드실듯",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043523.1.10",
          "form": "아아ㅋㅋ",
          "original_form": "아아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043523.1.11",
          "form": "지난번에 맛보시고 마음에 드셨던거 같은데",
          "original_form": "지난번에 맛보시고 마음에 드셨던거 같은데",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043523.1.12",
          "form": "어디든 잘 곁들여 먹음 좋지",
          "original_form": "어디든 잘 곁들여 먹음 좋지",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043523.1.13",
          "form": "다음날 드시려다가 짜찌키 없어서 못드심",
          "original_form": "다음날 드시려다가 짜찌키 없어서 못드심",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043523.1.14",
          "form": "슬픈 얘기군",
          "original_form": "슬픈 얘기군",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043523.1.15",
          "form": "얼른 만들어 드려야겠네",
          "original_form": "얼른 만들어 드려야겠네",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043523.1.16",
          "form": "응ㅋㅋ 내일 내가 만들어드리든가 해야겠어",
          "original_form": "응ㅋㅋ 내일 내가 만들어드리든가 해야겠어",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043523.1.17",
          "form": "크으 원조 요리사님 출동!",
          "original_form": "크으 원조 요리사님 출동!",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043523.1.18",
          "form": "출동하겠습니더",
          "original_form": "출동하겠습니더",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043523.1.19",
          "form": "낼점심이 아마 치킨에 짜찌키 될 가능성 높음",
          "original_form": "낼점심이 아마 치킨에 짜찌키 될 가능성 높음",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043523.1.20",
          "form": "아아 그렇군요",
          "original_form": "아아 그렇군요",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043523.1.21",
          "form": "점심식사 결정!",
          "original_form": "점심식사 결정!",
          "speaker_id": "1",
          "time": "20210907 19:35"
        }
      ]
    }
