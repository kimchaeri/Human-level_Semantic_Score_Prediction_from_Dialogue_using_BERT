      "utterance": [
        {
          "id": "MDRW2100045705.1.1",
          "form": "가격은 잘 모르겠어요!",
          "original_form": "가격은 잘 모르겠어요!",
          "speaker_id": "2",
          "time": "20210908 05:45"
        },
        {
          "id": "MDRW2100045705.1.2",
          "form": "가족들이 과일을 좋아해서 자주 사놓긴 하는데",
          "original_form": "가족들이 과일을 좋아해서 자주 사놓긴 하는데",
          "speaker_id": "2",
          "time": "20210908 05:45"
        },
        {
          "id": "MDRW2100045705.1.3",
          "form": "저는 가아끔 먹어서 과일 시세는 잘 모르겠어욬ㅋㅋ큐ㅠㅠ",
          "original_form": "저는 가아끔 먹어서 과일 시세는 잘 모르겠어욬ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:45"
        },
        {
          "id": "MDRW2100045705.1.4",
          "form": "아보카도나 양배추같은건 오히려 제 손으로 사는데",
          "original_form": "아보카도나 양배추같은건 오히려 제 손으로 사는데",
          "speaker_id": "2",
          "time": "20210908 05:46"
        },
        {
          "id": "MDRW2100045705.1.5",
          "form": "과일 비싸니까 자주 안들여다봐서 얼만지 감도 안와요 ㅠ",
          "original_form": "과일 비싸니까 자주 안들여다봐서 얼만지 감도 안와요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045705.1.6",
          "form": "한개씩도 팔면 좋겠어요",
          "original_form": "한개씩도 팔면 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045705.1.7",
          "form": "맞아요 박스단위로 파니까ㅋㅋㅋㅋ",
          "original_form": "맞아요 박스단위로 파니까ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045705.1.8",
          "form": "감히 살 엄두가 안나욬ㅋㅋㅋㅋㅋ",
          "original_form": "감히 살 엄두가 안나욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045705.1.9",
          "form": "저 혼자 먹으려고 박스 하나를 다 사기도 그렇고",
          "original_form": "저 혼자 먹으려고 박스 하나를 다 사기도 그렇고",
          "speaker_id": "2",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045705.1.10",
          "form": "그나마 가족이랑 같이 사니까 곁다리로 먹을 수 있긴 한데",
          "original_form": "그나마 가족이랑 같이 사니까 곁다리로 먹을 수 있긴 한데",
          "speaker_id": "2",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045705.1.11",
          "form": "저 혼자 살때면 살 엄두도 못냈을 것 같아요",
          "original_form": "저 혼자 살때면 살 엄두도 못냈을 것 같아요",
          "speaker_id": "2",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045705.1.12",
          "form": "전 가족이랑 사는데도 잘 안먹어서 없어요ㅠㅠㅠ",
          "original_form": "전 가족이랑 사는데도 잘 안먹어서 없어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045705.1.13",
          "form": "확실히 그건 가족마다 성향이 다르더라구요ㅠㅠㅠ",
          "original_form": "확실히 그건 가족마다 성향이 다르더라구요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045705.1.14",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045705.1.15",
          "form": "다들 잘먹으면 살텐데 그게아니라 아쉬워요",
          "original_form": "다들 잘먹으면 살텐데 그게아니라 아쉬워요",
          "speaker_id": "1",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045705.1.16",
          "form": "친척집 우유는 배달까지 해드시는데 과일은 불모지였고",
          "original_form": "친척집 우유는 배달까지 해드시는데 과일은 불모지였고",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045705.1.17",
          "form": "과일먹기가 귀찮긴 하더라구요",
          "original_form": "과일먹기가 귀찮긴 하더라구요",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045705.1.18",
          "form": "껍질도 처리해야하고",
          "original_form": "껍질도 처리해야하고",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045705.1.19",
          "form": "저는 씨앗이 제일 싫어요",
          "original_form": "저는 씨앗이 제일 싫어요",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045705.1.20",
          "form": "깨끗하게 먹을수도 없고",
          "original_form": "깨끗하게 먹을수도 없고",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045705.1.21",
          "form": "음쓰로도 못버리고요",
          "original_form": "음쓰로도 못버리고요",
          "speaker_id": "1",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045705.1.22",
          "form": "맛아요ㅠㅠㅠㅠㅠ",
          "original_form": "맛아요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045705.1.23",
          "form": "복숭아 씨 주위에 붙은 과육도 너무 아깝고",
          "original_form": "복숭아 씨 주위에 붙은 과육도 너무 아깝고",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045705.1.24",
          "form": "뜯기는 힘들고",
          "original_form": "뜯기는 힘들고",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045705.1.25",
          "form": "그니까요 어떻게 처리하세요?",
          "original_form": "그니까요 어떻게 처리하세요?",
          "speaker_id": "1",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045705.1.26",
          "form": "수습할 수 있을 만큼 수습해서",
          "original_form": "수습할 수 있을 만큼 수습해서",
          "speaker_id": "2",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045705.1.27",
          "form": "햇볕에 말렸던 것 같아요",
          "original_form": "햇볕에 말렸던 것 같아요",
          "speaker_id": "2",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045705.1.28",
          "form": "그 담은 아마 쓰레기통이였나?",
          "original_form": "그 담은 아마 쓰레기통이였나?",
          "speaker_id": "2",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045705.1.29",
          "form": "저는 물에 담궈서 뿔려가지고 한번더 과육 떼내고 단물 뻐지게하고",
          "original_form": "저는 물에 담궈서 뿔려가지고 한번더 과육 떼내고 단물 뻐지게하고",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100045705.1.30",
          "form": "휴지로 물기닦고",
          "original_form": "휴지로 물기닦고",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045705.1.31",
          "form": "비닐봉지에 밀봉해서 종량제에 버려요",
          "original_form": "비닐봉지에 밀봉해서 종량제에 버려요",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045705.1.32",
          "form": "진짜 굼뱅이 너무 싫어서",
          "original_form": "진짜 굼뱅이 너무 싫어서",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045705.1.33",
          "form": "최대한 난리쳐서 버려요ㅜㅜ 너무힘들지않나요",
          "original_form": "최대한 난리쳐서 버려요ㅜㅜ 너무힘들지않나요",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045705.1.34",
          "form": "맞아요 너무 힘들어요ㅠㅠㅠㅠ",
          "original_form": "맞아요 너무 힘들어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045705.1.35",
          "form": "그래서 잘 안먹는 것도 있구요ㅠㅠㅠㅠㅠ",
          "original_form": "그래서 잘 안먹는 것도 있구요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045705.1.36",
          "form": "아보카도는 깔끔하게 떨어져서 좀 편한데",
          "original_form": "아보카도는 깔끔하게 떨어져서 좀 편한데",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045705.1.37",
          "form": "복숭아는 그 달달함만큼 고난을 맛보게 만들더라구요",
          "original_form": "복숭아는 그 달달함만큼 고난을 맛보게 만들더라구요",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045705.1.38",
          "form": "아보카도는 씨도 이쁘다구요",
          "original_form": "아보카도는 씨도 이쁘다구요",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045705.1.39",
          "form": "동글동글 ㅠ",
          "original_form": "동글동글 ㅠ",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045705.1.40",
          "form": "뭐 붙을일도 없어요",
          "original_form": "뭐 붙을일도 없어요",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045705.1.41",
          "form": "최고야",
          "original_form": "최고야",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045705.1.42",
          "form": "맞아요... 까기도 쉽고",
          "original_form": "맞아요... 까기도 쉽고",
          "speaker_id": "2",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045705.1.43",
          "form": "걍 반으로 둘러둘러 칼집내서 똑 가르면",
          "original_form": "걍 반으로 둘러둘러 칼집내서 똑 가르면",
          "speaker_id": "2",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045705.1.44",
          "form": "한번에 떨어지고 씨앗도 똑 떨어지고",
          "original_form": "한번에 떨어지고 씨앗도 똑 떨어지고",
          "speaker_id": "2",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045705.1.45",
          "form": "껍질도 다 익었다면 쉽게 분리되고",
          "original_form": "껍질도 다 익었다면 쉽게 분리되고",
          "speaker_id": "2",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045705.1.46",
          "form": "그니까요 아보카도는 효자네요",
          "original_form": "그니까요 아보카도는 효자네요",
          "speaker_id": "1",
