      "utterance": [
        {
          "id": "MDRW2100016057.1.1",
          "form": "가면 다사고 싶어요",
          "original_form": "가면 다사고 싶어요",
          "speaker_id": "2",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016057.1.2",
          "form": "요즘 없겠지만 용팔이들 조심하세유ㅋㅋ",
          "original_form": "요즘 없겠지만 용팔이들 조심하세유ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016057.1.3",
          "form": "오 그런가요",
          "original_form": "오 그런가요",
          "speaker_id": "2",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016057.1.4",
          "form": "조심해야 겠어요 ㅜㅜ",
          "original_form": "조심해야 겠어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016057.1.5",
          "form": "아들은 레트로 게임인가 산대요",
          "original_form": "아들은 레트로 게임인가 산대요",
          "speaker_id": "2",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016057.1.6",
          "form": "뭘살지",
          "original_form": "뭘살지",
          "speaker_id": "2",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016057.1.7",
          "form": "레트로!!",
          "original_form": "레트로!!",
          "speaker_id": "1",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016057.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016057.1.9",
          "form": "닌텐도 안들여놓으셨나요ㅋㅋ",
          "original_form": "닌텐도 안들여놓으셨나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016057.1.10",
          "form": "고3이 게임만",
          "original_form": "고3이 게임만",
          "speaker_id": "2",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016057.1.11",
          "form": "없어요 ㅋㅋ",
          "original_form": "없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016057.1.12",
          "form": "와 장성하셨네요",
          "original_form": "와 장성하셨네요",
          "speaker_id": "1",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016057.1.13",
          "form": "존경스러워요",
          "original_form": "존경스러워요",
          "speaker_id": "1",
          "time": "20210826 08:57"
        },
        {
          "id": "MDRW2100016057.1.14",
          "form": "그거있음 공부 아예안해요 ㅋㅋ",
          "original_form": "그거있음 공부 아예안해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016057.1.15",
          "form": "에휴 어릴때가 좋아요",
          "original_form": "에휴 어릴때가 좋아요",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016057.1.16",
          "form": "진정한 실력자는",
          "original_form": "진정한 실력자는",
          "speaker_id": "1",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016057.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016057.1.18",
          "form": "집에서 쉬고 수업 잘듣고ㅋㅋ",
          "original_form": "집에서 쉬고 수업 잘듣고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016057.1.19",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016057.1.20",
          "form": "용산에 큰돈 쓰겠어요",
          "original_form": "용산에 큰돈 쓰겠어요",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016057.1.21",
          "form": "헉 주머니 빵구나서 돌아옵니다",
          "original_form": "헉 주머니 빵구나서 돌아옵니다",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100016057.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016057.1.23",
          "form": "첫째가 좀 아쉽긴 하겠군요",
          "original_form": "첫째가 좀 아쉽긴 하겠군요",
          "speaker_id": "1",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016057.1.24",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016057.1.25",
          "form": "노트북 사줄거라",
          "original_form": "노트북 사줄거라",
          "speaker_id": "2",
          "time": "20210826 08:59"
        },
        {
          "id": "MDRW2100016057.1.26",
