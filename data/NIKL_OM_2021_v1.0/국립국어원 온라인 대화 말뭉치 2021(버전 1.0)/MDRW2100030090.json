      "utterance": [
        {
          "id": "MDRW2100030090.1.1",
          "form": "행복하세요 두 분",
          "original_form": "행복하세요 두 분",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100030090.1.2",
          "form": "주례선생님처럼",
          "original_form": "주례선생님처럼",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030090.1.3",
          "form": "왜그러세요",
          "original_form": "왜그러세요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030090.1.4",
          "form": "결혼 3년만에",
          "original_form": "결혼 3년만에",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030090.1.5",
          "form": "검은머리 파뿌리되서",
          "original_form": "검은머리 파뿌리되서",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030090.1.6",
          "form": "제가 이제 \r\n널 그만 사랑해도 되냐고",
          "original_form": "제가 이제 \r\n널 그만 사랑해도 되냐고",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030090.1.7",
          "form": "물었었네여",
          "original_form": "물었었네여",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030090.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030090.1.9",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030090.1.10",
          "form": "남편한테요?",
          "original_form": "남편한테요?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030090.1.11",
          "form": "그럼 남편한테 묻지",
          "original_form": "그럼 남편한테 묻지",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030090.1.12",
          "form": "누구한테 묻습니꽈",
          "original_form": "누구한테 묻습니꽈",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030090.1.13",
          "form": "그랬더니 모래요?",
          "original_form": "그랬더니 모래요?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030090.1.14",
          "form": "맞았어요",
          "original_form": "맞았어요",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030090.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100030090.1.16",
          "form": "같이 때리셨나요?",
          "original_form": "같이 때리셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100030090.1.17",
          "form": "결혼기념일에 그랬으니",
          "original_form": "결혼기념일에 그랬으니",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030090.1.18",
          "form": "때릴만도했겠다 싶어서",
          "original_form": "때릴만도했겠다 싶어서",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030090.1.19",
          "form": "때리진 않았습니다ㅋㅋㅋㅋㅋ",
          "original_form": "때리진 않았습니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030090.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아니 결혼기념일에",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아니 결혼기념일에",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100030090.1.21",
          "form": "왜 그런소리를,...",
          "original_form": "왜 그런소리를,...",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100030090.1.22",
          "form": "동갑이시죠?",
          "original_form": "동갑이시죠?",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100030090.1.23",
          "form": "ㅋㅋ네:) 저희는 동갑이에요",
          "original_form": "ㅋㅋ네:) 저희는 동갑이에요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030090.1.24",
          "form": "친구같고 좋네요?",
          "original_form": "친구같고 좋네요?",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030090.1.25",
          "form": "친구같은게 아니라 친구에염ㅋㅋ",
          "original_form": "친구같은게 아니라 친구에염ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030090.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 사이가 좋은거 맞죠?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 사이가 좋은거 맞죠?",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030090.1.27",
          "form": "안좋아보이나여??",
          "original_form": "안좋아보이나여??",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030090.1.28",
          "form": "좋아보여요",
          "original_form": "좋아보여요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030090.1.29",
          "form": "장난도치고",
          "original_form": "장난도치고",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030090.1.30",
          "form": "때리고",
          "original_form": "때리고",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030090.1.31",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030090.1.32",
          "form": "다들 좋아보인다 하더라구영ㅋㅋㅋㅋ",
