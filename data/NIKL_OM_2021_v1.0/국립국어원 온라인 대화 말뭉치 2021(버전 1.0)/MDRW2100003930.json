        },
        {
          "id": "MDRW2100003930.1.4",
          "form": "테슬라를 모른다니 그게뭔말이야",
          "original_form": "테슬라를 모른다니 그게뭔말이야",
          "speaker_id": "1",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003930.1.5",
          "form": "테슬라 가끔 봐요",
          "original_form": "테슬라 가끔 봐요",
          "speaker_id": "2",
          "time": "20210821 14:00"
        },
        {
          "id": "MDRW2100003930.1.6",
          "form": "ai 학과는 어디있나요?",
