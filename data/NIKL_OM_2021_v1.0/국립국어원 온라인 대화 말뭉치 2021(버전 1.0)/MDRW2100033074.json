      "utterance": [
        {
          "id": "MDRW2100033074.1.1",
          "form": "게임기나 빔프로젝터 같은 전자기기?",
          "original_form": "게임기나 빔프로젝터 같은 전자기기?",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033074.1.2",
          "form": "나처럼 가난한 학생에게는... 인터넷 잘 되는 노트북",
          "original_form": "나처럼 가난한 학생에게는... 인터넷 잘 되는 노트북",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033074.1.3",
          "form": "근데 빔프로젝터 괜찮은듯",
          "original_form": "근데 빔프로젝터 괜찮은듯",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100033074.1.4",
          "form": "보통 게임이랑 넷플릭스나 왓챠 많이 쓰지",
          "original_form": "보통 게임이랑 넷플릭스나 왓챠 많이 쓰지",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033074.1.5",
          "form": "독서도 놀이가 될 수 있나",
          "original_form": "독서도 놀이가 될 수 있나",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033074.1.6",
          "form": "플레이스테이션 스위치 그런거...",
          "original_form": "플레이스테이션 스위치 그런거...",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033074.1.7",
          "form": "우리 엄마는... 오븐",
          "original_form": "우리 엄마는... 오븐",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033074.1.8",
          "form": "취미니까 괜찮지 않나",
          "original_form": "취미니까 괜찮지 않나",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033074.1.9",
          "form": "요즘은 집에서 골프도 치던데",
          "original_form": "요즘은 집에서 골프도 치던데",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033074.1.10",
          "form": "뭐라고?",
          "original_form": "뭐라고?",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033074.1.11",
          "form": "장난 아니네....",
          "original_form": "장난 아니네....",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033074.1.12",
          "form": "아 홈트를 위한 요가 매트",
          "original_form": "아 홈트를 위한 요가 매트",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033074.1.13",
          "form": "집에서 할 수 있는 거...",
          "original_form": "집에서 할 수 있는 거...",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033074.1.14",
          "form": "요즘 빈백 같은 것도 다 그걸 위한 용품 아닌가ㅋ",
          "original_form": "요즘 빈백 같은 것도 다 그걸 위한 용품 아닌가ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100033074.1.15",
          "form": "그런 거지 ㅋ",
          "original_form": "그런 거지 ㅋ",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100033074.1.16",
          "form": "요즘 그것도 있던데 그... 스크래치북 말고",
          "original_form": "요즘 그것도 있던데 그... 스크래치북 말고",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100033074.1.17",
          "form": "내 룸메가 하던데.... 번호대로 색칠하면 그림 하나 완성되는 거",
          "original_form": "내 룸메가 하던데.... 번호대로 색칠하면 그림 하나 완성되는 거",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100033074.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033074.1.19",
          "form": "맞아 그거랑 보석공예",
          "original_form": "맞아 그거랑 보석공예",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033074.1.20",
          "form": "보석공예요?",
          "original_form": "보석공예요?",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100033074.1.21",
          "form": "아니ㅋ",
          "original_form": "아니ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033074.1.22",
          "form": "그...보석...",
          "original_form": "그...보석...",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033074.1.23",
          "form": "보석 자수",
          "original_form": "보석 자수",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033074.1.24",
          "form": "아하 뭔지 몰라 검색하고 오겠음",
          "original_form": "아하 뭔지 몰라 검색하고 오겠음",
          "speaker_id": "2",
          "time": "20210906 15:44"
        }
