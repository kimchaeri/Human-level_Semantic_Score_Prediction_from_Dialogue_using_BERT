      "utterance": [
        {
          "id": "MDRW2100040560.1.1",
          "form": "팀장은 괜찮은데 과장이 문제예요",
          "original_form": "팀장은 괜찮은데 과장이 문제예요",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040560.1.2",
          "form": "ㅠㅠㅠㅠ 가서 잘때 콧구멍 휴지로 막아줄까요",
          "original_form": "ㅠㅠㅠㅠ 가서 잘때 콧구멍 휴지로 막아줄까요",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040560.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 그러고싶네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 진짜 그러고싶네요",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040560.1.4",
          "form": "일적인거 외에도 트집잡아서 ㅓ",
          "original_form": "일적인거 외에도 트집잡아서 ㅓ",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040560.1.5",
          "form": "진짜 싫어요ㅜ",
          "original_form": "진짜 싫어요ㅜ",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040560.1.6",
          "form": "다 그런가봐요..ㅎ ㅋ..",
          "original_form": "다 그런가봐요..ㅎ ㅋ..",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040560.1.7",
          "form": "저 프사도 간섭 받았어요",
          "original_form": "저 프사도 간섭 받았어요",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040560.1.8",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040560.1.9",
          "form": "진짜 별",
          "original_form": "진짜 별",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040560.1.10",
          "form": "와 프사까지 ᆢ",
          "original_form": "와 프사까지 ᆢ",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040560.1.11",
          "form": "풍경이었는데",
          "original_form": "풍경이었는데",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040560.1.12",
          "form": "진짜 별꼴이네요",
          "original_form": "진짜 별꼴이네요",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040560.1.13",
          "form": "왜 얼굴 안하녜서 그냥 짜증나서 아예 프사 없앴거든요",
          "original_form": "왜 얼굴 안하녜서 그냥 짜증나서 아예 프사 없앴거든요",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040560.1.14",
          "form": "그랬더니 넌 애가 진짜 꽉막혔다",
          "original_form": "그랬더니 넌 애가 진짜 꽉막혔다",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040560.1.15",
          "form": "이러더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "이러더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040560.1.16",
          "form": "진짜 꽉막히신분이 ㅎㅎ",
          "original_form": "진짜 꽉막히신분이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040560.1.17",
          "form": "할 말이 없어서 아... 그런게 아닌데... ㅎㅎ",
          "original_form": "할 말이 없어서 아... 그런게 아닌데... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040560.1.18",
          "form": "이러고 넘겼는데 이미 꽉막힌애로 소문났어요",
          "original_form": "이러고 넘겼는데 이미 꽉막힌애로 소문났어요",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040560.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040560.1.20",
          "form": "그냥 트집잡기위해 사나봐요ㅜ",
          "original_form": "그냥 트집잡기위해 사나봐요ㅜ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040560.1.21",
          "form": "매사 부정적 ᆢ",
          "original_form": "매사 부정적 ᆢ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040560.1.22",
          "form": "그러게요.. 왜 그러고 살까요",
          "original_form": "그러게요.. 왜 그러고 살까요",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040560.1.23",
          "form": "사람 괴롭히면 자기도 기분 나쁘지 않나요?",
          "original_form": "사람 괴롭히면 자기도 기분 나쁘지 않나요?",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040560.1.24",
          "form": "그니까요 그것도 계속같이 볼사인데",
