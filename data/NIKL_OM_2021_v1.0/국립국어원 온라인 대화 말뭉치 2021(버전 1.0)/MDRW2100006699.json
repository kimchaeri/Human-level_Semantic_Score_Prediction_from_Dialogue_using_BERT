      "utterance": [
        {
          "id": "MDRW2100006699.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100006699.1.2",
          "form": "여행 다들 좋아하지 않을까요",
          "original_form": "여행 다들 좋아하지 않을까요",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100006699.1.3",
          "form": "마지막 여행지가 어디에요?",
          "original_form": "마지막 여행지가 어디에요?",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100006699.1.4",
          "form": "제주도요!",
          "original_form": "제주도요!",
          "speaker_id": "2",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100006699.1.5",
          "form": "오 좋았겠다",
          "original_form": "오 좋았겠다",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100006699.1.6",
          "form": "누구랑 가셨어요?",
          "original_form": "누구랑 가셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100006699.1.7",
          "form": "동기들이랑 갔어요",
          "original_form": "동기들이랑 갔어요",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006699.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006699.1.9",
          "form": "저는 지난달말에 부산으로 호캉스다녀왔어요",
          "original_form": "저는 지난달말에 부산으로 호캉스다녀왔어요",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100006699.1.10",
          "form": "헐 완전 부러운데요?",
          "original_form": "헐 완전 부러운데요?",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100006699.1.11",
          "form": "여름휴가로 짧게 다녀왔어요",
          "original_form": "여름휴가로 짧게 다녀왔어요",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100006699.1.12",
          "form": "여름에는 더우니까 호캉스가 짱이죠",
          "original_form": "여름에는 더우니까 호캉스가 짱이죠",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100006699.1.13",
          "form": "네 좋았어요",
          "original_form": "네 좋았어요",
          "speaker_id": "1",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100006699.1.14",
          "form": "맛있는 것도 드셨겠네요",
          "original_form": "맛있는 것도 드셨겠네요",
          "speaker_id": "2",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006699.1.15",
          "form": "거대갈비라고 해운대에 유명한 고깃집있는데",
          "original_form": "거대갈비라고 해운대에 유명한 고깃집있는데",
          "speaker_id": "1",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100006699.1.16",
          "form": "거기 다녀왔었고",
          "original_form": "거기 다녀왔었고",
          "speaker_id": "1",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100006699.1.17",
          "form": "꼬막정찬도 먹었어요",
          "original_form": "꼬막정찬도 먹었어요",
          "speaker_id": "1",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100006699.1.18",
          "form": "체크님은 제주도에서 뭐하셨어요?",
          "original_form": "체크님은 제주도에서 뭐하셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100006699.1.19",
          "form": "오오 이름만 들어도 범상치 않네요",
          "original_form": "오오 이름만 들어도 범상치 않네요",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100006699.1.20",
          "form": "저는 친구들이랑 고기 꾸워먹고...",
          "original_form": "저는 친구들이랑 고기 꾸워먹고...",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100006699.1.21",
          "form": "배타고 그랬던 거 같아요",
          "original_form": "배타고 그랬던 거 같아요",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100006699.1.22",
          "form": "바나나보트?",
          "original_form": "바나나보트?",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006699.1.23",
          "form": "요트?",
          "original_form": "요트?",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006699.1.24",
          "form": "음 모터보트요!",
          "original_form": "음 모터보트요!",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006699.1.25",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006699.1.26",
          "form": "스노쿨링이나 다이빙 하셨나요?",
          "original_form": "스노쿨링이나 다이빙 하셨나요?",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006699.1.27",
          "form": "아뇨 물에 들어가긴 싫어서요ㅠ",
          "original_form": "아뇨 물에 들어가긴 싫어서요ㅠ",
          "speaker_id": "2",
