          "id": "MDRW2100005887.1.31",
          "form": "...?",
          "original_form": "...?",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.32",
          "form": "엫",
          "original_form": "엫",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.33",
          "form": "플라톤의사상에",
          "original_form": "플라톤의사상에",
