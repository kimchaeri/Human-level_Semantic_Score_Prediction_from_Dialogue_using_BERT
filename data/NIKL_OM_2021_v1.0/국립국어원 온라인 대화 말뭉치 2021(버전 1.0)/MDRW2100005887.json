          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005887.1.4",
          "form": "고백록은",
          "original_form": "고백록은",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005887.1.5",
          "form": "근데 너무 옛날에 번역한 버전이라 그런지 눈에 안 들어와..",
          "original_form": "근데 너무 옛날에 번역한 버전이라 그런지 눈에 안 들어와..",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005887.1.6",
          "form": "어떤이야기",
          "original_form": "어떤이야기",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005887.1.7",
          "form": "오 ㅇㅈㅇㅈ",
          "original_form": "오 ㅇㅈㅇㅈ",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005887.1.8",
          "form": "그런책들",
          "original_form": "그런책들",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005887.1.9",
          "form": "있지",
          "original_form": "있지",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005887.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005887.1.11",
          "form": "이 책은...",
          "original_form": "이 책은...",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005887.1.12",
          "form": "예아",
          "original_form": "예아",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005887.1.13",
          "form": "기원후 3세기 철학자 겸 주교가 쓴",
          "original_form": "기원후 3세기 철학자 겸 주교가 쓴",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005887.1.14",
          "form": "본인의 회고록...?",
          "original_form": "본인의 회고록...?",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005887.1.15",
          "form": "오오....",
          "original_form": "오오....",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005887.1.16",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005887.1.17",
          "form": "거의뭐",
          "original_form": "거의뭐",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005887.1.18",
          "form": "일기",
          "original_form": "일기",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005887.1.19",
          "form": "그런건가",
          "original_form": "그런건가",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005887.1.20",
          "form": "약간 서양 역사에서 기독교를 절대적 위치로 올려놓은 책이라 해야 하나...",
          "original_form": "약간 서양 역사에서 기독교를 절대적 위치로 올려놓은 책이라 해야 하나...",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005887.1.21",
          "form": "플라톤의 사상에 천주교의 신 개념을 접목시켜서",
          "original_form": "플라톤의 사상에 천주교의 신 개념을 접목시켜서",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005887.1.22",
          "form": "오오...",
          "original_form": "오오...",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100005887.1.23",
          "form": "기독교를 전파하는데 큰 공헌한 책이라고 해서 읽었는데",
          "original_form": "기독교를 전파하는데 큰 공헌한 책이라고 해서 읽었는데",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.24",
          "form": "오오옹오오",
          "original_form": "오오옹오오",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.25",
          "form": "읽다보니 그냥 본인 일기더라고...",
          "original_form": "읽다보니 그냥 본인 일기더라고...",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.26",
          "form": "어아....",
          "original_form": "어아....",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.27",
          "form": "일기였다...",
          "original_form": "일기였다...",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.28",
          "form": "헐 그럼",
          "original_form": "헐 그럼",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.29",
          "form": "천주교를 이용해서",
          "original_form": "천주교를 이용해서",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005887.1.30",
          "form": "기독교를 전파시킨건가",
          "original_form": "기독교를 전파시킨건가",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
