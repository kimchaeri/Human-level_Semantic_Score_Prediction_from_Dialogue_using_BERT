      "utterance": [
        {
          "id": "MDRW2100013272.1.1",
          "form": "나는 시험 개쉬웠는데 거의다 오픈북이라",
          "original_form": "나는 시험 개쉬웠는데 거의다 오픈북이라",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013272.1.2",
          "form": "수강신청도 주위사람이 다 맞춰서 해줌;",
          "original_form": "수강신청도 주위사람이 다 맞춰서 해줌;",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013272.1.3",
          "form": "나도 오픈북이였음",
          "original_form": "나도 오픈북이였음",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.4",
          "form": "대부분 실습이라",
          "original_form": "대부분 실습이라",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.5",
          "form": "어렵지도않았음",
          "original_form": "어렵지도않았음",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.6",
          "form": "근데 고딩때가 젤 재밌었는데ㅠ",
          "original_form": "근데 고딩때가 젤 재밌었는데ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.7",
          "form": "대학왜갔냐",
          "original_form": "대학왜갔냐",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.8",
          "form": "돈젤아까움",
          "original_form": "돈젤아까움",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.9",
          "form": "난 초딩때ㅋㅋㅋㅋ",
          "original_form": "난 초딩때ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.10",
          "form": "대본만세",
          "original_form": "대본만세",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.11",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.12",
          "form": "돌아가고싶다 초딩때루",
          "original_form": "돌아가고싶다 초딩때루",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.13",
          "form": "초딩땐 잘 기억안남",
          "original_form": "초딩땐 잘 기억안남",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.14",
          "form": "나는 다시 돌아간다면",
          "original_form": "나는 다시 돌아간다면",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.15",
          "form": "대학은 안갈래",
          "original_form": "대학은 안갈래",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013272.1.16",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013272.1.17",
          "form": "대학은",
          "original_form": "대학은",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013272.1.18",
          "form": "진짜 무의미",
          "original_form": "진짜 무의미",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013272.1.19",
          "form": "동아리는 안들었었는데",
          "original_form": "동아리는 안들었었는데",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013272.1.20",
          "form": "동아리먼데",
          "original_form": "동아리먼데",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013272.1.21",
          "form": "동아리가 있었나",
          "original_form": "동아리가 있었나",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013272.1.22",
          "form": "과제도 대구엑스코인가",
          "original_form": "과제도 대구엑스코인가",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.23",
          "form": "근데 고등학교때 동아리했는뎅",
          "original_form": "근데 고등학교때 동아리했는뎅",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.24",
          "form": "거기 직업박람회",
          "original_form": "거기 직업박람회",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.25",
          "form": "독서동아리",
          "original_form": "독서동아리",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.26",
          "form": "갔다오는거였음",
          "original_form": "갔다오는거였음",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.27",
          "form": "재밌겠는디",
          "original_form": "재밌겠는디",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.28",
          "form": "난 동아리없었는데",
          "original_form": "난 동아리없었는데",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.29",
          "form": "근데 여름때라서",
          "original_form": "근데 여름때라서",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.30",
          "form": "뭐가 나쁜말이지",
          "original_form": "뭐가 나쁜말이지",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013272.1.31",
          "form": "난 동아리에서 견학인가 그것도 갔는뎅",
          "original_form": "난 동아리에서 견학인가 그것도 갔는뎅",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013272.1.32",
          "form": "무슨 누구누구생가",
          "original_form": "무슨 누구누구생가",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013272.1.33",
          "form": "기억은 안나지만",
          "original_form": "기억은 안나지만",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013272.1.34",
          "form": "과제는 당일치기가 제 맛",
          "original_form": "과제는 당일치기가 제 맛",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013272.1.35",
          "form": "나는 초딩때 젤기억남았던게3학년때",
          "original_form": "나는 초딩때 젤기억남았던게3학년때",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013272.1.36",
          "form": "담임쌤이 우리 공부시켜놓고 의자에 앉아서 담배핀거",
          "original_form": "담임쌤이 우리 공부시켜놓고 의자에 앉아서 담배핀거",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013272.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013272.1.38",
          "form": "도른사람아냐",
          "original_form": "도른사람아냐",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013272.1.39",
