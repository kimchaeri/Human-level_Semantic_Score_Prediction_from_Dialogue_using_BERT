      "utterance": [
        {
          "id": "MDRW2100006614.1.1",
          "form": "나 콘서트 가고 싶어",
          "original_form": "나 콘서트 가고 싶어",
          "speaker_id": "1",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100006614.1.2",
          "form": "콘서트 가고싶다",
          "original_form": "콘서트 가고싶다",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006614.1.3",
          "form": "콘서트 당장 가고싶다",
          "original_form": "콘서트 당장 가고싶다",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006614.1.4",
          "form": "콘서트...",
          "original_form": "콘서트...",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006614.1.5",
          "form": "세븐틴 콘서트 가고싶어 진짜",
          "original_form": "세븐틴 콘서트 가고싶어 진짜",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006614.1.6",
          "form": "그래도 가면 안돼",
          "original_form": "그래도 가면 안돼",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006614.1.7",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006614.1.8",
          "form": "위험해",
          "original_form": "위험해",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006614.1.9",
          "form": "어짜피 안해 코로나때문에",
          "original_form": "어짜피 안해 코로나때문에",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006614.1.10",
          "form": "이번에 승식이 뮤지컬이랑",
          "original_form": "이번에 승식이 뮤지컬이랑",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.11",
          "form": "광주에서 하면 가던가",
          "original_form": "광주에서 하면 가던가",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.12",
          "form": "도겸이 뮤지컬 했는데",
          "original_form": "도겸이 뮤지컬 했는데",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.13",
          "form": "둘닾못닽어",
          "original_form": "둘닾못닽어",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.14",
          "form": "둘다 못갔어",
          "original_form": "둘다 못갔어",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.15",
          "form": "서울에서 해서",
          "original_form": "서울에서 해서",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.16",
          "form": "도겸이 엑스칼리버 가고싶었는데..",
          "original_form": "도겸이 엑스칼리버 가고싶었는데..",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.17",
          "form": "비엔날레 가",
          "original_form": "비엔날레 가",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.18",
          "form": "우리 남편 도겸이 뮤지컬 하는거 봐야하는데",
          "original_form": "우리 남편 도겸이 뮤지컬 하는거 봐야하는데",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.19",
          "form": "끝났나",
          "original_form": "끝났나",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006614.1.20",
          "form": "멀고 힘들어",
          "original_form": "멀고 힘들어",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006614.1.21",
          "form": "전시회는 가도 될것같은데",
          "original_form": "전시회는 가도 될것같은데",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006614.1.22",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006614.1.23",
          "form": "뮤지컬 !!!",
          "original_form": "뮤지컬 !!!",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006614.1.24",
          "form": "근데 다 서울에서 하니까 못가지",
          "original_form": "근데 다 서울에서 하니까 못가지",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006614.1.25",
          "form": "그냥 걸으면서 보니까",
          "original_form": "그냥 걸으면서 보니까",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006614.1.26",
