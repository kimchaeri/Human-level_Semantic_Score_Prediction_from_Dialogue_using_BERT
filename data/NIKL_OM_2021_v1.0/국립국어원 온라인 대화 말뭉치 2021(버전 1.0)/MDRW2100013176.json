      "utterance": [
        {
          "id": "MDRW2100013176.1.1",
          "form": "차이카드 아세요?",
          "original_form": "차이카드 아세요?",
          "speaker_id": "1",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013176.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013176.1.3",
          "form": "그건 뭐에요?",
          "original_form": "그건 뭐에요?",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013176.1.4",
          "form": "결제하면 번개 적립해주는데",
          "original_form": "결제하면 번개 적립해주는데",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100013176.1.5",
          "form": "오늘 더블 적립이거든요",
          "original_form": "오늘 더블 적립이거든요",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100013176.1.6",
          "form": "아 좋은가요?",
          "original_form": "아 좋은가요?",
          "speaker_id": "2",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100013176.1.7",
          "form": "네 번개 바꿔서 상품 할인 받응 수 있어요",
          "original_form": "네 번개 바꿔서 상품 할인 받응 수 있어요",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100013176.1.8",
          "form": "신용카드에요?",
          "original_form": "신용카드에요?",
          "speaker_id": "2",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100013176.1.9",
          "form": "체크카드요!",
          "original_form": "체크카드요!",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100013176.1.10",
          "form": "어디서 받나요?",
          "original_form": "어디서 받나요?",
          "speaker_id": "2",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100013176.1.11",
          "form": "은행?",
          "original_form": "은행?",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013176.1.12",
          "form": "아뇨 차이카드 쳐서 받는데",
          "original_form": "아뇨 차이카드 쳐서 받는데",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013176.1.13",
          "form": "인기가 엄청 많아서",
          "original_form": "인기가 엄청 많아서",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013176.1.14",
          "form": "몇달 기다려야할거에요..",
          "original_form": "몇달 기다려야할거에요..",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013176.1.15",
          "form": "그 정도로 좋아요?",
          "original_form": "그 정도로 좋아요?",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013176.1.16",
          "form": "옛날엔 좋았는데",
          "original_form": "옛날엔 좋았는데",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013176.1.17",
          "form": "요즘엔 좀 아쉬워요",
          "original_form": "요즘엔 좀 아쉬워요",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100013176.1.18",
          "form": "어디에 쓰세요?",
          "original_form": "어디에 쓰세요?",
          "speaker_id": "2",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100013176.1.19",
          "form": "저는 오늘 그걸로 바디로션 샀어요 ㅎㅎ",
          "original_form": "저는 오늘 그걸로 바디로션 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100013176.1.20",
          "form": "어디꺼여?",
          "original_form": "어디꺼여?",
          "speaker_id": "2",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100013176.1.21",
          "form": "더바디샵이요!",
          "original_form": "더바디샵이요!",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013176.1.22",
          "form": "점점 건조해져서 ㅎㅎ",
          "original_form": "점점 건조해져서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013176.1.23",
          "form": "아 거기 제품 좋죠",
          "original_form": "아 거기 제품 좋죠",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013176.1.24",
          "form": "저도 써요",
          "original_form": "저도 써요",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013176.1.25",
          "form": "저 트레이더스 갔다왔어요",
          "original_form": "저 트레이더스 갔다왔어요",
          "speaker_id": "1",
          "time": "20210827 22:32"
        }
      ]
