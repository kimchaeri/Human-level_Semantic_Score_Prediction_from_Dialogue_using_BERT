      "utterance": [
        {
          "id": "MDRW2100007556.1.1",
          "form": "19군번이면 핸드폰도 사용했어요?",
          "original_form": "19군번이면 핸드폰도 사용했어요?",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007556.1.2",
          "form": "네네 자 딱 자대배치 받는 달에",
          "original_form": "네네 자 딱 자대배치 받는 달에",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007556.1.3",
          "form": "핸드폰 허가됐어요",
          "original_form": "핸드폰 허가됐어요",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007556.1.4",
          "form": "오 좋다",
          "original_form": "오 좋다",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007556.1.5",
          "form": "행복하게 지냈죠",
          "original_form": "행복하게 지냈죠",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007556.1.6",
          "form": "저희는 허가된다는 소문 돌고 될거같은데 하고 못썼어서 ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "저희는 허가된다는 소문 돌고 될거같은데 하고 못썼어서 ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007556.1.7",
          "form": "요즘에 아직 있는 후임들 얘기 들어보면",
          "original_form": "요즘에 아직 있는 후임들 얘기 들어보면",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007556.1.8",
          "form": "핸드폰 연등도 생겼대요",
          "original_form": "핸드폰 연등도 생겼대요",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007556.1.9",
          "form": "오....?",
          "original_form": "오....?",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007556.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 신기하네요 폰연등이라니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 신기하네요 폰연등이라니",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007556.1.11",
          "form": "그쵸? ㅋㅋㅋㅋㅋ가끔 무한연등으로도 받는다고",
          "original_form": "그쵸? ㅋㅋㅋㅋㅋ가끔 무한연등으로도 받는다고",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007556.1.12",
          "form": "하더라고요",
          "original_form": "하더라고요",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007556.1.13",
          "form": "와.........ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 신세계다",
          "original_form": "와.........ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 신세계다",
          "speaker_id": "2",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007556.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이젠 그 소문 돈대요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이젠 그 소문 돈대요",
          "speaker_id": "1",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007556.1.15",
          "form": "국방부에서 병사도 간부랑 머리 길이 똑같이 기르게해준다고",
          "original_form": "국방부에서 병사도 간부랑 머리 길이 똑같이 기르게해준다고",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007556.1.16",
          "form": "안에서 뭔가 얘기가 있나봐요",
          "original_form": "안에서 뭔가 얘기가 있나봐요",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007556.1.17",
          "form": "아 예전에 그런 기사 본 것 같아요",
          "original_form": "아 예전에 그런 기사 본 것 같아요",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007556.1.18",
          "form": "간부형 머리도 허용해야한다 뭐다 하는거",
          "original_form": "간부형 머리도 허용해야한다 뭐다 하는거",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007556.1.19",
          "form": "맞아요 뭔가 안에서 이것저것 바꾸려고 하는 것 같긴하더라고요",
          "original_form": "맞아요 뭔가 안에서 이것저것 바꾸려고 하는 것 같긴하더라고요",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007556.1.20",
          "form": "그래봤자 군대지만 ㅋㅋㅋㅋ",
          "original_form": "그래봤자 군대지만 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007556.1.21",
          "form": "ㅋㅋㅋㅋ 사실 머리길이는 크게 상관없을 것 같아서",
          "original_form": "ㅋㅋㅋㅋ 사실 머리길이는 크게 상관없을 것 같아서",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007556.1.22",
          "form": "ㅋㅋㅋㅋㅋ맞아요 그래봐야 군대라....ㅋㅋㅋ큐ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 그래봐야 군대라....ㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007556.1.23",
          "form": "아아 아직도 동기 분들이랑 연락도 하고 그러세요??",
          "original_form": "아아 아직도 동기 분들이랑 연락도 하고 그러세요??",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007556.1.24",
          "form": "ㅋㅋㅋ 자주는 아닌데 생일이나 이런때 해요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 자주는 아닌데 생일이나 이런때 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007556.1.25",
          "form": "뭔가 일이 있을때?",
          "original_form": "뭔가 일이 있을때?",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007556.1.26",
          "form": "파랑님은 연락 하세요?",
          "original_form": "파랑님은 연락 하세요?",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007556.1.27",
          "form": "오 그래도 관계유지가 계속 되긴하나보네요",
          "original_form": "오 그래도 관계유지가 계속 되긴하나보네요",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007556.1.28",
          "form": "저희는 이제 1년이라 단톡방이 있긴해요 ㅋㅋㅋㅋ",
          "original_form": "저희는 이제 1년이라 단톡방이 있긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007556.1.29",
          "form": "ㅋㅋㅋㅋㅋ 저희도 거의 단톡방으로 연락해요",
          "original_form": "ㅋㅋㅋㅋㅋ 저희도 거의 단톡방으로 연락해요",
