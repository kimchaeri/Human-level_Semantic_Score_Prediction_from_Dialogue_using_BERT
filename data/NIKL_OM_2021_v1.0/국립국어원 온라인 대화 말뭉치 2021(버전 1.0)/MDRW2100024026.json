      "utterance": [
        {
          "id": "MDRW2100024026.1.1",
          "form": "저는 스테이크 좋아해요",
          "original_form": "저는 스테이크 좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024026.1.2",
          "form": "특히 안심....",
          "original_form": "특히 안심....",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024026.1.3",
          "form": "스테이크~",
          "original_form": "스테이크~",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024026.1.4",
          "form": "안심!",
          "original_form": "안심!",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024026.1.5",
          "form": "고기는 주로 어디서 사서드세요?",
          "original_form": "고기는 주로 어디서 사서드세요?",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024026.1.6",
          "form": "마트에서 사서 구워먹어요",
          "original_form": "마트에서 사서 구워먹어요",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024026.1.7",
          "form": "저 그 푸드장이라는 고기 전문 쇼핑몰에서",
          "original_form": "저 그 푸드장이라는 고기 전문 쇼핑몰에서",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024026.1.8",
          "form": "사먹는데",
          "original_form": "사먹는데",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024026.1.9",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024026.1.10",
          "form": "여기 질 좋아요",
          "original_form": "여기 질 좋아요",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024026.1.11",
          "form": "인터넷으로도 구매할 수 있다니",
          "original_form": "인터넷으로도 구매할 수 있다니",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024026.1.12",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024026.1.13",
          "form": "진공포장되어서",
          "original_form": "진공포장되어서",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024026.1.14",
          "form": "아이스팩 빵빵하게 와요",
          "original_form": "아이스팩 빵빵하게 와요",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024026.1.15",
          "form": "오 하긴 저도 최근에 연어장 사먹었어요",
          "original_form": "오 하긴 저도 최근에 연어장 사먹었어요",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024026.1.16",
          "form": "아이디어스에서 사먹었는데 맛있더라고요",
          "original_form": "아이디어스에서 사먹었는데 맛있더라고요",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024026.1.17",
          "form": "아 아이디어스 연어장 그거 저 본것같아요ㅋㅋㅋㅋ",
          "original_form": "아 아이디어스 연어장 그거 저 본것같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024026.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024026.1.19",
          "form": "연어장도 팔다니...!",
          "original_form": "연어장도 팔다니...!",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024026.1.20",
          "form": "유명한거였군요",
          "original_form": "유명한거였군요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024026.1.21",
          "form": "어쩐지 맛있더라니",
          "original_form": "어쩐지 맛있더라니",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100024026.1.22",
          "form": "넹넹 광고하더라구용ㅋㅋㅋㅋ 맛있어서 유명한가봐여",
          "original_form": "넹넹 광고하더라구용ㅋㅋㅋㅋ 맛있어서 유명한가봐여",
          "speaker_id": "1",
          "time": "20210830 21:25"
        }
      ]
