      "utterance": [
        {
          "id": "MDRW2100030920.1.1",
          "form": "머리가 환절기 되면 더 많이 빠지는 것 같아요",
          "original_form": "머리가 환절기 되면 더 많이 빠지는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030920.1.2",
          "form": "머리숱도 적은데 걱정이 된다고요",
          "original_form": "머리숱도 적은데 걱정이 된다고요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.3",
          "form": "저는 언제나 많이 빠져요",
          "original_form": "저는 언제나 많이 빠져요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.4",
          "form": "머리 숱이 걱정이에요",
          "original_form": "머리 숱이 걱정이에요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.5",
          "form": "근데 우리 나이 친구들은 대부분 걱정이에요",
          "original_form": "근데 우리 나이 친구들은 대부분 걱정이에요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.6",
          "form": "저도요 머리 기르면 더 엉켜서 그런지 많이 빠진 기분이어서 기름기 힘들어요",
          "original_form": "저도요 머리 기르면 더 엉켜서 그런지 많이 빠진 기분이어서 기름기 힘들어요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.7",
          "form": "안 되면 가발 구 하지요",
          "original_form": "안 되면 가발 구 하지요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.8",
          "form": "전에는 머리숱이 엄청 많았잖아요",
          "original_form": "전에는 머리숱이 엄청 많았잖아요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.9",
          "form": "가발도 잘 나오니까요",
          "original_form": "가발도 잘 나오니까요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.10",
          "form": "20년 전",
          "original_form": "20년 전",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.11",
          "form": "요즘은 가발도 잘 되어 있죠",
          "original_form": "요즘은 가발도 잘 되어 있죠",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.12",
          "form": "그것도 다 한때더라구요",
          "original_form": "그것도 다 한때더라구요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.13",
          "form": "맞아요 그래서 관리의 중요성을 느끼게 되네요",
          "original_form": "맞아요 그래서 관리의 중요성을 느끼게 되네요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.14",
          "form": "이제는 숱이 많지 않아요",
          "original_form": "이제는 숱이 많지 않아요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030920.1.15",
          "form": "남자면 빡빡이도 괜찮은데",
          "original_form": "남자면 빡빡이도 괜찮은데",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030920.1.16",
          "form": "빠지는게 보일 때마다 걱정이 돼요",
          "original_form": "빠지는게 보일 때마다 걱정이 돼요",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030920.1.17",
          "form": "여자 빡빡이도 괜찮지 않을까요",
          "original_form": "여자 빡빡이도 괜찮지 않을까요",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030920.1.18",
          "form": "머리 감을 때마다 장난 아니죠",
          "original_form": "머리 감을 때마다 장난 아니죠",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030920.1.19",
          "form": "더 있을 수도 있잖아요 편견을 가지지 맙시다",
          "original_form": "더 있을 수도 있잖아요 편견을 가지지 맙시다",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030920.1.20",
          "form": "하루 안 감으면 다음날 더 빠져요",
          "original_form": "하루 안 감으면 다음날 더 빠져요",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030920.1.21",
          "form": "편견 이라기보다는 시선을 받기 싫어요",
          "original_form": "편견 이라기보다는 시선을 받기 싫어요",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030920.1.22",
          "form": "머리 귀찮을 때 특히 주말에 하루 정도 안 감을 때도 많아요",
          "original_form": "머리 귀찮을 때 특히 주말에 하루 정도 안 감을 때도 많아요",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030920.1.23",
          "form": "남이 하는 건 상관 없죠",
          "original_form": "남이 하는 건 상관 없죠",
          "speaker_id": "1",
          "time": "20210904 14:18"
        }
      ]
