      "utterance": [
        {
          "id": "MDRW2100006782.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100006782.1.2",
          "form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "original_form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100006782.1.3",
          "form": "가을이오면 어느 것이 가장 기대되시나요?",
          "original_form": "가을이오면 어느 것이 가장 기대되시나요?",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100006782.1.4",
          "form": "날이 좋으니 드라이브 기대되요",
          "original_form": "날이 좋으니 드라이브 기대되요",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100006782.1.5",
          "form": "드라이브 좋지요",
          "original_form": "드라이브 좋지요",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100006782.1.6",
          "form": "저도 가까운데 다녀올까 생각중이랍니다",
          "original_form": "저도 가까운데 다녀올까 생각중이랍니다",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006782.1.7",
          "form": "코로나로 돌아다니진 못하고 드라이브라도 가고싶네요",
          "original_form": "코로나로 돌아다니진 못하고 드라이브라도 가고싶네요",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006782.1.8",
          "form": "아직은 안심하고 돌아다니기 그렇지요",
          "original_form": "아직은 안심하고 돌아다니기 그렇지요",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006782.1.9",
          "form": "가을 먹거리도 좋아하시나요?",
          "original_form": "가을 먹거리도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006782.1.10",
          "form": "네 불안해서 집에만잇게되네여",
          "original_form": "네 불안해서 집에만잇게되네여",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006782.1.11",
          "form": "그래도 하늘이 예뻐서 나가보고 싶네요",
          "original_form": "그래도 하늘이 예뻐서 나가보고 싶네요",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006782.1.12",
          "form": "오늘 해가 나서 하늘이 보기 좋네요",
          "original_form": "오늘 해가 나서 하늘이 보기 좋네요",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100006782.1.13",
          "form": "네 이제 비는 끝났나봐요",
          "original_form": "네 이제 비는 끝났나봐요",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100006782.1.14",
          "form": "가을 장마가 끝난 듯 해서 좋습니다",
          "original_form": "가을 장마가 끝난 듯 해서 좋습니다",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100006782.1.15",
          "form": "네 정말 비는 지겨웠어요",
          "original_form": "네 정말 비는 지겨웠어요",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100006782.1.16",
          "form": "이제는 높고 푸른 가을 하늘이 나오겠지요",
          "original_form": "이제는 높고 푸른 가을 하늘이 나오겠지요",
          "speaker_id": "1",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100006782.1.17",
          "form": "단풍도 들테고",
          "original_form": "단풍도 들테고",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100006782.1.18",
          "form": "네 가을볕은 타지도 않는다는데 \n드라이브 많이 가야겠어요ㅋ",
          "original_form": "네 가을볕은 타지도 않는다는데 \n드라이브 많이 가야겠어요ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100006782.1.19",
          "form": "날씨가 좋아서 드라이브만 해도 기분 전환이 될 겁니다",
          "original_form": "날씨가 좋아서 드라이브만 해도 기분 전환이 될 겁니다",
          "speaker_id": "1",
          "time": "20210908 09:39"
        }
      ]
