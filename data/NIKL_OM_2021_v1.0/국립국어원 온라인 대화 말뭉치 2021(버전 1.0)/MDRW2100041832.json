      "utterance": [
        {
          "id": "MDRW2100041832.1.1",
          "form": "ㅋㅋㅋㅋㅋ 네 여기 날씨 선선해요",
          "original_form": "ㅋㅋㅋㅋㅋ 네 여기 날씨 선선해요",
          "speaker_id": "1",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041832.1.2",
          "form": "근데 비가 오네요..",
          "original_form": "근데 비가 오네요..",
          "speaker_id": "1",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041832.1.3",
          "form": "여기도요..",
          "original_form": "여기도요..",
          "speaker_id": "2",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041832.1.4",
          "form": "우산챙기셨어요?",
          "original_form": "우산챙기셨어요?",
          "speaker_id": "2",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041832.1.5",
          "form": "네 전 양우산이라서",
          "original_form": "네 전 양우산이라서",
          "speaker_id": "1",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041832.1.6",
          "form": "매일 들고다니거든요ㅋㅋㅋㅋ",
          "original_form": "매일 들고다니거든요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041832.1.7",
          "form": "아하 저도그거들어요",
          "original_form": "아하 저도그거들어요",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041832.1.8",
          "form": "비오는거좋아하세요?",
          "original_form": "비오는거좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041832.1.9",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041832.1.10",
          "form": "전 아뇨 ㅠㅠ",
          "original_form": "전 아뇨 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041832.1.11",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041832.1.12",
          "form": "저도 물젖는거 싫어해요ㅠ",
          "original_form": "저도 물젖는거 싫어해요ㅠ",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041832.1.13",
          "form": "습하죵..",
          "original_form": "습하죵..",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041832.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041832.1.15",
          "form": "분명 쌀쌀한데 에어컨을 찾게되는",
          "original_form": "분명 쌀쌀한데 에어컨을 찾게되는",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041832.1.16",
          "form": "마법쓰",
          "original_form": "마법쓰",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041832.1.17",
          "form": "ㅎㅎㅎ이불덮으면최고죠",
          "original_form": "ㅎㅎㅎ이불덮으면최고죠",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041832.1.18",
          "form": "뽀송한이불",
          "original_form": "뽀송한이불",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041832.1.19",
          "form": "이불 + 에어컨",
          "original_form": "이불 + 에어컨",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041832.1.20",
          "form": "끝짱납니다",
          "original_form": "끝짱납니다",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041832.1.21",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041832.1.22",
          "form": "비그치면가을올것같아요",
          "original_form": "비그치면가을올것같아요",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041832.1.23",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041832.1.24",
          "form": "추석지나고는요!",
          "original_form": "추석지나고는요!",
          "speaker_id": "1",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041832.1.25",
          "form": "ㅎㅎ가을옷꺼내셨나요",
          "original_form": "ㅎㅎ가을옷꺼내셨나요",
          "speaker_id": "2",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100041832.1.26",
          "form": "아직이요 ㅋㅋ",
          "original_form": "아직이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041832.1.27",
          "form": "근데 오늘 긴팔 입었어요",
          "original_form": "근데 오늘 긴팔 입었어요",
