      "utterance": [
        {
          "id": "MDRW2100007493.1.1",
          "form": "근데 요즘 좀 기상이변이 심한게 느껴져요",
          "original_form": "근데 요즘 좀 기상이변이 심한게 느껴져요",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.2",
          "form": "아 맞아 빙하녹고있다그래서ㅠ",
          "original_form": "아 맞아 빙하녹고있다그래서ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.3",
          "form": "환경오염 체감하게되는거같아요",
          "original_form": "환경오염 체감하게되는거같아요",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.4",
          "form": "앞으로 더 더워질거라는게ㅠㅠ",
          "original_form": "앞으로 더 더워질거라는게ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.5",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.6",
          "form": "한국 날씨 동남아따라간다고",
          "original_form": "한국 날씨 동남아따라간다고",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.7",
          "form": "과일도 10년이내로 바뀔거래요",
          "original_form": "과일도 10년이내로 바뀔거래요",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.8",
          "form": "맞아요 이제 비 많이오고",
          "original_form": "맞아요 이제 비 많이오고",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.9",
          "form": "장마만 3개월일거라고",
          "original_form": "장마만 3개월일거라고",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.10",
          "form": "국지성 호우도 심해지고",
          "original_form": "국지성 호우도 심해지고",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007493.1.11",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.12",
          "form": "진짜 경각심 가져야해요ㅠ",
          "original_form": "진짜 경각심 가져야해요ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.13",
          "form": "맞아요ㅜㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.14",
          "form": "근데 이미 돌이킬 수 없댜요",
          "original_form": "근데 이미 돌이킬 수 없댜요",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.15",
          "form": "지금도 죽겠는데 진짜..",
          "original_form": "지금도 죽겠는데 진짜..",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.16",
          "form": "더 심해지면",
          "original_form": "더 심해지면",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.17",
          "form": "더 악화시키지만 않길 바랄뿐이라면서요",
          "original_form": "더 악화시키지만 않길 바랄뿐이라면서요",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.18",
          "form": "진짜요..",
          "original_form": "진짜요..",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.19",
          "form": "환경오염 규제해야해요",
          "original_form": "환경오염 규제해야해요",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007493.1.20",
          "form": "플라스틱도 줄이고 탄소배출도 줄이고",
          "original_form": "플라스틱도 줄이고 탄소배출도 줄이고",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
