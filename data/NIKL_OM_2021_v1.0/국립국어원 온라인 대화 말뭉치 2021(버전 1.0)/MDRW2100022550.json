      "utterance": [
        {
          "id": "MDRW2100022550.1.1",
          "form": "종교있으신가요?",
          "original_form": "종교있으신가요?",
          "speaker_id": "1",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022550.1.2",
          "form": "무교입니다 있으신가여~?",
          "original_form": "무교입니다 있으신가여~?",
          "speaker_id": "2",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022550.1.3",
          "form": "불교인데 거의 안가요",
          "original_form": "불교인데 거의 안가요",
          "speaker_id": "1",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022550.1.4",
          "form": "그럼 신천지에 대해 어케 생각하세요?",
          "original_form": "그럼 신천지에 대해 어케 생각하세요?",
          "speaker_id": "1",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022550.1.5",
          "form": "왜들 저럴까싶어요",
          "original_form": "왜들 저럴까싶어요",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022550.1.6",
          "form": "어차피 사람 만나려고 가는 교회면",
          "original_form": "어차피 사람 만나려고 가는 교회면",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022550.1.7",
          "form": "아무데나 가도 상관없단 생각은 안드시나요?",
          "original_form": "아무데나 가도 상관없단 생각은 안드시나요?",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022550.1.8",
          "form": "근데 목사에 힘인지",
          "original_form": "근데 목사에 힘인지",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022550.1.9",
          "form": "교인들보면 그렇지않더라구요",
          "original_form": "교인들보면 그렇지않더라구요",
          "speaker_id": "2",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022550.1.10",
          "form": "어땠는데요?",
          "original_form": "어땠는데요?",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022550.1.11",
          "form": "이사가도 다니던곳으로 다니는경우도잇더라구요",
          "original_form": "이사가도 다니던곳으로 다니는경우도잇더라구요",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022550.1.12",
          "form": "너무 멀면 힘들겟지만 그들집단에 결속력이",
          "original_form": "너무 멀면 힘들겟지만 그들집단에 결속력이",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022550.1.13",
          "form": "네 제가 말한건 그뜻이 아니구요",
          "original_form": "네 제가 말한건 그뜻이 아니구요",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022550.1.14",
          "form": "어차피 그렇게 결속력으로 다니는거면요 성경도 별로 안읽고요",
          "original_form": "어차피 그렇게 결속력으로 다니는거면요 성경도 별로 안읽고요",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022550.1.15",
          "form": "그냥 어쩌다 신천지에 갔는데 거기 사람들이 잘해주고 소속감 느끼면 다니게될거같거든요",
          "original_form": "그냥 어쩌다 신천지에 갔는데 거기 사람들이 잘해주고 소속감 느끼면 다니게될거같거든요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022550.1.16",
          "form": "그걸 사이비종교라고 욕하기가 좀 애매해보여서요. 어차피 정통교회도 교리 열심히 보는 사람 얼마 안되어보여서요",
          "original_form": "그걸 사이비종교라고 욕하기가 좀 애매해보여서요. 어차피 정통교회도 교리 열심히 보는 사람 얼마 안되어보여서요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022550.1.17",
          "form": "그만큼 기댈곳이 필요한게아닐까요?",
          "original_form": "그만큼 기댈곳이 필요한게아닐까요?",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022550.1.18",
          "form": "사람들이 욕하게된건 모임에서 터졌기 때문이 큰거같아여",
          "original_form": "사람들이 욕하게된건 모임에서 터졌기 때문이 큰거같아여",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022550.1.19",
          "form": "그곳에 시작되서 퍼진게 컸기때문에",
          "original_form": "그곳에 시작되서 퍼진게 컸기때문에",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022550.1.20",
          "form": "네 요즘엔 아무도 관심없더라구요",
          "original_form": "네 요즘엔 아무도 관심없더라구요",
          "speaker_id": "1",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022550.1.21",
          "form": "그쳐 벌써 작년얘기고",
          "original_form": "그쳐 벌써 작년얘기고",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022550.1.22",
          "form": "이제 너무 퍼지고 일어나니 또 저러는구나하고",
          "original_form": "이제 너무 퍼지고 일어나니 또 저러는구나하고",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022550.1.23",
          "form": "그냥 나만 조심하지 이러면서 개인 위생이나 방역에 더 신경쓰는거같아요ㅠ",
          "original_form": "그냥 나만 조심하지 이러면서 개인 위생이나 방역에 더 신경쓰는거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022550.1.24",
          "form": "이거 퍼져서 장사망한 사람들은",
          "original_form": "이거 퍼져서 장사망한 사람들은",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022550.1.25",
          "form": "참 원망스럽겠더라구요",
          "original_form": "참 원망스럽겠더라구요",
          "speaker_id": "1",
          "time": "20210829 15:55"
        }
      ]
    }
  ]
