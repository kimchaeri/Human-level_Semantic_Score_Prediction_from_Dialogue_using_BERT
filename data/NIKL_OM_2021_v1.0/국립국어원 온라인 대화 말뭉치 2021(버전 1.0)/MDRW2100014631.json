      "utterance": [
        {
          "id": "MDRW2100014631.1.1",
          "form": "오늘저녁은 뭐드셨나요??",
          "original_form": "오늘저녁은 뭐드셨나요??",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014631.1.2",
          "form": "저 숯불 고기 덮밥이요",
          "original_form": "저 숯불 고기 덮밥이요",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014631.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014631.1.4",
          "form": "저는 라면먹었엉ㅅ!!",
          "original_form": "저는 라면먹었엉ㅅ!!",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014631.1.5",
          "form": "숯불 고기 덮밥은 직접 해드신건가요??",
          "original_form": "숯불 고기 덮밥은 직접 해드신건가요??",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014631.1.6",
          "form": "아뇨 사서먹었는데 여기",
          "original_form": "아뇨 사서먹었는데 여기",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014631.1.7",
          "form": "육회비빔밥이 맛있더라고요",
          "original_form": "육회비빔밥이 맛있더라고요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014631.1.8",
          "form": "아 라면",
          "original_form": "아 라면",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014631.1.9",
          "form": "진라면인가요",
          "original_form": "진라면인가요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014631.1.10",
          "form": "틀렸엉ㅅ",
          "original_form": "틀렸엉ㅅ",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014631.1.11",
          "form": "어요!",
          "original_form": "어요!",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014631.1.12",
          "form": "열라면입니다",
          "original_form": "열라면입니다",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014631.1.13",
          "form": "아깝다",
          "original_form": "아깝다",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014631.1.14",
          "form": "세글자 중 하나만 맞추면 됐었는데",
          "original_form": "세글자 중 하나만 맞추면 됐었는데",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014631.1.15",
          "form": "라면에",
          "original_form": "라면에",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.16",
          "form": "면 먼저",
          "original_form": "면 먼저",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ거저먹는거 아닌가요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ거저먹는거 아닌가요??",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.18",
          "form": "vs 스프먼저",
          "original_form": "vs 스프먼저",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.19",
          "form": "음.. 저는 면먼저요!",
          "original_form": "음.. 저는 면먼저요!",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.20",
          "form": "님은 뭐먼저 인가요?",
          "original_form": "님은 뭐먼저 인가요?",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.21",
          "form": "물먼저 금지",
          "original_form": "물먼저 금지",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.22",
          "form": "전 스프",
          "original_form": "전 스프",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.23",
          "form": "꼬들면을 위해",
          "original_form": "꼬들면을 위해",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014631.1.24",
          "form": "식초 두방울 정도 넣으면",
          "original_form": "식초 두방울 정도 넣으면",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014631.1.25",
          "form": "꼬들면되는거 아시나요",
          "original_form": "꼬들면되는거 아시나요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014631.1.26",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014631.1.27",
          "form": "그걸 왜 지금 알려주시죠?",
          "original_form": "그걸 왜 지금 알려주시죠?",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014631.1.28",
          "form": "헛살았다",
          "original_form": "헛살았다",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014631.1.29",
          "form": "하지만 앞으로 인생을 찬란하게 살면 된다",
          "original_form": "하지만 앞으로 인생을 찬란하게 살면 된다",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014631.1.30",
          "form": "제덕분에 많은걸 알아가시네요",
          "original_form": "제덕분에 많은걸 알아가시네요",
          "speaker_id": "1",
          "time": "20210825 20:16"
        }
      ]
    }
  ]
