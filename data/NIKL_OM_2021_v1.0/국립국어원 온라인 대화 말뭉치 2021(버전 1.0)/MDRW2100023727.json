      "utterance": [
        {
          "id": "MDRW2100023727.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100023727.1.2",
          "form": "넹 반려동물 키우시나요?",
          "original_form": "넹 반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100023727.1.3",
          "form": "아뇨..ㅠㅠ 키우고싶다는 생각은 많리하고있어요.\r\nname2은요?",
          "original_form": "아뇨..ㅠㅠ 키우고싶다는 생각은 많리하고있어요.\r\n&name2&은요?",
          "speaker_id": "1",
          "time": "20210830 15:46"
        },
        {
          "id": "MDRW2100023727.1.4",
          "form": "name2은 키우시는 동물이 있으세여?",
          "original_form": "&name2&은 키우시는 동물이 있으세여?",
          "speaker_id": "1",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100023727.1.5",
          "form": "저도 키우고 싶어요",
          "original_form": "저도 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100023727.1.6",
          "form": "ㅋㅋㅋ저는 고양이 키우고싶어요..",
          "original_form": "ㅋㅋㅋ저는 고양이 키우고싶어요..",
          "speaker_id": "1",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100023727.1.7",
          "form": "음 굳이 따지면 금붕어...?정돜ㅋㅋㅋㅋ",
          "original_form": "음 굳이 따지면 금붕어...?정돜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100023727.1.8",
          "form": "아 금붕어 키워보셧어요??ㅋㅋㅋ",
          "original_form": "아 금붕어 키워보셧어요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100023727.1.9",
          "form": "헉 고양이 귀엽죠ㅠㅠ",
          "original_form": "헉 고양이 귀엽죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100023727.1.10",
          "form": "귀엽긴하죠 ㅋㅋㅋ",
          "original_form": "귀엽긴하죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100023727.1.11",
          "form": "네네 금붕어 어릴때 키우고 요즘은 안키워요",
          "original_form": "네네 금붕어 어릴때 키우고 요즘은 안키워요",
          "speaker_id": "2",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100023727.1.12",
          "form": "통통하고 색깔도 주황색 검정색이라 귀여워여",
          "original_form": "통통하고 색깔도 주황색 검정색이라 귀여워여",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023727.1.13",
          "form": "오 저도여.. 열대어도 키워보고 병아리도 키워봤러요",
          "original_form": "오 저도여.. 열대어도 키워보고 병아리도 키워봤러요",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023727.1.14",
          "form": "마자여 그냥 보고있으면 평화로워져요",
          "original_form": "마자여 그냥 보고있으면 평화로워져요",
          "speaker_id": "2",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023727.1.15",
          "form": "ㅋㅋㅋㅋ 그런데 비린내가 넘 심해서 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그런데 비린내가 넘 심해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023727.1.16",
          "form": "오 병아리!!! 어릴때 할아버지들이 파는거 많이 봤는데",
          "original_form": "오 병아리!!! 어릴때 할아버지들이 파는거 많이 봤는데",
          "speaker_id": "2",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023727.1.17",
          "form": "예전에 금붕어한테는 미안하지만 \r\n어린이날때 막 금붕어주는 마트도 잇엇어요..",
          "original_form": "예전에 금붕어한테는 미안하지만 \r\n어린이날때 막 금붕어주는 마트도 잇엇어요..",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023727.1.18",
          "form": "맞아요 비린내땜에 물 바꿔주는거 중요해요",
          "original_form": "맞아요 비린내땜에 물 바꿔주는거 중요해요",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023727.1.19",
          "form": "엄마가 금붕어 엄청많이 가져와서 곤란햇던기억이..ㅎ",
          "original_form": "엄마가 금붕어 엄청많이 가져와서 곤란햇던기억이..ㅎ",
          "speaker_id": "1",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023727.1.20",
          "form": "그쳐... 흑 나쁜인긴이네요..",
          "original_form": "그쳐... 흑 나쁜인긴이네요..",
          "speaker_id": "1",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023727.1.21",
          "form": "마트에서 봉다리에 주는거 추억이다",
          "original_form": "마트에서 봉다리에 주는거 추억이다",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023727.1.22",
          "form": "병아리도 마트에서 받아왔는데",
          "original_form": "병아리도 마트에서 받아왔는데",
          "speaker_id": "1",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023727.1.23",
          "form": "헉 그로구나",
          "original_form": "헉 그로구나",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023727.1.24",
          "form": "금방죽더라고요..",
          "original_form": "금방죽더라고요..",
          "speaker_id": "1",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023727.1.25",
          "form": "아무래도ㅠㅠ",
          "original_form": "아무래도ㅠㅠ",
          "speaker_id": "2",
