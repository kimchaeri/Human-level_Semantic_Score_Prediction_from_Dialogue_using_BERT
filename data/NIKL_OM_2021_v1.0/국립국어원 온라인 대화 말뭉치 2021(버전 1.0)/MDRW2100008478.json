      "utterance": [
        {
          "id": "MDRW2100008478.1.1",
          "form": "날이 오락가락하네요~",
          "original_form": "날이 오락가락하네요~",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008478.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008478.1.3",
          "form": "내일은 태풍온대요",
          "original_form": "내일은 태풍온대요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008478.1.4",
          "form": "아 진짜요??? 헐 ㅠㅠㅠㅠㅠㅠㅜ",
          "original_form": "아 진짜요??? 헐 ㅠㅠㅠㅠㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008478.1.5",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008478.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008478.1.7",
          "form": "여름에 비안와서 더워서 어쩌냐 비좀와라.. 할때는 안오더니",
          "original_form": "여름에 비안와서 더워서 어쩌냐 비좀와라.. 할때는 안오더니",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008478.1.8",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008478.1.9",
          "form": "장마가 두번온다는 말이 있었어요",
          "original_form": "장마가 두번온다는 말이 있었어요",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008478.1.10",
          "form": "어멈멈.....",
          "original_form": "어멈멈.....",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008478.1.11",
          "form": "도쿄올림픽할때 태풍온다는 얘기는 들었는데ㅜㅜ",
          "original_form": "도쿄올림픽할때 태풍온다는 얘기는 들었는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008478.1.12",
          "form": "기후위기 문제가 심각해보여요",
          "original_form": "기후위기 문제가 심각해보여요",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008478.1.13",
          "form": "태풍 낼 올건가보더라구요",
          "original_form": "태풍 낼 올건가보더라구요",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008478.1.14",
          "form": "별일없어야할텐데...",
          "original_form": "별일없어야할텐데...",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008478.1.15",
          "form": "아하~~",
          "original_form": "아하~~",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008478.1.16",
          "form": "지난주까지만해도 더웠는데 신기하게 이제 반팔이면 춥기도 하더라구요 ㅋㅋ",
          "original_form": "지난주까지만해도 더웠는데 신기하게 이제 반팔이면 춥기도 하더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008478.1.17",
          "form": "챰 신기..",
          "original_form": "챰 신기..",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008478.1.18",
          "form": "그죠.. 금방 더위가 사라졌어요",
          "original_form": "그죠.. 금방 더위가 사라졌어요",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008478.1.19",
          "form": "더위 많이 타시는 편이세요?",
          "original_form": "더위 많이 타시는 편이세요?",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008478.1.20",
          "form": "네ㅜㅜ 저는 더우면 숨이 안쉬어지더라구요ㅠ",
          "original_form": "네ㅜㅜ 저는 더우면 숨이 안쉬어지더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008478.1.21",
          "form": "더운거 괜찮으세요??",
          "original_form": "더운거 괜찮으세요??",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008478.1.22",
          "form": "저도 더우면 불편하긴 한데요 몇년전에 동남아 일주일 갔다오고나니",
          "original_form": "저도 더우면 불편하긴 한데요 몇년전에 동남아 일주일 갔다오고나니",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008478.1.23",
          "form": "적응이된거같아요",
          "original_form": "적응이된거같아요",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008478.1.24",
          "form": "한증막이려니 하고 참게되더라구요 ㅡㅡ",
          "original_form": "한증막이려니 하고 참게되더라구요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008478.1.25",
          "form": "잠깐 딴소린데 500개 채울 수 있을까요",
          "original_form": "잠깐 딴소린데 500개 채울 수 있을까요",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008478.1.26",
          "form": "동남아는 진짜 덥죠~",
          "original_form": "동남아는 진짜 덥죠~",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008478.1.27",
          "form": "뭐가 나쁜말이지..?",
          "original_form": "뭐가 나쁜말이지..?",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008478.1.28",
          "form": "나쁜말요?",
          "original_form": "나쁜말요?",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008478.1.29",
          "form": "아니에요 ㅎㅎㅎ",
          "original_form": "아니에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008478.1.30",
          "form": "오늘 시원한듯 은근히 습하네요~",
          "original_form": "오늘 시원한듯 은근히 습하네요~",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008478.1.31",
          "form": "네 지역어디세요? 저는 경남이요",
          "original_form": "네 지역어디세요? 저는 경남이요",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008478.1.32",
          "form": "저는 더위는 잘타면서 추위도 잘타요 ㅋㅋㅋ",
          "original_form": "저는 더위는 잘타면서 추위도 잘타요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008478.1.33",
          "form": "서울이요!",
          "original_form": "서울이요!",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008478.1.34",
          "form": "아하 네 ㅋㅋㅋ",
          "original_form": "아하 네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008478.1.35",
          "form": "추위는 잘 견디는 편이세용??",
          "original_form": "추위는 잘 견디는 편이세용??",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008478.1.36",
          "form": "잘 모르겠어요 그냥 참아요 ㅋㅋㅋ",
          "original_form": "잘 모르겠어요 그냥 참아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008478.1.37",
          "form": "여기가 수영장이려니 하고 참습니다 ㅋㅋ",
          "original_form": "여기가 수영장이려니 하고 참습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008478.1.38",
          "form": "저는 추우면 진짜 몸이 너무 떨려서 히트텍이 나온게 신의한수..",
          "original_form": "저는 추우면 진짜 몸이 너무 떨려서 히트텍이 나온게 신의한수..",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008478.1.39",
          "form": "아하 그거 효과좋은가봐요",
          "original_form": "아하 그거 효과좋은가봐요",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008478.1.40",
          "form": "안에 내복도안입고 겨울을 어떻게 났었는지 모르겠어요 ㅋㅋㅋ",
          "original_form": "안에 내복도안입고 겨울을 어떻게 났었는지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008478.1.41",
          "form": "일단 여러겹입어요 ㅋㅋㅋ",
          "original_form": "일단 여러겹입어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008478.1.42",
          "form": "히트텍도 막 두개씩 입어요 ㅋㅋ",
          "original_form": "히트텍도 막 두개씩 입어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008478.1.43",
          "form": "네 ㅋㅋㅋㅋㅋ 많이추우시군요",
          "original_form": "네 ㅋㅋㅋㅋㅋ 많이추우시군요",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008478.1.44",
          "form": "추워요...",
          "original_form": "추워요...",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008478.1.45",
          "form": "서울이 원래이렇게 추웠나 싶을정도로 요즘 겨울엔 진짜 추운거 같아요",
          "original_form": "서울이 원래이렇게 추웠나 싶을정도로 요즘 겨울엔 진짜 추운거 같아요",
          "speaker_id": "1",
          "time": "20210822 16:39"
        }
      ]
