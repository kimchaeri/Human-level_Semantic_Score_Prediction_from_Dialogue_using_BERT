      "utterance": [
        {
          "id": "MDRW2100015242.1.1",
          "form": "ㅠㅁㅠ name1님 먹을거 챙겨드리고 싶다...",
          "original_form": "ㅠㅁㅠ &name1&님 먹을거 챙겨드리고 싶다...",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015242.1.2",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015242.1.3",
          "form": "맛있는거 파는데는 잘 알아요...",
          "original_form": "맛있는거 파는데는 잘 알아요...",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015242.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015242.1.5",
          "form": "챙겨드리고 싶다길래",
          "original_form": "챙겨드리고 싶다길래",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015242.1.6",
          "form": "요리해주는거 좋아하시는줄...",
          "original_form": "요리해주는거 좋아하시는줄...",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015242.1.7",
          "form": "밀키트 조리까지는 가능합니다",
          "original_form": "밀키트 조리까지는 가능합니다",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015242.1.8",
          "form": "그정도면",
          "original_form": "그정도면",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015242.1.9",
          "form": "굶지는 않겠네요 ㅋㅋㅋ",
          "original_form": "굶지는 않겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015242.1.10",
          "form": "진짜 생존을 위한 요리죠ㅋㅋ",
          "original_form": "진짜 생존을 위한 요리죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015242.1.11",
          "form": "제일 자신있는게 모에요?",
          "original_form": "제일 자신있는게 모에요?",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015242.1.12",
          "form": "최근에 해먹었던 감바스 밀키트요...ㅋㅋ",
          "original_form": "최근에 해먹었던 감바스 밀키트요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015242.1.13",
          "form": "밀키트 없는 거로는 김치볶음밥이요",
          "original_form": "밀키트 없는 거로는 김치볶음밥이요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015242.1.14",
          "form": "감바스 맛나겠다",
          "original_form": "감바스 맛나겠다",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015242.1.15",
          "form": "밀키트로해도 맛있죠",
          "original_form": "밀키트로해도 맛있죠",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015242.1.16",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015242.1.17",
          "form": "네 애슐리 밀키트 샀었는데 맛있더라구요",
          "original_form": "네 애슐리 밀키트 샀었는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015242.1.18",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015242.1.19",
          "form": "바게트와 같이",
          "original_form": "바게트와 같이",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015242.1.20",
          "form": "크흐 또 배고파졌어요... 감바스 밀키트 다시 살까봐요",
          "original_form": "크흐 또 배고파졌어요... 감바스 밀키트 다시 살까봐요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015242.1.21",
          "form": "양은 어때요?",
          "original_form": "양은 어때요?",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015242.1.22",
          "form": "많아요?",
          "original_form": "많아요?",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015242.1.23",
          "form": "양은 꽤 됐어요",
          "original_form": "양은 꽤 됐어요",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015242.1.24",
          "form": "저도 사먹어볼까봐요",
          "original_form": "저도 사먹어볼까봐요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015242.1.25",
          "form": "먹는얘기는 참 끝이없죠ㅋㅋㅋ",
          "original_form": "먹는얘기는 참 끝이없죠ㅋㅋㅋ",
