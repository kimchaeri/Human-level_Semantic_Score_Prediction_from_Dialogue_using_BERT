        {
          "id": "MDRW2100047246.1.1",
          "form": "최근 구매하신 물건이 있으신가요?",
          "original_form": "최근 구매하신 물건이 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100047246.1.2",
          "form": "브리타 정수기요",
          "original_form": "브리타 정수기요",
          "speaker_id": "1",
          "time": "20210826 01:19"
        },
        {
          "id": "MDRW2100047246.1.3",
          "form": "어떤 경로로 구매하셨나요?",
          "original_form": "어떤 경로로 구매하셨나요?",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100047246.1.4",
          "form": "온라인에서요",
          "original_form": "온라인에서요",
          "speaker_id": "1",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100047246.1.5",
          "form": "구매 희망 물건이 있으신가요?",
          "original_form": "구매 희망 물건이 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100047246.1.6",
          "form": "노트북사고싶네요 님은요?",
          "original_form": "노트북사고싶네요 님은요?",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100047246.1.7",
          "form": "저는 갤워치요",
          "original_form": "저는 갤워치요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100047246.1.8",
          "form": "중고거래해보셨나요?",
          "original_form": "중고거래해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100047246.1.9",
          "form": "중고거래 몇 번 해봤어요",
          "original_form": "중고거래 몇 번 해봤어요",
          "speaker_id": "2",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100047246.1.10",
          "form": "뭐 팔아보셨어요?",
          "original_form": "뭐 팔아보셨어요?",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100047246.1.11",
          "form": "전공책이랑 화분 팔아봤어요",
          "original_form": "전공책이랑 화분 팔아봤어요",
