      "utterance": [
        {
          "id": "MDRW2100020677.1.1",
          "form": "제일 하기 싫은 집안일이 뭐세요?",
          "original_form": "제일 하기 싫은 집안일이 뭐세요?",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020677.1.2",
          "form": "저는 설거지요..",
          "original_form": "저는 설거지요..",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100020677.1.3",
          "form": "동생이 있는데 꼭 저만 치우네요 ㅎㅎㅠ",
          "original_form": "동생이 있는데 꼭 저만 치우네요 ㅎㅎㅠ",
          "speaker_id": "2",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100020677.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100020677.1.5",
          "form": "저는 욕실청소요...",
          "original_form": "저는 욕실청소요...",
          "speaker_id": "1",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100020677.1.6",
          "form": "비위상해서요ㅠ",
          "original_form": "비위상해서요ㅠ",
          "speaker_id": "1",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100020677.1.7",
          "form": "식기세척기 사고싶으시겠다",
          "original_form": "식기세척기 사고싶으시겠다",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020677.1.8",
          "form": "으악 그게 있었네요",
          "original_form": "으악 그게 있었네요",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020677.1.9",
          "form": "여름이라 그런지 냄새도 잘 나는 것 같아요",
          "original_form": "여름이라 그런지 냄새도 잘 나는 것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020677.1.10",
          "form": "그죠ㅠㅠ 제가 너무 싫어해서",
          "original_form": "그죠ㅠㅠ 제가 너무 싫어해서",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020677.1.11",
          "form": "룸메가 맡아서 해주고있어요",
          "original_form": "룸메가 맡아서 해주고있어요",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020677.1.12",
          "form": "대신 저는 요리를 다 하는것으로 거래성사했어요",
          "original_form": "대신 저는 요리를 다 하는것으로 거래성사했어요",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020677.1.13",
          "form": "아ㅋㅋㅋ좋네요!",
          "original_form": "아ㅋㅋㅋ좋네요!",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100020677.1.14",
          "form": "자취하시는군요",
          "original_form": "자취하시는군요",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100020677.1.15",
          "form": "네 친구랑 살고있어요",
          "original_form": "네 친구랑 살고있어요",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100020677.1.16",
          "form": "가족이랑 같이사시는거죠?",
          "original_form": "가족이랑 같이사시는거죠?",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020677.1.17",
          "form": "부럽네요..",
          "original_form": "부럽네요..",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020677.1.18",
          "form": "네 저는 가족들이랑 살아요",
          "original_form": "네 저는 가족들이랑 살아요",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020677.1.19",
          "form": "자취하면 편한 점이 더 많나요? 어때요?ㅠ",
          "original_form": "자취하면 편한 점이 더 많나요? 어때요?ㅠ",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020677.1.20",
          "form": "자유로운게 좋아요",
          "original_form": "자유로운게 좋아요",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020677.1.21",
          "form": "모든걸 책임져야하고",
          "original_form": "모든걸 책임져야하고",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100020677.1.22",
          "form": "월세가 나간다는게 단점이예요",
          "original_form": "월세가 나간다는게 단점이예요",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100020677.1.23",
          "form": "아하 자유로운 점이 있군요..",
          "original_form": "아하 자유로운 점이 있군요..",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100020677.1.24",
          "form": "저도 준비 좀 되면 자취 해봐야겠어요",
          "original_form": "저도 준비 좀 되면 자취 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100020677.1.25",
          "form": "개인적으로는 돈이 나가도 자취가 좋아요",
          "original_form": "개인적으로는 돈이 나가도 자취가 좋아요",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100020677.1.26",
          "form": "그렇군요ㅋㅋㅠ",
          "original_form": "그렇군요ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100020677.1.27",
          "form": "자기 공간이 있다는 점이 좋을 것 같아요",
          "original_form": "자기 공간이 있다는 점이 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100020677.1.28",
          "form": "그죠 가족들과 억지로 붙어 있지 않아도 된다는게 좋아요",
          "original_form": "그죠 가족들과 억지로 붙어 있지 않아도 된다는게 좋아요",
          "speaker_id": "1",
          "time": "20210828 20:25"
        }
      ]
    }
