          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043535.1.16",
          "form": "슴슴은 취소할게요",
          "original_form": "슴슴은 취소할게요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043535.1.17",
          "form": "아하 ㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043535.1.18",
          "form": "맑은국이 좋죠 ㅎㅎ",
          "original_form": "맑은국이 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043535.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043535.1.20",
          "form": "저도 고춧가루가득보다 맑은 국이 좋아요",
          "original_form": "저도 고춧가루가득보다 맑은 국이 좋아요",
