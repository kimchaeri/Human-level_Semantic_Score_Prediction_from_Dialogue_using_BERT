        },
        {
          "id": "MDRW2100041893.1.2",
          "form": "반려동물있으세요?!",
          "original_form": "반려동물있으세요?!",
          "speaker_id": "1",
          "time": "20210906 17:14"
        },
        {
          "id": "MDRW2100041893.1.3",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041893.1.4",
          "form": "동물은 없는데 ㅋㅋ 구피 키우고 있어요 ㅋ",
          "original_form": "동물은 없는데 ㅋㅋ 구피 키우고 있어요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041893.1.5",
          "form": "반려동물 키우세요",
          "original_form": "반려동물 키우세요",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041893.1.6",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041893.1.7",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041893.1.8",
          "form": "귀여운구피!!",
          "original_form": "귀여운구피!!",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041893.1.9",
          "form": "네 구피 귀여워요 ㅋㅋ",
          "original_form": "네 구피 귀여워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041893.1.10",
          "form": "키우기에도 너무 쉽구요",
          "original_form": "키우기에도 너무 쉽구요",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041893.1.11",
          "form": "몇마리키우세요?!!",
          "original_form": "몇마리키우세요?!!",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041893.1.12",
          "form": "저는 지금 4마리요 ㅋㅋ",
          "original_form": "저는 지금 4마리요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041893.1.13",
          "form": "전에는 수십마리 키웠었는데 감당이 안되더라구요 ㅋ",
          "original_form": "전에는 수십마리 키웠었는데 감당이 안되더라구요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041893.1.14",
          "form": "어항 물갈이해주는데도 손이 막 아프고 그래서 ㅋ",
          "original_form": "어항 물갈이해주는데도 손이 막 아프고 그래서 ㅋ",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041893.1.15",
          "form": "아주 조금만 키우고 있어요",
          "original_form": "아주 조금만 키우고 있어요",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041893.1.16",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041893.1.17",
          "form": "수십마리는 너무 힘들었을것같아요",
          "original_form": "수십마리는 너무 힘들었을것같아요",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041893.1.18",
          "form": "4마리친구들 이름은뭔가요~?",
          "original_form": "4마리친구들 이름은뭔가요~?",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041893.1.19",
          "form": "이름은 안지어줬는데 ㅋㅋㅋ",
          "original_form": "이름은 안지어줬는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041893.1.20",
          "form": "아이들이 그냥 구피라고 해요 ㅋㅋ 물꼬기",
          "original_form": "아이들이 그냥 구피라고 해요 ㅋㅋ 물꼬기",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041893.1.21",
          "form": "이러면서요 ㅋㅋ",
          "original_form": "이러면서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041893.1.22",
          "form": "ㅋㅋㄱㅋㄱ물꼬기ㅠ",
          "original_form": "ㅋㅋㄱㅋㄱ물꼬기ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041893.1.23",
          "form": "반려동물은 키우고싶은생각 있으세요?",
          "original_form": "반려동물은 키우고싶은생각 있으세요?",
