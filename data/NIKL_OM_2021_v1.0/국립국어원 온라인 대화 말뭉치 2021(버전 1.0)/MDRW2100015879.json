        },
        {
          "id": "MDRW2100015879.1.4",
          "form": "아 저도 무교에요ㅋㅋ",
          "original_form": "아 저도 무교에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015879.1.5",
          "form": "종교 가지실 생각 없어요?",
          "original_form": "종교 가지실 생각 없어요?",
          "speaker_id": "2",
          "time": "20210826 02:19"
        },
        {
          "id": "MDRW2100015879.1.6",
          "form": "네 님은요?",
          "original_form": "네 님은요?",
          "speaker_id": "1",
          "time": "20210826 02:19"
        },
        {
          "id": "MDRW2100015879.1.7",
          "form": "저도요 딱히 필요성을 모르겠어서...",
          "original_form": "저도요 딱히 필요성을 모르겠어서...",
          "speaker_id": "2",
          "time": "20210826 02:19"
        },
        {
          "id": "MDRW2100015879.1.8",
          "form": "저는 저를 믿어서 필요없네요",
          "original_form": "저는 저를 믿어서 필요없네요",
          "speaker_id": "1",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015879.1.9",
          "form": "절 가보신적있나요",
