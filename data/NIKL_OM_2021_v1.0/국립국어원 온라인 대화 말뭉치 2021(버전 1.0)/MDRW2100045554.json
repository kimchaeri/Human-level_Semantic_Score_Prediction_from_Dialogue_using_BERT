          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045554.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045554.1.3",
          "form": "돈고라는 단어가 너무 웃겨요",
          "original_form": "돈고라는 단어가 너무 웃겨요",
          "speaker_id": "2",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045554.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아파본적 있으세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아파본적 있으세요?",
          "speaker_id": "1",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100045554.1.5",
          "form": "저는 맨날 매운거 먹으면...",
          "original_form": "저는 맨날 매운거 먹으면...",
          "speaker_id": "2",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100045554.1.6",
          "form": "예..",
          "original_form": "예..",
          "speaker_id": "2",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100045554.1.7",
