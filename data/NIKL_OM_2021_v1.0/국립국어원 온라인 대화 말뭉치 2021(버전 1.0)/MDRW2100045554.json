        },
        {
          "id": "MDRW2100045554.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100045554.1.20",
          "form": "변비보다 더 안좋은거 아세요??",
          "original_form": "변비보다 더 안좋은거 아세요??",
