          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045554.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045554.1.31",
          "form": "배아프신 후에는 좌욕하세요",
          "original_form": "배아프신 후에는 좌욕하세요",
          "speaker_id": "1",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045554.1.32",
