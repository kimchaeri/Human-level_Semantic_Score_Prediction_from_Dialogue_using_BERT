      "utterance": [
        {
          "id": "MDRW2100025861.1.1",
          "form": "걔들 중국어로",
          "original_form": "걔들 중국어로",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025861.1.2",
          "form": "음방에서노래부르던데",
          "original_form": "음방에서노래부르던데",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025861.1.3",
          "form": "웨이션브이",
          "original_form": "웨이션브이",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025861.1.4",
          "form": "그냥중국갔으면좋겠다",
          "original_form": "그냥중국갔으면좋겠다",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025861.1.5",
          "form": "셉도",
          "original_form": "셉도",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025861.1.6",
          "form": "슴이 진짜 답 없어\r\n영어버젼 있는데도 중국어로 음방 돌림",
          "original_form": "슴이 진짜 답 없어\r\n영어버젼 있는데도 중국어로 음방 돌림",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025861.1.7",
          "form": "아니 왜....?",
          "original_form": "아니 왜....?",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025861.1.8",
          "form": "글고 한국음방이 그걸",
          "original_form": "글고 한국음방이 그걸",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025861.1.9",
          "form": "허가하는게 더웃겨",
          "original_form": "허가하는게 더웃겨",
          "speaker_id": "1",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100025861.1.10",
          "form": "맞어 방송국도.... 그걸 왜....",
          "original_form": "맞어 방송국도.... 그걸 왜....",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025861.1.11",
          "form": "아니...",
          "original_form": "아니...",
          "speaker_id": "1",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025861.1.12",
          "form": "그런거못하게 막아야지",
          "original_form": "그런거못하게 막아야지",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025861.1.13",
          "form": "그니깐\r\n심의가 괜히 있냐고....",
          "original_form": "그니깐\r\n심의가 괜히 있냐고....",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025861.1.14",
          "form": "제발 중국몽 버려^_ㅜ",
          "original_form": "제발 중국몽 버려^_ㅜ",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025861.1.15",
          "form": "제발",
          "original_form": "제발",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025861.1.16",
          "form": "심의제발",
          "original_form": "심의제발",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025861.1.17",
          "form": "아니 그나라에서도",
          "original_form": "아니 그나라에서도",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025861.1.18",
          "form": "100프로외국어로 노래하는거 제한하던데",
          "original_form": "100프로외국어로 노래하는거 제한하던데",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025861.1.19",
          "form": "왜우리나라는 그게허가가되는거지",
          "original_form": "왜우리나라는 그게허가가되는거지",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025861.1.20",
          "form": "맞엉...100퍼 영어노래도 띠용이여..",
          "original_form": "맞엉...100퍼 영어노래도 띠용이여..",
          "speaker_id": "2",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100025861.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋ 좀 일관성이있어야지...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋ 좀 일관성이있어야지...",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100025861.1.22",
          "form": "진짜 이러다가 셉도",
          "original_form": "진짜 이러다가 셉도",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100025861.1.23",
          "form": "분리되는거 아냐",
          "original_form": "분리되는거 아냐",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100025861.1.24",
          "form": "팬들은 그걸 원할지도....",
          "original_form": "팬들은 그걸 원할지도....",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025861.1.25",
          "form": "꼴보기싫어",
          "original_form": "꼴보기싫어",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025861.1.26",
          "form": "캐럿도 말만못했지",
          "original_form": "캐럿도 말만못했지",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025861.1.27",
          "form": "다 비슷하게느끼나보던데",
          "original_form": "다 비슷하게느끼나보던데",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025861.1.28",
          "form": "정신머리박혀있는 사람들은 ㅇㅇ....",
          "original_form": "정신머리박혀있는 사람들은 ㅇㅇ....",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025861.1.29",
          "form": "ㅇㅇ....특히 세븐틴은 중멤이 인기멤도 아니지않나...",
          "original_form": "ㅇㅇ....특히 세븐틴은 중멤이 인기멤도 아니지않나...",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100025861.1.30",
          "form": "그리고 걔들중에 누구지",
          "original_form": "그리고 걔들중에 누구지",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025861.1.31",
          "form": "준인가 걔는",
          "original_form": "준인가 걔는",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025861.1.32",
          "form": "좀 분량에서도 소외됐다고하던데",
          "original_form": "좀 분량에서도 소외됐다고하던데",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025861.1.33",
          "form": "거긴 근데... 인원이 하도 많아서",
          "original_form": "거긴 근데... 인원이 하도 많아서",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025861.1.34",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025861.1.35",
          "form": "자기가 감 없으면",
          "original_form": "자기가 감 없으면",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025861.1.36",
          "form": "열셋이라",
          "original_form": "열셋이라",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025861.1.37",
          "form": "걍 소외될수밖에....",
          "original_form": "걍 소외될수밖에....",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025861.1.38",
          "form": "ㅎ....",
          "original_form": "ㅎ....",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025861.1.39",
          "form": "좀 본인이 나서야하는데",
          "original_form": "좀 본인이 나서야하는데",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025861.1.40",
          "form": "그런것도 아닌것같고",
          "original_form": "그런것도 아닌것같고",
