      "utterance": [
        {
          "id": "MDRW2100039800.1.1",
          "form": "직장다니세요?",
          "original_form": "직장다니세요?",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039800.1.2",
          "form": "네 저는 카페에서 일해요",
          "original_form": "네 저는 카페에서 일해요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039800.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039800.1.4",
          "form": "저는 영상쪽이요",
          "original_form": "저는 영상쪽이요",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039800.1.5",
          "form": "요즘 까페 괜찮으신가요?ㅜㅜㅜ",
          "original_form": "요즘 까페 괜찮으신가요?ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039800.1.6",
          "form": "사내카페라 사람이  들끓네요 ㅠㅠ",
          "original_form": "사내카페라 사람이  들끓네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039800.1.7",
          "form": "코로나고 자시고 없다는",
          "original_form": "코로나고 자시고 없다는",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039800.1.8",
          "form": "그래서 백신 빨리 맞으셨구나 ㅜㅜㅜ",
          "original_form": "그래서 백신 빨리 맞으셨구나 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039800.1.9",
          "form": "체인점이예요?",
          "original_form": "체인점이예요?",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039800.1.10",
          "form": "영화쪽? 방송쪽? 유투브?",
          "original_form": "영화쪽? 방송쪽? 유투브?",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039800.1.11",
          "form": "이러닝쪽이요 ㅎㅎ",
          "original_form": "이러닝쪽이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039800.1.12",
          "form": "체인점은 아니에요 ㅎㅎ",
          "original_form": "체인점은 아니에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039800.1.13",
          "form": "개인으로 맞았어요 ㅜㅜ",
          "original_form": "개인으로 맞았어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039800.1.14",
          "form": "코로나가 무서워서",
          "original_form": "코로나가 무서워서",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039800.1.15",
          "form": "백신 부작용보다 더 무서운 코로나",
          "original_form": "백신 부작용보다 더 무서운 코로나",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039800.1.16",
          "form": "그쵸 사람 그렇게 많이 드나들면",
          "original_form": "그쵸 사람 그렇게 많이 드나들면",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039800.1.17",
          "form": "진짜 무서울거같아요",
          "original_form": "진짜 무서울거같아요",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039800.1.18",
          "form": "저희도 사람 드나드는데",
          "original_form": "저희도 사람 드나드는데",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039800.1.19",
          "form": "까페에 비할바는 아니죠ㄷㄷ",
          "original_form": "까페에 비할바는 아니죠ㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039800.1.20",
          "form": "이러닝 뭔지 찾아보고왔어요 ㅋㅋ",
          "original_form": "이러닝 뭔지 찾아보고왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039800.1.21",
          "form": "아 ㅋㅋ 인터넷 강의같은거예요",
          "original_form": "아 ㅋㅋ 인터넷 강의같은거예요",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039800.1.22",
          "form": "촬영을 하시는거게쬬?",
          "original_form": "촬영을 하시는거게쬬?",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039800.1.23",
          "form": "네네 그래서 선생님들 드나들어요",
          "original_form": "네네 그래서 선생님들 드나들어요",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039800.1.24",
          "form": "선생님들은 또 다른업체도 막 다니셔서",
          "original_form": "선생님들은 또 다른업체도 막 다니셔서",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039800.1.25",
          "form": "이분들도 사실 무섭습니다",
          "original_form": "이분들도 사실 무섭습니다",
          "speaker_id": "1",
          "time": "20210903 21:43"
        }
      ]
    }
