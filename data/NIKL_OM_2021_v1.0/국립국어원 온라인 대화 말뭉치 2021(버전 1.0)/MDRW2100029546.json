      "utterance": [
        {
          "id": "MDRW2100029546.1.1",
          "form": "아뇨 게임내에ㅓ",
          "original_form": "아뇨 게임내에ㅓ",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029546.1.2",
          "form": "버는돈을",
          "original_form": "버는돈을",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029546.1.3",
          "form": "투자로 돌려야해요",
          "original_form": "투자로 돌려야해요",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029546.1.4",
          "form": "현금결제도 하시나요",
          "original_form": "현금결제도 하시나요",
          "speaker_id": "1",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100029546.1.5",
          "form": "웬만해ㅓㄴ 안해요",
          "original_form": "웬만해ㅓㄴ 안해요",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029546.1.6",
          "form": "돈아까워요",
          "original_form": "돈아까워요",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029546.1.7",
          "form": "맞아요 공짜게임이 좋죠",
          "original_form": "맞아요 공짜게임이 좋죠",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029546.1.8",
          "form": "예전에 타이니팜 때 조금",
          "original_form": "예전에 타이니팜 때 조금",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029546.1.9",
          "form": "돈썼어요",
          "original_form": "돈썼어요",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029546.1.10",
          "form": "왜 썼나요",
          "original_form": "왜 썼나요",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029546.1.11",
          "form": "농장게임인가요",
          "original_form": "농장게임인가요",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029546.1.12",
          "form": "좋은 동물 키우고싶어서요",
          "original_form": "좋은 동물 키우고싶어서요",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.13",
          "form": "확률게임이여ㅓ오",
          "original_form": "확률게임이여ㅓ오",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.15",
          "form": "현금결제 해야만 재밌는 게임도 있지요",
          "original_form": "현금결제 해야만 재밌는 게임도 있지요",
          "speaker_id": "1",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.16",
          "form": "요즘믄",
          "original_form": "요즘믄",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.17",
          "form": "현질안하려구요",
          "original_form": "현질안하려구요",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.18",
          "form": "카카오게임도 귀엽고요",
          "original_form": "카카오게임도 귀엽고요",
          "speaker_id": "1",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.19",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.20",
          "form": "롤에는 합니다",
          "original_form": "롤에는 합니다",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.21",
          "form": "많이했어요",
          "original_form": "많이했어요",
          "speaker_id": "2",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.22",
          "form": "넷마블 맞고도 좋아했어요",
          "original_form": "넷마블 맞고도 좋아했어요",
          "speaker_id": "1",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029546.1.23",
          "form": "스킨많이샀어요",
          "original_form": "스킨많이샀어요",
          "speaker_id": "2",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100029546.1.24",
          "form": "롤은 현질하는군요",
          "original_form": "롤은 현질하는군요",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100029546.1.25",
          "form": "스킨충이에요",
          "original_form": "스킨충이에요",
          "speaker_id": "2",
          "time": "20210903 10:12"
        }
      ]
