      "utterance": [
        {
          "id": "MDRW2100015221.1.1",
          "form": "ㅇㄴ 종교 없다하셨죠?",
          "original_form": "ㅇㄴ 종교 없다하셨죠?",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015221.1.2",
          "form": "하지만..!!! 민초는 로열입",
          "original_form": "하지만..!!! 민초는 로열입",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015221.1.3",
          "form": "ㅇㄴ",
          "original_form": "ㅇㄴ",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015221.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015221.1.5",
          "form": "부먹 vs 찍먹",
          "original_form": "부먹 vs 찍먹",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015221.1.6",
          "form": "하 난 쩝쩝석사인가봐요",
          "original_form": "하 난 쩝쩝석사인가봐요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015221.1.7",
          "form": "오늘 뭐드셨다 했죠?",
          "original_form": "오늘 뭐드셨다 했죠?",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015221.1.8",
          "form": "크리스마스 때 교회가봤어요?",
          "original_form": "크리스마스 때 교회가봤어요?",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015221.1.9",
          "form": "헉 아뇨",
          "original_form": "헉 아뇨",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015221.1.10",
          "form": "교회 클스마스때 가보셨어요?!",
          "original_form": "교회 클스마스때 가보셨어요?!",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015221.1.11",
          "form": "저는..",
          "original_form": "저는..",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015221.1.12",
          "form": "저 클스마스때 빵집알바해서",
          "original_form": "저 클스마스때 빵집알바해서",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015221.1.13",
          "form": "의지와 상관없이",
          "original_form": "의지와 상관없이",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015221.1.14",
          "form": "아아 집안이 기독교시군요",
          "original_form": "아아 집안이 기독교시군요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015221.1.15",
          "form": "아니요 할머니가.ㅈ.",
          "original_form": "아니요 할머니가.ㅈ.",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015221.1.16",
          "form": "할머니만 기독교셨는데",
          "original_form": "할머니만 기독교셨는데",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.17",
          "form": "워매 할머니따라 집안사람들 다갔나요",
          "original_form": "워매 할머니따라 집안사람들 다갔나요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.18",
          "form": "의지와 상관없이 끌려가서",
          "original_form": "의지와 상관없이 끌려가서",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.20",
          "form": "실로폰 연주했어요",
          "original_form": "실로폰 연주했어요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.21",
          "form": "(환장)",
          "original_form": "(환장)",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.22",
          "form": "할머니 뿌듯하셨겠다",
          "original_form": "할머니 뿌듯하셨겠다",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.23",
          "form": "할머니 '만' 뿌듯하셨군요",
          "original_form": "할머니 '만' 뿌듯하셨군요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.24",
          "form": "그러셨을까요",
          "original_form": "그러셨을까요",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.25",
          "form": "와 실로폰이요??",
          "original_form": "와 실로폰이요??",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.26",
          "form": "그랬던거같기도",
          "original_form": "그랬던거같기도",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.27",
          "form": "그걸로 고요한밤..",
          "original_form": "그걸로 고요한밤..",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.28",
          "form": "고럼요 손주는 숨만쉬어도 뿌듯하대요",
          "original_form": "고럼요 손주는 숨만쉬어도 뿌듯하대요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015221.1.29",
          "form": "거룩한밤..",
          "original_form": "거룩한밤..",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015221.1.30",
          "form": "어둠애...",
          "original_form": "어둠애...",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015221.1.31",
          "form": "묻힌밤..",
          "original_form": "묻힌밤..",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015221.1.32",
          "form": "묻.어.",
          "original_form": "묻.어.",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015221.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
