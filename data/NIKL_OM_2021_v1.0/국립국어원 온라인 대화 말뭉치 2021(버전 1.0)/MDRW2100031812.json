      "utterance": [
        {
          "id": "MDRW2100031812.1.1",
          "form": "반려동물있어?",
          "original_form": "반려동물있어?",
          "speaker_id": "2",
          "time": "20210905 01:14"
        },
        {
          "id": "MDRW2100031812.1.2",
          "form": "영양제 챙겨 먹고 건강하게 우리 고양이와 오래 살아야 해",
          "original_form": "영양제 챙겨 먹고 건강하게 우리 고양이와 오래 살아야 해",
          "speaker_id": "1",
          "time": "20210905 01:14"
        },
        {
          "id": "MDRW2100031812.1.3",
          "form": "고양이 한 마리랑 사는 중이야",
          "original_form": "고양이 한 마리랑 사는 중이야",
          "speaker_id": "1",
          "time": "20210905 01:14"
        },
        {
          "id": "MDRW2100031812.1.4",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210905 01:14"
        },
        {
          "id": "MDRW2100031812.1.5",
          "form": "행복하겠다",
          "original_form": "행복하겠다",
          "speaker_id": "2",
          "time": "20210905 01:14"
        },
        {
          "id": "MDRW2100031812.1.6",
          "form": "이름이뭐야?",
          "original_form": "이름이뭐야?",
          "speaker_id": "2",
          "time": "20210905 01:14"
        },
        {
          "id": "MDRW2100031812.1.7",
          "form": "name3",
          "original_form": "&name3&",
          "speaker_id": "1",
          "time": "20210905 01:16"
        },
        {
          "id": "MDRW2100031812.1.8",
          "form": "다니는 동물병원에 동명이견 삼십 마리는 있는 듯",
          "original_form": "다니는 동물병원에 동명이견 삼십 마리는 있는 듯",
          "speaker_id": "1",
          "time": "20210905 01:16"
        },
        {
          "id": "MDRW2100031812.1.9",
          "form": "아ㅠ name3이",
          "original_form": "아ㅠ &name3&이",
          "speaker_id": "2",
          "time": "20210905 01:17"
        },
        {
          "id": "MDRW2100031812.1.10",
          "form": "턱시도애옹이야?\r\n올검애옹이야?",
          "original_form": "턱시도애옹이야?\r\n올검애옹이야?",
          "speaker_id": "2",
          "time": "20210905 01:17"
        },
        {
          "id": "MDRW2100031812.1.11",
          "form": "삼색이야?",
          "original_form": "삼색이야?",
          "speaker_id": "2",
          "time": "20210905 01:17"
        },
        {
          "id": "MDRW2100031812.1.12",
          "form": "고등어태비얌~~~",
          "original_form": "고등어태비얌~~~",
          "speaker_id": "1",
          "time": "20210905 01:17"
        },
        {
          "id": "MDRW2100031812.1.13",
          "form": "갈색 털에 검정 무늬가 멋드러진 코리안숏헤어지",
          "original_form": "갈색 털에 검정 무늬가 멋드러진 코리안숏헤어지",
          "speaker_id": "1",
          "time": "20210905 01:17"
        },
        {
          "id": "MDRW2100031812.1.14",
          "form": "ㅠ 3개나던졌는데 틀렸네",
          "original_form": "ㅠ 3개나던졌는데 틀렸네",
          "speaker_id": "2",
          "time": "20210905 01:18"
        },
        {
          "id": "MDRW2100031812.1.15",
          "form": "고등어가제일깜찍해",
          "original_form": "고등어가제일깜찍해",
          "speaker_id": "2",
          "time": "20210905 01:18"
        },
        {
          "id": "MDRW2100031812.1.16",
          "form": "내가좋아하는애도 고등어야 사진보낼수있었으면 이미보내는건데,,",
          "original_form": "내가좋아하는애도 고등어야 사진보낼수있었으면 이미보내는건데,,",
          "speaker_id": "2",
          "time": "20210905 01:18"
        },
        {
          "id": "MDRW2100031812.1.17",
          "form": "사진 안 봐도 이미 내 입속에서 주둥이 삼백 번 빨렸을 귀여움인걸...",
          "original_form": "사진 안 봐도 이미 내 입속에서 주둥이 삼백 번 빨렸을 귀여움인걸...",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031812.1.18",
          "form": "그 고등어 친구는 눈여겨보는 길고양이 친구야?",
          "original_form": "그 고등어 친구는 눈여겨보는 길고양이 친구야?",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031812.1.19",
          "form": "어 나 회사에 있던 애옹이야",
          "original_form": "어 나 회사에 있던 애옹이야",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031812.1.20",
          "form": "정말깜찍했어",
          "original_form": "정말깜찍했어",
          "speaker_id": "2",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031812.1.21",
          "form": "회사 복지를 그런 식으로... 비겁한",
          "original_form": "회사 복지를 그런 식으로... 비겁한",
          "speaker_id": "1",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100031812.1.22",
          "form": "그거아니었으면 못버텼을 회사생활",
          "original_form": "그거아니었으면 못버텼을 회사생활",
          "speaker_id": "2",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100031812.1.23",
          "form": "name3이는 애교많아?",
          "original_form": "&name3&이는 애교많아?",
          "speaker_id": "2",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100031812.1.24",
          "form": "우리 집 남자들한테만^^",
          "original_form": "우리 집 남자들한테만^^",
          "speaker_id": "1",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100031812.1.25",
          "form": "남성호르몬에 끌리는 수컷거세고양이^^",
          "original_form": "남성호르몬에 끌리는 수컷거세고양이^^",
          "speaker_id": "1",
          "time": "20210905 01:27"
        },
        {
          "id": "MDRW2100031812.1.26",
          "form": "ㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㅋ사라진땡콩,..",
          "original_form": "ㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㅋ사라진땡콩,..",
          "speaker_id": "2",
          "time": "20210905 01:27"
        },
        {
          "id": "MDRW2100031812.1.27",
          "form": "자신이 잃어버린 것에 대한 갈구일까",
          "original_form": "자신이 잃어버린 것에 대한 갈구일까",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
