      "utterance": [
        {
          "id": "MDRW2100007477.1.1",
          "form": "너 보고 싶은 공연 하나만 골라",
          "original_form": "너 보고 싶은 공연 하나만 골라",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007477.1.2",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007477.1.3",
          "form": "지금 당장은 스위니토드",
          "original_form": "지금 당장은 스위니토드",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007477.1.4",
          "form": "왜 하필 스위니토드야?",
          "original_form": "왜 하필 스위니토드야?",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007477.1.5",
          "form": "내 옆에 주무시고계신분이 예전에 공연을 너무 잘해주셨어.",
          "original_form": "내 옆에 주무시고계신분이 예전에 공연을 너무 잘해주셨어.",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007477.1.6",
          "form": "또해주면좋겠다제발",
          "original_form": "또해주면좋겠다제발",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007477.1.7",
          "form": "네 옆에 주무시고 계신 분이",
          "original_form": "네 옆에 주무시고 계신 분이",
          "speaker_id": "2",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007477.1.8",
          "form": "옥주현이야 조승우야",
          "original_form": "옥주현이야 조승우야",
          "speaker_id": "2",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007477.1.9",
          "form": "헷갈려서 그래",
          "original_form": "헷갈려서 그래",
          "speaker_id": "2",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007477.1.10",
          "form": "나 둘사이에 껴서자는데",
          "original_form": "나 둘사이에 껴서자는데",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007477.1.11",
          "form": "몰랐냐?",
          "original_form": "몰랐냐?",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007477.1.12",
          "form": "몰랐어",
          "original_form": "몰랐어",
          "speaker_id": "2",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007477.1.13",
          "form": "나는 지킬앤하이드도 보고싶어",
          "original_form": "나는 지킬앤하이드도 보고싶어",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007477.1.14",
          "form": "너 뮤지컬 말고 연극은 관심없어?",
          "original_form": "너 뮤지컬 말고 연극은 관심없어?",
          "speaker_id": "2",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007477.1.15",
          "form": "관심은 있는데 아직 발은 안담궜어",
          "original_form": "관심은 있는데 아직 발은 안담궜어",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007477.1.16",
          "form": "넌 있?",
          "original_form": "넌 있?",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007477.1.17",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007477.1.18",
          "form": "나는 히스토리 보이즈라고 있긴 있는데",
          "original_form": "나는 히스토리 보이즈라고 있긴 있는데",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007477.1.19",
          "form": "너",
          "original_form": "너",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007477.1.20",
          "form": "그거당장멈춰라",
          "original_form": "그거당장멈춰라",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007477.1.21",
          "form": "이건 배우 탓이 크고 진짜 관심있는건 살아있는 자를 수선하기",
          "original_form": "이건 배우 탓이 크고 진짜 관심있는건 살아있는 자를 수선하기",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007477.1.22",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007477.1.23",
          "form": "인생을 어디까지 꼴아박으려는거야,",
          "original_form": "인생을 어디까지 꼴아박으려는거야,",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007477.1.24",
          "form": "어차피 그 캐스트 안 돌아와",
          "original_form": "어차피 그 캐스트 안 돌아와",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007477.1.25",
          "form": "연극 뮤지컬에 한 번 빠지면 돌아올 수 없어",
          "original_form": "연극 뮤지컬에 한 번 빠지면 돌아올 수 없어",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007477.1.26",
          "form": "나 방금 비속어 사용할 뻔 했다",
          "original_form": "나 방금 비속어 사용할 뻔 했다",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007477.1.27",
          "form": "우린 이미 글렀어",
          "original_form": "우린 이미 글렀어",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007477.1.28",
          "form": "오늘 비가 오긴 했지",
          "original_form": "오늘 비가 오긴 했지",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007477.1.29",
          "form": "우리라니 동급화하지말아줘",
          "original_form": "우리라니 동급화하지말아줘",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007477.1.30",
          "form": "우리 둘다 특정 뮤지컬 이야기하면 눈물 흘리잖아",
          "original_form": "우리 둘다 특정 뮤지컬 이야기하면 눈물 흘리잖아",
          "speaker_id": "2",
          "time": "20210822 02:30"
        }
      ]
    }
  ]
