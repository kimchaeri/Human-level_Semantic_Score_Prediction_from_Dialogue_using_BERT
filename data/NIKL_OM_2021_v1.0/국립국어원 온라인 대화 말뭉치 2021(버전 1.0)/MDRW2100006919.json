        },
        {
          "id": "MDRW2100006919.1.2",
          "form": "이번 여름에 여행을 다녀오셨나요?",
          "original_form": "이번 여름에 여행을 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006919.1.3",
          "form": "아뇨ㅠㅠ 여행은 못다녀왔아요 ㅠㅠ",
          "original_form": "아뇨ㅠㅠ 여행은 못다녀왔아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006919.1.4",
          "form": "어디 다녀오셨나요?",
          "original_form": "어디 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006919.1.5",
          "form": "저는 강원도에 잠시 다녀왔는데",
          "original_form": "저는 강원도에 잠시 다녀왔는데",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006919.1.6",
          "form": "코로나 때문에 어딜 가도 늘 불안해서",
          "original_form": "코로나 때문에 어딜 가도 늘 불안해서",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006919.1.7",
          "form": "여행이 즐겁기만 하진 않았던 것 같아요",
          "original_form": "여행이 즐겁기만 하진 않았던 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006919.1.8",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006919.1.9",
          "form": "코로나 끝나면 어디를 가장 가고프세요?",
          "original_form": "코로나 끝나면 어디를 가장 가고프세요?",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006919.1.10",
          "form": "한국은 여수",
          "original_form": "한국은 여수",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006919.1.11",
          "form": "해외는 미국에 가고 싶어요!",
          "original_form": "해외는 미국에 가고 싶어요!",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006919.1.12",
          "form": "와 저도 여수 가고 싶다는 생각을 몇 년째 하고 있는데 쉽지가 않아요",
          "original_form": "와 저도 여수 가고 싶다는 생각을 몇 년째 하고 있는데 쉽지가 않아요",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006919.1.13",
          "form": "어렸을 때 가족이랑 향일암에서 새해 일출을 봤던 추억 때문에 다시 가보고픈데",
          "original_form": "어렸을 때 가족이랑 향일암에서 새해 일출을 봤던 추억 때문에 다시 가보고픈데",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006919.1.14",
          "form": "언제쯤 갈 수 있을지 ㅎㅎ",
          "original_form": "언제쯤 갈 수 있을지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006919.1.15",
          "form": "미국은 가면 뭐 하고 싶으세요? 저는 디즈니월드!",
          "original_form": "미국은 가면 뭐 하고 싶으세요? 저는 디즈니월드!",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006919.1.16",
          "form": "여수에 향일암이라는 곳이 있군요!",
          "original_form": "여수에 향일암이라는 곳이 있군요!",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006919.1.17",
          "form": "저는 미국 동부쪽에는 여행해본적이 있어거",
          "original_form": "저는 미국 동부쪽에는 여행해본적이 있어거",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006919.1.18",
          "form": "서부쪽에 가보고싶어요!",
          "original_form": "서부쪽에 가보고싶어요!",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006919.1.19",
          "form": "물론 디즈니월드 꼭 가야죠 ㅠㅠㅠㅠ",
          "original_form": "물론 디즈니월드 꼭 가야죠 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006919.1.20",
          "form": "오 저는 서부만 가봤어요 동부 궁금해요",
          "original_form": "오 저는 서부만 가봤어요 동부 궁금해요",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006919.1.21",
          "form": "캘리는 정말 온난하더라고요",
          "original_form": "캘리는 정말 온난하더라고요",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006919.1.22",
          "form": "여기 살면 정말 긍정긍정한 캘리걸이 되겠구나... 싶었죠 ㅋㅋㅋ",
          "original_form": "여기 살면 정말 긍정긍정한 캘리걸이 되겠구나... 싶었죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006919.1.23",
          "form": "ㅋㅋㅋㅋ오 완전 반대네요",
          "original_form": "ㅋㅋㅋㅋ오 완전 반대네요",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006919.1.24",
          "form": "인앤아웃 햄버거 맛있어요!!!!!!!",
          "original_form": "인앤아웃 햄버거 맛있어요!!!!!!!",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006919.1.25",
          "form": "오 맞아요 저도 가서 먹었어요 ㅋㅋ",
          "original_form": "오 맞아요 저도 가서 먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006919.1.26",
          "form": "그건 아직 안 들어왔죠? 언젠가 들어오려나 쉑쉑처럼",
          "original_form": "그건 아직 안 들어왔죠? 언젠가 들어오려나 쉑쉑처럼",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006919.1.27",
          "form": "ㅋㅋㅋ네 인앤아웃은 아직 안들어왔어요",
          "original_form": "ㅋㅋㅋ네 인앤아웃은 아직 안들어왔어요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006919.1.28",
          "form": "한국에 만약 들어오면 땅콩도 줄까요?ㅋㅋㅋㅋ",
          "original_form": "한국에 만약 들어오면 땅콩도 줄까요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006919.1.29",
          "form": "엇 땅콩도 나왔었구나! 사실 맛은 기억이 안 나요 그냥 갔단 것만 기억에 남을 뿐 ㅋㅋㅋ",
          "original_form": "엇 땅콩도 나왔었구나! 사실 맛은 기억이 안 나요 그냥 갔단 것만 기억에 남을 뿐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006919.1.30",
          "form": "여행은 동행 있는 것과 혼자 중 뭘 더 좋아하세요?",
          "original_form": "여행은 동행 있는 것과 혼자 중 뭘 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006919.1.31",
          "form": "음 전 혼자 있는걸 좋아하는 편인데",
          "original_form": "음 전 혼자 있는걸 좋아하는 편인데",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006919.1.32",
          "form": "평소에 혼자 있어서 그런지",
          "original_form": "평소에 혼자 있어서 그런지",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006919.1.33",
          "form": "여행은 동행이 있는게 좋은 것 같아요!",
          "original_form": "여행은 동행이 있는게 좋은 것 같아요!",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006919.1.34",
          "form": "오 저랑 비슷하시네요 ㅎㅎ",
          "original_form": "오 저랑 비슷하시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006919.1.35",
          "form": "엥 뭐지",
          "original_form": "엥 뭐지",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100006919.1.36",
          "form": "여튼 그게 동행 최대 매력 같아요",
          "original_form": "여튼 그게 동행 최대 매력 같아요",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100006919.1.37",
          "form": "추천해주고싶은 여행지 있으신가요~?",
          "original_form": "추천해주고싶은 여행지 있으신가요~?",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100006919.1.38",
          "form": "저는 국내는 경주랑 평창이 좋아요",
          "original_form": "저는 국내는 경주랑 평창이 좋아요",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100006919.1.39",
          "form": "경주는 정말 멋진 도시 같아요 어른 되고 가보니 초딩 수학여행 때랑은 전혀 다른 감동 ㅠㅠ",
          "original_form": "경주는 정말 멋진 도시 같아요 어른 되고 가보니 초딩 수학여행 때랑은 전혀 다른 감동 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100006919.1.40",
          "form": "혹시 경주 어디가 좋으셨나요?",
          "original_form": "혹시 경주 어디가 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100006919.1.41",
          "form": "뻔하지만 불국사랑 안압지요 ㅎㅎ",
          "original_form": "뻔하지만 불국사랑 안압지요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100006919.1.42",
          "form": "안압지 저도 좋았어요",
          "original_form": "안압지 저도 좋았어요",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100006919.1.43",
          "form": "야경이 진짜...",
          "original_form": "야경이 진짜...",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100006919.1.44",
          "form": "감동이더라구요 ㅠㅠㅠ",
          "original_form": "감동이더라구요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100006919.1.45",
          "form": "거기에서 놀면",
          "original_form": "거기에서 놀면",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100006919.1.46",
          "form": "얼마나 흥이 절로 났을까 싶어요",
          "original_form": "얼마나 흥이 절로 났을까 싶어요",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100006919.1.47",
          "form": "ㅋㅋㅋㅋ술이 절로 들어갔을 것 같아요",
          "original_form": "ㅋㅋㅋㅋ술이 절로 들어갔을 것 같아요",
          "speaker_id": "2",
          "time": "20210822 00:45"
        }
      ]
