      "utterance": [
        {
          "id": "MDRW2100020786.1.1",
          "form": "나도 전혀 생각 못한 특성이라",
          "original_form": "나도 전혀 생각 못한 특성이라",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020786.1.2",
          "form": "요리하긴 편할듯 ㅋㅋㅋㅋ",
          "original_form": "요리하긴 편할듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020786.1.3",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 얘는 설정이 그럼",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 얘는 설정이 그럼",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020786.1.4",
          "form": "억케되지",
          "original_form": "억케되지",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020786.1.5",
          "form": "스네즈나야 출신 되는건가?",
          "original_form": "스네즈나야 출신 되는건가?",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020786.1.6",
          "form": "일단 몬드로 분류되어있대",
          "original_form": "일단 몬드로 분류되어있대",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020786.1.7",
          "form": "정확한건 게임 업데이트 돼봐야 할듯",
          "original_form": "정확한건 게임 업데이트 돼봐야 할듯",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020786.1.8",
          "form": "오 그럼",
          "original_form": "오 그럼",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020786.1.9",
          "form": "아예 드래곤스파인 출신되는건가?",
          "original_form": "아예 드래곤스파인 출신되는건가?",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020786.1.10",
          "form": "그럴지도?",
          "original_form": "그럴지도?",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020786.1.11",
          "form": "모션이랑 의상 봤는데",
          "original_form": "모션이랑 의상 봤는데",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020786.1.12",
          "form": "일단 맨다리 아니고 옷 톤도 어두워셔",
          "original_form": "일단 맨다리 아니고 옷 톤도 어두워셔",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020786.1.13",
          "form": "일러보다는 괜찮아보임",
          "original_form": "일러보다는 괜찮아보임",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020786.1.14",
          "form": "ㅇㅇ그치만 원작에 비해 너무 가느다랗긴 하더라",
          "original_form": "ㅇㅇ그치만 원작에 비해 너무 가느다랗긴 하더라",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020786.1.15",
          "form": "그래도 이게 어디야",
          "original_form": "그래도 이게 어디야",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020786.1.16",
          "form": "좀 쓸만하면 좋겠다",
          "original_form": "좀 쓸만하면 좋겠다",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020786.1.17",
          "form": "그럼 차라리 소를 향한 미련을 좀 덜어낼텐데",
          "original_form": "그럼 차라리 소를 향한 미련을 좀 덜어낼텐데",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020786.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020786.1.19",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020786.1.20",
          "form": "성능 나쁘지 않아보이는데",
          "original_form": "성능 나쁘지 않아보이는데",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020786.1.21",
          "form": "감우의 대체재고 소랑은 다른 계열인듯",
          "original_form": "감우의 대체재고 소랑은 다른 계열인듯",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020786.1.22",
          "form": "ㅇㅏ근데 대검이나 장병기캐 필요한데",
          "original_form": "ㅇㅏ근데 대검이나 장병기캐 필요한데",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020786.1.23",
          "form": "지금 딜에 약간",
          "original_form": "지금 딜에 약간",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020786.1.24",
          "form": "벽느끼는 중이라",
          "original_form": "벽느끼는 중이라",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020786.1.25",
          "form": "뭔가 탱커나 근딜?",
          "original_form": "뭔가 탱커나 근딜?",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020786.1.26",
          "form": "ㅇㅇ.....",
          "original_form": "ㅇㅇ.....",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020786.1.27",
          "form": "4성 한손검캐들만",
          "original_form": "4성 한손검캐들만",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020786.1.28",
          "form": "꽉차가지고",
          "original_form": "꽉차가지고",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020786.1.29",
          "form": "딜 모자라....",
          "original_form": "딜 모자라....",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020786.1.30",
          "form": "바위츄츄폭도 방패를 못깨",
          "original_form": "바위츄츄폭도 방패를 못깨",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020786.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020786.1.32",
          "form": "얘 특성이 빙결인데",
          "original_form": "얘 특성이 빙결인데",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020786.1.33",
          "form": "직접 써봐야겠지만",
          "original_form": "직접 써봐야겠지만",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020786.1.34",
          "form": "난 설마",
          "original_form": "난 설마",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020786.1.35",
          "form": "꽤 강력해보임",
          "original_form": "꽤 강력해보임",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020786.1.36",
          "form": "무조건 동결상태인가 했네",
          "original_form": "무조건 동결상태인가 했네",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020786.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020786.1.38",
          "form": "그럼 ㄹㅇ개사긴데",
          "original_form": "그럼 ㄹㅇ개사긴데",
          "speaker_id": "2",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020786.1.39",
          "form": "그정돈 아닐듯",
          "original_form": "그정돈 아닐듯",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020786.1.40",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020786.1.41",
          "form": "난 원소 단일에도 좀 효과 있었으면 좋겠어",
          "original_form": "난 원소 단일에도 좀 효과 있었으면 좋겠어",
          "speaker_id": "2",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020786.1.42",
          "form": "얼음원소 뭍으면 좀 느려진다던가",
          "original_form": "얼음원소 뭍으면 좀 느려진다던가",
          "speaker_id": "2",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020786.1.43",
          "form": "그거 있어",
          "original_form": "그거 있어",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020786.1.44",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "2",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020786.1.45",
          "form": "근데 별로",
          "original_form": "근데 별로",
          "speaker_id": "2",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020786.1.46",
          "form": "얼음 뭍으면 지금도 느려지지 않아ㅡ",
          "original_form": "얼음 뭍으면 지금도 느려지지 않아ㅡ",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020786.1.47",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020786.1.48",
          "form": "체감은 안돼",
          "original_form": "체감은 안돼",
          "speaker_id": "2",
          "time": "20210828 12:41"
        }
      ]
