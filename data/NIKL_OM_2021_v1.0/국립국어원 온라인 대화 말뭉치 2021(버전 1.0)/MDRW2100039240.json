      "utterance": [
        {
          "id": "MDRW2100039240.1.1",
          "form": "요즘.... 유튜브 뭐보시나용...",
          "original_form": "요즘.... 유튜브 뭐보시나용...",
          "speaker_id": "1",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100039240.1.2",
          "form": "다꾸....영상......",
          "original_form": "다꾸....영상......",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039240.1.3",
          "form": "애니 노래 작업송으로 틀어놓기...",
          "original_form": "애니 노래 작업송으로 틀어놓기...",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100039240.1.4",
          "form": "다꾸.... 전 다꾸 영상 보면 약간",
          "original_form": "다꾸.... 전 다꾸 영상 보면 약간",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039240.1.5",
          "form": "마법? 같은 느낌이에요 먼가...너무 대단해",
          "original_form": "마법? 같은 느낌이에요 먼가...너무 대단해",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039240.1.6",
          "form": "전 요즘 그알 열심히 본당....",
          "original_form": "전 요즘 그알 열심히 본당....",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039240.1.7",
          "form": "요즘 그알 무슨 에피가 젤 재밌었어요",
          "original_form": "요즘 그알 무슨 에피가 젤 재밌었어요",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039240.1.8",
          "form": "저 앉아서 그알 본 마지막이 한강 사건이었던 것 같아",
          "original_form": "저 앉아서 그알 본 마지막이 한강 사건이었던 것 같아",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039240.1.9",
          "form": "전 요즘 그알 안 보구... 예전 에피들 긁어모아 보는 거 조아해요ㅋㅋㅋㅋ인기많은 거...",
          "original_form": "전 요즘 그알 안 보구... 예전 에피들 긁어모아 보는 거 조아해요ㅋㅋㅋㅋ인기많은 거...",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039240.1.10",
          "form": "오늘도 그...머시기냐 제주 청부살인사건?",
          "original_form": "오늘도 그...머시기냐 제주 청부살인사건?",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039240.1.11",
          "form": "그거 봤네용...",
          "original_form": "그거 봤네용...",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039240.1.12",
          "form": "그알 때매 범인이 잡혔대용",
          "original_form": "그알 때매 범인이 잡혔대용",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039240.1.13",
          "form": "아아 그거 뉴스 봤어 그알 다시 해주는 것도 잠깐 봤는데",
          "original_form": "아아 그거 뉴스 봤어 그알 다시 해주는 것도 잠깐 봤는데",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039240.1.14",
          "form": "??? 싶더라고 얘 방송 나와서 자백하는 건가 하고",
          "original_form": "??? 싶더라고 얘 방송 나와서 자백하는 건가 하고",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039240.1.15",
          "form": "그니까요 보면서도 깜짝 놀랐는데",
          "original_form": "그니까요 보면서도 깜짝 놀랐는데",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039240.1.16",
          "form": "이게 방송 나갔었다고? 하면서ㅋㅋㅋㅋ",
          "original_form": "이게 방송 나갔었다고? 하면서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039240.1.17",
          "form": "다행이에요 잡혀서...",
          "original_form": "다행이에요 잡혀서...",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100039240.1.18",
          "form": "애니노래는 머 들어용? 늘 듣던 걸루?",
          "original_form": "애니노래는 머 들어용? 늘 듣던 걸루?",
          "speaker_id": "1",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100039240.1.19",
          "form": "요즘은 주술이랑 다이에이 돌림노래인 듯",
          "original_form": "요즘은 주술이랑 다이에이 돌림노래인 듯",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100039240.1.20",
          "form": "활발한 에너지..다이에이",
          "original_form": "활발한 에너지..다이에이",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100039240.1.21",
          "form": "대충 슬프고 힘든 이미지...주술",
          "original_form": "대충 슬프고 힘든 이미지...주술",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100039240.1.22",
          "form": "ㅋㅋㅋㅋㅋ주술 애니도 있구나...",
          "original_form": "ㅋㅋㅋㅋㅋ주술 애니도 있구나...",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039240.1.23",
          "form": "당연한가",
          "original_form": "당연한가",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039240.1.24",
          "form": "노래 함 들어봐야겠어요",
          "original_form": "노래 함 들어봐야겠어요",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039240.1.25",
          "form": "주술 먼가 도쿄구울 느낌 나는 장르라고",
          "original_form": "주술 먼가 도쿄구울 느낌 나는 장르라고",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039240.1.26",
          "form": "혼자 생갇하고 있었던...",
          "original_form": "혼자 생갇하고 있었던...",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039240.1.27",
          "form": "사람이 대충 못볼꼴로 죽는단 점에서 비슷할지도...(?",
          "original_form": "사람이 대충 못볼꼴로 죽는단 점에서 비슷할지도...(?",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039240.1.28",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ그렇군....고어틱한 느낌인가요 역시...",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ그렇군....고어틱한 느낌인가요 역시...",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100039240.1.29",
          "form": "오프닝엔딩 잘뽑았어..",
          "original_form": "오프닝엔딩 잘뽑았어..",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039240.1.30",
          "form": "그런데 이제 2쿨 엔딩은 들으면 눈물나는",
          "original_form": "그런데 이제 2쿨 엔딩은 들으면 눈물나는",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039240.1.31",
