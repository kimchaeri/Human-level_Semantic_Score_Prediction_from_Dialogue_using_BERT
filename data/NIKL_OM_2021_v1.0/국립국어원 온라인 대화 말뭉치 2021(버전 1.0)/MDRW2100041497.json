      "utterance": [
        {
          "id": "MDRW2100041497.1.1",
          "form": "물은 마셔줘야 혈액순환이 잘 된대요",
          "original_form": "물은 마셔줘야 혈액순환이 잘 된대요",
          "speaker_id": "2",
          "time": "20210906 06:58"
        },
        {
          "id": "MDRW2100041497.1.2",
          "form": "저도 일어나자마자 물 마시고",
          "original_form": "저도 일어나자마자 물 마시고",
          "speaker_id": "2",
          "time": "20210906 06:58"
        },
        {
          "id": "MDRW2100041497.1.3",
          "form": "이제 아침 먹으려고 하고 있습니다",
          "original_form": "이제 아침 먹으려고 하고 있습니다",
          "speaker_id": "2",
          "time": "20210906 06:58"
        },
        {
          "id": "MDRW2100041497.1.4",
          "form": "그니깐요ㅜㅜ",
          "original_form": "그니깐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 07:02"
        },
        {
          "id": "MDRW2100041497.1.5",
          "form": "오늘아침은 초코우유에요",
          "original_form": "오늘아침은 초코우유에요",
          "speaker_id": "1",
          "time": "20210906 07:02"
        },
        {
          "id": "MDRW2100041497.1.6",
          "form": "저는 오늘 아침 계란치즈 토스트 해먹엤네요",
          "original_form": "저는 오늘 아침 계란치즈 토스트 해먹엤네요",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100041497.1.7",
          "form": "오 맛있었겠당ㅎㅎ",
          "original_form": "오 맛있었겠당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100041497.1.8",
          "form": "전 오늘 아침  편의점 샌드위치에요",
          "original_form": "전 오늘 아침  편의점 샌드위치에요",
          "speaker_id": "1",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100041497.1.9",
          "form": "편의점 샌드위치도 종류가 꽤 되죠?",
          "original_form": "편의점 샌드위치도 종류가 꽤 되죠?",
          "speaker_id": "2",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100041497.1.10",
          "form": "저는 참치샐러드샌드위치먹었어요",
          "original_form": "저는 참치샐러드샌드위치먹었어요",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100041497.1.11",
          "form": "맛있는거 먹었네요",
          "original_form": "맛있는거 먹었네요",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100041497.1.12",
          "form": "참치도 있으니  단백질 섭취도 됐네요",
          "original_form": "참치도 있으니  단백질 섭취도 됐네요",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100041497.1.13",
          "form": "편의점에서 잘 사먹나요?",
          "original_form": "편의점에서 잘 사먹나요?",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100041497.1.14",
          "form": "군것질용도 가끔씩사먹어요",
          "original_form": "군것질용도 가끔씩사먹어요",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100041497.1.15",
          "form": "근데 아침에 아침을 잘 못먹다보니",
          "original_form": "근데 아침에 아침을 잘 못먹다보니",
          "speaker_id": "1",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100041497.1.16",
          "form": "겨속갈것같아요",
          "original_form": "겨속갈것같아요",
          "speaker_id": "1",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100041497.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100041497.1.18",
          "form": "편의점도 잘 이용하면 원플러스원도 있고 괜찮다고 하더라구요",
          "original_form": "편의점도 잘 이용하면 원플러스원도 있고 괜찮다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100041497.1.19",
          "form": "이번에 지원금 나오는 것도 편의점 사용 가능하다고 하던데요",
          "original_form": "이번에 지원금 나오는 것도 편의점 사용 가능하다고 하던데요",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100041497.1.20",
          "form": "맞아요 잘찾아보고 사먹을려구요ㅜㅜ",
          "original_form": "맞아요 잘찾아보고 사먹을려구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100041497.1.21",
          "form": "아침마다 그럴려고하니 만만치않네요",
          "original_form": "아침마다 그럴려고하니 만만치않네요",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100041497.1.22",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100041497.1.23",
          "form": "고생이 많네요",
          "original_form": "고생이 많네요",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100041497.1.24",
          "form": "입에 맞는 제품도 잘 찾아야 할텐데요",
          "original_form": "입에 맞는 제품도 잘 찾아야 할텐데요",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100041497.1.25",
          "form": "아침못먹으니깐 힘이 없더라구요ㅠㅠ",
          "original_form": "아침못먹으니깐 힘이 없더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100041497.1.26",
          "form": "맛있는거잘찾아먹어야죵ㅜㅜ",
          "original_form": "맛있는거잘찾아먹어야죵ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100041497.1.27",
          "form": "그니까요.. 점심때까지 힘들면 어떻게해요",
          "original_form": "그니까요.. 점심때까지 힘들면 어떻게해요",
          "speaker_id": "2",
          "time": "20210907 09:54"
        }
      ]
