      "utterance": [
        {
          "id": "MDRW2100006820.1.1",
          "form": "책 읽으시나요",
          "original_form": "책 읽으시나요",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100006820.1.2",
          "form": "육아책을 주로 읽어요",
          "original_form": "육아책을 주로 읽어요",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100006820.1.3",
          "form": "예전에는 책 읽는 거 좋아했는데",
          "original_form": "예전에는 책 읽는 거 좋아했는데",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100006820.1.4",
          "form": "아 아이 있으신가요",
          "original_form": "아 아이 있으신가요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100006820.1.5",
          "form": "네 8살 5살 아이가 있어서",
          "original_form": "네 8살 5살 아이가 있어서",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100006820.1.6",
          "form": "주로 육아서적을 읽는 편이예요",
          "original_form": "주로 육아서적을 읽는 편이예요",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100006820.1.7",
          "form": "오은영님 책도 읽으시나요",
          "original_form": "오은영님 책도 읽으시나요",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100006820.1.8",
          "form": "네 그럼요^^",
          "original_form": "네 그럼요^^",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100006820.1.9",
          "form": "오박사님 책은 필수죠",
          "original_form": "오박사님 책은 필수죠",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100006820.1.10",
          "form": "많은 도움을 받고있어요",
          "original_form": "많은 도움을 받고있어요",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100006820.1.11",
          "form": "어떤 책 좋아하세요??",
          "original_form": "어떤 책 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100006820.1.12",
          "form": "여러권 있으신가요",
          "original_form": "여러권 있으신가요",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100006820.1.13",
          "form": "네 여러권 있어요",
          "original_form": "네 여러권 있어요",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100006820.1.14",
          "form": "아이들 책도 많이 사시나요",
          "original_form": "아이들 책도 많이 사시나요",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100006820.1.15",
          "form": "아이들 책도 많이 사는 편이예요",
          "original_form": "아이들 책도 많이 사는 편이예요",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100006820.1.16",
          "form": "8살 5살이라",
          "original_form": "8살 5살이라",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100006820.1.17",
          "form": "보는 책이 달라져",
          "original_form": "보는 책이 달라져",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100006820.1.18",
          "form": "전집을 많이 사는 편이죠",
          "original_form": "전집을 많이 사는 편이죠",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100006820.1.19",
          "form": "요즘은 뭘 보나요?아이들이",
          "original_form": "요즘은 뭘 보나요?아이들이",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100006820.1.20",
          "form": "모든 책이 늘다르죠",
          "original_form": "모든 책이 늘다르죠",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100006820.1.21",
          "form": "그때그때 본인 보고 싶은거 봐요",
          "original_form": "그때그때 본인 보고 싶은거 봐요",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100006820.1.22",
          "form": "책 뭐 읽으세요??",
          "original_form": "책 뭐 읽으세요??",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100006820.1.23",
          "form": "저는 잘 안읽어요",
          "original_form": "저는 잘 안읽어요",
          "speaker_id": "1",
          "time": "20210908 10:21"
        }
      ]
