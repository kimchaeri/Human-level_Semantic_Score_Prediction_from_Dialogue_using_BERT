      "utterance": [
        {
          "id": "MDRW2100020250.1.1",
          "form": "안녕 나도 배고파",
          "original_form": "안녕 나도 배고파",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020250.1.2",
          "form": "마라탕 먹고 싶다",
          "original_form": "마라탕 먹고 싶다",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020250.1.3",
          "form": "마라탕좋아해?",
          "original_form": "마라탕좋아해?",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020250.1.4",
          "form": "엄청 좋아해 매콤해서 좋아",
          "original_form": "엄청 좋아해 매콤해서 좋아",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020250.1.5",
          "form": "나도 매운 거 좋아해서 좋아해",
          "original_form": "나도 매운 거 좋아해서 좋아해",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020250.1.6",
          "form": "어떤 재료 넣는 거를 좋아하니",
          "original_form": "어떤 재료 넣는 거를 좋아하니",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020250.1.7",
          "form": "스팸 은 필수야",
          "original_form": "스팸 은 필수야",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020250.1.8",
          "form": "버섯도 맛있어",
          "original_form": "버섯도 맛있어",
          "speaker_id": "1",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020250.1.9",
          "form": "넙적 장면도 꼭 들어가야 해",
          "original_form": "넙적 장면도 꼭 들어가야 해",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020250.1.10",
          "form": "당면",
          "original_form": "당면",
          "speaker_id": "2",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020250.1.11",
          "form": "청경채 도 꼭 넣어야 해",
          "original_form": "청경채 도 꼭 넣어야 해",
          "speaker_id": "1",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020250.1.12",
          "form": "땅콩소스 찍어 먹는 게 맛있지",
          "original_form": "땅콩소스 찍어 먹는 게 맛있지",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020250.1.13",
          "form": "나는 마라 샨 거보다는 마라탕이 좋아",
          "original_form": "나는 마라 샨 거보다는 마라탕이 좋아",
          "speaker_id": "1",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020250.1.14",
          "form": "나는 마라샹궈는 아직 안 먹어 봤소",
          "original_form": "나는 마라샹궈는 아직 안 먹어 봤소",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020250.1.15",
          "form": "마라탕은 정말 일주일에 한 번도 만나 먹을 수 있어",
          "original_form": "마라탕은 정말 일주일에 한 번도 만나 먹을 수 있어",
          "speaker_id": "1",
          "time": "20210828 00:45"
        },
