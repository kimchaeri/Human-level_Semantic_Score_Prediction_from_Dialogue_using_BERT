      "utterance": [
        {
          "id": "MDRW2100007513.1.1",
          "form": "드라마",
          "original_form": "드라마",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007513.1.2",
          "form": "잘 안보시나",
          "original_form": "잘 안보시나",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007513.1.3",
          "form": "그 펜트하우스",
          "original_form": "그 펜트하우스",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007513.1.4",
          "form": "엇 봅니다",
          "original_form": "엇 봅니다",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007513.1.5",
          "form": "아 어제꺼 봤어요?",
          "original_form": "아 어제꺼 봤어요?",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007513.1.6",
          "form": "악마판사인가",
          "original_form": "악마판사인가",
          "speaker_id": "2",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007513.1.7",
          "form": "악 어제 너무 징그럽지 않았나요",
          "original_form": "악 어제 너무 징그럽지 않았나요",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.8",
          "form": "그 샹들리에 떨어진거 봤어요",
          "original_form": "그 샹들리에 떨어진거 봤어요",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.9",
          "form": "어제 진짜 뒤로 자빠질번",
          "original_form": "어제 진짜 뒤로 자빠질번",
          "speaker_id": "1",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.10",
          "form": "너무 잔인하고 자극적이고",
          "original_form": "너무 잔인하고 자극적이고",
          "speaker_id": "1",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.11",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.12",
          "form": "와 예상도 못 한 전개라 충격이 두 배",
          "original_form": "와 예상도 못 한 전개라 충격이 두 배",
          "speaker_id": "1",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.13",
          "form": "아니 그거 약간 고어 영화같은데서나 본건데",
          "original_form": "아니 그거 약간 고어 영화같은데서나 본건데",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.14",
          "form": "너무 충격이었어요",
          "original_form": "너무 충격이었어요",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.15",
          "form": "진짜 속안좋더라구요",
          "original_form": "진짜 속안좋더라구요",
          "speaker_id": "1",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.16",
          "form": "벌받아도 그런식을 원한게 아니었는데",
          "original_form": "벌받아도 그런식을 원한게 아니었는데",
          "speaker_id": "1",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.17",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007513.1.18",
          "form": "저렇게 까지 한다고? 싶은",
          "original_form": "저렇게 까지 한다고? 싶은",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007513.1.19",
          "form": "보통 법 처벌받고 끝나는데",
          "original_form": "보통 법 처벌받고 끝나는데",
          "speaker_id": "2",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007513.1.20",
          "form": "정도가 없는",
          "original_form": "정도가 없는",
          "speaker_id": "2",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007513.1.21",
          "form": "미쳤어요 진짜",
          "original_form": "미쳤어요 진짜",
          "speaker_id": "1",
          "time": "20210822 02:42"
        }
      ]
