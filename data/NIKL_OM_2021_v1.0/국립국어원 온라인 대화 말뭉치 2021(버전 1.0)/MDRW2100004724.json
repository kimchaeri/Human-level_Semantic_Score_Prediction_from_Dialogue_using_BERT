      "utterance": [
        {
          "id": "MDRW2100004724.1.1",
          "form": "저녁먹으러 도착한다ㅋ",
          "original_form": "저녁먹으러 도착한다ㅋ",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004724.1.2",
          "form": "음식이 뭘까 ㅋ궁금하네ㅋ",
          "original_form": "음식이 뭘까 ㅋ궁금하네ㅋ",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004724.1.3",
          "form": "머였어?ㅋ",
          "original_form": "머였어?ㅋ",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004724.1.4",
          "form": "대구탕, 소불고기, 미역국, 진매채볶음, 김치, 계란찜",
          "original_form": "대구탕, 소불고기, 미역국, 진매채볶음, 김치, 계란찜",
          "speaker_id": "1",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004724.1.5",
          "form": "ㅋㅋㅋ진짜 많았음ㅋㅋ",
          "original_form": "ㅋㅋㅋ진짜 많았음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004724.1.6",
          "form": "이야 다하신거?",
          "original_form": "이야 다하신거?",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004724.1.7",
          "form": "응응 음식잘하셔ㅋ",
          "original_form": "응응 음식잘하셔ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004724.1.8",
          "form": "나도 음식 잘하고싶네ㅋ",
          "original_form": "나도 음식 잘하고싶네ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004724.1.9",
          "form": "요리를 잘 못해서 ㅠㅜ",
          "original_form": "요리를 잘 못해서 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004724.1.10",
          "form": "와~ 그러니까 멋지시네",
          "original_form": "와~ 그러니까 멋지시네",
          "speaker_id": "2",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004724.1.11",
          "form": "일하시면서",
          "original_form": "일하시면서",
          "speaker_id": "1",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100004724.1.12",
          "form": "대단하신듯ㅋㅋ",
          "original_form": "대단하신듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100004724.1.13",
          "form": "이제 집에간다",
          "original_form": "이제 집에간다",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100004724.1.14",
          "form": "애들 씻겨야되는데 ㅋㅋ",
          "original_form": "애들 씻겨야되는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100004724.1.15",
          "form": "쉽지않네ㅋㅋ",
          "original_form": "쉽지않네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100004724.1.16",
          "form": "ㅋㅋㅋ오늘ㅋㅋ마이 늦겠네 취침이",
          "original_form": "ㅋㅋㅋ오늘ㅋㅋ마이 늦겠네 취침이",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004724.1.17",
          "form": "애들은 재웠어?",
          "original_form": "애들은 재웠어?",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100004724.1.18",
          "form": "아니ㅋname3이는 이제 씻구나오고 name4는 밥먹는중",
          "original_form": "아니ㅋ&name3&이는 이제 씻구나오고 &name4&는 밥먹는중",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100004724.1.19",
          "form": "이제 밥먹나?ㅋㅋㅋㄱㅋㄱㅋ",
          "original_form": "이제 밥먹나?ㅋㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004724.1.20",
          "form": "name4는 밥시간이 애매해ㅠ아침을 열시까지 먹거든ㅠ",
          "original_form": "&name4&는 밥시간이 애매해ㅠ아침을 열시까지 먹거든ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100004724.1.21",
          "form": "그라면 12.1시쯤  간식 먹고자면",
          "original_form": "그라면 12.1시쯤  간식 먹고자면",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100004724.1.22",
          "form": "4시에인나ㅠㅠ요즘은",
          "original_form": "4시에인나ㅠㅠ요즘은",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100004724.1.23",
          "form": "name3이있으면 더 안자서ㅠname3이 2시나갈때까지 안자ㅠ",
          "original_form": "&name3&이있으면 더 안자서ㅠ&name3&이 2시나갈때까지 안자ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100004724.1.24",
          "form": "아 진짜ㅜㅜ",
          "original_form": "아 진짜ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100004724.1.25",
          "form": "저번에도 이랬던것같긴하다",
          "original_form": "저번에도 이랬던것같긴하다",
