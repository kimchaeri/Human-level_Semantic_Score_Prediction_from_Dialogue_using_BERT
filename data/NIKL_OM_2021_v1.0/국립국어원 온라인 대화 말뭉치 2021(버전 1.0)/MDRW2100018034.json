      "utterance": [
        {
          "id": "MDRW2100018034.1.1",
          "form": "비 그텻나요?",
          "original_form": "비 그텻나요?",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018034.1.2",
          "form": "네 비 그쳤어요",
          "original_form": "네 비 그쳤어요",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018034.1.3",
          "form": "후텁지근하네요",
          "original_form": "후텁지근하네요",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018034.1.4",
          "form": "8월말에 이런 날씨 첨인거 같네요..",
          "original_form": "8월말에 이런 날씨 첨인거 같네요..",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018034.1.5",
          "form": "장마같아요",
          "original_form": "장마같아요",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018034.1.6",
          "form": "네ㅜ태풍 여파이기도 하고 복합적인가봐요",
          "original_form": "네ㅜ태풍 여파이기도 하고 복합적인가봐요",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018034.1.7",
          "form": "겨울의 시베리아 여신이 그립기도하고...",
          "original_form": "겨울의 시베리아 여신이 그립기도하고...",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018034.1.8",
          "form": "축축한거보간 추운게 나은듯",
          "original_form": "축축한거보간 추운게 나은듯",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018034.1.9",
          "form": "네 저도 겨울이 더좋아서 얼른 여름 가버렷 ㅜㅜ",
          "original_form": "네 저도 겨울이 더좋아서 얼른 여름 가버렷 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018034.1.10",
          "form": "여름 좋아하는 사람은 악마.. ㅋㅋㅋ",
          "original_form": "여름 좋아하는 사람은 악마.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018034.1.11",
          "form": "여른엔 옷도 자주 갈아입어야하구..",
          "original_form": "여른엔 옷도 자주 갈아입어야하구..",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100018034.1.12",
          "form": "맞습니다 여기서 만난 분들은 name1님 빼고 다 여름이 좋대요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞습니다 여기서 만난 분들은 &name1&님 빼고 다 여름이 좋대요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018034.1.13",
          "form": "에어컨도 ㄱ싫으시대요ㅠㅠ",
          "original_form": "에어컨도 ㄱ싫으시대요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018034.1.14",
          "form": "엥, 진짜요?",
          "original_form": "엥, 진짜요?",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018034.1.15",
          "form": "여른 정말 극악인데... 어떤 분들 만나신거에요? ㅋㅋㅋㅋ",
          "original_form": "여른 정말 극악인데... 어떤 분들 만나신거에요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018034.1.16",
          "form": "넹 겨울이 너무 싫으시대요 ㅎㅎ",
          "original_form": "넹 겨울이 너무 싫으시대요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018034.1.17",
          "form": "부러월어요",
          "original_form": "부러월어요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018034.1.18",
          "form": "요즘은 더워도 그래도 좀 살만해요",
          "original_form": "요즘은 더워도 그래도 좀 살만해요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018034.1.19",
          "form": "그쵸... 저녁은 제법 시원한데...",
          "original_form": "그쵸... 저녁은 제법 시원한데...",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018034.1.20",
          "form": "이놈의 비가...",
          "original_form": "이놈의 비가...",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018034.1.21",
          "form": "그쵸 그래도 오늘은 비가 안왔어요 여기는",
          "original_form": "그쵸 그래도 오늘은 비가 안왔어요 여기는",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018034.1.22",
          "form": "다행",
          "original_form": "다행",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018034.1.23",
          "form": "장화사고싶어요",
          "original_form": "장화사고싶어요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018034.1.24",
          "form": "가을장마 올것 같으니까..",
          "original_form": "가을장마 올것 같으니까..",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018034.1.25",
          "form": "다시더워졌어요",
          "original_form": "다시더워졌어요",
          "speaker_id": "2",
          "time": "20210827 22:31"
        }
      ]
    }
