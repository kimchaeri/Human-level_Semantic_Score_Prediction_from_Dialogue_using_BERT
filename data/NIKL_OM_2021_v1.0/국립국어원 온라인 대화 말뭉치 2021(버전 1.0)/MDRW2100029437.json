          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210903 08:11"
        },
        {
          "id": "MDRW2100029437.1.3",
          "form": "아니요 ㅎㅎ",
          "original_form": "아니요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:11"
        },
        {
          "id": "MDRW2100029437.1.4",
          "form": "전 구피요",
          "original_form": "전 구피요",
          "speaker_id": "1",
          "time": "20210903 08:12"
        },
        {
          "id": "MDRW2100029437.1.5",
          "form": "열대어!",
          "original_form": "열대어!",
          "speaker_id": "2",
          "time": "20210903 14:44"
        },
        {
          "id": "MDRW2100029437.1.6",
          "form": "맞죠?",
          "original_form": "맞죠?",
          "speaker_id": "2",
          "time": "20210903 14:44"
        },
        {
          "id": "MDRW2100029437.1.7",
          "form": "저도 키웠었다는",
          "original_form": "저도 키웠었다는",
          "speaker_id": "2",
          "time": "20210903 14:44"
        },
        {
          "id": "MDRW2100029437.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029437.1.9",
          "form": "힘들죠?",
          "original_form": "힘들죠?",
          "speaker_id": "1",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029437.1.10",
          "form": "네 힘들죠",
          "original_form": "네 힘들죠",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100029437.1.11",
          "form": "비린내도 나고",
          "original_form": "비린내도 나고",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100029437.1.12",
          "form": "네.맞아오",
          "original_form": "네.맞아오",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100029437.1.13",
          "form": "물갈아주는게 너무 힘들던데요",
          "original_form": "물갈아주는게 너무 힘들던데요",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100029437.1.14",
          "form": "일이에요",
          "original_form": "일이에요",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100029437.1.15",
          "form": "그래서 다 죽여먹고",
          "original_form": "그래서 다 죽여먹고",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100029437.1.16",
          "form": "이제 안키워요",
          "original_form": "이제 안키워요",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100029437.1.17",
          "form": "저도 그랄듯",
          "original_form": "저도 그랄듯",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100029437.1.18",
          "form": "아직 잘키우고 계세요?",
          "original_form": "아직 잘키우고 계세요?",
