      "utterance": [
        {
          "id": "MDRW2100030541.1.1",
          "form": "중고거래는 해봤나요?",
          "original_form": "중고거래는 해봤나요?",
          "speaker_id": "1",
          "time": "20210904 05:51"
        },
        {
          "id": "MDRW2100030541.1.2",
          "form": "네 해봤어요",
          "original_form": "네 해봤어요",
          "speaker_id": "2",
          "time": "20210904 07:23"
        },
        {
          "id": "MDRW2100030541.1.3",
          "form": "저는 안하다가 올해 당근마켓을 시작했어요",
          "original_form": "저는 안하다가 올해 당근마켓을 시작했어요",
          "speaker_id": "1",
          "time": "20210904 07:41"
        },
        {
          "id": "MDRW2100030541.1.4",
          "form": "name2님도 당근하시나요?",
          "original_form": "&name2&님도 당근하시나요?",
          "speaker_id": "1",
          "time": "20210904 07:41"
        },
        {
          "id": "MDRW2100030541.1.5",
          "form": "판매할게 많이 없어서 구매한 적은 있어요ㅎㅎ",
          "original_form": "판매할게 많이 없어서 구매한 적은 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 07:41"
        },
        {
          "id": "MDRW2100030541.1.6",
          "form": "주로 구매를 하셨군요",
          "original_form": "주로 구매를 하셨군요",
          "speaker_id": "1",
          "time": "20210904 07:42"
        },
        {
          "id": "MDRW2100030541.1.7",
          "form": "전 주로 판매를 했어요",
          "original_form": "전 주로 판매를 했어요",
          "speaker_id": "1",
          "time": "20210904 07:42"
        },
        {
          "id": "MDRW2100030541.1.8",
          "form": "집에 있는 물건들 처분하고 싶어서요",
          "original_form": "집에 있는 물건들 처분하고 싶어서요",
          "speaker_id": "1",
          "time": "20210904 07:42"
        },
        {
          "id": "MDRW2100030541.1.9",
          "form": "주로 어떤 물건을 판매하셨나요?",
          "original_form": "주로 어떤 물건을 판매하셨나요?",
          "speaker_id": "2",
          "time": "20210904 07:43"
        },
        {
          "id": "MDRW2100030541.1.10",
          "form": "사실 이사를 거의 안다녀서 묵은 짐들이 많았거든요",
          "original_form": "사실 이사를 거의 안다녀서 묵은 짐들이 많았거든요",
          "speaker_id": "1",
          "time": "20210904 07:44"
        },
        {
          "id": "MDRW2100030541.1.11",
          "form": "그랬군요",
          "original_form": "그랬군요",
          "speaker_id": "2",
          "time": "20210904 07:44"
        },
        {
          "id": "MDRW2100030541.1.12",
          "form": "그래서 일단 오래된 물건들 팔았어요",
          "original_form": "그래서 일단 오래된 물건들 팔았어요",
          "speaker_id": "1",
          "time": "20210904 07:44"
        },
        {
          "id": "MDRW2100030541.1.13",
          "form": "카세트테이프 같은거요",
          "original_form": "카세트테이프 같은거요",
          "speaker_id": "1",
          "time": "20210904 07:44"
        },
        {
          "id": "MDRW2100030541.1.14",
          "form": "구매자가 있었나보네요",
          "original_form": "구매자가 있었나보네요",
          "speaker_id": "2",
          "time": "20210904 07:45"
        },
        {
          "id": "MDRW2100030541.1.15",
          "form": "얼마에 판매하셨나요?",
          "original_form": "얼마에 판매하셨나요?",
          "speaker_id": "2",
          "time": "20210904 07:45"
        },
        {
          "id": "MDRW2100030541.1.16",
          "form": "올리자마자 연락이 오더라구요",
          "original_form": "올리자마자 연락이 오더라구요",
          "speaker_id": "1",
          "time": "20210904 07:45"
        },
        {
          "id": "MDRW2100030541.1.17",
          "form": "몇달전이라 크게 기억은 안나는데",
          "original_form": "몇달전이라 크게 기억은 안나는데",
          "speaker_id": "1",
          "time": "20210904 07:45"
        },
        {
          "id": "MDRW2100030541.1.18",
          "form": "아마  개당 천원에 팔았던 거 같아요",
          "original_form": "아마  개당 천원에 팔았던 거 같아요",
          "speaker_id": "1",
          "time": "20210904 07:45"
        },
        {
          "id": "MDRW2100030541.1.19",
          "form": "아무래도 물건이 많지 않아서 그런 것 같네요ㅎㅎ",
          "original_form": "아무래도 물건이 많지 않아서 그런 것 같네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 07:45"
        },
        {
          "id": "MDRW2100030541.1.20",
          "form": "테이프가 수십개 있었는데 다 팔았어요",
          "original_form": "테이프가 수십개 있었는데 다 팔았어요",
          "speaker_id": "1",
          "time": "20210904 07:46"
        },
        {
          "id": "MDRW2100030541.1.21",
          "form": "그분의 취향과 맞았나봐요ㅎㅎ",
          "original_form": "그분의 취향과 맞았나봐요ㅎㅎ",
