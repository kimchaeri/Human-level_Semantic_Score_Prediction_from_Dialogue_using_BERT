      "utterance": [
        {
          "id": "MDRW2100030636.1.1",
          "form": "어떤알바 하세요",
          "original_form": "어떤알바 하세요",
          "speaker_id": "1",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030636.1.2",
          "form": "저 주류 판촉알바해요!",
          "original_form": "저 주류 판촉알바해요!",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030636.1.3",
          "form": "힘드실것같아요ㅠ",
          "original_form": "힘드실것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030636.1.4",
          "form": "구래도 페이가 쎄지않나요?",
          "original_form": "구래도 페이가 쎄지않나요?",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030636.1.5",
          "form": "단기라 쎄요!",
          "original_form": "단기라 쎄요!",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100030636.1.6",
          "form": "많이 못 벌죠 대신ㅜㅜ",
          "original_form": "많이 못 벌죠 대신ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030636.1.7",
          "form": "구렇균요ㅠㅠ 항상 조심하세요",
          "original_form": "구렇균요ㅠㅠ 항상 조심하세요",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030636.1.8",
          "form": "ㅜㅠㅠ name1님도... 몸에 무리되지 않게 일하세요...!!",
          "original_form": "ㅜㅠㅠ &name1&님도... 몸에 무리되지 않게 일하세요...!!",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100030636.1.9",
          "form": "네네 고맙습니다ㅠㅠ",
          "original_form": "네네 고맙습니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100030636.1.10",
          "form": "하시는일 다 잘될거에요",
          "original_form": "하시는일 다 잘될거에요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100030636.1.11",
          "form": "감사합니다ㅜㅜ name1님도 하시는 일마다 대박 잘되길 바래요!!",
          "original_form": "감사합니다ㅜㅜ &name1&님도 하시는 일마다 대박 잘되길 바래요!!",
          "speaker_id": "2",
          "time": "20210905 10:55"
        },
        {
          "id": "MDRW2100030636.1.12",
          "form": "감사해오",
          "original_form": "감사해오",
          "speaker_id": "1",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100030636.1.13",
          "form": "오늘은 쉬세요?",
          "original_form": "오늘은 쉬세요?",
          "speaker_id": "1",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100030636.1.14",
          "form": "출근하고 이제 퇴근해서 집 왔어요ㅜㅜ",
          "original_form": "출근하고 이제 퇴근해서 집 왔어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100030636.1.15",
          "form": "토일 근무에요!",
          "original_form": "토일 근무에요!",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100030636.1.16",
          "form": "아ㅠㅠ오늘도 갔군요ㅠㅠ",
          "original_form": "아ㅠㅠ오늘도 갔군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100030636.1.17",
          "form": "고생하셨어요",
          "original_form": "고생하셨어요",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030636.1.18",
          "form": "넵ㅠㅠ 코로나검사까지 해서ㅋㅋㅋ 오늘 되게 피곤해요",
          "original_form": "넵ㅠㅠ 코로나검사까지 해서ㅋㅋㅋ 오늘 되게 피곤해요",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030636.1.19",
          "form": "name1이님은 오늘 휴무신가요??",
          "original_form": "&name1&이님은 오늘 휴무신가요??",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030636.1.20",
          "form": "네네 오늘은 쉬었어요",
          "original_form": "네네 오늘은 쉬었어요",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030636.1.21",
          "form": "코로나 왜요?ㅠ",
          "original_form": "코로나 왜요?ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100030636.1.22",
          "form": "알바하는데 검사하라고 하시더라구요ㅋㅋㅋ..",
          "original_form": "알바하는데 검사하라고 하시더라구요ㅋㅋㅋ..",
