      "utterance": [
        {
          "id": "MDRW2100045085.1.1",
          "form": "회요!",
          "original_form": "회요!",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045085.1.2",
          "form": "사실 회는 식당가서 먹는 게 좋잖아요 밑반찬도 나오고",
          "original_form": "사실 회는 식당가서 먹는 게 좋잖아요 밑반찬도 나오고",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045085.1.3",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045085.1.4",
          "form": "사비스도 많이 주고요",
          "original_form": "사비스도 많이 주고요",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045085.1.5",
          "form": "서비스",
          "original_form": "서비스",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045085.1.6",
          "form": "근데 가끔 너무 먹고 싶을 때가 있어서",
          "original_form": "근데 가끔 너무 먹고 싶을 때가 있어서",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045085.1.7",
          "form": "그때 배달앱으로 시키는 것 같아요",
          "original_form": "그때 배달앱으로 시키는 것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045085.1.8",
          "form": "그러면 회 초밥 회덮밥",
          "original_form": "그러면 회 초밥 회덮밥",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045085.1.9",
          "form": "뭐거 제일 좋으세요?",
          "original_form": "뭐거 제일 좋으세요?",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045085.1.10",
          "form": "음...배달 시켜먹기는 초밥이 제일 좋은 것 같아요",
          "original_form": "음...배달 시켜먹기는 초밥이 제일 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045085.1.11",
          "form": "회덮밥은 생선회는 별로고 육회면 좋아요 ㅋㄱㅋㅋㄱ",
          "original_form": "회덮밥은 생선회는 별로고 육회면 좋아요 ㅋㄱㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045085.1.12",
          "form": "초밥 너무 좋아요",
          "original_form": "초밥 너무 좋아요",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045085.1.13",
          "form": "어떤 초밥 좋아하세요?",
          "original_form": "어떤 초밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045085.1.14",
          "form": "연어초밥 제일 좋아해요 근데 요즘 광어나 우럭도 좋더라구요",
          "original_form": "연어초밥 제일 좋아해요 근데 요즘 광어나 우럭도 좋더라구요",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045085.1.15",
          "form": "헐 저도 연어랑 광어가 제일 좋아요",
          "original_form": "헐 저도 연어랑 광어가 제일 좋아요",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045085.1.16",
          "form": "얘네 인기쟁이들인가요? 다들 연어랑 광어 얘기하시네요",
          "original_form": "얘네 인기쟁이들인가요? 다들 연어랑 광어 얘기하시네요",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045085.1.17",
          "form": "ㅋㅋㄱㅋㅋ 아무래도 대표메뉴들이죠",
          "original_form": "ㅋㅋㄱㅋㅋ 아무래도 대표메뉴들이죠",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045085.1.18",
          "form": "ㅋㅋㅋㅋ저 광어 지느러미가 제일 좋아요",
          "original_form": "ㅋㅋㅋㅋ저 광어 지느러미가 제일 좋아요",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045085.1.19",
          "form": "연어보다 더 좋아요",
          "original_form": "연어보다 더 좋아요",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045085.1.20",
          "form": "오 저는 아직 부위별로 다른 차이는 몰라서요",
          "original_form": "오 저는 아직 부위별로 다른 차이는 몰라서요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045085.1.21",
          "form": "광어지느러미가 더 고소한가요??",
          "original_form": "광어지느러미가 더 고소한가요??",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045085.1.22",
          "form": "네 맞아요 그리고 식감이 재밌어요",
          "original_form": "네 맞아요 그리고 식감이 재밌어요",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045085.1.23",
          "form": "갑자기 표현하려니까 말이 안나오는데 ㅋㅋㅋㅋㅋ",
          "original_form": "갑자기 표현하려니까 말이 안나오는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045085.1.24",
          "form": "어떤식으로요?",
          "original_form": "어떤식으로요?",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045085.1.25",
          "form": "그냥 회처럼 설겅설겅 씹히는 느낌이 아니고",
          "original_form": "그냥 회처럼 설겅설겅 씹히는 느낌이 아니고",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045085.1.26",
          "form": "꼬독꼬독?",
          "original_form": "꼬독꼬독?",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045085.1.27",
          "form": "그렇다고 씹는게 어려운건 아니에요",
          "original_form": "그렇다고 씹는게 어려운건 아니에요",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045085.1.28",
          "form": "꼬독꼬독?",
          "original_form": "꼬독꼬독?",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045085.1.29",
          "form": "근데 씹기 어렵고 이런건 아니에요",
          "original_form": "근데 씹기 어렵고 이런건 아니에요",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
