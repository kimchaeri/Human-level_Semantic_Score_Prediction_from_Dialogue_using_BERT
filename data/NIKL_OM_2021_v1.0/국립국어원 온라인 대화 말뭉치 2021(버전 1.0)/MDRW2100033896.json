      "utterance": [
        {
          "id": "MDRW2100033896.1.1",
          "form": "패션에 관심 있으신가요?",
          "original_form": "패션에 관심 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100033896.1.2",
          "form": "관심은 있는데 관심만큼 잘입진 못해요ㅋㅋ",
          "original_form": "관심은 있는데 관심만큼 잘입진 못해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100033896.1.3",
          "form": "관심있으세요?",
          "original_form": "관심있으세요?",
          "speaker_id": "2",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100033896.1.4",
          "form": "관심이야 있는데 저도 센스가 꽝이라서요",
          "original_form": "관심이야 있는데 저도 센스가 꽝이라서요",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100033896.1.5",
          "form": "점점 편한옷만 찾게돼요ㅠ",
          "original_form": "점점 편한옷만 찾게돼요ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100033896.1.6",
          "form": "편한게 1순위 편하면서 예쁘면 땡큐고요 ㅋㅋ",
          "original_form": "편한게 1순위 편하면서 예쁘면 땡큐고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100033896.1.7",
          "form": "맞아요ㅋㅋㅋ그런것도 스타일이던데ㅎㅎ",
          "original_form": "맞아요ㅋㅋㅋ그런것도 스타일이던데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100033896.1.8",
          "form": "펑버짐 하지만 아줌마 같지 않은 옷들 있잖아요",
          "original_form": "펑버짐 하지만 아줌마 같지 않은 옷들 있잖아요",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033896.1.9",
          "form": "그쵸 이제 딱붙는건 진짜 신경쓸때나 입구ㅎㅎ",
          "original_form": "그쵸 이제 딱붙는건 진짜 신경쓸때나 입구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033896.1.10",
          "form": "딱 붙는거 입으면 힘주고 있어야 해요 ㅋㅋㅋ",
          "original_form": "딱 붙는거 입으면 힘주고 있어야 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100033896.1.11",
          "form": "아 맞아요ㅠㅠ눈물이ㅋㅋㅋ",
          "original_form": "아 맞아요ㅠㅠ눈물이ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100033896.1.12",
          "form": "전 애낳기 전이랑 몸무게도 같은데 옷은 안들어가요 ㅠ",
          "original_form": "전 애낳기 전이랑 몸무게도 같은데 옷은 안들어가요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100033896.1.13",
          "form": "ㅠㅠ나이들수록  진짜그래요 옷을 다 새로사야하는ㅜㅜ",
          "original_form": "ㅠㅠ나이들수록  진짜그래요 옷을 다 새로사야하는ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100033896.1.14",
          "form": "맞아요. 그리고 어릴때 산 옷은 애기 옷 같아요 ㅠ",
          "original_form": "맞아요. 그리고 어릴때 산 옷은 애기 옷 같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:49"
        }
      ]
