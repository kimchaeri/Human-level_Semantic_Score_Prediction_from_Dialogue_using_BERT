      "utterance": [
        {
          "id": "MDRW2100015229.1.1",
          "form": "저 엄외요",
          "original_form": "저 엄외요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015229.1.2",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015229.1.3",
          "form": "엄외라",
          "original_form": "엄외라",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015229.1.4",
          "form": "전 요거트먹습니다",
          "original_form": "전 요거트먹습니다",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015229.1.5",
          "form": "엄외는 초코볼이 다했다 아니다",
          "original_form": "엄외는 초코볼이 다했다 아니다",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015229.1.6",
          "form": "좀",
          "original_form": "좀",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015229.1.7",
          "form": "좀 라이트한 입맛이시네요",
          "original_form": "좀 라이트한 입맛이시네요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.9",
          "form": "아 지금",
          "original_form": "아 지금",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.10",
          "form": "그러면 민초에요 반민초에요",
          "original_form": "그러면 민초에요 반민초에요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.11",
          "form": "요거트가 라이트 하다했나요",
          "original_form": "요거트가 라이트 하다했나요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.12",
          "form": "당연히 반민초죠",
          "original_form": "당연히 반민초죠",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.13",
          "form": "ㅇㄴ",
          "original_form": "ㅇㄴ",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.14",
          "form": "민쵸를 어떻게 먹어",
          "original_form": "민쵸를 어떻게 먹어",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.15",
          "form": "10센치 멀어젺다",
          "original_form": "10센치 멀어젺다",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.16",
          "form": "설마",
          "original_form": "설마",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.17",
          "form": "민초?",
          "original_form": "민초?",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.18",
          "form": "에이",
          "original_form": "에이",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.19",
          "form": "아니죠?",
          "original_form": "아니죠?",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.20",
          "form": "ㄹㅇ 민초 왜 안드시죠?",
          "original_form": "ㄹㅇ 민초 왜 안드시죠?",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.21",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015229.1.22",
          "form": "민초 치약맛",
          "original_form": "민초 치약맛",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.23",
          "form": "민초는 태생부터 로열이라고요",
          "original_form": "민초는 태생부터 로열이라고요",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.24",
          "form": "근데 초코가박혀있어",
          "original_form": "근데 초코가박혀있어",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.25",
          "form": "와우10센치나 멀어지다니",
          "original_form": "와우10센치나 멀어지다니",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.26",
          "form": "실수로 요거트가 된 것과는 다른.",
          "original_form": "실수로 요거트가 된 것과는 다른.",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.27",
          "form": "실수라니",
          "original_form": "실수라니",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.28",
          "form": "차단금지 차단금지",
          "original_form": "차단금지 차단금지",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.29",
          "form": "실수라니요",
          "original_form": "실수라니요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.30",
          "form": "와우 요거트를",
          "original_form": "와우 요거트를",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.31",
          "form": "알겟어요. 그 발언은 사과하죠.",
          "original_form": "알겟어요. 그 발언은 사과하죠.",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.32",
          "form": "우리 요거트를..!",
          "original_form": "우리 요거트를..!",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015229.1.33",
          "form": "배라가서 파인트에 3개지만 담는다면?",
          "original_form": "배라가서 파인트에 3개지만 담는다면?",
