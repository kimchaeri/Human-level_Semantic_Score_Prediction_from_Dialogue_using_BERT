      "utterance": [
        {
          "id": "MDRW2100005627.1.1",
          "form": "다른 사람이랑 대화하다가 중학교 때 생각 남",
          "original_form": "다른 사람이랑 대화하다가 중학교 때 생각 남",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005627.1.2",
          "form": "독서 캠프",
          "original_form": "독서 캠프",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005627.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005627.1.4",
          "form": "나 아직도 그거 폴라로이드 있어",
          "original_form": "나 아직도 그거 폴라로이드 있어",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005627.1.5",
          "form": "나도 찾으면 어디 있을 거 같음",
          "original_form": "나도 찾으면 어디 있을 거 같음",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005627.1.6",
          "form": "너무 추억이야",
          "original_form": "너무 추억이야",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005627.1.7",
          "form": "사실 도시락만 기억남",
          "original_form": "사실 도시락만 기억남",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005627.1.8",
          "form": "진짜 파도파도 중학교 때는 행복한 기억이 잔뜩임",
          "original_form": "진짜 파도파도 중학교 때는 행복한 기억이 잔뜩임",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005627.1.9",
          "form": "도시락 존맛탱",
          "original_form": "도시락 존맛탱",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005627.1.10",
          "form": "나도 도시락이 한 60퍼 지분",
          "original_form": "나도 도시락이 한 60퍼 지분",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005627.1.11",
          "form": "ㅋㅋㅋㅋㅋ레알 한솥",
          "original_form": "ㅋㅋㅋㅋㅋ레알 한솥",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005627.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005627.1.13",
          "form": "그냥 그 분위기가 좋았음",
          "original_form": "그냥 그 분위기가 좋았음",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005627.1.14",
          "form": "대부분 친한 애들이고",
          "original_form": "대부분 친한 애들이고",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005627.1.15",
          "form": "같이 책읽고 떠들다가 맛있는 거 먹고 영화보고",
          "original_form": "같이 책읽고 떠들다가 맛있는 거 먹고 영화보고",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005627.1.16",
          "form": "맞아ㅜ",
          "original_form": "맞아ㅜ",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005627.1.17",
          "form": "솔직히 중학교 애들 대부분 지금 생각해도 안 좋긴 했는데",
          "original_form": "솔직히 중학교 애들 대부분 지금 생각해도 안 좋긴 했는데",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005627.1.18",
          "form": "친한 애들은 일단 좋았고",
          "original_form": "친한 애들은 일단 좋았고",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005627.1.19",
          "form": "고등학교 대학교에서",
          "original_form": "고등학교 대학교에서",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005627.1.20",
          "form": "더 이상한 애들 만나니까",
          "original_form": "더 이상한 애들 만나니까",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005627.1.21",
          "form": "그냥 중학교가 그리움...",
          "original_form": "그냥 중학교가 그리움...",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005627.1.22",
          "form": "어 맞아...",
          "original_form": "어 맞아...",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005627.1.23",
          "form": "분명 힘든 일도 많았는데",
          "original_form": "분명 힘든 일도 많았는데",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005627.1.24",
          "form": "미화 엄청 됨",
          "original_form": "미화 엄청 됨",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005627.1.25",
          "form": "축제도 그정도면 재밌었음",
          "original_form": "축제도 그정도면 재밌었음",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005627.1.26",
          "form": "허즈밴든가 그것도 ㄹㅇ 일탈",
          "original_form": "허즈밴든가 그것도 ㄹㅇ 일탈",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005627.1.27",
          "form": "나중에 락페스티벌 같은데 같이 가자",
          "original_form": "나중에 락페스티벌 같은데 같이 가자",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005627.1.28",
          "form": "재밌을 거 같음",
          "original_form": "재밌을 거 같음",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005627.1.29",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005627.1.30",
          "form": "락페 진짜 가고싶음",
          "original_form": "락페 진짜 가고싶음",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005627.1.31",
          "form": "완전 재밌게 놀 거 같아",
          "original_form": "완전 재밌게 놀 거 같아",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005627.1.32",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005627.1.33",
          "form": "광란의 파티인데",
          "original_form": "광란의 파티인데",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005627.1.34",
          "form": "코로나 때문에 할 수 있는게 없네",
          "original_form": "코로나 때문에 할 수 있는게 없네",
          "speaker_id": "1",
          "time": "20210821 23:36"
        }
      ]
