      "utterance": [
        {
          "id": "MDRW2100022081.1.1",
          "form": "검색해봐야지 으흐흫",
          "original_form": "검색해봐야지 으흐흫",
          "speaker_id": "2",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100022081.1.2",
          "form": "저는 전갈이랑 매뚜기 그리고 비둘기 요리를",
          "original_form": "저는 전갈이랑 매뚜기 그리고 비둘기 요리를",
          "speaker_id": "1",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100022081.1.3",
          "form": "여행가서 먹어본적이 있어요",
          "original_form": "여행가서 먹어본적이 있어요",
          "speaker_id": "1",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100022081.1.4",
          "form": "ㅇㅁㅇ..... 희귀요리 찾는 분이시구나...",
          "original_form": "ㅇㅁㅇ..... 희귀요리 찾는 분이시구나...",
          "speaker_id": "2",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100022081.1.5",
          "form": "외국여행이요?",
          "original_form": "외국여행이요?",
          "speaker_id": "2",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100022081.1.6",
          "form": "전 아직 외국여행은 안가봤어요",
          "original_form": "전 아직 외국여행은 안가봤어요",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.7",
          "form": "아뇨 그런건 아니니데 어쩌다보니",
          "original_form": "아뇨 그런건 아니니데 어쩌다보니",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.9",
          "form": "한국이 좋아..ㅋㅋㅋㅋㅋ",
          "original_form": "한국이 좋아..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.10",
          "form": "국내여행에서는",
          "original_form": "국내여행에서는",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.11",
          "form": "오홍. 먹어볼 수 있으면 먹을만도 하죠 응응",
          "original_form": "오홍. 먹어볼 수 있으면 먹을만도 하죠 응응",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.12",
          "form": "노말핫것들 먹었죠",
          "original_form": "노말핫것들 먹었죠",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.13",
          "form": "한국에서 노말한 게 외국에선 특이할 수도 있어요 ㅋㅋㅋㅋ",
          "original_form": "한국에서 노말한 게 외국에선 특이할 수도 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.14",
          "form": "그렇죠ㅎㅎ",
          "original_form": "그렇죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.15",
          "form": "외국에서 내장 잘 안먹는다잖아요",
          "original_form": "외국에서 내장 잘 안먹는다잖아요",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.16",
          "form": "우리는 환장하는데..ㅋㅋㅋㅋㅋ",
          "original_form": "우리는 환장하는데..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.17",
          "form": "아까 말씀하신 선지 같은거",
          "original_form": "아까 말씀하신 선지 같은거",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022081.1.18",
          "form": "맞아요 선지 순대 그런거",
          "original_form": "맞아요 선지 순대 그런거",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022081.1.19",
          "form": "소시지도 내장에 고기 넣은거 아닌가요",
          "original_form": "소시지도 내장에 고기 넣은거 아닌가요",
          "speaker_id": "1",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022081.1.20",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022081.1.21",
          "form": "내장에 고기 많이 넣고 훈연하면 소세지고",
          "original_form": "내장에 고기 많이 넣고 훈연하면 소세지고",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022081.1.22",
          "form": "닭발 골뱅이 번데기",
          "original_form": "닭발 골뱅이 번데기",
          "speaker_id": "1",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022081.1.23",
          "form": "피 많이 넣고 당면도 넣고 삶으면 순대고",
          "original_form": "피 많이 넣고 당면도 넣고 삶으면 순대고",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022081.1.24",
          "form": "이런거 보면 외국인들 기겁하겠죠",
          "original_form": "이런거 보면 외국인들 기겁하겠죠",
          "speaker_id": "1",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022081.1.25",
          "form": "닭발 먹고싶다 ㅠㅠㅜ...",
          "original_form": "닭발 먹고싶다 ㅠㅠㅜ...",
          "speaker_id": "2",
          "time": "20210829 02:26"
        }
      ]
