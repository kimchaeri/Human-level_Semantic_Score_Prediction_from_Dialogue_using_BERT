      "utterance": [
        {
          "id": "MDRW2100026395.1.1",
          "form": "응 야채도 제법들어가",
          "original_form": "응 야채도 제법들어가",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026395.1.2",
          "form": "야채 들어가?",
          "original_form": "야채 들어가?",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026395.1.3",
          "form": "정답! 김치찌개",
          "original_form": "정답! 김치찌개",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026395.1.4",
          "form": "와 짱잘맞춘다",
          "original_form": "와 짱잘맞춘다",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026395.1.5",
          "form": "너 천재니",
          "original_form": "너 천재니",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026395.1.6",
          "form": "아님 내가 너무 한식파인가",
          "original_form": "아님 내가 너무 한식파인가",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026395.1.7",
          "form": "캬캬캬캬캬캬캬캬캬캬 뿌듯하다",
          "original_form": "캬캬캬캬캬캬캬캬캬캬 뿌듯하다",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026395.1.8",
          "form": "우리 친구 음식취향",
          "original_form": "우리 친구 음식취향",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026395.1.9",
          "form": "내가 간파하지",
          "original_form": "내가 간파하지",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026395.1.10",
          "form": "다시 네 차례야",
          "original_form": "다시 네 차례야",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026395.1.11",
          "form": "생각 완료",
          "original_form": "생각 완료",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026395.1.12",
          "form": "음 한식 중식 양식 일식중에 뭐니",
          "original_form": "음 한식 중식 양식 일식중에 뭐니",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026395.1.13",
          "form": "중식입니다",
          "original_form": "중식입니다",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026395.1.14",
          "form": "탕수육이지?",
          "original_form": "탕수육이지?",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026395.1.15",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ벌써 구체적으로?!",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ벌써 구체적으로?!",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026395.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026395.1.17",
          "form": "너 요즘 먹고싶어했으니까",
          "original_form": "너 요즘 먹고싶어했으니까",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026395.1.18",
          "form": "탕수육 아닙니다 맛있겠다",
          "original_form": "탕수육 아닙니다 맛있겠다",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026395.1.19",
          "form": "아이참 내 생각 해주기는",
          "original_form": "아이참 내 생각 해주기는",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026395.1.20",
          "form": "다음에 같이 먹으러 가자",
          "original_form": "다음에 같이 먹으러 가자",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026395.1.21",
          "form": "그래 너무좋다 \n탕수육짱좋아하걸랑",
          "original_form": "그래 너무좋다 \n탕수육짱좋아하걸랑",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026395.1.22",
          "form": "앗싸 좋아",
          "original_form": "앗싸 좋아",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026395.1.23",
          "form": "그럼 빨간양념이니?",
          "original_form": "그럼 빨간양념이니?",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026395.1.24",
          "form": "네 빨갛습니다",
          "original_form": "네 빨갛습니다",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026395.1.25",
          "form": "짬뽕이구나???",
          "original_form": "짬뽕이구나???",
