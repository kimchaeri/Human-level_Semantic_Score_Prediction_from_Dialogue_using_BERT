        },
        {
          "id": "MDRW2100019893.1.2",
          "form": "안녕하세요~! 반갑습니다.",
          "original_form": "안녕하세요~! 반갑습니다.",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019893.1.3",
          "form": "온라인 커뮤니티 하시는 거 있으세요?",
          "original_form": "온라인 커뮤니티 하시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019893.1.4",
          "form": "저는 블로그, 다음카페 합니다!",
          "original_form": "저는 블로그, 다음카페 합니다!",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019893.1.5",
          "form": "우와 저도 블로그 하고 있어요!",
          "original_form": "우와 저도 블로그 하고 있어요!",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019893.1.6",
          "form": "다음은 가입만 해놓고 잘 안들어가게 되네요 ㅎㅎ",
          "original_form": "다음은 가입만 해놓고 잘 안들어가게 되네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019893.1.7",
          "form": "저도 그냥 보기만해요!",
          "original_form": "저도 그냥 보기만해요!",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019893.1.8",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019893.1.9",
          "form": "그쵸 뭔가 의견을 남기다가",
          "original_form": "그쵸 뭔가 의견을 남기다가",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019893.1.10",
          "form": "괜히... 시비가 붙을 수도 있고....!",
          "original_form": "괜히... 시비가 붙을 수도 있고....!",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019893.1.11",
          "form": "블로그는 어떤 주제로 운영하시나요?",
          "original_form": "블로그는 어떤 주제로 운영하시나요?",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019893.1.12",
          "form": "저는 일상 블로그요!",
          "original_form": "저는 일상 블로그요!",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019893.1.13",
          "form": "앗 저도 일상 블로그입니다 ㅎㅎ",
          "original_form": "앗 저도 일상 블로그입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.14",
          "form": "전문 블로그처럼 운영하는 분들 대단해요",
          "original_form": "전문 블로그처럼 운영하는 분들 대단해요",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.15",
          "form": "저는 결국 제 관심사를 다 다루고싶어져서",
          "original_form": "저는 결국 제 관심사를 다 다루고싶어져서",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.16",
          "form": "사실 제가 name2님을 안다면. ....",
          "original_form": "사실 제가 &name2&님을 안다면. ....",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.17",
          "form": "잡탕이 되더라구요!",
          "original_form": "잡탕이 되더라구요!",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.18",
          "form": "앗....?!",
          "original_form": "앗....?!",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.19",
          "form": "혹시...!!!",
          "original_form": "혹시...!!!",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ^_ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ^_ㅜ",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.21",
          "form": "누구신가요...! 두둥",
          "original_form": "누구신가요...! 두둥",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019893.1.22",
          "form": "두근...세근...네근...!",
          "original_form": "두근...세근...네근...!",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019893.1.23",
          "form": "블로그 댓글에 정체를 공개 해놓았습니다^_^",
          "original_form": "블로그 댓글에 정체를 공개 해놓았습니다^_^",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019893.1.24",
          "form": "ㅍㅎㅎ",
          "original_form": "ㅍㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019893.1.25",
          "form": "너무 신나네요",
          "original_form": "너무 신나네요",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019893.1.26",
          "form": "저도 블로그에서 보고 깜짝 놀랐어요!! 설마설마 했는데!!",
          "original_form": "저도 블로그에서 보고 깜짝 놀랐어요!! 설마설마 했는데!!",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019893.1.27",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019893.1.28",
          "form": "여기에서 어디까지",
          "original_form": "여기에서 어디까지",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019893.1.29",
          "form": "말을 해도 되는지 모르겠네요",
          "original_form": "말을 해도 되는지 모르겠네요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019893.1.30",
          "form": "그러게요..",
          "original_form": "그러게요..",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019893.1.31",
          "form": "온라인 커뮤니티 이야기 해야하는데;;;",
          "original_form": "온라인 커뮤니티 이야기 해야하는데;;;",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019893.1.32",
          "form": "그럼 온라인 커뮤 이야기 해요!",
          "original_form": "그럼 온라인 커뮤 이야기 해요!",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019893.1.33",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019893.1.34",
          "form": "저는 다음 카페를",
          "original_form": "저는 다음 카페를",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019893.1.35",
          "form": "어플도 깔아놨는데 이상하게 안들어가게 되더라구요,,",
          "original_form": "어플도 깔아놨는데 이상하게 안들어가게 되더라구요,,",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019893.1.36",
          "form": "하말넘많 카페랑 소그노 카페를 주로 들어갔는데",
          "original_form": "하말넘많 카페랑 소그노 카페를 주로 들어갔는데",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019893.1.37",
          "form": "리젠이 그렇게 활발하지 않아서인지,, 결국 기억에서 잊혀지는,,,",
          "original_form": "리젠이 그렇게 활발하지 않아서인지,, 결국 기억에서 잊혀지는,,,",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019893.1.38",
          "form": "어라리.. 하말넘많 카페가 있는줄은 몰랐네요!",
          "original_form": "어라리.. 하말넘많 카페가 있는줄은 몰랐네요!",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019893.1.39",
          "form": "소그노는 가입했는데",
          "original_form": "소그노는 가입했는데",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019893.1.40",
          "form": "맞아요. 리젠이 활발하지 않아서 잘 안들어가게 되더라고요!",
          "original_form": "맞아요. 리젠이 활발하지 않아서 잘 안들어가게 되더라고요!",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019893.1.41",
          "form": "하말넘많 카페가 더 규모 크고 리젠도 활발하더라구요~!",
          "original_form": "하말넘많 카페가 더 규모 크고 리젠도 활발하더라구요~!",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019893.1.42",
          "form": "아니근데 저 블로그에",
          "original_form": "아니근데 저 블로그에",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019893.1.43",
          "form": "헉 그런가요?! 당장 가입한다......",
          "original_form": "헉 그런가요?! 당장 가입한다......",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019893.1.44",
          "form": "특정 이웃님들 글만 자주 보이고",
          "original_form": "특정 이웃님들 글만 자주 보이고",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019893.1.45",
          "form": "네네네!! 근데 여성임을 증명하는 과정이 있어요",
          "original_form": "네네네!! 근데 여성임을 증명하는 과정이 있어요",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019893.1.46",
          "form": "엇 저도 글이 전부 보이지는 않는 것 같더라고요",
          "original_form": "엇 저도 글이 전부 보이지는 않는 것 같더라고요",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019893.1.47",
          "form": "저 블로그 피드에 안보이는 이웃님들 많이 계셔서",
          "original_form": "저 블로그 피드에 안보이는 이웃님들 많이 계셔서",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019893.1.48",
          "form": "나중에 들어갔을 때 안읽은 글 많을 때 민망하고 죄송해요 ㅜㅜ",
          "original_form": "나중에 들어갔을 때 안읽은 글 많을 때 민망하고 죄송해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019893.1.49",
          "form": "이상하게 name3님 글도 안보여서",
          "original_form": "이상하게 &name3&님 글도 안보여서",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019893.1.50",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019893.1.51",
          "form": "네,,",
          "original_form": "네,,",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019893.1.52",
          "form": "이상하게 보이는 분들만 보여서,,",
          "original_form": "이상하게 보이는 분들만 보여서,,",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019893.1.53",
          "form": "그렇더라구요,, name1님도 그러신가요?!",
          "original_form": "그렇더라구요,, &name1&님도 그러신가요?!",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019893.1.54",
          "form": "네네 저도 그렇더라고요",
          "original_form": "네네 저도 그렇더라고요",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019893.1.55",
          "form": "그래서 가끔 다른 이웃분들 블로그에 들어가서",
          "original_form": "그래서 가끔 다른 이웃분들 블로그에 들어가서",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019893.1.56",
          "form": "못 본 글 읽고 나오고 해요",
          "original_form": "못 본 글 읽고 나오고 해요",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019893.1.57",
          "form": "보니까 500명까지만 보인다는데",
          "original_form": "보니까 500명까지만 보인다는데",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019893.1.58",
          "form": "무슨 원리인지 모르겠네요,,,,",
          "original_form": "무슨 원리인지 모르겠네요,,,,",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019893.1.59",
          "form": "코로나 때문에 확실히",
          "original_form": "코로나 때문에 확실히",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019893.1.60",
          "form": "온라인 커뮤니티가 현생에 차지하는 비중도 엄청나지는 것 같아요",
          "original_form": "온라인 커뮤니티가 현생에 차지하는 비중도 엄청나지는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019893.1.61",
          "form": "맞아요.. 아무래도 밖에 못 나가니까",
