        {
          "id": "MDRW2100021866.1.1",
          "form": "대충?",
          "original_form": "대충?",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100021866.1.2",
          "form": "최근에 일어난 사건이",
          "original_form": "최근에 일어난 사건이",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.3",
          "form": "대충 150년 전에",
          "original_form": "대충 150년 전에",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.4",
          "form": "악룡 두란이랑 사신수 드발린이 싸우다가",
          "original_form": "악룡 두란이랑 사신수 드발린이 싸우다가",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.5",
          "form": "고탑왕?",
          "original_form": "고탑왕?",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.6",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.7",
          "form": "나 방금 뒤지다가",
          "original_form": "나 방금 뒤지다가",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.8",
          "form": "두란이 죽고 드발린이 오염돼서 날뛴거엿잖아",
          "original_form": "두란이 죽고 드발린이 오염돼서 날뛴거엿잖아",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.9",
          "form": "악룡 알베도 스승이 만든거란 얘기를",
          "original_form": "악룡 알베도 스승이 만든거란 얘기를",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.10",
          "form": "봣다",
          "original_form": "봣다",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.11",
          "form": "알베도 제작자인가?",
          "original_form": "알베도 제작자인가?",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021866.1.12",
          "form": "여튼",
          "original_form": "여튼",
          "speaker_id": "2",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021866.1.13",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021866.1.14",
          "form": "시뇨라는",
          "original_form": "시뇨라는",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021866.1.15",
          "form": "그 시절 사람임",
          "original_form": "그 시절 사람임",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021866.1.16",
          "form": "억케 안늙고 살아잇지??",
          "original_form": "억케 안늙고 살아잇지??",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021866.1.17",
          "form": "아마 우인단의 힘일걸로 예상함",
          "original_form": "아마 우인단의 힘일걸로 예상함",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021866.1.18",
          "form": "시뇨라는 원래 음유시인이엇음",
          "original_form": "시뇨라는 원래 음유시인이엇음",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021866.1.19",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021866.1.20",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.21",
          "form": "음유시인이라니",
          "original_form": "음유시인이라니",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.22",
          "form": "ㄹㅇ안어울려....",
          "original_form": "ㄹㅇ안어울려....",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.23",
          "form": "그리고 페보니우스 기사단의 초대 기사랑 썸을 탔엇는데",
          "original_form": "그리고 페보니우스 기사단의 초대 기사랑 썸을 탔엇는데",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.24",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.25",
          "form": "수메르 아카데미를 가게됨",
          "original_form": "수메르 아카데미를 가게됨",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.26",
          "form": "기사가?",
          "original_form": "기사가?",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.27",
          "form": "본인이?",
          "original_form": "본인이?",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.28",
          "form": "ㄴㄴ 시뇨라가",
          "original_form": "ㄴㄴ 시뇨라가",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.29",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.30",
          "form": "그래서 화염마녀 성유물 중 모래시계 보면",
          "original_form": "그래서 화염마녀 성유물 중 모래시계 보면",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021866.1.31",
          "form": "아카데미를 가는 동안 시계가 한바퀴 돌았다고",
          "original_form": "아카데미를 가는 동안 시계가 한바퀴 돌았다고",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021866.1.32",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021866.1.33",
          "form": "아이템 설명이 있어",
          "original_form": "아이템 설명이 있어",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021866.1.34",
          "form": "떨어져있었구나",
          "original_form": "떨어져있었구나",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021866.1.35",
          "form": "설마 그사이에 죽음?",
          "original_form": "설마 그사이에 죽음?",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021866.1.36",
          "form": "ㅇㅇ 그리고 수메르에 가있는 동안",
          "original_form": "ㅇㅇ 그리고 수메르에 가있는 동안",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021866.1.37",
          "form": "두란이 나타나서 몬드가 뒤집어지고",
          "original_form": "두란이 나타나서 몬드가 뒤집어지고",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021866.1.38",
          "form": "기사가 죽어서",
          "original_form": "기사가 죽어서",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021866.1.39",
          "form": "와우네",
          "original_form": "와우네",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021866.1.40",
          "form": "시뇨라가 돌아왔을때는",
          "original_form": "시뇨라가 돌아왔을때는",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021866.1.41",
          "form": "드발린이 두란을 죽이고 오염돼서 날뛰는 상황이었음",
          "original_form": "드발린이 두란을 죽이고 오염돼서 날뛰는 상황이었음",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021866.1.42",
          "form": "그래서 시뇨라는 자기 생명력을 깎아서 드발린을 막았는데",
          "original_form": "그래서 시뇨라는 자기 생명력을 깎아서 드발린을 막았는데",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021866.1.43",
          "form": "벤티가 막음?",
          "original_form": "벤티가 막음?",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021866.1.44",
          "form": "자기 몸도 불에 타오르니까",
          "original_form": "자기 몸도 불에 타오르니까",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021866.1.45",
          "form": "몬드 사람들이 시뇨라가 무섭다고 문을 잠그고",
          "original_form": "몬드 사람들이 시뇨라가 무섭다고 문을 잠그고",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021866.1.46",
          "form": "아무도 안받아들여줌",
          "original_form": "아무도 안받아들여줌",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021866.1.47",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021866.1.48",
          "form": "하지만 시뇨라는",
          "original_form": "하지만 시뇨라는",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021866.1.49",
          "form": "그런건 신경 안썼음",
          "original_form": "그런건 신경 안썼음",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021866.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021866.1.51",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021866.1.52",
          "form": "이유는 모르지만",
          "original_form": "이유는 모르지만",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021866.1.53",
          "form": "화염으로 마물을 죽이다가",
          "original_form": "화염으로 마물을 죽이다가",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021866.1.54",
          "form": "소원을 들어주는 봉화가 있다고",
          "original_form": "소원을 들어주는 봉화가 있다고",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021866.1.55",
          "form": "찾아간 곳에서 우인단을 만났고",
          "original_form": "찾아간 곳에서 우인단을 만났고",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021866.1.56",
          "form": "거기서 최초의 우인단을 만나서 스네즈나야의 신이랑 계약하게 됨",
          "original_form": "거기서 최초의 우인단을 만나서 스네즈나야의 신이랑 계약하게 됨",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021866.1.57",
          "form": "근데 그럼",
          "original_form": "근데 그럼",
          "speaker_id": "2",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021866.1.58",
          "form": "불이 사안의 힘이 아니야?",
          "original_form": "불이 사안의 힘이 아니야?",
          "speaker_id": "2",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021866.1.59",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021866.1.60",
          "form": "신의 눈이 불이고",
          "original_form": "신의 눈이 불이고",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021866.1.61",
          "form": "우인단이랑 계약하면서 받은 힘이 얼음이야",
          "original_form": "우인단이랑 계약하면서 받은 힘이 얼음이야",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021866.1.62",
          "form": "잘 보면",
          "original_form": "잘 보면",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021866.1.63",
          "form": "프롤로그 영상에",
          "original_form": "프롤로그 영상에",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021866.1.64",
          "form": "모두 들어가있음",
          "original_form": "모두 들어가있음",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021866.1.65",
          "form": "시뇨라가 벤티보고",
          "original_form": "시뇨라가 벤티보고",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021866.1.66",
          "form": "집나간 햄스터가 돌아왔네 어쩌네 빈정댄건",
          "original_form": "집나간 햄스터가 돌아왔네 어쩌네 빈정댄건",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021866.1.67",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
