      "utterance": [
        {
          "id": "MDRW2100007752.1.1",
          "form": "마케팅이면 재택근무 가능하지 않아요?",
          "original_form": "마케팅이면 재택근무 가능하지 않아요?",
          "speaker_id": "1",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007752.1.2",
          "form": "ㅋㅋㅋ여긴 그런거없어여...",
          "original_form": "ㅋㅋㅋ여긴 그런거없어여...",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100007752.1.3",
          "form": "아이구...",
          "original_form": "아이구...",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100007752.1.4",
          "form": "재택근무하는 친구들은",
          "original_form": "재택근무하는 친구들은",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100007752.1.5",
          "form": "목요일 재택하고",
          "original_form": "목요일 재택하고",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100007752.1.6",
          "form": "금요일 반차내고",
          "original_form": "금요일 반차내고",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100007752.1.7",
          "form": "쭉 쉬던데",
          "original_form": "쭉 쉬던데",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100007752.1.8",
          "form": "부럽네여...ㅜ여긴 재택근무해도 충분할텐데 왜 안하지",
          "original_form": "부럽네여...ㅜ여긴 재택근무해도 충분할텐데 왜 안하지",
          "speaker_id": "2",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100007752.1.9",
          "form": "곧 자유를 찾아 떠날거에요...",
          "original_form": "곧 자유를 찾아 떠날거에요...",
          "speaker_id": "2",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100007752.1.10",
          "form": "도비 이즈 프리",
          "original_form": "도비 이즈 프리",
          "speaker_id": "1",
          "time": "20210822 17:45"
        },
        {
          "id": "MDRW2100007752.1.11",
          "form": "근데 선생님도 재택이 되나요?",
          "original_form": "근데 선생님도 재택이 되나요?",
          "speaker_id": "1",
          "time": "20210822 17:45"
        },
        {
          "id": "MDRW2100007752.1.12",
          "form": "선생님은 과외선생님이면 학생네 가서 가르치니까 재택이라고 해야하려나요...",
          "original_form": "선생님은 과외선생님이면 학생네 가서 가르치니까 재택이라고 해야하려나요...",
          "speaker_id": "2",
          "time": "20210822 17:47"
        },
        {
          "id": "MDRW2100007752.1.13",
          "form": "요즘은 영어를 화상으로 가르치는 곳도 있어서 그건 재택가능할 것 같아요",
          "original_form": "요즘은 영어를 화상으로 가르치는 곳도 있어서 그건 재택가능할 것 같아요",
          "speaker_id": "2",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100007752.1.14",
          "form": "학교쌤은 케바케...?",
          "original_form": "학교쌤은 케바케...?",
          "speaker_id": "2",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100007752.1.15",
          "form": "학원쌤은 재택근무따위 없을걸요...",
          "original_form": "학원쌤은 재택근무따위 없을걸요...",
          "speaker_id": "2",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100007752.1.16",
          "form": "하긴 아직 세상은 충분히 기계화되지 못했으닌깐요",
          "original_form": "하긴 아직 세상은 충분히 기계화되지 못했으닌깐요",
          "speaker_id": "1",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100007752.1.17",
          "form": "언젠가 기계가 지배하는 세상이 오면",
          "original_form": "언젠가 기계가 지배하는 세상이 오면",
          "speaker_id": "1",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100007752.1.18",
          "form": "선생님도 재택일텐데",
          "original_form": "선생님도 재택일텐데",
          "speaker_id": "1",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100007752.1.19",
          "form": "ㅋㅋㅋ기계가 지배하면 선생님자리 없어질걸요...",
          "original_form": "ㅋㅋㅋ기계가 지배하면 선생님자리 없어질걸요...",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100007752.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100007752.1.21",
          "form": "기계도 누군가 가르쳐야되지 않을까요?",
          "original_form": "기계도 누군가 가르쳐야되지 않을까요?",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100007752.1.22",
          "form": "그건 아마 다른직업군이 생기겠죠...ㅜ",
          "original_form": "그건 아마 다른직업군이 생기겠죠...ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100007752.1.23",
          "form": "재택은 그저 꿈일 뿐인가",
          "original_form": "재택은 그저 꿈일 뿐인가",
          "speaker_id": "1",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100007752.1.24",
          "form": "백수가 되어도 말 그대로 재택가능ㅋㅋㅋㅋㅋㅋ",
          "original_form": "백수가 되어도 말 그대로 재택가능ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100007752.1.25",
          "form": "어허",
          "original_form": "어허",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100007752.1.26",
          "form": "백수라니",
          "original_form": "백수라니",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100007752.1.27",
          "form": "자택경비원이라고 부르세요",
          "original_form": "자택경비원이라고 부르세요",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100007752.1.28",
          "form": "ㅋㅋㅋㅋ돈은 못벌잖아여...ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ돈은 못벌잖아여...ㅜㅜ",
