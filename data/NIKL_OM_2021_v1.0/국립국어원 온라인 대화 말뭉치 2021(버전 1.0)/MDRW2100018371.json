      "utterance": [
        {
          "id": "MDRW2100018371.1.1",
          "form": "내일 뭐먹지",
          "original_form": "내일 뭐먹지",
          "speaker_id": "1",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100018371.1.2",
          "form": "맛있는거 먹어",
          "original_form": "맛있는거 먹어",
          "speaker_id": "2",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100018371.1.3",
          "form": "라면 어때",
          "original_form": "라면 어때",
          "speaker_id": "2",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100018371.1.4",
          "form": "요새 못 먹어서",
          "original_form": "요새 못 먹어서",
          "speaker_id": "2",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100018371.1.5",
          "form": "땡겨",
          "original_form": "땡겨",
          "speaker_id": "2",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100018371.1.6",
          "form": "라면 좋지",
          "original_form": "라면 좋지",
          "speaker_id": "1",
          "time": "20210827 00:30"
        },
        {
          "id": "MDRW2100018371.1.7",
          "form": "아니면 라멘으로 먹어줘",
          "original_form": "아니면 라멘으로 먹어줘",
          "speaker_id": "2",
          "time": "20210827 00:31"
        },
        {
          "id": "MDRW2100018371.1.8",
          "form": "아니 일본식 라면",
          "original_form": "아니 일본식 라면",
          "speaker_id": "2",
          "time": "20210827 00:31"
        },
        {
          "id": "MDRW2100018371.1.9",
          "form": "바른말 써야지",
          "original_form": "바른말 써야지",
          "speaker_id": "2",
          "time": "20210827 00:31"
        },
        {
          "id": "MDRW2100018371.1.10",
          "form": "지금 못 먹어서 꽂힌거 같아",
          "original_form": "지금 못 먹어서 꽂힌거 같아",
          "speaker_id": "2",
          "time": "20210827 00:31"
        },
        {
          "id": "MDRW2100018371.1.11",
          "form": "라멘도 좋지만 라멘은 자주 먹고싶진 않음",
          "original_form": "라멘도 좋지만 라멘은 자주 먹고싶진 않음",
          "speaker_id": "1",
          "time": "20210827 00:32"
        },
        {
          "id": "MDRW2100018371.1.12",
          "form": "반년에 한번쯤이 딱 좋은듯",
          "original_form": "반년에 한번쯤이 딱 좋은듯",
          "speaker_id": "1",
          "time": "20210827 00:32"
        },
        {
          "id": "MDRW2100018371.1.13",
          "form": "라면 취향은 어때",
          "original_form": "라면 취향은 어때",
          "speaker_id": "2",
          "time": "20210827 00:34"
        },
        {
          "id": "MDRW2100018371.1.14",
          "form": "나는 설익어도 좋고 푹 익어도 좋고 적당히 익은 것도 좋고",
          "original_form": "나는 설익어도 좋고 푹 익어도 좋고 적당히 익은 것도 좋고",
          "speaker_id": "2",
          "time": "20210827 00:35"
        },
        {
          "id": "MDRW2100018371.1.15",
          "form": "난 꼬들한 게 좋아",
          "original_form": "난 꼬들한 게 좋아",
          "speaker_id": "1",
          "time": "20210827 00:35"
        },
        {
          "id": "MDRW2100018371.1.16",
          "form": "계란은 풀어서 끓이니",
          "original_form": "계란은 풀어서 끓이니",
          "speaker_id": "1",
          "time": "20210827 00:35"
        },
        {
          "id": "MDRW2100018371.1.17",
          "form": "제일 좋아하는건 라면이 잘 익었을 때 계란 풀어먹는게 좋아",
          "original_form": "제일 좋아하는건 라면이 잘 익었을 때 계란 풀어먹는게 좋아",
          "speaker_id": "2",
          "time": "20210827 00:35"
        },
        {
          "id": "MDRW2100018371.1.18",
          "form": "다들 싫어하던데ㅋㅋㅋ나는 국물까지 한 입거리 할 수 있어서 좋아해",
          "original_form": "다들 싫어하던데ㅋㅋㅋ나는 국물까지 한 입거리 할 수 있어서 좋아해",
          "speaker_id": "2",
          "time": "20210827 00:35"
        },
        {
          "id": "MDRW2100018371.1.19",
          "form": "왜냐면 국물만 있으면 나트륨 생각한다고 안 먹거든",
          "original_form": "왜냐면 국물만 있으면 나트륨 생각한다고 안 먹거든",
          "speaker_id": "2",
          "time": "20210827 00:36"
        },
        {
          "id": "MDRW2100018371.1.20",
          "form": "근데 계란이 풀렸잖아?",
          "original_form": "근데 계란이 풀렸잖아?",
          "speaker_id": "2",
          "time": "20210827 00:36"
        },
        {
          "id": "MDRW2100018371.1.21",
          "form": "이건 먹어야지",
          "original_form": "이건 먹어야지",
          "speaker_id": "2",
          "time": "20210827 00:36"
        },
        {
          "id": "MDRW2100018371.1.22",
          "form": "난 계란 풀고 계란 하나 더 넣어서 반숙하는 거 좋아함",
          "original_form": "난 계란 풀고 계란 하나 더 넣어서 반숙하는 거 좋아함",
          "speaker_id": "1",
          "time": "20210827 00:36"
        },
        {
          "id": "MDRW2100018371.1.23",
          "form": "왜 하나만 넣죠? 두개 넣으면 되는데 ㅎㅎ",
          "original_form": "왜 하나만 넣죠? 두개 넣으면 되는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:36"
        },
        {
          "id": "MDRW2100018371.1.24",
          "form": "그게 아니어도 계란푼게 취향인데 누가 물어보면 이렇게 말하고 있음",
          "original_form": "그게 아니어도 계란푼게 취향인데 누가 물어보면 이렇게 말하고 있음",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.25",
          "form": "반숙 좋지",
          "original_form": "반숙 좋지",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.26",
          "form": "계란은 한끼에 2개반이 적정이야",
          "original_form": "계란은 한끼에 2개반이 적정이야",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.27",
          "form": "그래서 나는 3개 먹음",
          "original_form": "그래서 나는 3개 먹음",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.28",
          "form": "라면얘기하니까 라면먹고싶잖아",
          "original_form": "라면얘기하니까 라면먹고싶잖아",
          "speaker_id": "1",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.29",
          "form": "이 밤중에",
          "original_form": "이 밤중에",
          "speaker_id": "1",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.30",
          "form": "완숙보단 반숙",
          "original_form": "완숙보단 반숙",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.31",
          "form": "반숙보단 완반숙이지",
          "original_form": "반숙보단 완반숙이지",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.32",
          "form": "ㅎㅎ먹자 라면",
          "original_form": "ㅎㅎ먹자 라면",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.33",
          "form": "내일 아침은 샐러드임",
          "original_form": "내일 아침은 샐러드임",
          "speaker_id": "1",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100018371.1.34",
          "form": "나 대신 파송송 계란탁 해줘",
          "original_form": "나 대신 파송송 계란탁 해줘",
          "speaker_id": "2",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.35",
          "form": "계란은 탁탁으로 부~탁~해요",
          "original_form": "계란은 탁탁으로 부~탁~해요",
          "speaker_id": "2",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.36",
          "form": "양배추랑 적양배추랑 또 뭐들어갔지",
          "original_form": "양배추랑 적양배추랑 또 뭐들어갔지",
          "speaker_id": "1",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.37",
          "form": "양배추도 좋지",
          "original_form": "양배추도 좋지",
          "speaker_id": "2",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.38",
          "form": "식이섬유 풍부하고",
          "original_form": "식이섬유 풍부하고",
          "speaker_id": "2",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.39",
          "form": "발사믹 소스 뿌려서 먹음",
          "original_form": "발사믹 소스 뿌려서 먹음",
          "speaker_id": "1",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.40",
          "form": "소스까지 뿌린다면 라면 반찬에 딱이야",
          "original_form": "소스까지 뿌린다면 라면 반찬에 딱이야",
          "speaker_id": "2",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.41",
          "form": "아주 딱이야",
          "original_form": "아주 딱이야",
          "speaker_id": "2",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.42",
          "form": "별이 다스엇개!",
          "original_form": "별이 다스엇개!",
          "speaker_id": "2",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100018371.1.43",
          "form": "ㅋㅋㅋㅋㅋ 라면얘기 왜 계속하냐고",
          "original_form": "ㅋㅋㅋㅋㅋ 라면얘기 왜 계속하냐고",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018371.1.44",
          "form": "내가 꽂혀서 그래ㅋㅋㅋ",
          "original_form": "내가 꽂혀서 그래ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018371.1.45",
          "form": "주말에 컵누들 먹어야겠다",
          "original_form": "주말에 컵누들 먹어야겠다",
          "speaker_id": "2",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018371.1.46",
          "form": "라면 맛있겠다",
          "original_form": "라면 맛있겠다",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018371.1.47",
          "form": "컵누들 나도 좋아해",
          "original_form": "컵누들 나도 좋아해",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018371.1.48",
          "form": "컵누들에 닭가슴살은 사랑이야",
          "original_form": "컵누들에 닭가슴살은 사랑이야",
          "speaker_id": "2",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018371.1.49",
          "form": "컵누들 매운맛 좋아",
          "original_form": "컵누들 매운맛 좋아",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018371.1.50",
          "form": "괜히 국물까지 다 먹음",
          "original_form": "괜히 국물까지 다 먹음",
          "speaker_id": "2",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018371.1.51",
          "form": "그거 큰컵으로 있으면 좋겠다고 생각함",
          "original_form": "그거 큰컵으로 있으면 좋겠다고 생각함",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.52",
          "form": "나도 매운 맛 좋아",
          "original_form": "나도 매운 맛 좋아",
          "speaker_id": "2",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.53",
          "form": "큰 컵...나는 글쎄",
          "original_form": "큰 컵...나는 글쎄",
          "speaker_id": "2",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.54",
          "form": "컵누들에 닭가슴살 이분 맛잘알이네",
          "original_form": "컵누들에 닭가슴살 이분 맛잘알이네",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.55",
          "form": "육개장 큰컵이 별로이듯",
          "original_form": "육개장 큰컵이 별로이듯",
          "speaker_id": "2",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.56",
          "form": "컵누들도...",
          "original_form": "컵누들도...",
          "speaker_id": "2",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.57",
          "form": "그건 또 그렇긴 한데",
          "original_form": "그건 또 그렇긴 한데",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.58",
          "form": "작은컵 감질난다고",
          "original_form": "작은컵 감질난다고",
          "speaker_id": "1",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.59",
          "form": "나는 그래서 컵누들 2개 먹어",
          "original_form": "나는 그래서 컵누들 2개 먹어",
          "speaker_id": "2",
          "time": "20210827 00:40"
        },
        {
          "id": "MDRW2100018371.1.60",
          "form": "근데 양심상 한개만 먹어...",
          "original_form": "근데 양심상 한개만 먹어...",
          "speaker_id": "2",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018371.1.61",
          "form": "슬퍼",
          "original_form": "슬퍼",
          "speaker_id": "2",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018371.1.62",
          "form": "엌 이건 예상 못함",
          "original_form": "엌 이건 예상 못함",
          "speaker_id": "1",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018371.1.63",
          "form": "비빔면이 2개가 적정이듯",
          "original_form": "비빔면이 2개가 적정이듯",
          "speaker_id": "2",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018371.1.64",
          "form": "컵누들도 2개",
          "original_form": "컵누들도 2개",
          "speaker_id": "2",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100018371.1.65",
