      "utterance": [
        {
          "id": "MDRW2100006671.1.1",
          "form": "일은 적성에 맞으시죠?",
          "original_form": "일은 적성에 맞으시죠?",
          "speaker_id": "1",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100006671.1.2",
          "form": "운동좋아하세요?",
          "original_form": "운동좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100006671.1.3",
          "form": "네 꾸준히 합니다 ㅎ",
          "original_form": "네 꾸준히 합니다 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100006671.1.4",
          "form": "오 운동 잘하실 것 같아요",
          "original_form": "오 운동 잘하실 것 같아요",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006671.1.5",
          "form": "저는 겨우 숨만 쉽니다ㅋㅋㅋㅋ",
          "original_form": "저는 겨우 숨만 쉽니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006671.1.6",
          "form": "잘하진 않고 꾸준히 하려고 노력합니다",
          "original_form": "잘하진 않고 꾸준히 하려고 노력합니다",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100006671.1.7",
          "form": "꾸준함이 가장 중요한 거니까요",
          "original_form": "꾸준함이 가장 중요한 거니까요",
          "speaker_id": "2",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100006671.1.8",
          "form": "제 동생도 운동 시작했던데 전 매번 작심삼일이네요ㅋㅋ",
          "original_form": "제 동생도 운동 시작했던데 전 매번 작심삼일이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006671.1.9",
          "form": "동생분은 어떤 운동하시나요",
          "original_form": "동생분은 어떤 운동하시나요",
          "speaker_id": "1",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100006671.1.10",
          "form": "헬스 다니더라구요. 막내라 이제 성인돼서 신났어요ㅋㅋ",
          "original_form": "헬스 다니더라구요. 막내라 이제 성인돼서 신났어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100006671.1.11",
          "form": "아 저도 딱 그때부터 헬스했어요 ㅎㅎ",
          "original_form": "아 저도 딱 그때부터 헬스했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100006671.1.12",
          "form": "와 그럼 오래하셨네요?! 완전 경력자신데",
          "original_form": "와 그럼 오래하셨네요?! 완전 경력자신데",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006671.1.13",
          "form": "근데 중간중간 쉬는때도 많았구요",
          "original_form": "근데 중간중간 쉬는때도 많았구요",
          "speaker_id": "1",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100006671.1.14",
          "form": "에이 전 아예 안하는데요ㅋㅋㅋ",
          "original_form": "에이 전 아예 안하는데요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100006671.1.15",
          "form": "근데 확실히 해가 바뀔 수록 여기저기 아파서 운동을 하긴 해야겠어요",
          "original_form": "근데 확실히 해가 바뀔 수록 여기저기 아파서 운동을 하긴 해야겠어요",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100006671.1.16",
          "form": "맞아요 과거엔 몸만들려고 한건데 지금은 건강을 위해...",
          "original_form": "맞아요 과거엔 몸만들려고 한건데 지금은 건강을 위해...",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006671.1.17",
          "form": "그죠...근육이 있어야 자세도 바르구요ㅠㅠ",
          "original_form": "그죠...근육이 있어야 자세도 바르구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:24"
        }
      ]
