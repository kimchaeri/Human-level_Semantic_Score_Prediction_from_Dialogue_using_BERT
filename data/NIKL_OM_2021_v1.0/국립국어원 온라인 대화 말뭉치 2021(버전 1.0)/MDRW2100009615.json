      "utterance": [
        {
          "id": "MDRW2100009615.1.1",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "1",
          "time": "20210823 01:44"
        },
        {
          "id": "MDRW2100009615.1.2",
          "form": "학교에서 다 배운걸",
          "original_form": "학교에서 다 배운걸",
          "speaker_id": "1",
          "time": "20210823 01:44"
        },
        {
          "id": "MDRW2100009615.1.3",
          "form": "또 가서 또 배우니까 흥미도없고",
          "original_form": "또 가서 또 배우니까 흥미도없고",
          "speaker_id": "1",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.4",
          "form": "사실 대학이라는 곳이..",
          "original_form": "사실 대학이라는 곳이..",
          "speaker_id": "1",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.5",
          "form": "아 그렇겠네요",
          "original_form": "아 그렇겠네요",
          "speaker_id": "2",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.6",
          "form": "진짜 말만 대학이고",
          "original_form": "진짜 말만 대학이고",
          "speaker_id": "1",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.7",
          "form": "고등학교 때 이미 마스터했으니",
          "original_form": "고등학교 때 이미 마스터했으니",
          "speaker_id": "2",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.8",
          "form": "가르치는건 음.. 싶은거 있잖아요",
          "original_form": "가르치는건 음.. 싶은거 있잖아요",
          "speaker_id": "1",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.9",
          "form": "가라판으로 돌아가던가요?",
          "original_form": "가라판으로 돌아가던가요?",
          "speaker_id": "2",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.11",
          "form": "사실 교대도 그렇긴 해요",
          "original_form": "사실 교대도 그렇긴 해요",
          "speaker_id": "2",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.12",
          "form": "음......",
          "original_form": "음......",
          "speaker_id": "2",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.13",
          "form": "어디든 다 비슷한가봐요ㅋㅋ",
          "original_form": "어디든 다 비슷한가봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009615.1.14",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009615.1.15",
          "form": "전 다른 대학 다니는 친구들",
          "original_form": "전 다른 대학 다니는 친구들",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009615.1.16",
          "form": "진짜 돈을 버린다면 버리는..느낌",
          "original_form": "진짜 돈을 버린다면 버리는..느낌",
          "speaker_id": "1",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009615.1.17",
          "form": "부러웠던 게",
          "original_form": "부러웠던 게",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009615.1.18",
          "form": "뭔가 있어보이더라고요",
          "original_form": "뭔가 있어보이더라고요",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009615.1.19",
          "form": "저희는",
          "original_form": "저희는",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009615.1.20",
          "form": "개나리 노란 꽃그늘 아래",
          "original_form": "개나리 노란 꽃그늘 아래",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009615.1.21",
          "form": "이런 노래 맞춰서",
          "original_form": "이런 노래 맞춰서",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009615.1.22",
          "form": "가지런히  놓여있는 꼬까신하나",
          "original_form": "가지런히  놓여있는 꼬까신하나",
          "speaker_id": "1",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.23",
          "form": "율동 짜고 있고",
          "original_form": "율동 짜고 있고",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.24",
          "form": "네 그거요",
          "original_form": "네 그거요",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.27",
          "form": "실과 시간에 토마토 키우고",
          "original_form": "실과 시간에 토마토 키우고",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.28",
          "form": "그거 좋아보이는데",
          "original_form": "그거 좋아보이는데",
          "speaker_id": "1",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.29",
          "form": "수영 배우고",
          "original_form": "수영 배우고",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.30",
          "form": "괜찮은데?",
          "original_form": "괜찮은데?",
          "speaker_id": "1",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.31",
          "form": "아 그게 그나마 재밌었어요",
          "original_form": "아 그게 그나마 재밌었어요",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.32",
          "form": "이 두개가",
          "original_form": "이 두개가",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009615.1.33",
          "form": "피아노랑 무용 코딩이 진짜 힘들었고",
          "original_form": "피아노랑 무용 코딩이 진짜 힘들었고",
          "speaker_id": "2",
          "time": "20210823 01:48"
        }
      ]
    }
