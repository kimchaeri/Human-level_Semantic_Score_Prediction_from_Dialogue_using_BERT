        {
          "id": "MDRW2100020130.1.1",
          "form": "name1은 배달음식 자주드세요?",
          "original_form": "&name1&은 배달음식 자주드세요?",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020130.1.2",
          "form": "가끔여 ㅎㅎ",
          "original_form": "가끔여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020130.1.3",
          "form": "name2은 어떤 음식 좋아해여?",
          "original_form": "&name2&은 어떤 음식 좋아해여?",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020130.1.4",
          "form": "전 다좋아해요ㅎ 뭐든안가리는데 오이빼구요ㅎㅎ",
          "original_form": "전 다좋아해요ㅎ 뭐든안가리는데 오이빼구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020130.1.5",
          "form": "김치종류 좋아해요ㅎ 김치찌게, 특히 볶은거ㅎㅎ",
          "original_form": "김치종류 좋아해요ㅎ 김치찌게, 특히 볶은거ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020130.1.6",
          "form": "어 전 가리는게 좀 되요",
          "original_form": "어 전 가리는게 좀 되요",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020130.1.7",
          "form": "name1은요?ㅎ",
          "original_form": "&name1&은요?ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020130.1.8",
          "form": "전 안좋아하는거 말하는게 빨라욮ㅋ",
          "original_form": "전 안좋아하는거 말하는게 빨라욮ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020130.1.9",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020130.1.10",
          "form": "생선회 . 육회  . 번데기 안먹어요 ㅋ",
          "original_form": "생선회 . 육회  . 번데기 안먹어요 ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020130.1.11",
          "form": "오오ㅎ",
          "original_form": "오오ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020130.1.12",
          "form": "그럴수있오요ㅎ",
          "original_form": "그럴수있오요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020130.1.13",
          "form": "멍뭉탕 염소탕 요런거  안먹구요",
          "original_form": "멍뭉탕 염소탕 요런거  안먹구요",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020130.1.14",
          "form": "그건 저도 싫어요ㅎㅎ",
          "original_form": "그건 저도 싫어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020130.1.15",
          "form": "어죽 안먹어요 민물고기 패스 추어탕 포함",
          "original_form": "어죽 안먹어요 민물고기 패스 추어탕 포함",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020130.1.16",
          "form": "생걸 잘 안드시는군요ㅎ",
          "original_form": "생걸 잘 안드시는군요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020130.1.17",
          "form": "징그러운거나ㅎ",
          "original_form": "징그러운거나ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020130.1.18",
          "form": "아뇨 떠 그렇지더 않아요",
          "original_form": "아뇨 떠 그렇지더 않아요",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020130.1.19",
          "form": "아아ㅎㅎ",
          "original_form": "아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020130.1.20",
          "form": "산낙진 킬러에요",
