      "utterance": [
        {
          "id": "MDRW2100045208.1.1",
          "form": "ㅇㅇㅇ..",
          "original_form": "ㅇㅇㅇ..",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045208.1.2",
          "form": "전설의 영화",
          "original_form": "전설의 영화",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045208.1.3",
          "form": "그 얼어붙은 영화투잦심리에",
          "original_form": "그 얼어붙은 영화투잦심리에",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045208.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그게 스노우볼 굴러간거였구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그게 스노우볼 굴러간거였구나",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045208.1.5",
          "form": "뮤지컬로 옮겨가는바람에",
          "original_form": "뮤지컬로 옮겨가는바람에",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045208.1.6",
          "form": "우리나라 뮤시장이",
          "original_form": "우리나라 뮤시장이",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045208.1.7",
          "form": "커졌던것...",
          "original_form": "커졌던것...",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.9",
          "form": "그래서요새는 뮤지컬도 수출하던데",
          "original_form": "그래서요새는 뮤지컬도 수출하던데",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.10",
          "form": "일본으로...",
          "original_form": "일본으로...",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.11",
          "form": "한쪽 문이 닫히면 한쪽 문이 열리네..",
          "original_form": "한쪽 문이 닫히면 한쪽 문이 열리네..",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.12",
          "form": "우리나라 창뮤?",
          "original_form": "우리나라 창뮤?",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.13",
          "form": "그 유툽",
          "original_form": "그 유툽",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.14",
          "form": "팔릴만한거 뭐있나",
          "original_form": "팔릴만한거 뭐있나",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.15",
          "form": "프랑켄..?",
          "original_form": "프랑켄..?",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.16",
          "form": "좋아하진 않지만 호카손이",
          "original_form": "좋아하진 않지만 호카손이",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.17",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.18",
          "form": "비교많이하는거같은데",
          "original_form": "비교많이하는거같은데",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.20",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.21",
          "form": "라센이 팔림 심지어",
          "original_form": "라센이 팔림 심지어",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045208.1.22",
          "form": "우리나라버전으로",
          "original_form": "우리나라버전으로",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045208.1.23",
          "form": "디용",
          "original_form": "디용",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045208.1.24",
          "form": "오디가 라센으로 지킬팔고",
          "original_form": "오디가 라센으로 지킬팔고",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045208.1.25",
          "form": "그게... 가능한거구나",
          "original_form": "그게... 가능한거구나",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045208.1.26",
          "form": "엠뮤가 삼총사팔고",
          "original_form": "엠뮤가 삼총사팔고",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045208.1.27",
          "form": "원작이 아니라 리메이크작을 사가다니",
          "original_form": "원작이 아니라 리메이크작을 사가다니",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045208.1.28",
          "form": "ㅇㅇㅇ게다가 우리나라는 티저나 영상도 기깔나게 뽑고",
          "original_form": "ㅇㅇㅇ게다가 우리나라는 티저나 영상도 기깔나게 뽑고",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045208.1.29",
          "form": "노래못하면 가차없어서",
          "original_form": "노래못하면 가차없어서",
          "speaker_id": "1",
          "time": "20210908 01:20"
        }
      ]
    }
  ]
