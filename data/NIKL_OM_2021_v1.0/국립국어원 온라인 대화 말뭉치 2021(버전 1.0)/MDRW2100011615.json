      "utterance": [
        {
          "id": "MDRW2100011615.1.1",
          "form": "아니 그언니가 다른데로 발령나서 금방 헤어졌음",
          "original_form": "아니 그언니가 다른데로 발령나서 금방 헤어졌음",
          "speaker_id": "2",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100011615.1.2",
          "form": "또 직원들이 내한테 길들여져서 괜찮았다 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "또 직원들이 내한테 길들여져서 괜찮았다 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100011615.1.3",
          "form": "그럼 다행이네요~그거 진짜 스트레스임 ㅋㅋㅋㅋ",
          "original_form": "그럼 다행이네요~그거 진짜 스트레스임 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:38"
        },
        {
          "id": "MDRW2100011615.1.4",
          "form": "잘 길들였네요 온니 ㅋㅋㅋㅋ",
          "original_form": "잘 길들였네요 온니 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:38"
        },
        {
          "id": "MDRW2100011615.1.5",
          "form": "윽시 아쉬웠을듯 ㅋㅋㅋ",
          "original_form": "윽시 아쉬웠을듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:38"
        },
        {
          "id": "MDRW2100011615.1.6",
          "form": "그래 ㅎㅎ\r\n예의바른것도 중간 정도만 하믄 되지",
          "original_form": "그래 ㅎㅎ\r\n예의바른것도 중간 정도만 하믄 되지",
          "speaker_id": "2",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011615.1.7",
          "form": "무슨 변기청소에 구두정리까지...",
          "original_form": "무슨 변기청소에 구두정리까지...",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011615.1.8",
          "form": "그렇게는 안하고 싶음",
          "original_form": "그렇게는 안하고 싶음",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011615.1.9",
          "form": "인사만 잘하믄되지 ㅋㅋㅋㅋ",
          "original_form": "인사만 잘하믄되지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011615.1.10",
          "form": "맞아요언니~그건 너무 갔슈 ㅋㅋㅋ",
          "original_form": "맞아요언니~그건 너무 갔슈 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011615.1.11",
          "form": "같이 일하는사람이 스트레스 받음~",
          "original_form": "같이 일하는사람이 스트레스 받음~",
          "speaker_id": "1",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011615.1.12",
          "form": "그러이 ㅎㅎ",
          "original_form": "그러이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011615.1.13",
          "form": "혼자 일하는건 어떠노??",
          "original_form": "혼자 일하는건 어떠노??",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011615.1.14",
          "form": "젱 좋져 ㅋㅋㅋ한번씩 심심할때도 있지만~",
          "original_form": "젱 좋져 ㅋㅋㅋ한번씩 심심할때도 있지만~",
          "speaker_id": "1",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011615.1.15",
          "form": "솔직히 사람스트레스가 젱 힘드니깐요~",
          "original_form": "솔직히 사람스트레스가 젱 힘드니깐요~",
          "speaker_id": "1",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011615.1.16",
          "form": "그렇지 나도 혼자서 해보고싶다",
          "original_form": "그렇지 나도 혼자서 해보고싶다",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.17",
          "form": "언니 심심할껄요??",
          "original_form": "언니 심심할껄요??",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그런가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그런가",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.19",
          "form": "근데 뭐 눈치 볼건 없어서 ㅋㅋㅋㅋ",
          "original_form": "근데 뭐 눈치 볼건 없어서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.20",
          "form": "짧게...심심하지않게...",
          "original_form": "짧게...심심하지않게...",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.21",
          "form": "긴 시간말고 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "긴 시간말고 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.22",
          "form": "다 장단점이 있겠쥬??",
          "original_form": "다 장단점이 있겠쥬??",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.23",
          "form": "아르바이트 인생 ㅋㅋㅋㅋ",
          "original_form": "아르바이트 인생 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.24",
          "form": "지금은 길시간 일을 안하니 딱 좋다",
          "original_form": "지금은 길시간 일을 안하니 딱 좋다",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011615.1.25",
          "form": "애들 키울때는 어쩔수 없으니",
          "original_form": "애들 키울때는 어쩔수 없으니",
          "speaker_id": "2",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011615.1.26",
          "form": "애들 키우면서 일하는게 쉽지않음",
          "original_form": "애들 키우면서 일하는게 쉽지않음",
          "speaker_id": "2",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011615.1.27",
          "form": "name3언니 일하는 시간대가 제일 괜찮은듯",
          "original_form": "&name3&언니 일하는 시간대가 제일 괜찮은듯",
          "speaker_id": "2",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011615.1.28",
          "form": "맞아여~애들보내고 바로가서~",
          "original_form": "맞아여~애들보내고 바로가서~",
          "speaker_id": "1",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011615.1.29",
          "form": "글고 필요하면 집에.들렸다 가도되고~",
          "original_form": "글고 필요하면 집에.들렸다 가도되고~",
          "speaker_id": "1",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011615.1.30",
          "form": "그게 진짜 큰듯요^^",
          "original_form": "그게 진짜 큰듯요^^",
          "speaker_id": "1",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011615.1.31",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011615.1.32",
          "form": "중간에 애들 밥 챙겨주러 와도 되니",
          "original_form": "중간에 애들 밥 챙겨주러 와도 되니",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011615.1.33",
          "form": "그게 좋지",
          "original_form": "그게 좋지",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011615.1.34",
          "form": "그래가 난도 자격증 준비했습죠 ㅋㅋㅋㅋ",
          "original_form": "그래가 난도 자격증 준비했습죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011615.1.35",
          "form": "끝났지만 ㅋㅋㅋㅋㅋ",
          "original_form": "끝났지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011615.1.36",
          "form": "근데 예전에 내가 뭐 팔아 라는 얘기를 진짜 많이 들었어",
          "original_form": "근데 예전에 내가 뭐 팔아 라는 얘기를 진짜 많이 들었어",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100011615.1.37",
          "form": "화장품 같은 거 팔아도 잘 팔리겠다고",
          "original_form": "화장품 같은 거 팔아도 잘 팔리겠다고",
