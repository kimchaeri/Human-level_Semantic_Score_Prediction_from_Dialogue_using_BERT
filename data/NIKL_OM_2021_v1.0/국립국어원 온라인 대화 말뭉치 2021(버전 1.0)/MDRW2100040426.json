      "utterance": [
        {
          "id": "MDRW2100040426.1.1",
          "form": "좋은 사람과 좋은곳에서",
          "original_form": "좋은 사람과 좋은곳에서",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100040426.1.2",
          "form": "좋은거보며 맛난거먹는게",
          "original_form": "좋은거보며 맛난거먹는게",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100040426.1.3",
          "form": "최고야",
          "original_form": "최고야",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100040426.1.4",
          "form": "맞아!!! 정말 최고야",
          "original_form": "맞아!!! 정말 최고야",
          "speaker_id": "1",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100040426.1.5",
          "form": "아니근데",
          "original_form": "아니근데",
          "speaker_id": "1",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100040426.1.6",
          "form": "음식까지 맛있기는 힘든걸까?",
          "original_form": "음식까지 맛있기는 힘든걸까?",
          "speaker_id": "1",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100040426.1.7",
          "form": "아주 비싼곳이면 가능할지도,,,,'0'",
          "original_form": "아주 비싼곳이면 가능할지도,,,,'0'",
          "speaker_id": "2",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100040426.1.8",
          "form": "카페들도 맛없고 뷰좋은곳은 많앜ㅋㅋㅋ",
          "original_form": "카페들도 맛없고 뷰좋은곳은 많앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100040426.1.9",
          "form": "와,,,",
          "original_form": "와,,,",
          "speaker_id": "1",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100040426.1.10",
          "form": "뷰좋은곳에 내가 레스토랑 내고싶닼ㅋㅋ",
          "original_form": "뷰좋은곳에 내가 레스토랑 내고싶닼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040426.1.11",
          "form": "나도 맨날 그 생각 해,,",
          "original_form": "나도 맨날 그 생각 해,,",
          "speaker_id": "1",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040426.1.12",
          "form": "특히 카페~!",
          "original_form": "특히 카페~!",
          "speaker_id": "1",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040426.1.13",
          "form": "음식점은 책임질 것이 많아보여서",
          "original_form": "음식점은 책임질 것이 많아보여서",
          "speaker_id": "1",
          "time": "20210904 19:41"
        },
        {
          "id": "MDRW2100040426.1.14",
          "form": "음 역시 씨엘 가는게 가장 괜찮을거같아..",
          "original_form": "음 역시 씨엘 가는게 가장 괜찮을거같아..{emoji:☆}",
          "speaker_id": "1",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100040426.1.15",
          "form": "ㅎㅎ잠깐 저녁먹고올게요",
          "original_form": "ㅎㅎ잠깐 저녁먹고올게요",
          "speaker_id": "2",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100040426.1.16",
          "form": "진짜 카페 만들구싶오 그지",
          "original_form": "진짜 카페 만들구싶오 그지",
          "speaker_id": "2",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100040426.1.17",
          "form": "응응!!!",
          "original_form": "응응!!!",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100040426.1.18",
          "form": "나도 이제 저녁 먹었어 ㅎㅎㅎ 짱 배부르다!",
          "original_form": "나도 이제 저녁 먹었어 ㅎㅎㅎ 짱 배부르다!",
          "speaker_id": "1",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100040426.1.19",
          "form": "맞아.. 그리고 예쁜 접시 이런걸로 플레이팅 하고싶어ㅠ",
          "original_form": "맞아.. 그리고 예쁜 접시 이런걸로 플레이팅 하고싶어ㅠ",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100040426.1.20",
          "form": "ㅎㅎ저녁모먹었어",
          "original_form": "ㅎㅎ저녁모먹었어",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100040426.1.21",
          "form": "맛난거먹었어?",
          "original_form": "맛난거먹었어?",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100040426.1.22",
          "form": "씨엘가는걸루 하구 낼까지 시간있으니 다른 곳 괜찮은곳 발견하묜 거기루 갈까아?",
          "original_form": "씨엘가는걸루 하구 낼까지 시간있으니 다른 곳 괜찮은곳 발견하묜 거기루 갈까아?",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100040426.1.23",
          "form": "팥죽이랑 수제비 먹었는데 정말 다 싱거웠지뭐야~!~!!",
          "original_form": "팥죽이랑 수제비 먹었는데 정말 다 싱거웠지뭐야~!~!!",
          "speaker_id": "1",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100040426.1.24",
          "form": "아주즇아 아주 좋아~!",
          "original_form": "아주즇아 아주 좋아~!",
          "speaker_id": "1",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100040426.1.25",
          "form": "나는 정말 좋아~!",
          "original_form": "나는 정말 좋아~!{emoji:☆}",
          "speaker_id": "1",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100040426.1.26",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100040426.1.27",
          "form": "집에서 먹었어?",
          "original_form": "집에서 먹었어?",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100040426.1.28",
          "form": "팥죽 맛있겠다",
          "original_form": "팥죽 맛있겠다",
          "speaker_id": "2",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040426.1.29",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040426.1.30",
          "form": "안먹은지 무지 오래됐어 ㅇㅎ",
          "original_form": "안먹은지 무지 오래됐어 ㅇㅎ",
          "speaker_id": "2",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040426.1.31",
          "form": "오늘 완전 파워집순이",
          "original_form": "오늘 완전 파워집순이",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040426.1.32",
          "form": "내일 외출하니까 오늘은 집에있는날이야",
          "original_form": "내일 외출하니까 오늘은 집에있는날이야",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040426.1.33",
          "form": "주말 내내 외출하면 기력이 달린달까..",
          "original_form": "주말 내내 외출하면 기력이 달린달까..{emoji:☆}",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040426.1.34",
          "form": "응응 근데 아주 싱거웠어!",
          "original_form": "응응 근데 아주 싱거웠어!",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040426.1.35",
          "form": "민덩이는 뭐먹었어?!",
          "original_form": "민덩이는 뭐먹었어?!",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100040426.1.36",
          "form": "몸에는 좋았을거야 :):)",
          "original_form": "몸에는 좋았을거야 :):)",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100040426.1.37",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040426.1.38",
          "form": "이따가 엄빠랑 산책하러 나가야지",
          "original_form": "이따가 엄빠랑 산책하러 나가야지",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040426.1.39",
          "form": "민덩이는 오늘",
          "original_form": "민덩이는 오늘",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040426.1.40",
          "form": "나는 김치찌개 먹었어",
          "original_form": "나는 김치찌개 먹었어",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040426.1.41",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040426.1.42",
          "form": "ㅋㅋㅋㅋ사이 진짜 좋아 신기해야",
          "original_form": "ㅋㅋㅋㅋ사이 진짜 좋아 신기해야",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040426.1.43",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040426.1.44",
          "form": "돼지고기가 들어있으려나?!",
          "original_form": "돼지고기가 들어있으려나?!",
          "speaker_id": "1",
