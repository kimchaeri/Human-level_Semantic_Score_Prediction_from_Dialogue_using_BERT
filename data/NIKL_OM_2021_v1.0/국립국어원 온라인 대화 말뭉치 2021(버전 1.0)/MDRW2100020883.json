      "utterance": [
        {
          "id": "MDRW2100020883.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020883.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100020883.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100020883.1.4",
          "form": "네 빵 먹었어요",
          "original_form": "네 빵 먹었어요",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020883.1.5",
          "form": "오 빵 좋아하시나여?",
          "original_form": "오 빵 좋아하시나여?",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020883.1.6",
          "form": "네 완전 빵수니 ㅎㅎ",
          "original_form": "네 완전 빵수니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020883.1.7",
          "form": "저두요~ 빵투어 이런것두해요 ㅋㅋㅋ",
          "original_form": "저두요~ 빵투어 이런것두해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020883.1.8",
          "form": "와우 ㅎㅎㅎ 그거 좋네요",
          "original_form": "와우 ㅎㅎㅎ 그거 좋네요",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020883.1.9",
          "form": "맞아요~ 요새는 빵지순례간다고 하더라구욬ㅋㅋㅋ",
          "original_form": "맞아요~ 요새는 빵지순례간다고 하더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020883.1.10",
          "form": "ㅎㅎ 카레빵인데 고기가 들어서 맛있어요",
          "original_form": "ㅎㅎ 카레빵인데 고기가 들어서 맛있어요",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020883.1.11",
          "form": "오 카레빵 자주드시나봐요?",
          "original_form": "오 카레빵 자주드시나봐요?",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020883.1.12",
          "form": "이번에 신상 나왔대서 먹어봤어요 ㅎㅎ",
          "original_form": "이번에 신상 나왔대서 먹어봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020883.1.13",
          "form": "오! 어디빵집인가요?",
          "original_form": "오! 어디빵집인가요?",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020883.1.14",
          "form": "뜨레쥬르요 ㅎㅎ",
          "original_form": "뜨레쥬르요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020883.1.15",
          "form": "아하! 뚜레쥬르좋아하세요 파리바게트더좋아하세여?",
          "original_form": "아하! 뚜레쥬르좋아하세요 파리바게트더좋아하세여?",
          "speaker_id": "1",
          "time": "20210828 14:38"
        },
        {
          "id": "MDRW2100020883.1.16",
          "form": "전 둘다 똑같은 거 같아요 ㅎㅎ",
          "original_form": "전 둘다 똑같은 거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020883.1.17",
          "form": "ㅋㅋㅋㅋ 맞아여 거기서거기",
          "original_form": "ㅋㅋㅋㅋ 맞아여 거기서거기",
          "speaker_id": "1",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020883.1.18",
          "form": "그래도 나름 점바점이 있더라구요",
          "original_form": "그래도 나름 점바점이 있더라구요",
          "speaker_id": "1",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020883.1.19",
          "form": "ㅎㅎ 빵은 사랑",
          "original_form": "ㅎㅎ 빵은 사랑",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020883.1.20",
          "form": "맞아요 점바점",
          "original_form": "맞아요 점바점",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020883.1.21",
          "form": "ㅋㅋㅋ 어떤빵종류 좋아하세요?",
          "original_form": "ㅋㅋㅋ 어떤빵종류 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020883.1.22",
          "form": "전 담백한거 좋아해요",
          "original_form": "전 담백한거 좋아해요",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020883.1.23",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100020883.1.24",
          "form": "오! 전 치아바타같은 사워도우좋아해요~",
          "original_form": "오! 전 치아바타같은 사워도우좋아해요~",
