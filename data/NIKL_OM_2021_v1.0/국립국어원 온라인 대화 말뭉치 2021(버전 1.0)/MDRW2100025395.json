      "utterance": [
        {
          "id": "MDRW2100025395.1.1",
          "form": "대머리 아저씨 나오는 게임",
          "original_form": "대머리 아저씨 나오는 게임",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025395.1.2",
          "form": "꿈의정원....업데이트안되서",
          "original_form": "꿈의정원....업데이트안되서",
          "speaker_id": "1",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025395.1.3",
          "form": "플레이가안돼",
          "original_form": "플레이가안돼",
          "speaker_id": "1",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025395.1.4",
          "form": "운영팀한테 메일세번보냈는데",
          "original_form": "운영팀한테 메일세번보냈는데",
          "speaker_id": "1",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025395.1.5",
          "form": "답이없어",
          "original_form": "답이없어",
          "speaker_id": "1",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025395.1.6",
          "form": "꿈의정원도 가챠가 있나요?",
          "original_form": "꿈의정원도 가챠가 있나요?",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025395.1.7",
          "form": "대신아이템",
          "original_form": "대신아이템",
          "speaker_id": "1",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025395.1.8",
          "form": "팔아",
          "original_form": "팔아",
          "speaker_id": "1",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025395.1.9",
          "form": "돈 많이 벌겠네",
          "original_form": "돈 많이 벌겠네",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025395.1.10",
          "form": "난이도를",
          "original_form": "난이도를",
          "speaker_id": "1",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025395.1.11",
          "form": "재도전권을팔아",
          "original_form": "재도전권을팔아",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025395.1.12",
          "form": "그거알아??? 그 대머리아저씨",
          "original_form": "그거알아??? 그 대머리아저씨",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025395.1.13",
          "form": "개발자랑 똑같이생김",
          "original_form": "개발자랑 똑같이생김",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025395.1.14",
          "form": "바로 또 킹받네",
          "original_form": "바로 또 킹받네",
          "speaker_id": "2",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025395.1.15",
          "form": "대머리 볼때마다 울화가 치밈",
          "original_form": "대머리 볼때마다 울화가 치밈",
          "speaker_id": "2",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025395.1.16",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ그래서 막 아이돌처럼",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ그래서 막 아이돌처럼",
          "speaker_id": "1",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025395.1.17",
          "form": "사진자꾸풀던데",
          "original_form": "사진자꾸풀던데",
          "speaker_id": "1",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025395.1.18",
          "form": "자캐인듯",
          "original_form": "자캐인듯",
          "speaker_id": "1",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025395.1.19",
          "form": "유튜브광고때문인거같아",
          "original_form": "유튜브광고때문인거같아",
          "speaker_id": "2",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025395.1.20",
          "form": "ㅇㅇㅇㅇㅇ 광고를넣어야",
          "original_form": "ㅇㅇㅇㅇㅇ 광고를넣어야",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025395.1.21",
          "form": "하니까",
          "original_form": "하니까",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025395.1.22",
          "form": "아근더 그게임 너무 광고랑달라서",
          "original_form": "아근더 그게임 너무 광고랑달라서",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025395.1.23",
          "form": "영국서는 광고규제먹음",
          "original_form": "영국서는 광고규제먹음",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025395.1.24",
          "form": "너무사기수준",
          "original_form": "너무사기수준",
          "speaker_id": "1",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100025395.1.25",
          "form": "맞아여 그냥",
          "original_form": "맞아여 그냥",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025395.1.26",
          "form": "퍼즐이면서 광고는 무슨",
          "original_form": "퍼즐이면서 광고는 무슨",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025395.1.27",
          "form": "다른게임인거처럼 나옴",
          "original_form": "다른게임인거처럼 나옴",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025395.1.28",
          "form": "더 킹받죠",
          "original_form": "더 킹받죠",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025395.1.29",
          "form": "ㅇㅇ 킹받아",
          "original_form": "ㅇㅇ 킹받아",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025395.1.30",
          "form": "아니 나는 집고치는게임하고 싶은데",
          "original_form": "아니 나는 집고치는게임하고 싶은데",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025395.1.31",
          "form": "알고보니 퍼즐게임",
          "original_form": "알고보니 퍼즐게임",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025395.1.32",
          "form": "집고치는 게임이 아니었어",
          "original_form": "집고치는 게임이 아니었어",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025395.1.33",
          "form": "갑분퍼즐게밈",
          "original_form": "갑분퍼즐게밈",
          "speaker_id": "2",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025395.1.34",
          "form": "ㅇㅇㅇ 그냥 가구배치는 부차적인거",
          "original_form": "ㅇㅇㅇ 그냥 가구배치는 부차적인거",
          "speaker_id": "1",
          "time": "20210901 10:57"
        }
      ]
