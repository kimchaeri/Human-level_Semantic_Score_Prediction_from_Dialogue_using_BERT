      "utterance": [
        {
          "id": "MDRW2100019552.1.1",
          "form": "딱 좋은데 좀 습했어",
          "original_form": "딱 좋은데 좀 습했어",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019552.1.2",
          "form": "그래도 좀 습한 것만 빼면 좋았던거 같아",
          "original_form": "그래도 좀 습한 것만 빼면 좋았던거 같아",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019552.1.3",
          "form": "맞아 근데 돌아올 땐 좀 춥더라",
          "original_form": "맞아 근데 돌아올 땐 좀 춥더라",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019552.1.4",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019552.1.5",
          "form": "난 별로 안춥던데",
          "original_form": "난 별로 안춥던데",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019552.1.6",
          "form": "응 난 좀 추웠어",
          "original_form": "응 난 좀 추웠어",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019552.1.7",
          "form": "미세먼지는 별로 없었지?",
          "original_form": "미세먼지는 별로 없었지?",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019552.1.8",
          "form": "느끼기엔 별로 없었던 거 같아",
          "original_form": "느끼기엔 별로 없었던 거 같아",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019552.1.9",
          "form": "계속 이런 날씨면 좋겠다",
          "original_form": "계속 이런 날씨면 좋겠다",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019552.1.10",
          "form": "맞아 그럼 좋을 거 같아",
          "original_form": "맞아 그럼 좋을 거 같아",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019552.1.11",
          "form": "겨울엔 집에만 있을 예정 ㅠㅜ",
          "original_form": "겨울엔 집에만 있을 예정 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019552.1.12",
          "form": "나도 못나가 전기장판과 나는 하나야",
          "original_form": "나도 못나가 전기장판과 나는 하나야",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019552.1.13",
          "form": "이불 밖은 위험해 이건가?",
          "original_form": "이불 밖은 위험해 이건가?",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019552.1.14",
          "form": "응 난 전기이불이랑도 한 몸이야",
          "original_form": "응 난 전기이불이랑도 한 몸이야",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019552.1.15",
          "form": "ㅋㅋㅋㅋㅋ 그럴 수 있지",
          "original_form": "ㅋㅋㅋㅋㅋ 그럴 수 있지",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019552.1.16",
          "form": "오늘 산책 갔어?",
          "original_form": "오늘 산책 갔어?",
