      "utterance": [
        {
          "id": "MDRW2100021950.1.1",
          "form": "거기 오늘 비왓어?",
          "original_form": "거기 오늘 비왓어?",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021950.1.2",
          "form": "쪼금왔다고 뉴스나오던데",
          "original_form": "쪼금왔다고 뉴스나오던데",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021950.1.3",
          "form": "여긴 흐리기만",
          "original_form": "여긴 흐리기만",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021950.1.4",
          "form": "낼은 비온대",
          "original_form": "낼은 비온대",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021950.1.5",
          "form": "월요일엔 안왔으면 좋겠네..",
          "original_form": "월요일엔 안왔으면 좋겠네..",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021950.1.6",
          "form": "월요일은 안오는거 같음",
          "original_form": "월요일은 안오는거 같음",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021950.1.7",
          "form": "다행쓰",
          "original_form": "다행쓰",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021950.1.8",
          "form": "기상청 못믿지만",
          "original_form": "기상청 못믿지만",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021950.1.9",
          "form": "앜ㅋㅋ",
          "original_form": "앜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021950.1.10",
          "form": "태풍은 더오려나?",
          "original_form": "태풍은 더오려나?",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021950.1.11",
          "form": "믿음 박살",
          "original_form": "믿음 박살",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021950.1.12",
          "form": "가을태풍올걸",
          "original_form": "가을태풍올걸",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021950.1.13",
          "form": "안돼...ㅜㅜ",
          "original_form": "안돼...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021950.1.14",
          "form": "ㅋㅋㅋㅋ 추석전후로 온거같은데",
          "original_form": "ㅋㅋㅋㅋ 추석전후로 온거같은데",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021950.1.15",
          "form": "추석때오면 안되는데",
          "original_form": "추석때오면 안되는데",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
