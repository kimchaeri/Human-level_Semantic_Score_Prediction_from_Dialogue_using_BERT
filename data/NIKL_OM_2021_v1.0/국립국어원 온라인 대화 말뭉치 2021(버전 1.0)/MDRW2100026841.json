      "utterance": [
        {
          "id": "MDRW2100026841.1.1",
          "form": "정말 뇌속에 뭐가든걸까요ㅋㅋ",
          "original_form": "정말 뇌속에 뭐가든걸까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026841.1.2",
          "form": "그러니까 말이에요",
          "original_form": "그러니까 말이에요",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026841.1.3",
          "form": "아니 대학원에 연구하러 오지",
          "original_form": "아니 대학원에 연구하러 오지",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026841.1.4",
          "form": "연애하러 오냐고....;",
          "original_form": "연애하러 오냐고....;",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026841.1.5",
          "form": "진짜 뇌를 파버리고싶어요",
          "original_form": "진짜 뇌를 파버리고싶어요",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026841.1.6",
          "form": "하 진짜 너무구려서 어디서부터 어디까지 말해야할지",
          "original_form": "하 진짜 너무구려서 어디서부터 어디까지 말해야할지",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026841.1.7",
          "form": "그니까요 그런 인간들이",
          "original_form": "그니까요 그런 인간들이",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026841.1.8",
          "form": "석박사 따고",
          "original_form": "석박사 따고",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026841.1.9",
          "form": "사회에서 기득권층이 된다는 게...",
          "original_form": "사회에서 기득권층이 된다는 게...",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026841.1.10",
          "form": "더러워요 진짜",
          "original_form": "더러워요 진짜",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026841.1.11",
          "form": "진짜 뇌가 없나",
          "original_form": "진짜 뇌가 없나",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026841.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026841.1.13",
          "form": "학부생이 왜 자기 발로 대학원 연구실에 인턴으로 오는지",
          "original_form": "학부생이 왜 자기 발로 대학원 연구실에 인턴으로 오는지",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026841.1.14",
          "form": "연애하자고 그러겠냐고 멍청이들아...ㅋㅋㅋㅋㅈㄱㅈㅋ",
          "original_form": "연애하자고 그러겠냐고 멍청이들아...ㅋㅋㅋㅋㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026841.1.15",
          "form": "그러니까요!!!!",
          "original_form": "그러니까요!!!!",
          "speaker_id": "2",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100026841.1.16",
          "form": "걔네들 막상 연애해도 잘 해주지도 않을거면서...",
          "original_form": "걔네들 막상 연애해도 잘 해주지도 않을거면서...",
          "speaker_id": "2",
          "time": "20210902 08:04"
        },
        {
          "id": "MDRW2100026841.1.17",
          "form": "학부생들을 뭘로 보는건지 ",
          "original_form": "학부생들을 뭘로 보는건지 ",
          "speaker_id": "2",
          "time": "20210902 08:04"
        },
        {
          "id": "MDRW2100026841.1.18",
          "form": "아진짜 잘 해주지도않을거 인정이요ㅋㅋㅋ",
          "original_form": "아진짜 잘 해주지도않을거 인정이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100026841.1.19",
          "form": "그니까요!",
          "original_form": "그니까요!",
          "speaker_id": "2",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100026841.1.20",
          "form": "name1 열람실에 잘 도착하셨나요?",
          "original_form": "&name1& 열람실에 잘 도착하셨나요?",
          "speaker_id": "2",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100026841.1.21",
          "form": "네네 실강하나들었어요ㅋㅋ",
          "original_form": "네네 실강하나들었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100026841.1.22",
          "form": "오늘 화실기 오티 실강으로하는데ㅋㅋ",
          "original_form": "오늘 화실기 오티 실강으로하는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100026841.1.23",
          "form": "ㄱㄱㄱ교수님 분반이거든요",
          "original_form": "ㄱㄱㄱ교수님 분반이거든요",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100026841.1.24",
          "form": "교수님이 인턴들 기억하신다는데.... 잊어주세요",
          "original_form": "교수님이 인턴들 기억하신다는데.... 잊어주세요",
