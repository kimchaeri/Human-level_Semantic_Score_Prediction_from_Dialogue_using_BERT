      "utterance": [
        {
          "id": "MDRW2100030591.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210904 09:39"
        },
        {
          "id": "MDRW2100030591.1.2",
          "form": "안냐새요",
          "original_form": "안냐새요",
          "speaker_id": "2",
          "time": "20210904 09:39"
        },
        {
          "id": "MDRW2100030591.1.3",
          "form": "어떤 음식을 좋아하시나요?",
          "original_form": "어떤 음식을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 09:43"
        },
        {
          "id": "MDRW2100030591.1.4",
          "form": "파스타용 ~",
          "original_form": "파스타용 ~",
          "speaker_id": "2",
          "time": "20210904 09:43"
        },
        {
          "id": "MDRW2100030591.1.5",
          "form": "곱창 전어 꽃게 회",
          "original_form": "곱창 전어 꽃게 회",
          "speaker_id": "2",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030591.1.6",
          "form": "만두",
          "original_form": "만두",
          "speaker_id": "2",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030591.1.7",
          "form": "비빔국수",
          "original_form": "비빔국수",
          "speaker_id": "2",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030591.1.8",
          "form": "칼국수",
          "original_form": "칼국수",
          "speaker_id": "2",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030591.1.9",
          "form": "세상에!!  보기만했는데 벌써 배고파지는 느낌이에요",
          "original_form": "세상에!!  보기만했는데 벌써 배고파지는 느낌이에요",
          "speaker_id": "1",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030591.1.10",
          "form": "저도 파스타를 좋아해요!",
          "original_form": "저도 파스타를 좋아해요!",
          "speaker_id": "1",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030591.1.11",
          "form": "모든것이 맛잇어유",
          "original_form": "모든것이 맛잇어유",
          "speaker_id": "2",
          "time": "20210904 09:44"
        },
        {
          "id": "MDRW2100030591.1.12",
          "form": "어떤 파스타 좋아하세요?",
          "original_form": "어떤 파스타 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 09:45"
        },
        {
          "id": "MDRW2100030591.1.13",
          "form": "먹을 최고죠 ㅎㅎ",
          "original_form": "먹을 최고죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 09:45"
        },
        {
          "id": "MDRW2100030591.1.14",
          "form": "라자냐 ~ 크림소스~ 클래식까르보나라~ 알리오올리오~",
          "original_form": "라자냐 ~ 크림소스~ 클래식까르보나라~ 알리오올리오~",
          "speaker_id": "2",
          "time": "20210904 09:45"
        },
        {
          "id": "MDRW2100030591.1.15",
          "form": "뇨끼~~~",
          "original_form": "뇨끼~~~",
          "speaker_id": "2",
          "time": "20210904 09:45"
        },
        {
          "id": "MDRW2100030591.1.16",
          "form": "페스토~",
          "original_form": "페스토~",
          "speaker_id": "2",
          "time": "20210904 09:45"
        },
        {
          "id": "MDRW2100030591.1.17",
          "form": "하오어바츄?",
          "original_form": "하오어바츄?",
          "speaker_id": "2",
          "time": "20210904 09:46"
        },
        {
          "id": "MDRW2100030591.1.18",
          "form": "오우 취향이 비슷하시군요!",
          "original_form": "오우 취향이 비슷하시군요!",
          "speaker_id": "1",
          "time": "20210904 09:46"
        },
        {
          "id": "MDRW2100030591.1.19",
          "form": "크림소스 좋아해요",
          "original_form": "크림소스 좋아해요",
          "speaker_id": "1",
          "time": "20210904 09:46"
        },
        {
          "id": "MDRW2100030591.1.20",
          "form": "까르보나라도 좋아하고",
          "original_form": "까르보나라도 좋아하고",
          "speaker_id": "1",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.21",
          "form": "크~~~",
          "original_form": "크~~~",
          "speaker_id": "2",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.22",
          "form": "최근에 오일파스타에 맛들였어요 ㅋㅋ",
          "original_form": "최근에 오일파스타에 맛들였어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.23",
          "form": "오일파스타는 아직 입문 중이랍니다",
          "original_form": "오일파스타는 아직 입문 중이랍니다",
          "speaker_id": "1",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.24",
          "form": "훌륭하군요",
          "original_form": "훌륭하군요",
          "speaker_id": "2",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.25",
          "form": "뇨끼는 직접 만들어 먹어보려고 시도하고 있는데",
          "original_form": "뇨끼는 직접 만들어 먹어보려고 시도하고 있는데",
          "speaker_id": "1",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.26",
          "form": "대박.... 직접...ㄷㄷ",
          "original_form": "대박.... 직접...ㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.27",
          "form": "직접 만들어 드시나요?",
          "original_form": "직접 만들어 드시나요?",
          "speaker_id": "1",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.28",
          "form": "다른것도직접 ?",
          "original_form": "다른것도직접 ?",
          "speaker_id": "2",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.29",
          "form": "아니면 가게를 주로 이용하시나요?",
          "original_form": "아니면 가게를 주로 이용하시나요?",
          "speaker_id": "1",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.30",
          "form": "전 직접은...그냥 시판소스써요",
          "original_form": "전 직접은...그냥 시판소스써요",
          "speaker_id": "2",
          "time": "20210904 09:47"
        },
        {
          "id": "MDRW2100030591.1.31",
          "form": "맛잇는 파스타는 가게..ㅋㅋ",
          "original_form": "맛잇는 파스타는 가게..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:48"
        },
        {
          "id": "MDRW2100030591.1.32",
          "form": "ㅋㅋㅋㅋ 소스만 직접 만들어요",
          "original_form": "ㅋㅋㅋㅋ 소스만 직접 만들어요",
          "speaker_id": "1",
          "time": "20210904 09:48"
        },
        {
          "id": "MDRW2100030591.1.33",
          "form": "시판은 그냥 뭐 먹을만 ~~",
          "original_form": "시판은 그냥 뭐 먹을만 ~~",
          "speaker_id": "2",
          "time": "20210904 09:48"
        },
        {
          "id": "MDRW2100030591.1.34",
          "form": "시판중에서 추천해주실 소스가 있나요?",
          "original_form": "시판중에서 추천해주실 소스가 있나요?",
          "speaker_id": "1",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100030591.1.35",
          "form": "아니면 맛집 추천도 좋아요!",
          "original_form": "아니면 맛집 추천도 좋아요!",
          "speaker_id": "1",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100030591.1.36",
          "form": "ㅋㅋ 풀무원 갠찬던데용",
          "original_form": "ㅋㅋ 풀무원 갠찬던데용",
          "speaker_id": "2",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100030591.1.37",
          "form": "폰타나도",
          "original_form": "폰타나도",
          "speaker_id": "2",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100030591.1.38",
          "form": "오뚜기도 토마토류 갠츈...풀무원 로제",
          "original_form": "오뚜기도 토마토류 갠츈...풀무원 로제",
          "speaker_id": "2",
          "time": "20210904 09:49"
        },
        {
          "id": "MDRW2100030591.1.39",
          "form": "토마토는 헌트라는 소스가 있는데 그거 페이스트 섞어서 만들면 진짜 맛있어요",
          "original_form": "토마토는 헌트라는 소스가 있는데 그거 페이스트 섞어서 만들면 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210904 09:51"
        },
        {
          "id": "MDRW2100030591.1.40",
          "form": "짱 추천해요",
          "original_form": "짱 추천해요",
          "speaker_id": "1",
          "time": "20210904 09:51"
        },
        {
          "id": "MDRW2100030591.1.41",
          "form": "헉 페이스트는 어디서요?",
          "original_form": "헉 페이스트는 어디서요?",
          "speaker_id": "2",
          "time": "20210904 09:51"
        },
        {
          "id": "MDRW2100030591.1.42",
          "form": "마트에서 팔기도 하고",
          "original_form": "마트에서 팔기도 하고",
          "speaker_id": "1",
          "time": "20210904 09:56"
        },
        {
          "id": "MDRW2100030591.1.43",
          "form": "확실한건 백화점 지하 마트에서 팔아요",
          "original_form": "확실한건 백화점 지하 마트에서 팔아요",
          "speaker_id": "1",
          "time": "20210904 09:56"
        },
        {
          "id": "MDRW2100030591.1.44",
          "form": "대형 마트는 가끔 잘 살펴보면 팔기도 해요",
          "original_form": "대형 마트는 가끔 잘 살펴보면 팔기도 해요",
          "speaker_id": "1",
          "time": "20210904 09:56"
        }
      ]
