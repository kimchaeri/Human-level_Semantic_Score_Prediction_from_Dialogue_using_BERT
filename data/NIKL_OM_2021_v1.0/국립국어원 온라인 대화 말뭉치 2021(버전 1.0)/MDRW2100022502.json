      "utterance": [
        {
          "id": "MDRW2100022502.1.1",
          "form": "네 엄서용ㅋㅋ",
          "original_form": "네 엄서용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100022502.1.2",
          "form": "무슨일 하고싶으세요?",
          "original_form": "무슨일 하고싶으세요?",
          "speaker_id": "2",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100022502.1.3",
          "form": "딱히 없어요... ㅎㅎㅎ \r\n호기심 생기는건 가이드 같은거요..",
          "original_form": "딱히 없어요... ㅎㅎㅎ \r\n호기심 생기는건 가이드 같은거요..",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022502.1.4",
          "form": "뭔가 재밌을듯..",
          "original_form": "뭔가 재밌을듯..",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022502.1.5",
          "form": "여행 좋아하세여?ㅋㅋ",
          "original_form": "여행 좋아하세여?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022502.1.6",
          "form": "여행이랄까 그냥 돌아댕기는거요..",
          "original_form": "여행이랄까 그냥 돌아댕기는거요..",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022502.1.7",
          "form": "지금은 여행쪽이 너무 안좋아서 ㅠㅠ 정말추후네요 그쪽은 가이드 알바해봤는데 보람도 있지만 힘들었던 기억잌ㅋ",
          "original_form": "지금은 여행쪽이 너무 안좋아서 ㅠㅠ 정말추후네요 그쪽은 가이드 알바해봤는데 보람도 있지만 힘들었던 기억잌ㅋ",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022502.1.8",
          "form": "외근많은일이 좋더라구요..",
          "original_form": "외근많은일이 좋더라구요..",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022502.1.9",
          "form": "가이드 해보셧어여?",
          "original_form": "가이드 해보셧어여?",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022502.1.10",
          "form": "네 학생때 알바해봤어요",
          "original_form": "네 학생때 알바해봤어요",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022502.1.11",
          "form": "외근일은 여름엔힘들자나여 ㅋㅋ",
          "original_form": "외근일은 여름엔힘들자나여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022502.1.12",
          "form": "여른엔 차에서 땡땡이.. ㅋㅋㅋ",
          "original_form": "여른엔 차에서 땡땡이.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022502.1.13",
          "form": "어떤 가이드 였어요? 국내?",
          "original_form": "어떤 가이드 였어요? 국내?",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022502.1.14",
          "form": "네 국내요 ㅋㅋㅋ",
          "original_form": "네 국내요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100022502.1.15",
          "form": "차에서 땡땡이 ㅋㅋ최고네요 ㅋㅋㅋ 아메리카노 마시며~",
          "original_form": "차에서 땡땡이 ㅋㅋ최고네요 ㅋㅋㅋ 아메리카노 마시며~",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100022502.1.16",
          "form": "아조씨들 단체관광 인솔 이런거였나여?",
          "original_form": "아조씨들 단체관광 인솔 이런거였나여?",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100022502.1.17",
          "form": "아뇨 한국 여행온 수학여행 학생들요",
          "original_form": "아뇨 한국 여행온 수학여행 학생들요",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100022502.1.18",
          "form": "언어가 되셧나봐요?",
          "original_form": "언어가 되셧나봐요?",
          "speaker_id": "1",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100022502.1.19",
          "form": "아 언어가 안되어도 얼레벌레 되더라구요 ㅋ",
          "original_form": "아 언어가 안되어도 얼레벌레 되더라구요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022502.1.20",
          "form": "어떤 언어권 학생들이엇어요?",
          "original_form": "어떤 언어권 학생들이엇어요?",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022502.1.21",
          "form": "일본애들이요 ㅋㅋㅋㅋ",
          "original_form": "일본애들이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
