      "utterance": [
        {
          "id": "MDRW2100023092.1.1",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.2",
          "form": "아니 접근성 낮은게",
          "original_form": "아니 접근성 낮은게",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.3",
          "form": "왜 필요해",
          "original_form": "왜 필요해",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.4",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.5",
          "form": "필요하면 필요한사람들이",
          "original_form": "필요하면 필요한사람들이",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.6",
          "form": "공공기관은",
          "original_form": "공공기관은",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.7",
          "form": "알아서 찾겠지...",
          "original_form": "알아서 찾겠지...",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.8",
          "form": "거리감이 있어도 괜찮아...",
          "original_form": "거리감이 있어도 괜찮아...",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.9",
          "form": "서비스나 칼같이 해줘...",
          "original_form": "서비스나 칼같이 해줘...",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.10",
          "form": "ㅇㅇ.....",
          "original_form": "ㅇㅇ.....",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023092.1.11",
          "form": "안내영상은 정보 전달의 목적으로 만드는거라구",
          "original_form": "안내영상은 정보 전달의 목적으로 만드는거라구",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.12",
          "form": "이상한 드립치고 이상한 캐릭터 넣지 말라고...",
          "original_form": "이상한 드립치고 이상한 캐릭터 넣지 말라고...",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.13",
          "form": "물론 진짜로 위트있고 친절하고 접근성 좋으면 좋겠지",
          "original_form": "물론 진짜로 위트있고 친절하고 접근성 좋으면 좋겠지",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.14",
          "form": "근데 100이면 100",
          "original_form": "근데 100이면 100",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.15",
          "form": "선 못지킴",
          "original_form": "선 못지킴",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.16",
          "form": "ㅇㅇ 미스난다고",
          "original_form": "ㅇㅇ 미스난다고",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.17",
          "form": "심지어 그 드립을 넣는 사람이",
          "original_form": "심지어 그 드립을 넣는 사람이",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.18",
          "form": "어디서 대충",
          "original_form": "어디서 대충",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.19",
          "form": "요즘 젊은세대에서는 이런게 유행이라던데??",
          "original_form": "요즘 젊은세대에서는 이런게 유행이라던데??",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023092.1.20",
          "form": "하는 소리만 듣고 영상에 끼워넣은 경우",
          "original_form": "하는 소리만 듣고 영상에 끼워넣은 경우",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023092.1.21",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023092.1.22",
          "form": "내가 그거 성공한 케이스는 트위터 국립산립과학원?",
          "original_form": "내가 그거 성공한 케이스는 트위터 국립산립과학원?",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023092.1.23",
          "form": "정말 재미도 감동도 실용성도 없는",
          "original_form": "정말 재미도 감동도 실용성도 없는",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023092.1.24",
          "form": "정도밖에 못본듯",
          "original_form": "정도밖에 못본듯",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023092.1.25",
          "form": "ㅇㅇ 거기는 그리고",
          "original_form": "ㅇㅇ 거기는 그리고",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023092.1.26",
          "form": "드립은 진짜 일상트윗에서나 치고",
          "original_form": "드립은 진짜 일상트윗에서나 치고",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023092.1.27",
          "form": "ㅇㅇ좀 사기야",
          "original_form": "ㅇㅇ좀 사기야",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023092.1.28",
          "form": "고양이를 올리잖아",
          "original_form": "고양이를 올리잖아",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023092.1.29",
          "form": "치트키야",
          "original_form": "치트키야",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023092.1.30",
          "form": "정보전달은 오히려 일상트보다 거리감 두는 편",
          "original_form": "정보전달은 오히려 일상트보다 거리감 두는 편",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023092.1.31",
          "form": "ㅇㅇ정보전달은 딱딱하게",
          "original_form": "ㅇㅇ정보전달은 딱딱하게",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023092.1.32",
          "form": "잘 해",
          "original_form": "잘 해",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023092.1.33",
          "form": "여튼 열에 아홉은",
          "original_form": "여튼 열에 아홉은",
          "speaker_id": "2",
          "time": "20210829 21:41"
        }
      ]
    }
