        },
        {
          "id": "MDRW2100046048.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046048.1.3",
          "form": "아침드셧나요",
          "original_form": "아침드셧나요",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046048.1.4",
          "form": "미역국 해서 먹었어요",
          "original_form": "미역국 해서 먹었어요",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046048.1.5",
          "form": "뭐 드셨나요??",
          "original_form": "뭐 드셨나요??",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046048.1.6",
          "form": "오 맛있겠어요",
          "original_form": "오 맛있겠어요",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046048.1.7",
          "form": "저는아직용",
          "original_form": "저는아직용",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046048.1.8",
          "form": "뭐 드실 거예요??",
          "original_form": "뭐 드실 거예요??",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046048.1.9",
          "form": "사과먹을까해용",
          "original_form": "사과먹을까해용",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046048.1.10",
          "form": "ㅎㅎㅎ 오전사과 좋죠",
          "original_form": "ㅎㅎㅎ 오전사과 좋죠",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046048.1.11",
          "form": "저도 출근할때는 사과반쪽",
          "original_form": "저도 출근할때는 사과반쪽",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046048.1.12",
          "form": "견과류 먹고가요",
          "original_form": "견과류 먹고가요",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046048.1.13",
          "form": "오 건강식이네요",
          "original_form": "오 건강식이네요",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046048.1.14",
          "form": "네  빈속에 커피를 마셔 왔는데",
          "original_form": "네  빈속에 커피를 마셔 왔는데",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046048.1.15",
          "form": "이제 건강을 좀 생각하려구요",
          "original_form": "이제 건강을 좀 생각하려구요",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046048.1.16",
          "form": "그러면머리가맑아지조ㅡㅇ",
          "original_form": "그러면머리가맑아지조ㅡㅇ",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046048.1.17",
          "form": "견과류 먹고",
          "original_form": "견과류 먹고",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046048.1.18",
          "form": "커피마시니 더 맛있는거 같아요",
          "original_form": "커피마시니 더 맛있는거 같아요",
          "speaker_id": "2",
