      "utterance": [
        {
          "id": "MDRW2100033860.1.1",
          "form": "커피 즐기시나요",
          "original_form": "커피 즐기시나요",
          "speaker_id": "2",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100033860.1.2",
          "form": "네 한잔씩요",
          "original_form": "네 한잔씩요",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100033860.1.3",
          "form": "아메세요 라떼세요?",
          "original_form": "아메세요 라떼세요?",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033860.1.4",
          "form": "ㅋㅋㅋ 라떼쪽이요~ ㅋ",
          "original_form": "ㅋㅋㅋ 라떼쪽이요~ ㅋ",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100033860.1.5",
          "form": "아 달달이 좋아하시네요",
          "original_form": "아 달달이 좋아하시네요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100033860.1.6",
          "form": "전 집에선 아메",
          "original_form": "전 집에선 아메",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100033860.1.7",
          "form": "카페에선 달달이요",
          "original_form": "카페에선 달달이요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100033860.1.8",
          "form": "집엔 생크림이 없어서요",
          "original_form": "집엔 생크림이 없어서요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100033860.1.9",
          "form": "네 프라프치노도 좋아해요\r\n어떤카페 어떤 메뉴 좋아하세요~^^",
          "original_form": "네 프라프치노도 좋아해요\r\n어떤카페 어떤 메뉴 좋아하세요~^^",
          "speaker_id": "2",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100033860.1.10",
          "form": "컴포트는 와플이 2500원이라 같이 먹어요",
          "original_form": "컴포트는 와플이 2500원이라 같이 먹어요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100033860.1.11",
          "form": "오 와플.. 좋네요 사이드메뉴",
          "original_form": "오 와플.. 좋네요 사이드메뉴",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100033860.1.12",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100033860.1.13",
          "form": "전 스벅 유자음료",
          "original_form": "전 스벅 유자음료",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100033860.1.14",
          "form": "스벅 쿠키블루베리머핀이요",
          "original_form": "스벅 쿠키블루베리머핀이요",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100033860.1.15",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100033860.1.16",
          "form": "저는 자바칩프라프치노가 최애예요",
          "original_form": "저는 자바칩프라프치노가 최애예요",
          "speaker_id": "2",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100033860.1.17",
          "form": "오 저 안먹어봤는데 먹어보고 싶어요",
          "original_form": "오 저 안먹어봤는데 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100033860.1.18",
          "form": "담에 시킬래요 ㅋㅋ",
          "original_form": "담에 시킬래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100033860.1.19",
          "form": "달달이 좋아하시는데 프라프치노는 안드셨나봐요",
          "original_form": "달달이 좋아하시는데 프라프치노는 안드셨나봐요",
          "speaker_id": "2",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100033860.1.20",
          "form": "먹어봤어요",
          "original_form": "먹어봤어요",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100033860.1.21",
          "form": "저는 나가선 커피말고 다른 음료들 막 먹어요",
          "original_form": "저는 나가선 커피말고 다른 음료들 막 먹어요",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100033860.1.22",
          "form": "아 한잔 하고싶어요 얘기하니",
          "original_form": "아 한잔 하고싶어요 얘기하니",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100033860.1.23",
          "form": "오늘 마셨나요?",
          "original_form": "오늘 마셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100033860.1.24",
          "form": "전 아직요",
          "original_form": "전 아직요",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100033860.1.25",
          "form": "저도 아직이요~ 하루 한잔은 꼭 드시나봐요!",
          "original_form": "저도 아직이요~ 하루 한잔은 꼭 드시나봐요!",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100033860.1.26",
          "form": "에",
          "original_form": "에",
          "speaker_id": "1",
          "time": "20210825 13:28"
        }
      ]
    }
