      "utterance": [
        {
          "id": "MDRW2100039684.1.1",
          "form": "그냥 마법의 기계인 것 같으여 넣기만하면다돼",
          "original_form": "그냥 마법의 기계인 것 같으여 넣기만하면다돼",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100039684.1.2",
          "form": "일반 군고구마와 다른가요 맛이?",
          "original_form": "일반 군고구마와 다른가요 맛이?",
          "speaker_id": "2",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100039684.1.3",
          "form": "통째로 넣음 안되는 거죠?",
          "original_form": "통째로 넣음 안되는 거죠?",
          "speaker_id": "2",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100039684.1.4",
          "form": "음 그러셔도 되는데",
          "original_form": "음 그러셔도 되는데",
          "speaker_id": "1",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100039684.1.5",
          "form": "제일 유명한 건",
          "original_form": "제일 유명한 건",
          "speaker_id": "1",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100039684.1.6",
          "form": "3cm정도 굵기로 잘라서 굽는 거예여",
          "original_form": "3cm정도 굵기로 잘라서 굽는 거예여",
          "speaker_id": "1",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100039684.1.7",
          "form": "보면진짜 걍 나무토막같은데",
          "original_form": "보면진짜 걍 나무토막같은데",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039684.1.8",
          "form": "안이 진짜..개촉촉해서 ...",
          "original_form": "안이 진짜..개촉촉해서 ...",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039684.1.9",
          "form": "장난 어니에요...",
          "original_form": "장난 어니에요...",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039684.1.10",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039684.1.11",
          "form": "궁금궁금",
          "original_form": "궁금궁금",
          "speaker_id": "2",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039684.1.12",
          "form": "궁금해만하지말고!!!",
          "original_form": "궁금해만하지말고!!!",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039684.1.13",
          "form": "드셔보시라구여!!!!!",
          "original_form": "드셔보시라구여!!!!!",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039684.1.14",
          "form": "거기에 꿀바름 겉마속촉인 맛탕 느낌 날까요",
          "original_form": "거기에 꿀바름 겉마속촉인 맛탕 느낌 날까요",
          "speaker_id": "2",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039684.1.15",
          "form": "갑자기 스파르타 강사가 되셨얻ㄷㄷㄷ",
          "original_form": "갑자기 스파르타 강사가 되셨얻ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100039684.1.16",
          "form": "꿀..?",
          "original_form": "꿀..?",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100039684.1.17",
          "form": "모르겠어여 제가 맛탕을 해보지 않아서.....",
          "original_form": "모르겠어여 제가 맛탕을 해보지 않아서.....",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100039684.1.18",
          "form": "ㅠㅠㅠ고구마 맛탕먹고싶네요..",
          "original_form": "ㅠㅠㅠ고구마 맛탕먹고싶네요..",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100039684.1.19",
          "form": "ㅎㅎ 저 맛탕 속이 푸석해서 싫어하는데",
          "original_form": "ㅎㅎ 저 맛탕 속이 푸석해서 싫어하는데",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039684.1.20",
          "form": "속 촉촉한 맛탕 먹고프네요",
          "original_form": "속 촉촉한 맛탕 먹고프네요",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039684.1.21",
          "form": "앗 맛탕이 그런가요",
          "original_form": "앗 맛탕이 그런가요",
          "speaker_id": "1",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039684.1.22",
          "form": "혹시 급식에서만 드셔보셨나여!",
          "original_form": "혹시 급식에서만 드셔보셨나여!",
          "speaker_id": "1",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039684.1.23",
          "form": "아뇨 카페나 여기저기서 먹었는데",
          "original_form": "아뇨 카페나 여기저기서 먹었는데",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039684.1.24",
          "form": "죄다 푸석해요ㅜ",
          "original_form": "죄다 푸석해요ㅜ",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039684.1.25",
          "form": "제가 맛탕을 원래 안좋아했어서 기준이 높은 걸수도?",
          "original_form": "제가 맛탕을 원래 안좋아했어서 기준이 높은 걸수도?",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039684.1.26",
          "form": "어무니께",
          "original_form": "어무니께",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100039684.1.27",
          "form": "해달라고 해보세여!",
          "original_form": "해달라고 해보세여!",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100039684.1.28",
          "form": "뭔가 다르지 않을까여",
          "original_form": "뭔가 다르지 않을까여",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100039684.1.29",
          "form": "ㅋㅋ 등짝 스매싱 맞지 않을까여",
          "original_form": "ㅋㅋ 등짝 스매싱 맞지 않을까여",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100039684.1.30",
          "form": "너는 손이 없냐구ㅋㅋㅋ",
          "original_form": "너는 손이 없냐구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100039684.1.31",
          "form": "으아니...",
          "original_form": "으아니...",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039684.1.32",
          "form": "그런손을 물랴주셨다면..",
          "original_form": "그런손을 물랴주셨다면..",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039684.1.33",
          "form": "as가 필요합니다 어머니..",
          "original_form": "as가 필요합니다 어머니..",
