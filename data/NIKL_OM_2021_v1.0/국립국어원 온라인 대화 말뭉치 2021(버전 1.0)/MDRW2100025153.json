      "utterance": [
        {
          "id": "MDRW2100025153.1.1",
          "form": "당근마켓 해보셨어요?",
          "original_form": "당근마켓 해보셨어요?",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100025153.1.2",
          "form": "네 뭐러 샀더라,,",
          "original_form": "네 뭐러 샀더라,,",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025153.1.3",
          "form": "기억이 잘 안나는데 한개 샀어요",
          "original_form": "기억이 잘 안나는데 한개 샀어요",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025153.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025153.1.5",
          "form": "저는 주로 판매하는 편이에요 ㅋㅋ",
          "original_form": "저는 주로 판매하는 편이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025153.1.6",
          "form": "공기계도 필요없는거 생기면 팔구요",
          "original_form": "공기계도 필요없는거 생기면 팔구요",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025153.1.7",
          "form": "아니면 필요할 줄 알고 샀는데 쓸일이 없어진 물건들 올려놔요",
          "original_form": "아니면 필요할 줄 알고 샀는데 쓸일이 없어진 물건들 올려놔요",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025153.1.8",
          "form": "아 저번에 보색샴푸 당근으로 샀었어요",
          "original_form": "아 저번에 보색샴푸 당근으로 샀었어요",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025153.1.9",
          "form": "저랑 좀 반대시네요 전 어딘가 쓸일이 있을가해서",
          "original_form": "저랑 좀 반대시네요 전 어딘가 쓸일이 있을가해서",
          "speaker_id": "2",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100025153.1.10",
          "form": "공기계도 쌓아두고 똥값을 만들어요 ㅠ",
          "original_form": "공기계도 쌓아두고 똥값을 만들어요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100025153.1.11",
          "form": "ㅋㅋㅋㅋㅋ 공기계는 팔기 아깝긴 하죠!!",
          "original_form": "ㅋㅋㅋㅋㅋ 공기계는 팔기 아깝긴 하죠!!",
          "speaker_id": "1",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100025153.1.12",
          "form": "있으면 카메라도 그렇고 여기저기 써먹을 곳이 많잖아용",
          "original_form": "있으면 카메라도 그렇고 여기저기 써먹을 곳이 많잖아용",
          "speaker_id": "1",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100025153.1.13",
          "form": "아이폰 아니면 팔아두 좋은 가격이 안나오니까용..",
          "original_form": "아이폰 아니면 팔아두 좋은 가격이 안나오니까용..",
          "speaker_id": "1",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025153.1.14",
          "form": "ㅠㅠ 전 갤럭시라 팔아도 그닥,,",
          "original_form": "ㅠㅠ 전 갤럭시라 팔아도 그닥,,",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025153.1.15",
          "form": "네 뒀다가 은행이나 주식어플로 써요",
          "original_form": "네 뒀다가 은행이나 주식어플로 써요",
          "speaker_id": "2",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100025153.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 갤럭시 사용하세요? 저두요 ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 갤럭시 사용하세요? 저두요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100025153.1.17",
          "form": "주식도.. 어떻게 보면 쇼핑이라고 할수 있을까요?",
          "original_form": "주식도.. 어떻게 보면 쇼핑이라고 할수 있을까요?",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025153.1.18",
          "form": "주식 사는 얘길 하면 그럴지도요?",
          "original_form": "주식 사는 얘길 하면 그럴지도요?",
          "speaker_id": "2",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025153.1.19",
          "form": "저는 좀 장기투자를 하는 편인데",
          "original_form": "저는 좀 장기투자를 하는 편인데",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025153.1.20",
          "form": ".오오 머찌다",
          "original_form": ".오오 머찌다",
          "speaker_id": "2",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100025153.1.21",
          "form": "하나씩 사다가 망한게 많아서 ETF위주로 그냥 사는 것 같아요",
          "original_form": "하나씩 사다가 망한게 많아서 ETF위주로 그냥 사는 것 같아요",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100025153.1.22",
          "form": "장기투자라고 하시면",
          "original_form": "장기투자라고 하시면",
          "speaker_id": "2",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100025153.1.23",
          "form": "몇년 보유하셨나요?",
          "original_form": "몇년 보유하셨나요?",
          "speaker_id": "2",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100025153.1.24",
          "form": "주식 시작한지 얼마 안돼서 그정도까지는 아니구요 ㅋㅋㅋ",
          "original_form": "주식 시작한지 얼마 안돼서 그정도까지는 아니구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100025153.1.25",
          "form": "한 9개월 됐네여 지금 갖고있는 친구들은",
          "original_form": "한 9개월 됐네여 지금 갖고있는 친구들은",
          "speaker_id": "1",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100025153.1.26",
          "form": "작년 12월쯤 샀으니까용",
          "original_form": "작년 12월쯤 샀으니까용",
          "speaker_id": "1",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100025153.1.27",
          "form": "아닛 그럼 장기투자인데 손해봤다고 하기 뭐한데요",
          "original_form": "아닛 그럼 장기투자인데 손해봤다고 하기 뭐한데요",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025153.1.28",
          "form": "급전 필요한게 아니면 쭉 갖고있을 생각이에요",
          "original_form": "급전 필요한게 아니면 쭉 갖고있을 생각이에요",
          "speaker_id": "1",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025153.1.29",
          "form": "장기라고 하면 3년 5년 10년 봐야져",
          "original_form": "장기라고 하면 3년 5년 10년 봐야져",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025153.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸 이제 애완주식이 된거져",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸 이제 애완주식이 된거져",
          "speaker_id": "1",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025153.1.31",
          "form": "삼전도 지금은 떨어져도 ,, 5년뒤엔 올라있을거니깐요",
          "original_form": "삼전도 지금은 떨어져도 ,, 5년뒤엔 올라있을거니깐요",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025153.1.32",
          "form": "맞아요 지금이 할인찬스에요",
          "original_form": "맞아요 지금이 할인찬스에요",
          "speaker_id": "1",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025153.1.33",
          "form": "만약 문재인 정부다음의 이재명정부가 기본소득국가를 만들고 북에 퍼주자며 이재용을 또 감옥에 가두고 삥을 뜯고한다면",
          "original_form": "만약 문재인 정부다음의 이재명정부가 기본소득국가를 만들고 북에 퍼주자며 이재용을 또 감옥에 가두고 삥을 뜯고한다면",
          "speaker_id": "2",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025153.1.34",
          "form": "사실상 적금이에요",
          "original_form": "사실상 적금이에요",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025153.1.35",
          "form": "망할수도 있겠지만 삼전이 망하면 뭐 저만 망하는건 아니니깐^^",
          "original_form": "망할수도 있겠지만 삼전이 망하면 뭐 저만 망하는건 아니니깐^^",
          "speaker_id": "2",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025153.1.36",
          "form": "ㅋㅋㅋㅋㅋ 삼성은 이재용이 또 감옥가도 절대 안 망할것 같아요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 삼성은 이재용이 또 감옥가도 절대 안 망할것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025153.1.37",
          "form": "이번에도 플립 대박났잖아여",
          "original_form": "이번에도 플립 대박났잖아여",
          "speaker_id": "1",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025153.1.38",
          "form": "너무 사고싶어요",
          "original_form": "너무 사고싶어요",
          "speaker_id": "1",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025153.1.39",
          "form": "이재명이 자본주의적인 사람이라면 그러겠지만,,",
          "original_form": "이재명이 자본주의적인 사람이라면 그러겠지만,,",
          "speaker_id": "2",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025153.1.40",
