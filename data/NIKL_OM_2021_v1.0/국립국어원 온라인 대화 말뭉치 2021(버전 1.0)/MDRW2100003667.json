      "utterance": [
        {
          "id": "MDRW2100003667.1.1",
          "form": "오늘 비가 엄청나게 오네요 ㅠㅠ",
          "original_form": "오늘 비가 엄청나게 오네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003667.1.2",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "1",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003667.1.3",
          "form": "하늘에 구멍난거같아요ㅠㅠ",
          "original_form": "하늘에 구멍난거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003667.1.4",
          "form": "그러니까요!!! 어마어마해요",
          "original_form": "그러니까요!!! 어마어마해요",
          "speaker_id": "2",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003667.1.5",
          "form": "보니까 장마라는데 다음주까지 이어질거래요",
          "original_form": "보니까 장마라는데 다음주까지 이어질거래요",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003667.1.6",
          "form": "여름도 다 갔는데 장마라니..",
          "original_form": "여름도 다 갔는데 장마라니..",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003667.1.7",
          "form": "맞아요ㅜ정말 충격적...",
          "original_form": "맞아요ㅜ정말 충격적...",
          "speaker_id": "2",
          "time": "20210821 11:59"
        },
        {
          "id": "MDRW2100003667.1.8",
          "form": "우산써도 비를 다 맞아요 ㅎ",
          "original_form": "우산써도 비를 다 맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 11:59"
        },
        {
          "id": "MDRW2100003667.1.9",
          "form": "비오는 날 어떠신가요?",
          "original_form": "비오는 날 어떠신가요?",
          "speaker_id": "1",
          "time": "20210821 11:59"
        },
        {
          "id": "MDRW2100003667.1.10",
          "form": "맞아요.. 비가 세게오면 다 젖는거 같아요",
          "original_form": "맞아요.. 비가 세게오면 다 젖는거 같아요",
          "speaker_id": "1",
          "time": "20210821 11:59"
        },
        {
          "id": "MDRW2100003667.1.11",
          "form": "전 비 오는 날은 그냥.....",
          "original_form": "전 비 오는 날은 그냥.....",
          "speaker_id": "2",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003667.1.12",
          "form": "특히 우리나라는 습기가 많아서요",
          "original_form": "특히 우리나라는 습기가 많아서요",
          "speaker_id": "2",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003667.1.13",
          "form": "그래서 저는 비오는 날은 별로 좋아하지않아요",
          "original_form": "그래서 저는 비오는 날은 별로 좋아하지않아요",
          "speaker_id": "1",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003667.1.14",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003667.1.15",
          "form": "그래서 여름이 더 습한게 있나봐요ㅠ",
          "original_form": "그래서 여름이 더 습한게 있나봐요ㅠ",
          "speaker_id": "1",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003667.1.16",
          "form": "그럼 비 말고 눈은 어떠세요",
          "original_form": "그럼 비 말고 눈은 어떠세요",
          "speaker_id": "1",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003667.1.17",
          "form": "저 눈은 좋은데 녹았을 때는 ㅎ",
          "original_form": "저 눈은 좋은데 녹았을 때는 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003667.1.18",
          "form": "그래도 비보다는 좋아요",
          "original_form": "그래도 비보다는 좋아요",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003667.1.19",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003667.1.20",
          "form": "저도 눈이 더 나아요",
          "original_form": "저도 눈이 더 나아요",
          "speaker_id": "1",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003667.1.21",
          "form": "뭔가 눈은 오면 설레더라구요",
          "original_form": "뭔가 눈은 오면 설레더라구요",
          "speaker_id": "1",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003667.1.22",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210821 13:20"
        },
        {
          "id": "MDRW2100003667.1.23",
          "form": "눈오면 설레죠",
          "original_form": "눈오면 설레죠",
          "speaker_id": "2",
          "time": "20210821 13:20"
        },
        {
          "id": "MDRW2100003667.1.24",
          "form": "눈사람도 만들고 눈싸움도하고",
          "original_form": "눈사람도 만들고 눈싸움도하고",
          "speaker_id": "1",
          "time": "20210821 13:20"
        },
        {
          "id": "MDRW2100003667.1.25",
          "form": "요즘 눈오리많이 만들더라구요",
          "original_form": "요즘 눈오리많이 만들더라구요",
          "speaker_id": "1",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003667.1.26",
          "form": "오 맞아요 ㅠㅠㅠㅠ 눈오리",
          "original_form": "오 맞아요 ㅠㅠㅠㅠ 눈오리",
          "speaker_id": "2",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003667.1.27",
          "form": "거기도 비오나요?",
          "original_form": "거기도 비오나요?",
          "speaker_id": "2",
          "time": "20210823 08:09"
        }
      ]
    }
