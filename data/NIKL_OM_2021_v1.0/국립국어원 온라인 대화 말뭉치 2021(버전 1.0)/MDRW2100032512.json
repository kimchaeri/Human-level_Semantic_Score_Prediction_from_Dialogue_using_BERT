      "utterance": [
        {
          "id": "MDRW2100032512.1.1",
          "form": "암튼 스쿼트도",
          "original_form": "암튼 스쿼트도",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.2",
          "form": "발 각도나 그런거에 따라서 자극오는 곳이 다르니까",
          "original_form": "발 각도나 그런거에 따라서 자극오는 곳이 다르니까",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.4",
          "form": "잘 찾아보고 운동해봐",
          "original_form": "잘 찾아보고 운동해봐",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.5",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.6",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.7",
          "form": "요즘 유튜브에서 워낙 잘 가르쳐줘서",
          "original_form": "요즘 유튜브에서 워낙 잘 가르쳐줘서",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.8",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.9",
          "form": "그 삐약언니인가",
          "original_form": "그 삐약언니인가",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.10",
          "form": "운동영상 너무 만ㄹ음",
          "original_form": "운동영상 너무 만ㄹ음",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.11",
          "form": "그 사람 재밌더랔ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 사람 재밌더랔ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100032512.1.12",
          "form": "찾아서 하기 피곤항정도",
          "original_form": "찾아서 하기 피곤항정도",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032512.1.13",
          "form": "삐약언니?",
          "original_form": "삐약언니?",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032512.1.14",
          "form": "오키접수",
          "original_form": "오키접수",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032512.1.15",
          "form": "잠시만",
          "original_form": "잠시만",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032512.1.16",
          "form": "삐약 스핏인데",
          "original_form": "삐약 스핏인데",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032512.1.17",
          "form": "그 운동 가르쳐주는 언니가",
          "original_form": "그 운동 가르쳐주는 언니가",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032512.1.18",
          "form": "완전 하이텐션이라 개웃김ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "완전 하이텐션이라 개웃김ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100032512.1.19",
          "form": "아아아아아아 나 그사람알아",
          "original_form": "아아아아아아 나 그사람알아",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032512.1.20",
          "form": "그 나 발 치료받고와서",
          "original_form": "그 나 발 치료받고와서",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032512.1.21",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032512.1.22",
          "form": "니가 앉아서 할수있는영상 찾아보라해서",
          "original_form": "니가 앉아서 할수있는영상 찾아보라해서",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032512.1.23",
          "form": "쳐봤는데 그 팔만 운동하는 영상",
          "original_form": "쳐봤는데 그 팔만 운동하는 영상",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032512.1.24",
          "form": "그거 사람들이 추천하길래 해봣음",
          "original_form": "그거 사람들이 추천하길래 해봣음",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100032512.1.25",
          "form": "맞음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
