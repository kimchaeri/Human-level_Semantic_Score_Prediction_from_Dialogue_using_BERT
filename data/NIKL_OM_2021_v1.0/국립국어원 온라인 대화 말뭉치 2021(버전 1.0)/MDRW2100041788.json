      "utterance": [
        {
          "id": "MDRW2100041788.1.1",
          "form": "중고거래 해보셨나요?",
          "original_form": "중고거래 해보셨나요?",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041788.1.2",
          "form": "네네!! 근데 사보긴만 했어요!",
          "original_form": "네네!! 근데 사보긴만 했어요!",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041788.1.3",
          "form": "중고거래 해보셨나요?!",
          "original_form": "중고거래 해보셨나요?!",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100041788.1.4",
          "form": "저는 팝니다..ㅎㅎㅎ",
          "original_form": "저는 팝니다..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041788.1.5",
          "form": "팔려고 시작했어요",
          "original_form": "팔려고 시작했어요",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100041788.1.6",
          "form": "오 당근마켓으로 하세요??",
          "original_form": "오 당근마켓으로 하세요??",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041788.1.7",
          "form": "네 당근마켓이 쉽더라구요",
          "original_form": "네 당근마켓이 쉽더라구요",
          "speaker_id": "1",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100041788.1.8",
          "form": "다른건 할 줄 몰라요",
          "original_form": "다른건 할 줄 몰라요",
          "speaker_id": "1",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100041788.1.9",
          "form": "뭐 팔아보셨나요?!",
          "original_form": "뭐 팔아보셨나요?!",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100041788.1.10",
          "form": "방금 전에도 허리띠 팔았어요",
          "original_form": "방금 전에도 허리띠 팔았어요",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041788.1.11",
          "form": "우왕!! 그럼 샀던 가격대비 어느정도로 가격 낮춰서 파세요??",
          "original_form": "우왕!! 그럼 샀던 가격대비 어느정도로 가격 낮춰서 파세요??",
          "speaker_id": "2",
          "time": "20210906 16:05"
        },
        {
          "id": "MDRW2100041788.1.12",
          "form": "그게 한 15년전쯤인가 샀는데 한번도 안쓴 새거인채로 지금까지 있었거든요",
          "original_form": "그게 한 15년전쯤인가 샀는데 한번도 안쓴 새거인채로 지금까지 있었거든요",
          "speaker_id": "1",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100041788.1.13",
          "form": "그래서 원래 가격이 얼마인지 모르겠고",
          "original_form": "그래서 원래 가격이 얼마인지 모르겠고",
          "speaker_id": "1",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100041788.1.14",
          "form": "근데 유행은 지나서 내가 할 순 없고 그래서 중고로 내놨죠",
          "original_form": "근데 유행은 지나서 내가 할 순 없고 그래서 중고로 내놨죠",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100041788.1.15",
          "form": "몇천원에 팔았어요",
          "original_form": "몇천원에 팔았어요",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100041788.1.16",
          "form": "오 저도 팔고 싶은거 많은데.. ㅜㅜ",
          "original_form": "오 저도 팔고 싶은거 많은데.. ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041788.1.17",
          "form": "일단 시작해서 올려봐요",
          "original_form": "일단 시작해서 올려봐요",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100041788.1.18",
          "form": "이게 가격 정하는게 쉽지 않더라구요",
          "original_form": "이게 가격 정하는게 쉽지 않더라구요",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100041788.1.19",
          "form": "그니깐여 ㅜㅜ 근데 올리는것도 여간 부지런하지 않으면 ㅋㅋㅋㅋㅋ못할것같네요 ㅋㅋㅋㅋ",
          "original_form": "그니깐여 ㅜㅜ 근데 올리는것도 여간 부지런하지 않으면 ㅋㅋㅋㅋㅋ못할것같네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100041788.1.20",
          "form": "내 맘엔 이만큼 받고 싶은데 안나가면 초조해지면서 가격을 내리게 되죠",
          "original_form": "내 맘엔 이만큼 받고 싶은데 안나가면 초조해지면서 가격을 내리게 되죠",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100041788.1.21",
          "form": "살짝 눈치게임 같아요",
