          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045244.1.4",
          "form": "난 김남길 다시 붙는중인뎅",
          "original_form": "난 김남길 다시 붙는중인뎅",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045244.1.5",
          "form": "오늘 십분도 안봄",
          "original_form": "오늘 십분도 안봄",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045244.1.6",
          "form": "지금 이거하느라ㅋㅋㅋㅋ",
          "original_form": "지금 이거하느라ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045244.1.7",
          "form": "그건 탈덕아님",
          "original_form": "그건 탈덕아님",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045244.1.8",
          "form": "아니야ㅋㅋ",
          "original_form": "아니야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045244.1.9",
          "form": "소설본다구",
          "original_form": "소설본다구",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045244.1.10",
          "form": "관심을 못 끌뿐..",
