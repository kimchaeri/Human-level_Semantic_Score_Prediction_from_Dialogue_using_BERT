        },
        {
          "id": "MDRW2100008780.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008780.1.3",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008780.1.4",
          "form": "조금 합니다!!!",
          "original_form": "조금 합니다!!!",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008780.1.5",
          "form": "무슨 요리 잘하세요?",
          "original_form": "무슨 요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210822 18:33"
        },
        {
          "id": "MDRW2100008780.1.6",
          "form": "어젠 밀푀유나베요 ㅎㅎ",
          "original_form": "어젠 밀푀유나베요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008780.1.7",
          "form": "와ㅋㅋ",
          "original_form": "와ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008780.1.8",
          "form": "요리진짜 잘하시나봐요ㅜ",
          "original_form": "요리진짜 잘하시나봐요ㅜ",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008780.1.9",
          "form": "전똥손이라..ㅎㅎ",
          "original_form": "전똥손이라..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008780.1.10",
          "form": "ㅋㅋ밀키트 입니다",
          "original_form": "ㅋㅋ밀키트 입니다",
          "speaker_id": "1",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100008780.1.11",
          "form": "밀키트ㅋㅋㅋ",
          "original_form": "밀키트ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100008780.1.12",
          "form": "요즘 밀키트 너무 잘 되어있어서",
          "original_form": "요즘 밀키트 너무 잘 되어있어서",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100008780.1.13",
          "form": "좋긴하더라구요ㅎㅎ",
          "original_form": "좋긴하더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100008780.1.14",
          "form": "밀키트가 오히려 경제적",
          "original_form": "밀키트가 오히려 경제적",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100008780.1.15",
          "form": "혼자사는 사람에겐 좋은거 같더라구요",
          "original_form": "혼자사는 사람에겐 좋은거 같더라구요",
          "speaker_id": "2",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100008780.1.16",
          "form": "특히 요리를 자주 안하면",
          "original_form": "특히 요리를 자주 안하면",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100008780.1.17",
          "form": "이것저것 사도 버리는게 많으니까ㅎ",
          "original_form": "이것저것 사도 버리는게 많으니까ㅎ",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100008780.1.18",
          "form": "맞아요 양파 이런거 막 남아서ㅠ",
          "original_form": "맞아요 양파 이런거 막 남아서ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100008780.1.19",
          "form": "그래서 저도 배달이나 밀키트처럼 된 거 많이 사먹게 되는거 같더라구요ㅜ",
          "original_form": "그래서 저도 배달이나 밀키트처럼 된 거 많이 사먹게 되는거 같더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100008780.1.20",
          "form": "맞아요ㅠㅠ 소량씩 하기에는 ..",
          "original_form": "맞아요ㅠㅠ 소량씩 하기에는 ..",
          "speaker_id": "1",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100008780.1.21",
          "form": "그래도 예전보단 배달음식이 다양해서 좋은거 같아요ㅎㅎㅎ",
          "original_form": "그래도 예전보단 배달음식이 다양해서 좋은거 같아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100008780.1.22",
          "form": "예전엔 피자 치킨 중국집 뭐 이정도???",
          "original_form": "예전엔 피자 치킨 중국집 뭐 이정도???",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100008780.1.23",
          "form": "맞아요 요새 엄청 다양하죠",
          "original_form": "맞아요 요새 엄청 다양하죠",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100008780.1.24",
          "form": "근데 배달비가 좀 아깝긴 하죠ㅜ",
