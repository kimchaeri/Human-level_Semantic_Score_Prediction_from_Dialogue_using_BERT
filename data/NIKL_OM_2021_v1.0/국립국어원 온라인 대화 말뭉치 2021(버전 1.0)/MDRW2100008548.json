      "utterance": [
        {
          "id": "MDRW2100008548.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008548.1.2",
          "form": "아뇨 키우고 있는 동물은 없어요",
          "original_form": "아뇨 키우고 있는 동물은 없어요",
          "speaker_id": "2",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008548.1.3",
          "form": "키우고 있는 동물 있나요?",
          "original_form": "키우고 있는 동물 있나요?",
          "speaker_id": "2",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008548.1.4",
          "form": "키우고싶은데 여건이",
          "original_form": "키우고싶은데 여건이",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008548.1.5",
          "form": "아 반려동물 키우는 건 쉬운게 아닌 것 같아요",
          "original_form": "아 반려동물 키우는 건 쉬운게 아닌 것 같아요",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008548.1.6",
          "form": "시간 돈 어느하나 부족함없이 해줘야해서",
          "original_form": "시간 돈 어느하나 부족함없이 해줘야해서",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008548.1.7",
          "form": "케어가 힘들어",
          "original_form": "케어가 힘들어",
          "speaker_id": "1",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008548.1.8",
          "form": "그쵸 쉽지 않아요",
          "original_form": "그쵸 쉽지 않아요",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008548.1.9",
          "form": "돈 시간 많이 들어요",
          "original_form": "돈 시간 많이 들어요",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008548.1.10",
          "form": "혹시 키우고 싶은 동물은 어떤 동물인가요?",
          "original_form": "혹시 키우고 싶은 동물은 어떤 동물인가요?",
          "speaker_id": "2",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008548.1.11",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008548.1.12",
          "form": "강아지 귀엽죠",
          "original_form": "강아지 귀엽죠",
          "speaker_id": "2",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008548.1.13",
          "form": "요즘 날씨가 좋아서 산책하는 강아지들이 자주 보여서 좋아요",
          "original_form": "요즘 날씨가 좋아서 산책하는 강아지들이 자주 보여서 좋아요",
          "speaker_id": "2",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008548.1.14",
          "form": "네 귀요미",
          "original_form": "네 귀요미",
          "speaker_id": "1",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008548.1.15",
          "form": "다른거 키우고 싶은거 있어오?",
          "original_form": "다른거 키우고 싶은거 있어오?",
          "speaker_id": "1",
          "time": "20210823 07:21"
        },
        {
          "id": "MDRW2100008548.1.16",
          "form": "고양이 키우고 싶어요 길에서 마주치는 고양이들이 너무 귀여워요",
          "original_form": "고양이 키우고 싶어요 길에서 마주치는 고양이들이 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210823 07:38"
        },
        {
          "id": "MDRW2100008548.1.17",
          "form": "털관리는요?",
          "original_form": "털관리는요?",
          "speaker_id": "1",
          "time": "20210823 07:39"
        }
      ]
