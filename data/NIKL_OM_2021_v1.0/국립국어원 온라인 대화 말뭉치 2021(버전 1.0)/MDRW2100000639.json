      "utterance": [
        {
          "id": "MDRW2100000639.1.1",
          "form": "요새 아빠 뱃살이 넘 많이나옴",
          "original_form": "요새 아빠 뱃살이 넘 많이나옴",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000639.1.2",
          "form": "아빠 뱃살이 요새 넘 많이 나온거 아니요",
          "original_form": "아빠 뱃살이 요새 넘 많이 나온거 아니요",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000639.1.3",
          "form": "나이 들면 조금씩 다 나온다",
          "original_form": "나이 들면 조금씩 다 나온다",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000639.1.4",
          "form": "맛있는 것도 먹고 해서",
          "original_form": "맛있는 것도 먹고 해서",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000639.1.5",
          "form": "그래도 넘심각함",
          "original_form": "그래도 넘심각함",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000639.1.6",
          "form": "적당히 몸 관리해야지",
          "original_form": "적당히 몸 관리해야지",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000639.1.7",
          "form": "무리하면 병온다",
          "original_form": "무리하면 병온다",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000639.1.8",
          "form": "여름에는 너무 더워서",
          "original_form": "여름에는 너무 더워서",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000639.1.9",
          "form": "뒷산에 못갔는데",
          "original_form": "뒷산에 못갔는데",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000639.1.10",
          "form": "좀 시원해지면",
          "original_form": "좀 시원해지면",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000639.1.11",
          "form": "구랭! 집에만 있어서 더 그렇다",
          "original_form": "구랭! 집에만 있어서 더 그렇다",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000639.1.12",
          "form": "밖에 운동하러 자주 나갑세~",
          "original_form": "밖에 운동하러 자주 나갑세~",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000639.1.13",
          "form": "강아지 데리고 가야지",
          "original_form": "강아지 데리고 가야지",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000639.1.14",
          "form": "구랭 ㅋ 강쥐들이랑 같이 운동 좋네",
          "original_form": "구랭 ㅋ 강쥐들이랑 같이 운동 좋네",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000639.1.15",
          "form": "벌써 가을로 접어드는 것 같네",
          "original_form": "벌써 가을로 접어드는 것 같네",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000639.1.16",
          "form": "떨어지는 밤도 주워 먹어야지",
          "original_form": "떨어지는 밤도 주워 먹어야지",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000639.1.17",
          "form": "ㅋㅋㅋ 살빼려고 운동하려다가 살찌겠누",
          "original_form": "ㅋㅋㅋ 살빼려고 운동하려다가 살찌겠누",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000639.1.18",
          "form": "어릴적 생각하면서",
          "original_form": "어릴적 생각하면서",
          "speaker_id": "2",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000639.1.19",
          "form": "어릴때는 꿀밤도 주웠었는데",
          "original_form": "어릴때는 꿀밤도 주웠었는데",
          "speaker_id": "2",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000639.1.20",
          "form": "ㅎㅎ 시골엔 많았을텐디 여긴 잘 없을듯",
          "original_form": "ㅎㅎ 시골엔 많았을텐디 여긴 잘 없을듯",
          "speaker_id": "1",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000639.1.21",
          "form": "그리고 있어도 다람쥐들 먹어야지~",
          "original_form": "그리고 있어도 다람쥐들 먹어야지~",
          "speaker_id": "1",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000639.1.22",
          "form": "내가 먼저지",
          "original_form": "내가 먼저지",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000639.1.23",
          "form": "욕심쟁이?",
          "original_form": "욕심쟁이?",
          "speaker_id": "1",
          "time": "20210819 17:28"
        }
      ]
