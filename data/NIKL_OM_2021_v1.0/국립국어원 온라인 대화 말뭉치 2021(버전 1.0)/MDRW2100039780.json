      "utterance": [
        {
          "id": "MDRW2100039780.1.1",
          "form": "앗 약간 그런게 있긴 하죠",
          "original_form": "앗 약간 그런게 있긴 하죠",
          "speaker_id": "2",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100039780.1.2",
          "form": "저도 딱 노래만 듣는데 저는 한곡만 계속 듣는거 좋아해서",
          "original_form": "저도 딱 노래만 듣는데 저는 한곡만 계속 듣는거 좋아해서",
          "speaker_id": "2",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100039780.1.3",
          "form": "계속 같은 노래만 반복하고있어요..",
          "original_form": "계속 같은 노래만 반복하고있어요..",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039780.1.4",
          "form": "저도 그럴 때 있어요ㅋㅋㅌ",
          "original_form": "저도 그럴 때 있어요ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039780.1.5",
          "form": "좋어하는 k돌 있으신가요",
          "original_form": "좋어하는 k돌 있으신가요",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039780.1.6",
          "form": "레드벨벳이요",
          "original_form": "레드벨벳이요",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039780.1.7",
          "form": "노래가 너무 좋아요",
          "original_form": "노래가 너무 좋아요",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039780.1.8",
          "form": "아앗!!! 맞다 기억났어요!! 저도 레드벨벳..!!",
          "original_form": "아앗!!! 맞다 기억났어요!! 저도 레드벨벳..!!",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039780.1.9",
          "form": "ㅋㅋㅋ제일 좋아하는 노래 있으세요?",
          "original_form": "ㅋㅋㅋ제일 좋아하는 노래 있으세요?",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039780.1.10",
          "form": "여름 레드벨벳은",
          "original_form": "여름 레드벨벳은",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039780.1.11",
          "form": "역시 zoo아니면 모히또..!!",
          "original_form": "역시 zoo아니면 모히또..!!",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039780.1.12",
          "form": "라고 생각합니다..!",
          "original_form": "라고 생각합니다..!",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039780.1.13",
          "form": "아 제 픽은 유베럴노랑 한여름의 크리스마스예요",
          "original_form": "아 제 픽은 유베럴노랑 한여름의 크리스마스예요",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039780.1.14",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039780.1.15",
          "form": "유베럴노..!",
          "original_form": "유베럴노..!",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039780.1.16",
          "form": "넘 좋아요",
          "original_form": "넘 좋아요",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039780.1.17",
          "form": "저 레드벨벳 너무 좋아해서",
          "original_form": "저 레드벨벳 너무 좋아해서",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039780.1.18",
          "form": "슈퍼스타 에스엠?",
          "original_form": "슈퍼스타 에스엠?",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039780.1.19",
          "form": "그 게임아세요?",
          "original_form": "그 게임아세요?",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039780.1.20",
          "form": "그걸 레드벨벳 노래만 주구장창 했었엉ㅅ",
          "original_form": "그걸 레드벨벳 노래만 주구장창 했었엉ㅅ",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039780.1.21",
          "form": "리듬게임이요?",
          "original_form": "리듬게임이요?",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039780.1.22",
          "form": "ㅋㅋㅋㅋㅌ",
          "original_form": "ㅋㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039780.1.23",
          "form": "네..ㅎㅎ.. 리듬게임..!",
          "original_form": "네..ㅎㅎ.. 리듬게임..!",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039780.1.24",
          "form": "원래 리듬게임은 좋아하는 노래로 해야 제맛이죠",
          "original_form": "원래 리듬게임은 좋아하는 노래로 해야 제맛이죠",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039780.1.25",
          "form": "전 rnb스타일 수록곡 완전 좋아해요",
          "original_form": "전 rnb스타일 수록곡 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039780.1.26",
          "form": "저도 이번 수록곡도 아직 안들어봣는데",
          "original_form": "저도 이번 수록곡도 아직 안들어봣는데",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039780.1.27",
          "form": "얼른 들어봐여겠엉ㅅ",
          "original_form": "얼른 들어봐여겠엉ㅅ",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039780.1.28",
          "form": "이번 수록곡 중에서는 푸슁앤풀링이 제일 좋아요!",
          "original_form": "이번 수록곡 중에서는 푸슁앤풀링이 제일 좋아요!",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039780.1.29",
          "form": "프슁앤 풀링!",
          "original_form": "프슁앤 풀링!",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039780.1.30",
          "form": "꼭 기억해서 들어볼게요!!",
          "original_form": "꼭 기억해서 들어볼게요!!",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039780.1.31",
          "form": "헬스할때도 노래 많이 듣는편이거든요!!",
          "original_form": "헬스할때도 노래 많이 듣는편이거든요!!",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039780.1.32",
          "form": "저 그래서 요즘 런닝할 때 레드벨벳 앨범 듣고 있어요ㅋㅋ",
          "original_form": "저 그래서 요즘 런닝할 때 레드벨벳 앨범 듣고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039780.1.33",
          "form": "저 지금 플랭크",
          "original_form": "저 지금 플랭크",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039780.1.34",
          "form": "도전하묜서 들어볼게요.. 넘 힘들어요 시작도 전에..",
          "original_form": "도전하묜서 들어볼게요.. 넘 힘들어요 시작도 전에..",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039780.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래요 전 플랭크할 때 유튜브 보면 잘 되던데요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래요 전 플랭크할 때 유튜브 보면 잘 되던데요",
