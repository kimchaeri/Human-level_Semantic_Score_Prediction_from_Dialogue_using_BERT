      "utterance": [
        {
          "id": "MDRW2100005174.1.1",
          "form": "내 반려동물은 평소와 다름없이 건강해.",
          "original_form": "내 반려동물은 평소와 다름없이 건강해.",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005174.1.2",
          "form": "사진 보여줄까?",
          "original_form": "사진 보여줄까?",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005174.1.3",
          "form": "하지만 사진 못보내지",
          "original_form": "하지만 사진 못보내지",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005174.1.4",
          "form": "반려동물에 대해 어떻게 생각하니?",
          "original_form": "반려동물에 대해 어떻게 생각하니?",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005174.1.5",
          "form": "ㅋㅋㅋㅋ 맞아 카톡으로 보내줘",
          "original_form": "ㅋㅋㅋㅋ 맞아 카톡으로 보내줘",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005174.1.6",
          "form": "반려동물... 부담이 크지",
          "original_form": "반려동물... 부담이 크지",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005174.1.7",
          "form": "부담이 크지",
          "original_form": "부담이 크지",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005174.1.8",
          "form": "아무래도",
          "original_form": "아무래도",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005174.1.9",
          "form": "갑자기 반려동물이 생긴 기분은 어떠니",
          "original_form": "갑자기 반려동물이 생긴 기분은 어떠니",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005174.1.10",
          "form": "처음에는 좋았는데 점점 귀찮고 싫어지더라.",
          "original_form": "처음에는 좋았는데 점점 귀찮고 싫어지더라.",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005174.1.11",
          "form": "그런거 치고는 넘 좋아하는 것 같은걸",
          "original_form": "그런거 치고는 넘 좋아하는 것 같은걸",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005174.1.12",
          "form": "그래보였니? 애증인거같아.",
          "original_form": "그래보였니? 애증인거같아.",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005174.1.13",
          "form": "애증.. 혐관",
          "original_form": "애증.. 혐관",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005174.1.14",
          "form": "도리는 널 좋아하잖아",
          "original_form": "도리는 널 좋아하잖아",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005174.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 귀엽고 좋은데 챙겨주긴 너무 귀찮아.. 넌 키워본 적 있니?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 귀엽고 좋은데 챙겨주긴 너무 귀찮아.. 넌 키워본 적 있니?",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005174.1.16",
          "form": "어릴때 새를 키운 적 있어",
          "original_form": "어릴때 새를 키운 적 있어",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005174.1.17",
          "form": "새 사료도 따로 나오나?",
          "original_form": "새 사료도 따로 나오나?",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005174.1.18",
          "form": "응응 그럼!! 곡식이랑 이런거 섞인 사료 나와",
          "original_form": "응응 그럼!! 곡식이랑 이런거 섞인 사료 나와",
