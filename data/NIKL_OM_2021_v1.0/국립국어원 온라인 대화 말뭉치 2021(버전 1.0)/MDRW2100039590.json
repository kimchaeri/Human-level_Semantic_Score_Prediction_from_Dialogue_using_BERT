      "utterance": [
        {
          "id": "MDRW2100039590.1.1",
          "form": "네! 영화는 장르 안가리고 다 봐요ㅎㅎ",
          "original_form": "네! 영화는 장르 안가리고 다 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039590.1.2",
          "form": "헐 좋아하는 거 잇어요??",
          "original_form": "헐 좋아하는 거 잇어요??",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039590.1.3",
          "form": "저 재난영허ㅏ 좋아하는데",
          "original_form": "저 재난영허ㅏ 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039590.1.4",
          "form": "뭐 볼까 고밍이라서요",
          "original_form": "뭐 볼까 고밍이라서요",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039590.1.5",
          "form": "유명한것들은 다 보셨을거같아서...ㅋㅋㅋㅋ",
          "original_form": "유명한것들은 다 보셨을거같아서...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039590.1.6",
          "form": "컨테이젼은 보셨죠? 코로나랑 비슷한 상황...",
          "original_form": "컨테이젼은 보셨죠? 코로나랑 비슷한 상황...",
          "speaker_id": "2",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039590.1.7",
          "form": "엇 안봤습니다",
          "original_form": "엇 안봤습니다",
          "speaker_id": "1",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039590.1.8",
          "form": "혹시 이스케이프?",
          "original_form": "혹시 이스케이프?",
          "speaker_id": "1",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039590.1.9",
          "form": "이거 아시나요",
          "original_form": "이거 아시나요",
          "speaker_id": "1",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039590.1.10",
          "form": "안본거네여! 평점 나쁘지 않은데요??",
          "original_form": "안본거네여! 평점 나쁘지 않은데요??",
          "speaker_id": "2",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039590.1.11",
          "form": "네 유명하대요!!!",
          "original_form": "네 유명하대요!!!",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039590.1.12",
          "form": "상어 영화는요?",
          "original_form": "상어 영화는요?",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039590.1.13",
          "form": "47미터 2 보셨어요?",
          "original_form": "47미터 2 보셨어요?",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039590.1.14",
          "form": "이거 저 영화관에서 숨도못쉬고 봤어요",
          "original_form": "이거 저 영화관에서 숨도못쉬고 봤어요",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039590.1.15",
          "form": "네ㅋㅋㅋㅋㅋ 인정이요!!",
          "original_form": "네ㅋㅋㅋㅋㅋ 인정이요!!",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039590.1.16",
          "form": "저 그거 1도 봤는데 재밌어요",
          "original_form": "저 그거 1도 봤는데 재밌어요",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039590.1.17",
          "form": "1보다 2가 더 무섭지않나여?ㅜㅜㅜ",
          "original_form": "1보다 2가 더 무섭지않나여?ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039590.1.18",
          "form": "이거보고 상어꿈도 많이꿨어요..너무 무서워..",
          "original_form": "이거보고 상어꿈도 많이꿨어요..너무 무서워..",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039590.1.19",
          "form": "전 1이 더 재밌긴 했어요!!",
          "original_form": "전 1이 더 재밌긴 했어요!!",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039590.1.20",
          "form": "2는 너무 답답 ㅠㅠ",
          "original_form": "2는 너무 답답 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039590.1.21",
          "form": "그 굳이 가지 말라는 곳을 휴",
          "original_form": "그 굳이 가지 말라는 곳을 휴",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039590.1.22",
          "form": "ㅋㅋㅋㅋ그건 그래여",
          "original_form": "ㅋㅋㅋㅋ그건 그래여",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039590.1.23",
          "form": "영화 주인공들은 늘 바보같은짓을 하죠...",
          "original_form": "영화 주인공들은 늘 바보같은짓을 하죠...",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039590.1.24",
          "form": "정말요 -_-..",
          "original_form": "정말요 -_-..",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039590.1.25",
          "form": "근데 상어 진짜 무섭더라구요",
          "original_form": "근데 상어 진짜 무섭더라구요",
