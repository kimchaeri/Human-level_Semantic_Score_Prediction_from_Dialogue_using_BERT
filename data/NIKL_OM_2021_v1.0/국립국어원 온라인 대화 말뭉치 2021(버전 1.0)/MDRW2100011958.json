      "utterance": [
        {
          "id": "MDRW2100011958.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011958.1.2",
          "form": "키우는 반려동물 있으세요?",
          "original_form": "키우는 반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011958.1.3",
          "form": "아뇨 고양이 키워보고시퍼요",
          "original_form": "아뇨 고양이 키워보고시퍼요",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011958.1.4",
          "form": "아하 저는 알레르기가 있어서ㅠㅠ",
          "original_form": "아하 저는 알레르기가 있어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011958.1.5",
          "form": "아그럼 강아지 키우시나요?",
          "original_form": "아그럼 강아지 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011958.1.6",
          "form": "아니요 동물은 아무것도 안 키워요!",
          "original_form": "아니요 동물은 아무것도 안 키워요!",
          "speaker_id": "2",
          "time": "20210824 18:01"
        },
        {
          "id": "MDRW2100011958.1.7",
          "form": "아 키워보고 싶으신건있나요",
          "original_form": "아 키워보고 싶으신건있나요",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011958.1.8",
          "form": "아니요ㅠㅠ 제가 케어해주기는 너무 어려울 것 같아서..",
          "original_form": "아니요ㅠㅠ 제가 케어해주기는 너무 어려울 것 같아서..",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011958.1.9",
          "form": "고양이 좋아하세요?",
          "original_form": "고양이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011958.1.10",
          "form": "고양이 눈이 너무 귀여워요ㅠ",
          "original_form": "고양이 눈이 너무 귀여워요ㅠ",
          "speaker_id": "1",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011958.1.11",
          "form": "고양이 유튜브같은 것도 보세요?",
          "original_form": "고양이 유튜브같은 것도 보세요?",
          "speaker_id": "2",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011958.1.12",
          "form": "아뇨 따로보진않아요ㅎㅎ",
          "original_form": "아뇨 따로보진않아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100011958.1.13",
          "form": "요즘 귀여운 고양이들 영상도 많더라구요ㅋㅋ",
          "original_form": "요즘 귀여운 고양이들 영상도 많더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011958.1.14",
          "form": "오 한번 찾아봐야겠어요",
          "original_form": "오 한번 찾아봐야겠어요",
          "speaker_id": "1",
