      "utterance": [
        {
          "id": "MDRW2100012140.1.1",
          "form": "너는 동물 키워?",
          "original_form": "너는 동물 키워?",
          "speaker_id": "1",
          "time": "20210824 19:34"
        },
        {
          "id": "MDRW2100012140.1.2",
          "form": "안키워",
          "original_form": "안키워",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012140.1.3",
          "form": "왜안키워",
          "original_form": "왜안키워",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012140.1.4",
          "form": "난 고양이키워",
          "original_form": "난 고양이키워",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012140.1.5",
          "form": "고양이 이름이 뭐야?",
          "original_form": "고양이 이름이 뭐야?",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012140.1.6",
          "form": "또치",
          "original_form": "또치",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012140.1.7",
          "form": "무슨종이야(",
          "original_form": "무슨종이야(",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012140.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012140.1.9",
          "form": "그냥 길고양이랄까 뭐라해야돼",
          "original_form": "그냥 길고양이랄까 뭐라해야돼",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.10",
          "form": "몰라?",
          "original_form": "몰라?",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.11",
          "form": "그냥 흔히 길에서 보이는 애들이야",
          "original_form": "그냥 흔히 길에서 보이는 애들이야",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.12",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.13",
          "form": "냥줍햇거든",
          "original_form": "냥줍햇거든",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.14",
          "form": "몇살이야?",
          "original_form": "몇살이야?",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.15",
          "form": "간택당햇엄",
          "original_form": "간택당햇엄",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.16",
          "form": "몰라 ㅇㅅㅇ",
          "original_form": "몰라 ㅇㅅㅇ",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.17",
          "form": "키우는거 맞지?",
          "original_form": "키우는거 맞지?",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.18",
          "form": "1살?",
          "original_form": "1살?",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.19",
          "form": "아가때 데려왔어",
          "original_form": "아가때 데려왔어",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012140.1.20",
          "form": "얼마나 키웠어?",
          "original_form": "얼마나 키웠어?",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012140.1.21",
          "form": "1년!",
          "original_form": "1년!",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012140.1.22",
          "form": "털 많이 날려?",
          "original_form": "털 많이 날려?",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012140.1.23",
          "form": "애가 말을 알아듣나봐",
          "original_form": "애가 말을 알아듣나봐",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012140.1.24",
          "form": "짱많이날려",
          "original_form": "짱많이날려",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012140.1.25",
          "form": "이름부르면 개처럼 달려와",
          "original_form": "이름부르면 개처럼 달려와",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012140.1.26",
          "form": "똑똑한가봐",
          "original_form": "똑똑한가봐",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012140.1.27",
          "form": "배변은 잘해?",
          "original_form": "배변은 잘해?",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012140.1.28",
          "form": "그 고양이 모래? 놔두면 알아서 거기에다해",
          "original_form": "그 고양이 모래? 놔두면 알아서 거기에다해",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012140.1.29",
          "form": "멋있다",
          "original_form": "멋있다",
          "speaker_id": "2",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012140.1.30",
          "form": "여자야 남자야?",
          "original_form": "여자야 남자야?",
          "speaker_id": "2",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012140.1.31",
          "form": "여자애",
          "original_form": "여자애",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012140.1.32",
          "form": "귀여워",
          "original_form": "귀여워",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012140.1.33",
          "form": "귀엽겠다",
          "original_form": "귀엽겠다",
          "speaker_id": "2",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012140.1.34",
          "form": "또치도 너 좋대?",
          "original_form": "또치도 너 좋대?",
          "speaker_id": "2",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012140.1.35",
          "form": "너도 나중에 키워 또치 나 엄청좋아해",
          "original_form": "너도 나중에 키워 또치 나 엄청좋아해",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012140.1.36",
          "form": "나도 고양이 키워보고싶긴해",
          "original_form": "나도 고양이 키워보고싶긴해",
          "speaker_id": "2",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100012140.1.37",
          "form": "같이키우자",
          "original_form": "같이키우자",
          "speaker_id": "1",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100012140.1.38",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210824 19:39"
        }
      ]
