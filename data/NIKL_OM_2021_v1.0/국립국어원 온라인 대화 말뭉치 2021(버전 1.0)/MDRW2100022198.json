      "utterance": [
        {
          "id": "MDRW2100022198.1.1",
          "form": "화장실 청소는 주1회 하시나요?",
          "original_form": "화장실 청소는 주1회 하시나요?",
          "speaker_id": "2",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100022198.1.2",
          "form": "거실화장실은 제 담당이라 제가 하는데",
          "original_form": "거실화장실은 제 담당이라 제가 하는데",
          "speaker_id": "2",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100022198.1.3",
          "form": "화장실청소는 이틀에 한번은 합니다 ㅋㅋ",
          "original_form": "화장실청소는 이틀에 한번은 합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100022198.1.4",
          "form": "존경합니다",
          "original_form": "존경합니다",
          "speaker_id": "2",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100022198.1.5",
          "form": "애기가 같이 쓰게되면",
          "original_form": "애기가 같이 쓰게되면",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022198.1.6",
          "form": "부지런하세요",
          "original_form": "부지런하세요",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022198.1.7",
          "form": "어쩔수없이 하게 됩니다 ㅋㅋㅋㅋ",
          "original_form": "어쩔수없이 하게 됩니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022198.1.8",
          "form": "아들인가요?",
          "original_form": "아들인가요?",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022198.1.9",
          "form": "딸입니다 ㅋㅋㅋㅋ",
          "original_form": "딸입니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022198.1.10",
          "form": "아 부럽습니다",
          "original_form": "아 부럽습니다",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022198.1.11",
          "form": "딸이라 자주 청슈해줘야해요ㅠ",
          "original_form": "딸이라 자주 청슈해줘야해요ㅠ",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022198.1.12",
          "form": "아들이십니까??",
          "original_form": "아들이십니까??",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022198.1.13",
          "form": "부럽습니다~",
          "original_form": "부럽습니다~",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.14",
          "form": "네 아들입니다",
          "original_form": "네 아들입니다",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.15",
          "form": "딸 딸 인가요?",
          "original_form": "딸 딸 인가요?",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.16",
          "form": "넵~외동인가요??",
          "original_form": "넵~외동인가요??",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.17",
          "form": "진짜 너무 부럽네요",
          "original_form": "진짜 너무 부럽네요",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.18",
          "form": "네 아직 외동입니다",
          "original_form": "네 아직 외동입니다",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.19",
          "form": "고민중입니다",
          "original_form": "고민중입니다",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.20",
          "form": "둘째는..",
          "original_form": "둘째는..",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.21",
          "form": "동생을 만들어줘야되나",
          "original_form": "동생을 만들어줘야되나",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100022198.1.22",
          "form": "와이프도 힘들어하고",
          "original_form": "와이프도 힘들어하고",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022198.1.23",
          "form": "낳기전까진 계속 고민되는거 같습니다~",
          "original_form": "낳기전까진 계속 고민되는거 같습니다~",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022198.1.24",
          "form": "육아하면 아내의 삶이 없어질것같아",
          "original_form": "육아하면 아내의 삶이 없어질것같아",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022198.1.25",
          "form": "자기 삶을 살앗으면 싶습니다",
          "original_form": "자기 삶을 살앗으면 싶습니다",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022198.1.26",
          "form": "희생만할것 같아요",
          "original_form": "희생만할것 같아요",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022198.1.27",
          "form": "하나가 있건 둘이있건 애를 낳은순간부터는..",
          "original_form": "하나가 있건 둘이있건 애를 낳은순간부터는..",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022198.1.28",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.29",
          "form": "그럴수밖에 없는듯요ㅠ",
          "original_form": "그럴수밖에 없는듯요ㅠ",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.30",
          "form": "엄마는 정말 위대합니다",
          "original_form": "엄마는 정말 위대합니다",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.31",
          "form": "내년부터ㅅ",
          "original_form": "내년부터ㅅ",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.32",
          "form": "아빠도 위대합니다 ㅋㅋㅋ",
          "original_form": "아빠도 위대합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.33",
          "form": "제가 직장어린이집 가면",
          "original_form": "제가 직장어린이집 가면",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.34",
          "form": "데리고 다녀야되는데",
          "original_form": "데리고 다녀야되는데",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.35",
          "form": "와이프가 자유가 생길것같아서",
          "original_form": "와이프가 자유가 생길것같아서",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.36",
          "form": "좋습니다",
          "original_form": "좋습니다",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100022198.1.37",
          "form": "그쯤..둘째가 생길것 같습니다 ㅋㅋㅋㅋ",
          "original_form": "그쯤..둘째가 생길것 같습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022198.1.38",
          "form": "공부도 좀하고 하고싶은거 했으면 좋겠네요",
          "original_form": "공부도 좀하고 하고싶은거 했으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100022198.1.39",
          "form": "사실 요즘에 사이가 좋아서",
          "original_form": "사실 요즘에 사이가 좋아서",
          "speaker_id": "2",
          "time": "20210829 11:00"
        }
      ]
    }
