      "utterance": [
        {
          "id": "MDRW2100003027.1.1",
          "form": "애들은 이제 그림 그리고 놀아",
          "original_form": "애들은 이제 그림 그리고 놀아",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003027.1.2",
          "form": "화가 될라나 왜케 열심히 그려 ㅋ",
          "original_form": "화가 될라나 왜케 열심히 그려 ㅋ",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003027.1.3",
          "form": "몰라~하루에도 수십장씩",
          "original_form": "몰라~하루에도 수십장씩",
          "speaker_id": "1",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003027.1.4",
          "form": "재활용 쓰레기만 잔뜩 쌓이더만 ㅋㅋ",
          "original_form": "재활용 쓰레기만 잔뜩 쌓이더만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003027.1.5",
          "form": "오늘 분리수거 하는데도 아주 종이 쓰레기가 한가득이야",
          "original_form": "오늘 분리수거 하는데도 아주 종이 쓰레기가 한가득이야",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100003027.1.6",
          "form": "우리 때는 도화지 한 장에 정성을 다해서 그렸는데 ㅎㅎ",
          "original_form": "우리 때는 도화지 한 장에 정성을 다해서 그렸는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003027.1.7",
          "form": "라떼는 말이야~~ ㅋㅋㅋㅋ",
          "original_form": "라떼는 말이야~~ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003027.1.8",
          "form": "안그럴려고 해도 라떼가 자꾸 나옴 ㅋㅋㅋ",
          "original_form": "안그럴려고 해도 라떼가 자꾸 나옴 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003027.1.9",
          "form": "ㅎㅎ 그러지마요 넘 나이들어 보여",
          "original_form": "ㅎㅎ 그러지마요 넘 나이들어 보여",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003027.1.10",
          "form": "ㅎㅎㅎ 아저씨 맞는데 뭘",
          "original_form": "ㅎㅎㅎ 아저씨 맞는데 뭘",
          "speaker_id": "2",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003027.1.11",
          "form": "종이 낭비가 심해서 좀 아껴쓰라고 말해야겠어",
          "original_form": "종이 낭비가 심해서 좀 아껴쓰라고 말해야겠어",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003027.1.12",
          "form": "ㅇㅇ 평소에 절약해야지",
          "original_form": "ㅇㅇ 평소에 절약해야지",
          "speaker_id": "2",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003027.1.13",
          "form": "넉넉히 살아서 아까운 줄 몰라 ㅠ",
          "original_form": "넉넉히 살아서 아까운 줄 몰라 ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003027.1.14",
          "form": "엄마아빠는 둘 다 짠돌이인데 ㅎ",
          "original_form": "엄마아빠는 둘 다 짠돌이인데 ㅎ",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003027.1.15",
          "form": "은연중에 닮겠지? 기대해봐야지",
          "original_form": "은연중에 닮겠지? 기대해봐야지",
