      "utterance": [
        {
          "id": "MDRW2100042256.1.1",
          "form": "운동하는 멋진 나자신",
          "original_form": "운동하는 멋진 나자신",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042256.1.2",
          "form": "여기에 취해서 운동하게 돼요ㅎㅎ",
          "original_form": "여기에 취해서 운동하게 돼요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042256.1.3",
          "form": "네 거울앞에서 동작 잘 되면 기분 최고예요!",
          "original_form": "네 거울앞에서 동작 잘 되면 기분 최고예요!",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042256.1.4",
          "form": "ㅎㅎ맞아요 운동하면서 거울보기",
          "original_form": "ㅎㅎ맞아요 운동하면서 거울보기",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042256.1.5",
          "form": "근데 전 사실 아직 초보라 거울 볼 여유가 많진 않아요ㅜ",
          "original_form": "근데 전 사실 아직 초보라 거울 볼 여유가 많진 않아요ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042256.1.6",
          "form": "저 그래서 필라하다 발레핏도 했어요 야매였지만",
          "original_form": "저 그래서 필라하다 발레핏도 했어요 야매였지만",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042256.1.7",
          "form": "발레핏 기분 최고예요",
          "original_form": "발레핏 기분 최고예요",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042256.1.8",
          "form": "아 발레핏",
          "original_form": "아 발레핏",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042256.1.9",
          "form": "소개만 봤었는데",
          "original_form": "소개만 봤었는데",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042256.1.10",
          "form": "또 내모습에 도취됩니다ㅋㅋ",
          "original_form": "또 내모습에 도취됩니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042256.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042256.1.12",
          "form": "저는 통통이라 발레핏은 엄두가 안나요ㅜ",
          "original_form": "저는 통통이라 발레핏은 엄두가 안나요ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042256.1.13",
          "form": "사실 저도 통통인데 ㅋㅋ 쌤한테 저를 투영하고ㅋㅋ",
          "original_form": "사실 저도 통통인데 ㅋㅋ 쌤한테 저를 투영하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042256.1.14",
          "form": "아~ 꿀팁이네요",
          "original_form": "아~ 꿀팁이네요",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042256.1.15",
          "form": "운동강사 샘한테 나를 투영한다ㅎㅎ",
          "original_form": "운동강사 샘한테 나를 투영한다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042256.1.16",
          "form": "넵넵 제 기분이 중요하니까요!",
          "original_form": "넵넵 제 기분이 중요하니까요!",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042256.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042256.1.18",
          "form": "기분좋게 운동해야죠",
          "original_form": "기분좋게 운동해야죠",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042256.1.19",
          "form": "발레핏 도전해보세요!",
          "original_form": "발레핏 도전해보세요!",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042256.1.20",
          "form": "홈트는 안하세요?",
          "original_form": "홈트는 안하세요?",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042256.1.21",
          "form": "홈트는 몇번 했는데",
          "original_form": "홈트는 몇번 했는데",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042256.1.22",
          "form": "꾸준히는 못하고ㅠ",
          "original_form": "꾸준히는 못하고ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042256.1.23",
          "form": "그렇긴해요ㅜ 강제성이 없음...",
          "original_form": "그렇긴해요ㅜ 강제성이 없음...",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100042256.1.24",
          "form": "자세 체크도 안되고요",
          "original_form": "자세 체크도 안되고요",
          "speaker_id": "2",
          "time": "20210906 21:33"
        }
      ]
    }
