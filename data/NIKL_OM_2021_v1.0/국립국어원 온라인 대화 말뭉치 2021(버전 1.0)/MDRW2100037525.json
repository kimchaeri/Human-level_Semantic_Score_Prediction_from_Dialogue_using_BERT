      "utterance": [
        {
          "id": "MDRW2100037525.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 03:42"
        },
        {
          "id": "MDRW2100037525.1.2",
          "form": "홀맨이 광고에 나왔죠?",
          "original_form": "홀맨이 광고에 나왔죠?",
          "speaker_id": "1",
          "time": "20210901 03:42"
        },
        {
          "id": "MDRW2100037525.1.3",
          "form": "어떤 광고에 나왔나요?",
          "original_form": "어떤 광고에 나왔나요?",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100037525.1.4",
          "form": "엘지 핸드폰광고요 본적 있으새여?",
          "original_form": "엘지 핸드폰광고요 본적 있으새여?",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037525.1.5",
          "form": "아니요 본적없어여",
          "original_form": "아니요 본적없어여",
          "speaker_id": "2",
          "time": "20210901 15:10"
        },
        {
          "id": "MDRW2100037525.1.6",
          "form": "티비에 나왔나요?",
          "original_form": "티비에 나왔나요?",
          "speaker_id": "2",
          "time": "20210901 15:10"
        },
        {
          "id": "MDRW2100037525.1.7",
          "form": "네 티비광고요ㅎ 초딩때 인형도 나왔죠",
          "original_form": "네 티비광고요ㅎ 초딩때 인형도 나왔죠",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100037525.1.8",
          "form": "저희집에 인형은 있었어요",
          "original_form": "저희집에 인형은 있었어요",
          "speaker_id": "2",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100037525.1.9",
          "form": "말하는 인형",
          "original_form": "말하는 인형",
          "speaker_id": "2",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100037525.1.10",
          "form": "귀엽지 않나요?ㅜㅠ",
          "original_form": "귀엽지 않나요?ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100037525.1.11",
          "form": "귀여웠어요ㅋㅋㅋ\r\n그 당시에는 그렇게 말도하고 불빛도 나오고 감정설정하고 그런게 되는 인형이 많지 않았어서 신기하기도 했구요",
          "original_form": "귀여웠어요ㅋㅋㅋ\r\n그 당시에는 그렇게 말도하고 불빛도 나오고 감정설정하고 그런게 되는 인형이 많지 않았어서 신기하기도 했구요",
          "speaker_id": "2",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100037525.1.12",
          "form": "요즘 이벤트도 많이하고 있어용",
          "original_form": "요즘 이벤트도 많이하고 있어용",
          "speaker_id": "1",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100037525.1.13",
          "form": "이벤트에 다 응모하고 계신가요",
          "original_form": "이벤트에 다 응모하고 계신가요",
          "speaker_id": "2",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100037525.1.14",
          "form": "네 제가 받을슈 있는건 다 받으려고 하는중이애요",
          "original_form": "네 제가 받을슈 있는건 다 받으려고 하는중이애요",
          "speaker_id": "1",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100037525.1.15",
          "form": "어떤게 가장 마음에 들었나요?",
          "original_form": "어떤게 가장 마음에 들었나요?",
          "speaker_id": "2",
          "time": "20210901 15:21"
        }
      ]
