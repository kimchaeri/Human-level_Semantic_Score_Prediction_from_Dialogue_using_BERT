      "utterance": [
        {
          "id": "MDRW2100032538.1.1",
          "form": "국민지원금 받을 예정이신가요?",
          "original_form": "국민지원금 받을 예정이신가요?",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032538.1.2",
          "form": "저 내일 신청 하는날이예요!!!!",
          "original_form": "저 내일 신청 하는날이예요!!!!",
          "speaker_id": "2",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100032538.1.3",
          "form": "바로 하시는군요ㅋㅋ",
          "original_form": "바로 하시는군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100032538.1.4",
          "form": "사람 꽤 몰리겠죠?",
          "original_form": "사람 꽤 몰리겠죠?",
          "speaker_id": "1",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100032538.1.5",
          "form": "그럴거같아요 ㅠㅠ 대기타야 하나봐요 ㅋㅋㅋ",
          "original_form": "그럴거같아요 ㅠㅠ 대기타야 하나봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100032538.1.6",
          "form": "받으시면 어떤거 하실거세요?",
          "original_form": "받으시면 어떤거 하실거세요?",
          "speaker_id": "2",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100032538.1.7",
          "form": "전 목요일인데 봐서 천천히 할까봐요ㅎㅎ",
          "original_form": "전 목요일인데 봐서 천천히 할까봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100032538.1.8",
          "form": "지금 딱히 생각한 거는 없고 병원이랑 동네 마트에서 많이 쓸 것 같아요",
          "original_form": "지금 딱히 생각한 거는 없고 병원이랑 동네 마트에서 많이 쓸 것 같아요",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100032538.1.9",
          "form": "오 저도 마트에서 많이 쓸거같아요!!",
          "original_form": "오 저도 마트에서 많이 쓸거같아요!!",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032538.1.10",
          "form": "피부과에서도 가능한가봐요 ㅎㅎ",
          "original_form": "피부과에서도 가능한가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032538.1.11",
          "form": "저도 치과 가려고 생각 중이에요",
          "original_form": "저도 치과 가려고 생각 중이에요",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032538.1.12",
          "form": "아아!! 치과!! 치과 돈 많이 들죠 ㅠㅠ",
          "original_form": "아아!! 치과!! 치과 돈 많이 들죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032538.1.13",
          "form": "아 얼마전에 치료햇는데 나중에 할걸 그랫어여 ㅋㅋ",
          "original_form": "아 얼마전에 치료햇는데 나중에 할걸 그랫어여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032538.1.14",
          "form": "치과에도 사람 좀 몰릴 것 같네요 ㅎㅎ",
          "original_form": "치과에도 사람 좀 몰릴 것 같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032538.1.15",
          "form": "맞아요!! 피부과도 지원금 때문에 바쁠거래요 ㅋㅋㅋ",
          "original_form": "맞아요!! 피부과도 지원금 때문에 바쁠거래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032538.1.16",
          "form": "예약하고 가야겠네요 ㅎㅎ",
          "original_form": "예약하고 가야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032538.1.17",
          "form": "병원은 지원금으로 돈 잘벌겟어요 ㅠ",
          "original_form": "병원은 지원금으로 돈 잘벌겟어요 ㅠ",
