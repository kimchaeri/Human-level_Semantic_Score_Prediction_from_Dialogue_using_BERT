      "utterance": [
        {
          "id": "MDRW2100039353.1.1",
          "form": "새우가 먹고 싶어요",
          "original_form": "새우가 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100039353.1.2",
          "form": "새우 좋죠",
          "original_form": "새우 좋죠",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039353.1.3",
          "form": "알러지 없으신가봐요",
          "original_form": "알러지 없으신가봐요",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039353.1.4",
          "form": "네 다행히도요",
          "original_form": "네 다행히도요",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100039353.1.5",
          "form": "새우 알러지는 없다가도 생기더라고요",
          "original_form": "새우 알러지는 없다가도 생기더라고요",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039353.1.6",
          "form": "저는 꼭 피곤하면 올라와요",
          "original_form": "저는 꼭 피곤하면 올라와요",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039353.1.7",
          "form": "원래 새우 알러지 있으셨어요?",
          "original_form": "원래 새우 알러지 있으셨어요?",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039353.1.8",
          "form": "원래는 없었어요",
          "original_form": "원래는 없었어요",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039353.1.9",
          "form": "맛을 아는데 못먹게 되는 게 젤 슬픈거 같아요",
          "original_form": "맛을 아는데 못먹게 되는 게 젤 슬픈거 같아요",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039353.1.10",
          "form": "제 친구도 새우러버였는데 알러지생겨서 ㅜㅠ",
          "original_form": "제 친구도 새우러버였는데 알러지생겨서 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039353.1.11",
          "form": "ㅠㅠ 힘드시겠어요",
          "original_form": "ㅠㅠ 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039353.1.12",
          "form": "저는 먹는 알러지는 없는데 먼지 알레르기가 있는지",
          "original_form": "저는 먹는 알러지는 없는데 먼지 알레르기가 있는지",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100039353.1.13",
          "form": "큰도시의 길가로 가면 기침이 엄청 나더라고요",
          "original_form": "큰도시의 길가로 가면 기침이 엄청 나더라고요",
          "speaker_id": "1",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100039353.1.14",
          "form": "마스크 써도 그러세요?",
          "original_form": "마스크 써도 그러세요?",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039353.1.15",
          "form": "큰 도시로 간게 코로나 이전이라",
          "original_form": "큰 도시로 간게 코로나 이전이라",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039353.1.16",
          "form": "마스크를 안써봤네요 ㅋㅋ 매연 같은 거 때문인듯하니",
          "original_form": "마스크를 안써봤네요 ㅋㅋ 매연 같은 거 때문인듯하니",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039353.1.17",
          "form": "좀 ㄱㅊ할것같긴해요",
          "original_form": "좀 ㄱㅊ할것같긴해요",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039353.1.18",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039353.1.19",
          "form": "여기는 차 자체가 많지 않아서 ㅋㅋㅋ",
          "original_form": "여기는 차 자체가 많지 않아서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039353.1.20",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039353.1.21",
          "form": "피자 먹었어요",
          "original_form": "피자 먹었어요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039353.1.22",
          "form": "아 부럽네요 무슨 피자 드셨어요?",
          "original_form": "아 부럽네요 무슨 피자 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039353.1.23",
          "form": "그냥 치즈 피자 먹었어요",
          "original_form": "그냥 치즈 피자 먹었어요",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100039353.1.24",
          "form": "클래식이죠",
          "original_form": "클래식이죠",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100039353.1.25",
          "form": "저도 저번 주말에 임실치즈피자에서 고르곤졸라 시켜먹었는데",
          "original_form": "저도 저번 주말에 임실치즈피자에서 고르곤졸라 시켜먹었는데",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039353.1.26",
          "form": "역시 클래식이 좋더라고요",
          "original_form": "역시 클래식이 좋더라고요",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039353.1.27",
          "form": "깔끔하고...",
          "original_form": "깔끔하고...",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039353.1.28",
          "form": "임실치즈피자 한번도 못먹어 봤어요ㅠ",
          "original_form": "임실치즈피자 한번도 못먹어 봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039353.1.29",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039353.1.30",
          "form": "여기도 이번에 처음 생겼어요",
          "original_form": "여기도 이번에 처음 생겼어요",
          "speaker_id": "1",
          "time": "20210903 23:02"
        }
      ]
