      "utterance": [
        {
          "id": "MDRW2100010596.1.1",
          "form": "오늘 옥수수 쪘어",
          "original_form": "오늘 옥수수 쪘어",
          "speaker_id": "2",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010596.1.2",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010596.1.3",
          "form": "근데 안에 약간 속대 쪽으로 거뭇거뭇한데 괜찮나",
          "original_form": "근데 안에 약간 속대 쪽으로 거뭇거뭇한데 괜찮나",
          "speaker_id": "2",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010596.1.4",
          "form": "뭐? 옥수수 알이?",
          "original_form": "뭐? 옥수수 알이?",
          "speaker_id": "1",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010596.1.5",
          "form": "먹어도 되나 모르겠네",
          "original_form": "먹어도 되나 모르겠네",
          "speaker_id": "2",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010596.1.6",
          "form": "그거 아마 수확한지 좀 오래돼서 그럴걸",
          "original_form": "그거 아마 수확한지 좀 오래돼서 그럴걸",
          "speaker_id": "1",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010596.1.7",
          "form": "먹어도 상관은 없겠지?",
          "original_form": "먹어도 상관은 없겠지?",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010596.1.8",
          "form": "내가 보고 얘기해줄게~ 아마 상관은 없을거야",
          "original_form": "내가 보고 얘기해줄게~ 아마 상관은 없을거야",
          "speaker_id": "1",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010596.1.9",
          "form": "괜히 먹었다가 배탈날까봐 ㅎㅎ",
          "original_form": "괜히 먹었다가 배탈날까봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010596.1.10",
          "form": "수확하고 오래되면 검게 변하더라고",
          "original_form": "수확하고 오래되면 검게 변하더라고",
          "speaker_id": "1",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010596.1.11",
          "form": "ㅇㅇ 있다가 와서 좀 봐줘",
          "original_form": "ㅇㅇ 있다가 와서 좀 봐줘",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010596.1.12",
          "form": "알았어 애들 아직 먹이지 마",
          "original_form": "알았어 애들 아직 먹이지 마",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010596.1.13",
          "form": "당연하지 확인하고 먹일께",
          "original_form": "당연하지 확인하고 먹일께",
          "speaker_id": "2",
