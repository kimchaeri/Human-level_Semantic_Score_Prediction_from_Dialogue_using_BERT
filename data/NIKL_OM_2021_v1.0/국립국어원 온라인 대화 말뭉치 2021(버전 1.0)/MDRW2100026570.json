      "utterance": [
        {
          "id": "MDRW2100026570.1.1",
          "form": "요즘에 뭐 구매하신 상품 있으세요??",
          "original_form": "요즘에 뭐 구매하신 상품 있으세요??",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026570.1.2",
          "form": "저맨날 생수사고있어요?...ㅋㅋ",
          "original_form": "저맨날 생수사고있어요?...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026570.1.3",
          "form": "거의 생필품이 요즘 집앞에 계속..있네요",
          "original_form": "거의 생필품이 요즘 집앞에 계속..있네요",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026570.1.4",
          "form": "아 아무래도 많이 안나가시겠어요",
          "original_form": "아 아무래도 많이 안나가시겠어요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026570.1.5",
          "form": "세탁세제  핸드워시",
          "original_form": "세탁세제  핸드워시",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026570.1.6",
          "form": "폼클렌징",
          "original_form": "폼클렌징",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026570.1.7",
          "form": "나가기는 맨날나가는데",
          "original_form": "나가기는 맨날나가는데",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026570.1.8",
          "form": "코로나라 생필품 진짜 많이 쓰고 있는거 같아요",
          "original_form": "코로나라 생필품 진짜 많이 쓰고 있는거 같아요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026570.1.9",
          "form": "물건은 다 인터넷으로삽니당ㅋㅋㅋㅋㅋ",
          "original_form": "물건은 다 인터넷으로삽니당ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026570.1.10",
          "form": "예전이었으면 나가서 구경도 하고 그러는데 아쉽긴 해요",
          "original_form": "예전이었으면 나가서 구경도 하고 그러는데 아쉽긴 해요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026570.1.11",
          "form": "음.. 마트도가고 하는데 ...",
          "original_form": "음.. 마트도가고 하는데 ...",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026570.1.12",
          "form": "전 인터넷이 더 저렴해서",
          "original_form": "전 인터넷이 더 저렴해서",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026570.1.13",
          "form": "대부분  온라인 쇼핑해요^^",
          "original_form": "대부분  온라인 쇼핑해요^^",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026570.1.14",
          "form": "그러시구나! 전 둘러보는거 좋아해서요 신상품 나온거 구경도 하고 ㅋㅋ",
          "original_form": "그러시구나! 전 둘러보는거 좋아해서요 신상품 나온거 구경도 하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026570.1.15",
          "form": "저도 가끔가긴가는데 코로나때문에 자주는 못가요ㅜ",
          "original_form": "저도 가끔가긴가는데 코로나때문에 자주는 못가요ㅜ",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100026570.1.16",
          "form": "그래도 눈으로보고 쇼핑하는것도 넘좋죠",
          "original_form": "그래도 눈으로보고 쇼핑하는것도 넘좋죠",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100026570.1.17",
          "form": "쇼핑할맛나구요ㅋㅋ",
          "original_form": "쇼핑할맛나구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100026570.1.18",
          "form": "옷도 입어보고 신발도 신어보고~ㅎㅎ",
          "original_form": "옷도 입어보고 신발도 신어보고~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100026570.1.19",
          "form": "맞아요 요즘은 못가서 아쉬워요",
          "original_form": "맞아요 요즘은 못가서 아쉬워요",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100026570.1.20",
          "form": "오늘은  애들 음료 주문한게 배송왔어요",
          "original_form": "오늘은  애들 음료 주문한게 배송왔어요",
          "speaker_id": "2",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100026570.1.21",
          "form": "근데 정신없어서 깜빡하고",
          "original_form": "근데 정신없어서 깜빡하고",
          "speaker_id": "2",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100026570.1.22",
          "form": "현관앞에 그냥뒀네요 ??ㅋㅋ 생각난김에  구매한거 얼른 열어봐야게써요",
          "original_form": "현관앞에 그냥뒀네요 ??ㅋㅋ 생각난김에  구매한거 얼른 열어봐야게써요",
          "speaker_id": "2",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100026570.1.23",
          "form": "요즘 애들은 음료수 뭐 먹어요?ㅋㅋㅋ",
          "original_form": "요즘 애들은 음료수 뭐 먹어요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100026570.1.24",
          "form": "그 요미요미라고",
          "original_form": "그 요미요미라고",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100026570.1.25",
          "form": "응가잘싸게해주는 푸룬사과 종종 먹어요",
          "original_form": "응가잘싸게해주는 푸룬사과 종종 먹어요",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100026570.1.26",
          "form": "오늘은   수세미작두콩즙도 배송왔네요 요건 다른브랜드꺼에요 ㅋㅋ",
          "original_form": "오늘은   수세미작두콩즙도 배송왔네요 요건 다른브랜드꺼에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100026570.1.27",
          "form": "아 요미요미 그거 맛있어 보이더라고요 ㅎㅎ",
          "original_form": "아 요미요미 그거 맛있어 보이더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100026570.1.28",
          "form": "저는 오늘 가을옷 보러 다녀왔어요",
          "original_form": "저는 오늘 가을옷 보러 다녀왔어요",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100026570.1.29",
