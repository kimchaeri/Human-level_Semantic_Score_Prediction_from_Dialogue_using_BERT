      "utterance": [
        {
          "id": "MDRW2100012459.1.1",
          "form": "요즘 태교독서 좀 하시나요?",
          "original_form": "요즘 태교독서 좀 하시나요?",
          "speaker_id": "1",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012459.1.2",
          "form": "유튜브로 태교해요 ㅋㅋㅋ",
          "original_form": "유튜브로 태교해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012459.1.3",
          "form": "그래도 최근에 주식책을 많이 샀네요",
          "original_form": "그래도 최근에 주식책을 많이 샀네요",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012459.1.4",
          "form": "ㅋㅋㅋㅋ 태교를 주식책으로~ 주식영재가 태어나겠군요 ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 태교를 주식책으로~ 주식영재가 태어나겠군요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012459.1.5",
          "form": "부디 ㅋㅋㅋㅋ",
          "original_form": "부디 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012459.1.6",
          "form": "주식책 뭐 사셨어요?",
          "original_form": "주식책 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012459.1.7",
          "form": "최고의 주식 최적의 타이밍",
          "original_form": "최고의 주식 최적의 타이밍",
          "speaker_id": "2",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100012459.1.8",
          "form": "차트의 기술",
          "original_form": "차트의 기술",
          "speaker_id": "2",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100012459.1.9",
          "form": "ㅋㅋㅋㅋ 최적의 타이밍이 언제라고 하던가요?",
          "original_form": "ㅋㅋㅋㅋ 최적의 타이밍이 언제라고 하던가요?",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012459.1.10",
          "form": "알면 제가 이렇게 살지 않을꺼 같아요",
          "original_form": "알면 제가 이렇게 살지 않을꺼 같아요",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100012459.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100012459.1.12",
          "form": "ㅋㅋㅋㅋ 아 그러네요 ㅋㅋㅋ 알았다면 이미 부자가 되었겠어요",
          "original_form": "ㅋㅋㅋㅋ 아 그러네요 ㅋㅋㅋ 알았다면 이미 부자가 되었겠어요",
          "speaker_id": "1",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100012459.1.13",
          "form": "사신 주식책 좀 읽으셨나요?",
          "original_form": "사신 주식책 좀 읽으셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:30"
        },
        {
          "id": "MDRW2100012459.1.14",
          "form": "읽으려고 노력중인데... 쉽지 않다는 ㅋㅋㅋ",
          "original_form": "읽으려고 노력중인데... 쉽지 않다는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100012459.1.15",
          "form": "일주일간 열심히 읽으신거죠? 공모주 관련 책더 있나요?",
          "original_form": "일주일간 열심히 읽으신거죠? 공모주 관련 책더 있나요?",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100012459.1.16",
          "form": "공모주는 딱히 책을 산건 아니고 유튜브 봐요",
          "original_form": "공모주는 딱히 책을 산건 아니고 유튜브 봐요",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100012459.1.17",
          "form": "잘 정리해주더라구요 ㅋㅋㅋ",
