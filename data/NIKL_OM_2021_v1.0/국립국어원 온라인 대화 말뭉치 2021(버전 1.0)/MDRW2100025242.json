      "utterance": [
        {
          "id": "MDRW2100025242.1.1",
          "form": "음식은 어떤거 좋아하세요?ㅎㅎ",
          "original_form": "음식은 어떤거 좋아하세요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:29"
        },
        {
          "id": "MDRW2100025242.1.2",
          "form": "전 해산물 1순위 고기 2순위요!",
          "original_form": "전 해산물 1순위 고기 2순위요!",
          "speaker_id": "1",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025242.1.3",
          "form": "name2은욪?",
          "original_form": "&name2&은욪?",
          "speaker_id": "1",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025242.1.4",
          "form": "회 좋아합니다",
          "original_form": "회 좋아합니다",
          "speaker_id": "2",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025242.1.5",
          "form": "혹시 연어회신가요",
          "original_form": "혹시 연어회신가요",
          "speaker_id": "2",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025242.1.6",
          "form": "회는 다 좋아해요, 물론 연어도 사랑합니닺ㅎ",
          "original_form": "회는 다 좋아해요, 물론 연어도 사랑합니닺ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:36"
        },
        {
          "id": "MDRW2100025242.1.7",
          "form": "무슨 회 좋아하세요?",
          "original_form": "무슨 회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 02:36"
        },
        {
          "id": "MDRW2100025242.1.8",
          "form": "ㅋㅋ회를 좋아하신다니",
          "original_form": "ㅋㅋ회를 좋아하신다니",
          "speaker_id": "2",
          "time": "20210901 02:36"
        },
        {
          "id": "MDRW2100025242.1.9",
          "form": "먹잘알이시군요",
          "original_form": "먹잘알이시군요",
          "speaker_id": "2",
          "time": "20210901 02:36"
        },
        {
          "id": "MDRW2100025242.1.10",
          "form": "연어회 최애입니다 ㅎㅎ",
          "original_form": "연어회 최애입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:37"
        },
        {
          "id": "MDRW2100025242.1.11",
          "form": "연어회 맛나죠ㅎ 전 회덮밥일때가 넘 좋아요ㅎ",
          "original_form": "연어회 맛나죠ㅎ 전 회덮밥일때가 넘 좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:37"
        },
        {
          "id": "MDRW2100025242.1.12",
          "form": "초밥처럼 올려먹는거ㅎ",
          "original_form": "초밥처럼 올려먹는거ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:38"
        },
        {
          "id": "MDRW2100025242.1.13",
          "form": "ㅋㅋ회덮밥은 조그만 회로 먹잖아요!",
          "original_form": "ㅋㅋ회덮밥은 조그만 회로 먹잖아요!",
          "speaker_id": "2",
          "time": "20210901 02:39"
        },
        {
          "id": "MDRW2100025242.1.14",
          "form": "아 근데 생각하니까",
          "original_form": "아 근데 생각하니까",
          "speaker_id": "2",
          "time": "20210901 02:39"
        },
        {
          "id": "MDRW2100025242.1.15",
          "form": "맛있긴하겠당",
          "original_form": "맛있긴하겠당",
          "speaker_id": "2",
          "time": "20210901 02:39"
        },
        {
          "id": "MDRW2100025242.1.16",
          "form": "어 아뇨",
          "original_form": "어 아뇨",
          "speaker_id": "1",
          "time": "20210901 02:39"
        },
        {
          "id": "MDRW2100025242.1.17",
          "form": "고추장 비비는 그런 회덮밥말구",
          "original_form": "고추장 비비는 그런 회덮밥말구",
          "speaker_id": "1",
          "time": "20210901 02:39"
        },
        {
          "id": "MDRW2100025242.1.18",
          "form": "연어회를 밥위에 꽃처럼 펼쳐깐거요ㅎ",
          "original_form": "연어회를 밥위에 꽃처럼 펼쳐깐거요ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:40"
        },
        {
          "id": "MDRW2100025242.1.19",
          "form": "오 그게 뭐지",
          "original_form": "오 그게 뭐지",
          "speaker_id": "2",
          "time": "20210901 02:40"
        },
        {
          "id": "MDRW2100025242.1.20",
          "form": "아 초밥처럼~~",
          "original_form": "아 초밥처럼~~",
          "speaker_id": "2",
          "time": "20210901 02:41"
        },
        {
          "id": "MDRW2100025242.1.21",
          "form": "숟가락위에 밥얹고 회 얹고 무순 얹고 와사비 간장 올려 먹음JMT",
          "original_form": "숟가락위에 밥얹고 회 얹고 무순 얹고 와사비 간장 올려 먹음JMT",
          "speaker_id": "1",
          "time": "20210901 02:41"
        },
        {
          "id": "MDRW2100025242.1.22",
          "form": "이해했어요 ㅎㅎ",
          "original_form": "이해했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:41"
        },
        {
          "id": "MDRW2100025242.1.23",
          "form": "네네 연어회덮밥은 그렇게 잘 나와서ㅎ",
          "original_form": "네네 연어회덮밥은 그렇게 잘 나와서ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:41"
