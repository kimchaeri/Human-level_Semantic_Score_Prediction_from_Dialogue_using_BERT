      "utterance": [
        {
          "id": "MDRW2100041512.1.1",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "1",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100041512.1.2",
          "form": "네.. 출근했습니다..",
          "original_form": "네.. 출근했습니다..",
          "speaker_id": "2",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100041512.1.3",
          "form": "눈물나네요..",
          "original_form": "눈물나네요..",
          "speaker_id": "2",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100041512.1.4",
          "form": "정상이죠?",
          "original_form": "정상이죠?",
          "speaker_id": "2",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100041512.1.5",
          "form": "네 지극히 정상입니다",
          "original_form": "네 지극히 정상입니다",
          "speaker_id": "1",
          "time": "20210906 08:17"
        },
        {
          "id": "MDRW2100041512.1.6",
          "form": "8시출근인가봐요",
          "original_form": "8시출근인가봐요",
          "speaker_id": "1",
          "time": "20210906 08:18"
        },
        {
          "id": "MDRW2100041512.1.7",
          "form": "앗 아뇨 출근시간은 9신데.. 이해가 안가시겠지만..",
          "original_form": "앗 아뇨 출근시간은 9신데.. 이해가 안가시겠지만..",
          "speaker_id": "2",
          "time": "20210906 08:18"
        },
        {
          "id": "MDRW2100041512.1.8",
          "form": "잉찍 와서 밥을 먹어요..",
          "original_form": "잉찍 와서 밥을 먹어요..",
          "speaker_id": "2",
          "time": "20210906 08:18"
        },
        {
          "id": "MDRW2100041512.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 08:18"
        },
        {
          "id": "MDRW2100041512.1.10",
          "form": "헉 아침밥이 잘나오나요?",
          "original_form": "헉 아침밥이 잘나오나요?",
          "speaker_id": "1",
          "time": "20210906 08:56"
        },
        {
          "id": "MDRW2100041512.1.11",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210906 08:56"
        },
        {
          "id": "MDRW2100041512.1.12",
          "form": "아뇨..! 제가 도시락으로 싸와여..ㅎ",
          "original_form": "아뇨..! 제가 도시락으로 싸와여..ㅎ",
          "speaker_id": "2",
          "time": "20210906 09:42"
        },
        {
          "id": "MDRW2100041512.1.13",
          "form": "점심 저녁까지 다 싸와서 그냥 다 사무실에서 먹어요..ㅎㅎ",
          "original_form": "점심 저녁까지 다 싸와서 그냥 다 사무실에서 먹어요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 09:43"
        },
        {
          "id": "MDRW2100041512.1.14",
          "form": "헉 진짜요??",
          "original_form": "헉 진짜요??",
          "speaker_id": "1",
          "time": "20210906 09:49"
        },
        {
          "id": "MDRW2100041512.1.15",
          "form": "저도 도시락 싸는데..",
          "original_form": "저도 도시락 싸는데..",
          "speaker_id": "1",
          "time": "20210906 09:49"
        },
        {
          "id": "MDRW2100041512.1.16",
          "form": "아침까지싸오시는 건 신기하네요",
          "original_form": "아침까지싸오시는 건 신기하네요",
          "speaker_id": "1",
          "time": "20210906 09:49"
        },
        {
          "id": "MDRW2100041512.1.17",
          "form": "ㅋㅋㅋㅋㅋ전 사실 완전 직장은 아니고 반 알바정도..?",
          "original_form": "ㅋㅋㅋㅋㅋ전 사실 완전 직장은 아니고 반 알바정도..?",
          "speaker_id": "2",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100041512.1.18",
          "form": "그래서 크게 부담이없어요",
          "original_form": "그래서 크게 부담이없어요",
          "speaker_id": "2",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100041512.1.19",
          "form": "아 그래요??ㅋㅋㅋ 반 알바는 뭐죵?..",
          "original_form": "아 그래요??ㅋㅋㅋ 반 알바는 뭐죵?..",
          "speaker_id": "1",
          "time": "20210906 10:29"
        },
        {
          "id": "MDRW2100041512.1.20",
          "form": "정규직은 아니고",
          "original_form": "정규직은 아니고",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100041512.1.21",
          "form": "그냥 사무직알바같은 느낌이에요!!",
          "original_form": "그냥 사무직알바같은 느낌이에요!!",
          "speaker_id": "2",
          "time": "20210906 10:38"
        },
        {
          "id": "MDRW2100041512.1.22",
          "form": "오 그래듀 마음이 편하겠어요",
          "original_form": "오 그래듀 마음이 편하겠어요",
          "speaker_id": "1",
          "time": "20210906 10:43"
        },
        {
          "id": "MDRW2100041512.1.23",
          "form": "맞아요 그냥 공부랑 병행하기 좋는 그런 일인거같아요!",
          "original_form": "맞아요 그냥 공부랑 병행하기 좋는 그런 일인거같아요!",
          "speaker_id": "2",
          "time": "20210906 10:57"
        },
        {
          "id": "MDRW2100041512.1.24",
          "form": "아 공부도하셔요~? 무슨공부하세요ㅡ",
          "original_form": "아 공부도하셔요~? 무슨공부하세요ㅡ",
          "speaker_id": "1",
          "time": "20210906 11:03"
        },
        {
          "id": "MDRW2100041512.1.25",
          "form": "이번엔 제대로 취업을 하기 위해!",
          "original_form": "이번엔 제대로 취업을 하기 위해!",
          "speaker_id": "2",
          "time": "20210906 11:13"
        },
        {
          "id": "MDRW2100041512.1.26",
          "form": "취업을 위한 공부입니당..!",
          "original_form": "취업을 위한 공부입니당..!",
          "speaker_id": "2",
          "time": "20210906 11:13"
        },
        {
          "id": "MDRW2100041512.1.27",
          "form": "취준중이네요 아직 ㅠㅜ",
          "original_form": "취준중이네요 아직 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210906 11:13"
        },
        {
          "id": "MDRW2100041512.1.28",
          "form": "와 진짜 힘들죠 ㅠㅠ 저도 취준 했을 때 너무 힘들었어요 ㅠㅠ",
          "original_form": "와 진짜 힘들죠 ㅠㅠ 저도 취준 했을 때 너무 힘들었어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 11:16"
        },
        {
          "id": "MDRW2100041512.1.29",
          "form": "나이도있었어서",
          "original_form": "나이도있었어서",
          "speaker_id": "1",
          "time": "20210906 11:16"
        },
        {
          "id": "MDRW2100041512.1.30",
          "form": "힘든 기간인거 같아요",
          "original_form": "힘든 기간인거 같아요",
          "speaker_id": "2",
          "time": "20210906 11:45"
        },
        {
          "id": "MDRW2100041512.1.31",
          "form": "맞아요ㅜㅜ 저도 그래요 진짜 취준 기간이 제일 심적으로",
          "original_form": "맞아요ㅜㅜ 저도 그래요 진짜 취준 기간이 제일 심적으로",
          "speaker_id": "2",
          "time": "20210906 11:45"
        }
      ]
