      "utterance": [
        {
          "id": "MDRW2100020529.1.1",
          "form": "김치에 젓갈 들어갔냐 안들어갔냐로 싸웁니다",
          "original_form": "김치에 젓갈 들어갔냐 안들어갔냐로 싸웁니다",
          "speaker_id": "2",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100020529.1.2",
          "form": "약간 아시잖아요ㅋㅋㅋ 이제 혼날 나이가 아니라는거ㅋㅋㅋㄱㅋ",
          "original_form": "약간 아시잖아요ㅋㅋㅋ 이제 혼날 나이가 아니라는거ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100020529.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸 근데 맨날 혼나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸 근데 맨날 혼나요",
          "speaker_id": "2",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100020529.1.4",
          "form": "먹기 싫은데 이유가있나",
          "original_form": "먹기 싫은데 이유가있나",
          "speaker_id": "1",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100020529.1.5",
          "form": "저에게 당근이 그렇습니다",
          "original_form": "저에게 당근이 그렇습니다",
          "speaker_id": "1",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100020529.1.6",
          "form": "그냥 자식은 평생 혼나나봐요",
          "original_form": "그냥 자식은 평생 혼나나봐요",
          "speaker_id": "2",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100020529.1.7",
          "form": "엇 오이파이신데 당근이 불호인건 좀 신선하네요",
          "original_form": "엇 오이파이신데 당근이 불호인건 좀 신선하네요",
          "speaker_id": "2",
          "time": "20210828 09:07"
        },
        {
          "id": "MDRW2100020529.1.8",
          "form": "물론 이해합니다",
          "original_form": "물론 이해합니다",
          "speaker_id": "2",
          "time": "20210828 09:07"
        },
        {
          "id": "MDRW2100020529.1.9",
          "form": "생당근은 먹는데",
          "original_form": "생당근은 먹는데",
          "speaker_id": "1",
          "time": "20210828 09:07"
        },
        {
          "id": "MDRW2100020529.1.10",
          "form": "익힌건 흐물텅해서",
          "original_form": "익힌건 흐물텅해서",
          "speaker_id": "1",
          "time": "20210828 09:07"
        },
        {
          "id": "MDRW2100020529.1.11",
          "form": "아 식감문제네요",
          "original_form": "아 식감문제네요",
          "speaker_id": "2",
          "time": "20210828 09:07"
        },
        {
          "id": "MDRW2100020529.1.12",
          "form": "저도 가지 튀김은 좋아하는데",
          "original_form": "저도 가지 튀김은 좋아하는데",
          "speaker_id": "2",
          "time": "20210828 09:07"
        },
        {
          "id": "MDRW2100020529.1.13",
          "form": "음 맞아요",
          "original_form": "음 맞아요",
          "speaker_id": "1",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.14",
          "form": "다른 가지반찬은 흐물렁해서 불호에요",
          "original_form": "다른 가지반찬은 흐물렁해서 불호에요",
          "speaker_id": "2",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.15",
          "form": "물에데친거",
          "original_form": "물에데친거",
          "speaker_id": "1",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.16",
          "form": "너무싫어",
          "original_form": "너무싫어",
          "speaker_id": "1",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.17",
          "form": "생가지 튀긴가지 먹는데 물에만들어가면..",
          "original_form": "생가지 튀긴가지 먹는데 물에만들어가면..",
          "speaker_id": "1",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.18",
          "form": "그쵸 카레속 당근을 생각하면 되나요",
          "original_form": "그쵸 카레속 당근을 생각하면 되나요",
          "speaker_id": "2",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.19",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.20",
          "form": "카레좋아하시나요",
          "original_form": "카레좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.21",
          "form": "카레 어릴 때 첨먹었을 때가 기억나요",
          "original_form": "카레 어릴 때 첨먹었을 때가 기억나요",
          "speaker_id": "2",
          "time": "20210828 09:08"
        },
        {
          "id": "MDRW2100020529.1.22",
          "form": "어땠나요",
          "original_form": "어땠나요",
          "speaker_id": "1",
          "time": "20210828 09:09"
        },
        {
          "id": "MDRW2100020529.1.23",
          "form": "너무 맛이없어서 토한적은 처음이었죠,,,",
          "original_form": "너무 맛이없어서 토한적은 처음이었죠,,,",
          "speaker_id": "2",
          "time": "20210828 09:09"
        },
        {
          "id": "MDRW2100020529.1.24",
          "form": "근데 신기하게",
          "original_form": "근데 신기하게",
          "speaker_id": "2",
          "time": "20210828 09:09"
        },
        {
          "id": "MDRW2100020529.1.25",
          "form": "요즘은 꽤 좋아합니다",
          "original_form": "요즘은 꽤 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 09:09"
        },
        {
          "id": "MDRW2100020529.1.26",
          "form": "크면서 변하는거 같아요",
          "original_form": "크면서 변하는거 같아요",
          "speaker_id": "1",
          "time": "20210828 09:09"
        }
      ]
    }
  ]
