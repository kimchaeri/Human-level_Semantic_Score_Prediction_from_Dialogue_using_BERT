      "utterance": [
        {
          "id": "MDRW2100021001.1.1",
          "form": "요즘 따라 여행가고 싶네요 ㅜㅜ",
          "original_form": "요즘 따라 여행가고 싶네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100021001.1.2",
          "form": "저도요ㅠㅠ",
          "original_form": "저도요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100021001.1.3",
          "form": "못나가서 더 그런걸까요?",
          "original_form": "못나가서 더 그런걸까요?",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100021001.1.4",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100021001.1.5",
          "form": "코로나 지금 거의 2년 다되가잖아요",
          "original_form": "코로나 지금 거의 2년 다되가잖아요",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100021001.1.6",
          "form": "다들 여행이 근질근질 할거에요",
          "original_form": "다들 여행이 근질근질 할거에요",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100021001.1.7",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021001.1.8",
          "form": "해외는 진짜 꿈이에요",
          "original_form": "해외는 진짜 꿈이에요",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021001.1.9",
          "form": "해외보단 국내가 그래도 좀 안전한 거 같아요",
          "original_form": "해외보단 국내가 그래도 좀 안전한 거 같아요",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021001.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021001.1.11",
          "form": "바다가고싶어요!",
          "original_form": "바다가고싶어요!",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021001.1.12",
          "form": "동해바다 vs 서해바다",
          "original_form": "동해바다 vs 서해바다",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021001.1.13",
          "form": "서해!",
          "original_form": "서해!",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021001.1.14",
          "form": "갯벌 좋아해요?",
          "original_form": "갯벌 좋아해요?",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021001.1.15",
          "form": "한번밖에 안가봤어요ㅠ",
          "original_form": "한번밖에 안가봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100021001.1.16",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100021001.1.17",
          "form": "갯벌 진짜 꿀잼이에요 ㅋㅋㅋㅋ",
          "original_form": "갯벌 진짜 꿀잼이에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021001.1.18",
          "form": "거기서 게도 잡고 조개도 잡고~ 근데 옷 버릴 각오 해야되는 게 맴찢..",
          "original_form": "거기서 게도 잡고 조개도 잡고~ 근데 옷 버릴 각오 해야되는 게 맴찢..",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021001.1.19",
          "form": "우와 가족끼리 같이 가고싶네요",
          "original_form": "우와 가족끼리 같이 가고싶네요",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021001.1.20",
          "form": "버릴옷 필수,..",
          "original_form": "버릴옷 필수,..{emoji:☆}",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021001.1.21",
          "form": "조카 있어요?",
          "original_form": "조카 있어요?",
          "speaker_id": "1",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021001.1.22",
          "form": "조카들이 많이 좋아할 것 같아요 ㅎㅎ",
          "original_form": "조카들이 많이 좋아할 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021001.1.23",
          "form": "아직 조카가 없어요ㅜㅜ",
          "original_form": "아직 조카가 없어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100021001.1.24",
          "form": "아 애들이랑 같이가면 진짜 재밌겠네요",
          "original_form": "아 애들이랑 같이가면 진짜 재밌겠네요",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100021001.1.25",
          "form": "애들이랑 같이 가면 재밌는데ㅋㅋㅋ 자신을 포기해야해요",
          "original_form": "애들이랑 같이 가면 재밌는데ㅋㅋㅋ 자신을 포기해야해요",
          "speaker_id": "1",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100021001.1.26",
          "form": "원주민 될 지도 몰라요 ㅎㅎ",
          "original_form": "원주민 될 지도 몰라요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:34"
