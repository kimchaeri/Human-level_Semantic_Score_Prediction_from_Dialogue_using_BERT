          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034444.1.29",
          "form": "ㅋㅋ쪼매나요 ㅋㅋ",
          "original_form": "ㅋㅋ쪼매나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034444.1.30",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
