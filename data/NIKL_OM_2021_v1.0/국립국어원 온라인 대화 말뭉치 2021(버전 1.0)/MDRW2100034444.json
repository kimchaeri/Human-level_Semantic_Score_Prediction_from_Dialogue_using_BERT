          "original_form": "네 부대찌개요",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100034444.1.3",
          "form": "서울와서 볼일보느라",
          "original_form": "서울와서 볼일보느라",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100034444.1.4",
          "form": "사먹었어요",
          "original_form": "사먹었어요",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100034444.1.5",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100034444.1.6",
          "form": "떡볶이 먹었어요ㅋㅋ",
          "original_form": "떡볶이 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100034444.1.7",
          "form": "오 부대찌개 맛있죠~",
          "original_form": "오 부대찌개 맛있죠~",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100034444.1.8",
          "form": "앙 떡볶이 죻죠",
          "original_form": "앙 떡볶이 죻죠",
          "speaker_id": "2",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100034444.1.9",
          "form": "점심에 딱입니다",
          "original_form": "점심에 딱입니다",
          "speaker_id": "2",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100034444.1.10",
          "form": "어디서 드셨어요?",
          "original_form": "어디서 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100034444.1.11",
          "form": "시장에서 먹었어요",
          "original_form": "시장에서 먹었어요",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034444.1.12",
          "form": "옛날 그 맛 떡볶이에요",
          "original_form": "옛날 그 맛 떡볶이에요",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034444.1.13",
          "form": "오 시장",
          "original_form": "오 시장",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034444.1.14",
