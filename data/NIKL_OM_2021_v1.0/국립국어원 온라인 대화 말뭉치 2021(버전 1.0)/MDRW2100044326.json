        {
          "id": "MDRW2100044326.1.1",
          "form": "짬뽕 짜장 중 뭐가 좋으신가요?",
          "original_form": "짬뽕 짜장 중 뭐가 좋으신가요?",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044326.1.2",
          "form": "짜장요",
          "original_form": "짜장요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044326.1.3",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044326.1.4",
          "form": "그럼 사이다랑 콜라중에는요?",
          "original_form": "그럼 사이다랑 콜라중에는요?",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044326.1.5",
          "form": "콜라요",
          "original_form": "콜라요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044326.1.6",
          "form": "앗.. 전 사이다 파예요",
          "original_form": "앗.. 전 사이다 파예요",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044326.1.7",
          "form": "그럼 콜라랑 펩시중에서는요?",
          "original_form": "그럼 콜라랑 펩시중에서는요?",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044326.1.8",
          "form": "코카콜라요",
          "original_form": "코카콜라요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044326.1.9",
          "form": "전 콜라랑 펩시 맛의 차이를.잘 모르겠더라고요",
          "original_form": "전 콜라랑 펩시 맛의 차이를.잘 모르겠더라고요",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044326.1.10",
          "form": "톡쏘는맛?",
          "original_form": "톡쏘는맛?",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044326.1.11",
          "form": "콜라가 더 쏘나요?",
          "original_form": "콜라가 더 쏘나요?",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044326.1.12",
          "form": "그런것같아요",
          "original_form": "그런것같아요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044326.1.13",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044326.1.14",
          "form": "전 칠성이랑 스프라이트 중에는 칠성이거든요",
          "original_form": "전 칠성이랑 스프라이트 중에는 칠성이거든요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044326.1.15",
          "form": "저두칠성요",
