      "utterance": [
        {
          "id": "MDRW2100042615.1.1",
          "form": "핸드크림 바르세요??",
          "original_form": "핸드크림 바르세요??",
          "speaker_id": "1",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100042615.1.2",
          "form": "네 저는 발라요",
          "original_form": "네 저는 발라요",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042615.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042615.1.4",
          "form": "4계절내내?",
          "original_form": "4계절내내?",
          "speaker_id": "1",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042615.1.5",
          "form": "일년내내 ㅎㅎ",
          "original_form": "일년내내 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042615.1.6",
          "form": "오 대단하세요",
          "original_form": "오 대단하세요",
          "speaker_id": "1",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042615.1.7",
          "form": "안발라요 ??",
          "original_form": "안발라요 ??",
          "speaker_id": "2",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042615.1.8",
          "form": "바르려고 노력은 하는데",
          "original_form": "바르려고 노력은 하는데",
          "speaker_id": "1",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042615.1.9",
          "form": "습관이 안들더라고요",
          "original_form": "습관이 안들더라고요",
          "speaker_id": "1",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042615.1.10",
          "form": "아이고, 겨울에도 안발라요 ?",
          "original_form": "아이고, 겨울에도 안발라요 ?",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042615.1.11",
          "form": "겨울에는 진짜 바르려고 노력해요ㅠ",
          "original_form": "겨울에는 진짜 바르려고 노력해요ㅠ",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042615.1.12",
          "form": "겨울은 특히 더 발라야돼요",
          "original_form": "겨울은 특히 더 발라야돼요",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042615.1.13",
          "form": "일년내내 바르면 더 좋고",
          "original_form": "일년내내 바르면 더 좋고",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042615.1.14",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042615.1.15",
          "form": "아무래도",
          "original_form": "아무래도",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042615.1.16",
          "form": "바르는게좋은데ㅠ",
          "original_form": "바르는게좋은데ㅠ",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042615.1.17",
          "form": "습관이 잘 안들죠 ?",
          "original_form": "습관이 잘 안들죠 ?",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042615.1.18",
          "form": "네ㅠ 아무래도ㅋㅋ",
          "original_form": "네ㅠ 아무래도ㅋㅋ",
          "speaker_id": "1",
