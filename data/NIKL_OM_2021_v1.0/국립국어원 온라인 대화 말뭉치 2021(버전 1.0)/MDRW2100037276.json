      "utterance": [
        {
          "id": "MDRW2100037276.1.1",
          "form": "로맨스 쓸 줄은 안대?",
          "original_form": "로맨스 쓸 줄은 안대?",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037276.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.3",
          "form": "솔직히 여기서 뒤집으려면",
          "original_form": "솔직히 여기서 뒤집으려면",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.4",
          "form": "몰라... 그러니까 루트도 없이 냈겠지 ㅎ",
          "original_form": "몰라... 그러니까 루트도 없이 냈겠지 ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.5",
          "form": "사실 태운줄 알았는데",
          "original_form": "사실 태운줄 알았는데",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.6",
          "form": "혼을 본인이 보관하고있어서",
          "original_form": "혼을 본인이 보관하고있어서",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.7",
          "form": "살릴수있다던가",
          "original_form": "살릴수있다던가",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.8",
          "form": "약간 막판에",
          "original_form": "약간 막판에",
          "speaker_id": "1",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.9",
          "form": "그딴 맥락없는 전개밖에",
          "original_form": "그딴 맥락없는 전개밖에",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.10",
          "form": "없어보이고",
          "original_form": "없어보이고",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.11",
          "form": "주인공이 꿈같은거 꾸는대",
          "original_form": "주인공이 꿈같은거 꾸는대",
          "speaker_id": "1",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100037276.1.12",
          "form": "수연이가 나와서",
          "original_form": "수연이가 나와서",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.13",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.14",
          "form": "기억속에서 영원히 살아있는거야 어쩌고저쩌고",
          "original_form": "기억속에서 영원히 살아있는거야 어쩌고저쩌고",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.15",
          "form": "하는 대사를 던져서",
          "original_form": "하는 대사를 던져서",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.16",
          "form": "벌써",
          "original_form": "벌써",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.17",
          "form": "빌런용서떡밥",
          "original_form": "빌런용서떡밥",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.18",
          "form": "뿌리는것같은데",
          "original_form": "뿌리는것같은데",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.19",
          "form": "설화계 인물은 어차피 이야기꾼이 만드는거니까",
          "original_form": "설화계 인물은 어차피 이야기꾼이 만드는거니까",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.20",
          "form": "주인공이 수연이 이야기를 봤었고",
          "original_form": "주인공이 수연이 이야기를 봤었고",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.21",
          "form": "그걸 그대로 쓰면 되살아나는거 아니냐고",
          "original_form": "그걸 그대로 쓰면 되살아나는거 아니냐고",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.22",
          "form": "애초에 걘 왜 악신이고",
          "original_form": "애초에 걘 왜 악신이고",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.23",
          "form": "왜 악신이면서",
          "original_form": "왜 악신이면서",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100037276.1.24",
          "form": "행복회로 돌리고있던 ㅋㅋㅋ",
          "original_form": "행복회로 돌리고있던 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037276.1.25",
          "form": "주인공 안죽인거임?",
          "original_form": "주인공 안죽인거임?",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037276.1.26",
          "form": "내가보기엔",
          "original_form": "내가보기엔",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037276.1.27",
          "form": "주인공이 막",
          "original_form": "주인공이 막",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037276.1.28",
          "form": "악신으로 태어났다고 꼭 해를 끼치리란 법은 없잖아요",
          "original_form": "악신으로 태어났다고 꼭 해를 끼치리란 법은 없잖아요",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037276.1.29",
          "form": "해대던거에 좀 의외라고 느껴서?",
          "original_form": "해대던거에 좀 의외라고 느껴서?",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037276.1.30",
          "form": "그리고 주인공이 죽으면",
          "original_form": "그리고 주인공이 죽으면",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.31",
          "form": "설화를 아무도 못읽어서?",
          "original_form": "설화를 아무도 못읽어서?",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.32",
          "form": "아니 꽤",
          "original_form": "아니 꽤",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.33",
          "form": "오래 알고지냈잖아",
          "original_form": "오래 알고지냈잖아",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.34",
          "form": "아 수연이?",
          "original_form": "아 수연이?",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.35",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.36",
          "form": "수연1",
          "original_form": "수연1",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.37",
          "form": "이",
          "original_form": "이",
          "speaker_id": "2",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.38",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.39",
          "form": "저승아씨 설화가",
          "original_form": "저승아씨 설화가",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.40",
          "form": "용왕의 딸이 못되게 굴다가 죽은 사람의 혼을 돌보는 곳으로 쫒겨나 저승아씨라고 불리게 되었다",
          "original_form": "용왕의 딸이 못되게 굴다가 죽은 사람의 혼을 돌보는 곳으로 쫒겨나 저승아씨라고 불리게 되었다",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037276.1.41",
          "form": "이거여서",
          "original_form": "이거여서",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037276.1.42",
          "form": "근데 왜",
          "original_form": "근데 왜",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037276.1.43",
          "form": "설정값으로 정해진 악행은 이미 끝난 상태인듯",
          "original_form": "설정값으로 정해진 악행은 이미 끝난 상태인듯",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037276.1.44",
          "form": "악신이야??",
          "original_form": "악신이야??",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037276.1.45",
          "form": "일단 배역이 악역이라?",
          "original_form": "일단 배역이 악역이라?",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037276.1.46",
          "form": "배역이 악역이면 다 그렇게 뷰르나봄",
          "original_form": "배역이 악역이면 다 그렇게 뷰르나봄",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037276.1.47",
          "form": "아니 뭔",
          "original_form": "아니 뭔",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037276.1.48",
          "form": "남주가 2대2로 나눠지는데",
          "original_form": "남주가 2대2로 나눠지는데",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.49",
          "form": "유예는 사람 돕는 도깨비",
          "original_form": "유예는 사람 돕는 도깨비",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.50",
          "form": "백은 이무기",
          "original_form": "백은 이무기",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.51",
          "form": "그니까 딱히 나쁜짓 하진 않겠지만",
          "original_form": "그니까 딱히 나쁜짓 하진 않겠지만",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.52",
          "form": "일케는 설화에서 착한역할~중립값이고",
          "original_form": "일케는 설화에서 착한역할~중립값이고",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.53",
          "form": "활은 설화에서",
          "original_form": "활은 설화에서",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.54",
          "form": "분류상 악신이니까 죽여야된다 그거임?",
          "original_form": "분류상 악신이니까 죽여야된다 그거임?",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.55",
          "form": "위대한 신선의 못된 그림자임",
          "original_form": "위대한 신선의 못된 그림자임",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.56",
          "form": "그런애가 왜",
          "original_form": "그런애가 왜",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.57",
          "form": "달의 신 보좌야?",
          "original_form": "달의 신 보좌야?",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037276.1.58",
          "form": "서은한은 이름을 찾기 전까지는 악행을 하는 악우고",
          "original_form": "서은한은 이름을 찾기 전까지는 악행을 하는 악우고",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037276.1.59",
          "form": "나도 전대 신이 뭔 생각이었는지는 모르겠음",
          "original_form": "나도 전대 신이 뭔 생각이었는지는 모르겠음",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037276.1.60",
          "form": "근데 설화계에서는 일단",
          "original_form": "근데 설화계에서는 일단",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037276.1.61",
          "form": "아니 애초에 자격자 기준이 뭔데",
          "original_form": "아니 애초에 자격자 기준이 뭔데",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037276.1.62",
          "form": "그림자가 아니라 선인 본체인척 하면서",
          "original_form": "그림자가 아니라 선인 본체인척 하면서",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037276.1.63",
          "form": "숨기고있는듯",
          "original_form": "숨기고있는듯",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037276.1.64",
          "form": "그건 2에나 나오려나",
          "original_form": "그건 2에나 나오려나",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037276.1.65",
          "form": "진짜 이상하다",
          "original_form": "진짜 이상하다",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037276.1.66",
          "form": "솔직히 해결은",
          "original_form": "솔직히 해결은",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037276.1.67",
          "form": "내가 보기엔 전대 신도",
          "original_form": "내가 보기엔 전대 신도",
