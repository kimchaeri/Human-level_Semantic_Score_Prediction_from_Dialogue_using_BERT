      "utterance": [
        {
          "id": "MDRW2100024847.1.1",
          "form": "전 아직학생이에요ㅋㅋ",
          "original_form": "전 아직학생이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100024847.1.2",
          "form": "아직 구직활동도 해본적없답니다",
          "original_form": "아직 구직활동도 해본적없답니다",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100024847.1.3",
          "form": "오.. 어느 쪽으로 가겠다 진로는 고민중이신가요?",
          "original_form": "오.. 어느 쪽으로 가겠다 진로는 고민중이신가요?",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100024847.1.4",
          "form": "반도체소재요!",
          "original_form": "반도체소재요!",
          "speaker_id": "1",
          "time": "20210907 09:22"
        },
        {
          "id": "MDRW2100024847.1.5",
          "form": "대학원 갈 예정이랍니다!",
          "original_form": "대학원 갈 예정이랍니다!",
          "speaker_id": "1",
          "time": "20210907 09:22"
        },
        {
          "id": "MDRW2100024847.1.6",
          "form": "좀이따 교수님께 메일을 보낼거에요",
          "original_form": "좀이따 교수님께 메일을 보낼거에요",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100024847.1.7",
          "form": "오 반도체 진짜 멋있어요!!! 대학원가는 것두요",
          "original_form": "오 반도체 진짜 멋있어요!!! 대학원가는 것두요",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100024847.1.8",
          "form": "공부하는거 진짜 힘든데 대단해요",
          "original_form": "공부하는거 진짜 힘든데 대단해요",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100024847.1.9",
          "form": "ㅠㅠ 대학생이 죄를 지으면",
          "original_form": "ㅠㅠ 대학생이 죄를 지으면",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100024847.1.10",
          "form": "대학원에 갑니다...",
          "original_form": "대학원에 갑니다...",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100024847.1.11",
          "form": "앜ㅋㅋㅋㅋ왜요 대학원생들 부럽던데 전",
          "original_form": "앜ㅋㅋㅋㅋ왜요 대학원생들 부럽던데 전",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100024847.1.12",
          "form": "대학원생들...좋아보이나요....?",
          "original_form": "대학원생들...좋아보이나요....?",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100024847.1.13",
          "form": "ㅋㅋㅋ쿠ㅡㅠㅜㅜㅠ",
          "original_form": "ㅋㅋㅋ쿠ㅡㅠㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100024847.1.14",
          "form": "ㅋㅋㅋㅋㅋ그래두 대학원 졸업하면 취직하고나서 시작점이 많이 다르더라구요! 갈수 있는 환경과 여유와 이제..멘탈?ㅋㅋㅋ이 있으면 가는게 좋은거 같아요!",
          "original_form": "ㅋㅋㅋㅋㅋ그래두 대학원 졸업하면 취직하고나서 시작점이 많이 다르더라구요! 갈수 있는 환경과 여유와 이제..멘탈?ㅋㅋㅋ이 있으면 가는게 좋은거 같아요!",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100024847.1.15",
          "form": "오 많이 다른가요?",
          "original_form": "오 많이 다른가요?",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100024847.1.16",
          "form": "저는 이공계였는데 동기들 보면 대학원까지 마친 친구들은 초봉시작이 완전 다르더라구요 학사까지 하고 취업한 친구들은 연구직으로 못가기도하구",
          "original_form": "저는 이공계였는데 동기들 보면 대학원까지 마친 친구들은 초봉시작이 완전 다르더라구요 학사까지 하고 취업한 친구들은 연구직으로 못가기도하구",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100024847.1.17",
          "form": "근데 또 사람마다 많이 다르겠죠? ㅎㅎ",
          "original_form": "근데 또 사람마다 많이 다르겠죠? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100024847.1.18",
          "form": "맞아요 그래서 고민인것같아요",
          "original_form": "맞아요 그래서 고민인것같아요",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100024847.1.19",
          "form": "2년 알차게 보내면 더 좋은것같구 의미없이 지나가면",
          "original_form": "2년 알차게 보내면 더 좋은것같구 의미없이 지나가면",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100024847.1.20",
          "form": "오히려 아까울수도있고",
          "original_form": "오히려 아까울수도있고",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100024847.1.21",
          "form": "분명 잘 보내실겁니다 힘내세요!!",
          "original_form": "분명 잘 보내실겁니다 힘내세요!!",
          "speaker_id": "2",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100024847.1.22",
          "form": "저는 오래 공부할 자신이 없어서 관뒀는뎈ㅋㅋ 그래서 대단해보여요 멋지십니다",
          "original_form": "저는 오래 공부할 자신이 없어서 관뒀는뎈ㅋㅋ 그래서 대단해보여요 멋지십니다",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
