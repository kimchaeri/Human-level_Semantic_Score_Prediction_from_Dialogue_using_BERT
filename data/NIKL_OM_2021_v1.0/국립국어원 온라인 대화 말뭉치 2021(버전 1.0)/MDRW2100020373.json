      "utterance": [
        {
          "id": "MDRW2100020373.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020373.1.2",
          "form": "초등학교때 가장 인상이 깊었던 학원이잇나요",
          "original_form": "초등학교때 가장 인상이 깊었던 학원이잇나요",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020373.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020373.1.4",
          "form": "저는 있는데",
          "original_form": "저는 있는데",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020373.1.5",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020373.1.6",
          "form": "노트 빽빽이랑 진도나가서 주기적으로 달란트시장같은거 햇엇어여",
          "original_form": "노트 빽빽이랑 진도나가서 주기적으로 달란트시장같은거 햇엇어여",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020373.1.7",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020373.1.8",
          "form": "공부한만큼 스티커적립해서 달란트시장 화폐로 가능햇어요",
          "original_form": "공부한만큼 스티커적립해서 달란트시장 화폐로 가능햇어요",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020373.1.9",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020373.1.10",
          "form": "떡볶이 그립네요",
          "original_form": "떡볶이 그립네요",
          "speaker_id": "1",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020373.1.11",
          "form": "거기서먹던",
          "original_form": "거기서먹던",
          "speaker_id": "1",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020373.1.12",
          "form": "떡볶이 먹고 싶으면 분식집가면 돼요",
          "original_form": "떡볶이 먹고 싶으면 분식집가면 돼요",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020373.1.13",
          "form": "그런방법이",
          "original_form": "그런방법이",
          "speaker_id": "1",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020373.1.14",
          "form": "배달시키던가",
          "original_form": "배달시키던가",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020373.1.15",
          "form": "방석도 좋앗는데..ㅜ",
          "original_form": "방석도 좋앗는데..ㅜ",
