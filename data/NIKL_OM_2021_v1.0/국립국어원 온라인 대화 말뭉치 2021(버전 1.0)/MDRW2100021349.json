      "utterance": [
        {
          "id": "MDRW2100021349.1.1",
          "form": "먹는낙이 최고죠ㅋㅋㅋ",
          "original_form": "먹는낙이 최고죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:56"
        },
        {
          "id": "MDRW2100021349.1.2",
          "form": "고롬요 샌드위치나 하나 사먹을까봐요",
          "original_form": "고롬요 샌드위치나 하나 사먹을까봐요",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021349.1.3",
          "form": "샌드위치 맛있죠 ㅜㅜ",
          "original_form": "샌드위치 맛있죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021349.1.4",
          "form": "건강식아닌가요!",
          "original_form": "건강식아닌가요!",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021349.1.5",
          "form": "나름 건강식이져!? 서브웨이같은건 야채 듬뿍!!!",
          "original_form": "나름 건강식이져!? 서브웨이같은건 야채 듬뿍!!!",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021349.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021349.1.7",
          "form": "쿠키필수입니다!!ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "쿠키필수입니다!!ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021349.1.8",
          "form": "악마의 속삭임이네요...",
          "original_form": "악마의 속삭임이네요...",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021349.1.9",
          "form": "쿠키는 못참지...",
          "original_form": "쿠키는 못참지...",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021349.1.10",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021349.1.11",
          "form": "쿠키어느거 좋아하세요",
          "original_form": "쿠키어느거 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021349.1.12",
          "form": "마카다미아 들어간거욧..",
          "original_form": "마카다미아 들어간거욧..",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021349.1.13",
          "form": "근데 너무 달아요ㅜㅠㅜ커피필수",
          "original_form": "근데 너무 달아요ㅜㅠㅜ커피필수",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100021349.1.14",
          "form": "하 그거 너무맛있죠",
          "original_form": "하 그거 너무맛있죠",
          "speaker_id": "1",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021349.1.15",
          "form": "갑자기 먹고싶네요..",
          "original_form": "갑자기 먹고싶네요..",
          "speaker_id": "1",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100021349.1.16",
          "form": "서브웨이 고고...?",
          "original_form": "서브웨이 고고...?",
          "speaker_id": "2",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100021349.1.17",
          "form": "저도 악마의 속삭임이 들리네요..",
          "original_form": "저도 악마의 속삭임이 들리네요..",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100021349.1.18",
          "form": "서브웨이는 다이어트식이죠?!ㅋㅋㅋㅋㅋㅋ",
          "original_form": "서브웨이는 다이어트식이죠?!ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100021349.1.19",
          "form": "당연하죠ㅎㅎ절반이 야채니까요",
          "original_form": "당연하죠ㅎㅎ절반이 야채니까요",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100021349.1.20",
          "form": "야채=저칼로리",
          "original_form": "야채=저칼로리",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100021349.1.21",
          "form": "정답입니다!!",
          "original_form": "정답입니다!!",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021349.1.22",
