      "utterance": [
        {
          "id": "MDRW2100000334.1.1",
          "form": "나 캡슐머신샀어 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나 캡슐머신샀어 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 07:22"
        },
        {
          "id": "MDRW2100000334.1.2",
          "form": "좀 편하지",
          "original_form": "좀 편하지",
          "speaker_id": "1",
          "time": "20210819 07:23"
        },
        {
          "id": "MDRW2100000334.1.3",
          "form": "아침마다 한잔씩 내려가는데 너무 편해",
          "original_form": "아침마다 한잔씩 내려가는데 너무 편해",
          "speaker_id": "2",
          "time": "20210819 07:23"
        },
        {
          "id": "MDRW2100000334.1.4",
          "form": "나는 캡슐비 아끼려고 회사가서 내려마셔 ㅋㅋ",
          "original_form": "나는 캡슐비 아끼려고 회사가서 내려마셔 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 07:23"
        },
        {
          "id": "MDRW2100000334.1.5",
          "form": "우리도 사무실에서 드립해서 먹음  ㅋㅋ",
          "original_form": "우리도 사무실에서 드립해서 먹음  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 07:24"
        },
        {
          "id": "MDRW2100000334.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 07:24"
        },
        {
          "id": "MDRW2100000334.1.7",
          "form": "과장이",
          "original_form": "과장이",
          "speaker_id": "2",
          "time": "20210819 07:25"
        },
        {
          "id": "MDRW2100000334.1.8",
          "form": "폴바셋원두 좋아해서",
          "original_form": "폴바셋원두 좋아해서",
          "speaker_id": "2",
          "time": "20210819 07:25"
        },
        {
          "id": "MDRW2100000334.1.9",
          "form": "그걸로먹음  ㅋㅋㅋ",
          "original_form": "그걸로먹음  ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 07:25"
        },
        {
          "id": "MDRW2100000334.1.10",
          "form": "오 폴바셋",
          "original_form": "오 폴바셋",
          "speaker_id": "1",
          "time": "20210819 08:09"
        },
        {
          "id": "MDRW2100000334.1.11",
          "form": "나는 똥싸고 컴퓨터 앞에 앉아있다",
          "original_form": "나는 똥싸고 컴퓨터 앞에 앉아있다",
          "speaker_id": "1",
          "time": "20210819 08:10"
        },
        {
          "id": "MDRW2100000334.1.12",
          "form": "나는 이제 주차완료..들어가기실타...ㅋㅋㅋ",
          "original_form": "나는 이제 주차완료..들어가기실타...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 08:12"
        },
        {
          "id": "MDRW2100000334.1.13",
          "form": "힘을내",
