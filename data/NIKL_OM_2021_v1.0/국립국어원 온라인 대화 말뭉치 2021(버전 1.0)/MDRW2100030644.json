      "utterance": [
        {
          "id": "MDRW2100030644.1.1",
          "form": "아침 드셨나요",
          "original_form": "아침 드셨나요",
          "speaker_id": "2",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030644.1.2",
          "form": "먹었습니다",
          "original_form": "먹었습니다",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030644.1.3",
          "form": "감자샐러드 샌드위치 아시나요",
          "original_form": "감자샐러드 샌드위치 아시나요",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030644.1.4",
          "form": "그것은 무엇인가요",
          "original_form": "그것은 무엇인가요",
          "speaker_id": "2",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030644.1.5",
          "form": "어제 낮에 만들어 두었지요",
          "original_form": "어제 낮에 만들어 두었지요",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100030644.1.6",
          "form": "맛있나요",
          "original_form": "맛있나요",
          "speaker_id": "2",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030644.1.7",
          "form": "감자를 삶은 다음에 으깹니다",
          "original_form": "감자를 삶은 다음에 으깹니다",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030644.1.8",
          "form": "전 김밥을 먹고 있습니다",
          "original_form": "전 김밥을 먹고 있습니다",
          "speaker_id": "2",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030644.1.9",
          "form": "마요네즈랑 머스터드랑 각종 채소를 넣어서",
          "original_form": "마요네즈랑 머스터드랑 각종 채소를 넣어서",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030644.1.10",
          "form": "버무리 지요",
          "original_form": "버무리 지요",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030644.1.11",
          "form": "정말 맛있겠네요 저도 다음에 한번 주세요",
          "original_form": "정말 맛있겠네요 저도 다음에 한번 주세요",
          "speaker_id": "2",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030644.1.12",
          "form": "모닝 빵에다가 넣어 먹습니다",
          "original_form": "모닝 빵에다가 넣어 먹습니다",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100030644.1.13",
          "form": "만들기 귀찮습니다",
          "original_form": "만들기 귀찮습니다",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030644.1.14",
          "form": "모닝빵 정말 오랜만에 듣네요 맛있겠어요",
          "original_form": "모닝빵 정말 오랜만에 듣네요 맛있겠어요",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030644.1.15",
          "form": "사서 드리지요",
          "original_form": "사서 드리지요",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030644.1.16",
          "form": "김밥은 어디서 났나요",
          "original_form": "김밥은 어디서 났나요",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030644.1.17",
          "form": "그 정도면 귀찮지는 않지요",
          "original_form": "그 정도면 귀찮지는 않지요",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030644.1.18",
          "form": "그래도 안 할 거잖아요",
          "original_form": "그래도 안 할 거잖아요",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030644.1.19",
          "form": "만들 자신 있습니까",
          "original_form": "만들 자신 있습니까",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030644.1.20",
          "form": "맞아요 안 할 거예요 남이 해 준게 제일 맛있어요",
          "original_form": "맞아요 안 할 거예요 남이 해 준게 제일 맛있어요",
          "speaker_id": "2",
          "time": "20210904 10:59"
        }
      ]
