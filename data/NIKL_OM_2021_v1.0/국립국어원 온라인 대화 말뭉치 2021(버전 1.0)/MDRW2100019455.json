      "utterance": [
        {
          "id": "MDRW2100019455.1.1",
          "form": "재택근무 일자리를 구하고 있어",
          "original_form": "재택근무 일자리를 구하고 있어",
          "speaker_id": "1",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019455.1.2",
          "form": "그게 뭐야?",
          "original_form": "그게 뭐야?",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019455.1.3",
          "form": "말그대로 내 집에서 근무를 하는거야",
          "original_form": "말그대로 내 집에서 근무를 하는거야",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019455.1.4",
          "form": "출근 퇴근이 없는거야?",
          "original_form": "출근 퇴근이 없는거야?",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019455.1.5",
          "form": "그렇다고 할 수 있지",
          "original_form": "그렇다고 할 수 있지",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019455.1.6",
          "form": "그럼 시급 계산이 어렵지 않나?",
          "original_form": "그럼 시급 계산이 어렵지 않나?",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019455.1.7",
          "form": "그래서 보통 일의 양을 정해놔",
          "original_form": "그래서 보통 일의 양을 정해놔",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019455.1.8",
          "form": "아 일의 단위로 돈을 받는거구나",
          "original_form": "아 일의 단위로 돈을 받는거구나",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019455.1.9",
          "form": "맞아 어떻게 생각해?",
          "original_form": "맞아 어떻게 생각해?",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019455.1.10",
          "form": "편한 점이 많을 것같아",
          "original_form": "편한 점이 많을 것같아",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019455.1.11",
          "form": "어떤점이?",
          "original_form": "어떤점이?",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019455.1.12",
          "form": "난 출근때 화장하는게 귀찮아서",
          "original_form": "난 출근때 화장하는게 귀찮아서",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019455.1.13",
          "form": "혼자 집에서 일하는거 같은게 편하긴 해",
          "original_form": "혼자 집에서 일하는거 같은게 편하긴 해",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019455.1.14",
          "form": "맞아 방해 덜받고",
          "original_form": "맞아 방해 덜받고",
          "speaker_id": "2",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019455.1.15",
          "form": "맞아 인간관계 걱정도 덜하지",
          "original_form": "맞아 인간관계 걱정도 덜하지",
          "speaker_id": "1",
          "time": "20210827 19:36"
        }
