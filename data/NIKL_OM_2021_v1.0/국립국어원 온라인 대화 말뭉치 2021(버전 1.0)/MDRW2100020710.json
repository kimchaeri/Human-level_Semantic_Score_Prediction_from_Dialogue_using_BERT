      "utterance": [
        {
          "id": "MDRW2100020710.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020710.1.2",
          "form": "지금 국수나무 왓어요",
          "original_form": "지금 국수나무 왓어요",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020710.1.3",
          "form": "오 국수! 저는 냉면이요!",
          "original_form": "오 국수! 저는 냉면이요!",
          "speaker_id": "2",
          "time": "20210828 13:29"
        },
        {
          "id": "MDRW2100020710.1.4",
          "form": "저녁은 수육이에요",
          "original_form": "저녁은 수육이에요",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100020710.1.5",
          "form": "행복한 돼지임다....ㅎㅎ",
          "original_form": "행복한 돼지임다....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100020710.1.6",
          "form": "크으 주말은 맛있는거로 채워줘야죠!!",
          "original_form": "크으 주말은 맛있는거로 채워줘야죠!!",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100020710.1.7",
          "form": "저는 피자 먹었슴닷",
          "original_form": "저는 피자 먹었슴닷",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100020710.1.8",
          "form": "어디꺼드셧나용",
          "original_form": "어디꺼드셧나용",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100020710.1.9",
          "form": "시장 수제 피자요!!!",
          "original_form": "시장 수제 피자요!!!",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100020710.1.10",
          "form": "불고기였는데 불고기 좋아하시나용?",
          "original_form": "불고기였는데 불고기 좋아하시나용?",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100020710.1.11",
          "form": "없어서 못먹져ㅋㅋㅋ",
          "original_form": "없어서 못먹져ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100020710.1.12",
          "form": "저도용ㅋㅋ 고기는 먹어도 먹고싶어요ㅋㅋ",
          "original_form": "저도용ㅋㅋ 고기는 먹어도 먹고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100020710.1.13",
          "form": "시장 수제 피자 맛잇겟어요ㅜ",
          "original_form": "시장 수제 피자 맛잇겟어요ㅜ",
          "speaker_id": "1",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100020710.1.14",
          "form": "저는 치킨보다 피자파라서 ㅎㅎ",
          "original_form": "저는 치킨보다 피자파라서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100020710.1.15",
          "form": "어디 피자 좋아하세요?!",
          "original_form": "어디 피자 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100020710.1.16",
          "form": "진짜 다 좋아하는데",
          "original_form": "진짜 다 좋아하는데",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100020710.1.17",
          "form": "원픽은 도미노!",
          "original_form": "원픽은 도미노!",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100020710.1.18",
          "form": "도미노가 요즘 인기 많은거같아요",
          "original_form": "도미노가 요즘 인기 많은거같아요",
