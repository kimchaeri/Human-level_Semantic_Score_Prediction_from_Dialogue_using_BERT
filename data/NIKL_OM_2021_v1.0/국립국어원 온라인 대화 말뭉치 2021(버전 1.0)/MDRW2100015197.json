      "utterance": [
        {
          "id": "MDRW2100015197.1.1",
          "form": "name1님은 무슨음식 싫어하시나요",
          "original_form": "&name1&님은 무슨음식 싫어하시나요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015197.1.2",
          "form": "저는 곱창 닭발 못먹어요",
          "original_form": "저는 곱창 닭발 못먹어요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015197.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015197.1.4",
          "form": "오 그러시군요",
          "original_form": "오 그러시군요",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015197.1.5",
          "form": "저는 배부를때 못먹어요.",
          "original_form": "저는 배부를때 못먹어요.",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015197.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015197.1.7",
          "form": "못드시는게 하나도 없으신가봐요",
          "original_form": "못드시는게 하나도 없으신가봐요",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015197.1.8",
          "form": "수정과 싫어해요 ㅋㅋㅋ 계피냄새때문에",
          "original_form": "수정과 싫어해요 ㅋㅋㅋ 계피냄새때문에",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015197.1.9",
          "form": "오우 전 계피향 좋아하는데",
          "original_form": "오우 전 계피향 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015197.1.10",
          "form": "민초는요?",
          "original_form": "민초는요?",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015197.1.11",
          "form": "흠...즐겁진 않아요",
          "original_form": "흠...즐겁진 않아요",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015197.1.12",
          "form": "요새 많이 나오더래요",
          "original_form": "요새 많이 나오더래요",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015197.1.13",
          "form": "아이쿠",
          "original_form": "아이쿠",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015197.1.14",
          "form": "치약맛을 느끼시나봐요",
          "original_form": "치약맛을 느끼시나봐요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015197.1.15",
          "form": "저는 민초사랑단이라",
          "original_form": "저는 민초사랑단이라",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015197.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015197.1.17",
          "form": "저는 배고프면 먹습니다",
          "original_form": "저는 배고프면 먹습니다",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015197.1.18",
          "form": "그냥 반반이시군요",
          "original_form": "그냥 반반이시군요",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015197.1.19",
          "form": "베라 무슨맛드세야",
          "original_form": "베라 무슨맛드세야",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015197.1.20",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015197.1.21",
          "form": "저는 엄마는외계인같은거요",
          "original_form": "저는 엄마는외계인같은거요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015197.1.22",
          "form": "만월회라는 음료파는 사이트아세요?",
          "original_form": "만월회라는 음료파는 사이트아세요?",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015197.1.23",
          "form": "아니요 무슨 중국느낌 나는데요",
          "original_form": "아니요 무슨 중국느낌 나는데요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015197.1.24",
          "form": "어떤음료 파는곳인가요",
          "original_form": "어떤음료 파는곳인가요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015197.1.25",
          "form": "음료 원액파는 사이튼데요 여기 민초가 기가막힙니다",
          "original_form": "음료 원액파는 사이튼데요 여기 민초가 기가막힙니다",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015197.1.26",
          "form": "오우 궁금한데요 어느정돈지",
          "original_form": "오우 궁금한데요 어느정돈지",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015197.1.27",
          "form": "울나라기업이구 온라인판매해요",
          "original_form": "울나라기업이구 온라인판매해요",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015197.1.28",
          "form": "오프라인 카페가 있음 가볼텐데",
          "original_form": "오프라인 카페가 있음 가볼텐데",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015197.1.29",
          "form": "아하..라온카페라는 한군데 납품하는데 좀 멀죠",
          "original_form": "아하..라온카페라는 한군데 납품하는데 좀 멀죠",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015197.1.30",
          "form": "근데 헤메이는 민초단들이 여기 정착할정도로 유명하답니다",
          "original_form": "근데 헤메이는 민초단들이 여기 정착할정도로 유명하답니다",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015197.1.31",
          "form": "되게 잘 아시네요ㅋㅋ",
          "original_form": "되게 잘 아시네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015197.1.32",
          "form": "한번 시도해볼게요",
          "original_form": "한번 시도해볼게요",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015197.1.33",
          "form": "저는 흑임자랑 말차 시켜먹었어요",
          "original_form": "저는 흑임자랑 말차 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015197.1.34",
          "form": "저녁은 가족과 드시나요?",
          "original_form": "저녁은 가족과 드시나요?",
          "speaker_id": "2",
          "time": "20210826 18:00"
        }
      ]
