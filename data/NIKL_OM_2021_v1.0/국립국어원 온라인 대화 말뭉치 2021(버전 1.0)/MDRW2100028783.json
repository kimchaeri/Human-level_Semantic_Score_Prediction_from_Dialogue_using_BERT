      "utterance": [
        {
          "id": "MDRW2100028783.1.1",
          "form": "엄마가 해준 밥이 제일 맛있어요",
          "original_form": "엄마가 해준 밥이 제일 맛있어요",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028783.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100028783.1.3",
          "form": "아침 뭐드세요",
          "original_form": "아침 뭐드세요",
          "speaker_id": "2",
          "time": "20210903 08:44"
        },
        {
          "id": "MDRW2100028783.1.4",
          "form": "저는 간단하게 간장계란밥 먹었어요!",
          "original_form": "저는 간단하게 간장계란밥 먹었어요!",
          "speaker_id": "1",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100028783.1.5",
          "form": "점심 드셧어요?",
          "original_form": "점심 드셧어요?",
          "speaker_id": "1",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100028783.1.6",
          "form": "네네 감자탕 시켜먹었어요",
          "original_form": "네네 감자탕 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100028783.1.7",
          "form": "우아 배달 시켜 드셧나요?",
          "original_form": "우아 배달 시켜 드셧나요?",
          "speaker_id": "1",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100028783.1.8",
          "form": "네네 배달먹었어요",
          "original_form": "네네 배달먹었어요",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100028783.1.9",
          "form": "뭐드셨나요",
          "original_form": "뭐드셨나요",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100028783.1.10",
          "form": "저는 오늘 한정식 다녀왓어여",
          "original_form": "저는 오늘 한정식 다녀왓어여",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100028783.1.11",
          "form": "진짜 비싸드라구요 ㅠ",
          "original_form": "진짜 비싸드라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100028783.1.12",
          "form": "와 한정식 ㄷ잡채도나와ㅛ나요ㅠㅠ",
          "original_form": "와 한정식 ㄷ잡채도나와ㅛ나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100028783.1.13",
          "form": "네네!! 잡채랑 회랑 튀김이랑 고기 ㅜㅜ",
          "original_form": "네네!! 잡채랑 회랑 튀김이랑 고기 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100028783.1.14",
          "form": "양이 너무 적어요 근데 ㅋㅋ",
          "original_form": "양이 너무 적어요 근데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100028783.1.15",
          "form": "오오 그쵸 조금씩 나오죠?ㅜㅜ",
          "original_form": "오오 그쵸 조금씩 나오죠?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100028783.1.16",
          "form": "네네 ㅜㅜ 고기가 엄청 적어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 ㅜㅜ 고기가 엄청 적어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100028783.1.17",
          "form": "밥이랑 된장찌개 나와서 그걸로 배채웟어요 ㅋㅋ",
          "original_form": "밥이랑 된장찌개 나와서 그걸로 배채웟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100028783.1.18",
          "form": "맞아요 다들 전채요리마냔 작고",
          "original_form": "맞아요 다들 전채요리마냔 작고",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100028783.1.19",
          "form": "식사로 배채우기...",
          "original_form": "식사로 배채우기...",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100028783.1.20",
          "form": "맞아여 ㅋㅋㅋㅋㅋㅋㅋ 또가고 싶네요 ㅜㅜ",
          "original_form": "맞아여 ㅋㅋㅋㅋㅋㅋㅋ 또가고 싶네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100028783.1.21",
          "form": "입에서 녹드라구요 ㅠㅠ",
          "original_form": "입에서 녹드라구요 ㅠㅠ",
