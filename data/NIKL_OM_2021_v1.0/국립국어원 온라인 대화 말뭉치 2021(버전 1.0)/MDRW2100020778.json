      "utterance": [
        {
          "id": "MDRW2100020778.1.1",
          "form": "오 name1님 안녕하세요?",
          "original_form": "오 &name1&님 안녕하세요?",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020778.1.2",
          "form": "저는 미역국으로 아점 먹었어요",
          "original_form": "저는 미역국으로 아점 먹었어요",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020778.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020778.1.4",
          "form": "아 저는요 어제 밤에 만들어둔 티라미스를 먹었어요",
          "original_form": "아 저는요 어제 밤에 만들어둔 티라미스를 먹었어요",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020778.1.5",
          "form": "오 티라미스 좋아하세요?",
          "original_form": "오 티라미스 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020778.1.6",
          "form": "레이디핑거빵이 없어서 호밀식빵에 인스턴트 커피와 설탕으로 민든 시럽을적셔서 깔았더니",
          "original_form": "레이디핑거빵이 없어서 호밀식빵에 인스턴트 커피와 설탕으로 민든 시럽을적셔서 깔았더니",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020778.1.7",
          "form": "곡물이 씹히는 티라미스가 . ....",
          "original_form": "곡물이 씹히는 티라미스가 . ....",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020778.1.8",
          "form": "좋아해요ㅋㅋㅋ",
          "original_form": "좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020778.1.9",
          "form": "ㅋㅋㅋ쫄깃하겠는걸요",
          "original_form": "ㅋㅋㅋ쫄깃하겠는걸요",
          "speaker_id": "2",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020778.1.10",
          "form": "모닝커피는 하셨나요?",
          "original_form": "모닝커피는 하셨나요?",
          "speaker_id": "2",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020778.1.11",
          "form": "티라미스에 인스턴트 커피가 3티스푼 들어가서 그게 커피겸 아침이에요",
          "original_form": "티라미스에 인스턴트 커피가 3티스푼 들어가서 그게 커피겸 아침이에요",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020778.1.12",
          "form": "마스카포네 치즈를 사서 ㅋㅋ 반은 케이크만들고 반은 티라미스로 먹었어요",
          "original_form": "마스카포네 치즈를 사서 ㅋㅋ 반은 케이크만들고 반은 티라미스로 먹었어요",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020778.1.13",
          "form": "ㅎㅎ그렇군요 베(이킹)잘알 이신데요?",
          "original_form": "ㅎㅎ그렇군요 베(이킹)잘알 이신데요?",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020778.1.14",
          "form": "케잌은 치즈케잌하셨어요?",
          "original_form": "케잌은 치즈케잌하셨어요?",
          "speaker_id": "2",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020778.1.15",
          "form": "네! 베이킹은 잘못하는데",
          "original_form": "네! 베이킹은 잘못하는데",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020778.1.16",
          "form": "마스카포네가 유크림이 많은 부드러운 치즈라서 냉장고안에서도 여름이면 일주일만에 곰팡이가 자란적도 있어서",
          "original_form": "마스카포네가 유크림이 많은 부드러운 치즈라서 냉장고안에서도 여름이면 일주일만에 곰팡이가 자란적도 있어서",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020778.1.17",
          "form": "시도하는게 중요한거죠!",
          "original_form": "시도하는게 중요한거죠!",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020778.1.18",
          "form": "빨리쓰셔야겧네요",
          "original_form": "빨리쓰셔야겧네요",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020778.1.19",
          "form": "달력에 메모해두고 얼른 먹었어요ㅋㅋㅋ",
          "original_form": "달력에 메모해두고 얼른 먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020778.1.20",
          "form": "ㅋㅋㅋㅋ그러시구나",
          "original_form": "ㅋㅋㅋㅋ그러시구나",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020778.1.21",
          "form": "맛있겠어요!",
          "original_form": "맛있겠어요!",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020778.1.22",
          "form": "크림치즈 좋아하세요?",
          "original_form": "크림치즈 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020778.1.23",
          "form": "맛있었어요! 치즈맛이 80퍼더라구요 ㅋㅋ",
          "original_form": "맛있었어요! 치즈맛이 80퍼더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020778.1.24",
          "form": "저는 빵에 크림처럼 들어있는거 말고 치즈케잌들어간거 좋아해요",
          "original_form": "저는 빵에 크림처럼 들어있는거 말고 치즈케잌들어간거 좋아해요",
          "speaker_id": "2",
          "time": "20210828 12:32"
        }
      ]
    }
