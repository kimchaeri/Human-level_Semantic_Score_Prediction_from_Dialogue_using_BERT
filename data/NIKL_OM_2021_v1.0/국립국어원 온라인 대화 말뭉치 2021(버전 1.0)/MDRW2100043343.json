        {
          "id": "MDRW2100043343.1.1",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043343.1.2",
          "form": "그게 우리 아빠에요ㅋㅋㅋㅋ",
          "original_form": "그게 우리 아빠에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043343.1.3",
          "form": "보통 그런 사람들 받는 게 백만원이 넘더라",
          "original_form": "보통 그런 사람들 받는 게 백만원이 넘더라",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043343.1.4",
          "form": "나라에서 주는 돈만 받아도",
          "original_form": "나라에서 주는 돈만 받아도",
          "speaker_id": "1",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043343.1.5",
          "form": "요즘에는",
          "original_form": "요즘에는",
          "speaker_id": "1",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043343.1.6",
          "form": "자식들 소득도 안보고",
          "original_form": "자식들 소득도 안보고",
          "speaker_id": "1",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043343.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ우리집도 있잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ우리집도 있잖아",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043343.1.8",
          "form": "옛날에는 자식들 있고 소득 있으면 안 해줬잖아요",
          "original_form": "옛날에는 자식들 있고 소득 있으면 안 해줬잖아요",
          "speaker_id": "1",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043343.1.9",
          "form": "천년나무 메이트들 ㅋㅋㅋㅋ",
          "original_form": "천년나무 메이트들 ㅋㅋㅋㅋ",
