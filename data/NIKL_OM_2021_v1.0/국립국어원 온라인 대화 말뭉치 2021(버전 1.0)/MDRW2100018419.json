      "utterance": [
        {
          "id": "MDRW2100018419.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018419.1.2",
          "form": "전 서울살아요 혹시 어디 사세요?",
          "original_form": "전 서울살아요 혹시 어디 사세요?",
          "speaker_id": "1",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018419.1.3",
          "form": "떡볶이 이야기 하니까 갑자기 야식이 먹고싶어지네요",
          "original_form": "떡볶이 이야기 하니까 갑자기 야식이 먹고싶어지네요",
          "speaker_id": "1",
          "time": "20210827 02:06"
        },
        {
          "id": "MDRW2100018419.1.4",
          "form": "너무 배고파졌어요 ㅎ",
          "original_form": "너무 배고파졌어요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 02:06"
        },
        {
          "id": "MDRW2100018419.1.5",
          "form": "그러게요 야식 맛있겠다",
          "original_form": "그러게요 야식 맛있겠다",
          "speaker_id": "2",
          "time": "20210827 02:07"
        },
        {
          "id": "MDRW2100018419.1.6",
          "form": "그렇지만 저는 혼자 살아서 음식 남으면 곤란하더라구요",
          "original_form": "그렇지만 저는 혼자 살아서 음식 남으면 곤란하더라구요",
          "speaker_id": "2",
          "time": "20210827 02:07"
        },
        {
          "id": "MDRW2100018419.1.7",
          "form": "아 자취하시나봐요?",
          "original_form": "아 자취하시나봐요?",
          "speaker_id": "1",
          "time": "20210827 02:07"
        },
        {
          "id": "MDRW2100018419.1.8",
          "form": "네 기숙사에 살아요",
          "original_form": "네 기숙사에 살아요",
          "speaker_id": "2",
          "time": "20210827 02:08"
        },
        {
          "id": "MDRW2100018419.1.9",
          "form": "룸메이트도 없답니다...ㅠ",
          "original_form": "룸메이트도 없답니다...ㅠ",
          "speaker_id": "2",
          "time": "20210827 02:08"
        },
        {
          "id": "MDRW2100018419.1.10",
          "form": "아이고 그럼 외로우시겠네요",
          "original_form": "아이고 그럼 외로우시겠네요",
          "speaker_id": "1",
          "time": "20210827 02:08"
        },
        {
          "id": "MDRW2100018419.1.11",
          "form": "그럼 집안일은 혼자 다하시겠어요",
          "original_form": "그럼 집안일은 혼자 다하시겠어요",
          "speaker_id": "1",
          "time": "20210827 02:08"
        },
        {
          "id": "MDRW2100018419.1.12",
          "form": "네 맞아요 혼자라 집안일 할게 많이 없긴 한데",
          "original_form": "네 맞아요 혼자라 집안일 할게 많이 없긴 한데",
          "speaker_id": "2",
          "time": "20210827 02:09"
        },
        {
          "id": "MDRW2100018419.1.13",
          "form": "대신 제가 다 혼자해야 하니까...",
          "original_form": "대신 제가 다 혼자해야 하니까...",
          "speaker_id": "2",
          "time": "20210827 02:09"
        },
        {
          "id": "MDRW2100018419.1.14",
          "form": "고생이네요ㅠㅠ",
          "original_form": "고생이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:09"
        },
        {
          "id": "MDRW2100018419.1.15",
          "form": "그렇지만 가족들이랑 같이 살아도 집안일은 제가 거의 다 하는거 같아요",
          "original_form": "그렇지만 가족들이랑 같이 살아도 집안일은 제가 거의 다 하는거 같아요",
          "speaker_id": "1",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018419.1.16",
          "form": "아...ㅠㅠ 맞아요 보통 어머니들이 하시니까",
          "original_form": "아...ㅠㅠ 맞아요 보통 어머니들이 하시니까",
          "speaker_id": "2",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018419.1.17",
          "form": "그래도 남편이랑 딸이 설거지나 빨래 해줘서 고맙더라고요",
          "original_form": "그래도 남편이랑 딸이 설거지나 빨래 해줘서 고맙더라고요",
          "speaker_id": "1",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018419.1.18",
          "form": "우와 효녀 두셨네여 ㅎㅎ",
          "original_form": "우와 효녀 두셨네여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018419.1.19",
          "form": "아 빨래 저도 내일 해야하는데 귀찮네요",
          "original_form": "아 빨래 저도 내일 해야하는데 귀찮네요",
          "speaker_id": "2",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018419.1.20",
          "form": "빨래 너무 귀찮아요",
          "original_form": "빨래 너무 귀찮아요",
          "speaker_id": "2",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018419.1.21",
          "form": "맞아요 빨래 귀찮아요 정말!!",
          "original_form": "맞아요 빨래 귀찮아요 정말!!",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018419.1.22",
          "form": "세탁물 세탁기에 넣어놓고 다시 꺼내고",
          "original_form": "세탁물 세탁기에 넣어놓고 다시 꺼내고",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018419.1.23",
          "form": "무겁잖아요 또",
          "original_form": "무겁잖아요 또",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018419.1.24",
          "form": "공감되네요 정말ㅠㅠㅠ 그거 다시 너는 것도 힘들어요",
          "original_form": "공감되네요 정말ㅠㅠㅠ 그거 다시 너는 것도 힘들어요",
          "speaker_id": "2",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018419.1.25",
          "form": "그래서 빨래 건조기 사고 싶더라니까요ㅋㅋㅋㅋ",
          "original_form": "그래서 빨래 건조기 사고 싶더라니까요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018419.1.26",
          "form": "건조기 좋죠 전 식기세척기 탐나요",
          "original_form": "건조기 좋죠 전 식기세척기 탐나요",
          "speaker_id": "1",
          "time": "20210827 02:13"
        },
        {
          "id": "MDRW2100018419.1.27",
          "form": "집안일은 너무 귀찮아서ㅋㅋ",
          "original_form": "집안일은 너무 귀찮아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018419.1.28",
          "form": "식기세척기도 좋죠~",
          "original_form": "식기세척기도 좋죠~",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018419.1.29",
          "form": "맞아요 집안일은 넘 귀찮습니다 ㅎㅎ",
          "original_form": "맞아요 집안일은 넘 귀찮습니다 ㅎㅎ",
          "speaker_id": "2",
