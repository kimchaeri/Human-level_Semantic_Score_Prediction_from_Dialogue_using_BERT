      "utterance": [
        {
          "id": "MDRW2100006983.1.1",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006983.1.2",
          "form": "오오 요새 장바구니에 여름옷과 겨울옷이 담겼어요ㅋㅋ",
          "original_form": "오오 요새 장바구니에 여름옷과 겨울옷이 담겼어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006983.1.3",
          "form": "한심ㅋㅋㅋㅋ",
          "original_form": "한심ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006983.1.4",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006983.1.5",
          "form": "결정장애도 있고ㅋㅋㅋㅋ",
          "original_form": "결정장애도 있고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006983.1.6",
          "form": "요즘에는 역시즘이라고 해서 여름에 겨울옷 싸게 팔잖아요^^",
          "original_form": "요즘에는 역시즘이라고 해서 여름에 겨울옷 싸게 팔잖아요^^",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006983.1.7",
          "form": "돈두 없구ㅋㅋㅋㅋ",
          "original_form": "돈두 없구ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006983.1.8",
          "form": "역시즌..",
          "original_form": "역시즌..",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006983.1.9",
          "form": "악.ㅋㅋㅋㅋㅋ 저는 장바구니는 거의 이용하지 않는듯요",
          "original_form": "악.ㅋㅋㅋㅋㅋ 저는 장바구니는 거의 이용하지 않는듯요",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006983.1.10",
          "form": "겨울 옷 짧은 거 작년부터 유행이라길래 함 사보려구요ㅋㅋ",
          "original_form": "겨울 옷 짧은 거 작년부터 유행이라길래 함 사보려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006983.1.11",
          "form": "아 전 장바구니에 담아놓고 결정을 미뤄요ㅋㅋㅋㅋ",
          "original_form": "아 전 장바구니에 담아놓고 결정을 미뤄요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006983.1.12",
          "form": "잘 하는 선택일까 아닐까 내일의 내가 결정 다시 하자 뭐 이런ㅋㅋ",
          "original_form": "잘 하는 선택일까 아닐까 내일의 내가 결정 다시 하자 뭐 이런ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006983.1.13",
          "form": "그렇더라고요??",
          "original_form": "그렇더라고요??",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006983.1.14",
          "form": "롱패딩이 작년부터 잘 안보임.ㅎㅎ",
          "original_form": "롱패딩이 작년부터 잘 안보임.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006983.1.15",
          "form": "좀 덜 추워져서 그런건지ㅋㅋㅋ",
          "original_form": "좀 덜 추워져서 그런건지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006983.1.16",
          "form": "맨날 롱만 입어서 작년에 머쓱했어요ㅋㅋㅋ",
          "original_form": "맨날 롱만 입어서 작년에 머쓱했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006983.1.17",
          "form": "저는 진짜 추위 많이타서..",
          "original_form": "저는 진짜 추위 많이타서..",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100006983.1.18",
          "form": "올해도 롱 입을 꺼 같아서..ㅋㅋㅋ",
          "original_form": "올해도 롱 입을 꺼 같아서..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100006983.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 겨울엔 롱이 정답이긴 하죠!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 겨울엔 롱이 정답이긴 하죠!",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100006983.1.20",
          "form": "아 혹시 당근 해본적 있으세요?",
          "original_form": "아 혹시 당근 해본적 있으세요?",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100006983.1.21",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100006983.1.22",
          "form": "당근은 나의 삶...",
          "original_form": "당근은 나의 삶...",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100006983.1.23",
          "form": "아기 키우니까 당근 진짜 자주해요",
          "original_form": "아기 키우니까 당근 진짜 자주해요",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100006983.1.24",
          "form": "오오 제가 안해본걸 다 해보셨군요ㅋㅋㅋ",
          "original_form": "오오 제가 안해본걸 다 해보셨군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100006983.1.25",
          "form": "당근하세여?",
          "original_form": "당근하세여?",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100006983.1.26",
          "form": "안하시는구나..",
          "original_form": "안하시는구나..",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006983.1.27",
          "form": "ㅋㅋㅋㅋ 함도 안해봤어요",
          "original_form": "ㅋㅋㅋㅋ 함도 안해봤어요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006983.1.28",
          "form": "아이용품이 사용기간이 진짜 짧아서",
          "original_form": "아이용품이 사용기간이 진짜 짧아서",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006983.1.29",
          "form": "오오 좋을 거 같애요",
          "original_form": "오오 좋을 거 같애요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006983.1.30",
          "form": "물건이 새거에요.새거..ㅋㅋㅋㅋㅋ",
          "original_form": "물건이 새거에요.새거..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006983.1.31",
          "form": "ㅋㅋㅋㅋㅋ 마자요 애들은 쑥쑥 커서 옷도 그렇고",
          "original_form": "ㅋㅋㅋㅋㅋ 마자요 애들은 쑥쑥 커서 옷도 그렇고",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006983.1.32",
          "form": "특히 첫째만 쓰고 나오는 물건은 안쓰고 그냥 나오는 물건도 많아요..ㅋㅋㅋㅋ",
          "original_form": "특히 첫째만 쓰고 나오는 물건은 안쓰고 그냥 나오는 물건도 많아요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006983.1.33",
          "form": "엄마가 있는줄 몰라서 안쓴것들.ㅋㅋㅋㅋ",
          "original_form": "엄마가 있는줄 몰라서 안쓴것들.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100006983.1.34",
          "form": "헐 아깝겠다ㅋㅋㅋㅋ",
          "original_form": "헐 아깝겠다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100006983.1.35",
          "form": "그거려고 당근이 있는 거겠지만ㅋㅋㅋㅋ",
          "original_form": "그거려고 당근이 있는 거겠지만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100006983.1.36",
          "form": "그쵸.ㅋㅋㅋㅋ",
          "original_form": "그쵸.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100006983.1.37",
          "form": "당근하면 재밋는 사람들 많아요",
          "original_form": "당근하면 재밋는 사람들 많아요",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100006983.1.38",
          "form": "팔고싶은 게 있어서 함 해볼까 하는데 좀 겔러서 미루고 있어요 계속ㅋㅋ",
          "original_form": "팔고싶은 게 있어서 함 해볼까 하는데 좀 겔러서 미루고 있어요 계속ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100006983.1.39",
          "form": "후기는 마니 봤어요ㅋㅋ",
          "original_form": "후기는 마니 봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100006983.1.40",
          "form": "부인몰래 겜샀다가 눈물을 머금고 파는사람.ㅋㅌ",
          "original_form": "부인몰래 겜샀다가 눈물을 머금고 파는사람.ㅋㅌ",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100006983.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋ 맴찢",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맴찢",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100006983.1.42",
          "form": "선입금 해줄테니 드림이라고 말해달라믄 사람..",
          "original_form": "선입금 해줄테니 드림이라고 말해달라믄 사람..",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100006983.1.43",
          "form": "웃겨요 진짜.ㅋㅋㅋ",
          "original_form": "웃겨요 진짜.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100006983.1.44",
          "form": "헐 중고나라 후기같은 상황인가보다ㅋㅋㅋㅋ",
          "original_form": "헐 중고나라 후기같은 상황인가보다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100006983.1.45",
          "form": "그런 거도 잼날 거 같아요ㅋㅋㅋ 흔쾌히 해줄듯ㅋㅋㅋ",
          "original_form": "그런 거도 잼날 거 같아요ㅋㅋㅋ 흔쾌히 해줄듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100006983.1.46",
          "form": "네네 부인이 의심의 눈초리로 감시함. 진짜 같이 오셔요 ㅋㅋㅋㅋ",
          "original_form": "네네 부인이 의심의 눈초리로 감시함. 진짜 같이 오셔요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100006983.1.47",
          "form": "허얼 그렇게까지ㅋㅋㅋㅋ 너무하네",
          "original_form": "허얼 그렇게까지ㅋㅋㅋㅋ 너무하네",
          "speaker_id": "2",
          "time": "20210822 00:38"
        }
      ]
