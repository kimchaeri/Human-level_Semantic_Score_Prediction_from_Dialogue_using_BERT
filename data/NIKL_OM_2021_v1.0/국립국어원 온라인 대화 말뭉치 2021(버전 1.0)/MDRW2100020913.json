      "utterance": [
        {
          "id": "MDRW2100020913.1.1",
          "form": "이번주 습하더니 오늘은. 맑아요",
          "original_form": "이번주 습하더니 오늘은. 맑아요",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020913.1.2",
          "form": "거긴 비안오나요?",
          "original_form": "거긴 비안오나요?",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020913.1.3",
          "form": "저는 출발할땐 맑고 덥더니",
          "original_form": "저는 출발할땐 맑고 덥더니",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020913.1.4",
          "form": "세종 도착하니 흐리고",
          "original_form": "세종 도착하니 흐리고",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020913.1.5",
          "form": "빗방울이 한두방울 떨어지네요",
          "original_form": "빗방울이 한두방울 떨어지네요",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020913.1.6",
          "form": "오늘 날씨 맑고 비올것  같지 않아요",
          "original_form": "오늘 날씨 맑고 비올것  같지 않아요",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020913.1.7",
          "form": "아침에비해 살짝 덥네요",
          "original_form": "아침에비해 살짝 덥네요",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020913.1.8",
          "form": "기상청이 안맞는건지 ㅋ",
          "original_form": "기상청이 안맞는건지 ㅋ",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020913.1.9",
          "form": "잘 안맞죠 ㅋ 지난주 지지난주 외출할때 우산 필수",
          "original_form": "잘 안맞죠 ㅋ 지난주 지지난주 외출할때 우산 필수",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020913.1.10",
          "form": "스콜처럼 왔다가서 ㅜㅠ",
          "original_form": "스콜처럼 왔다가서 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020913.1.11",
          "form": "황당했어요",
          "original_form": "황당했어요",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020913.1.12",
          "form": "동남아 날씨랑 비슷해지는것 같아요",
          "original_form": "동남아 날씨랑 비슷해지는것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020913.1.13",
          "form": "봄 가을 짧고 여름덥고..장마처럼 비오기 보다 스콜같이 ㅇ비왔다 그치고",
          "original_form": "봄 가을 짧고 여름덥고..장마처럼 비오기 보다 스콜같이 ㅇ비왔다 그치고",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020913.1.14",
          "form": "맞아요 동남아 딱 그날씨예요",
          "original_form": "맞아요 동남아 딱 그날씨예요",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100020913.1.15",
          "form": "미세먼지 없어서 좋네요",
          "original_form": "미세먼지 없어서 좋네요",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100020913.1.16",
          "form": "맞아요 요새는 황사 미세먼지 이야기가 없네요",
          "original_form": "맞아요 요새는 황사 미세먼지 이야기가 없네요",
          "speaker_id": "1",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100020913.1.17",
          "form": "더우면 더워서 못나가고\r\n안더우면 미세먼지땜에 못나가고\r\n아니면 이젠 추워서 못나가네요 ㅎㅎ",
          "original_form": "더우면 더워서 못나가고\r\n안더우면 미세먼지땜에 못나가고\r\n아니면 이젠 추워서 못나가네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100020913.1.18",
          "form": "이번주 비오면서 날씨가 갑자기 추워졌어요.",
          "original_form": "이번주 비오면서 날씨가 갑자기 추워졌어요.",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100020913.1.19",
          "form": "글쵸 반팔이 이제 스산한기분이 들어요",
          "original_form": "글쵸 반팔이 이제 스산한기분이 들어요",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100020913.1.20",
          "form": "커피도 따뜻한걸 찾게되구요",
          "original_form": "커피도 따뜻한걸 찾게되구요",
