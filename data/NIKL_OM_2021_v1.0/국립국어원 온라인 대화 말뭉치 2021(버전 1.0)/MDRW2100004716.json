      "utterance": [
        {
          "id": "MDRW2100004716.1.1",
          "form": "오늘은 비가 너무오네요ㅜㅜ",
          "original_form": "오늘은 비가 너무오네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004716.1.2",
          "form": "여긴 비오다 그친거같아요",
          "original_form": "여긴 비오다 그친거같아요",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004716.1.3",
          "form": "비많이오나요?",
          "original_form": "비많이오나요?",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004716.1.4",
          "form": "대부분그치나봐요 저희동네는 아직도 비오는중이에요",
          "original_form": "대부분그치나봐요 저희동네는 아직도 비오는중이에요",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004716.1.5",
          "form": "태풍이 온다는 얘기가 있어요",
          "original_form": "태풍이 온다는 얘기가 있어요",
          "speaker_id": "2",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004716.1.6",
          "form": "이런.... 태풍 피해 없어야할텐데 말이죠",
          "original_form": "이런.... 태풍 피해 없어야할텐데 말이죠",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004716.1.7",
          "form": "그냥 조금 오는줄알았는데 일기예보 다시 봐야겠어요",
          "original_form": "그냥 조금 오는줄알았는데 일기예보 다시 봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004716.1.8",
          "form": "예전에 제생일날태풍와서 너무 슬펐었어요ㅠㅠ",
          "original_form": "예전에 제생일날태풍와서 너무 슬펐었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004716.1.9",
          "form": "저런ㅠㅠ 올해는 태풍없이 조용히 지나가나 했더니",
          "original_form": "저런ㅠㅠ 올해는 태풍없이 조용히 지나가나 했더니",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004716.1.10",
          "form": "여름에 생일이신가봐요",
          "original_form": "여름에 생일이신가봐요",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004716.1.11",
          "form": "네 9월이에요 올해는 날씨좋기를ㅋㅋ",
          "original_form": "네 9월이에요 올해는 날씨좋기를ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004716.1.12",
          "form": "9월이면 가을날씨니 올해는 화창할거예요ㅎ",
          "original_form": "9월이면 가을날씨니 올해는 화창할거예요ㅎ",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004716.1.13",
          "form": "높고푸른 하늘 ~~!",
          "original_form": "높고푸른 하늘 ~~!",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004716.1.14",
          "form": "가을하늘이 참좋죠ㅎㅎ 좀만있으면 겨울올거같아요ㅎㅎ",
          "original_form": "가을하늘이 참좋죠ㅎㅎ 좀만있으면 겨울올거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004716.1.15",
          "form": "트렌치코트 몇번입음 패딩꺼내야돼요ㅎㅎ",
          "original_form": "트렌치코트 몇번입음 패딩꺼내야돼요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004716.1.16",
          "form": "그쵸 왜이리아쉬운지ㅋㅋ 옷정리도 곧 해야겠어요",
          "original_form": "그쵸 왜이리아쉬운지ㅋㅋ 옷정리도 곧 해야겠어요",
