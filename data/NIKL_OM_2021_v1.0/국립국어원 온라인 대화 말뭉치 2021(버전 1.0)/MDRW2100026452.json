      "utterance": [
        {
          "id": "MDRW2100026452.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그러니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그러니",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026452.1.2",
          "form": "그럼 다른 질문 할게",
          "original_form": "그럼 다른 질문 할게",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026452.1.3",
          "form": "근데 이미 이정도면 정답을 알고있는거 아니냐",
          "original_form": "근데 이미 이정도면 정답을 알고있는거 아니냐",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026452.1.4",
          "form": "야채 들어가?",
          "original_form": "야채 들어가?",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026452.1.5",
          "form": "아냐 난 몰라",
          "original_form": "아냐 난 몰라",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026452.1.6",
          "form": "그렇구만 야채많이들어가",
          "original_form": "그렇구만 야채많이들어가",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026452.1.7",
          "form": "면 요리야?",
          "original_form": "면 요리야?",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026452.1.8",
          "form": "아니 면은 안들어가",
          "original_form": "아니 면은 안들어가",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026452.1.9",
          "form": "으음 뭘까",
          "original_form": "으음 뭘까",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026452.1.10",
          "form": "매워?",
          "original_form": "매워?",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026452.1.11",
          "form": "내가 많이 좋아하는 음식중 하나지",
          "original_form": "내가 많이 좋아하는 음식중 하나지",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026452.1.12",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026452.1.13",
          "form": "정답! 인도카레!!!",
          "original_form": "정답! 인도카레!!!",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026452.1.14",
          "form": "진짜 날 잘아는구먼유",
          "original_form": "진짜 날 잘아는구먼유",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026452.1.15",
          "form": "ㅋㅋㅋㅋㅋ암요",
          "original_form": "ㅋㅋㅋㅋㅋ암요",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026452.1.16",
          "form": "거의 마음 속에 들어갔다 나왔제",
          "original_form": "거의 마음 속에 들어갔다 나왔제",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026452.1.17",
          "form": "인도카레의 어느 점이 좋니",
          "original_form": "인도카레의 어느 점이 좋니",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026452.1.18",
          "form": "그 특유의 풍미가 너무좋아",
          "original_form": "그 특유의 풍미가 너무좋아",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026452.1.19",
          "form": "나 조상중에 인도인이 있는거 아닐까",
          "original_form": "나 조상중에 인도인이 있는거 아닐까",
