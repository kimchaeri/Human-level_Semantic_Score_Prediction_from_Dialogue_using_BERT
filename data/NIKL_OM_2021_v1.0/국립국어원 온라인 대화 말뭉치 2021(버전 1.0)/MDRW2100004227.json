      "utterance": [
        {
          "id": "MDRW2100004227.1.1",
          "form": "안녕하새요~",
          "original_form": "안녕하새요~",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004227.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004227.1.3",
          "form": "책읽는거 좋아하세요?",
          "original_form": "책읽는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004227.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004227.1.5",
          "form": "무슨 장르 좋아하세요",
          "original_form": "무슨 장르 좋아하세요",
          "speaker_id": "1",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004227.1.6",
          "form": "추리나 로맨스요",
          "original_form": "추리나 로맨스요",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004227.1.7",
          "form": "오 제일 재밌게 본거 추천해주실 수 있나요?",
          "original_form": "오 제일 재밌게 본거 추천해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004227.1.8",
          "form": "저는 책 장르 안가리고 보거든요",
          "original_form": "저는 책 장르 안가리고 보거든요",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004227.1.9",
          "form": "소설이면 다 좋아해서",
          "original_form": "소설이면 다 좋아해서",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004227.1.10",
          "form": "음..루팡에 딸이랑 돌이킬수없는 약속 이요",
          "original_form": "음..루팡에 딸이랑 돌이킬수없는 약속 이요",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004227.1.11",
          "form": "둘다 재밌어보이는 제목이네요",
          "original_form": "둘다 재밌어보이는 제목이네요",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004227.1.12",
          "form": "돌이킬수없는 약속은 로멘스예요?",
          "original_form": "돌이킬수없는 약속은 로멘스예요?",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004227.1.13",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004227.1.14",
          "form": "아..로맨스도 있긴한데",
          "original_form": "아..로맨스도 있긴한데",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004227.1.15",
          "form": "추리 위주?",
          "original_form": "추리 위주?",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004227.1.16",
          "form": "할머니가 남자에게 15년뒤 감옥에서 나오는 한 남자를 죽여달라는? 이야기에요 감옥에 가있는 남자가 할머니 딸을 죽여가지고",
          "original_form": "할머니가 남자에게 15년뒤 감옥에서 나오는 한 남자를 죽여달라는? 이야기에요 감옥에 가있는 남자가 할머니 딸을 죽여가지고",
          "speaker_id": "2",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004227.1.17",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004227.1.18",
          "form": "엄청 흥미로운 내용이네요",
          "original_form": "엄청 흥미로운 내용이네요",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004227.1.19",
          "form": "재밌을거같아요",
          "original_form": "재밌을거같아요",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004227.1.20",
          "form": "추천해줘서 고마워요 꼭 읽어볼게요",
          "original_form": "추천해줘서 고마워요 꼭 읽어볼게요",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004227.1.21",
          "form": "루팡에 딸도 재밌어요ㅎ",
          "original_form": "루팡에 딸도 재밌어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.22",
          "form": "그래보여요",
          "original_form": "그래보여요",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.23",
          "form": "둘 다 제목이",
          "original_form": "둘 다 제목이",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.24",
          "form": "현재 2권까지 나와서ㅋㅋ",
          "original_form": "현재 2권까지 나와서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.25",
          "form": "읽고싶게 만드는",
          "original_form": "읽고싶게 만드는",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.26",
          "form": "아 시리즈작이에요?",
          "original_form": "아 시리즈작이에요?",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.27",
          "form": "아아 기다리는 거 힘들겠다",
          "original_form": "아아 기다리는 거 힘들겠다",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.28",
          "form": "아직 2권은 사기전이여서ㅋㅋ",
          "original_form": "아직 2권은 사기전이여서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.29",
          "form": "아마 2권으로 끝날거 같아요",
          "original_form": "아마 2권으로 끝날거 같아요",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004227.1.30",
          "form": "1권이 루팡에 딸이고 2권은 루팡에 딸이 홈즈에 딸을 만났다 에요",
          "original_form": "1권이 루팡에 딸이고 2권은 루팡에 딸이 홈즈에 딸을 만났다 에요",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004227.1.31",
          "form": "아 다행이네요",
          "original_form": "아 다행이네요",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004227.1.32",
          "form": "저는 기다리는 걸 못하겠더라고요",
          "original_form": "저는 기다리는 걸 못하겠더라고요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004227.1.33",
          "form": "저는 츠지무라 미즈키 작가님 작품들을",
          "original_form": "저는 츠지무라 미즈키 작가님 작품들을",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004227.1.34",
          "form": "요즘 재밌게 읽고있는데",
          "original_form": "요즘 재밌게 읽고있는데",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004227.1.35",
          "form": "특히 얼음고래가 좋았어요",
          "original_form": "특히 얼음고래가 좋았어요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004227.1.36",
          "form": "일본 문학 괜찮으시면 읽어보시는 것도 추천해요 ㅎㅎ",
          "original_form": "일본 문학 괜찮으시면 읽어보시는 것도 추천해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004227.1.37",
          "form": "추천 감사해요ㅎ",
          "original_form": "추천 감사해요ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:05"
        }
      ]
