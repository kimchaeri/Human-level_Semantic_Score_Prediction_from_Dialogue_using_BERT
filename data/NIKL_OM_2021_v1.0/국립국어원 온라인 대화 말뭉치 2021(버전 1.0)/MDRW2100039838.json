      "utterance": [
        {
          "id": "MDRW2100039838.1.1",
          "form": "그래요?저 진짜 유치원생보다 영어 못할텐데요",
          "original_form": "그래요?저 진짜 유치원생보다 영어 못할텐데요",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039838.1.2",
          "form": "항상 여행가면 ㅋㅋ 말한마디 못해본",
          "original_form": "항상 여행가면 ㅋㅋ 말한마디 못해본",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039838.1.3",
          "form": "한번 시간 내서 도전해볼게요!",
          "original_form": "한번 시간 내서 도전해볼게요!",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039838.1.4",
          "form": "ㅋㅋㅋ어짜피 한국에서 영어 좀 한다고 해봤자 미국가면 발음이나 속도때문에",
          "original_form": "ㅋㅋㅋ어짜피 한국에서 영어 좀 한다고 해봤자 미국가면 발음이나 속도때문에",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039838.1.5",
          "form": "다 비슷하게 눈치로 대화하게 되는것 같아요",
          "original_form": "다 비슷하게 눈치로 대화하게 되는것 같아요",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039838.1.6",
          "form": "그리구 이제 스마트폰도있으니까요!!ㅋㅋㅋㅋ",
          "original_form": "그리구 이제 스마트폰도있으니까요!!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039838.1.7",
          "form": "아하 그렇긴하겠네요",
          "original_form": "아하 그렇긴하겠네요",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100039838.1.8",
          "form": "막 보면 인종차별 이런거땜에 괜히 걱정도 했다죠 ㅋㅋ",
          "original_form": "막 보면 인종차별 이런거땜에 괜히 걱정도 했다죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039838.1.9",
          "form": "아 맞아요 근데 코로나 이후에는 괜히 동양인 차별할까봐 걱정도 돼요 ㅠ 중국때매",
          "original_form": "아 맞아요 근데 코로나 이후에는 괜히 동양인 차별할까봐 걱정도 돼요 ㅠ 중국때매",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039838.1.10",
          "form": "더 심해졌다잖아여ㅠ",
          "original_form": "더 심해졌다잖아여ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039838.1.11",
          "form": "조롱하는것도 그렇고",
          "original_form": "조롱하는것도 그렇고",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039838.1.12",
          "form": "아 중국우한폐렴. 진짜...",
          "original_form": "아 중국우한폐렴. 진짜...",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039838.1.13",
          "form": "글구 뭔가 유럽도 다시 가고싶었었는데",
          "original_form": "글구 뭔가 유럽도 다시 가고싶었었는데",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039838.1.14",
          "form": "코로나때 자유어쩌구 하면서 마스크 안쓰고 시위하고",
          "original_form": "코로나때 자유어쩌구 하면서 마스크 안쓰고 시위하고",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039838.1.15",
          "form": "이런것들 보니까.. 그 사람들 무식한거 티나서 예전만큼",
          "original_form": "이런것들 보니까.. 그 사람들 무식한거 티나서 예전만큼",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039838.1.16",
          "form": "다시 가고싶은 맘도 크진않아졌어요 ㅠ ㅠ",
          "original_form": "다시 가고싶은 맘도 크진않아졌어요 ㅠ ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039838.1.17",
          "form": "아 그렇네요 맞아요 ㅎ 그거 진짜 무식했엉노",
          "original_form": "아 그렇네요 맞아요 ㅎ 그거 진짜 무식했엉노",
          "speaker_id": "2",
          "time": "20210904 09:08"
        },
        {
          "id": "MDRW2100039838.1.18",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210904 09:08"
        },
        {
          "id": "MDRW2100039838.1.19",
          "form": "그 생각하니 갑자기 가기싫네요",
          "original_form": "그 생각하니 갑자기 가기싫네요",
          "speaker_id": "2",
          "time": "20210904 09:08"
        },
        {
          "id": "MDRW2100039838.1.20",
          "form": "국내도 가보고싶은곳 너무 많아요",
          "original_form": "국내도 가보고싶은곳 너무 많아요",
          "speaker_id": "2",
          "time": "20210904 09:08"
        },
        {
          "id": "MDRW2100039838.1.21",
          "form": "맞아요 그쵸 ㅋㅋ국내도 예쁜곳이 많는 것 같아요 ㅋㅋ",
          "original_form": "맞아요 그쵸 ㅋㅋ국내도 예쁜곳이 많는 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 09:29"
        },
        {
          "id": "MDRW2100039838.1.22",
          "form": "어디 가보고 싶으세요?",
          "original_form": "어디 가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210904 09:29"
        },
        {
          "id": "MDRW2100039838.1.23",
          "form": "저~ 경주요",
          "original_form": "저~ 경주요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100039838.1.24",
          "form": "경주 한번도 안가봤어요",
          "original_form": "경주 한번도 안가봤어요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100039838.1.25",
          "form": "오오 ㅋㅋ 저도 어렸을적에 기차여행? 내일로? 할때 딱한벙 가봐서",
          "original_form": "오오 ㅋㅋ 저도 어렸을적에 기차여행? 내일로? 할때 딱한벙 가봐서",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039838.1.26",
          "form": "또가고싶어요",
          "original_form": "또가고싶어요",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039838.1.27",
          "form": "좋다하더라고요 ㅎ 너무 멀어서 못가봤어요",
          "original_form": "좋다하더라고요 ㅎ 너무 멀어서 못가봤어요",
          "speaker_id": "2",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100039838.1.28",
          "form": "맞아요 경주는 정말 마음 먹고 가야되죠ㅠ",
          "original_form": "맞아요 경주는 정말 마음 먹고 가야되죠ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100039838.1.29",
          "form": "네ㅠ그리고 울릉도도 가보고싶어용",
          "original_form": "네ㅠ그리고 울릉도도 가보고싶어용",
          "speaker_id": "2",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100039838.1.30",
          "form": "울릉도도 가고싶어요!",
          "original_form": "울릉도도 가고싶어요!",
          "speaker_id": "2",
          "time": "20210904 16:16"
        }
      ]
    }
