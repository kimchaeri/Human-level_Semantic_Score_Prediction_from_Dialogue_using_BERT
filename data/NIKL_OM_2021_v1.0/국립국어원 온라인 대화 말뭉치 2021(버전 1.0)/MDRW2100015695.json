      "utterance": [
        {
          "id": "MDRW2100015695.1.1",
          "form": "진짜 신기하더라",
          "original_form": "진짜 신기하더라",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015695.1.2",
          "form": "근데 못버리는 것들은 어떻게햐?",
          "original_form": "근데 못버리는 것들은 어떻게햐?",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015695.1.3",
          "form": "너무비싸다.....",
          "original_form": "너무비싸다.....",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015695.1.4",
          "form": "못버리는것들은 그냥 원래버리던데로 버리거나 일반쓰레기..",
          "original_form": "못버리는것들은 그냥 원래버리던데로 버리거나 일반쓰레기..",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015695.1.5",
          "form": "어머님 플렉스하셧넼ㅋㅋ",
          "original_form": "어머님 플렉스하셧넼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015695.1.6",
          "form": "어지간한건 다 된다고는하는데 좀 작게 잘라서 버려야되",
          "original_form": "어지간한건 다 된다고는하는데 좀 작게 잘라서 버려야되",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015695.1.7",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015695.1.8",
          "form": "나도갖고싶은데 호기심으로는 쳐다도 못보겠다",
          "original_form": "나도갖고싶은데 호기심으로는 쳐다도 못보겠다",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015695.1.9",
          "form": "놀러가면 보여쥬ㅓ",
          "original_form": "놀러가면 보여쥬ㅓ",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015695.1.10",
          "form": "미생물 먹을거 들고갈겡 간식",
          "original_form": "미생물 먹을거 들고갈겡 간식",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015695.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015695.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진짜 웃긴게 그 미생물 먹이 안주면 잘안됨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진짜 웃긴게 그 미생물 먹이 안주면 잘안됨",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015695.1.13",
          "form": "근데 생활방식에 따라서 분쇄형이 맞을수도 있을거같아",
          "original_form": "근데 생활방식에 따라서 분쇄형이 맞을수도 있을거같아",
          "speaker_id": "1",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015695.1.14",
          "form": "탄수화물을 좋아한다며?!",
          "original_form": "탄수화물을 좋아한다며?!",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015695.1.15",
          "form": "허겁지겁한다던데...",
          "original_form": "허겁지겁한다던데...",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015695.1.16",
          "form": "응응 ㅋㅋ밥이앙 면 이런거는 바로 흙잘됨",
          "original_form": "응응 ㅋㅋ밥이앙 면 이런거는 바로 흙잘됨",
          "speaker_id": "1",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015695.1.17",
          "form": "그러면 그 흙이 많아지면 거름쓰고?",
          "original_form": "그러면 그 흙이 많아지면 거름쓰고?",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015695.1.18",
          "form": "신기하네 와우",
          "original_form": "신기하네 와우",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015695.1.19",
          "form": "어머님은 어떻게 이런 정보를 아심",
          "original_form": "어머님은 어떻게 이런 정보를 아심",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015695.1.20",
          "form": "엄마가 산게아니라아빠가삼 ㅋㅋㅋ아빠가 자꾸 막 홈쇼핑이런거봄ㅋㅋ",
          "original_form": "엄마가 산게아니라아빠가삼 ㅋㅋㅋ아빠가 자꾸 막 홈쇼핑이런거봄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015695.1.21",
          "form": "와 어울려 아버님 환경친화적",
          "original_form": "와 어울려 아버님 환경친화적",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015695.1.22",
          "form": "드라이기 사고싶어",
          "original_form": "드라이기 사고싶어",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015695.1.23",
          "form": "넌 어떤거써?",
          "original_form": "넌 어떤거써?",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015695.1.24",
          "form": "난 그냥오래된겈ㅋㅋㅋ 브랜드뭔지모르겠음",
          "original_form": "난 그냥오래된겈ㅋㅋㅋ 브랜드뭔지모르겠음",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015695.1.25",
          "form": "우리집꺼 옛날에 산건데 바람도 약하고 귀찮",
          "original_form": "우리집꺼 옛날에 산건데 바람도 약하고 귀찮",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015695.1.26",
          "form": "머리 잘 안말리긴 하는데 바람쎈거 하나 사고싶어",
          "original_form": "머리 잘 안말리긴 하는데 바람쎈거 하나 사고싶어",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015695.1.27",
          "form": "브랜드 막 지워져서모르겠는데 그냥 마트에 흔히파는거였음",
          "original_form": "브랜드 막 지워져서모르겠는데 그냥 마트에 흔히파는거였음",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015695.1.28",
          "form": "미용실드라이기같은거ㅋㅋㅋㅋ",
          "original_form": "미용실드라이기같은거ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015695.1.29",
          "form": "그거좋다던데 jmw?",
          "original_form": "그거좋다던데 jmw?",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015695.1.30",
          "form": "아 쿠팡에서 봣다",
          "original_form": "아 쿠팡에서 봣다",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015695.1.31",
          "form": "니가 사라는거 말고는 난 죄다 쿠팡이네ㅋㅋㅋ",
          "original_form": "니가 사라는거 말고는 난 죄다 쿠팡이네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015695.1.32",
          "form": "그거 고모집네서 써봤는데 바람소리가 크긴큰데 엄청 좋은지는모르겠어",
          "original_form": "그거 고모집네서 써봤는데 바람소리가 크긴큰데 엄청 좋은지는모르겠어",
          "speaker_id": "1",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015695.1.33",
          "form": "헐ㅋㅋㅋ",
          "original_form": "헐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
