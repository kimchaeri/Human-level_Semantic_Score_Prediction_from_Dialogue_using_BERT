      "utterance": [
        {
          "id": "MDRW2100021842.1.1",
          "form": "로션 골라야된다",
          "original_form": "로션 골라야된다",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021842.1.2",
          "form": "내가 지난번에 골랐었는데 왜 놓쳐가지고",
          "original_form": "내가 지난번에 골랐었는데 왜 놓쳐가지고",
          "speaker_id": "1",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021842.1.3",
          "form": "그게 그런줄 몰랐지 할인 언제 다시 하냐",
          "original_form": "그게 그런줄 몰랐지 할인 언제 다시 하냐",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100021842.1.4",
          "form": "몰라ㅜ 그정도로 크게 할인 잘 안하는데",
          "original_form": "몰라ㅜ 그정도로 크게 할인 잘 안하는데",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021842.1.5",
          "form": "좀 찾아봐봐 그때 샀어야 되는데 엄청 비싸더라",
          "original_form": "좀 찾아봐봐 그때 샀어야 되는데 엄청 비싸더라",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021842.1.6",
          "form": "맞아 원래 비싸지 그때 할인 진짜 많이 한거야",
          "original_form": "맞아 원래 비싸지 그때 할인 진짜 많이 한거야",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021842.1.7",
          "form": "그런거 같다 이제 진짜 사야되는데 찾아봐",
          "original_form": "그런거 같다 이제 진짜 사야되는데 찾아봐",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021842.1.8",
          "form": "알아씀 괜찮은거 찾아보께",
          "original_form": "알아씀 괜찮은거 찾아보께",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021842.1.9",
          "form": "로션으로 골라?",
          "original_form": "로션으로 골라?",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021842.1.10",
          "form": "그럼 머 있는데",
          "original_form": "그럼 머 있는데",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021842.1.11",
          "form": "크림도 있징 상관없지? 로션이나 크림이나",
          "original_form": "크림도 있징 상관없지? 로션이나 크림이나",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021842.1.12",
          "form": "ㅇㅇ아무거나 촉촉하고 안끈적이는 거로 해",
          "original_form": "ㅇㅇ아무거나 촉촉하고 안끈적이는 거로 해",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021842.1.13",
          "form": "아라쏘 다시 찾아보께",
          "original_form": "아라쏘 다시 찾아보께",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100021842.1.14",
          "form": "몇개 골라봐 그중에 고르자",
          "original_form": "몇개 골라봐 그중에 고르자",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021842.1.15",
          "form": "보고에서 과자 산거 발송했대",
          "original_form": "보고에서 과자 산거 발송했대",
