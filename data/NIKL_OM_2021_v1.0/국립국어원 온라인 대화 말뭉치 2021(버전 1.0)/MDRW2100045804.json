      "utterance": [
        {
          "id": "MDRW2100045804.1.1",
          "form": "나 얼마전에 접종했잖아",
          "original_form": "나 얼마전에 접종했잖아",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045804.1.2",
          "form": "오 백신맞았어?",
          "original_form": "오 백신맞았어?",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045804.1.3",
          "form": "응응 2자 맞았어 너는?",
          "original_form": "응응 2자 맞았어 너는?",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045804.1.4",
          "form": "나는 2차 맞은지 꽤 오래됐어",
          "original_form": "나는 2차 맞은지 꽤 오래됐어",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045804.1.5",
          "form": "아 이미 맞았구나",
          "original_form": "아 이미 맞았구나",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045804.1.6",
          "form": "응응 코로나 안걸리게 더 관리 잘해야지",
          "original_form": "응응 코로나 안걸리게 더 관리 잘해야지",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045804.1.7",
          "form": "건강 잘 챙겨야지 진짜ㅜㅜ",
          "original_form": "건강 잘 챙겨야지 진짜ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045804.1.8",
          "form": "코로나 무섭다 너무 조심해",
          "original_form": "코로나 무섭다 너무 조심해",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045804.1.9",
          "form": "응응 면역력도 키워야지",
          "original_form": "응응 면역력도 키워야지",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045804.1.10",
          "form": "그래도 우리 둘다 2차까지  접종해서 다행이다",
          "original_form": "그래도 우리 둘다 2차까지  접종해서 다행이다",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045804.1.11",
          "form": "맞아 걸릴 확룰이 좀 줄지않을까",
          "original_form": "맞아 걸릴 확룰이 좀 줄지않을까",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045804.1.12",
          "form": "코로나 안걸리게 마스크 잘쓰고",
          "original_form": "코로나 안걸리게 마스크 잘쓰고",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100045804.1.13",
          "form": "응응 손도 잘 씻어라",
          "original_form": "응응 손도 잘 씻어라",
