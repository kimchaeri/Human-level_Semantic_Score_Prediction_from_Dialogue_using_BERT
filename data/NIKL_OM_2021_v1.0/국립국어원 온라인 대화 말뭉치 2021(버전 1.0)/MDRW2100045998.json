      "utterance": [
        {
          "id": "MDRW2100045998.1.1",
          "form": "저는 남이 해준거는 잘먹어요 ㅎ",
          "original_form": "저는 남이 해준거는 잘먹어요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045998.1.2",
          "form": "채소많이 나와도 좋고",
          "original_form": "채소많이 나와도 좋고",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045998.1.3",
          "form": "남자직원들은 고기없는 날은",
          "original_form": "남자직원들은 고기없는 날은",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045998.1.4",
          "form": "다이어트식단이라고 투덜 ㅎ",
          "original_form": "다이어트식단이라고 투덜 ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045998.1.5",
          "form": "저는 속도편하고 좋더라구요",
          "original_form": "저는 속도편하고 좋더라구요",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045998.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ 다이어트",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 다이어트",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045998.1.7",
          "form": "오 저도 구내식당이ㅛ었음 좋겟어요 ㅠㅠ",
          "original_form": "오 저도 구내식당이ㅛ었음 좋겟어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045998.1.8",
          "form": "점심메뉴 걱정 안하구",
          "original_form": "점심메뉴 걱정 안하구",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045998.1.9",
          "form": "그게 정말 좋아요",
          "original_form": "그게 정말 좋아요",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045998.1.10",
          "form": "오늘은 안동찜닭 이라네요",
          "original_form": "오늘은 안동찜닭 이라네요",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045998.1.11",
          "form": "우아 찜닭 너무 좋아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우아 찜닭 너무 좋아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100045998.1.12",
          "form": "식단표가 있나요??",
          "original_form": "식단표가 있나요??",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100045998.1.13",
          "form": "방금 팝업이 오네요",
          "original_form": "방금 팝업이 오네요",
          "speaker_id": "1",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100045998.1.14",
          "form": "찜닭이라고 먹을꺼냐고 ㅋ",
          "original_form": "찜닭이라고 먹을꺼냐고 ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100045998.1.15",
          "form": "오오 ㅋㅋㅋ 찜닭 좋아하세요?",
          "original_form": "오오 ㅋㅋㅋ 찜닭 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100045998.1.16",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100045998.1.17",
          "form": "치킨은 사랑이죠",
          "original_form": "치킨은 사랑이죠",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100045998.1.18",
          "form": "그 찜닭에 당면..? 같은게 너무 좋아요 ㅋㅋ",
          "original_form": "그 찜닭에 당면..? 같은게 너무 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100045998.1.19",
          "form": "그것두 별미죠",
          "original_form": "그것두 별미죠",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100045998.1.20",
          "form": "기대되는데요",
          "original_form": "기대되는데요",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100045998.1.21",
          "form": "구내식당에 당면도 넣어줄지",
          "original_form": "구내식당에 당면도 넣어줄지",
          "speaker_id": "1",
          "time": "20210908 10:41"
        },
        {
          "id": "MDRW2100045998.1.22",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋ 구내식당 가면 셀프로 푸시나요?",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋ 구내식당 가면 셀프로 푸시나요?",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100045998.1.23",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100045998.1.24",
          "form": "셀프로 퍼요",
          "original_form": "셀프로 퍼요",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100045998.1.25",
          "form": "우아 너무 좋네요!!!",
          "original_form": "우아 너무 좋네요!!!",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100045998.1.26",
          "form": "사람 많나요??",
          "original_form": "사람 많나요??",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100045998.1.27",
          "form": "요즘 코로나로 10분단위로",
          "original_form": "요즘 코로나로 10분단위로",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100045998.1.28",
          "form": "부서별로 나눠서가요",
          "original_form": "부서별로 나눠서가요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100045998.1.29",
          "form": "아아 그럼 안전 하겟내요 ㅠㅜㅜ",
          "original_form": "아아 그럼 안전 하겟내요 ㅠㅜㅜ",
