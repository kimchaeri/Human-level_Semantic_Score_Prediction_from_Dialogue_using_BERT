      "utterance": [
        {
          "id": "MDRW2100007733.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007733.1.2",
          "form": "안녕하세요 반려동물 키우세요?",
          "original_form": "안녕하세요 반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007733.1.3",
          "form": "네 저희집은 강아지 키워요",
          "original_form": "네 저희집은 강아지 키워요",
          "speaker_id": "1",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007733.1.4",
          "form": "작은 개예요?",
          "original_form": "작은 개예요?",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007733.1.5",
          "form": "네 시츄에요",
          "original_form": "네 시츄에요",
          "speaker_id": "1",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100007733.1.6",
          "form": "귀엽겠네요",
          "original_form": "귀엽겠네요",
          "speaker_id": "2",
          "time": "20210822 10:13"
        },
        {
          "id": "MDRW2100007733.1.7",
          "form": "요즘 산책 얼마나 하세요?",
          "original_form": "요즘 산책 얼마나 하세요?",
          "speaker_id": "2",
          "time": "20210822 10:13"
        },
        {
          "id": "MDRW2100007733.1.8",
          "form": "이틀에 한번은 하는거같아요",
          "original_form": "이틀에 한번은 하는거같아요",
          "speaker_id": "1",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007733.1.9",
          "form": "혹시 반려동물 키우세요?",
          "original_form": "혹시 반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007733.1.10",
          "form": "아뇨 옛날에 햄스터 키워봤어요",
          "original_form": "아뇨 옛날에 햄스터 키워봤어요",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007733.1.11",
          "form": "햄스터가 평균 수명이 어떻게 돼요?",
          "original_form": "햄스터가 평균 수명이 어떻게 돼요?",
          "speaker_id": "1",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007733.1.12",
          "form": "2~3년 정도요 엄청 짧죠",
          "original_form": "2~3년 정도요 엄청 짧죠",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007733.1.13",
          "form": "아 진짜요? 속상하시겠네요",
          "original_form": "아 진짜요? 속상하시겠네요",
          "speaker_id": "1",
          "time": "20210822 10:38"
        },
        {
          "id": "MDRW2100007733.1.14",
          "form": "죽기 전에 점점 늙어가는 거 볼 때가 더 속상하더라고요",
          "original_form": "죽기 전에 점점 늙어가는 거 볼 때가 더 속상하더라고요",
          "speaker_id": "2",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007733.1.15",
          "form": "아 저희집강아지도 이제 9살이네요",
          "original_form": "아 저희집강아지도 이제 9살이네요",
