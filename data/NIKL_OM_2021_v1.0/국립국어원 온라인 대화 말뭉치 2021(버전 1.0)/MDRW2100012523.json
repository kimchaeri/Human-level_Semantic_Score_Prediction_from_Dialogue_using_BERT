        },
        {
          "id": "MDRW2100012523.1.2",
          "form": "넹 자주봐용",
          "original_form": "넹 자주봐용",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012523.1.3",
          "form": "오 어떤거보시나요",
          "original_form": "오 어떤거보시나요",
          "speaker_id": "1",
          "time": "20210825 10:06"
        },
        {
          "id": "MDRW2100012523.1.4",
          "form": "알고리즘에 따라 거의 모든걸 다 보는듯해요 ㅋㅋㅋ",
          "original_form": "알고리즘에 따라 거의 모든걸 다 보는듯해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100012523.1.5",
          "form": "오 구독 누구하셨나요",
          "original_form": "오 구독 누구하셨나요",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100012523.1.6",
          "form": "구독은 안해요!!",
          "original_form": "구독은 안해요!!",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100012523.1.7",
          "form": "그냥 뜨는거 다 봐서요",
          "original_form": "그냥 뜨는거 다 봐서요",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100012523.1.8",
          "form": "넷플은 보시나요",
          "original_form": "넷플은 보시나요",
          "speaker_id": "1",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100012523.1.9",
          "form": "저 티빙봐요",
          "original_form": "저 티빙봐요",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100012523.1.10",
          "form": "오 저도티빙 봐요ㅎㅎ",
          "original_form": "오 저도티빙 봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100012523.1.11",
          "form": "보통 예능보세요 드라마 보세요?",
          "original_form": "보통 예능보세요 드라마 보세요?",
