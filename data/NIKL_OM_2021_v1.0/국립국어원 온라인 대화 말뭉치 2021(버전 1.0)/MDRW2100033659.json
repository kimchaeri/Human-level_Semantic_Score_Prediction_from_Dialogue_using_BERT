      "utterance": [
        {
          "id": "MDRW2100033659.1.1",
          "form": "가고싶은 여행지가",
          "original_form": "가고싶은 여행지가",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100033659.1.2",
          "form": "있으신가요",
          "original_form": "있으신가요",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100033659.1.3",
          "form": "지금은 경주요",
          "original_form": "지금은 경주요",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100033659.1.4",
          "form": "이맘때 자전거타고 돌아다니면 좋아서요",
          "original_form": "이맘때 자전거타고 돌아다니면 좋아서요",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100033659.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100033659.1.6",
          "form": "전 그냥 해외요 ㅠㅠㅠㅠ",
          "original_form": "전 그냥 해외요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100033659.1.7",
          "form": "경주도 예쁘더라구요",
          "original_form": "경주도 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100033659.1.8",
          "form": "해외좋죠ㅠ 해외어디가고싶으세요?",
          "original_form": "해외좋죠ㅠ 해외어디가고싶으세요?",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100033659.1.9",
          "form": "전 캐나다요 !!!!",
          "original_form": "전 캐나다요 !!!!",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100033659.1.10",
          "form": "어디가고싶으세요?",
          "original_form": "어디가고싶으세요?",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100033659.1.11",
          "form": "저는 동남아요 레져도 하고 과일도 먹고 호텔에서  푹쉬고싶어요",
          "original_form": "저는 동남아요 레져도 하고 과일도 먹고 호텔에서  푹쉬고싶어요",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100033659.1.12",
          "form": "동남아가서",
          "original_form": "동남아가서",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100033659.1.13",
          "form": "완전 단 과일 먹고싶네요",
          "original_form": "완전 단 과일 먹고싶네요",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100033659.1.14",
          "form": "그죠 단과일 트럭에서 대충 썰어주는거 먹고싶어요",
          "original_form": "그죠 단과일 트럭에서 대충 썰어주는거 먹고싶어요",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100033659.1.15",
          "form": "네.. 자금 당장 먹고싶네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네.. 자금 당장 먹고싶네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100033659.1.16",
          "form": "캐나다는 어디를 가고싶으세요?",
          "original_form": "캐나다는 어디를 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100033659.1.17",
          "form": "토론토요 ㅋㅋㅋㅋㅋㅋ!",
          "original_form": "토론토요 ㅋㅋㅋㅋㅋㅋ!",
          "speaker_id": "1",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100033659.1.18",
          "form": "아니면 벤쿠버 그냥 어디등요..",
          "original_form": "아니면 벤쿠버 그냥 어디등요..",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100033659.1.19",
          "form": "토론토에는 뭐가있나요?",
          "original_form": "토론토에는 뭐가있나요?",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100033659.1.20",
          "form": "아뇨 그건 아닌데",
          "original_form": "아뇨 그건 아닌데",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033659.1.21",
          "form": "그냥 가고싶어요 ㅎ",
          "original_form": "그냥 가고싶어요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033659.1.22",
          "form": "아! 제가 너무 미리 생각했네요",
          "original_form": "아! 제가 너무 미리 생각했네요",
          "speaker_id": "2",
          "time": "20210824 21:57"
        }
      ]
