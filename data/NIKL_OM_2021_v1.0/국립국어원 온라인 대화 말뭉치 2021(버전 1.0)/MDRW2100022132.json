      "utterance": [
        {
          "id": "MDRW2100022132.1.1",
          "form": "요즘 머리스타일이 고민이에요",
          "original_form": "요즘 머리스타일이 고민이에요",
          "speaker_id": "2",
          "time": "20210830 10:06"
        },
        {
          "id": "MDRW2100022132.1.2",
          "form": "저도 고민이에요ㅠㅜ",
          "original_form": "저도 고민이에요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100022132.1.3",
          "form": "환절기라 그런지 머리가",
          "original_form": "환절기라 그런지 머리가",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100022132.1.4",
          "form": "많이 빠져서",
          "original_form": "많이 빠져서",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100022132.1.5",
          "form": "볼륨이...ㅠㅠ",
          "original_form": "볼륨이...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100022132.1.6",
          "form": "머리 왤케 빠질까요ㅠㅠ 무슨 털갈이하는 것처럼 빠져요ㅠㅠ",
          "original_form": "머리 왤케 빠질까요ㅠㅠ 무슨 털갈이하는 것처럼 빠져요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100022132.1.7",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100022132.1.8",
          "form": "자고 일어나면 진짜 많이 빠져있어요ㅠ",
          "original_form": "자고 일어나면 진짜 많이 빠져있어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100022132.1.9",
          "form": "저는 머리도 길어서 머리 말리면 엄마가 질색해요 돌아다니는 거 다 제 머리카락이라고",
          "original_form": "저는 머리도 길어서 머리 말리면 엄마가 질색해요 돌아다니는 거 다 제 머리카락이라고",
          "speaker_id": "2",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100022132.1.10",
          "form": "저도 많이 긴편인데",
          "original_form": "저도 많이 긴편인데",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100022132.1.11",
          "form": "요즘 관리 어떻게 하세요??",
          "original_form": "요즘 관리 어떻게 하세요??",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100022132.1.12",
          "form": "일할 때 머리망해야해서 그냥 둬요 ㅋ...",
          "original_form": "일할 때 머리망해야해서 그냥 둬요 ㅋ...",
          "speaker_id": "2",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100022132.1.13",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100022132.1.14",
          "form": "머리망하면 모근이 당긴다고 해야할까요?",
          "original_form": "머리망하면 모근이 당긴다고 해야할까요?",
          "speaker_id": "1",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100022132.1.15",
          "form": "그래서 더 빠지는것같아요ㅠㅠ",
          "original_form": "그래서 더 빠지는것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100022132.1.16",
          "form": "아 그래서 더 빠지는 걸까ㅠㅠ",
          "original_form": "아 그래서 더 빠지는 걸까ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100022132.1.17",
          "form": "네네 저도 한때 머리 망사로 고정하고 다니던적이있는데",
          "original_form": "네네 저도 한때 머리 망사로 고정하고 다니던적이있는데",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100022132.1.18",
          "form": "많이 빠져서ㅠㅠ",
          "original_form": "많이 빠져서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100022132.1.19",
          "form": "두피에 상처도 생기구요ㅠ",
          "original_form": "두피에 상처도 생기구요ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100022132.1.20",
          "form": "헉ㅠㅠㅠ 앞으로 조심해야겠네요",
          "original_form": "헉ㅠㅠㅠ 앞으로 조심해야겠네요",
          "speaker_id": "2",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100022132.1.21",
          "form": "지금은 괜찮으신가요?",
          "original_form": "지금은 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100022132.1.22",
          "form": "넹넹 두피클리닉 센터 두세달정도 다니고",
          "original_form": "넹넹 두피클리닉 센터 두세달정도 다니고",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100022132.1.23",
          "form": "영양제 하고 그랬더니",
          "original_form": "영양제 하고 그랬더니",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100022132.1.24",
          "form": "많이 좋아졌어요ㅎㅎ",
          "original_form": "많이 좋아졌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100022132.1.25",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "2",
          "time": "20210830 12:57"
        }
      ]
    }
  ]
