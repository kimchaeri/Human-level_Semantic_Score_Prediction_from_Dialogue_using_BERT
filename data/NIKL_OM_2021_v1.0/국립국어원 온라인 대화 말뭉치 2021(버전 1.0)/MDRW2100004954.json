      "utterance": [
        {
          "id": "MDRW2100004954.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004954.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004954.1.3",
          "form": "반려동물 키우시나요!??",
          "original_form": "반려동물 키우시나요!??",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004954.1.4",
          "form": "아니요. 강아지나 고양이 키우고싶어요.",
          "original_form": "아니요. 강아지나 고양이 키우고싶어요.",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004954.1.5",
          "form": "키우는거 있으세요?",
          "original_form": "키우는거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004954.1.6",
          "form": "아뇨! 저도 안키워요!",
          "original_form": "아뇨! 저도 안키워요!",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004954.1.7",
          "form": "오..키우면 키우고 싶은 종 같은것도 있으세요???",
          "original_form": "오..키우면 키우고 싶은 종 같은것도 있으세요???",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004954.1.8",
          "form": "키우고싶은 동물있으세요?",
          "original_form": "키우고싶은 동물있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004954.1.9",
          "form": "아 저는 작은강아지들요 ㅋㅋ종은 크게 상관이없어요.",
          "original_form": "아 저는 작은강아지들요 ㅋㅋ종은 크게 상관이없어요.",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004954.1.10",
          "form": "오 저는 아마 안키울것같은데",
          "original_form": "오 저는 아마 안키울것같은데",
          "speaker_id": "1",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004954.1.11",
          "form": "큰개? 키우는 로망?? 만 있어요",
          "original_form": "큰개? 키우는 로망?? 만 있어요",
          "speaker_id": "1",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004954.1.12",
          "form": "아아 ㅋㅋㅋㅋ 로망 좋죠ㅋㅋ 큰개도 멋있죠ㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋ 로망 좋죠ㅋㅋ 큰개도 멋있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004954.1.13",
          "form": "산책하다 큰 개 보거나 시골에서 진돗개보면 왠지 멋있고",
          "original_form": "산책하다 큰 개 보거나 시골에서 진돗개보면 왠지 멋있고",
          "speaker_id": "1",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004954.1.14",
          "form": "또 큰 친구들이 오히려 순한 느낌도 더 있어서",
          "original_form": "또 큰 친구들이 오히려 순한 느낌도 더 있어서",
          "speaker_id": "1",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004954.1.15",
          "form": "힐링되더라구요!",
          "original_form": "힐링되더라구요!",
          "speaker_id": "1",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004954.1.16",
          "form": "네 맞아요. ㅋㅋ 저도 멋져보이더라구요ㅋ",
          "original_form": "네 맞아요. ㅋㅋ 저도 멋져보이더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004954.1.17",
          "form": "저는 아이들이 동물을 좋아해서",
          "original_form": "저는 아이들이 동물을 좋아해서",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004954.1.18",
          "form": "나중에 키울것같은 느낌이에요.",
          "original_form": "나중에 키울것같은 느낌이에요.",
          "speaker_id": "2",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004954.1.19",
          "form": "오오오 아이가 있으셨군요!",
          "original_form": "오오오 아이가 있으셨군요!",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004954.1.20",
          "form": "앗 자녀분들로 수정할게욥...!",
          "original_form": "앗 자녀분들로 수정할게욥...!",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004954.1.21",
          "form": "우왕 도란도란 행복하실 것 같아요!",
          "original_form": "우왕 도란도란 행복하실 것 같아요!",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004954.1.22",
          "form": "네네 ㅋㅋ 주위에 강아지,고양이 보면 키우자고 난리네요ㅋㅋ",
          "original_form": "네네 ㅋㅋ 주위에 강아지,고양이 보면 키우자고 난리네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004954.1.23",
          "form": "한창 크면서 키우고 싶어질때가 있죠!!",
          "original_form": "한창 크면서 키우고 싶어질때가 있죠!!",
          "speaker_id": "1",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004954.1.24",
          "form": "저도 어렸을때 동생이랑 강아지 키우자고 했었는데..!",
          "original_form": "저도 어렸을때 동생이랑 강아지 키우자고 했었는데..!",
          "speaker_id": "1",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004954.1.25",
          "form": "네 ㅋㅋ 지금 그런것같아요",
          "original_form": "네 ㅋㅋ 지금 그런것같아요",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004954.1.26",
          "form": "어무니가 단칼에 거절하셨습니다",
          "original_form": "어무니가 단칼에 거절하셨습니다",
          "speaker_id": "1",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004954.1.27",
          "form": "똥강아지들로 충분하다며",
          "original_form": "똥강아지들로 충분하다며",
          "speaker_id": "1",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004954.1.28",
          "form": "ㅋㅋㅋㄱㅋㅋㅋ네네 ㅋㅋ맞아요.",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋ네네 ㅋㅋ맞아요.",
