      "utterance": [
        {
          "id": "MDRW2100028966.1.1",
          "form": "그치 어디로 이사를 가서 전학을 간거야?",
          "original_form": "그치 어디로 이사를 가서 전학을 간거야?",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028966.1.2",
          "form": "굳이 이사가기에는 구축아파트잖아 게다가 걔네집 삼형제에 형 이제 대학들어가고 막내 아직 초2란말이야....",
          "original_form": "굳이 이사가기에는 구축아파트잖아 게다가 걔네집 삼형제에 형 이제 대학들어가고 막내 아직 초2란말이야....",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028966.1.3",
          "form": "무슨일이지",
          "original_form": "무슨일이지",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028966.1.4",
          "form": "지금 돈 오지게 많이 들시기라서 학원도 안오는거거든",
          "original_form": "지금 돈 오지게 많이 들시기라서 학원도 안오는거거든",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028966.1.5",
          "form": "딱 감이 오더라고 이거 강전이구나....",
          "original_form": "딱 감이 오더라고 이거 강전이구나....",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028966.1.6",
          "form": "강전뭔지알아? ㅋㅋㅋ",
          "original_form": "강전뭔지알아? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028966.1.7",
          "form": "헐 강전????ㅋㅋㅋㅋ",
          "original_form": "헐 강전????ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028966.1.8",
          "form": "뭘어쩧게 해야 강전이야?",
          "original_form": "뭘어쩧게 해야 강전이야?",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028966.1.9",
          "form": "아니근데 만약에 학폭이다 이러면 애들도 다 알거아님",
          "original_form": "아니근데 만약에 학폭이다 이러면 애들도 다 알거아님",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028966.1.10",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028966.1.11",
          "form": "헐 뭐임",
          "original_form": "헐 뭐임",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028966.1.12",
          "form": "학교 좁은데 다 소문날거아니야 여름방학내내 아무도 몰랐던거야",
          "original_form": "학교 좁은데 다 소문날거아니야 여름방학내내 아무도 몰랐던거야",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028966.1.13",
          "form": "그래서 난 얘가 누구를 막 사이버불링을 했나 했거둔?",
          "original_form": "그래서 난 얘가 누구를 막 사이버불링을 했나 했거둔?",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.14",
          "form": "전적이 있었어서....",
          "original_form": "전적이 있었어서....",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.15",
          "form": "이제 알려진거야???",
          "original_form": "이제 알려진거야???",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.16",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.17",
          "form": "난 몰카찍었나햇음ㅋㅌ",
          "original_form": "난 몰카찍었나햇음ㅋㅌ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.18",
          "form": "어어 근데 알고보니 성희롱이었음...",
          "original_form": "어어 근데 알고보니 성희롱이었음...",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.19",
          "form": "그러니 바로 강전이지..",
          "original_form": "그러니 바로 강전이지..",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.20",
          "form": "와 몰카쎄다 ㅋㅋㅋ 중학생이 ...그런경우가있나? ㅋㅋㅋㅋ 있을수도있지만...",
          "original_form": "와 몰카쎄다 ㅋㅋㅋ 중학생이 ...그런경우가있나? ㅋㅋㅋㅋ 있을수도있지만...",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.21",
          "form": "아 누구를 성희롱햇길래",
          "original_form": "아 누구를 성희롱햇길래",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028966.1.22",
          "form": "같은반 친구 ㅜ",
          "original_form": "같은반 친구 ㅜ",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028966.1.23",
          "form": "안알려지고",
          "original_form": "안알려지고",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028966.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028966.1.25",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028966.1.26",
          "form": "진짜 완전 어제 싱숭생숭하더라",
          "original_form": "진짜 완전 어제 싱숭생숭하더라",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028966.1.27",
          "form": "성희롱이라니ㅜㅜㅜㅜ",
          "original_form": "성희롱이라니ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028966.1.28",
          "form": "너무 어릴때부터 보던 애니까",
          "original_form": "너무 어릴때부터 보던 애니까",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028966.1.29",
          "form": "걔가? 걔가왜? 이런느낌",
          "original_form": "걔가? 걔가왜? 이런느낌",
          "speaker_id": "1",
