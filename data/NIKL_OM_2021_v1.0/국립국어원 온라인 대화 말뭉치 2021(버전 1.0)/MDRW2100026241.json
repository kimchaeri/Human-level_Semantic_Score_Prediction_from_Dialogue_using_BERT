      "utterance": [
        {
          "id": "MDRW2100026241.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026241.1.2",
          "form": "강아지 키우세요?",
          "original_form": "강아지 키우세요?",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026241.1.3",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026241.1.4",
          "form": "아녀ㅜㅜ 안키워요ㅜ",
          "original_form": "아녀ㅜㅜ 안키워요ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026241.1.5",
          "form": "아하 뭐 키우시는 동물 있으세요?",
          "original_form": "아하 뭐 키우시는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026241.1.6",
          "form": "저는 강아지 키워요",
          "original_form": "저는 강아지 키워요",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026241.1.7",
          "form": "아녀 저는 동물을 키워본적이 없어요",
          "original_form": "아녀 저는 동물을 키워본적이 없어요",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026241.1.8",
          "form": "근데 키워보고는 싶어요",
          "original_form": "근데 키워보고는 싶어요",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026241.1.9",
          "form": "오 어떤 동물 키워보고 싶으세요?",
          "original_form": "오 어떤 동물 키워보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026241.1.10",
          "form": "전 푸들 키워요~",
          "original_form": "전 푸들 키워요~",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026241.1.11",
          "form": "저도 강아지 고양이 좋아해서 고민이에요 ㅋ",
          "original_form": "저도 강아지 고양이 좋아해서 고민이에요 ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026241.1.12",
          "form": "나중에 독립했을때 키우고싶어요 지금은 못키워요ㅜ",
          "original_form": "나중에 독립했을때 키우고싶어요 지금은 못키워요ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026241.1.13",
          "form": "아하 지금은 본가에서 사세요? 그럼 동물 잘 못키우더라구요ㅠㅠ",
          "original_form": "아하 지금은 본가에서 사세요? 그럼 동물 잘 못키우더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026241.1.14",
          "form": "네 맞아요ㅜ",
          "original_form": "네 맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026241.1.15",
          "form": "제 주변은 고양이를 많이 키워서 고양이 키우고 싶기도 하고",
          "original_form": "제 주변은 고양이를 많이 키워서 고양이 키우고 싶기도 하고",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026241.1.16",
          "form": "근데 아직 멀었어요 독립하려면..ㅠ",
          "original_form": "근데 아직 멀었어요 독립하려면..ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026241.1.17",
          "form": "아하ㅠㅠ 근데 독립하면 다 돈이라서 독립하기가 어렵더라구요",
          "original_form": "아하ㅠㅠ 근데 독립하면 다 돈이라서 독립하기가 어렵더라구요",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026241.1.18",
          "form": "고양이 넘 좋죠~ 귀엽구",
          "original_form": "고양이 넘 좋죠~ 귀엽구",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026241.1.19",
          "form": "고양이도 키워보셨어요?",
          "original_form": "고양이도 키워보셨어요?",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026241.1.20",
          "form": "저도 고양이 파였다가 언니가 강아지 갑자기 데려온 케이스예요 ㅋㅋ",
          "original_form": "저도 고양이 파였다가 언니가 강아지 갑자기 데려온 케이스예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026241.1.21",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026241.1.22",
          "form": "아뇨ㅠㅠ 한번도 ㅜㅠ",
          "original_form": "아뇨ㅠㅠ 한번도 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026241.1.23",
          "form": "강아지파예요 이젠 ㅋㅋ",
          "original_form": "강아지파예요 이젠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026241.1.24",
          "form": "푸들이 애교가 많은가봐요~",
          "original_form": "푸들이 애교가 많은가봐요~",
          "speaker_id": "2",
          "time": "20210901 21:38"
        }
      ]
    }
