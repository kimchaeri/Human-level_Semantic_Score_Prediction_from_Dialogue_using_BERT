      "utterance": [
        {
          "id": "MDRW2100004382.1.1",
          "form": "젤 좋아하는 음식 뭐에요?",
          "original_form": "젤 좋아하는 음식 뭐에요?",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004382.1.2",
          "form": "지금은 다이어트중이라 맨날 단호박 먹고있는대",
          "original_form": "지금은 다이어트중이라 맨날 단호박 먹고있는대",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004382.1.3",
          "form": "마라탕을 엄청 좋아했어요",
          "original_form": "마라탕을 엄청 좋아했어요",
          "speaker_id": "2",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004382.1.4",
          "form": "마라 좋아하세요?",
          "original_form": "마라 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004382.1.5",
          "form": "안 먹어봐서 모르겠어요 맛있어요?",
          "original_form": "안 먹어봐서 모르겠어요 맛있어요?",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004382.1.6",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004382.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004382.1.8",
          "form": "엄청 맛있고 자극적이에요",
          "original_form": "엄청 맛있고 자극적이에요",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004382.1.9",
          "form": "매운거 좋아하는 한국인들의 마음을",
          "original_form": "매운거 좋아하는 한국인들의 마음을",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004382.1.10",
          "form": "강타했죠 ㅎ",
          "original_form": "강타했죠 ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004382.1.11",
          "form": "저랑 친구들은 마라없이 못살것처럼 엄청 먹었거근요",
          "original_form": "저랑 친구들은 마라없이 못살것처럼 엄청 먹었거근요",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004382.1.12",
          "form": "후식으로는 밀크티 샥~ 먹어주고",
          "original_form": "후식으로는 밀크티 샥~ 먹어주고",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004382.1.13",
          "form": "name1님은 무슨 음식 좋아하시나요",
          "original_form": "&name1&님은 무슨 음식 좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004382.1.14",
          "form": "아 다음에 꼭 먹어불게요! 저는 떡볶이 좋아해요!",
          "original_form": "아 다음에 꼭 먹어불게요! 저는 떡볶이 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004382.1.15",
          "form": "아 떡볶이도 너무좋조",
          "original_form": "아 떡볶이도 너무좋조",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004382.1.16",
          "form": "사드시나요?",
          "original_form": "사드시나요?",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004382.1.17",
          "form": "아니면 직접 해드시는 편?",
          "original_form": "아니면 직접 해드시는 편?",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004382.1.18",
          "form": "엄마가 해준거 먹거나  사먹어요!",
          "original_form": "엄마가 해준거 먹거나  사먹어요!",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004382.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004382.1.20",
          "form": "저는 엽떡",
          "original_form": "저는 엽떡",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004382.1.21",
          "form": "진짜 좋아해요",
          "original_form": "진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004382.1.22",
          "form": "다음날 속이 불타지만",
          "original_form": "다음날 속이 불타지만",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.23",
          "form": "맞아요 엽떡 맛있죠!! 근데 매운거 못먹어서 착한맛만 시켜먹어요..",
          "original_form": "맞아요 엽떡 맛있죠!! 근데 매운거 못먹어서 착한맛만 시켜먹어요..",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.24",
          "form": "여기 응 댕 이 라는",
          "original_form": "여기 응 댕 이 라는",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.25",
          "form": "단어를",
          "original_form": "단어를",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.26",
          "form": "못치내요",
          "original_form": "못치내요",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.28",
          "form": "아아 저도",
          "original_form": "아아 저도",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.29",
          "form": "보통맛 먹다가 죽는줄 알았어요",
          "original_form": "보통맛 먹다가 죽는줄 알았어요",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.30",
          "form": "그래도 요즘은 좀 순해진거같던대",
          "original_form": "그래도 요즘은 좀 순해진거같던대",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004382.1.31",
          "form": "혹시 아이스크림 좋아하세요?",
          "original_form": "혹시 아이스크림 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004382.1.32",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004382.1.33",
          "form": "배라 최애 아이스크림!",
          "original_form": "배라 최애 아이스크림!",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004382.1.34",
          "form": "얘기해봐여 ㅎㅎ",
          "original_form": "얘기해봐여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004382.1.35",
          "form": "엄마는 외계인!! 퐁퐁님은요?",
          "original_form": "엄마는 외계인!! 퐁퐁님은요?",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004382.1.36",
          "form": "저는 뉴옥치즈케익이요 ㅎㅎ",
          "original_form": "저는 뉴옥치즈케익이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004382.1.37",
          "form": "치즈를 너무 사랑해서",
          "original_form": "치즈를 너무 사랑해서",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004382.1.38",
          "form": "민초는 어때요?",
          "original_form": "민초는 어때요?",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004382.1.39",
          "form": "호불호 많이 갈리는거",
          "original_form": "호불호 많이 갈리는거",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004382.1.40",
          "form": "민초 있으면 먹어요! 막 엄청 좋아하지는 않고",
          "original_form": "민초 있으면 먹어요! 막 엄청 좋아하지는 않고",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004382.1.41",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004382.1.42",
          "form": "저는 적당히 좋아하는 편이에요",
          "original_form": "저는 적당히 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004382.1.43",
          "form": "배라는 참 맛있게 잘만들었던데",
          "original_form": "배라는 참 맛있게 잘만들었던데",
          "speaker_id": "2",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004382.1.44",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210821 16:57"
        }
      ]
    }
