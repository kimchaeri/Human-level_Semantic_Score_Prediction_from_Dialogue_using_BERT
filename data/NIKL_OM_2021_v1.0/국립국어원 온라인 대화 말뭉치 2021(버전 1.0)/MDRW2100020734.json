      "utterance": [
        {
          "id": "MDRW2100020734.1.1",
          "form": "그곳은 프렌차이즌가요",
          "original_form": "그곳은 프렌차이즌가요",
          "speaker_id": "2",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020734.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020734.1.3",
          "form": "로컬식당인데",
          "original_form": "로컬식당인데",
          "speaker_id": "1",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020734.1.4",
          "form": "저런..ㅠㅠ",
          "original_form": "저런..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.5",
          "form": "제가 오산잠시 살 때",
          "original_form": "제가 오산잠시 살 때",
          "speaker_id": "1",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.6",
          "form": "궐동에 구들짱황소곱창이라고ㅈ거기서 팔았어요",
          "original_form": "궐동에 구들짱황소곱창이라고ㅈ거기서 팔았어요",
          "speaker_id": "1",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.7",
          "form": "하 오산이라니",
          "original_form": "하 오산이라니",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.8",
          "form": "저 음식점 이름 기억진짜못하는데",
          "original_form": "저 음식점 이름 기억진짜못하는데",
          "speaker_id": "1",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.9",
          "form": "너무 먼 곳이네요",
          "original_form": "너무 먼 곳이네요",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.10",
          "form": "저건맛있어서 기억..",
          "original_form": "저건맛있어서 기억..",
          "speaker_id": "1",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.11",
          "form": "엇 저두요ㅋㅋㅋㅋ",
          "original_form": "엇 저두요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.12",
          "form": "그쵸..",
          "original_form": "그쵸..",
          "speaker_id": "1",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.14",
          "form": "늘 친구들한테 끌려가서",
          "original_form": "늘 친구들한테 끌려가서",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.15",
          "form": "이름 기억을 못해요",
          "original_form": "이름 기억을 못해요",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020734.1.16",
          "form": "간판 안보고 들어감..",
          "original_form": "간판 안보고 들어감..",
          "speaker_id": "2",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020734.1.17",
          "form": "이름이 뭐가중요한가요",
          "original_form": "이름이 뭐가중요한가요",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020734.1.18",
          "form": "위치만 알면 됐지",
          "original_form": "위치만 알면 됐지",
          "speaker_id": "2",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020734.1.19",
          "form": "길만알면되는거지",
          "original_form": "길만알면되는거지",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
