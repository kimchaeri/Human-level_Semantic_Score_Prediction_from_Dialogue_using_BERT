      "utterance": [
        {
          "id": "MDRW2100003968.1.1",
          "form": "재벌 살인사건 하네",
          "original_form": "재벌 살인사건 하네",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003968.1.2",
          "form": "다시 광고로 돌아왔네 또 길게 하겠지",
          "original_form": "다시 광고로 돌아왔네 또 길게 하겠지",
          "speaker_id": "2",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003968.1.3",
          "form": "막장재벌이래",
          "original_form": "막장재벌이래",
          "speaker_id": "2",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003968.1.4",
          "form": "중간에 광고 15분은 하는듯",
          "original_form": "중간에 광고 15분은 하는듯",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003968.1.5",
          "form": "피자1인식 광고 괜찮다",
          "original_form": "피자1인식 광고 괜찮다",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003968.1.6",
          "form": "조정석 도대체 광고 몇개를 해",
          "original_form": "조정석 도대체 광고 몇개를 해",
          "speaker_id": "1",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003968.1.7",
          "form": "파파존스 아직도 영업하는데 있나?",
          "original_form": "파파존스 아직도 영업하는데 있나?",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003968.1.8",
          "form": "광안에 하나 봤다",
          "original_form": "광안에 하나 봤다",
          "speaker_id": "1",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003968.1.9",
          "form": "조정석광고 10개 넘는다고 하던데",
          "original_form": "조정석광고 10개 넘는다고 하던데",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003968.1.10",
          "form": "저번에 개런티 많은 순위에 있었네",
          "original_form": "저번에 개런티 많은 순위에 있었네",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003968.1.11",
          "form": "파파존스 광고 나와서 깜짝 놀랬다 ㅋㅋ",
          "original_form": "파파존스 광고 나와서 깜짝 놀랬다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003968.1.12",
          "form": "아직 안 망했다ㅋ",
          "original_form": "아직 안 망했다ㅋ",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003968.1.13",
          "form": "ㅋㅋ저 트롯 가수 남자가 영탁이가?",
          "original_form": "ㅋㅋ저 트롯 가수 남자가 영탁이가?",
          "speaker_id": "2",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003968.1.14",
          "form": "어 영탁이 맞아",
          "original_form": "어 영탁이 맞아",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003968.1.15",
          "form": "엘지 톤프리 광고 남자분이 춤을 너무 잘 추네",
          "original_form": "엘지 톤프리 광고 남자분이 춤을 너무 잘 추네",
          "speaker_id": "2",
          "time": "20210821 14:20"
        }
      ]
