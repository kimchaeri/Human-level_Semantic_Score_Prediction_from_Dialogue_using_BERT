        },
        {
          "id": "MDRW2100006331.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006331.1.3",
          "form": "넹 반가워요",
          "original_form": "넹 반가워요",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006331.1.4",
          "form": "일하고계세요?",
          "original_form": "일하고계세요?",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006331.1.5",
          "form": "전 알바하고있어요!",
          "original_form": "전 알바하고있어요!",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100006331.1.6",
          "form": "업무 스트레스는 어떠세요?",
          "original_form": "업무 스트레스는 어떠세요?",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006331.1.7",
          "form": "너무 심한 거 같아요..",
          "original_form": "너무 심한 거 같아요..",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006331.1.8",
          "form": "어떤거 때문에요?",
          "original_form": "어떤거 때문에요?",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006331.1.9",
          "form": "제가 패스트푸드점이랑 빵집에서 일하는데",
          "original_form": "제가 패스트푸드점이랑 빵집에서 일하는데",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006331.1.10",
          "form": "특히 패스트푸드점이 사람간 스트레스랑 일적으로도 힘든편이예요",
          "original_form": "특히 패스트푸드점이 사람간 스트레스랑 일적으로도 힘든편이예요",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006331.1.11",
          "form": "아 그러겠네요",
          "original_form": "아 그러겠네요",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006331.1.12",
          "form": "고객 있는 업무가 특히 그런거같아요",
          "original_form": "고객 있는 업무가 특히 그런거같아요",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006331.1.13",
          "form": "맞아요 고객응대가 진짜 힘들어요..",
          "original_form": "맞아요 고객응대가 진짜 힘들어요..",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006331.1.14",
          "form": "name2은 어떤 일하세요?",
          "original_form": "&name2&은 어떤 일하세요?",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006331.1.15",
          "form": "저는 그냥 회사 사무직인데",
          "original_form": "저는 그냥 회사 사무직인데",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006331.1.16",
          "form": "외부고객은 외부고객대로 스트레스고",
          "original_form": "외부고객은 외부고객대로 스트레스고",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006331.1.17",
          "form": "같이 일하는 직원은 직원대로 스트레스예요 ㅋㅋㅋ ㅠ",
          "original_form": "같이 일하는 직원은 직원대로 스트레스예요 ㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006331.1.18",
          "form": "아아 알죠알죠",
          "original_form": "아아 알죠알죠",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006331.1.19",
          "form": "원래 아무리 힘들어도 같이 일하는 사람이 좋으면 참을만한데",
          "original_form": "원래 아무리 힘들어도 같이 일하는 사람이 좋으면 참을만한데",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006331.1.20",
          "form": "그게 아니면 진짜 스트레스더라구요",
          "original_form": "그게 아니면 진짜 스트레스더라구요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006331.1.21",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006331.1.22",
          "form": "좋은 사람 만나기 진짜 힘들어요",
          "original_form": "좋은 사람 만나기 진짜 힘들어요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006331.1.23",
          "form": "사무직이면 윗사람들도 많을텐데 엄청 힘들겠네요..",
          "original_form": "사무직이면 윗사람들도 많을텐데 엄청 힘들겠네요..",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006331.1.24",
          "form": "그래도 아예 윗사람은 사실상 만날 일이 없어서",
          "original_form": "그래도 아예 윗사람은 사실상 만날 일이 없어서",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006331.1.25",
          "form": "바로 위아래 사람들만 잘만나면 되는데",
          "original_form": "바로 위아래 사람들만 잘만나면 되는데",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006331.1.26",
          "form": "그게 힘드네요 ㅋㅋㅋㅋ ㅠ",
          "original_form": "그게 힘드네요 ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006331.1.27",
          "form": "아그래요?",
          "original_form": "아그래요?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006331.1.28",
          "form": "근데 그게 또 쉽지가 않죠",
          "original_form": "근데 그게 또 쉽지가 않죠",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006331.1.29",
          "form": "그쵸 직장엔 진짜 별의 별 사람들이 다 오는거같아요",
          "original_form": "그쵸 직장엔 진짜 별의 별 사람들이 다 오는거같아요",
          "speaker_id": "2",
          "time": "20210907 23:05"
        }
      ]
