      "utterance": [
        {
          "id": "MDRW2100035394.1.1",
          "form": "배달비가 조금만 싸지면 좋겠어요ㅜ",
          "original_form": "배달비가 조금만 싸지면 좋겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100035394.1.2",
          "form": "맞아요 ㅜㅜ 요즘은 기본 2000원이죠 ㅠㅠ",
          "original_form": "맞아요 ㅜㅜ 요즘은 기본 2000원이죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:14"
        },
        {
          "id": "MDRW2100035394.1.3",
          "form": "전 그래서 배달비 없는곳에서 시키려하는데",
          "original_form": "전 그래서 배달비 없는곳에서 시키려하는데",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100035394.1.4",
          "form": "잘없더라구요 ㅋㅋ",
          "original_form": "잘없더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100035394.1.5",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035394.1.6",
          "form": "진짜 맛있겠다 싶은데는 3000원이 많더라구요",
          "original_form": "진짜 맛있겠다 싶은데는 3000원이 많더라구요",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100035394.1.7",
          "form": "이 동네가 문제인걸까요ㅋㅋ",
          "original_form": "이 동네가 문제인걸까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100035394.1.8",
          "form": "맞아요 ㅋㅋ 3000원도 많아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋ 3000원도 많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035394.1.9",
          "form": "요즘은 또 비 오면 500원 추가 공휴일 500원 추가",
          "original_form": "요즘은 또 비 오면 500원 추가 공휴일 500원 추가",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100035394.1.10",
          "form": "이렇던걸요??",
          "original_form": "이렇던걸요??",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100035394.1.11",
          "form": "비와도 추가 붙는곳이 있어요?",
          "original_form": "비와도 추가 붙는곳이 있어요?",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035394.1.12",
          "form": "이야.. 장사 너무 잘하네요",
          "original_form": "이야.. 장사 너무 잘하네요",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035394.1.13",
          "form": "네 ㅋㅋㅋ 막 심야시간에 붙는곳도 있구요",
          "original_form": "네 ㅋㅋㅋ 막 심야시간에 붙는곳도 있구요",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100035394.1.14",
          "form": "뭐 심하게 오면 받는걸 이해하지만 ㅋㅋ",
          "original_form": "뭐 심하게 오면 받는걸 이해하지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100035394.1.15",
          "form": "조금오는데 받는건 너무한거같아요 ㅋㅋ",
          "original_form": "조금오는데 받는건 너무한거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100035394.1.16",
          "form": "우와..ㅋㅋ 진짜요",
          "original_form": "우와..ㅋㅋ 진짜요",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100035394.1.17",
          "form": "전에 논란있ㅇㅓ서 좀 나아질까했는데 그렇지도 않네요",
          "original_form": "전에 논란있ㅇㅓ서 좀 나아질까했는데 그렇지도 않네요",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100035394.1.18",
          "form": "맞아요 ㅋㅋ 그냥 배달비만 오르고 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ 그냥 배달비만 오르고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100035394.1.19",
          "form": "교촌이 시작이였잖아요 !",
          "original_form": "교촌이 시작이였잖아요 !",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100035394.1.20",
          "form": "교촌!! 맞아요 그게 시작이였죠!",
          "original_form": "교촌!! 맞아요 그게 시작이였죠!",
          "speaker_id": "2",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100035394.1.21",
          "form": "한동안 안먹었었어요!",
          "original_form": "한동안 안먹었었어요!",
          "speaker_id": "2",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100035394.1.22",
          "form": "맞아요 ㅠㅠ 근데 또 아예 안먹을수도 없고 !",
          "original_form": "맞아요 ㅠㅠ 근데 또 아예 안먹을수도 없고 !",
          "speaker_id": "1",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100035394.1.23",
          "form": "허니콤보 ... 넘 맛있어서",
          "original_form": "허니콤보 ... 넘 맛있어서",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100035394.1.24",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100035394.1.25",
          "form": "그래서 포장하고..ㅋㅋㅋ",
          "original_form": "그래서 포장하고..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100035394.1.26",
          "form": "아 맞아요 포장해요 ㅋㅋㅋ",
          "original_form": "아 맞아요 포장해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100035394.1.27",
          "form": "또 교촌은 점바점이 심하잖아요ㅋㅋ",
          "original_form": "또 교촌은 점바점이 심하잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100035394.1.28",
          "form": "역시 그런거죠?!",
          "original_form": "역시 그런거죠?!",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035394.1.29",
          "form": "저희 동네만 그런줄 알았어요",
