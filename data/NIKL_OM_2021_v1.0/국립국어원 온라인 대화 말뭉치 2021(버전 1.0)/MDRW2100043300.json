      "utterance": [
        {
          "id": "MDRW2100043300.1.1",
          "form": "좋아하는 드라마 있으세요?",
          "original_form": "좋아하는 드라마 있으세요?",
          "speaker_id": "1",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043300.1.2",
          "form": "요즘에 챙겨보는 드라마는 없고 넷플로 가끔 프렌즈도 보고 브레드이발소도 보고 해요",
          "original_form": "요즘에 챙겨보는 드라마는 없고 넷플로 가끔 프렌즈도 보고 브레드이발소도 보고 해요",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043300.1.3",
          "form": "어떤 드라마 좋아하세여??",
          "original_form": "어떤 드라마 좋아하세여??",
          "speaker_id": "2",
          "time": "20210907 17:09"
        },
        {
          "id": "MDRW2100043300.1.4",
          "form": "전 새로시작한 갯마을차차차봐요",
          "original_form": "전 새로시작한 갯마을차차차봐요",
          "speaker_id": "1",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100043300.1.5",
          "form": "오 예쁘더라고요",
          "original_form": "오 예쁘더라고요",
          "speaker_id": "2",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100043300.1.6",
          "form": "네 신민아 여전히 예뻐요",
          "original_form": "네 신민아 여전히 예뻐요",
          "speaker_id": "1",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043300.1.7",
          "form": "로맨스는 내용 상관없이 예쁘면 오케이ㅠㅠ",
          "original_form": "로맨스는 내용 상관없이 예쁘면 오케이ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043300.1.8",
          "form": "ㅋㅋㅋ 김선호도 이쁘져",
          "original_form": "ㅋㅋㅋ 김선호도 이쁘져",
          "speaker_id": "1",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043300.1.9",
          "form": "신민아씨 진짜...",
          "original_form": "신민아씨 진짜...",
          "speaker_id": "2",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043300.1.10",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043300.1.11",
          "form": "내용 재미있나요??",
          "original_form": "내용 재미있나요??",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043300.1.12",
          "form": "예전에 재미있게 봤던거라 재미잇어요",
          "original_form": "예전에 재미있게 봤던거라 재미잇어요",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043300.1.13",
          "form": "재미있나 보네용??",
          "original_form": "재미있나 보네용??",
          "speaker_id": "2",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100043300.1.14",
          "form": "기다려서 보는거 귀찮으니 기다렸다가 볼까봐요",
          "original_form": "기다려서 보는거 귀찮으니 기다렸다가 볼까봐요",
          "speaker_id": "2",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100043300.1.15",
          "form": "아 하긴그렇긴해요\r\n감질맛나죠",
          "original_form": "아 하긴그렇긴해요\r\n감질맛나죠",
          "speaker_id": "1",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100043300.1.16",
          "form": "쫙 몰아서",
          "original_form": "쫙 몰아서",
          "speaker_id": "2",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100043300.1.17",
          "form": "그게 좋긴해요 안끈기고",
          "original_form": "그게 좋긴해요 안끈기고",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043300.1.18",
          "form": "ㅎㅎ 재미있으시다니 한번 보겠습니다",
          "original_form": "ㅎㅎ 재미있으시다니 한번 보겠습니다",
          "speaker_id": "2",
          "time": "20210907 17:15"
        }
      ]
