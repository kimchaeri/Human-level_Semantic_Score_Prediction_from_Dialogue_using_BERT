      "utterance": [
        {
          "id": "MDRW2100040873.1.1",
          "form": "답장이 없어서 언니 잠들었나보다 했는데 또 나쁜말이라고 잘렸어",
          "original_form": "답장이 없어서 언니 잠들었나보다 했는데 또 나쁜말이라고 잘렸어",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100040873.1.2",
          "form": "ㅋㅋㅋㅋ난너가 잠들줄알앗어 ㅋ",
          "original_form": "ㅋㅋㅋㅋ난너가 잠들줄알앗어 ㅋ",
          "speaker_id": "2",
          "time": "20210905 10:55"
        },
        {
          "id": "MDRW2100040873.1.3",
          "form": "ㅋㅋㅋ딴사람이랑 보쌈족발얘기해서",
          "original_form": "ㅋㅋㅋ딴사람이랑 보쌈족발얘기해서",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100040873.1.4",
          "form": "배고팡 ㅜㅠㅋㅋㅋ",
          "original_form": "배고팡 ㅜㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100040873.1.5",
          "form": "ㅋㅋㅋ족발은 진짜 맛있게 먹어본 기억이 잘없어ㅋㅋㅋ모자라게 먹어야하나",
          "original_form": "ㅋㅋㅋ족발은 진짜 맛있게 먹어본 기억이 잘없어ㅋㅋㅋ모자라게 먹어야하나",
          "speaker_id": "1",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100040873.1.6",
          "form": "ㅋㅋ족발 넘비싸 !!!",
          "original_form": "ㅋㅋ족발 넘비싸 !!!",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100040873.1.7",
          "form": "아버지표 수육먹음되!!!",
          "original_form": "아버지표 수육먹음되!!!",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100040873.1.8",
          "form": "ㅋㅋㅋ족발은 껍질이 쫄깃하지않나요?",
          "original_form": "ㅋㅋㅋ족발은 껍질이 쫄깃하지않나요?",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100040873.1.9",
          "form": "아 그렇긴하지만",
          "original_form": "아 그렇긴하지만",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100040873.1.10",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100040873.1.11",
          "form": "엄마가 흑돼지 족시켜서 족발햇는데",
          "original_form": "엄마가 흑돼지 족시켜서 족발햇는데",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100040873.1.12",
          "form": "족발집하는아저씨가 씨육수?를 가져왔데",
          "original_form": "족발집하는아저씨가 씨육수?를 가져왔데",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100040873.1.13",
          "form": "그래서 그걸 좀 넣고 뭐만들고",
          "original_form": "그래서 그걸 좀 넣고 뭐만들고",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100040873.1.14",
          "form": "또둿다가 쓰고 쓰고햇데",
          "original_form": "또둿다가 쓰고 쓰고햇데",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100040873.1.15",
          "form": "다리가 하나에 5처넌이엿나",
          "original_form": "다리가 하나에 5처넌이엿나",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100040873.1.16",
          "form": "우왕ㅋㅋㅋㅋ대박이다....",
          "original_form": "우왕ㅋㅋㅋㅋ대박이다....",
          "speaker_id": "1",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100040873.1.17",
          "form": "그 육수만드는법은 안알려주더래 ㅋㅋㅋㅋ",
          "original_form": "그 육수만드는법은 안알려주더래 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100040873.1.18",
          "form": "흑돼지 족.발 더 맛있을듯",
          "original_form": "흑돼지 족.발 더 맛있을듯",
          "speaker_id": "1",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100040873.1.19",
          "form": "직접가져왓데 ㅋㅋㅋ",
          "original_form": "직접가져왓데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100040873.1.20",
          "form": "ㅋㅋㅋㅋ가게영업비밀",
          "original_form": "ㅋㅋㅋㅋ가게영업비밀",
          "speaker_id": "1",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100040873.1.21",
          "form": "근데 그아저씨 가게닫앗다",
          "original_form": "근데 그아저씨 가게닫앗다",
          "speaker_id": "2",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100040873.1.22",
          "form": "그래서 옆에 중국집에선가 일한다햇으",
          "original_form": "그래서 옆에 중국집에선가 일한다햇으",
          "speaker_id": "2",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100040873.1.23",
          "form": "헉 ㅠㅠㅠ 코로나때문에 ?",
          "original_form": "헉 ㅠㅠㅠ 코로나때문에 ?",
