      "utterance": [
        {
          "id": "MDRW2100015281.1.1",
          "form": "거기날씬어떤가여",
          "original_form": "거기날씬어떤가여",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015281.1.2",
          "form": "비가 쏟아지메요",
          "original_form": "비가 쏟아지메요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015281.1.3",
          "form": "지금도요????!!!!",
          "original_form": "지금도요????!!!!",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015281.1.4",
          "form": "사실 지금은 주룩주룩와요",
          "original_form": "사실 지금은 주룩주룩와요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015281.1.5",
          "form": "그래서 닉네임 name2했어요",
          "original_form": "그래서 닉네임 &name2&했어요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015281.1.6",
          "form": "ㄱㅋㅋㄱㅋㄱㅋㅋㅋ",
          "original_form": "ㄱㅋㅋㄱㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015281.1.7",
          "form": "그 동네는 날씨 어때요",
          "original_form": "그 동네는 날씨 어때요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015281.1.8",
          "form": "맑았으면 닉네임도맑았을텐데..",
          "original_form": "맑았으면 닉네임도맑았을텐데..",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015281.1.9",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015281.1.10",
          "form": "문열어보기가싫어서 모르겠어요..",
          "original_form": "문열어보기가싫어서 모르겠어요..",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015281.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㄲㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄲㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015281.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ내일도 비올까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ내일도 비올까요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015281.1.13",
          "form": "내일은안왔으면좋겠어요",
          "original_form": "내일은안왔으면좋겠어요",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015281.1.14",
          "form": "근데올것같아요",
          "original_form": "근데올것같아요",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015281.1.15",
          "form": "맞아요 사실 오늘도 이럴줄은 몰랐어요",
          "original_form": "맞아요 사실 오늘도 이럴줄은 몰랐어요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015281.1.16",
          "form": "맞아요 날씨왜이래요",
          "original_form": "맞아요 날씨왜이래요",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015281.1.17",
          "form": "오늘 긴팔 입었어요",
          "original_form": "오늘 긴팔 입었어요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015281.1.18",
          "form": "여기서기상청욕해도되나요",
          "original_form": "여기서기상청욕해도되나요",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015281.1.19",
          "form": "옷 어디꺼냐고 남친이 물어봤어요",
          "original_form": "옷 어디꺼냐고 남친이 물어봤어요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015281.1.20",
          "form": "오ㄱㅋㄲㅋ",
          "original_form": "오ㄱㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015281.1.21",
          "form": "좀 이상해서 물어보나 기분이 이상했는데 아니었네요",
          "original_form": "좀 이상해서 물어보나 기분이 이상했는데 아니었네요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015281.1.22",
          "form": "이뻐서물어본걸거예용",
          "original_form": "이뻐서물어본걸거예용",
