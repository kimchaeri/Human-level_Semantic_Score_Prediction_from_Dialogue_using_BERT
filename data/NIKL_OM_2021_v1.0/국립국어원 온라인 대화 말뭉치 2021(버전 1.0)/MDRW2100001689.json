      "utterance": [
        {
          "id": "MDRW2100001689.1.1",
          "form": "해쭈 있자나 너가 알려줬잖아",
          "original_form": "해쭈 있자나 너가 알려줬잖아",
          "speaker_id": "2",
          "time": "20210820 12:29"
        },
        {
          "id": "MDRW2100001689.1.2",
          "form": "너는 언제알게된겨?",
          "original_form": "너는 언제알게된겨?",
          "speaker_id": "2",
          "time": "20210820 12:29"
        },
        {
          "id": "MDRW2100001689.1.3",
          "form": "니 태연 위켄드 들어봄???",
          "original_form": "니 태연 위켄드 들어봄???",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100001689.1.4",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100001689.1.5",
          "form": "언니야가 하도틀어서",
          "original_form": "언니야가 하도틀어서",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100001689.1.6",
          "form": "못들어볼수가 없엏지",
          "original_form": "못들어볼수가 없엏지",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100001689.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.8",
          "form": "아니 노래가 너무 좋자나",
          "original_form": "아니 노래가 너무 좋자나",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.9",
          "form": "태연 솔로노래 많이 들었었는데",
          "original_form": "태연 솔로노래 많이 들었었는데",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.10",
          "form": "그중에 제일 졸아",
          "original_form": "그중에 제일 졸아",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.11",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.12",
          "form": "그정도야??",
          "original_form": "그정도야??",
          "speaker_id": "1",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.13",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "1",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.14",
          "form": "나는 좋기는 한데",
          "original_form": "나는 좋기는 한데",
          "speaker_id": "1",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.15",
          "form": "막 대박 요정도는 아냐",
          "original_form": "막 대박 요정도는 아냐",
          "speaker_id": "1",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.16",
          "form": "아..그래?",
          "original_form": "아..그래?",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.17",
          "form": "쳇",
          "original_form": "쳇",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.18",
          "form": "머 그럴수잇지",
          "original_form": "머 그럴수잇지",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.19",
          "form": "그럼 왓두아콜유는??",
          "original_form": "그럼 왓두아콜유는??",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.20",
          "form": "이것도 별로야?",
          "original_form": "이것도 별로야?",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100001689.1.21",
          "form": "그건 짱좋음",
          "original_form": "그건 짱좋음",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100001689.1.22",
          "form": "왓두아콜유~남이잔아~",
          "original_form": "왓두아콜유~남이잔아~",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100001689.1.23",
          "form": "별일없는척 말을건네~",
          "original_form": "별일없는척 말을건네~",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100001689.1.24",
          "form": "그렇게 꼭 껴안았는데~음~",
          "original_form": "그렇게 꼭 껴안았는데~음~",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100001689.1.25",
          "form": "진짜 태연 천재같애",
          "original_form": "진짜 태연 천재같애",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100001689.1.26",
          "form": "소시 있었을땐 잘 몰랐는디",
          "original_form": "소시 있었을땐 잘 몰랐는디",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100001689.1.27",
          "form": "내가 그때 어리기도했구",
          "original_form": "내가 그때 어리기도했구",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100001689.1.28",
          "form": "마자 소시때는",
          "original_form": "마자 소시때는",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100001689.1.29",
          "form": "우리가 초딩이였으니께",
          "original_form": "우리가 초딩이였으니께",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100001689.1.30",
          "form": "허루",
          "original_form": "허루",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100001689.1.31",
          "form": "아마 넌 유딩이였을걸??",
          "original_form": "아마 넌 유딩이였을걸??",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100001689.1.32",
          "form": "그때가 2007년이니깐",
          "original_form": "그때가 2007년이니깐",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100001689.1.33",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100001689.1.34",
          "form": "그래 오래됐나",
          "original_form": "그래 오래됐나",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100001689.1.35",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100001689.1.36",
          "form": "소시 이번에 14년찬가",
          "original_form": "소시 이번에 14년찬가",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100001689.1.37",
          "form": "그렇대",
          "original_form": "그렇대",
          "speaker_id": "2",
          "time": "20210820 13:06"
        }
      ]
    }
