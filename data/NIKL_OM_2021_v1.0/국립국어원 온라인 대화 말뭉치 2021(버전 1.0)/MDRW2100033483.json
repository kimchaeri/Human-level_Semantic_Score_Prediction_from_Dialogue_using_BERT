      "utterance": [
        {
          "id": "MDRW2100033483.1.1",
          "form": "너 연애 해봤어?",
          "original_form": "너 연애 해봤어?",
          "speaker_id": "1",
          "time": "20210824 00:35"
        },
        {
          "id": "MDRW2100033483.1.2",
          "form": "아니 나는 혼자가 너무 편하더라",
          "original_form": "아니 나는 혼자가 너무 편하더라",
          "speaker_id": "2",
          "time": "20210824 00:35"
        },
        {
          "id": "MDRW2100033483.1.3",
          "form": "여전하구나",
          "original_form": "여전하구나",
          "speaker_id": "1",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.4",
          "form": "너는 중학생때도 그랬었지.",
          "original_form": "너는 중학생때도 그랬었지.",
          "speaker_id": "1",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.5",
          "form": "너는? 여전히 그사람이랑 잘 만나고있어?",
          "original_form": "너는? 여전히 그사람이랑 잘 만나고있어?",
          "speaker_id": "2",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.6",
          "form": "요즘 주위에서 연애하도 싶어하는 애들도 꽤 있더라",
          "original_form": "요즘 주위에서 연애하도 싶어하는 애들도 꽤 있더라",
          "speaker_id": "1",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.7",
          "form": "아니? 난 6년전에 헤어졌어.",
          "original_form": "아니? 난 6년전에 헤어졌어.",
          "speaker_id": "1",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.8",
          "form": "내가 근황얘길 안해줫구나.",
          "original_form": "내가 근황얘길 안해줫구나.",
          "speaker_id": "1",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.9",
          "form": "세상에 내가 소식이 너무 느리구나",
          "original_form": "세상에 내가 소식이 너무 느리구나",
          "speaker_id": "2",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.10",
          "form": "미안해",
          "original_form": "미안해",
          "speaker_id": "2",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.11",
          "form": "헤어진지 꽤 오래됐어...",
          "original_form": "헤어진지 꽤 오래됐어...",
          "speaker_id": "1",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.12",
          "form": "혹시 사별했어...?",
          "original_form": "혹시 사별했어...?",
          "speaker_id": "2",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.13",
          "form": "괜찮아..",
          "original_form": "괜찮아..",
          "speaker_id": "1",
          "time": "20210824 00:36"
        },
        {
          "id": "MDRW2100033483.1.14",
          "form": "성격차이로..ㅜ",
          "original_form": "성격차이로..ㅜ",
          "speaker_id": "1",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100033483.1.15",
          "form": "어머.",
          "original_form": "어머.",
          "speaker_id": "1",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100033483.1.16",
          "form": "아아 놀래라 아니구나",
          "original_form": "아아 놀래라 아니구나",
          "speaker_id": "2",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100033483.1.17",
          "form": "잘 살아있어.",
          "original_form": "잘 살아있어.",
          "speaker_id": "1",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100033483.1.18",
          "form": "네가 물어봐놓고ㅠ뭘 놀라미ㅠ",
          "original_form": "네가 물어봐놓고ㅠ뭘 놀라미ㅠ",
          "speaker_id": "1",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100033483.1.19",
          "form": "소문을 들었는데 거짓소문인가봐",
          "original_form": "소문을 들었는데 거짓소문인가봐",
          "speaker_id": "2",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100033483.1.20",
          "form": "거짓된 소문을 믿지말고ㅠ조심해줘.",
          "original_form": "거짓된 소문을 믿지말고ㅠ조심해줘.",
          "speaker_id": "1",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100033483.1.21",
          "form": "응응 앞으로는 직접 물어볼께",
          "original_form": "응응 앞으로는 직접 물어볼께",
          "speaker_id": "2",
          "time": "20210824 00:38"
        }
      ]
