        },
        {
          "id": "MDRW2100000879.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000879.1.3",
          "form": "다이어트 중이신가요",
          "original_form": "다이어트 중이신가요",
          "speaker_id": "1",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000879.1.4",
          "form": "식단을 조절 중이예요",
          "original_form": "식단을 조절 중이예요",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000879.1.5",
          "form": "식단을 무엇에 드시나요",
          "original_form": "식단을 무엇에 드시나요",
          "speaker_id": "1",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000879.1.6",
          "form": "아침은 고구마나 옥수수를 먹고\r\n점심은 시리얼을 먹습니다",
          "original_form": "아침은 고구마나 옥수수를 먹고\r\n점심은 시리얼을 먹습니다",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000879.1.7",
          "form": "옥수수는 살찐다고하던대",
          "original_form": "옥수수는 살찐다고하던대",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000879.1.8",
          "form": "저도 좋아하는대",
          "original_form": "저도 좋아하는대",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000879.1.9",
          "form": "못먹고있어요",
          "original_form": "못먹고있어요",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000879.1.10",
          "form": "앗 정말요",
          "original_form": "앗 정말요",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000879.1.11",
          "form": "그건 몰랐네요",
          "original_form": "그건 몰랐네요",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000879.1.12",
          "form": "앞으론 빼야겠네요",
          "original_form": "앞으론 빼야겠네요",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000879.1.13",
          "form": "그 콜라비",
          "original_form": "그 콜라비",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000879.1.14",
          "form": "드셔보세요",
          "original_form": "드셔보세요",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000879.1.15",
          "form": "생으로 먹기는 너무 딱딱하지 않나요",
          "original_form": "생으로 먹기는 너무 딱딱하지 않나요",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000879.1.16",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000879.1.17",
          "form": "괜찮아요",
          "original_form": "괜찮아요",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000879.1.18",
          "form": "혹시 성형 해보셨나요",
          "original_form": "혹시 성형 해보셨나요",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000879.1.19",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000879.1.20",
          "form": "할 계획은 없으신가요",
          "original_form": "할 계획은 없으신가요",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000879.1.21",
          "form": "네 무서워서요",
          "original_form": "네 무서워서요",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000879.1.22",
          "form": "성형 하셨어요?",
          "original_form": "성형 하셨어요?",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000879.1.23",
          "form": "쌍수요",
          "original_form": "쌍수요",
          "speaker_id": "1",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100000879.1.24",
          "form": "앗 저도 그건 하고 싶긴 하더라고요",
          "original_form": "앗 저도 그건 하고 싶긴 하더라고요",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100000879.1.25",
          "form": "하세요",
          "original_form": "하세요",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100000879.1.26",
          "form": "진짜 안아프고 삶이 행복해져요",
          "original_form": "진짜 안아프고 삶이 행복해져요",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100000879.1.27",
          "form": "요즘 옷을 사고싶은대 키가작아 어찌입어야하나 고민이많아요",
          "original_form": "요즘 옷을 사고싶은대 키가작아 어찌입어야하나 고민이많아요",
          "speaker_id": "1",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100000879.1.28",
          "form": "키큰사람 키작은사람 스타일링 해놓은거 많아서\r\n전 그런거 보고 참고해서 사요",
          "original_form": "키큰사람 키작은사람 스타일링 해놓은거 많아서\r\n전 그런거 보고 참고해서 사요",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100000879.1.29",
          "form": "옷살때 메이커 위주로 사시나요?",
          "original_form": "옷살때 메이커 위주로 사시나요?",
          "speaker_id": "1",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100000879.1.30",
          "form": "아니요 그렇지는 않아요",
          "original_form": "아니요 그렇지는 않아요",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100000879.1.31",
          "form": "옷은 무슨색상을 좋아하세요?",
          "original_form": "옷은 무슨색상을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100000879.1.32",
          "form": "원색보다는 파스텔톤을 좋아해서요",
          "original_form": "원색보다는 파스텔톤을 좋아해서요",
          "speaker_id": "2",
          "time": "20210820 11:56"
        }
      ]
    }
