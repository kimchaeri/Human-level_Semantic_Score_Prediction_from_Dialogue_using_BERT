        {
          "id": "MDRW2100031072.1.1",
          "form": "적당한 거리가 있어 좋아요",
          "original_form": "적당한 거리가 있어 좋아요",
          "speaker_id": "2",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100031072.1.2",
          "form": "네 학교는 힘들어요ㅜ",
          "original_form": "네 학교는 힘들어요ㅜ",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031072.1.3",
          "form": "학교는 학생일때 좋죠",
          "original_form": "학교는 학생일때 좋죠",
          "speaker_id": "2",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100031072.1.4",
          "form": "저는 좋아요",
          "original_form": "저는 좋아요",
          "speaker_id": "1",
          "time": "20210904 19:17"
        },
        {
          "id": "MDRW2100031072.1.5",
          "form": "네 그러시군요 시험 스트레스는요",
          "original_form": "네 그러시군요 시험 스트레스는요",
          "speaker_id": "2",
          "time": "20210904 19:18"
        },
        {
          "id": "MDRW2100031072.1.6",
          "form": "시험 없어졌어요",
          "original_form": "시험 없어졌어요",
          "speaker_id": "1",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100031072.1.7",
          "form": "요즘 학교 영교시 없나요?",
          "original_form": "요즘 학교 영교시 없나요?",
          "speaker_id": "2",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100031072.1.8",
          "form": "네 야자해요",
          "original_form": "네 야자해요",
          "speaker_id": "1",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031072.1.9",
          "form": "헉 저희 12시까지 했는데 요즘은 그정돈 아니죠",
          "original_form": "헉 저희 12시까지 했는데 요즘은 그정돈 아니죠",
          "speaker_id": "2",
          "time": "20210904 20:04"
        },
        {
          "id": "MDRW2100031072.1.10",
          "form": "네 저는 9시까지해요",
          "original_form": "네 저는 9시까지해요",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031072.1.11",
          "form": "넹 성실하시네요",
          "original_form": "넹 성실하시네요",
