      "utterance": [
        {
          "id": "MDRW2100045287.1.1",
          "form": "언니 지금 룸메랑 같이 생활하는건가?",
          "original_form": "언니 지금 룸메랑 같이 생활하는건가?",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045287.1.2",
          "form": "근데 9월 비대면이라 룸메가 고향에서 안오는중!",
          "original_form": "근데 9월 비대면이라 룸메가 고향에서 안오는중!",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045287.1.3",
          "form": "7월부터 지금까지 쭉 혼자지내고있어",
          "original_form": "7월부터 지금까지 쭉 혼자지내고있어",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045287.1.4",
          "form": "오 그럼 독방?? 엄청 편하겠다",
          "original_form": "오 그럼 독방?? 엄청 편하겠다",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045287.1.5",
          "form": "나중에 언니 시간 괜찮을 때 놀러갈래 ㅎㅎ",
          "original_form": "나중에 언니 시간 괜찮을 때 놀러갈래 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045287.1.6",
          "form": "올거면 9월에 와야해ㅠㅠ 가능해?",
          "original_form": "올거면 9월에 와야해ㅠㅠ 가능해?",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045287.1.7",
          "form": "헐 되려나?! 상황보고 괜찮으면 갈게ㅠㅠ",
          "original_form": "헐 되려나?! 상황보고 괜찮으면 갈게ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045287.1.8",
          "form": "나는 외동이라 그런가 예전에 룸메있었을 때 좋았거든 ㅎㅎ안외롭구",
          "original_form": "나는 외동이라 그런가 예전에 룸메있었을 때 좋았거든 ㅎㅎ안외롭구",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045287.1.9",
          "form": "친한 룸메면 좋지",
          "original_form": "친한 룸메면 좋지",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045287.1.10",
          "form": "나는 내가 노래듣고 영상보는걸 좋아해서",
          "original_form": "나는 내가 노래듣고 영상보는걸 좋아해서",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045287.1.11",
          "form": "그걸 매번 이어폰끼고 봐야하는게",
          "original_form": "그걸 매번 이어폰끼고 봐야하는게",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045287.1.12",
          "form": "가장스트레스같아",
          "original_form": "가장스트레스같아",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045287.1.13",
          "form": "아 생활패턴 맞추는거나 서로 배려해야하는게 힘들지ㅜㅜ",
          "original_form": "아 생활패턴 맞추는거나 서로 배려해야하는게 힘들지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045287.1.14",
          "form": "룸메한테 피해주면안되니까",
          "original_form": "룸메한테 피해주면안되니까",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045287.1.15",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045287.1.16",
          "form": "언니랑 룸메하면 우리는 생패가 같아서 편할텐데",
          "original_form": "언니랑 룸메하면 우리는 생패가 같아서 편할텐데",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045287.1.17",
          "form": "와 진짜재밌겠다",
          "original_form": "와 진짜재밌겠다",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045287.1.18",
          "form": "근데 그러면",
          "original_form": "근데 그러면",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045287.1.19",
          "form": "우리 현생 못살거같아",
          "original_form": "우리 현생 못살거같아",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045287.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 학점 난리나는거아냐?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 학점 난리나는거아냐?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045287.1.21",
          "form": "언니는 아침에 씻어 저녁에 씻어?",
          "original_form": "언니는 아침에 씻어 저녁에 씻어?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045287.1.22",
          "form": "이것도 룸메랑 맞춰야하잖아",
          "original_form": "이것도 룸메랑 맞춰야하잖아",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045287.1.23",
          "form": "나는 거의 무조건 저녁!",
          "original_form": "나는 거의 무조건 저녁!",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045287.1.24",
          "form": "아침에 씻을여유가없어",
          "original_form": "아침에 씻을여유가없어",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045287.1.25",
          "form": "아 인정인정 일찍 일어나야해서 힘들지 ㅋㅋㅋ",
          "original_form": "아 인정인정 일찍 일어나야해서 힘들지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045287.1.26",
          "form": "나는 룸메가 저녁에 씻으면 아침에씻구 아침에 씻으면 저녁에 씼었어",
          "original_form": "나는 룸메가 저녁에 씻으면 아침에씻구 아침에 씻으면 저녁에 씼었어",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045287.1.27",
          "form": "완전 맞춤형이네",
          "original_form": "완전 맞춤형이네",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045287.1.28",
          "form": "우리 나중에 이웃으로 꼭 지내자 ㅎㅎ",
          "original_form": "우리 나중에 이웃으로 꼭 지내자 ㅎㅎ",
