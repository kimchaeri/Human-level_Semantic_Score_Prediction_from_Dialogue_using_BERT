      "utterance": [
        {
          "id": "MDRW2100016315.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016315.1.2",
          "form": "혹시좋아하는 운동 있어요?",
          "original_form": "혹시좋아하는 운동 있어요?",
          "speaker_id": "1",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016315.1.3",
          "form": "안녕하세요 저는 배드민턴 치는거 좋아해요",
          "original_form": "안녕하세요 저는 배드민턴 치는거 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100016315.1.4",
          "form": "배드민턴 치시는분 많으시네요",
          "original_form": "배드민턴 치시는분 많으시네요",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016315.1.5",
          "form": "저는수영 좋아해요",
          "original_form": "저는수영 좋아해요",
          "speaker_id": "1",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016315.1.6",
          "form": "오 저도 수영하는거 좋아해요 코로나 이후로는 잘 못가고있지만 ㅜㅜ",
          "original_form": "오 저도 수영하는거 좋아해요 코로나 이후로는 잘 못가고있지만 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016315.1.7",
          "form": "수영장가고싶은데 갈수가없죠",
          "original_form": "수영장가고싶은데 갈수가없죠",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016315.1.8",
          "form": "수영잘하세요?",
          "original_form": "수영잘하세요?",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016315.1.9",
          "form": "잘하진않지만 할수는 있는정도 같아요 ㅋ ㅋㅋㅋ\r\n가장 좋아하는 영법은 뭐에요?",
          "original_form": "잘하진않지만 할수는 있는정도 같아요 ㅋ ㅋㅋㅋ\r\n가장 좋아하는 영법은 뭐에요?",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016315.1.10",
          "form": "자유형이요!",
          "original_form": "자유형이요!",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016315.1.11",
          "form": "사실 접영 평영은 잘 못해요",
          "original_form": "사실 접영 평영은 잘 못해요",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016315.1.12",
          "form": "배우긴했는데 금방 가라앉아요ㅠ",
          "original_form": "배우긴했는데 금방 가라앉아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016315.1.13",
          "form": "계속연습해야하는데 할수가없으니 슬퍼요",
          "original_form": "계속연습해야하는데 할수가없으니 슬퍼요",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100016315.1.14",
          "form": "저도 접영은 몇번 못가고 가라앉게 돼요 흑흑",
          "original_form": "저도 접영은 몇번 못가고 가라앉게 돼요 흑흑",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100016315.1.15",
          "form": "저는 배영이 제일 좋더라구요",
          "original_form": "저는 배영이 제일 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016315.1.16",
          "form": "ㅎㅎ 접영이 고비인가봐요",
          "original_form": "ㅎㅎ 접영이 고비인가봐요",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016315.1.17",
          "form": "배영 좋죠ㅋㅋ",
          "original_form": "배영 좋죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016315.1.18",
          "form": "물에떠있을수있어서ㅋㅋ",
          "original_form": "물에떠있을수있어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016315.1.19",
          "form": "맞아요 물에 떠있는 느낌이 너무 좋아요",
          "original_form": "맞아요 물에 떠있는 느낌이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016315.1.20",
          "form": "혹시 패러글라이딩해보셨어요?",
          "original_form": "혹시 패러글라이딩해보셨어요?",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016315.1.21",
          "form": "저 최근에 했거든요ㅋㅋ 올해의버킷리스트여가지구",
          "original_form": "저 최근에 했거든요ㅋㅋ 올해의버킷리스트여가지구",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016315.1.22",
          "form": "우와 아니욤못해봤어요 저도 언젠가는 해보고 싶어요",
          "original_form": "우와 아니욤못해봤어요 저도 언젠가는 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016315.1.23",
          "form": "어땠어요 ???",
          "original_form": "어땠어요 ???",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016315.1.24",
          "form": "생각만큼 익사이팅하지는않았어요.",
          "original_form": "생각만큼 익사이팅하지는않았어요.",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016315.1.25",
          "form": "처음에 뛰기전이 겁나지만, 막상 뛸때는 어렵지않았어요.",
          "original_form": "처음에 뛰기전이 겁나지만, 막상 뛸때는 어렵지않았어요.",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016315.1.26",
          "form": "비행시간이 10분정도쯤이였는데 아주 천천히 내려와요.",
          "original_form": "비행시간이 10분정도쯤이였는데 아주 천천히 내려와요.",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016315.1.27",
          "form": "짚라인보다 더 시시한느낌? 이랄까요",
          "original_form": "짚라인보다 더 시시한느낌? 이랄까요",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016315.1.28",
          "form": "그치만 한번쯤 해볼만해요!",
          "original_form": "그치만 한번쯤 해볼만해요!",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016315.1.29",
          "form": "나중에 해외나가서  풍경좋고 매우 높은곳에서 다시 해보고싶어요",
          "original_form": "나중에 해외나가서  풍경좋고 매우 높은곳에서 다시 해보고싶어요",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016315.1.30",
          "form": "오 되게 의외네요 짚라인이 더 무서운 느낌이군요",
          "original_form": "오 되게 의외네요 짚라인이 더 무서운 느낌이군요",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016315.1.31",
          "form": "짚라인도 아직 못해봤거든요ㅜㅜ",
          "original_form": "짚라인도 아직 못해봤거든요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016315.1.32",
          "form": "풍경 좋은곳에서 타면 좋을것 같긴해요",
          "original_form": "풍경 좋은곳에서 타면 좋을것 같긴해요",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016315.1.33",
          "form": "저는무거운거 높은거 좋아하진않지만, 할만했어요!",
          "original_form": "저는무거운거 높은거 좋아하진않지만, 할만했어요!",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016315.1.34",
          "form": "기회되면 도전해보세요!",
          "original_form": "기회되면 도전해보세요!",
          "speaker_id": "1",
          "time": "20210826 12:15"
        }
      ]
