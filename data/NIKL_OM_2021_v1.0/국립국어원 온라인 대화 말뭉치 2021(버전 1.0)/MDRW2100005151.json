      "utterance": [
        {
          "id": "MDRW2100005151.1.1",
          "form": "뭐해?",
          "original_form": "뭐해?",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005151.1.2",
          "form": "쇼핑은 인쇼가 최고지 ㅋㅋㅋ",
          "original_form": "쇼핑은 인쇼가 최고지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005151.1.3",
          "form": "너를 찾았슈 ㅋㅋㅋㅋ",
          "original_form": "너를 찾았슈 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005151.1.4",
          "form": "햄스터 너무 커서 집 새로 샀는데ㅠ",
          "original_form": "햄스터 너무 커서 집 새로 샀는데ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005151.1.5",
          "form": "돈덩어리임..ㅠㅠ",
          "original_form": "돈덩어리임..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005151.1.6",
          "form": "ㅋㅋㅋㅋㅋ맞아. 인쇼가 최고",
          "original_form": "ㅋㅋㅋㅋㅋ맞아. 인쇼가 최고",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005151.1.7",
          "form": "ㅋㅋㅋㅋ아 웃겨 햄스터 집 새로샀나?",
          "original_form": "ㅋㅋㅋㅋ아 웃겨 햄스터 집 새로샀나?",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005151.1.8",
          "form": "인터넷으로샀어?",
          "original_form": "인터넷으로샀어?",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005151.1.9",
          "form": "쿠팡으로ㅠ 토끼맘큼 커질까 겁난다ㅠ",
          "original_form": "쿠팡으로ㅠ 토끼맘큼 커질까 겁난다ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005151.1.10",
          "form": "ㅋㅋㅋㅋ얼마나 커지려나?",
          "original_form": "ㅋㅋㅋㅋ얼마나 커지려나?",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.11",
          "form": "궁금하네. ㅋㅋ",
          "original_form": "궁금하네. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.12",
          "form": "튼튼한거 샀어?",
          "original_form": "튼튼한거 샀어?",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.13",
          "form": "지금샀는 집도 인터넷이랑 가격차이.너무 나더라ㅠ",
          "original_form": "지금샀는 집도 인터넷이랑 가격차이.너무 나더라ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.14",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.15",
          "form": "얼마나?",
          "original_form": "얼마나?",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.16",
          "form": "오프라인으로는 못사겠으뮤ㅠ",
          "original_form": "오프라인으로는 못사겠으뮤ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.17",
          "form": "2만원;",
          "original_form": "2만원;",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.18",
          "form": "맞어. 오프라인은 못사겠더라",
          "original_form": "맞어. 오프라인은 못사겠더라",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.19",
          "form": "헉 많이 차이나네",
          "original_form": "헉 많이 차이나네",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005151.1.20",
          "form": "햄스터도 인터넷네 찾아보니 5천원에도 분양하더라 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "햄스터도 인터넷네 찾아보니 5천원에도 분양하더라 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005151.1.21",
          "form": "아 진짜가?",
          "original_form": "아 진짜가?",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005151.1.22",
          "form": "진짜 쫌 알아보고 사야되는데..",
          "original_form": "진짜 쫌 알아보고 사야되는데..",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005151.1.23",
          "form": "가격차 너무 심하네",
          "original_form": "가격차 너무 심하네",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005151.1.24",
          "form": "맨날 눈티맞고 다님 ㅋㅋㅋ",
          "original_form": "맨날 눈티맞고 다님 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005151.1.25",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005151.1.26",
          "form": "아.....씁쓸하구나",
          "original_form": "아.....씁쓸하구나",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005151.1.27",
          "form": "ㅋㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005151.1.28",
          "form": "물고기는 싼가 몰라도 슬프다이ㅠ",
          "original_form": "물고기는 싼가 몰라도 슬프다이ㅠ",
