        {
          "id": "MDRW2100015130.1.1",
          "form": "계단 걷기 운동 최고다",
          "original_form": "계단 걷기 운동 최고다",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015130.1.2",
          "form": "그니깡 ㅋㅋ 엄마가 그래서 튼튼한듯",
          "original_form": "그니깡 ㅋㅋ 엄마가 그래서 튼튼한듯",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015130.1.3",
          "form": "그래 내 따라서 계단 걷기 좀 해라",
          "original_form": "그래 내 따라서 계단 걷기 좀 해라",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015130.1.4",
          "form": "알았옹 이따가 같이 가자!",
          "original_form": "알았옹 이따가 같이 가자!",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015130.1.5",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015130.1.6",
          "form": "맨날엄마혼자만 하고~!",
          "original_form": "맨날엄마혼자만 하고~!",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015130.1.7",
          "form": "네가 안 했잖아",
          "original_form": "네가 안 했잖아",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015130.1.8",
          "form": "ㅋㅋ 이제 해야겠다 안하려고하면 좀 끌고가줘용",
          "original_form": "ㅋㅋ 이제 해야겠다 안하려고하면 좀 끌고가줘용",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015130.1.9",
          "form": "가능할런지",
          "original_form": "가능할런지",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015130.1.10",
          "form": "진짜다!",
          "original_form": "진짜다!",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015130.1.11",
          "form": "그래 이따가 20층까지 올라가자",
