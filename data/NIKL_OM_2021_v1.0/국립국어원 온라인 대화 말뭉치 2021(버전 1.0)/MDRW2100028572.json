      "utterance": [
        {
          "id": "MDRW2100028572.1.1",
          "form": "최근 잘 산템 있으세요",
          "original_form": "최근 잘 산템 있으세요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028572.1.2",
          "form": "최근에~ 외장하드 잘 샀다고 생각햇어요!",
          "original_form": "최근에~ 외장하드 잘 샀다고 생각햇어요!",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028572.1.3",
          "form": "아하 저도 샀는데",
          "original_form": "아하 저도 샀는데",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028572.1.4",
          "form": "잘 안꼬내지네요",
          "original_form": "잘 안꼬내지네요",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028572.1.5",
          "form": "사실 충동적으로 산것도있지만",
          "original_form": "사실 충동적으로 산것도있지만",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028572.1.6",
          "form": "그래도 나쁘지않은거같아요",
          "original_form": "그래도 나쁘지않은거같아요",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028572.1.7",
          "form": "컴퓨터가 용량이 작아서 그런가",
          "original_form": "컴퓨터가 용량이 작아서 그런가",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028572.1.8",
          "form": "밎아요 남는건 파일뿐",
          "original_form": "밎아요 남는건 파일뿐",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028572.1.9",
          "form": "맞아요 ㅎ 게임넣기도 괜찮더라고요",
          "original_form": "맞아요 ㅎ 게임넣기도 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028572.1.10",
          "form": "영상도 넣고",
          "original_form": "영상도 넣고",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028572.1.11",
          "form": "와 게임도 들어가요?",
          "original_form": "와 게임도 들어가요?",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028572.1.12",
          "form": "네네 ㅎ 1테라로 샀더니 들어가더라고요 ㅎ",
          "original_form": "네네 ㅎ 1테라로 샀더니 들어가더라고요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028572.1.13",
          "form": "와 1테라 한 10만원 넘으셨겠네요??",
          "original_form": "와 1테라 한 10만원 넘으셨겠네요??",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028572.1.14",
          "form": "네 맞아요ㅎㅎ좀 비쌌어요 무리 했조ㅡ",
          "original_form": "네 맞아요ㅎㅎ좀 비쌌어요 무리 했조ㅡ",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028572.1.15",
          "form": "그래도 요즘껀 가볍고 작고 좋더라고요",
          "original_form": "그래도 요즘껀 가볍고 작고 좋더라고요",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028572.1.16",
          "form": "맞아요 점점 기기가 좋아져요",
          "original_form": "맞아요 점점 기기가 좋아져요",
          "speaker_id": "1",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100028572.1.17",
          "form": "그러니깐요 ㅎ 하지만 돈이 없는게 단점",
          "original_form": "그러니깐요 ㅎ 하지만 돈이 없는게 단점",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100028572.1.18",
          "form": "최근에 사신거 있으세요?",
          "original_form": "최근에 사신거 있으세요?",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100028572.1.19",
          "form": "맞아여 흑흑",
          "original_form": "맞아여 흑흑",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100028572.1.20",
          "form": "전 구두하나사고",
          "original_form": "전 구두하나사고",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100028572.1.21",
          "form": "식기세척기사고싶어요",
          "original_form": "식기세척기사고싶어요",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100028572.1.22",
          "form": "오!! 구두",
          "original_form": "오!! 구두",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100028572.1.23",
          "form": "저도 식기세척기랑 건조기요",
          "original_form": "저도 식기세척기랑 건조기요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100028572.1.24",
          "form": "갖고싶어요",
          "original_form": "갖고싶어요",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100028572.1.25",
          "form": "와와 저도",
          "original_form": "와와 저도",
          "speaker_id": "1",
