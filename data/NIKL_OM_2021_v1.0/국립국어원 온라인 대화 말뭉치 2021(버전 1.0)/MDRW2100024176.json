      "utterance": [
        {
          "id": "MDRW2100024176.1.1",
          "form": "그동네 유명한 음식이 뭔가요?",
          "original_form": "그동네 유명한 음식이 뭔가요?",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024176.1.2",
          "form": "우리 동네는..",
          "original_form": "우리 동네는..",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024176.1.3",
          "form": "마카롱과 디저트 맛집이 많지요ㅜ",
          "original_form": "마카롱과 디저트 맛집이 많지요ㅜ",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024176.1.4",
          "form": "저는 단것을 못먹지만ㅜ",
          "original_form": "저는 단것을 못먹지만ㅜ",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024176.1.5",
          "form": "혹시 ㅇㅇㅇ길 이런곳 사시나요?",
          "original_form": "혹시 ㅇㅇㅇ길 이런곳 사시나요?",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024176.1.6",
          "form": "경리단길 뭐 이런곳?",
          "original_form": "경리단길 뭐 이런곳?",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024176.1.7",
          "form": "아뇽  그냥 무난한 도심이에요ㅜ",
          "original_form": "아뇽  그냥 무난한 도심이에요ㅜ",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024176.1.8",
          "form": "전 곱창 회  이런 파인데 괴롭ㅜ",
          "original_form": "전 곱창 회  이런 파인데 괴롭ㅜ",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024176.1.9",
          "form": "님 지역은요?",
          "original_form": "님 지역은요?",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024176.1.10",
          "form": "전 경상도인데예",
          "original_form": "전 경상도인데예",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024176.1.11",
          "form": "경상도 아십니꺼",
          "original_form": "경상도 아십니꺼",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024176.1.12",
          "form": "아시나욧?",
          "original_form": "아시나욧?",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024176.1.13",
          "form": "오옷 왜 모릅니꺼!!",
          "original_form": "오옷 왜 모릅니꺼!!",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024176.1.14",
          "form": "저 코로나 풀림 바로 여행갈거예요!",
          "original_form": "저 코로나 풀림 바로 여행갈거예요!",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024176.1.15",
          "form": "맛집 깁미깁미!!",
          "original_form": "맛집 깁미깁미!!",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024176.1.16",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024176.1.17",
          "form": "부산 개미집",
          "original_form": "부산 개미집",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100024176.1.18",
          "form": "좋아용 ㅋㅋ",
          "original_form": "좋아용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100024176.1.19",
          "form": "소주 두병드시고 가세여",
          "original_form": "소주 두병드시고 가세여",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100024176.1.20",
          "form": "개미집? 뭐 파나용",
          "original_form": "개미집? 뭐 파나용",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100024176.1.21",
          "form": "ㅡ딱 두병이 주량이에요ㅎ",
          "original_form": "ㅡ딱 두병이 주량이에요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100024176.1.22",
          "form": "새우 낙지 곱창",
          "original_form": "새우 낙지 곱창",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024176.1.23",
          "form": "한판에 다 나오요",
          "original_form": "한판에 다 나오요",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024176.1.24",
          "form": "낙곱새",
          "original_form": "낙곱새",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024176.1.25",
          "form": "헉 저 여수 갔다가 포차서 낙곱새 먹구 기절할뻔ㅜ",
          "original_form": "헉 저 여수 갔다가 포차서 낙곱새 먹구 기절할뻔ㅜ",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024176.1.26",
          "form": "부산 개미집 기억할게욧!!",
          "original_form": "부산 개미집 기억할게욧!!",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024176.1.27",
          "form": "서울 사세요?",
          "original_form": "서울 사세요?",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024176.1.28",
          "form": "네ㅎ 최애 여행지는 부산과 여수!",
          "original_form": "네ㅎ 최애 여행지는 부산과 여수!",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024176.1.29",
          "form": "오직 먹는것만 보고 다니죵ㅎ",
          "original_form": "오직 먹는것만 보고 다니죵ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024176.1.30",
          "form": "저도 먹깨비입니다",
          "original_form": "저도 먹깨비입니다",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024176.1.31",
          "form": "반가워요 서울먹깨비!",
          "original_form": "반가워요 서울먹깨비!",
          "speaker_id": "2",
          "time": "20210830 23:22"
        }
      ]
