      "utterance": [
        {
          "id": "MDRW2100034731.1.1",
          "form": "다운타우너아세요?",
          "original_form": "다운타우너아세요?",
          "speaker_id": "1",
          "time": "20210828 18:21"
        },
        {
          "id": "MDRW2100034731.1.2",
          "form": "아니요ㅋㅋㅋㅋ 찾아보니 버거집인가봐여",
          "original_form": "아니요ㅋㅋㅋㅋ 찾아보니 버거집인가봐여",
          "speaker_id": "2",
          "time": "20210828 18:21"
        },
        {
          "id": "MDRW2100034731.1.3",
          "form": "네 ㅋㅋㅋ 원래 버거 안좋아하는데",
          "original_form": "네 ㅋㅋㅋ 원래 버거 안좋아하는데",
          "speaker_id": "1",
          "time": "20210828 18:21"
        },
        {
          "id": "MDRW2100034731.1.4",
          "form": "거기는 빵만 먹어도 맛있어요..!!! 유명한 집이에요",
          "original_form": "거기는 빵만 먹어도 맛있어요..!!! 유명한 집이에요",
          "speaker_id": "1",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034731.1.5",
          "form": "오오 여기더 저장해야겠네요",
          "original_form": "오오 여기더 저장해야겠네요",
          "speaker_id": "2",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034731.1.6",
          "form": "네 강추합니다 ㅋㅋㅋ",
          "original_form": "네 강추합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034731.1.7",
          "form": "아보카도버거가 유명한데 전 기본 버거먹었어요",
          "original_form": "아보카도버거가 유명한데 전 기본 버거먹었어요",
          "speaker_id": "1",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100034731.1.8",
          "form": "가게가 많군여 어디가 젤 유명한 지점인가요",
          "original_form": "가게가 많군여 어디가 젤 유명한 지점인가요",
          "speaker_id": "2",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100034731.1.9",
          "form": "흠 사실 다 인기많긴한데",
          "original_form": "흠 사실 다 인기많긴한데",
          "speaker_id": "1",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100034731.1.10",
          "form": "이태원이랑 안국이 젤 많이 가는거같더라구요",
          "original_form": "이태원이랑 안국이 젤 많이 가는거같더라구요",
          "speaker_id": "1",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100034731.1.11",
          "form": "넹 감사해여ㅋㅋㅋ 서울 언제 놀러갈지 모르겠는데 가고싶네요 엄청",
          "original_form": "넹 감사해여ㅋㅋㅋ 서울 언제 놀러갈지 모르겠는데 가고싶네요 엄청",
          "speaker_id": "2",
          "time": "20210828 18:24"
        },
        {
          "id": "MDRW2100034731.1.12",
          "form": "추천합니다 ㅋㅋㅋㅋ",
          "original_form": "추천합니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034731.1.13",
          "form": "니뽕내뽕도 맛있어요",
          "original_form": "니뽕내뽕도 맛있어요",
          "speaker_id": "1",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034731.1.14",
          "form": "니뽕내뽕은 대학다닐때 많이 먹었어요ㅋㅋㅋㅋ",
          "original_form": "니뽕내뽕은 대학다닐때 많이 먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034731.1.15",
          "form": "아 ㅋㅋㅋㅋ아시는군요",
          "original_form": "아 ㅋㅋㅋㅋ아시는군요",
          "speaker_id": "1",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034731.1.16",
          "form": "전 거기 로뽕 최애에요",
          "original_form": "전 거기 로뽕 최애에요",
          "speaker_id": "1",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034731.1.17",
          "form": "ㅋㅋㅋㅋㅋ로제 마니아시네요",
          "original_form": "ㅋㅋㅋㅋㅋ로제 마니아시네요",
          "speaker_id": "2",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100034731.1.18",
          "form": "메뉴 이름 보면서 웃기다했었던 기억이 나요",
          "original_form": "메뉴 이름 보면서 웃기다했었던 기억이 나요",
          "speaker_id": "2",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100034731.1.19",
          "form": "앜ㅋㅋㅋㅋㅋ 그러네요",
          "original_form": "앜ㅋㅋㅋㅋㅋ 그러네요",
          "speaker_id": "1",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100034731.1.20",
          "form": "로제떡볶이 나오기전부터 좋아했었네요 ㅋㅋㅋ",
          "original_form": "로제떡볶이 나오기전부터 좋아했었네요 ㅋㅋㅋ",
