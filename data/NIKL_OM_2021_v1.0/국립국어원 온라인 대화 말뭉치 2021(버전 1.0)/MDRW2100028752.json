      "utterance": [
        {
          "id": "MDRW2100028752.1.1",
          "form": "최근 지름 품목이 있으신가요?",
          "original_form": "최근 지름 품목이 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028752.1.2",
          "form": "저는 최근에 닌텐도 스위치를 구매했어요~",
          "original_form": "저는 최근에 닌텐도 스위치를 구매했어요~",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028752.1.3",
          "form": "와 대박ㅋㅋㅋㅋ",
          "original_form": "와 대박ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028752.1.4",
          "form": "저는 반년쯤 전에 구매했어요~",
          "original_form": "저는 반년쯤 전에 구매했어요~",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028752.1.5",
          "form": "오 칩은 뭐 사셨나요?",
          "original_form": "오 칩은 뭐 사셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028752.1.6",
          "form": "저는 링피트하고 동숲하고 젤다 야숨이요!",
          "original_form": "저는 링피트하고 동숲하고 젤다 야숨이요!",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028752.1.7",
          "form": "오 저도 링피트랑 동숲 구매했어요ㅋㅋㅋㅋ",
          "original_form": "오 저도 링피트랑 동숲 구매했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028752.1.8",
          "form": "와앜ㅋㅋㅋㅋ",
          "original_form": "와앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028752.1.9",
          "form": "다른건 안사셨나요?",
          "original_form": "다른건 안사셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028752.1.10",
          "form": "네 아직 요 2개만 샀어요",
          "original_form": "네 아직 요 2개만 샀어요",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028752.1.11",
          "form": "저는 링피트 구매하고",
          "original_form": "저는 링피트 구매하고",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028752.1.12",
          "form": "방치하다가 요즘 좀 하고 있어요",
          "original_form": "방치하다가 요즘 좀 하고 있어요",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028752.1.13",
          "form": "저는 사고 딱 2번 했어요",
          "original_form": "저는 사고 딱 2번 했어요",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028752.1.14",
          "form": "힘들더라구요..ㅎ",
          "original_form": "힘들더라구요..ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028752.1.15",
          "form": "ㅋㅋㅋㅋ저도 그래서 중고로 팔까 하다가 후회할 것 같아서",
          "original_form": "ㅋㅋㅋㅋ저도 그래서 중고로 팔까 하다가 후회할 것 같아서",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028752.1.16",
          "form": "그냥 뒀어요",
