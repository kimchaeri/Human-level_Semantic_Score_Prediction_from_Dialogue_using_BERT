      "utterance": [
        {
          "id": "MDRW2100015769.1.1",
          "form": "스포츠 어떤 거 좋아하시나요",
          "original_form": "스포츠 어떤 거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015769.1.2",
          "form": "이번에 올림픽 재밌게 봤어요 배구 재밌더라구요",
          "original_form": "이번에 올림픽 재밌게 봤어요 배구 재밌더라구요",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015769.1.3",
          "form": "오 저두요",
          "original_form": "오 저두요",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015769.1.4",
          "form": "한편의 역전 드라마",
          "original_form": "한편의 역전 드라마",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015769.1.5",
          "form": "맞아요 크 김연경",
          "original_form": "맞아요 크 김연경",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015769.1.6",
          "form": "알고리즘이 추천해주는 대로 따라 보다보니",
          "original_form": "알고리즘이 추천해주는 대로 따라 보다보니",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015769.1.7",
          "form": "배구가 좋아졌어요",
          "original_form": "배구가 좋아졌어요",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015769.1.8",
          "form": "그니까여 매력녀 ㅠㅠ",
          "original_form": "그니까여 매력녀 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015769.1.9",
          "form": "너무 잘해..",
          "original_form": "너무 잘해..",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015769.1.10",
          "form": "동생인데 언니라고 부르고픈ㅎ",
          "original_form": "동생인데 언니라고 부르고픈ㅎ",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015769.1.11",
          "form": "저는 합법입니다 ㅎㅎㅎㅎㅎ",
          "original_form": "저는 합법입니다 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015769.1.12",
          "form": "이런ㅋ부럽네요ㅋㅋ. 펜싱도 보셨죠?",
          "original_form": "이런ㅋ부럽네요ㅋㅋ. 펜싱도 보셨죠?",
          "speaker_id": "2",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015769.1.13",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015769.1.14",
          "form": "어떻게 다들 잘생겼죠",
          "original_form": "어떻게 다들 잘생겼죠",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015769.1.15",
          "form": "다들 승무원하고 결혼하셨더라구요",
          "original_form": "다들 승무원하고 결혼하셨더라구요",
          "speaker_id": "2",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015769.1.16",
          "form": "참 하나만 잘해도 우레같은 박수를 받는데 잘생기기까지 하고",
          "original_form": "참 하나만 잘해도 우레같은 박수를 받는데 잘생기기까지 하고",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015769.1.17",
          "form": "인기가 있을수 밖에 없을듯 해요",
          "original_form": "인기가 있을수 밖에 없을듯 해요",
          "speaker_id": "2",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100015769.1.18",
          "form": "마장요 ㅎㅎㅎㅎ",
          "original_form": "마장요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:46"
        }
      ]
