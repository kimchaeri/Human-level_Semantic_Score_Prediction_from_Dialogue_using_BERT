      "utterance": [
        {
          "id": "MDRW2100033115.1.1",
          "form": "폰 갤럭시 쓰시나요?",
          "original_form": "폰 갤럭시 쓰시나요?",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033115.1.2",
          "form": "플립3 어떻게",
          "original_form": "플립3 어떻게",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033115.1.3",
          "form": "생각하세요 !!!@",
          "original_form": "생각하세요 !!!@",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033115.1.4",
          "form": "저 광고도 봤는데!!",
          "original_form": "저 광고도 봤는데!!",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033115.1.5",
          "form": "근데 디자인이 예쁘더라고요",
          "original_form": "근데 디자인이 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033115.1.6",
          "form": "실제로도 보셨어요?",
          "original_form": "실제로도 보셨어요?",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033115.1.7",
          "form": "아뇨.. 작년에 플립 산 애는 봤는데",
          "original_form": "아뇨.. 작년에 플립 산 애는 봤는데",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100033115.1.8",
          "form": "이번엔 못 봤어요 ㅜㅜ.",
          "original_form": "이번엔 못 봤어요 ㅜㅜ.",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033115.1.9",
          "form": "이번에는 좀 핫한 거 같아요",
          "original_form": "이번에는 좀 핫한 거 같아요",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033115.1.10",
          "form": "작년보다 인기가 많은 느낌,,?",
          "original_form": "작년보다 인기가 많은 느낌,,?",
          "speaker_id": "2",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100033115.1.11",
          "form": "오... 애플 쓰는 분들도 괜찮게 나왔다 하던데",
          "original_form": "오... 애플 쓰는 분들도 괜찮게 나왔다 하던데",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033115.1.12",
          "form": "진짠가봐요",
          "original_form": "진짠가봐요",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033115.1.13",
          "form": "네 그러니까요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 그러니까요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033115.1.14",
          "form": "사고싶진 않으세여?",
          "original_form": "사고싶진 않으세여?",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033115.1.15",
          "form": "음 사곤 싶은데",
          "original_form": "음 사곤 싶은데",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033115.1.16",
          "form": "비용도 있고..",
          "original_form": "비용도 있고..",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033115.1.17",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033115.1.18",
          "form": "요즘 폰 가격이 어마어마",
          "original_form": "요즘 폰 가격이 어마어마",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033115.1.19",
          "form": "그래도 예뻐서 고민되더라구요",
          "original_form": "그래도 예뻐서 고민되더라구요",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033115.1.20",
          "form": "그니까요.. 요즘 갤럭시가 뭐 많이 내네요 ㅎㅎ",
          "original_form": "그니까요.. 요즘 갤럭시가 뭐 많이 내네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033115.1.21",
          "form": "워치랑.. 폴드 플립..",
          "original_form": "워치랑.. 폴드 플립..",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033115.1.22",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033115.1.23",
          "form": "빠짝내는 거 같아여",
          "original_form": "빠짝내는 거 같아여",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033115.1.24",
          "form": "그래도 인기가 꽤 많더라구요",
          "original_form": "그래도 인기가 꽤 많더라구요",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033115.1.25",
          "form": "글게요.. 아마 잘 나가겠죠 ㅋㅋㅋ",
          "original_form": "글게요.. 아마 잘 나가겠죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033115.1.26",
          "form": "많이 사실 듯..",
          "original_form": "많이 사실 듯..",
          "speaker_id": "1",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100033115.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 그럴 거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그럴 거 같아요",
