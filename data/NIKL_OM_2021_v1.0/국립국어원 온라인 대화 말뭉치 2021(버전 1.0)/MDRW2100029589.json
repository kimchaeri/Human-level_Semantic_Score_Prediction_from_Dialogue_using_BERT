      "utterance": [
        {
          "id": "MDRW2100029589.1.1",
          "form": "날이 추워지는데",
          "original_form": "날이 추워지는데",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029589.1.2",
          "form": "추위 많이 타는 인간은 괜찮은지",
          "original_form": "추위 많이 타는 인간은 괜찮은지",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029589.1.3",
          "form": "나 가을 이불로 바꾸고 문 열어두지 않아",
          "original_form": "나 가을 이불로 바꾸고 문 열어두지 않아",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.4",
          "form": "매우 춥기 때문이다",
          "original_form": "매우 춥기 때문이다",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.5",
          "form": "문 안 열면 덥지 않아?",
          "original_form": "문 안 열면 덥지 않아?",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.6",
          "form": "오늘이 코트입을 수 있는 최적의 날씨인데",
          "original_form": "오늘이 코트입을 수 있는 최적의 날씨인데",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.7",
          "form": "아냐 난 딱좋아",
          "original_form": "아냐 난 딱좋아",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.8",
          "form": "약간 열면 춥고 닫으면 더워서",
          "original_form": "약간 열면 춥고 닫으면 더워서",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.9",
          "form": "열어두고 봄가을 이불 덮는중인데",
          "original_form": "열어두고 봄가을 이불 덮는중인데",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.10",
          "form": "물론 밤에 이불과 함께하다보면 더울때가 있는데",
          "original_form": "물론 밤에 이불과 함께하다보면 더울때가 있는데",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.11",
          "form": "역시 더위인간과 추위인간의 차이",
          "original_form": "역시 더위인간과 추위인간의 차이",
          "speaker_id": "1",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.12",
          "form": "그렇다고 문을 열면 너무 추워서",
          "original_form": "그렇다고 문을 열면 너무 추워서",
          "speaker_id": "2",
          "time": "20210903 10:48"
        },
        {
          "id": "MDRW2100029589.1.13",
          "form": "그냥 일어나서 열기 빼고 다시 누워",
          "original_form": "그냥 일어나서 열기 빼고 다시 누워",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029589.1.14",
          "form": "긴팔 맨투맨 입었다가 나는 땀이 나더라",
          "original_form": "긴팔 맨투맨 입었다가 나는 땀이 나더라",
          "speaker_id": "1",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029589.1.15",
          "form": "역시 부산 내려와서 사는건 어떤지?",
          "original_form": "역시 부산 내려와서 사는건 어떤지?",
          "speaker_id": "1",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029589.1.16",
          "form": "부산라인은 아직 더울만도",
          "original_form": "부산라인은 아직 더울만도",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029589.1.17",
          "form": "여긴 넘 춥따...",
          "original_form": "여긴 넘 춥따...",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029589.1.18",
          "form": "낮은 뜨겁고",
          "original_form": "낮은 뜨겁고",
          "speaker_id": "1",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029589.1.19",
          "form": "밤은 추워",
          "original_form": "밤은 추워",
          "speaker_id": "1",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029589.1.20",
          "form": "역시 난 수도권체질인가봐",
          "original_form": "역시 난 수도권체질인가봐",
          "speaker_id": "1",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029589.1.21",
          "form": "여기도 얼마전까지는 그랬는데 이젠 춥뜨아...",
          "original_form": "여기도 얼마전까지는 그랬는데 이젠 춥뜨아...",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029589.1.22",
          "form": "내가 안나가서 그런걸지도",
          "original_form": "내가 안나가서 그런걸지도",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029589.1.23",
          "form": "진짜 추위 많이 탄다",
          "original_form": "진짜 추위 많이 탄다",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029589.1.24",
          "form": "난 이제 일교차 커서 옷을 못 고르겠어",
          "original_form": "난 이제 일교차 커서 옷을 못 고르겠어",
          "speaker_id": "1",
          "time": "20210903 10:59"
        },
        {
          "id": "MDRW2100029589.1.25",
          "form": "친구는 그래서 반팔에 긴팔 겉옷 챙긴대",
          "original_form": "친구는 그래서 반팔에 긴팔 겉옷 챙긴대",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029589.1.26",
          "form": "나이스 선택인거같아",
          "original_form": "나이스 선택인거같아",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029589.1.27",
          "form": "맞아 겉옷이 그래도 괜찮은 선택인듯",
          "original_form": "맞아 겉옷이 그래도 괜찮은 선택인듯",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029589.1.28",
          "form": "내가 패션용 겉옷을 챙기지만 않는다면...",
          "original_form": "내가 패션용 겉옷을 챙기지만 않는다면...",
          "speaker_id": "1",
          "time": "20210903 11:39"
        }
      ]
