      "utterance": [
        {
          "id": "MDRW2100047511.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100047511.1.2",
          "form": "안녕하세요 해외여행 어디 가 보셨어요?",
          "original_form": "안녕하세요 해외여행 어디 가 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100047511.1.3",
          "form": "못가봤죠ㅎ",
          "original_form": "못가봤죠ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047511.1.4",
          "form": "코로나!",
          "original_form": "코로나!",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047511.1.5",
          "form": "코로나 때문에 어딜 가질 못하네요ㅠ",
          "original_form": "코로나 때문에 어딜 가질 못하네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100047511.1.6",
          "form": "국내 여행 가 본 곳 중에 기억에 남는 곳 있나요?",
          "original_form": "국내 여행 가 본 곳 중에 기억에 남는 곳 있나요?",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100047511.1.7",
          "form": "전 곡성도좋았어요",
          "original_form": "전 곡성도좋았어요",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100047511.1.8",
          "form": "처음 들어봤어요. 어디쯤 인가요?",
          "original_form": "처음 들어봤어요. 어디쯤 인가요?",
          "speaker_id": "1",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100047511.1.9",
          "form": "여수 옆에요ㅋㅋㅋㅋ",
          "original_form": "여수 옆에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100047511.1.10",
          "form": "영화도있잖아요ㄷㄷ",
          "original_form": "영화도있잖아요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100047511.1.11",
          "form": "처음들으셨나요?",
          "original_form": "처음들으셨나요?",
          "speaker_id": "2",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100047511.1.12",
          "form": "아 곡성이요ㅋㅋ 도까지 이어서 봐서 섬인가 했어요ㅋㅋㅋㅋ",
          "original_form": "아 곡성이요ㅋㅋ 도까지 이어서 봐서 섬인가 했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100047511.1.13",
          "form": "곡성에선 어디 가셨어요?",
          "original_form": "곡성에선 어디 가셨어요?",
          "speaker_id": "1",
          "time": "20210827 01:16"
        },
        {
          "id": "MDRW2100047511.1.14",
          "form": "ㅋㅋㅋ 어딜딱히갈곳은없더라구요",
          "original_form": "ㅋㅋㅋ 어딜딱히갈곳은없더라구요",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047511.1.15",
          "form": "여수간김에 가봤어요",
          "original_form": "여수간김에 가봤어요",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047511.1.16",
          "form": "그냥 풍경이 아름다운?",
          "original_form": "그냥 풍경이 아름다운?",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047511.1.17",
          "form": "ㅋㅋㅋㅋ 내일로갔다가",
          "original_form": "ㅋㅋㅋㅋ 내일로갔다가",
          "speaker_id": "2",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100047511.1.18",
          "form": "옆이라 그냥가봤어요ㅋㅋㅋ",
          "original_form": "옆이라 그냥가봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100047511.1.19",
          "form": "전 내일로 때 여수는 못 갔고 순천 갔었어요. 순천만 좋더라구요",
          "original_form": "전 내일로 때 여수는 못 갔고 순천 갔었어요. 순천만 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100047511.1.20",
          "form": "오 맞아요 순천 죽이죠",
          "original_form": "오 맞아요 순천 죽이죠",
          "speaker_id": "2",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100047511.1.21",
          "form": "전 순천ㅡ여수ㅡ곡성갔어요ㅋㅋㅋ",
          "original_form": "전 순천ㅡ여수ㅡ곡성갔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100047511.1.22",
          "form": "순천만 그림같더라고요",
          "original_form": "순천만 그림같더라고요",
          "speaker_id": "2",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100047511.1.23",
          "form": "언젠가 다시 가보고 싶네요ㅋㅋ",
          "original_form": "언젠가 다시 가보고 싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:34"
        }
      ]
    }
