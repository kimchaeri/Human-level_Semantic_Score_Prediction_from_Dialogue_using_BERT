      "utterance": [
        {
          "id": "MDRW2100003242.1.1",
          "form": "혹시 키우시는 동물 있어요?",
          "original_form": "혹시 키우시는 동물 있어요?",
          "speaker_id": "1",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003242.1.2",
          "form": "아니요 부모님이 애완견 키우세요",
          "original_form": "아니요 부모님이 애완견 키우세요",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003242.1.3",
          "form": "지인님은요?",
          "original_form": "지인님은요?",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003242.1.4",
          "form": "저희집은 강아지 키워요",
          "original_form": "저희집은 강아지 키워요",
          "speaker_id": "1",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003242.1.5",
          "form": "강아지 종류가 어떻게되세요?",
          "original_form": "강아지 종류가 어떻게되세요?",
          "speaker_id": "1",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003242.1.6",
          "form": "믹스견인데 어디서 데려온거라 아무리봐도 종을 모르겠더라구요ㅎㄴㅅ",
          "original_form": "믹스견인데 어디서 데려온거라 아무리봐도 종을 모르겠더라구요ㅎㄴㅅ",
          "speaker_id": "2",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003242.1.7",
          "form": "무슨종키우세요?",
          "original_form": "무슨종키우세요?",
          "speaker_id": "2",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003242.1.8",
          "form": "저희집은 시츄키우는데",
          "original_form": "저희집은 시츄키우는데",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003242.1.9",
          "form": "애완견 나이가 어떻게 되시나요?",
          "original_form": "애완견 나이가 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003242.1.10",
          "form": "시츄 귀엽죠 저희도 키웠었는데 3년전에 늙어서 보내줬네요 ㅠ",
          "original_form": "시츄 귀엽죠 저희도 키웠었는데 3년전에 늙어서 보내줬네요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003242.1.11",
          "form": "아... 저희집도 이제 강아지 나이가 9살이네요...",
          "original_form": "아... 저희집도 이제 강아지 나이가 9살이네요...",
          "speaker_id": "1",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003242.1.12",
          "form": "지금 키우는건 16년 정도된거같아요 오늘내일하구 있어서 걱정이예요",
          "original_form": "지금 키우는건 16년 정도된거같아요 오늘내일하구 있어서 걱정이예요",
          "speaker_id": "2",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003242.1.13",
          "form": "그래도 16년정도면 오래살긴 했네요",
          "original_form": "그래도 16년정도면 오래살긴 했네요",
          "speaker_id": "1",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003242.1.14",
          "form": "어디 아픈곳 있어요?",
          "original_form": "어디 아픈곳 있어요?",
          "speaker_id": "1",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003242.1.15",
          "form": "노령견이라 아무래도 심장도 안좋고",
          "original_form": "노령견이라 아무래도 심장도 안좋고",
          "speaker_id": "2",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003242.1.16",
          "form": "자궁쪽도 안좋은지 자꾸 뭐나오고 그러네요",
          "original_form": "자궁쪽도 안좋은지 자꾸 뭐나오고 그러네요",
          "speaker_id": "2",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003242.1.17",
          "form": "저희집 강아지도 가슴쪽에 혹이 하나 났더라구요",
          "original_form": "저희집 강아지도 가슴쪽에 혹이 하나 났더라구요",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003242.1.18",
          "form": "제거는 아직 안해도 되긴 한다던데",
          "original_form": "제거는 아직 안해도 되긴 한다던데",
          "speaker_id": "1",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003242.1.19",
          "form": "악성만 아니면 괜찮지 않을까요?",
          "original_form": "악성만 아니면 괜찮지 않을까요?",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003242.1.20",
          "form": "그래도 8살까지는 병같은게 없었는데 생겨서 그런지 걱정은 되더라구요",
          "original_form": "그래도 8살까지는 병같은게 없었는데 생겨서 그런지 걱정은 되더라구요",
          "speaker_id": "1",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003242.1.21",
          "form": "8살이면 그래도 꽤 나이먹어서 건강 챙기긴 해야겠어요",
          "original_form": "8살이면 그래도 꽤 나이먹어서 건강 챙기긴 해야겠어요",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003242.1.22",
          "form": "한마리 키우시는거예요?",
          "original_form": "한마리 키우시는거예요?",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003242.1.23",
          "form": "넵 시츄 한마리키워요",
          "original_form": "넵 시츄 한마리키워요",
          "speaker_id": "1",
          "time": "20210820 21:21"
        }
      ]
    }
