        },
        {
          "id": "MDRW2100019100.1.2",
          "form": "갑자기요?",
          "original_form": "갑자기요?",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019100.1.3",
          "form": "여름마다  여행갔었는데 못갔네요",
          "original_form": "여름마다  여행갔었는데 못갔네요",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019100.1.4",
          "form": "가을이나 겨울에 한번 다녀오세요",
          "original_form": "가을이나 겨울에 한번 다녀오세요",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019100.1.5",
          "form": "그럴까해요",
          "original_form": "그럴까해요",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019100.1.6",
          "form": "전 섬진강 일주 이런 거 해보고싶어요",
          "original_form": "전 섬진강 일주 이런 거 해보고싶어요",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019100.1.7",
          "form": "날 선선해지면 너무 좋겠어요",
          "original_form": "날 선선해지면 너무 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019100.1.8",
          "form": "네 단풍구경도 가고",
          "original_form": "네 단풍구경도 가고",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019100.1.9",
          "form": "단풍구경 너무 좋네요",
          "original_form": "단풍구경 너무 좋네요",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019100.1.10",
          "form": "제주도도 가고싶어요",
          "original_form": "제주도도 가고싶어요",
          "speaker_id": "2",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019100.1.11",
          "form": "저도 제주도 한달살기 이런 거 너무 해보고싶어요",
          "original_form": "저도 제주도 한달살기 이런 거 너무 해보고싶어요",
