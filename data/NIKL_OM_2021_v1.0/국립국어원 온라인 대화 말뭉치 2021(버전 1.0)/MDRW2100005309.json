      "utterance": [
        {
          "id": "MDRW2100005309.1.1",
          "form": "피자를 많이 좋아하시나 보네용~",
          "original_form": "피자를 많이 좋아하시나 보네용~",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005309.1.2",
          "form": "무슨 피자 좋아하세요?",
          "original_form": "무슨 피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005309.1.3",
          "form": "전 코스트코 가면 주로 불고기 위주로 먹었어요",
          "original_form": "전 코스트코 가면 주로 불고기 위주로 먹었어요",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005309.1.4",
          "form": "불고기 베이크도 좋아하구요!",
          "original_form": "불고기 베이크도 좋아하구요!",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005309.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005309.1.6",
          "form": "저도 불고기 피자요!!ㅎㅎ 베이크도 좋아해요!",
          "original_form": "저도 불고기 피자요!!ㅎㅎ 베이크도 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005309.1.7",
          "form": "전 치킨 베이크도 좋아해요 ㅎㅎ",
          "original_form": "전 치킨 베이크도 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005309.1.8",
          "form": "아 치킨 베이크도 엄청 맛있죠ㅎㅎㅎ",
          "original_form": "아 치킨 베이크도 엄청 맛있죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005309.1.9",
          "form": "코스트코는 베이크랑 피자가 엄청 싸고 좋았어요ㅎㅎㅎ",
          "original_form": "코스트코는 베이크랑 피자가 엄청 싸고 좋았어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005309.1.10",
          "form": "예전엔 코스트코에 양파도 있어서 좋았는데",
          "original_form": "예전엔 코스트코에 양파도 있어서 좋았는데",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005309.1.11",
          "form": "요즙은 양파는 없더라구요?ㅠㅠ",
          "original_form": "요즙은 양파는 없더라구요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005309.1.12",
          "form": "앗 맞아요 ㅠㅠ 양파랑 같이먹어야 진짜 맛난뎅",
          "original_form": "앗 맞아요 ㅠㅠ 양파랑 같이먹어야 진짜 맛난뎅",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005309.1.13",
          "form": "넘 아쉬워용 ㅋㅋㅋ 요새 푸드코트에서 먹는것도 안되고 ㅠㅠ 나쁜 코로나",
          "original_form": "넘 아쉬워용 ㅋㅋㅋ 요새 푸드코트에서 먹는것도 안되고 ㅠㅠ 나쁜 코로나",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005309.1.14",
          "form": "그쵸ㅠㅠ 양파를 왜 안주는건지",
          "original_form": "그쵸ㅠㅠ 양파를 왜 안주는건지",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005309.1.15",
          "form": "ㅠㅠ 아쉽아쉽 집에서 하면 그 맛이 안나는데 ㅠㅋㅋ",
          "original_form": "ㅠㅠ 아쉽아쉽 집에서 하면 그 맛이 안나는데 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005309.1.16",
          "form": "맞아요!ㅠㅠ 집에서 하면 양파 그렇게 다지는 것도 일이구...",
          "original_form": "맞아요!ㅠㅠ 집에서 하면 양파 그렇게 다지는 것도 일이구...",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005309.1.17",
          "form": "넹 ㅠㅠ언젠간 부활하길 ,,ㅠㅋㅋ",
          "original_form": "넹 ㅠㅠ언젠간 부활하길 ,,ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005309.1.18",
          "form": "피자 급 땡기네요 내일이라두 먹어야겠어요 ㅋㅋ",
          "original_form": "피자 급 땡기네요 내일이라두 먹어야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005309.1.19",
          "form": "좋죠ㅋㅋㅋ 저도 피자가 먹고싶네요ㅠㅠ",
          "original_form": "좋죠ㅋㅋㅋ 저도 피자가 먹고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005309.1.20",
          "form": "코스트코 가까우면 코스트코 피자 먹고싶어요",
          "original_form": "코스트코 가까우면 코스트코 피자 먹고싶어요",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005309.1.21",
          "form": "짜고 느끼하고 양많고 ㅋㅋ",
          "original_form": "짜고 느끼하고 양많고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005309.1.22",
          "form": "맞아요 ㅠㅋㅋㅋㅋㅋ 제대로 포만감있게 먹을수있져",
          "original_form": "맞아요 ㅠㅋㅋㅋㅋㅋ 제대로 포만감있게 먹을수있져",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005309.1.23",
          "form": "저두 차타고 좀 가야해서 ㅠㅠ자주 못가서 탈이에요",
          "original_form": "저두 차타고 좀 가야해서 ㅠㅠ자주 못가서 탈이에요",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005309.1.24",
          "form": "앗 저두요ㅠㅠ 제가 사는 지역은 코스트코가 두군데있는데 둘 다 머네요",
          "original_form": "앗 저두요ㅠㅠ 제가 사는 지역은 코스트코가 두군데있는데 둘 다 머네요",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005309.1.25",
          "form": "좀 가까우면 더 자주 먹었을거같아요",
          "original_form": "좀 가까우면 더 자주 먹었을거같아요",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005309.1.26",
          "form": "ㅠㅠㅠ 한번씩 큰 맘 먹고 가야되네용",
          "original_form": "ㅠㅠㅠ 한번씩 큰 맘 먹고 가야되네용",
