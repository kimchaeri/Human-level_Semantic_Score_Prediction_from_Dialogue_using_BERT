      "utterance": [
        {
          "id": "MDRW2100014288.1.1",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014288.1.2",
          "form": "게임은 별로 안좋아해요ㅎㅎ게임하세요?",
          "original_form": "게임은 별로 안좋아해요ㅎㅎ게임하세요?",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014288.1.3",
          "form": "아뇨 그럼 학창시절에는 하셨나요?",
          "original_form": "아뇨 그럼 학창시절에는 하셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014288.1.4",
          "form": "캔디크러시사가 이런거 많이 했었죠",
          "original_form": "캔디크러시사가 이런거 많이 했었죠",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014288.1.5",
          "form": "모두의마블이랑ㅋㅋㅋ",
          "original_form": "모두의마블이랑ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014288.1.6",
          "form": "아 예전에 무슨 팡..애니팡인가요 그것도인기였죠",
          "original_form": "아 예전에 무슨 팡..애니팡인가요 그것도인기였죠",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014288.1.7",
          "form": "맞아요 애니팡ㅋㅋ",
          "original_form": "맞아요 애니팡ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014288.1.8",
          "form": "그땐 유명한 게임이 많았는데 요새는 눈에 띄는게 없는거 같아요ㅜ",
          "original_form": "그땐 유명한 게임이 많았는데 요새는 눈에 띄는게 없는거 같아요ㅜ",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014288.1.9",
          "form": "저는 예전에 넷마블 테트리스..했던 기억만..",
          "original_form": "저는 예전에 넷마블 테트리스..했던 기억만..",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014288.1.10",
          "form": "ㅋㅋpc 게임 말씀하시는거죠? 그거 추억이네요ㅎㅎ",
          "original_form": "ㅋㅋpc 게임 말씀하시는거죠? 그거 추억이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014288.1.11",
          "form": "네... 진짜 오래전이죠",
          "original_form": "네... 진짜 오래전이죠",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014288.1.12",
          "form": "ㅋㅋ요새 괜찮은 게임은 아세요?",
          "original_form": "ㅋㅋ요새 괜찮은 게임은 아세요?",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014288.1.13",
          "form": "아뇨 게임엔 문외한이네요 ㅠ",
          "original_form": "아뇨 게임엔 문외한이네요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014288.1.14",
          "form": "ㅋㅋ저도요 마지막으로 남동생이 한다고 했던게 배틀그라운드 였네요ㅋㅋㅋ",
          "original_form": "ㅋㅋ저도요 마지막으로 남동생이 한다고 했던게 배틀그라운드 였네요ㅋㅋㅋ",
