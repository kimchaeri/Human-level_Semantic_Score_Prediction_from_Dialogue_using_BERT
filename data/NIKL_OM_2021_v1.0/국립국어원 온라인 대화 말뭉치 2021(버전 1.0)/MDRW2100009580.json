      "utterance": [
        {
          "id": "MDRW2100009580.1.1",
          "form": "오 의외로 한국작품이네요,",
          "original_form": "오 의외로 한국작품이네요,",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009580.1.2",
          "form": "연재물임?",
          "original_form": "연재물임?",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009580.1.3",
          "form": "아 작가님이 한국사람인데",
          "original_form": "아 작가님이 한국사람인데",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009580.1.4",
          "form": "일본에서 연재중이셔",
          "original_form": "일본에서 연재중이셔",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009580.1.5",
          "form": "그래서 수입한것임 ㅎㅎ",
          "original_form": "그래서 수입한것임 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009580.1.6",
          "form": "인재를 몰라보고 한국출판사 반성하세욧!",
          "original_form": "인재를 몰라보고 한국출판사 반성하세욧!",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009580.1.7",
          "form": "뭐지 이 역수입은 ㅋㅋ",
          "original_form": "뭐지 이 역수입은 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009580.1.8",
          "form": "왜 신암행어사 만화도 그랬다이가",
          "original_form": "왜 신암행어사 만화도 그랬다이가",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009580.1.9",
          "form": "양경일 그림",
          "original_form": "양경일 그림",
          "speaker_id": "1",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009580.1.10",
          "form": "한국에서 차였는데 일본연재하고 역수입한케이스",
          "original_form": "한국에서 차였는데 일본연재하고 역수입한케이스",
          "speaker_id": "1",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009580.1.11",
          "form": "name2님 최애 만화는 뭔가요? 하이큐?",
          "original_form": "&name2&님 최애 만화는 뭔가요? 하이큐?",
          "speaker_id": "1",
          "time": "20210823 09:35"
        },
        {
          "id": "MDRW2100009580.1.12",
          "form": "음 호오즈키는 냉철?",
          "original_form": "음 호오즈키는 냉철?",
          "speaker_id": "2",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009580.1.13",
          "form": "최근 완결 났어요",
          "original_form": "최근 완결 났어요",
          "speaker_id": "2",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009580.1.14",
          "form": "아하 안봤네",
          "original_form": "아하 안봤네",
          "speaker_id": "1",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009580.1.15",
          "form": "최근완결이라니 꽤 오래 연재 했네!",
          "original_form": "최근완결이라니 꽤 오래 연재 했네!",
          "speaker_id": "1",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009580.1.16",
          "form": "나중에 찾아봐야지 애니화되었다면 왓챠에있음ㅈㆍㅎ겠다",
          "original_form": "나중에 찾아봐야지 애니화되었다면 왓챠에있음ㅈㆍㅎ겠다",
          "speaker_id": "1",
          "time": "20210823 09:38"
        },
        {
          "id": "MDRW2100009580.1.17",
          "form": "있을거에요 웨이브에서는 봤어요",
          "original_form": "있을거에요 웨이브에서는 봤어요",
          "speaker_id": "2",
          "time": "20210823 09:49"
        },
        {
          "id": "MDRW2100009580.1.18",
          "form": "물론 만화책이 완결이지만ㅜㅜ",
          "original_form": "물론 만화책이 완결이지만ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 09:49"
        },
        {
          "id": "MDRW2100009580.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100009580.1.20",
          "form": "팬으로서는 기다림이 있어서 좋지않어?",
          "original_form": "팬으로서는 기다림이 있어서 좋지않어?",
          "speaker_id": "1",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100009580.1.21",
          "form": "사실 애니가 컬러풀하고 좋음",
          "original_form": "사실 애니가 컬러풀하고 좋음",
          "speaker_id": "2",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100009580.1.22",
          "form": "만화가 완결나버려서ㅜ",
          "original_form": "만화가 완결나버려서ㅜ",
          "speaker_id": "2",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100009580.1.23",
          "form": "애니가 소리까니 나오니 편함",
          "original_form": "애니가 소리까니 나오니 편함",
          "speaker_id": "1",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100009580.1.24",
          "form": "애니가 다음에 또나올지 모르겠음",
          "original_form": "애니가 다음에 또나올지 모르겠음",
          "speaker_id": "2",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100009580.1.25",
          "form": "완결 찾아봤어?",
          "original_form": "완결 찾아봤어?",
          "speaker_id": "1",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009580.1.26",
          "form": "ㅇㅇ 은근 이북으로 다 사 봤음 ㅋ",
          "original_form": "ㅇㅇ 은근 이북으로 다 사 봤음 ㅋ",
          "speaker_id": "2",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009580.1.27",
          "form": "최근 도서관 카드를 새로 발급 받았었는데",
          "original_form": "최근 도서관 카드를 새로 발급 받았었는데",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100009580.1.28",
          "form": "되게 좋음요",
          "original_form": "되게 좋음요",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100009580.1.29",
          "form": "음 신용카드같이생김",
          "original_form": "음 신용카드같이생김",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100009580.1.30",
          "form": "여하간 그래서 실용서를 좀빌림",
          "original_form": "여하간 그래서 실용서를 좀빌림",
          "speaker_id": "2",
          "time": "20210825 23:55"
        }
      ]
    }
  ]
