      "utterance": [
        {
          "id": "MDRW2100027099.1.1",
          "form": "여행유투버들은",
          "original_form": "여행유투버들은",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027099.1.2",
          "form": "여행도 하고 돈도벌고 부러워요",
          "original_form": "여행도 하고 돈도벌고 부러워요",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100027099.1.3",
          "form": "그러게요 부러워요 그런데 코로나때문에 많이 못갈것같긴해요",
          "original_form": "그러게요 부러워요 그런데 코로나때문에 많이 못갈것같긴해요",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027099.1.4",
          "form": "그래서 유투브로",
          "original_form": "그래서 유투브로",
          "speaker_id": "1",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027099.1.5",
          "form": "여행하는 기분 내는거 같아요",
          "original_form": "여행하는 기분 내는거 같아요",
          "speaker_id": "1",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100027099.1.6",
          "form": "그렇네요 영화는 어떤 장르 좋아하세요?",
          "original_form": "그렇네요 영화는 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100027099.1.7",
          "form": "다 잘보는데",
          "original_form": "다 잘보는데",
          "speaker_id": "1",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100027099.1.8",
          "form": "공포나 해리포터시리즈 좋아해요",
          "original_form": "공포나 해리포터시리즈 좋아해요",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100027099.1.9",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100027099.1.10",
          "form": "저도 공포나 마블시리즈 액션도 좋아하구요",
          "original_form": "저도 공포나 마블시리즈 액션도 좋아하구요",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100027099.1.11",
          "form": "넷플릭스는 이용하세요?",
          "original_form": "넷플릭스는 이용하세요?",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100027099.1.12",
          "form": "오 최근",
          "original_form": "오 최근",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100027099.1.13",
          "form": "샹치 개봉하더라구요",
          "original_form": "샹치 개봉하더라구요",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100027099.1.14",
          "form": "어떨거같아요?",
          "original_form": "어떨거같아요?",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100027099.1.15",
          "form": "재밌을것같아요 반응은 어떤지 모르겠네요",
          "original_form": "재밌을것같아요 반응은 어떤지 모르겠네요",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027099.1.16",
          "form": "평은 괜찮아 보여요",
          "original_form": "평은 괜찮아 보여요",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027099.1.17",
          "form": "저는 주말에 볼것같아요",
          "original_form": "저는 주말에 볼것같아요",
          "speaker_id": "1",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100027099.1.18",
          "form": "그렇군요 저도 조만간 보러가려구요 또 볼 예정이신 영화 있으세요?",
          "original_form": "그렇군요 저도 조만간 보러가려구요 또 볼 예정이신 영화 있으세요?",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100027099.1.19",
          "form": "맨인더다크 아시나요",
          "original_form": "맨인더다크 아시나요",
          "speaker_id": "1",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100027099.1.20",
          "form": "예 알아요 재밌게봤었어요",
          "original_form": "예 알아요 재밌게봤었어요",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027099.1.21",
          "form": "2가 나온다는대",
          "original_form": "2가 나온다는대",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027099.1.22",
          "form": "진짜 기대되요",
          "original_form": "진짜 기대되요",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027099.1.23",
          "form": "맨인더다크2 넘 기대돼요",
          "original_form": "맨인더다크2 넘 기대돼요",
