      "utterance": [
        {
          "id": "MDRW2100020128.1.1",
          "form": "코코미 얻으려면",
          "original_form": "코코미 얻으려면",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020128.1.2",
          "form": "지금부터 게임 시작해야해",
          "original_form": "지금부터 게임 시작해야해",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020128.1.3",
          "form": "하고..있..아니 게임 그 뭐야 렙업을 해야 한다는거지? 이나즈미에 가려면 한참 남았을거 같은걸...",
          "original_form": "하고..있..아니 게임 그 뭐야 렙업을 해야 한다는거지? 이나즈미에 가려면 한참 남았을거 같은걸...",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020128.1.4",
          "form": "라이덴도 예브네..",
          "original_form": "라이덴도 예브네..",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020128.1.5",
          "form": "ㅇㅇ 렙업해",
          "original_form": "ㅇㅇ 렙업해",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020128.1.6",
          "form": "라이덴도 캐디 예뻐",
          "original_form": "라이덴도 캐디 예뻐",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020128.1.7",
          "form": "일단 지금은 레벨업 빨리 해야해",
          "original_form": "일단 지금은 레벨업 빨리 해야해",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020128.1.8",
          "form": "아니면",
          "original_form": "아니면",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020128.1.9",
          "form": "게임 이벤트 캐릭터 말고",
          "original_form": "게임 이벤트 캐릭터 말고",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020128.1.10",
          "form": "통상 가챠 캐중에 마음에 드는 캐디 있어?",
          "original_form": "통상 가챠 캐중에 마음에 드는 캐디 있어?",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020128.1.11",
          "form": "내일의 나 부탁해(급기야",
          "original_form": "내일의 나 부탁해(급기야",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020128.1.12",
          "form": "통상가챠는 모르겠는데 함 찾아봐야",
          "original_form": "통상가챠는 모르겠는데 함 찾아봐야",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020128.1.13",
          "form": "이 게임이 캐릭터 디자인은 잘 뽑아",
          "original_form": "이 게임이 캐릭터 디자인은 잘 뽑아",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020128.1.14",
          "form": "쓰리디 모델링도 있고",
          "original_form": "쓰리디 모델링도 있고",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020128.1.15",
          "form": "글쿤.. 음.. 네이버에 치면 다 나오는게 아닌가 리사도 예쁘고 리사 뭔가 우리나라 게임 그 이름 사이퍼즈 나오는 친구 닮았는데",
          "original_form": "글쿤.. 음.. 네이버에 치면 다 나오는게 아닌가 리사도 예쁘고 리사 뭔가 우리나라 게임 그 이름 사이퍼즈 나오는 친구 닮았는데",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020128.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020128.1.17",
          "form": "리사는 기본 제공되는 캐릭터야",
          "original_form": "리사는 기본 제공되는 캐릭터야",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020128.1.18",
          "form": "스토리 좀 만 더 진행하면 얻을 수 있음",
          "original_form": "스토리 좀 만 더 진행하면 얻을 수 있음",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020128.1.19",
          "form": "ㅇㅎ 그래.. 가챠 캐릭터를 찾아봐야겠..",
          "original_form": "ㅇㅎ 그래.. 가챠 캐릭터를 찾아봐야겠..",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020128.1.20",
          "form": "5성 뽑고 갈거야?",
          "original_form": "5성 뽑고 갈거야?",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020128.1.21",
          "form": "섯섯은 리세 귀찮다고",
          "original_form": "섯섯은 리세 귀찮다고",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020128.1.22",
          "form": "그냥 성능 좋은 4성 썼어",
          "original_form": "그냥 성능 좋은 4성 썼어",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020128.1.23",
          "form": "고민 중인데..일단 지금 임시 이메일로 만든 계정이거든 함 뽑아보고",
          "original_form": "고민 중인데..일단 지금 임시 이메일로 만든 계정이거든 함 뽑아보고",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020128.1.24",
          "form": "좋으면 가고 아니면",
          "original_form": "좋으면 가고 아니면",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020128.1.25",
          "form": "쿨...하게...리셋...",
          "original_form": "쿨...하게...리셋...",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020128.1.26",
          "form": "그랭",
          "original_form": "그랭",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020128.1.27",
          "form": "얼른 7레벨을 만들어봐",
          "original_form": "얼른 7레벨을 만들어봐",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020128.1.28",
          "form": "응 조아조아... 그리고 취향은 종려랑 라이덴 코코미..빼면..묘하게 다들 미스인데 암튼 더 보겠슴다",
          "original_form": "응 조아조아... 그리고 취향은 종려랑 라이덴 코코미..빼면..묘하게 다들 미스인데 암튼 더 보겠슴다",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020128.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ 취향이 전부 5성이라니",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 취향이 전부 5성이라니",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020128.1.30",
          "form": "하지만 이 게임은 5성 캐릭터 디자인에 영혼을 갈아넣었기 때문에 이상하지 않지",
          "original_form": "하지만 이 게임은 5성 캐릭터 디자인에 영혼을 갈아넣었기 때문에 이상하지 않지",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020128.1.31",
          "form": "난 타르탈리아 캐릭터도 좋아해",
          "original_form": "난 타르탈리아 캐릭터도 좋아해",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020128.1.32",
          "form": "모델링은 애매한데 일러가 예쁨",
          "original_form": "모델링은 애매한데 일러가 예쁨",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020128.1.33",
          "form": "그러니까..왜..5성에..갈아드셨는지?ㅋㅋㅋㅋ 타르탈리아 아 갈색",
          "original_form": "그러니까..왜..5성에..갈아드셨는지?ㅋㅋㅋㅋ 타르탈리아 아 갈색",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020128.1.34",
          "form": "바바라? 도 귀여워보이긴해",
          "original_form": "바바라? 도 귀여워보이긴해",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020128.1.35",
          "form": "타르탈리아 주황색이야 ㅋㅋㅋㅋ",
          "original_form": "타르탈리아 주황색이야 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020128.1.36",
          "form": "바바라는 모델링이 예뻐",
          "original_form": "바바라는 모델링이 예뻐",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020128.1.37",
          "form": "바바라도 기본 지급 캐릭터",
          "original_form": "바바라도 기본 지급 캐릭터",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020128.1.38",
          "form": "하지만 추천은 안하나(위키 읽는 중",
          "original_form": "하지만 추천은 안하나(위키 읽는 중",
