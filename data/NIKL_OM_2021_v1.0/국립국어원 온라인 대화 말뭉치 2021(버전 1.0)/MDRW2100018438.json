      "utterance": [
        {
          "id": "MDRW2100018438.1.1",
          "form": "그러지 않을까요ㅎㅎ",
          "original_form": "그러지 않을까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100018438.1.2",
          "form": "혹시 맹장 수술해보셨어요?ㅎ",
          "original_form": "혹시 맹장 수술해보셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100018438.1.3",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018438.1.4",
          "form": "수술 하시나요",
          "original_form": "수술 하시나요",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100018438.1.5",
          "form": "아니요ㅎ 증상이 어떤가해서",
          "original_form": "아니요ㅎ 증상이 어떤가해서",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018438.1.6",
          "form": "어제부터 오른쪽 아랫배가 콕콕쑤시는데 맹장인가싶어서 여쭤봤어요ㅎ",
          "original_form": "어제부터 오른쪽 아랫배가 콕콕쑤시는데 맹장인가싶어서 여쭤봤어요ㅎ",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018438.1.7",
          "form": "ㅋㅋ아",
          "original_form": "ㅋㅋ아",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018438.1.8",
          "form": "그 여성분들 한달에 한번 오는",
          "original_form": "그 여성분들 한달에 한번 오는",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018438.1.9",
          "form": "그거 아닐까 싶은데요",
          "original_form": "그거 아닐까 싶은데요",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018438.1.10",
          "form": "그거면 여쭤보지도않았죠ㅋ",
          "original_form": "그거면 여쭤보지도않았죠ㅋ",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018438.1.11",
          "form": "옆구리도 쑤실려는게 신장이 문제인지",
          "original_form": "옆구리도 쑤실려는게 신장이 문제인지",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018438.1.12",
          "form": "건강하시죠?ㅎㅎ",
          "original_form": "건강하시죠?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018438.1.13",
          "form": "아 그런가 싶기도 하는데요",
          "original_form": "아 그런가 싶기도 하는데요",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018438.1.14",
          "form": "췌장인가",
          "original_form": "췌장인가",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018438.1.15",
          "form": "췌장은 감각이 없다던데요 그래서 췌장암걸림 거의 말기에발견",
          "original_form": "췌장은 감각이 없다던데요 그래서 췌장암걸림 거의 말기에발견",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018438.1.16",
          "form": "확실한정보는 아님ㅎ",
          "original_form": "확실한정보는 아님ㅎ",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018438.1.17",
          "form": "ㅋㅋ맞는거 같어요",
          "original_form": "ㅋㅋ맞는거 같어요",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100018438.1.18",
          "form": "나이먹으니 좀만 아파도 무섭ㅎ",
          "original_form": "나이먹으니 좀만 아파도 무섭ㅎ",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100018438.1.19",
          "form": "일주일 밤새 손가락운동 너무했드니 피로누적이고",
          "original_form": "일주일 밤새 손가락운동 너무했드니 피로누적이고",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100018438.1.20",
          "form": "ㅋㅋ손가락 아프실만...",
          "original_form": "ㅋㅋ손가락 아프실만...",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100018438.1.21",
          "form": "대단하세요 ㅋㅋㅋ",
          "original_form": "대단하세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100018438.1.22",
          "form": "주말은 시체처럼 암것도 안할거예요",
          "original_form": "주말은 시체처럼 암것도 안할거예요",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100018438.1.23",
          "form": "먹고 뒹굴ㅎ",
          "original_form": "먹고 뒹굴ㅎ",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100018438.1.24",
          "form": "곧 졸업하시지 않나요 ㅎㅎ",
          "original_form": "곧 졸업하시지 않나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100018438.1.25",
          "form": "푹 쉬셔도 ㅎㅎ",
          "original_form": "푹 쉬셔도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:40"
        }
      ]
    }
