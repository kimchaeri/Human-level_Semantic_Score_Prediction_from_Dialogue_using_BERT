      "utterance": [
        {
          "id": "MDRW2100003978.1.1",
          "form": "너희 재택근무한데",
          "original_form": "너희 재택근무한데",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003978.1.2",
          "form": "아니 그런말없어",
          "original_form": "아니 그런말없어",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003978.1.3",
          "form": "하면 좋을텐데",
          "original_form": "하면 좋을텐데",
          "speaker_id": "1",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003978.1.4",
          "form": "다른사람들은 말없고",
          "original_form": "다른사람들은 말없고",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003978.1.5",
          "form": "말은 하지 재택근무하면좋겠다 하고",
          "original_form": "말은 하지 재택근무하면좋겠다 하고",
          "speaker_id": "1",
          "time": "20210821 14:24"
        },
        {
          "id": "MDRW2100003978.1.6",
          "form": "대표님은 별로 인가봐 재택근무",
          "original_form": "대표님은 별로 인가봐 재택근무",
          "speaker_id": "1",
          "time": "20210821 14:24"
        },
        {
          "id": "MDRW2100003978.1.7",
          "form": "사장님이 말이없으면 안하는거네",
          "original_form": "사장님이 말이없으면 안하는거네",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003978.1.8",
          "form": "난 너무나 하고 싶다",
          "original_form": "난 너무나 하고 싶다",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003978.1.9",
          "form": "왜 하고싶은데",
          "original_form": "왜 하고싶은데",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003978.1.10",
          "form": "출퇴근이 너무 힘들어 왕복 4시간 힘들어",
          "original_form": "출퇴근이 너무 힘들어 왕복 4시간 힘들어",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003978.1.11",
          "form": "적응되지 않았어?",
          "original_form": "적응되지 않았어?",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003978.1.12",
          "form": "적응 되기는 햇지 오래 다녔으니",
          "original_form": "적응 되기는 햇지 오래 다녔으니",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003978.1.13",
          "form": "기대를하지마",
          "original_form": "기대를하지마",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003978.1.14",
          "form": "안해야지 하면서도 뉴스에 나오면 나는 ?",
          "original_form": "안해야지 하면서도 뉴스에 나오면 나는 ?",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003978.1.15",
          "form": "생각이 나는 거지",
          "original_form": "생각이 나는 거지",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003978.1.16",
          "form": "집에서 업무는 가능하고",
          "original_form": "집에서 업무는 가능하고",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003978.1.17",
          "form": "안될껄 ? 집컴이 너무 느려서",
          "original_form": "안될껄 ? 집컴이 너무 느려서",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003978.1.18",
          "form": "그럼 가 회사로",
          "original_form": "그럼 가 회사로",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003978.1.19",
          "form": "그래야지",
          "original_form": "그래야지",
          "speaker_id": "1",
          "time": "20210821 14:27"
        }
