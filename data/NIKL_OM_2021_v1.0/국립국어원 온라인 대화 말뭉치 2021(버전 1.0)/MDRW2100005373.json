      "utterance": [
        {
          "id": "MDRW2100005373.1.1",
          "form": "혹시 공연같은거 좋아하세요?",
          "original_form": "혹시 공연같은거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005373.1.2",
          "form": "저는 공연 엄청엄청 좋아하죠",
          "original_form": "저는 공연 엄청엄청 좋아하죠",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005373.1.3",
          "form": "어떤 주제 공연 보셨어요?",
          "original_form": "어떤 주제 공연 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005373.1.4",
          "form": "뮤지컬  위주로 보는거 같아요 다른건 잘 안보고요",
          "original_form": "뮤지컬  위주로 보는거 같아요 다른건 잘 안보고요",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005373.1.5",
          "form": "아 그래요",
          "original_form": "아 그래요",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005373.1.6",
          "form": "혹시 영화관은 언제",
          "original_form": "혹시 영화관은 언제",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005373.1.7",
          "form": "마지막으로 가셨어요?",
          "original_form": "마지막으로 가셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005373.1.8",
          "form": "음 저는 썸녀랑 6월에 간거같아요",
          "original_form": "음 저는 썸녀랑 6월에 간거같아요",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005373.1.9",
          "form": "어떤영화 보셨어요?",
          "original_form": "어떤영화 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005373.1.10",
          "form": "제가 기억으로 발신제한 본거같아요",
          "original_form": "제가 기억으로 발신제한 본거같아요",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005373.1.11",
          "form": "이번에 샹치 예고편 나왔던데",
          "original_form": "이번에 샹치 예고편 나왔던데",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005373.1.12",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005373.1.13",
          "form": "처음들어보는데요? 재미있어보이나요?",
          "original_form": "처음들어보는데요? 재미있어보이나요?",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005373.1.14",
          "form": "마블영환데",
          "original_form": "마블영환데",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005373.1.15",
          "form": "다음달에 개봉한다더라구요",
          "original_form": "다음달에 개봉한다더라구요",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005373.1.16",
          "form": "장르가 뭐에요?",
          "original_form": "장르가 뭐에요?",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005373.1.17",
          "form": "시무리우라는 중국쪽 배우가 반지가지고 싸우는 내용이에요",
          "original_form": "시무리우라는 중국쪽 배우가 반지가지고 싸우는 내용이에요",
          "speaker_id": "1",
          "time": "20210821 22:14"
        }
      ]
