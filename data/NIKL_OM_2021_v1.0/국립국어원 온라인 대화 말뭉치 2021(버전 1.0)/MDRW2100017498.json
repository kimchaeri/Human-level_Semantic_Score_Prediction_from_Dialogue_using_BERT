        {
          "id": "MDRW2100017498.1.1",
          "form": "싸이월드가 부활한대요",
          "original_form": "싸이월드가 부활한대요",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017498.1.2",
          "form": "너 그거 했었니?",
          "original_form": "너 그거 했었니?",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017498.1.3",
          "form": "네 제 데이타들도 있을거예요",
          "original_form": "네 제 데이타들도 있을거예요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017498.1.4",
          "form": "언제부터 볼 수 있대?",
          "original_form": "언제부터 볼 수 있대?",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017498.1.5",
          "form": "로그인한지 오래돼서 지금은 어렵대요",
          "original_form": "로그인한지 오래돼서 지금은 어렵대요",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017498.1.6",
          "form": "그럼 지금 못봐?",
          "original_form": "그럼 지금 못봐?",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017498.1.7",
          "form": "네 아직 날짜 안됐어요",
          "original_form": "네 아직 날짜 안됐어요",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017498.1.8",
          "form": "예전 사진들 궁금한대",
          "original_form": "예전 사진들 궁금한대",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017498.1.9",
          "form": "전 열리면 싹 지우려구요",
          "original_form": "전 열리면 싹 지우려구요",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017498.1.10",
          "form": "왜~ 아깝게",
          "original_form": "왜~ 아깝게",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017498.1.11",
          "form": "흑역사거든요",
          "original_form": "흑역사거든요",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017498.1.12",
          "form": "내눈엔 우리 딸 흑역사는 없어보이는데~",
          "original_form": "내눈엔 우리 딸 흑역사는 없어보이는데~",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017498.1.13",
          "form": "엄마라서 그래요",
          "original_form": "엄마라서 그래요",
          "speaker_id": "2",
