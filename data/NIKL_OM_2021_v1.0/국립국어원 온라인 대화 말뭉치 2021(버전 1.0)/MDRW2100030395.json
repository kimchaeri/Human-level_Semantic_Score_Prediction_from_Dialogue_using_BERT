      "utterance": [
        {
          "id": "MDRW2100030395.1.1",
          "form": "보다가 말하셨죠 더이상 안 나와서",
          "original_form": "보다가 말하셨죠 더이상 안 나와서",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.2",
          "form": "바람의 검심에 미쳤죠",
          "original_form": "바람의 검심에 미쳤죠",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.3",
          "form": "카카오페이지에서 다시 하는 것 같긴 해요",
          "original_form": "카카오페이지에서 다시 하는 것 같긴 해요",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.4",
          "form": "나의 과거입니다",
          "original_form": "나의 과거입니다",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.5",
          "form": "넷플릭스에 바람의 검심 드라마인지 영화인지 실사판 있더라고요",
          "original_form": "넷플릭스에 바람의 검심 드라마인지 영화인지 실사판 있더라고요",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.6",
          "form": "만화책 모으는 거 재밌었죠",
          "original_form": "만화책 모으는 거 재밌었죠",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.7",
          "form": "대답하는 궁금하지 않습니다",
          "original_form": "대답하는 궁금하지 않습니다",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.8",
          "form": "슬램덩크 재미있었는데요",
          "original_form": "슬램덩크 재미있었는데요",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.9",
          "form": "못생겼을 거예요",
          "original_form": "못생겼을 거예요",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.10",
          "form": "실사판은 못 보겠어요",
          "original_form": "실사판은 못 보겠어요",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.11",
          "form": "슬램덩크 재밌죠",
          "original_form": "슬램덩크 재밌죠",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.12",
          "form": "책은 책으로 끝내는게 멋져요",
          "original_form": "책은 책으로 끝내는게 멋져요",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.13",
          "form": "오글 거립니다",
          "original_form": "오글 거립니다",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.14",
          "form": "퇴마록 또 그렇구요",
          "original_form": "퇴마록 또 그렇구요",
          "speaker_id": "2",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030395.1.15",
          "form": "봉신연의도 그렇고요",
          "original_form": "봉신연의도 그렇고요",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030395.1.16",
          "form": "실사화 제발 하지 말았으면",
          "original_form": "실사화 제발 하지 말았으면",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030395.1.17",
          "form": "다 추억이네요",
          "original_form": "다 추억이네요",
          "speaker_id": "1",
          "time": "20210904 00:30"
        }
      ]
