      "utterance": [
        {
          "id": "MDRW2100007721.1.1",
          "form": "가장 최근에 뭐 사셨어요?",
          "original_form": "가장 최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007721.1.2",
          "form": "가을옷 사고 있어요ㅎㅎ 가을 기분내고 싶어서ㅎㅎ 최근에 구매하신게 있나여?",
          "original_form": "가을옷 사고 있어요ㅎㅎ 가을 기분내고 싶어서ㅎㅎ 최근에 구매하신게 있나여?",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007721.1.3",
          "form": "최근에 아쿠아슈즈랑 운동화샀어요",
          "original_form": "최근에 아쿠아슈즈랑 운동화샀어요",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007721.1.4",
          "form": "사고보니 전부 여름꺼군요 ㅋㅋ",
          "original_form": "사고보니 전부 여름꺼군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007721.1.5",
          "form": "아 저도 운동화사려고 보는데 어떤걸로 사셨어요?",
          "original_form": "아 저도 운동화사려고 보는데 어떤걸로 사셨어요?",
          "speaker_id": "1",
          "time": "20210822 10:00"
        },
        {
          "id": "MDRW2100007721.1.6",
          "form": "저는 운동화 두개샀는데",
          "original_form": "저는 운동화 두개샀는데",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007721.1.7",
          "form": "하나는 나이키 꺼 티몬으로 직구했고",
          "original_form": "하나는 나이키 꺼 티몬으로 직구했고",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007721.1.8",
          "form": "하나는 뉴발란스 코스트코에서 그냥 훅샀어요 ㅋㅋㅋ",
          "original_form": "하나는 뉴발란스 코스트코에서 그냥 훅샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007721.1.9",
          "form": "오 저도 런닝화 고르고 있어서 ㅠㅠ 인터넷으로 보다보면 못고르겠어요ㅎㅎ",
          "original_form": "오 저도 런닝화 고르고 있어서 ㅠㅠ 인터넷으로 보다보면 못고르겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007721.1.10",
          "form": "너무 종류가 많아서ㅋㅋ",
          "original_form": "너무 종류가 많아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007721.1.11",
          "form": "그죠ㅜ",
          "original_form": "그죠ㅜ",
          "speaker_id": "2",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007721.1.12",
          "form": "저는 1만보걷기하려고샀어요",
          "original_form": "저는 1만보걷기하려고샀어요",
          "speaker_id": "2",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007721.1.13",
          "form": "저도 운동때문에 ㅋㅋ갤럭시 워치도 사려구요ㅋㅋ",
          "original_form": "저도 운동때문에 ㅋㅋ갤럭시 워치도 사려구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007721.1.14",
          "form": "근데 역시 메쉬? 메시? 가 많이 덮인? 거로 산걸 많이신어요",
          "original_form": "근데 역시 메쉬? 메시? 가 많이 덮인? 거로 산걸 많이신어요",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007721.1.15",
          "form": "맞아요 근데 그거 겨울엔 춥다구 그래서 고만이네요ㅠㅠ",
          "original_form": "맞아요 근데 그거 겨울엔 춥다구 그래서 고만이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007721.1.16",
          "form": "근데 최근엔 비가 너무와서",
          "original_form": "근데 최근엔 비가 너무와서",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007721.1.17",
          "form": "가죽으로된 나이키는",
          "original_form": "가죽으로된 나이키는",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007721.1.18",
          "form": "아끼게되더라고요",
          "original_form": "아끼게되더라고요",
          "speaker_id": "2",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007721.1.19",
          "form": "그렇죠 제 뉴발도 벌써 난리가 났어요ㅎㅎ 하고 험하게 신어서ㅎㅎ",
          "original_form": "그렇죠 제 뉴발도 벌써 난리가 났어요ㅎㅎ 하고 험하게 신어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007721.1.20",
          "form": "악 저도 뉴발은 2만원 짜리라그런가 막신어요",
          "original_form": "악 저도 뉴발은 2만원 짜리라그런가 막신어요",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100007721.1.21",
          "form": "아쿠아슈즈는 사놓고 거의못신었어요ㅜ",
          "original_form": "아쿠아슈즈는 사놓고 거의못신었어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100007721.1.22",
          "form": "저도 그래요ㅠㅠ",
          "original_form": "저도 그래요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:10"
        }
      ]
    }
  ]
