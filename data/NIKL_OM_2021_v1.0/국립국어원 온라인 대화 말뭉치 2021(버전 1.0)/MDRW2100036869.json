      "utterance": [
        {
          "id": "MDRW2100036869.1.1",
          "form": "저는 예전에 잠실 롯데시어터에서",
          "original_form": "저는 예전에 잠실 롯데시어터에서",
          "speaker_id": "2",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100036869.1.2",
          "form": "타이타닉 뮤지컬 본 적이 있었는데",
          "original_form": "타이타닉 뮤지컬 본 적이 있었는데",
          "speaker_id": "2",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100036869.1.3",
          "form": "그것도 좀 별로였던 기억이 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그것도 좀 별로였던 기억이 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100036869.1.4",
          "form": "아 왜요? 뭐가 안좋았던건가요?",
          "original_form": "아 왜요? 뭐가 안좋았던건가요?",
          "speaker_id": "1",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100036869.1.5",
          "form": "처음에는 웅장하게 시작하다가",
          "original_form": "처음에는 웅장하게 시작하다가",
          "speaker_id": "2",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036869.1.6",
          "form": "결국 흐지부지한 결말이라고 해야하나...?!",
          "original_form": "결국 흐지부지한 결말이라고 해야하나...?!",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036869.1.7",
          "form": "그냥 배가 침몰하는 과정만 2시간동안",
          "original_form": "그냥 배가 침몰하는 과정만 2시간동안",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036869.1.8",
          "form": "보여주는 거더라구요... 그래서 실망했었어요!",
          "original_form": "보여주는 거더라구요... 그래서 실망했었어요!",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036869.1.9",
          "form": "헐 약간 어이없네요 스토리도없구;;;",
          "original_form": "헐 약간 어이없네요 스토리도없구;;;",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036869.1.10",
          "form": "그쵸 ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036869.1.11",
          "form": "뭔가 용두사미 느낌...?",
          "original_form": "뭔가 용두사미 느낌...?",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036869.1.12",
          "form": "name1님은 다른 연극도 안보시는거죠?!",
          "original_form": "&name1&님은 다른 연극도 안보시는거죠?!",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036869.1.13",
          "form": "네 살면서 연극 두번인가 본게 다에요",
          "original_form": "네 살면서 연극 두번인가 본게 다에요",
          "speaker_id": "1",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036869.1.14",
          "form": "누가 가자면 볼거같은데 혼자서 찾아보진않아서요",
          "original_form": "누가 가자면 볼거같은데 혼자서 찾아보진않아서요",
          "speaker_id": "1",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036869.1.15",
          "form": "저도 연극에 대해선 앞으로 그렇게 될 거 같아요 ㅋㅋㅋㅋ",
          "original_form": "저도 연극에 대해선 앞으로 그렇게 될 거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036869.1.16",
          "form": "name1님 취향 확고하셔서 좋네요!!!",
          "original_form": "&name1&님 취향 확고하셔서 좋네요!!!",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036869.1.17",
          "form": "요즘은 코로나 시국이어서",
          "original_form": "요즘은 코로나 시국이어서",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036869.1.18",
          "form": "웬만한 공연은 다 미뤄지거나 취소되는 것 같네요ㅠ",
          "original_form": "웬만한 공연은 다 미뤄지거나 취소되는 것 같네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036869.1.19",
          "form": "네 그래서 공연하는 사람들 힘들대요",
          "original_form": "네 그래서 공연하는 사람들 힘들대요",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036869.1.20",
          "form": "그럴거같아요",
          "original_form": "그럴거같아요",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036869.1.21",
          "form": "일자리를 유지하기 힘들거예요...",
          "original_form": "일자리를 유지하기 힘들거예요...",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036869.1.22",
          "form": "안그래도 예술이 배부르기 힘든 영역인데...",
          "original_form": "안그래도 예술이 배부르기 힘든 영역인데...",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036869.1.23",
          "form": "내 나라에서 해주는게 있지만 그것도 어느 정도 이름 있는 사람들이나 하는 거지",
          "original_form": "내 나라에서 해주는게 있지만 그것도 어느 정도 이름 있는 사람들이나 하는 거지",
          "speaker_id": "1",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036869.1.24",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "2",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100036869.1.25",
          "form": "예술인들 정말 힘들 것 같아요",
          "original_form": "예술인들 정말 힘들 것 같아요",
          "speaker_id": "2",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100036869.1.26",
          "form": "그래서 다들 생계유지를 위해 다른 일도 병행하더라구요",
          "original_form": "그래서 다들 생계유지를 위해 다른 일도 병행하더라구요",
          "speaker_id": "2",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100036869.1.27",
          "form": "네 그런 공공 일자리 잡는 것도 꽤 힘든가 보더라고요",
          "original_form": "네 그런 공공 일자리 잡는 것도 꽤 힘든가 보더라고요",
          "speaker_id": "1",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100036869.1.28",
          "form": "아 배우들이요?!",
          "original_form": "아 배우들이요?!",
          "speaker_id": "2",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100036869.1.29",
          "form": "연극인들을 위한 공공 일자리가 있나보군요!!!",
          "original_form": "연극인들을 위한 공공 일자리가 있나보군요!!!",
