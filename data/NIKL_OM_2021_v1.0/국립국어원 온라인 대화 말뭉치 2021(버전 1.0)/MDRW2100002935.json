      "utterance": [
        {
          "id": "MDRW2100002935.1.1",
          "form": "게임좋아해?",
          "original_form": "게임좋아해?",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002935.1.2",
          "form": "던파하고있어",
          "original_form": "던파하고있어",
          "speaker_id": "2",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002935.1.3",
          "form": "던파 오랜만에듣는다 ㅋㅋㅋ아직도그거하는구나",
          "original_form": "던파 오랜만에듣는다 ㅋㅋㅋ아직도그거하는구나",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002935.1.4",
          "form": "고등학교때 재밋게 하다가 그냥 심심해서 들어가보니까 골드가 있드라고 그래서 그냥 다시 시작했지 한달했나",
          "original_form": "고등학교때 재밋게 하다가 그냥 심심해서 들어가보니까 골드가 있드라고 그래서 그냥 다시 시작했지 한달했나",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002935.1.5",
          "form": "무슨게임해???",
          "original_form": "무슨게임해???",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002935.1.6",
          "form": "나는 집에서 가끔 오버워치? 폰겜많이해",
          "original_form": "나는 집에서 가끔 오버워치? 폰겜많이해",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002935.1.7",
          "form": "너는 fps를 못하겠더라 어지러워서 나이먹었나바",
          "original_form": "너는 fps를 못하겠더라 어지러워서 나이먹었나바",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002935.1.8",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002935.1.9",
          "form": "혹시 나이물어봐도돼?",
          "original_form": "혹시 나이물어봐도돼?",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002935.1.10",
          "form": "나는 33살이야",
          "original_form": "나는 33살이야",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002935.1.11",
          "form": "형이라고 부르렴",
          "original_form": "형이라고 부르렴",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002935.1.12",
          "form": "형아 나이 좀 있네 지금 회사아냐?",
          "original_form": "형아 나이 좀 있네 지금 회사아냐?",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002935.1.13",
          "form": "회산데 일이 농땡이 중이야 폰게임이나 할까",
          "original_form": "회산데 일이 농땡이 중이야 폰게임이나 할까",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002935.1.14",
          "form": "나도 어제부터 월루중이야 헤헤",
          "original_form": "나도 어제부터 월루중이야 헤헤",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002935.1.15",
          "form": "폰게임 어떤거해?",
          "original_form": "폰게임 어떤거해?",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002935.1.16",
          "form": "카트라이더",
          "original_form": "카트라이더",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002935.1.17",
          "form": "카트라이더 좀 해? 나도그거하다가 접었는데",
          "original_form": "카트라이더 좀 해? 나도그거하다가 접었는데",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002935.1.18",
          "form": "못하는데 현질을 7만원을 해버렸어 머리가 어떻게 됐었나봐",
          "original_form": "못하는데 현질을 7만원을 해버렸어 머리가 어떻게 됐었나봐",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002935.1.19",
          "form": "카트좋으면 내 실력도 좋아지는줄 알고 착각했어",
          "original_form": "카트좋으면 내 실력도 좋아지는줄 알고 착각했어",
