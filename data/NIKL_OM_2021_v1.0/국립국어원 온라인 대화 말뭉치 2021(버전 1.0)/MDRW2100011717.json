        },
        {
          "id": "MDRW2100011717.1.4",
          "form": "지하철 위로 자동차 타고 지나갔음",
          "original_form": "지하철 위로 자동차 타고 지나갔음",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100011717.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011717.1.6",
          "form": "진짜 안기준인가 걔 차는",
          "original_form": "진짜 안기준인가 걔 차는",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011717.1.7",
          "form": "개웃기네",
          "original_form": "개웃기네",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011717.1.8",
          "form": "부숴지지도 않음",
          "original_form": "부숴지지도 않음",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011717.1.9",
          "form": "우린 진짜",
          "original_form": "우린 진짜",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011717.1.10",
          "form": "좋아하는장르가 확실한듯",
          "original_form": "좋아하는장르가 확실한듯",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011717.1.11",
          "form": "그 차 타야함 진짜",
          "original_form": "그 차 타야함 진짜",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011717.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011717.1.13",
          "form": "그게ㅜ좋은거지 뭐",
          "original_form": "그게ㅜ좋은거지 뭐",
          "speaker_id": "1",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011717.1.14",
          "form": "시험시간에 코난 안보려고",
          "original_form": "시험시간에 코난 안보려고",
          "speaker_id": "1",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011717.1.15",
          "form": "방학때 많이 본거임 그런거임...",
          "original_form": "방학때 많이 본거임 그런거임...",
