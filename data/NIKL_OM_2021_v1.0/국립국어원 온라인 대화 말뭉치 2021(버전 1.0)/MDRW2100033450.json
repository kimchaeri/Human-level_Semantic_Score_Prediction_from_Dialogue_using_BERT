      "utterance": [
        {
          "id": "MDRW2100033450.1.1",
          "form": "우리 신랑도 요즘 방에 틀어박혀서 산다 ㅋㅋㅋ",
          "original_form": "우리 신랑도 요즘 방에 틀어박혀서 산다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033450.1.2",
          "form": "오늘도 태풍때문에 5시퇴근하고도 6시에",
          "original_form": "오늘도 태풍때문에 5시퇴근하고도 6시에",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.3",
          "form": "다시 소집할수도있다드라고",
          "original_form": "다시 소집할수도있다드라고",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.4",
          "form": "그래서 시댁가서 기다린다데",
          "original_form": "그래서 시댁가서 기다린다데",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.5",
          "form": "두말안했다 짜증나서",
          "original_form": "두말안했다 짜증나서",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.6",
          "form": "아.. 포항으로 꺽었던데",
          "original_form": "아.. 포항으로 꺽었던데",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.7",
          "form": "피해 있으려나",
          "original_form": "피해 있으려나",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.8",
          "form": "엇? 그럼 지금 시댁갔나?",
          "original_form": "엇? 그럼 지금 시댁갔나?",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.9",
          "form": "근데 6시30분까지도 연락없어서",
          "original_form": "근데 6시30분까지도 연락없어서",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.10",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033450.1.11",
          "form": "7시에 집에왔지",
          "original_form": "7시에 집에왔지",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033450.1.12",
          "form": "그냥 거서라면먹고 쉬다온거지",
          "original_form": "그냥 거서라면먹고 쉬다온거지",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033450.1.13",
          "form": "시댁서",
          "original_form": "시댁서",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033450.1.14",
          "form": "근데 솔직히 이게 생각하기 나름인데",
          "original_form": "근데 솔직히 이게 생각하기 나름인데",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033450.1.15",
          "form": "여기서 시댁까지 10분도 안걸리거든?",
          "original_form": "여기서 시댁까지 10분도 안걸리거든?",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033450.1.16",
          "form": "집에서가나 시댁서가나 십분차이라는거지",
          "original_form": "집에서가나 시댁서가나 십분차이라는거지",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033450.1.17",
          "form": "그냥 집에오기싫은거지 ㅋ안그나?ㅋ",
          "original_form": "그냥 집에오기싫은거지 ㅋ안그나?ㅋ",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033450.1.18",
          "form": "아 저녁???",
          "original_form": "아 저녁???",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033450.1.19",
          "form": "응저녁에 퇴근하고",
          "original_form": "응저녁에 퇴근하고",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033450.1.20",
          "form": "ㅋㅋㅋㅋㅋ아니.. 비오니깐.. 왔다갔다가 귀찮을 수도 있지 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아니.. 비오니깐.. 왔다갔다가 귀찮을 수도 있지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033450.1.21",
          "form": "아니지ㅋㅋ",
          "original_form": "아니지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033450.1.22",
          "form": "부를수도있고 안부를수도",
          "original_form": "부를수도있고 안부를수도",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033450.1.23",
          "form": "딴거보다.. 난 만원에 만원할인인데 그걸 재우고 나가라고 한다는게 이해가 안되네",
          "original_form": "딴거보다.. 난 만원에 만원할인인데 그걸 재우고 나가라고 한다는게 이해가 안되네",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033450.1.24",
          "form": "있는거잖아ㅋㅋ백프로면ㅋㅋ.",
          "original_form": "있는거잖아ㅋㅋ백프로면ㅋㅋ.",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033450.1.25",
          "form": "거기서가는게 맞는데",
          "original_form": "거기서가는게 맞는데",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033450.1.26",
          "form": "애매하니깐.. 글치.. 그건 솔찍히 난 이해가 된다 ㅋㅋ 왔다리갔다리 안해도 되니깐",
          "original_form": "애매하니깐.. 글치.. 그건 솔찍히 난 이해가 된다 ㅋㅋ 왔다리갔다리 안해도 되니깐",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033450.1.27",
          "form": "근데... ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ파바는 이해가 안된다 ㅋㅋ",
          "original_form": "근데... ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ파바는 이해가 안된다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033450.1.28",
          "form": "아니근데 거기서도 시댁까지 나오는시간이 있자나",
          "original_form": "아니근데 거기서도 시댁까지 나오는시간이 있자나",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100033450.1.29",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100033450.1.30",
          "form": "하루 종일 애보라는것도 아니고 왜 잠깐을 못나갔다오게 하는데?",
          "original_form": "하루 종일 애보라는것도 아니고 왜 잠깐을 못나갔다오게 하는데?",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100033450.1.31",
          "form": "그니깐 난다이해안됨",
          "original_form": "그니깐 난다이해안됨",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100033450.1.32",
          "form": "직접 좀 나갔다오라는 것도 아니고",
          "original_form": "직접 좀 나갔다오라는 것도 아니고",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100033450.1.33",
          "form": "여튼 시댁간것도 두말안했고",
          "original_form": "여튼 시댁간것도 두말안했고",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100033450.1.34",
          "form": "파바는좀 짜증나서 캤지 내가감옥사나캄서",
          "original_form": "파바는좀 짜증나서 캤지 내가감옥사나캄서",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100033450.1.35",
          "form": "나갈라다가 안나가고있으니까 문닫았는갑네 캄서 웃고",
          "original_form": "나갈라다가 안나가고있으니까 문닫았는갑네 캄서 웃고",
