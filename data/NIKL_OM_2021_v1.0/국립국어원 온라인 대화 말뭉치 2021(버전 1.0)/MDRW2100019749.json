        {
          "id": "MDRW2100019749.1.1",
          "form": "재택근무 많이 하세요?",
          "original_form": "재택근무 많이 하세요?",
          "speaker_id": "2",
          "time": "20210828 15:39"
        },
        {
          "id": "MDRW2100019749.1.2",
          "form": "아니여 계속 출근여",
          "original_form": "아니여 계속 출근여",
          "speaker_id": "1",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100019749.1.3",
          "form": "흑 저도요. 남편은 격주로 재택 해요",
          "original_form": "흑 저도요. 남편은 격주로 재택 해요",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100019749.1.4",
          "form": "나가는게 난듯요",
          "original_form": "나가는게 난듯요",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100019749.1.5",
          "form": "그쵸? 집에있음 한것도 없이 시간만 잘가요",
          "original_form": "그쵸? 집에있음 한것도 없이 시간만 잘가요",
          "speaker_id": "2",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100019749.1.6",
          "form": "집안일과 콜라보요",
          "original_form": "집안일과 콜라보요",
          "speaker_id": "1",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100019749.1.7",
          "form": "맞아요~ 특히 남편도 같이 재택이면 오노노노",
          "original_form": "맞아요~ 특히 남편도 같이 재택이면 오노노노",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100019749.1.8",
          "form": "으악ㅠㅠ",
          "original_form": "으악ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100019749.1.9",
          "form": "그냥 출근을ㅋ",
          "original_form": "그냥 출근을ㅋ",
