      "utterance": [
        {
          "id": "MDRW2100030252.1.1",
          "form": "근데 호불호 심해서",
          "original_form": "근데 호불호 심해서",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030252.1.2",
          "form": "악 마지막에 보낸 거 못 봤다 뭐라고 했니",
          "original_form": "악 마지막에 보낸 거 못 봤다 뭐라고 했니",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030252.1.3",
          "form": "싫어하는 사람은 진짜 싫어한대",
          "original_form": "싫어하는 사람은 진짜 싫어한대",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030252.1.4",
          "form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030252.1.5",
          "form": "면이 많이 씹혀서 좋아",
          "original_form": "면이 많이 씹혀서 좋아",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030252.1.6",
          "form": "극단적인 레시피군",
          "original_form": "극단적인 레시피군",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030252.1.7",
          "form": "응 극단의 괴식",
          "original_form": "응 극단의 괴식",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030252.1.8",
          "form": "ㅋㅋㅋㅋㅋ니가 먹고 괴식이라 하면 우짬",
          "original_form": "ㅋㅋㅋㅋㅋ니가 먹고 괴식이라 하면 우짬",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030252.1.9",
          "form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅌ짱웃겨",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅌ짱웃겨",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030252.1.10",
          "form": "스스로 인지하는 괴식",
          "original_form": "스스로 인지하는 괴식",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030252.1.11",
          "form": "내가 먹고있으면",
          "original_form": "내가 먹고있으면",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030252.1.12",
          "form": "엄마가 완전 애잔하게 쳐다봐",
          "original_form": "엄마가 완전 애잔하게 쳐다봐",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030252.1.13",
          "form": "왜 개밥먹냐고",
          "original_form": "왜 개밥먹냐고",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030252.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030252.1.15",
          "form": "너무해",
          "original_form": "너무해",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030252.1.16",
          "form": "니 생각에",
          "original_form": "니 생각에",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030252.1.17",
          "form": "토달볶보다 괴식이야?",
          "original_form": "토달볶보다 괴식이야?",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030252.1.18",
          "form": "그건 아님",
          "original_form": "그건 아님",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030252.1.19",
          "form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030252.1.20",
          "form": "토달볶에 감자넣는 사람의 의견우",
          "original_form": "토달볶에 감자넣는 사람의 의견우",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030252.1.21",
          "form": "어디다 비교하겠는교",
          "original_form": "어디다 비교하겠는교",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030252.1.22",
          "form": "진짜 웃긴다",
          "original_form": "진짜 웃긴다",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030252.1.23",
          "form": "서로 괴식취급",
          "original_form": "서로 괴식취급",
          "speaker_id": "1",
