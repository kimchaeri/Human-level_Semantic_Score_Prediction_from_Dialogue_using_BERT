      "utterance": [
        {
          "id": "MDRW2100017300.1.1",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017300.1.2",
          "form": "없어요ㅠ",
          "original_form": "없어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017300.1.3",
          "form": "저듀,...",
          "original_form": "저듀,...",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017300.1.4",
          "form": "마음은 댕집사 냥집사...",
          "original_form": "마음은 댕집사 냥집사...",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017300.1.5",
          "form": "있었으면좋겠어요",
          "original_form": "있었으면좋겠어요",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017300.1.6",
          "form": "저도 랜선 애기들 많거든요",
          "original_form": "저도 랜선 애기들 많거든요",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017300.1.7",
          "form": "어떤 이유가 있으세요? 털 알러지라든지?!",
          "original_form": "어떤 이유가 있으세요? 털 알러지라든지?!",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017300.1.8",
          "form": "아뇨 책임감이 무거워서요ㅠ 잘키울수있을까싶고",
          "original_form": "아뇨 책임감이 무거워서요ㅠ 잘키울수있을까싶고",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017300.1.9",
          "form": "그쵸ㅠ 길게는 10년 넘게 함께하니까....",
          "original_form": "그쵸ㅠ 길게는 10년 넘게 함께하니까....",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017300.1.10",
          "form": "저는 여윳돈이 없어서 망설이고 있어요",
          "original_form": "저는 여윳돈이 없어서 망설이고 있어요",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017300.1.11",
          "form": "맞아요 직장을 다니는데 온전히 돌봐줄수없을거 같아요",
          "original_form": "맞아요 직장을 다니는데 온전히 돌봐줄수없을거 같아요",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017300.1.12",
          "form": "돈이 워낙 많이 든다고해서ㅠㅠ",
          "original_form": "돈이 워낙 많이 든다고해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017300.1.13",
          "form": "그져 병원비가 사람보다 비싸던데요ㅜ",
          "original_form": "그져 병원비가 사람보다 비싸던데요ㅜ",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017300.1.14",
          "form": "지인이 여행가느라 제가 며칠 강아지 봐준 적 있거든요?",
          "original_form": "지인이 여행가느라 제가 며칠 강아지 봐준 적 있거든요?",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017300.1.15",
          "form": "너무 이뻐서 장난감 하나 사줬는데 1시간도 안돼서 다 망가진거 있죠...............ㅎㅎㅎ",
          "original_form": "너무 이뻐서 장난감 하나 사줬는데 1시간도 안돼서 다 망가진거 있죠...............ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017300.1.16",
          "form": "만원 넘는거였는뎈ㅋㅋㅋㅋㅋㅋ이런 자잘한 돈도 많이 들거같아요",
          "original_form": "만원 넘는거였는뎈ㅋㅋㅋㅋㅋㅋ이런 자잘한 돈도 많이 들거같아요",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017300.1.17",
          "form": "ㅋㅋㅋㄱ스치면 만원..",
          "original_form": "ㅋㅋㅋㄱ스치면 만원..",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017300.1.18",
          "form": "또 내아기가 되면 아낄수가없잖아요",
          "original_form": "또 내아기가 되면 아낄수가없잖아요",
          "speaker_id": "2",
          "time": "20210826 18:45"
        }
      ]
