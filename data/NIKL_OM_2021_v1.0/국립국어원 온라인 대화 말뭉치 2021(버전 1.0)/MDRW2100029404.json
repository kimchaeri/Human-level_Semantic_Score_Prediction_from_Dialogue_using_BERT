      "utterance": [
        {
          "id": "MDRW2100029404.1.1",
          "form": "야근 많이 하는 편이세요?",
          "original_form": "야근 많이 하는 편이세요?",
          "speaker_id": "1",
          "time": "20210903 07:10"
        },
        {
          "id": "MDRW2100029404.1.2",
          "form": "아니요 없습니다",
          "original_form": "아니요 없습니다",
          "speaker_id": "2",
          "time": "20210903 07:11"
        },
        {
          "id": "MDRW2100029404.1.3",
          "form": "와 좋은직장이네요",
          "original_form": "와 좋은직장이네요",
          "speaker_id": "1",
          "time": "20210903 07:11"
        },
        {
          "id": "MDRW2100029404.1.4",
          "form": "첫 직장에서 계속 근무중이세요?",
          "original_form": "첫 직장에서 계속 근무중이세요?",
          "speaker_id": "1",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100029404.1.5",
          "form": "아니요 네번째 이네요",
          "original_form": "아니요 네번째 이네요",
          "speaker_id": "2",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100029404.1.6",
          "form": "첫직장 이신가요",
          "original_form": "첫직장 이신가요",
          "speaker_id": "2",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100029404.1.7",
          "form": "이직준비 힘들지 않으셨어요?",
          "original_form": "이직준비 힘들지 않으셨어요?",
          "speaker_id": "1",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100029404.1.8",
          "form": "네 전 첫직장",
          "original_form": "네 전 첫직장",
          "speaker_id": "1",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100029404.1.9",
          "form": "무척 어렵고 힘들죠",
          "original_form": "무척 어렵고 힘들죠",
          "speaker_id": "2",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029404.1.10",
          "form": "직장은 얼마나 되셨나요",
          "original_form": "직장은 얼마나 되셨나요",
          "speaker_id": "2",
          "time": "20210903 07:29"
        },
        {
          "id": "MDRW2100029404.1.11",
          "form": "이제 7년정도",
          "original_form": "이제 7년정도",
          "speaker_id": "1",
          "time": "20210903 07:31"
        },
        {
          "id": "MDRW2100029404.1.12",
          "form": "오래되셨네요",
          "original_form": "오래되셨네요",
          "speaker_id": "2",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029404.1.13",
          "form": "전 지금 직장이 7년 째 이네요",
          "original_form": "전 지금 직장이 7년 째 이네요",
          "speaker_id": "2",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029404.1.14",
          "form": "와 그러시군요 직장 사람들은 괜찮으세요",
          "original_form": "와 그러시군요 직장 사람들은 괜찮으세요",
          "speaker_id": "1",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029404.1.15",
          "form": "네 다들 좋아요",
          "original_form": "네 다들 좋아요",
          "speaker_id": "2",
          "time": "20210903 07:40"
        },
        {
          "id": "MDRW2100029404.1.16",
          "form": "출근중 이신가요",
          "original_form": "출근중 이신가요",
          "speaker_id": "2",
          "time": "20210903 07:40"
        },
        {
          "id": "MDRW2100029404.1.17",
          "form": "넵 버스기다립니다",
          "original_form": "넵 버스기다립니다",
          "speaker_id": "1",
          "time": "20210903 07:41"
        },
        {
          "id": "MDRW2100029404.1.18",
          "form": "직장사람들이 좋아",
          "original_form": "직장사람들이 좋아",
          "speaker_id": "1",
          "time": "20210903 07:41"
        },
        {
          "id": "MDRW2100029404.1.19",
          "form": "스트레스 좀 덜하시겠어요",
          "original_form": "스트레스 좀 덜하시겠어요",
          "speaker_id": "1",
          "time": "20210903 07:41"
        },
        {
          "id": "MDRW2100029404.1.20",
          "form": "저도 짐 출근중 입니다",
          "original_form": "저도 짐 출근중 입니다",
          "speaker_id": "2",
          "time": "20210903 07:42"
        },
        {
          "id": "MDRW2100029404.1.21",
          "form": "아까 운전한다고 하셨죠?",
          "original_form": "아까 운전한다고 하셨죠?",
          "speaker_id": "1",
          "time": "20210903 07:46"
        },
        {
          "id": "MDRW2100029404.1.22",
          "form": "길 안 막히나요?",
          "original_form": "길 안 막히나요?",
          "speaker_id": "1",
          "time": "20210903 07:46"
        },
        {
          "id": "MDRW2100029404.1.23",
          "form": "출근길 항상 막히고 답답하죠",
          "original_form": "출근길 항상 막히고 답답하죠",
          "speaker_id": "2",
          "time": "20210903 07:51"
        }
