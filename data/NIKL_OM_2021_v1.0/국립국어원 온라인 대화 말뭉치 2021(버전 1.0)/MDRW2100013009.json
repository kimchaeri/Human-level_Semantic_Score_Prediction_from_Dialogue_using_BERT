      "utterance": [
        {
          "id": "MDRW2100013009.1.1",
          "form": "비와가 다행이였음 ㅋㅋㅋ",
          "original_form": "비와가 다행이였음 ㅋㅋㅋ",
          "speaker_id": "2",
