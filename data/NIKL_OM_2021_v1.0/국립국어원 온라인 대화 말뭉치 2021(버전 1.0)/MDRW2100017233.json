      "utterance": [
        {
          "id": "MDRW2100017233.1.1",
          "form": "병원에서는 금방 일정 나올꺼같이 말했는데",
          "original_form": "병원에서는 금방 일정 나올꺼같이 말했는데",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017233.1.2",
          "form": "아직 안 나왔네요",
          "original_form": "아직 안 나왔네요",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017233.1.3",
          "form": "name1님은 취준이시면 어디 준비하고 계신거예요??",
          "original_form": "&name1&님은 취준이시면 어디 준비하고 계신거예요??",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017233.1.4",
          "form": "많이밀렸나보네요ㄷㄷ두달전맞은 친구들은 바로나오던데",
          "original_form": "많이밀렸나보네요ㄷㄷ두달전맞은 친구들은 바로나오던데",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017233.1.5",
          "form": "저 사무직이요ㅋㅋㅋ",
          "original_form": "저 사무직이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017233.1.6",
          "form": "2차 일정도 내일쯤 문자오겠죠 ㅎ",
          "original_form": "2차 일정도 내일쯤 문자오겠죠 ㅎ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017233.1.7",
          "form": "사무직 준비중이신군여 ㄷㄷㄷ",
          "original_form": "사무직 준비중이신군여 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017233.1.8",
          "form": "ㅋㅋㅋㅋ네네",
          "original_form": "ㅋㅋㅋㅋ네네",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017233.1.9",
          "form": "백신이 참 얘기가많네요",
          "original_form": "백신이 참 얘기가많네요",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017233.1.10",
          "form": "그래요???",
          "original_form": "그래요???",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017233.1.11",
          "form": "9월초부터 사전예약한 일찍 한 사람들 백신맞아서 그런감",
          "original_form": "9월초부터 사전예약한 일찍 한 사람들 백신맞아서 그런감",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017233.1.12",
          "form": "부작용이니 뭐니",
          "original_form": "부작용이니 뭐니",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017233.1.13",
          "form": "언론이 너무난리긴해요",
          "original_form": "언론이 너무난리긴해요",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017233.1.14",
          "form": "아항 백신부작용ㅋㅋ",
          "original_form": "아항 백신부작용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017233.1.15",
          "form": "부작용 얘기들어보면 거의 카더라던데;;",
          "original_form": "부작용 얘기들어보면 거의 카더라던데;;",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017233.1.16",
          "form": "절레절레",
          "original_form": "절레절레",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017233.1.17",
          "form": "ㅋㅋㅋ 그니까요",
          "original_form": "ㅋㅋㅋ 그니까요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017233.1.18",
          "form": "과장이 좀",
          "original_form": "과장이 좀",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017233.1.19",
          "form": "있는것같아요",
          "original_form": "있는것같아요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017233.1.20",
          "form": "맞아맞아 ㅎ 저녁은 드셨어여???",
          "original_form": "맞아맞아 ㅎ 저녁은 드셨어여???",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017233.1.21",
          "form": "아직 고민중이랍니다",
          "original_form": "아직 고민중이랍니다",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017233.1.22",
          "form": "님은요?!",
          "original_form": "님은요?!",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017233.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017233.1.24",
          "form": "전 부대찌개 먹었어영",
          "original_form": "전 부대찌개 먹었어영",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017233.1.25",
          "form": "넘 먹어서 배 터질듯 ㄷ",
          "original_form": "넘 먹어서 배 터질듯 ㄷ",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017233.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017233.1.27",
          "form": "사리 뭐넣으셨나요",
          "original_form": "사리 뭐넣으셨나요",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017233.1.28",
          "form": "사리는 라면사리죠 헿",
          "original_form": "사리는 라면사리죠 헿",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017233.1.29",
          "form": "저녁메뉴 고민은 끝나셨나요",
          "original_form": "저녁메뉴 고민은 끝나셨나요",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017233.1.30",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017233.1.31",
          "form": "ㅋㅋㅋㅋ 네네",
          "original_form": "ㅋㅋㅋㅋ 네네",
          "speaker_id": "1",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100017233.1.32",
          "form": "가볍게",
          "original_form": "가볍게",
          "speaker_id": "1",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100017233.1.33",
          "form": "햄버거를 먹었어요!",
          "original_form": "햄버거를 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100017233.1.34",
          "form": "ㅋㅋ 햄버거 존맛탱!",
          "original_form": "ㅋㅋ 햄버거 존맛탱!",
          "speaker_id": "2",
          "time": "20210827 09:21"
        },
        {
          "id": "MDRW2100017233.1.35",
          "form": "햄버거하니 점심은 저두 햄버거 먹으러 가야할듯",
          "original_form": "햄버거하니 점심은 저두 햄버거 먹으러 가야할듯",
          "speaker_id": "2",
          "time": "20210827 09:41"
        }
      ]
