      "utterance": [
        {
          "id": "MDRW2100025648.1.1",
          "form": "아 그치..",
          "original_form": "아 그치..",
          "speaker_id": "2",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025648.1.2",
          "form": "그리고 하드하게 덕질하면",
          "original_form": "그리고 하드하게 덕질하면",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.3",
          "form": "그래도 퇴근길만 안기다리고 나만보고 나와버리며행복해ㅋㄱㅋㅋㅋㄱ",
          "original_form": "그래도 퇴근길만 안기다리고 나만보고 나와버리며행복해ㅋㄱㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.4",
          "form": "연뮤랑 배우 쪽이 진짜",
          "original_form": "연뮤랑 배우 쪽이 진짜",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.5",
          "form": "ㅇㅇ거기는 고인물",
          "original_form": "ㅇㅇ거기는 고인물",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.6",
          "form": "고인물 천지라",
          "original_form": "고인물 천지라",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.7",
          "form": "너무판이좁다보니",
          "original_form": "너무판이좁다보니",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.8",
          "form": "이름은몰라도얼굴은아는사람들",
          "original_form": "이름은몰라도얼굴은아는사람들",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.9",
          "form": "유입이 못견디고 나와서",
          "original_form": "유입이 못견디고 나와서",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.10",
          "form": "그 이명박짤",
          "original_form": "그 이명박짤",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.11",
          "form": "점점썩어가고",
          "original_form": "점점썩어가고",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.12",
          "form": "다아는사람들이구먼",
          "original_form": "다아는사람들이구먼",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.13",
          "form": "다 아는 사람들이구먼",
          "original_form": "다 아는 사람들이구먼",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.15",
          "form": "ㅇㅇㅇㅇㅇ 유입뚫기힘들어 ㅋㅋㅋㅋㄱㄱ",
          "original_form": "ㅇㅇㅇㅇㅇ 유입뚫기힘들어 ㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025648.1.16",
          "form": "마치 출근길 대중교통 느낌",
          "original_form": "마치 출근길 대중교통 느낌",
          "speaker_id": "2",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100025648.1.17",
          "form": "그리고 요샌 좀 덜한데",
          "original_form": "그리고 요샌 좀 덜한데",
          "speaker_id": "1",
          "time": "20210901 14:59"
        },
        {
          "id": "MDRW2100025648.1.18",
          "form": "인사는 안해도",
          "original_form": "인사는 안해도",
          "speaker_id": "2",
          "time": "20210901 14:59"
        },
        {
