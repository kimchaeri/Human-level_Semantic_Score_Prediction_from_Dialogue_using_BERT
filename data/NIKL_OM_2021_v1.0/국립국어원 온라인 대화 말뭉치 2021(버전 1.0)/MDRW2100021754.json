      "utterance": [
        {
          "id": "MDRW2100021754.1.1",
          "form": "저녁 뭐 먹었니",
          "original_form": "저녁 뭐 먹었니",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021754.1.2",
          "form": "치킨~~",
          "original_form": "치킨~~",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021754.1.3",
          "form": "위드 닭똥집 튀김",
          "original_form": "위드 닭똥집 튀김",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021754.1.4",
          "form": "오... 어디거?",
          "original_form": "오... 어디거?",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021754.1.5",
          "form": "가마치 통닭이라는데",
          "original_form": "가마치 통닭이라는데",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021754.1.6",
          "form": "들어본거 같은데",
          "original_form": "들어본거 같은데",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021754.1.7",
          "form": "맛있나?",
          "original_form": "맛있나?",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021754.1.8",
          "form": "이 동네는 맛 괜찮더라",
          "original_form": "이 동네는 맛 괜찮더라",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021754.1.9",
          "form": "옛날 통닭 맛 ㅋㅋㅋㅋ",
          "original_form": "옛날 통닭 맛 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021754.1.10",
          "form": "아ㅋㅋㅋ 울 동네에도 있는거다",
          "original_form": "아ㅋㅋㅋ 울 동네에도 있는거다",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021754.1.11",
          "form": "사실 옛날 통닭엔 별 관심없지만 닭똥집 튀김이 맛있어",
          "original_form": "사실 옛날 통닭엔 별 관심없지만 닭똥집 튀김이 맛있어",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021754.1.12",
          "form": "옛날 통닭 맛있는데",
          "original_form": "옛날 통닭 맛있는데",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021754.1.13",
          "form": "시장에서 파랑 같이 팔음",
          "original_form": "시장에서 파랑 같이 팔음",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021754.1.14",
          "form": "난 선호도로 따지면 떨어지는데 그냥 후라이드 취향이 아닌듯",
          "original_form": "난 선호도로 따지면 떨어지는데 그냥 후라이드 취향이 아닌듯",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021754.1.15",
          "form": "양념? 뿌링클?",
          "original_form": "양념? 뿌링클?",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021754.1.16",
          "form": "둘 다 조아",
          "original_form": "둘 다 조아",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021754.1.17",
          "form": "ㅋㅋㅋㅋ푸라닭 신메뉴 먹어보고 싶다",
          "original_form": "ㅋㅋㅋㅋ푸라닭 신메뉴 먹어보고 싶다",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021754.1.18",
          "form": "먹고 후기 ㄱㄱ",
          "original_form": "먹고 후기 ㄱㄱ",
