      "utterance": [
        {
          "id": "MDRW2100016694.1.1",
          "form": "안녕하세요! 반려동물 키우시나요?",
          "original_form": "안녕하세요! 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016694.1.2",
          "form": "지금은 못키우지만",
          "original_form": "지금은 못키우지만",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016694.1.3",
          "form": "나이더들어서 같이 시간보낼수잇을때 키우고싶어요",
          "original_form": "나이더들어서 같이 시간보낼수잇을때 키우고싶어요",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016694.1.4",
          "form": "맞아요 같이 시간보내는게 중요한것같아요...",
          "original_form": "맞아요 같이 시간보내는게 중요한것같아요...",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016694.1.5",
          "form": "키우고계세요?",
          "original_form": "키우고계세요?",
          "speaker_id": "2",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016694.1.6",
          "form": "네 저는 강아지 키워요! 강아지 좋아하세요?",
          "original_form": "네 저는 강아지 키워요! 강아지 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016694.1.7",
          "form": "좋아해요ㅠㅠ",
          "original_form": "좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016694.1.8",
          "form": "아직 책임질여건이 안되서 좋아만해요ㅠㅠ",
          "original_form": "아직 책임질여건이 안되서 좋아만해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016694.1.9",
          "form": "ㅠㅠㅠ저도 가족이 항상 집에 있어서 키울수있는것같아요 독립하려니 슬프네요...",
          "original_form": "ㅠㅠㅠ저도 가족이 항상 집에 있어서 키울수있는것같아요 독립하려니 슬프네요...",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016694.1.10",
          "form": "아 독립예정이시군요",
          "original_form": "아 독립예정이시군요",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016694.1.11",
          "form": "저는 독립해서살고잇어서 ㅠㅠ",
          "original_form": "저는 독립해서살고잇어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016694.1.12",
          "form": "나중에 나이들면 키우고싶어요",
          "original_form": "나중에 나이들면 키우고싶어요",
          "speaker_id": "2",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016694.1.13",
          "form": "반려견은 가족이라던데",
          "original_form": "반려견은 가족이라던데",
          "speaker_id": "2",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016694.1.14",
          "form": "눈에밟히겟어요",
          "original_form": "눈에밟히겟어요",
          "speaker_id": "2",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016694.1.15",
          "form": "네 그래도 멀리가는건 아니라 틈틈이 오려구요ㅠㅠ",
          "original_form": "네 그래도 멀리가는건 아니라 틈틈이 오려구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016694.1.16",
          "form": "댕댕이 무슨 종이에요?",
          "original_form": "댕댕이 무슨 종이에요?",
          "speaker_id": "2",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016694.1.17",
          "form": "갈색 푸들이요! 근데 좀 섞인건지 그냥 푸들보다는 좀더 커요ㅋㅋ",
          "original_form": "갈색 푸들이요! 근데 좀 섞인건지 그냥 푸들보다는 좀더 커요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016694.1.18",
          "form": "ㅋㅋㅋㅋ저는 못생긴 귀여운 강아지 좋아해요",
          "original_form": "ㅋㅋㅋㅋ저는 못생긴 귀여운 강아지 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016694.1.19",
          "form": "ㅋㅋㅋㅋㅋ강아지들 너무 다채롭게 귀여워요",
          "original_form": "ㅋㅋㅋㅋㅋ강아지들 너무 다채롭게 귀여워요",
          "speaker_id": "1",
          "time": "20210826 14:59"
        }
      ]
