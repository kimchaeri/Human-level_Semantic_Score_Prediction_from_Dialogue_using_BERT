      "utterance": [
        {
          "id": "MDRW2100032860.1.1",
          "form": "어제는 기아랑 한화 경기였네",
          "original_form": "어제는 기아랑 한화 경기였네",
          "speaker_id": "1",
          "time": "20210906 04:10"
        },
        {
          "id": "MDRW2100032860.1.2",
          "form": "ㅋㅋㅋㅋㅋ무승부 어이없어",
          "original_form": "ㅋㅋㅋㅋㅋ무승부 어이없어",
          "speaker_id": "2",
          "time": "20210906 04:10"
        },
        {
          "id": "MDRW2100032860.1.3",
          "form": "둘 중에 한 팀이라도 승점 챙기지",
          "original_form": "둘 중에 한 팀이라도 승점 챙기지",
          "speaker_id": "2",
          "time": "20210906 04:11"
        },
        {
          "id": "MDRW2100032860.1.4",
          "form": "꼴지들이 무승부로 승점도 못 챙기고ㅜㅠ",
          "original_form": "꼴지들이 무승부로 승점도 못 챙기고ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 04:11"
        },
        {
          "id": "MDRW2100032860.1.5",
          "form": "그러네. 케이티 위즈는 또 이겼던데.",
          "original_form": "그러네. 케이티 위즈는 또 이겼던데.",
          "speaker_id": "1",
          "time": "20210906 04:11"
        },
        {
          "id": "MDRW2100032860.1.6",
          "form": "그것도 11 대 0 으로 이겼던데? 대박이야 진짜",
          "original_form": "그것도 11 대 0 으로 이겼던데? 대박이야 진짜",
          "speaker_id": "2",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032860.1.7",
          "form": "올해는 케이티가 우승할 것 같아",
          "original_form": "올해는 케이티가 우승할 것 같아",
          "speaker_id": "1",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032860.1.8",
          "form": "어 정규시즌 우승은 무조건이고 한국시리즈 우승도 할 슈 있을 둣",
          "original_form": "어 정규시즌 우승은 무조건이고 한국시리즈 우승도 할 슈 있을 둣",
          "speaker_id": "2",
          "time": "20210906 04:12"
        },
        {
          "id": "MDRW2100032860.1.9",
          "form": "신생팀이 대단해. 작년에 엔씨도 그렇고.",
          "original_form": "신생팀이 대단해. 작년에 엔씨도 그렇고.",
          "speaker_id": "1",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032860.1.10",
          "form": "ㅋㅋㅋ 그거 생각하면 한화랑 기아가 진짜 더 못나보인다ㅠㅠ",
          "original_form": "ㅋㅋㅋ 그거 생각하면 한화랑 기아가 진짜 더 못나보인다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032860.1.11",
          "form": "이제 롯데랑도 게임차 벌어졌던데",
          "original_form": "이제 롯데랑도 게임차 벌어졌던데",
          "speaker_id": "2",
          "time": "20210906 04:13"
        },
        {
          "id": "MDRW2100032860.1.12",
          "form": "한화는 마땅한 투수도 타자도 없으니 어쩔 수 없지",
          "original_form": "한화는 마땅한 투수도 타자도 없으니 어쩔 수 없지",
          "speaker_id": "1",
          "time": "20210906 04:15"
        },
        {
          "id": "MDRW2100032860.1.13",
          "form": "하 오늘 문동주 한화 경기 직관 왔던데ㅠ",
          "original_form": "하 오늘 문동주 한화 경기 직관 왔던데ㅠ",
          "speaker_id": "2",
          "time": "20210906 04:16"
        },
        {
          "id": "MDRW2100032860.1.14",
          "form": "루키한테 지는 것부터 보여줬네ㅋㅋㅋㅋ",
          "original_form": "루키한테 지는 것부터 보여줬네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 04:17"
        },
        {
          "id": "MDRW2100032860.1.15",
          "form": "그래? 안타깝네.",
          "original_form": "그래? 안타깝네.",
          "speaker_id": "1",
          "time": "20210906 04:18"
        },
        {
          "id": "MDRW2100032860.1.16",
          "form": "그래도 5점씩 냈으니까 그나마 다행이지..",
          "original_form": "그래도 5점씩 냈으니까 그나마 다행이지..",
          "speaker_id": "1",
          "time": "20210906 04:20"
        },
        {
          "id": "MDRW2100032860.1.17",
          "form": "기아는 양현종이 한국 오면 경기력이 좀 나아지려나",
          "original_form": "기아는 양현종이 한국 오면 경기력이 좀 나아지려나",
          "speaker_id": "2",
          "time": "20210906 04:20"
        },
        {
          "id": "MDRW2100032860.1.18",
          "form": "이제 시즌도 막바진데 양현종 합류한다고 크게 달라지겠어.",
          "original_form": "이제 시즌도 막바진데 양현종 합류한다고 크게 달라지겠어.",
          "speaker_id": "1",
          "time": "20210906 04:21"
        },
        {
          "id": "MDRW2100032860.1.19",
          "form": "하긴 양현종 있을 때도 기아는 하위권이었지..ㅎ",
          "original_form": "하긴 양현종 있을 때도 기아는 하위권이었지..ㅎ",
