      "utterance": [
        {
          "id": "MDRW2100011868.1.1",
          "form": "안녕하세요 온라인이나 오프라인 모임 하시는거 있으신가요?",
          "original_form": "안녕하세요 온라인이나 오프라인 모임 하시는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011868.1.2",
          "form": "온라인카페 눈팅해요",
          "original_form": "온라인카페 눈팅해요",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100011868.1.3",
          "form": "아 저도 눈팅많이해여",
          "original_form": "아 저도 눈팅많이해여",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100011868.1.4",
          "form": "정모같은곳 나가본적있으세요?",
          "original_form": "정모같은곳 나가본적있으세요?",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100011868.1.5",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100011868.1.6",
          "form": "여름님은요?",
          "original_form": "여름님은요?",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100011868.1.7",
          "form": "저 독서모임 비슷한거해본적있는데",
          "original_form": "저 독서모임 비슷한거해본적있는데",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100011868.1.8",
          "form": "낯선사람들이랑 토론하려니못하겠어서 얼마못하고 그만뒀어요 ㅋㅋㅋㅋ",
          "original_form": "낯선사람들이랑 토론하려니못하겠어서 얼마못하고 그만뒀어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100011868.1.9",
          "form": "어떠셨어요",
          "original_form": "어떠셨어요",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100011868.1.10",
          "form": "아 저도 우려한부분이에요",
          "original_form": "아 저도 우려한부분이에요",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100011868.1.11",
          "form": "mbti i이신가요?",
          "original_form": "mbti i이신가요?",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100011868.1.12",
          "form": "네ㅜ낯도가리는데 말도하려니 영",
          "original_form": "네ㅜ낯도가리는데 말도하려니 영",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100011868.1.13",
          "form": "i맞아요ㅜ",
          "original_form": "i맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100011868.1.14",
          "form": "저도 i예요ㅋㅋ",
          "original_form": "저도 i예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100011868.1.15",
          "form": "그래서 눈팅만..",
          "original_form": "그래서 눈팅만..",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100011868.1.16",
          "form": "용기가 대단하시네요",
          "original_form": "용기가 대단하시네요",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100011868.1.17",
          "form": "ㅎㅎi들은 정모진짜 안맞는것같야요",
          "original_form": "ㅎㅎi들은 정모진짜 안맞는것같야요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100011868.1.18",
          "form": "온라인에서 열심히 ㅎㄹ",
          "original_form": "온라인에서 열심히 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100011868.1.19",
          "form": "호기심은 또많아서요 ㅋㅋㅋㅋ...근데 갔다가 된통혼이나고",
          "original_form": "호기심은 또많아서요 ㅋㅋㅋㅋ...근데 갔다가 된통혼이나고",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100011868.1.20",
          "form": "똥인지 된장인지 찍어먹어봐야 아시는 편이군요",
          "original_form": "똥인지 된장인지 찍어먹어봐야 아시는 편이군요",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100011868.1.21",
          "form": "저돕니다",
          "original_form": "저돕니다",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100011868.1.22",
          "form": "근데 오프 나갈 용기는 없어요ㅋㅋ",
          "original_form": "근데 오프 나갈 용기는 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100011868.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ저도그런데 완전인싸인친구가 절끄집어내는편이라 따라간거였어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도그런데 완전인싸인친구가 절끄집어내는편이라 따라간거였어요",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100011868.1.24",
          "form": "가자고해서 오 궁금은했는데 가볼까해서갔다가 아닌것같아서 도망...ㅋㅋㅋㅋ",
          "original_form": "가자고해서 오 궁금은했는데 가볼까해서갔다가 아닌것같아서 도망...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100011868.1.25",
          "form": "도망ㅋㅋㅋㅋ",
          "original_form": "도망ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100011868.1.26",
          "form": "극 외향적인 친구가 있어서 공감 갑니다..",
          "original_form": "극 외향적인 친구가 있어서 공감 갑니다..",
          "speaker_id": "2",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100011868.1.27",
          "form": "네 저희같은사람들 수집하는 외향적인친구들 꼭 있더라구요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 저희같은사람들 수집하는 외향적인친구들 꼭 있더라구요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
