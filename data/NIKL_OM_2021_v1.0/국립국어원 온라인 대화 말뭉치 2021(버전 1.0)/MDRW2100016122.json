      "utterance": [
        {
          "id": "MDRW2100016122.1.1",
          "form": ".",
          "original_form": ".",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100016122.1.2",
          "form": "마스크땜에 트러블이많이나요ㅠㅠ~",
          "original_form": "마스크땜에 트러블이많이나요ㅠㅠ~",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100016122.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100016122.1.4",
          "form": "덴탈쓰시나요?",
          "original_form": "덴탈쓰시나요?",
          "speaker_id": "1",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100016122.1.5",
          "form": "전 94써요ㅠㅠ",
          "original_form": "전 94써요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100016122.1.6",
          "form": "매일요 ㅠㅠ 제가 판매알바를 해서ㅠㅠ 여름에도 94만썼어요ㅠㅠ",
          "original_form": "매일요 ㅠㅠ 제가 판매알바를 해서ㅠㅠ 여름에도 94만썼어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100016122.1.7",
          "form": "저도 사람 많을땐 94쓰는데",
          "original_form": "저도 사람 많을땐 94쓰는데",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100016122.1.8",
          "form": "아 어쩔수가 없네요",
          "original_form": "아 어쩔수가 없네요",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100016122.1.9",
          "form": "네ㅠㅠ 되려주말엔 덴탈쓰고나가네요ㅠㅠ",
          "original_form": "네ㅠㅠ 되려주말엔 덴탈쓰고나가네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100016122.1.10",
          "form": "가까이 접촉하고 대화할일이적으니ㅠㅜ",
          "original_form": "가까이 접촉하고 대화할일이적으니ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100016122.1.11",
          "form": "진정팩이 좋다는데 저는 뭐 낫을땐 아무것도 안쓰고 싶더라구요",
          "original_form": "진정팩이 좋다는데 저는 뭐 낫을땐 아무것도 안쓰고 싶더라구요",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100016122.1.12",
          "form": "트러블패치를 사용하시나요?",
          "original_form": "트러블패치를 사용하시나요?",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100016122.1.13",
          "form": "네ㅠㅠ 고름생기고할땐 붙이고나와요ㅠ!",
          "original_form": "네ㅠㅠ 고름생기고할땐 붙이고나와요ㅠ!",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100016122.1.14",
          "form": "오 어디꺼 쓰세요?",
          "original_form": "오 어디꺼 쓰세요?",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100016122.1.15",
          "form": "저는 올영꺼 써요",
          "original_form": "저는 올영꺼 써요",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100016122.1.16",
          "form": "전 브랜드까진 모르겟는데 집에 3가지잇어요! 아모레사이트에서 파눈것도사보고",
          "original_form": "전 브랜드까진 모르겟는데 집에 3가지잇어요! 아모레사이트에서 파눈것도사보고",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100016122.1.17",
          "form": "핫딜같은거뜨면 사보고 해서ㅠㅠ",
          "original_form": "핫딜같은거뜨면 사보고 해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100016122.1.18",
          "form": "현명한소비자시네요",
          "original_form": "현명한소비자시네요",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100016122.1.19",
          "form": "이것저것써봣는데 정착한건없어요ㅠ올영껀어떤가요?",
          "original_form": "이것저것써봣는데 정착한건없어요ㅠ올영껀어떤가요?",
          "speaker_id": "2",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100016122.1.20",
          "form": "전 뭐 올라오면 케어존인가",
          "original_form": "전 뭐 올라오면 케어존인가",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100016122.1.21",
          "form": "짜면 올영꺼써요",
          "original_form": "짜면 올영꺼써요",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100016122.1.22",
          "form": "얇은필름이랑습윤패드 있는 차이가 있어요",
          "original_form": "얇은필름이랑습윤패드 있는 차이가 있어요",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100016122.1.23",
          "form": "추천한다기보다 가성비 나쁘지 않아요",
          "original_form": "추천한다기보다 가성비 나쁘지 않아요",
          "speaker_id": "1",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100016122.1.24",
          "form": "저도 케어존써본거같은데 제기억엔 엄청얇은거아닌가요??",
          "original_form": "저도 케어존써본거같은데 제기억엔 엄청얇은거아닌가요??",
          "speaker_id": "2",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100016122.1.25",
          "form": "가격도 천차만별이에요!",
          "original_form": "가격도 천차만별이에요!",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100016122.1.26",
          "form": "맞아요 곪기전에 진정되거나",
          "original_form": "맞아요 곪기전에 진정되거나",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100016122.1.27",
          "form": "아싸리 빨리 곪게 해줘요",
          "original_form": "아싸리 빨리 곪게 해줘요",
          "speaker_id": "1",
          "time": "20210827 14:11"
        }
      ]
    }
  ]
