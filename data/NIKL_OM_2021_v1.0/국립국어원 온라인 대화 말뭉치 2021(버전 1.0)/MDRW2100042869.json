      "utterance": [
        {
          "id": "MDRW2100042869.1.1",
          "form": "아르바이트 해봤어요?",
          "original_form": "아르바이트 해봤어요?",
          "speaker_id": "1",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100042869.1.2",
          "form": "안녕하세요~ 네 해봤어요",
          "original_form": "안녕하세요~ 네 해봤어요",
          "speaker_id": "2",
          "time": "20210907 10:25"
        },
        {
          "id": "MDRW2100042869.1.3",
          "form": "우와 전 아직 그런거 안해봤어요 어떤 알바가 제일 기억나요?",
          "original_form": "우와 전 아직 그런거 안해봤어요 어떤 알바가 제일 기억나요?",
          "speaker_id": "1",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100042869.1.4",
          "form": "호 텔에서 단기 알바 했던게 가장 재밌었던 것 같아요 ㅎㅎ",
          "original_form": "호 텔에서 단기 알바 했던게 가장 재밌었던 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042869.1.5",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042869.1.6",
          "form": "네 그래서 사실 알바하는거 조금 로망있어요",
          "original_form": "네 그래서 사실 알바하는거 조금 로망있어요",
          "speaker_id": "1",
          "time": "20210907 10:34"
        },
        {
          "id": "MDRW2100042869.1.7",
          "form": "ㅎㅎ 근데 별거없긴해요",
          "original_form": "ㅎㅎ 근데 별거없긴해요",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100042869.1.8",
          "form": "대학가면 할 수 있는 알바들 늘어날거에요!",
          "original_form": "대학가면 할 수 있는 알바들 늘어날거에요!",
          "speaker_id": "2",
          "time": "20210907 10:36"
        },
        {
          "id": "MDRW2100042869.1.9",
          "form": "알바하고 취업하신거예요?",
          "original_form": "알바하고 취업하신거예요?",
          "speaker_id": "1",
          "time": "20210907 10:36"
        },
        {
          "id": "MDRW2100042869.1.10",
          "form": "사실 전 고등학생때 알바했었어요",
          "original_form": "사실 전 고등학생때 알바했었어요",
          "speaker_id": "2",
          "time": "20210907 10:37"
        },
        {
          "id": "MDRW2100042869.1.11",
          "form": "길게는 안하고 조금씩",
          "original_form": "길게는 안하고 조금씩",
          "speaker_id": "2",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100042869.1.12",
          "form": "스트레스는 안받았어요?",
          "original_form": "스트레스는 안받았어요?",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100042869.1.13",
          "form": "아우 엄청 받았뎌",
          "original_form": "아우 엄청 받았뎌",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042869.1.14",
          "form": "받았죠",
          "original_form": "받았죠",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042869.1.15",
          "form": "어후.. 그래도 그때 경험이 도움이 돼요?",
          "original_form": "어후.. 그래도 그때 경험이 도움이 돼요?",
          "speaker_id": "1",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042869.1.16",
          "form": "저 원래 환타 안좋아하는데 롯데리아 알바할때 맨날 마셨어요 ㅋㅋㅋ 환타 먹어야 좀 속이 풀리는 것 같아섴ㅋㅋㅋ",
          "original_form": "저 원래 환타 안좋아하는데 롯데리아 알바할때 맨날 마셨어요 ㅋㅋㅋ 환타 먹어야 좀 속이 풀리는 것 같아섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042869.1.17",
          "form": "솔직히 도움된다기보단 되돌아보면 즐거운 경험인 것 같아요",
          "original_form": "솔직히 도움된다기보단 되돌아보면 즐거운 경험인 것 같아요",
          "speaker_id": "2",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100042869.1.18",
          "form": "오 그래도 돌아봐서 즐겁다겨 생각되면 좋은 경험이었네요",
          "original_form": "오 그래도 돌아봐서 즐겁다겨 생각되면 좋은 경험이었네요",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100042869.1.19",
          "form": "네네맞아요",
          "original_form": "네네맞아요",
          "speaker_id": "2",
