      "utterance": [
        {
          "id": "MDRW2100046402.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211020 18:07"
        },
        {
          "id": "MDRW2100046402.1.2",
          "form": "안녕하세요! 저녁은 드셨나요?",
          "original_form": "안녕하세요! 저녁은 드셨나요?",
          "speaker_id": "2",
          "time": "20211020 18:46"
        },
        {
          "id": "MDRW2100046402.1.3",
          "form": "네 이제 막 먹었어요 식사하셨나요?",
          "original_form": "네 이제 막 먹었어요 식사하셨나요?",
          "speaker_id": "1",
          "time": "20211020 19:21"
        },
        {
          "id": "MDRW2100046402.1.4",
          "form": "네 ㅎㅎ 저는 스파게티를 먹었어요",
          "original_form": "네 ㅎㅎ 저는 스파게티를 먹었어요",
          "speaker_id": "2",
          "time": "20211020 19:47"
        },
        {
          "id": "MDRW2100046402.1.5",
          "form": "저는 그냥 집밥 ㅋㅋ",
          "original_form": "저는 그냥 집밥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 19:47"
        },
        {
          "id": "MDRW2100046402.1.6",
          "form": "저도 집에서 해먹었어요ㅋㅋ 요즘 소스가 잘 나오더라구요",
          "original_form": "저도 집에서 해먹었어요ㅋㅋ 요즘 소스가 잘 나오더라구요",
          "speaker_id": "2",
          "time": "20211021 02:54"
        },
        {
          "id": "MDRW2100046402.1.7",
          "form": "퇴근시간이 다가옵니다 헤헤",
          "original_form": "퇴근시간이 다가옵니다 헤헤",
          "speaker_id": "1",
          "time": "20211021 17:16"
        },
        {
          "id": "MDRW2100046402.1.8",
          "form": "ㅎㅎㅎ맞아요 조금만 더 있음 또 저녁시간이네요?!",
          "original_form": "ㅎㅎㅎ맞아요 조금만 더 있음 또 저녁시간이네요?!",
          "speaker_id": "2",
          "time": "20211021 17:17"
        },
        {
          "id": "MDRW2100046402.1.9",
          "form": "전 어제 면 먹어서 오늘은 쌀먹으려구요 ㅋㅋ",
          "original_form": "전 어제 면 먹어서 오늘은 쌀먹으려구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211021 17:17"
        },
        {
          "id": "MDRW2100046402.1.10",
          "form": "전 오늘도 집밥 ㅠ",
          "original_form": "전 오늘도 집밥 ㅠ",
          "speaker_id": "1",
          "time": "20211021 17:19"
        },
        {
          "id": "MDRW2100046402.1.11",
          "form": "집에서 먹는 집밥이 최고죠!",
          "original_form": "집에서 먹는 집밥이 최고죠!",
          "speaker_id": "2",
          "time": "20211021 17:20"
        },
        {
          "id": "MDRW2100046402.1.12",
          "form": "그렇긴 한데 맨날 먹으면 좀 질려요 ㅋㅋ",
          "original_form": "그렇긴 한데 맨날 먹으면 좀 질려요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211021 17:20"
        },
        {
          "id": "MDRW2100046402.1.13",
          "form": "타지에 가면 항상 샹각나는데",
          "original_form": "타지에 가면 항상 샹각나는데",
          "speaker_id": "1",
          "time": "20211021 17:20"
        },
        {
          "id": "MDRW2100046402.1.14",
          "form": "ㅋㅋㅋ된찌 김찌 이런건 집떠나면 바로 생각나죠 ㅠ",
          "original_form": "ㅋㅋㅋ된찌 김찌 이런건 집떠나면 바로 생각나죠 ㅠ",
          "speaker_id": "2",
          "time": "20211021 17:21"
        },
        {
          "id": "MDRW2100046402.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211021 17:21"
        },
        {
          "id": "MDRW2100046402.1.16",
          "form": "오늘은 된찌 먹어야겠어요",
          "original_form": "오늘은 된찌 먹어야겠어요",
          "speaker_id": "1",
          "time": "20211021 17:22"
        },
        {
          "id": "MDRW2100046402.1.17",
          "form": "와우 맛있는 된찌와 함께 멋진 저녁시간 보내시길..",
          "original_form": "와우 맛있는 된찌와 함께 멋진 저녁시간 보내시길..",
          "speaker_id": "2",
