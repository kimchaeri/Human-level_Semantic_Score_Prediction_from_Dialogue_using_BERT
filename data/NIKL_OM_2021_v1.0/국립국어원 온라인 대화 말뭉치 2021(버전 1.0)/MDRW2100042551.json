      "utterance": [
        {
          "id": "MDRW2100042551.1.1",
          "form": "ㅋㅋㅋㅋ 저 상품평에 많이 당하는편...",
          "original_form": "ㅋㅋㅋㅋ 저 상품평에 많이 당하는편...",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042551.1.2",
          "form": "근데 그래도 후회는 없습니닼ㅋㅋㅋ",
          "original_form": "근데 그래도 후회는 없습니닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042551.1.3",
          "form": "맞아요.. 먹어봐야 알죸ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요.. 먹어봐야 알죸ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042551.1.4",
          "form": "안사곤 절대몰라뇨",
          "original_form": "안사곤 절대몰라뇨",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042551.1.5",
          "form": "ㅋㅋㅋ 요즘 또 상품평에 홀려서 산 물건이 있죠",
          "original_form": "ㅋㅋㅋ 요즘 또 상품평에 홀려서 산 물건이 있죠",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042551.1.6",
          "form": "그것은 트러플 핫소스...",
          "original_form": "그것은 트러플 핫소스...",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042551.1.7",
          "form": "우와트러플 ㅋㅋㅋㅋ",
          "original_form": "우와트러플 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042551.1.8",
          "form": "근데직접적으로 트러플을",
          "original_form": "근데직접적으로 트러플을",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042551.1.9",
          "form": "먹어본적이없어서",
          "original_form": "먹어본적이없어서",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042551.1.10",
          "form": "잇고없고차이를모르겠어욬ㅋㅋㅋ",
          "original_form": "잇고없고차이를모르겠어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042551.1.11",
          "form": "저두욬ㅋㅋ",
          "original_form": "저두욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042551.1.12",
          "form": "항상 궁금하지만 비싸섴ㅋㅋㅋㅋㅋ",
          "original_form": "항상 궁금하지만 비싸섴ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042551.1.13",
          "form": "그러니까요 ㅋㅋㅋㅋ",
          "original_form": "그러니까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042551.1.14",
          "form": "근데 트러플은 생으로 보관하기가 힘들어서 건조시키는거래요",
          "original_form": "근데 트러플은 생으로 보관하기가 힘들어서 건조시키는거래요",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042551.1.15",
          "form": "저 이번에 핫소스 사려다가 알게됐어요 ㅋㅋㅋㅋ",
          "original_form": "저 이번에 핫소스 사려다가 알게됐어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042551.1.16",
          "form": "아하 ㅋㅋ",
          "original_form": "아하 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042551.1.17",
          "form": "그냥 뭔가 넣으면엄청맛나다는데",
          "original_form": "그냥 뭔가 넣으면엄청맛나다는데",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042551.1.18",
          "form": "그 차이가궁금해욬ㅋㅋㅋㅋ",
          "original_form": "그 차이가궁금해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042551.1.19",
          "form": "트러플 핫소스를 사고 트러플 오일까지 사버렸습니다!!",
          "original_form": "트러플 핫소스를 사고 트러플 오일까지 사버렸습니다!!",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042551.1.20",
          "form": "풍미있어지는 향이란걸 알아버렸어요!!!",
          "original_form": "풍미있어지는 향이란걸 알아버렸어요!!!",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042551.1.21",
          "form": "오워어어어ㅓㅇ",
          "original_form": "오워어어어ㅓㅇ",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042551.1.22",
          "form": "풍미가 있어지는 향!",
          "original_form": "풍미가 있어지는 향!",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042551.1.23",
          "form": "음 거의 맛소금 급인건가요?",
          "original_form": "음 거의 맛소금 급인건가요?",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042551.1.24",
          "form": "네 맛소금 느낌이에요!!",
          "original_form": "네 맛소금 느낌이에요!!",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042551.1.25",
          "form": "핫소스 먹어보니까 튀김 요리에 잘어울리더라고요",
          "original_form": "핫소스 먹어보니까 튀김 요리에 잘어울리더라고요",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042551.1.26",
          "form": "샌드위치에 뿌려먹으면 또 맛있고요",
          "original_form": "샌드위치에 뿌려먹으면 또 맛있고요",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042551.1.27",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042551.1.28",
          "form": "어떤 트러플 오일사셨어요?",
          "original_form": "어떤 트러플 오일사셨어요?",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042551.1.29",
          "form": "스프레이로 뿌리는걸로 샀어요!!",
          "original_form": "스프레이로 뿌리는걸로 샀어요!!",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042551.1.30",
          "form": "사비니 트러플오일인가봐여",
          "original_form": "사비니 트러플오일인가봐여",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042551.1.31",
          "form": "워어 그런것두있군료",
          "original_form": "워어 그런것두있군료",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042551.1.32",
          "form": "스프레이 좋은거같아요",
          "original_form": "스프레이 좋은거같아요",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042551.1.33",
          "form": "추석이다보니 선물세트 잔득나와욬ㅋㅋㅋㅋ",
          "original_form": "추석이다보니 선물세트 잔득나와욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042551.1.34",
          "form": "스프레이 추천하는게",
          "original_form": "스프레이 추천하는게",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042551.1.35",
          "form": "살짝 뿌려먹어야해서",
          "original_form": "살짝 뿌려먹어야해서",
          "speaker_id": "2",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042551.1.36",
          "form": "칙칙 두번만 하면 좋더라고요",
          "original_form": "칙칙 두번만 하면 좋더라고요",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042551.1.37",
          "form": "너무 뿌리면 향이 강해서!!",
          "original_form": "너무 뿌리면 향이 강해서!!",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042551.1.38",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042551.1.39",
          "form": "정말 좋은 정보에욬ㅋㅋㅋ",
          "original_form": "정말 좋은 정보에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042551.1.40",
          "form": "그런거 하나도 몰라서 ㅎㅎ",
          "original_form": "그런거 하나도 몰라서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042551.1.41",
          "form": "보통 어디에먹으면되나요?",
          "original_form": "보통 어디에먹으면되나요?",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042551.1.42",
          "form": "스파게티랑",
          "original_form": "스파게티랑",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042551.1.43",
          "form": "샌드위치랑",
          "original_form": "샌드위치랑",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042551.1.44",
          "form": "튀김요리는 의외고 ㅎㅎ",
          "original_form": "튀김요리는 의외고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042551.1.45",
          "form": "넹 그런거에 살짝 뿌려먹으면 좋아요",
          "original_form": "넹 그런거에 살짝 뿌려먹으면 좋아요",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042551.1.46",
          "form": "트러플 핫소스가 진짜 기름진거에 잘 어울려요",
          "original_form": "트러플 핫소스가 진짜 기름진거에 잘 어울려요",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042551.1.47",
          "form": "오호홍 ㅋㅋㅋㅋ 그건 핫소스라서 그런거같아요 ㅎㅎㅎㅎ",
          "original_form": "오호홍 ㅋㅋㅋㅋ 그건 핫소스라서 그런거같아요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042551.1.48",
          "form": "진짜 매운건 기름진거좋죠",
          "original_form": "진짜 매운건 기름진거좋죠",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042551.1.49",
          "form": "샌드위치 가게에서 비싼맛 날때 있잖아요",
          "original_form": "샌드위치 가게에서 비싼맛 날때 있잖아요",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042551.1.50",
          "form": "그게 트러플 핫소스 덕분이구나를 알게됐습니다",
          "original_form": "그게 트러플 핫소스 덕분이구나를 알게됐습니다",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042551.1.51",
          "form": "ㅋㅋㅋㅋ 비싼맛이라고 하긴 웃기긴 한뎈ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 비싼맛이라고 하긴 웃기긴 한뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042551.1.52",
          "form": "고급진맛??",
          "original_form": "고급진맛??",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042551.1.53",
          "form": "정말 사보고싶네욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "정말 사보고싶네욬ㅋㅋㅋㅋㅋㅋㅋ",
