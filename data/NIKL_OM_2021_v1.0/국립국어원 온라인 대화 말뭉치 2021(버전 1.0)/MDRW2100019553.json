          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019553.1.6",
          "form": "타고난선택이십니다 ",
          "original_form": "타고난선택이십니다 ",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019553.1.7",
          "form": "감자도 당연히 시키겼겠죠?!",
          "original_form": "감자도 당연히 시키겼겠죠?!",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019553.1.8",
          "form": "감자에 튀김까지죠!",
          "original_form": "감자에 튀김까지죠!",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019553.1.9",
          "form": "역시 굳굳입니다 ㅋㅋㅋ",
          "original_form": "역시 굳굳입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019553.1.10",
          "form": "거기 대패볶음밥도 맛있어요",
          "original_form": "거기 대패볶음밥도 맛있어요",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019553.1.11",
          "form": "먹는거에 진심이라",
          "original_form": "먹는거에 진심이라",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019553.1.12",
          "form": "아아 맞아여!!!",
          "original_form": "아아 맞아여!!!",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019553.1.13",
