          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.6",
          "form": "하하하!",
          "original_form": "하하하!",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.7",
          "form": "보내줘요",
          "original_form": "보내줘요",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.8",
          "form": "오늘 날씨도 꾸리꾸리하고",
          "original_form": "오늘 날씨도 꾸리꾸리하고",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.9",
          "form": "여튼그럼",
          "original_form": "여튼그럼",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.10",
          "form": "그니깐요",
          "original_form": "그니깐요",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.11",
          "form": "이런날 집에잇어서",
          "original_form": "이런날 집에잇어서",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.12",
          "form": "참좋네요",
          "original_form": "참좋네요",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.13",
          "form": "약속없나요",
          "original_form": "약속없나요",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018772.1.14",
          "form": "비오는날 약속잡는",
          "original_form": "비오는날 약속잡는",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018772.1.15",
          "form": "하수가있나요",
          "original_form": "하수가있나요",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018772.1.16",
          "form": "오ㅡ홍",
          "original_form": "오ㅡ홍",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018772.1.17",
          "form": "이씨 없어서",
          "original_form": "이씨 없어서",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018772.1.18",
          "form": "고씨 혼자 일하심",
          "original_form": "고씨 혼자 일하심",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018772.1.19",
          "form": "많이바쁘심?",
          "original_form": "많이바쁘심?",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018772.1.20",
          "form": "옆에서 화이팅이라고 작게 전달해주삼",
          "original_form": "옆에서 화이팅이라고 작게 전달해주삼",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018772.1.21",
          "form": "플친 문의 옴",
          "original_form": "플친 문의 옴",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018772.1.22",
          "form": "걍 대화방보여드림",
          "original_form": "걍 대화방보여드림",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018772.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018772.1.24",
          "form": "고씨 화이팅!",
          "original_form": "고씨 화이팅!",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018772.1.25",
          "form": "이씨",
          "original_form": "이씨",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018772.1.26",
          "form": "플친많이옴??",
          "original_form": "플친많이옴??",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018772.1.27",
          "form": "일 관련해서",
          "original_form": "일 관련해서",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018772.1.28",
          "form": "올게없는데",
          "original_form": "올게없는데",
