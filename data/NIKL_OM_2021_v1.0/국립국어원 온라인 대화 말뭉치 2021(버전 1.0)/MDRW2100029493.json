        },
        {
          "id": "MDRW2100029493.1.2",
          "form": "네 혼자사니까 집안일을 대신 해줄 사람이 없네요 ㅋㅋㅋ",
          "original_form": "네 혼자사니까 집안일을 대신 해줄 사람이 없네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100029493.1.3",
          "form": "집안일 하시나요???",
          "original_form": "집안일 하시나요???",
