      "utterance": [
        {
          "id": "MDRW2100037502.1.1",
          "form": "곰젤리 하리보인가요?",
          "original_form": "곰젤리 하리보인가요?",
          "speaker_id": "2",
          "time": "20210901 01:50"
        },
        {
          "id": "MDRW2100037502.1.2",
          "form": "하리보는 사랑입니다",
          "original_form": "하리보는 사랑입니다{emoji:♡}",
          "speaker_id": "2",
          "time": "20210901 01:50"
        },
        {
          "id": "MDRW2100037502.1.3",
          "form": "네 하리보 맞아요ㅎ",
          "original_form": "네 하리보 맞아요ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:51"
        },
        {
          "id": "MDRW2100037502.1.4",
          "form": "하리보 노랑이인가요?",
          "original_form": "하리보 노랑이인가요?",
          "speaker_id": "2",
          "time": "20210901 01:51"
        },
        {
          "id": "MDRW2100037502.1.5",
          "form": "오후에 질겅질겅 씹으며 일하쥬ㅎ",
          "original_form": "오후에 질겅질겅 씹으며 일하쥬ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:51"
        },
        {
          "id": "MDRW2100037502.1.6",
          "form": "하리보 종류 넘나 많은것",
          "original_form": "하리보 종류 넘나 많은것",
          "speaker_id": "2",
          "time": "20210901 01:51"
        },
        {
          "id": "MDRW2100037502.1.7",
          "form": "섞여 있어요ㅎ",
          "original_form": "섞여 있어요ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:51"
        },
        {
          "id": "MDRW2100037502.1.8",
          "form": "캬 하리보는 그맛이쥬!!! 질겅질겅",
          "original_form": "캬 하리보는 그맛이쥬!!! 질겅질겅",
          "speaker_id": "2",
          "time": "20210901 01:51"
        },
        {
          "id": "MDRW2100037502.1.9",
          "form": "저는 기본이 제일 맛있더라구요!!! 다른거 과일즙 들어있고 이런건 너무 달아요ㅠ",
          "original_form": "저는 기본이 제일 맛있더라구요!!! 다른거 과일즙 들어있고 이런건 너무 달아요ㅠ",
          "speaker_id": "2",
          "time": "20210901 01:52"
        },
        {
          "id": "MDRW2100037502.1.10",
          "form": "ㅎㅎ 기본이 노랑이었나요?",
          "original_form": "ㅎㅎ 기본이 노랑이었나요?",
          "speaker_id": "1",
          "time": "20210901 01:52"
        },
        {
          "id": "MDRW2100037502.1.11",
          "form": "곰젤리는 귀엽기도 하고 맛도 있는데 씹는맛이 일품이에욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "곰젤리는 귀엽기도 하고 맛도 있는데 씹는맛이 일품이에욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:52"
        },
        {
          "id": "MDRW2100037502.1.12",
          "form": "보통 하리보 하면 골드베렌?",
          "original_form": "보통 하리보 하면 골드베렌?",
          "speaker_id": "2",
          "time": "20210901 01:52"
        },
        {
          "id": "MDRW2100037502.1.13",
          "form": "종류는 많은데 과일즙 들어간 건 없었어용",
          "original_form": "종류는 많은데 과일즙 들어간 건 없었어용",
          "speaker_id": "1",
          "time": "20210901 01:52"
        },
        {
          "id": "MDRW2100037502.1.14",
          "form": "그 노랑이 말하는거 같아요ㅎㅎ",
          "original_form": "그 노랑이 말하는거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:53"
        },
        {
          "id": "MDRW2100037502.1.15",
          "form": "앗 그건 다른건가보다",
          "original_form": "앗 그건 다른건가보다",
          "speaker_id": "2",
          "time": "20210901 01:53"
        },
        {
          "id": "MDRW2100037502.1.16",
          "form": "하리보 마니아시군요ㅎ",
          "original_form": "하리보 마니아시군요ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:53"
        },
        {
          "id": "MDRW2100037502.1.17",
          "form": "하리보 좋아해요~",
          "original_form": "하리보 좋아해요~",
          "speaker_id": "2",
          "time": "20210901 01:53"
        },
        {
          "id": "MDRW2100037502.1.18",
          "form": "그 통으로 사다가 하나씩 꺼내먹고 그랬어요 ㅋㅋㅋ",
          "original_form": "그 통으로 사다가 하나씩 꺼내먹고 그랬어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:53"
        },
        {
          "id": "MDRW2100037502.1.19",
          "form": "젤리류가 치아에는 안 좋다는데 진짜 넋놓고 씹게 되더라구요ㅎ",
          "original_form": "젤리류가 치아에는 안 좋다는데 진짜 넋놓고 씹게 되더라구요ㅎ",
