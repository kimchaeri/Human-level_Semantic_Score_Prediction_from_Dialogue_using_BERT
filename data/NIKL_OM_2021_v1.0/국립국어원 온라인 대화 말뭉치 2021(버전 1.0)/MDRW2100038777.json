      "utterance": [
        {
          "id": "MDRW2100038777.1.1",
          "form": "김밥요 name1님은여",
          "original_form": "김밥요 &name1&님은여",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038777.1.2",
          "form": "전 빵이요",
          "original_form": "전 빵이요",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038777.1.3",
          "form": "단팥빵",
          "original_form": "단팥빵",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038777.1.4",
          "form": "점심은 드셨어요?",
          "original_form": "점심은 드셨어요?",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038777.1.5",
          "form": "건강식아니네요",
          "original_form": "건강식아니네요",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038777.1.6",
          "form": "점심 먹엇죠 name1님은여",
          "original_form": "점심 먹엇죠 &name1&님은여",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038777.1.7",
          "form": "점심도 단팥빵이요'...",
          "original_form": "점심도 단팥빵이요'...",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038777.1.8",
          "form": "건강식이 아니네요 222",
          "original_form": "건강식이 아니네요 222",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038777.1.9",
          "form": "...아침은 라면이요",
          "original_form": "...아침은 라면이요",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038777.1.10",
          "form": "그와중에 세까 챙겨먹었지요ㅋㅋㅋ",
          "original_form": "그와중에 세까 챙겨먹었지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038777.1.11",
          "form": "왜그러시죠 요새 단명이 유행인가요",
          "original_form": "왜그러시죠 요새 단명이 유행인가요",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038777.1.12",
          "form": "오늘 일이 빨라서ㅜㅜ",
          "original_form": "오늘 일이 빨라서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038777.1.13",
          "form": "점심에 먹던 빵 챙겨나왔죠.'",
          "original_form": "점심에 먹던 빵 챙겨나왔죠.'",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038777.1.14",
          "form": "돈번거 의사한테 기부하고 싶으시군요 ㅋㅋ",
          "original_form": "돈번거 의사한테 기부하고 싶으시군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038777.1.15",
          "form": "내일은 맛있는거 먹을꺼에요",
          "original_form": "내일은 맛있는거 먹을꺼에요",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038777.1.16",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038777.1.17",
          "form": "쨋든 세끼 드신것에 큰 의미가 있네요",
          "original_form": "쨋든 세끼 드신것에 큰 의미가 있네요",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038777.1.18",
          "form": "야채구이먹엇슈",
          "original_form": "야채구이먹엇슈",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038777.1.19",
          "form": "그치요",
          "original_form": "그치요",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038777.1.20",
          "form": "그런거죠ㅋ",
          "original_form": "그런거죠ㅋ",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038777.1.21",
          "form": "전에는 삶은달걀 사서 드신다더니요",
          "original_form": "전에는 삶은달걀 사서 드신다더니요",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038777.1.22",
          "form": "이제 단팥빵이군요",
          "original_form": "이제 단팥빵이군요",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038777.1.23",
          "form": "저 돈이 없어요ㅋ",
          "original_form": "저 돈이 없어요ㅋ",
