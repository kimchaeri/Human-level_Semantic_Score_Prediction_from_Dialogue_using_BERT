      "utterance": [
        {
          "id": "MDRW2100044471.1.1",
          "form": "인상 깊은 겜!",
          "original_form": "인상 깊은 겜!",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044471.1.2",
          "form": "있나용?",
          "original_form": "있나용?",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044471.1.3",
          "form": "맞아 나 최근에 러스티 레이크 호텔 했어!",
          "original_form": "맞아 나 최근에 러스티 레이크 호텔 했어!",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044471.1.4",
          "form": "한글판 나온거 뒤늦게 안....",
          "original_form": "한글판 나온거 뒤늦게 안....",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044471.1.5",
          "form": "와 대박 요즘",
          "original_form": "와 대박 요즘",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.6",
          "form": "스트리머들 엄청 많이하더라구",
          "original_form": "스트리머들 엄청 많이하더라구",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.8",
          "form": "한글판이 언제 나왔는지 모르겠네",
          "original_form": "한글판이 언제 나왔는지 모르겠네",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.9",
          "form": "맞아맞아 햄튜브님 다음에 지컨님도 올리고...그래서 너무 좋았따네요",
          "original_form": "맞아맞아 햄튜브님 다음에 지컨님도 올리고...그래서 너무 좋았따네요",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.10",
          "form": "나는 영어로 했는지도 한글로 했는지도 기억이 안나서",
          "original_form": "나는 영어로 했는지도 한글로 했는지도 기억이 안나서",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.11",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.12",
          "form": "지컨님 영상 제목이 너무 웃겼어",
          "original_form": "지컨님 영상 제목이 너무 웃겼어",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.13",
          "form": "아 ㅋ ㅋ ㅋ",
          "original_form": "아 ㅋ ㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044471.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
