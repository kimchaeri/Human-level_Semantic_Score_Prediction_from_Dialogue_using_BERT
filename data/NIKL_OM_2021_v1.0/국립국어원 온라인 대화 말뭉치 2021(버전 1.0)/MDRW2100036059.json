      "utterance": [
        {
          "id": "MDRW2100036059.1.1",
          "form": "뭐 구매할 고 있으세요?",
          "original_form": "뭐 구매할 고 있으세요?",
          "speaker_id": "1",
          "time": "20210901 11:25"
        },
        {
          "id": "MDRW2100036059.1.2",
          "form": "저는 떡볶이 사고싶어요",
          "original_form": "저는 떡볶이 사고싶어요",
          "speaker_id": "2",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100036059.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100036059.1.4",
          "form": "헉.. 떡볶이.. 어떤거요?",
          "original_form": "헉.. 떡볶이.. 어떤거요?",
          "speaker_id": "1",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100036059.1.5",
          "form": "저는 국민학교떡볶이요ㅋㅋㅋ",
          "original_form": "저는 국민학교떡볶이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100036059.1.6",
          "form": "헉.. 저는",
          "original_form": "헉.. 저는",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100036059.1.7",
          "form": "청년다방이요..",
          "original_form": "청년다방이요..",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100036059.1.8",
          "form": "넘나게 먹고싶어요 . ...",
          "original_form": "넘나게 먹고싶어요 . ...",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100036059.1.9",
          "form": "거기 감자튀김도 맛있는데",
          "original_form": "거기 감자튀김도 맛있는데",
          "speaker_id": "2",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100036059.1.10",
          "form": "감자튀김 좋아하시나요?",
          "original_form": "감자튀김 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100036059.1.11",
          "form": "앗 완전요",
          "original_form": "앗 완전요",
          "speaker_id": "1",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100036059.1.12",
          "form": "거기 감튀 예술이죠",
          "original_form": "거기 감튀 예술이죠",
          "speaker_id": "1",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100036059.1.13",
          "form": "청년다방 어떤 떡볶이 좋아하시나요?",
          "original_form": "청년다방 어떤 떡볶이 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100036059.1.14",
          "form": "저 그냥이요..",
          "original_form": "저 그냥이요..",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100036059.1.15",
          "form": "그냥이 차돌인가요?!",
          "original_form": "그냥이 차돌인가요?!",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100036059.1.16",
          "form": "네 기본이 차돌이랑 오징어있는데 튀김없고 고기있는거면 차돌일꺼예요",
          "original_form": "네 기본이 차돌이랑 오징어있는데 튀김없고 고기있는거면 차돌일꺼예요",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100036059.1.17",
          "form": "네 그엄 차돌이요",
          "original_form": "네 그엄 차돌이요",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100036059.1.18",
          "form": "떡도 얇아서 맛있어요ㅋㅋㅋㅋㅋ",
          "original_form": "떡도 얇아서 맛있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100036059.1.19",
          "form": "청년다방떡볶이도 밀키트로 나와서 구매하면 좋을 것 같아요",
          "original_form": "청년다방떡볶이도 밀키트로 나와서 구매하면 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100036059.1.20",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100036059.1.21",
          "form": "정말요..",
          "original_form": "정말요..",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100036059.1.22",
          "form": "완전 좋은생각",
          "original_form": "완전 좋은생각",
