      "utterance": [
        {
          "id": "MDRW2100037017.1.1",
          "form": "아 신일 선풍기 그 아이 ㅋㅋ",
          "original_form": "아 신일 선풍기 그 아이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100037017.1.2",
          "form": "완벽하지 않음ㅜ",
          "original_form": "완벽하지 않음ㅜ",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100037017.1.3",
          "form": "왜 어떰?",
          "original_form": "왜 어떰?",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100037017.1.4",
          "form": "뭔가 무소음 15데시벨 믿고샀는데",
          "original_form": "뭔가 무소음 15데시벨 믿고샀는데",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100037017.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100037017.1.6",
          "form": "오래켜두면 고개회전할때 소리남",
          "original_form": "오래켜두면 고개회전할때 소리남",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100037017.1.7",
          "form": "고개돌릴래 뚝하는 소리남",
          "original_form": "고개돌릴래 뚝하는 소리남",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100037017.1.8",
          "form": "회전소리는",
          "original_form": "회전소리는",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100037017.1.9",
          "form": "뚝뚝하거나",
          "original_form": "뚝뚝하거나",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100037017.1.10",
          "form": "잡을수없었군..팬소리만 작은갑다 ㅋ",
          "original_form": "잡을수없었군..팬소리만 작은갑다 ㅋ",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100037017.1.11",
          "form": "좌에서한번",
          "original_form": "좌에서한번",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100037017.1.12",
          "form": "우에서한번남 ㅋㅋ",
          "original_form": "우에서한번남 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100037017.1.13",
          "form": "무슨 말인지 알랑가",
          "original_form": "무슨 말인지 알랑가",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100037017.1.14",
          "form": "회전할때 소리난다능거아녀 ㅋㅋ",
          "original_form": "회전할때 소리난다능거아녀 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100037017.1.15",
          "form": "계속나는게 아니라 좌우로 꺾을때 한번씩남 ㅋㅋ",
          "original_form": "계속나는게 아니라 좌우로 꺾을때 한번씩남 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100037017.1.16",
          "form": "음..특별한결함없으면 교환안될거같은데",
          "original_form": "음..특별한결함없으면 교환안될거같은데",
          "speaker_id": "1",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100037017.1.17",
          "form": "신경쓰일정도인가 그부분을 후기에 남겨봐바",
          "original_form": "신경쓰일정도인가 그부분을 후기에 남겨봐바",
          "speaker_id": "1",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100037017.1.18",
          "form": "교환도 애매한게 또 바닥이 좀 고른곳에 두면 또 ㅋㅋ덜나고",
          "original_form": "교환도 애매한게 또 바닥이 좀 고른곳에 두면 또 ㅋㅋ덜나고",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100037017.1.19",
          "form": "오래켜놓으면 나는거라",
          "original_form": "오래켜놓으면 나는거라",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100037017.1.20",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100037017.1.21",
          "form": "수리맡기기도애매함",
          "original_form": "수리맡기기도애매함",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100037017.1.22",
          "form": "그럼 제품 상담해보믄?",
          "original_form": "그럼 제품 상담해보믄?",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100037017.1.23",
          "form": "소리가 또 낮에는 크게 안느껴짐 ㅋㅋ",
          "original_form": "소리가 또 낮에는 크게 안느껴짐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100037017.1.24",
          "form": "ㅋㅋㅋ 예민하다카겟군",
          "original_form": "ㅋㅋㅋ 예민하다카겟군",
