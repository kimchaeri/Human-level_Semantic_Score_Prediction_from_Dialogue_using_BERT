      "utterance": [
        {
          "id": "MDRW2100044686.1.1",
          "form": "원두 신맛좋아해 고소항거조아해",
          "original_form": "원두 신맛좋아해 고소항거조아해",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044686.1.2",
          "form": "난 둘 다 잘 먹는데",
          "original_form": "난 둘 다 잘 먹는데",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044686.1.3",
          "form": "고소한게 더 맛있는듯",
          "original_form": "고소한게 더 맛있는듯",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044686.1.4",
          "form": "오 신것도 잘먹는구나",
          "original_form": "오 신것도 잘먹는구나",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044686.1.5",
          "form": "커피 잘알 이군",
          "original_form": "커피 잘알 이군",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044686.1.6",
          "form": "ㅋㅋㅋㅋ너무 신 건 또...너무 다크해도 별로....",
          "original_form": "ㅋㅋㅋㅋ너무 신 건 또...너무 다크해도 별로....",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044686.1.7",
          "form": "적당한게 최고지",
          "original_form": "적당한게 최고지",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044686.1.8",
          "form": "ㅋㅋㅋㅋㅋ마자",
          "original_form": "ㅋㅋㅋㅋㅋ마자",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044686.1.9",
          "form": "초코음료 마시니?",
          "original_form": "초코음료 마시니?",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044686.1.10",
          "form": "초코음려가 뭐지?..",
          "original_form": "초코음려가 뭐지?..",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044686.1.11",
          "form": "초코라떼 초코프라푸치노 이렁거",
          "original_form": "초코라떼 초코프라푸치노 이렁거",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044686.1.12",
          "form": "아하 ㅋㅋㅋ거의 안마심",
          "original_form": "아하 ㅋㅋㅋ거의 안마심",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044686.1.13",
          "form": "너무 달아여",
          "original_form": "너무 달아여",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044686.1.14",
          "form": "녹차랔데 조아해?",
          "original_form": "녹차랔데 조아해?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044686.1.15",
          "form": "1년에 2번쯤 먹을듯",
          "original_form": "1년에 2번쯤 먹을듯",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044686.1.16",
          "form": "올 그냥 녹차를 잘 안머거?",
          "original_form": "올 그냥 녹차를 잘 안머거?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044686.1.17",
          "form": "그건 아닌데 넘 찐한 음료를 잘 안마시는듯",
          "original_form": "그건 아닌데 넘 찐한 음료를 잘 안마시는듯",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044686.1.18",
          "form": "녹차라떼 좋아하니",
          "original_form": "녹차라떼 좋아하니",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044686.1.19",
          "form": "찐한거는 단거 말하는거야?",
          "original_form": "찐한거는 단거 말하는거야?",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044686.1.20",
          "form": "보통 단맛이 찐한거 같아여 ㅋㅋㅋ",
          "original_form": "보통 단맛이 찐한거 같아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044686.1.21",
          "form": "흑임자라떼는 좋아함ㅇㅇ",
          "original_form": "흑임자라떼는 좋아함ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044686.1.22",
          "form": "올글케느끼는군",
          "original_form": "올글케느끼는군",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044686.1.23",
          "form": "아 흑인자 꼬소해",
          "original_form": "아 흑인자 꼬소해",
          "speaker_id": "1",
          "time": "20210907 23:32"
        }
      ]
