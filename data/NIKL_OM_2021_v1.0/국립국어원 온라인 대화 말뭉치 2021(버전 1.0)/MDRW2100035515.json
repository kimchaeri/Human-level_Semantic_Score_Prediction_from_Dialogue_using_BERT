      "utterance": [
        {
          "id": "MDRW2100035515.1.1",
          "form": "안녕하세여 저녁 드셨나여?",
          "original_form": "안녕하세여 저녁 드셨나여?",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100035515.1.2",
          "form": "네 안녕하세요? 먹었어요",
          "original_form": "네 안녕하세요? 먹었어요",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100035515.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100035515.1.4",
          "form": "저도 방금 먹었습니다",
          "original_form": "저도 방금 먹었습니다",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035515.1.5",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035515.1.6",
          "form": "전 비비고 만두입니다",
          "original_form": "전 비비고 만두입니다",
          "speaker_id": "2",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100035515.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100035515.1.8",
          "form": "만두만 드셨어요?",
          "original_form": "만두만 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035515.1.9",
          "form": "전 감자탕이요",
          "original_form": "전 감자탕이요",
          "speaker_id": "1",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100035515.1.10",
          "form": "네 왕만두 두개만 간장에 직어먹었어요",
          "original_form": "네 왕만두 두개만 간장에 직어먹었어요",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100035515.1.11",
          "form": "감자탕은 식당서 드셨나요?",
          "original_form": "감자탕은 식당서 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100035515.1.12",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210829 20:57"
        },
        {
          "id": "MDRW2100035515.1.13",
          "form": "배달이요 ㅎ",
          "original_form": "배달이요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100035515.1.14",
          "form": "배 차나요?",
          "original_form": "배 차나요?",
          "speaker_id": "1",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100035515.1.15",
          "form": "비비고가 워낙크고 그리고 스콘을 하나 ㄱ간식으로 먹었어요",
          "original_form": "비비고가 워낙크고 그리고 스콘을 하나 ㄱ간식으로 먹었어요",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100035515.1.16",
          "form": "그러니 실제론 스콘과 만두가 제 밥인셈이에욬",
          "original_form": "그러니 실제론 스콘과 만두가 제 밥인셈이에욬",
          "speaker_id": "2",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100035515.1.17",
          "form": "아하ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100035515.1.18",
          "form": "배고플 거 같았는데 아니여서",
          "original_form": "배고플 거 같았는데 아니여서",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100035515.1.19",
          "form": "다행이네여ㅋㅋ",
          "original_form": "다행이네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100035515.1.20",
          "form": "굶고는 못살죠! ㅋㅋㅋ",
          "original_form": "굶고는 못살죠! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100035515.1.21",
          "form": "그리고 제가 이번에 산 비비고 한섬 왕만두가 좀 커서 다른 만두 4개치 되는 느김이에요",
          "original_form": "그리고 제가 이번에 산 비비고 한섬 왕만두가 좀 커서 다른 만두 4개치 되는 느김이에요",
          "speaker_id": "2",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035515.1.22",
          "form": "피가 얇아서 더 좋아요",
          "original_form": "피가 얇아서 더 좋아요",
          "speaker_id": "2",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035515.1.23",
          "form": "ㅋㅋㅋㅋㅋ아 정말요??!",
          "original_form": "ㅋㅋㅋㅋㅋ아 정말요??!",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100035515.1.24",
          "form": "맛이 귱금하네요..",
          "original_form": "맛이 귱금하네요..",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100035515.1.25",
          "form": "다른 만두보다 고기가 더 많고 피가 얇아오",
          "original_form": "다른 만두보다 고기가 더 많고 피가 얇아오",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100035515.1.26",
          "form": "씨제이건가요 시제이 브랜드 자체는 안 좋아하는데,,, 맛있어요",
          "original_form": "씨제이건가요 시제이 브랜드 자체는 안 좋아하는데,,, 맛있어요",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100035515.1.27",
          "form": "ㅋㅋㅋㅋㅋ 전만듀를 잘 안먹거등요",
          "original_form": "ㅋㅋㅋㅋㅋ 전만듀를 잘 안먹거등요",
