      "utterance": [
        {
          "id": "MDRW2100002595.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002595.1.2",
          "form": "좋아는 하는데 자주 가는편은 아니에요",
          "original_form": "좋아는 하는데 자주 가는편은 아니에요",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002595.1.3",
          "form": "특히 요즘은 코로나 때문에...",
          "original_form": "특히 요즘은 코로나 때문에...",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002595.1.4",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002595.1.5",
          "form": "저도 넘 좋아하는데 코로나때문에 못가네요 ㅠㅠ",
          "original_form": "저도 넘 좋아하는데 코로나때문에 못가네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002595.1.6",
          "form": "코로나가 끝나야 어디든 갈텐데",
          "original_form": "코로나가 끝나야 어디든 갈텐데",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002595.1.7",
          "form": "해외여행 가고싶은데 못가니까 넘 답답해요",
          "original_form": "해외여행 가고싶은데 못가니까 넘 답답해요",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002595.1.8",
          "form": "마자요 ㅠㅠ",
          "original_form": "마자요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002595.1.9",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002595.1.10",
          "form": "편하게 해외여행가려면",
          "original_form": "편하게 해외여행가려면",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002595.1.11",
          "form": "ㅠㅠㅠ 가보고 싶은곳 있으세요?",
          "original_form": "ㅠㅠㅠ 가보고 싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002595.1.12",
          "form": "몇년은 지나야할듯해서 슬퍼요",
          "original_form": "몇년은 지나야할듯해서 슬퍼요",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002595.1.13",
          "form": "동남아가서",
          "original_form": "동남아가서",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002595.1.14",
          "form": "그러게요 ㅠㅠ 슬퍼요",
          "original_form": "그러게요 ㅠㅠ 슬퍼요",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002595.1.15",
          "form": "열대과일 맘껏먹고 싶네요ㅋㅋ",
          "original_form": "열대과일 맘껏먹고 싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002595.1.16",
          "form": "저두요 열대과일 넘 맛나요 ㅋㅋ",
          "original_form": "저두요 열대과일 넘 맛나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002595.1.17",
          "form": "아쉬운대로 수입과일먹으면서",
          "original_form": "아쉬운대로 수입과일먹으면서",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002595.1.18",
          "form": "딱 코로나 직전에 베트남 갔었는데 넘 좋더라구요",
          "original_form": "딱 코로나 직전에 베트남 갔었는데 넘 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002595.1.19",
          "form": "마음을 달래고있어요ㅠ",
          "original_form": "마음을 달래고있어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002595.1.20",
          "form": "와 타이밍 좋으셨네용",
          "original_form": "와 타이밍 좋으셨네용",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002595.1.21",
          "form": "그때 못가셨음 더 아쉬울뻔했어요",
          "original_form": "그때 못가셨음 더 아쉬울뻔했어요",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002595.1.22",
          "form": "네~ ㅠㅠ 넘 아쉬울뻔 ㅠㅠ",
          "original_form": "네~ ㅠㅠ 넘 아쉬울뻔 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002595.1.23",
          "form": "그러게요 그래도 수입과일 많아서 다행이에요",
          "original_form": "그러게요 그래도 수입과일 많아서 다행이에요",
          "speaker_id": "1",
          "time": "20210820 13:57"
        }
      ]
