      "utterance": [
        {
          "id": "MDRW2100046198.1.1",
          "form": "해초산거언제와",
          "original_form": "해초산거언제와",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046198.1.2",
          "form": "아직 배송준비중 ㅠ",
          "original_form": "아직 배송준비중 ㅠ",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046198.1.3",
          "form": "주말에가져갈랬는데",
          "original_form": "주말에가져갈랬는데",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046198.1.4",
          "form": "그때까진 올거야",
          "original_form": "그때까진 올거야",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046198.1.5",
          "form": "집가서해초비빔밥먹어야지",
          "original_form": "집가서해초비빔밥먹어야지",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046198.1.6",
          "form": "오늘보내면 알려줄게",
          "original_form": "오늘보내면 알려줄게",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046198.1.7",
          "form": "몇개 남아써",
          "original_form": "몇개 남아써",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046198.1.8",
          "form": "없는데?",
          "original_form": "없는데?",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046198.1.9",
          "form": "그게다야?",
          "original_form": "그게다야?",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046198.1.10",
          "form": "마지막 ?",
          "original_form": "마지막 ?",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100046198.1.11",
          "form": "아 배송오면 집에가서 먹는다는말이었어",
          "original_form": "아 배송오면 집에가서 먹는다는말이었어",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046198.1.12",
          "form": "아 그랬구만 나는 청국장 먹고싶다 배송언제와",
          "original_form": "아 그랬구만 나는 청국장 먹고싶다 배송언제와",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046198.1.13",
          "form": "그러게언제와",
          "original_form": "그러게언제와",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046198.1.14",
          "form": "추석전 마지막배송이랬으니까 곧 오겠지 기대중 룰루 먹을것만 쇼핑해",
          "original_form": "추석전 마지막배송이랬으니까 곧 오겠지 기대중 룰루 먹을것만 쇼핑해",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046198.1.15",
          "form": "9월되고 옷삼?",
          "original_form": "9월되고 옷삼?",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046198.1.16",
          "form": "나도 먹을것만 쇼핑",
          "original_form": "나도 먹을것만 쇼핑",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046198.1.17",
          "form": "안삼",
          "original_form": "안삼",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046198.1.18",
          "form": "살이 찌는 계절이네요",
          "original_form": "살이 찌는 계절이네요",
