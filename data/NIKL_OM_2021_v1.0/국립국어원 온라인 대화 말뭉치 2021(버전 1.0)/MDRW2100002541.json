      "utterance": [
        {
          "id": "MDRW2100002541.1.1",
          "form": "혹시 동물 같은건 키",
          "original_form": "혹시 동물 같은건 키",
          "speaker_id": "1",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002541.1.2",
          "form": "키워?",
          "original_form": "키워?",
          "speaker_id": "1",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002541.1.3",
          "form": "아니 안키워",
          "original_form": "아니 안키워",
          "speaker_id": "2",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002541.1.4",
          "form": "너는 동물 키운적 있어?",
          "original_form": "너는 동물 키운적 있어?",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002541.1.5",
          "form": "나 고양이 기른적 있어",
          "original_form": "나 고양이 기른적 있어",
          "speaker_id": "1",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002541.1.6",
          "form": "어떤 고양이였어?",
          "original_form": "어떤 고양이였어?",
          "speaker_id": "2",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002541.1.7",
          "form": "종은 모르겠는데 삼색 고양이었어",
          "original_form": "종은 모르겠는데 삼색 고양이었어",
          "speaker_id": "1",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002541.1.8",
          "form": "오 삼색고양이 귀엽겠다",
          "original_form": "오 삼색고양이 귀엽겠다",
          "speaker_id": "2",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002541.1.9",
          "form": "귀여웠지만 꽤나 오두방정 떠는 얘였다구",
          "original_form": "귀여웠지만 꽤나 오두방정 떠는 얘였다구",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002541.1.10",
          "form": "휴지를 풀어대질 않나",
          "original_form": "휴지를 풀어대질 않나",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002541.1.11",
          "form": "화분을 파헤치질 않나",
          "original_form": "화분을 파헤치질 않나",
          "speaker_id": "1",
          "time": "20210820 13:30"
        },
        {
          "id": "MDRW2100002541.1.12",
          "form": "아.. 청소하느라 힘들었겠네",
          "original_form": "아.. 청소하느라 힘들었겠네",
          "speaker_id": "2",
          "time": "20210820 13:31"
        },
        {
          "id": "MDRW2100002541.1.13",
          "form": "거실쪽으로 두다다다 하면서 도망가더라",
          "original_form": "거실쪽으로 두다다다 하면서 도망가더라",
          "speaker_id": "1",
          "time": "20210820 13:32"
        },
        {
          "id": "MDRW2100002541.1.14",
          "form": "물바닥.. 골때리더라",
          "original_form": "물바닥.. 골때리더라",
          "speaker_id": "1",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002541.1.15",
          "form": "정말 활발한 고양이였네",
          "original_form": "정말 활발한 고양이였네",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002541.1.16",
          "form": "그래도 내가 우울할땐 옆에 다가와서 마치 위로 해주는것처럼 있어주더라",
          "original_form": "그래도 내가 우울할땐 옆에 다가와서 마치 위로 해주는것처럼 있어주더라",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002541.1.17",
          "form": "와 힐링되겠네",
          "original_form": "와 힐링되겠네",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002541.1.18",
          "form": "이런것때문에 반러동물을 키우게 되는것 같더라고",
          "original_form": "이런것때문에 반러동물을 키우게 되는것 같더라고",
          "speaker_id": "1",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002541.1.19",
          "form": "넌 나중에 키울 생각 있어?",
          "original_form": "넌 나중에 키울 생각 있어?",
          "speaker_id": "1",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002541.1.20",
          "form": "여유있으면 키우로 싶어",
          "original_form": "여유있으면 키우로 싶어",
          "speaker_id": "2",
          "time": "20210820 13:38"
        }
      ]
