      "utterance": [
        {
          "id": "MDRW2100013457.1.1",
          "form": "계신 곳 비오나요?",
          "original_form": "계신 곳 비오나요?",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013457.1.2",
          "form": "여긴 좀 맑아졌어요",
          "original_form": "여긴 좀 맑아졌어요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013457.1.3",
          "form": "구름이 좀있긴한데",
          "original_form": "구름이 좀있긴한데",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013457.1.4",
          "form": "그곳은요?",
          "original_form": "그곳은요?",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013457.1.5",
          "form": "여기도 지금은 해는 잠깐나요 계속 역하게 비왔었구요",
          "original_form": "여기도 지금은 해는 잠깐나요 계속 역하게 비왔었구요",
          "speaker_id": "1",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100013457.1.6",
          "form": "약하게",
          "original_form": "약하게",
          "speaker_id": "1",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100013457.1.7",
          "form": "장마철이 얼른 지나갔음 좋겠어요",
          "original_form": "장마철이 얼른 지나갔음 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100013457.1.8",
          "form": "이제 가을이라 비오고나면 춥더라고요",
          "original_form": "이제 가을이라 비오고나면 춥더라고요",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013457.1.9",
          "form": "맞아요 더운공기가 거의사라졌어요",
          "original_form": "맞아요 더운공기가 거의사라졌어요",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013457.1.10",
          "form": "올여름 더덜워서 겨울이 추울거래요...",
          "original_form": "올여름 더덜워서 겨울이 추울거래요...",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013457.1.11",
          "form": "아이고..폭설오면 큰일일텐데ㅜㅜ",
          "original_form": "아이고..폭설오면 큰일일텐데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013457.1.12",
          "form": "그러게요 여름 충분히 더웠던거같은데 폭설 한파 걱정이예요 롱패딩 믿고 버텨야죠 ㅠ",
          "original_form": "그러게요 여름 충분히 더웠던거같은데 폭설 한파 걱정이예요 롱패딩 믿고 버텨야죠 ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013457.1.13",
          "form": "하긴 롱패딩만 있으면 겨울은 문제없죠ㅋㅋ",
          "original_form": "하긴 롱패딩만 있으면 겨울은 문제없죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013457.1.14",
          "form": "필수템이죠",
          "original_form": "필수템이죠",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013457.1.15",
          "form": "한해한해 겨울이 넘넘 추워지네요 ㅠㅠ",
          "original_form": "한해한해 겨울이 넘넘 추워지네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013457.1.16",
          "form": "여름은 에어컨있어서 살겠는데 겨울추위는 고통스러워요ㅜㅜ",
          "original_form": "여름은 에어컨있어서 살겠는데 겨울추위는 고통스러워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013457.1.17",
          "form": "진짜 교울 찬바람은 고통스럽다는 표현이 딱  적절하네요!",
          "original_form": "진짜 교울 찬바람은 고통스럽다는 표현이 딱  적절하네요!",
          "speaker_id": "1",
          "time": "20210825 15:15"
        }
      ]
