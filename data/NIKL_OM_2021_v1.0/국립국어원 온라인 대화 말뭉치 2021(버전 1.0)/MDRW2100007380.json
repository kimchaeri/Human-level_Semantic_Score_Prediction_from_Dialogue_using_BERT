      "utterance": [
        {
          "id": "MDRW2100007380.1.1",
          "form": "저랑 꿈이 같으신데 꿈은 이루셨나요",
          "original_form": "저랑 꿈이 같으신데 꿈은 이루셨나요",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007380.1.2",
          "form": "안녕하세요... 노비입니다.....",
          "original_form": "안녕하세요... 노비입니다.....",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007380.1.3",
          "form": "꿈을 이루고자 매주 로또를 사고있어요",
          "original_form": "꿈을 이루고자 매주 로또를 사고있어요",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007380.1.4",
          "form": "저도 노비입니다..",
          "original_form": "저도 노비입니다..",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007380.1.5",
          "form": "핳...",
          "original_form": "핳...",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007380.1.6",
          "form": "저는 주로 연금복권을 사요",
          "original_form": "저는 주로 연금복권을 사요",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007380.1.7",
          "form": "동변상련이네요",
          "original_form": "동변상련이네요",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007380.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007380.1.9",
          "form": "어떤 일을 하시나요",
          "original_form": "어떤 일을 하시나요",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.10",
          "form": "저도 다음에는 연금복권 긁어봐야겠네요",
          "original_form": "저도 다음에는 연금복권 긁어봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.11",
          "form": "아 저는 공노비입니다",
          "original_form": "아 저는 공노비입니다",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.12",
          "form": "헐 저두 공노비",
          "original_form": "헐 저두 공노비",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.13",
          "form": "헉스",
          "original_form": "헉스",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.14",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.15",
          "form": "에휴....",
          "original_form": "에휴....",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.16",
          "form": "나랏밥 먹는 게 쉬운 일 아니죠",
          "original_form": "나랏밥 먹는 게 쉬운 일 아니죠",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.17",
          "form": "고생많으십니다...",
          "original_form": "고생많으십니다...",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007380.1.18",
          "form": "워라밸은 좀 챙기고 계신가요",
          "original_form": "워라밸은 좀 챙기고 계신가요",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007380.1.19",
          "form": "요 세계사 워낙 다양하잖아요 저는 다행히 일이 그렇게 바쁜 곳은 아니라 괜찮습니다",
          "original_form": "요 세계사 워낙 다양하잖아요 저는 다행히 일이 그렇게 바쁜 곳은 아니라 괜찮습니다",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007380.1.20",
          "form": "살짝 바쁜 곳에 있다가 한적한 곳으로 발령이 나서...",
          "original_form": "살짝 바쁜 곳에 있다가 한적한 곳으로 발령이 나서...",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007380.1.21",
          "form": "지역도 좀 한적한 곳이라 그게 힘들지만..ㅜㅜ",
          "original_form": "지역도 좀 한적한 곳이라 그게 힘들지만..ㅜㅜ",
