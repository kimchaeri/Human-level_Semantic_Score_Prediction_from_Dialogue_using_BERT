      "utterance": [
        {
          "id": "MDRW2100040409.1.1",
          "form": "보통은 그렇겠쥬?",
          "original_form": "보통은 그렇겠쥬?",
          "speaker_id": "2",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100040409.1.2",
          "form": "ㅠㅠ 난 근데 자세가 안좋은게",
          "original_form": "ㅠㅠ 난 근데 자세가 안좋은게",
          "speaker_id": "1",
          "time": "20210904 18:49"
        },
        {
          "id": "MDRW2100040409.1.3",
          "form": "왼쪽어깨랑 불편햇단말이야",
          "original_form": "왼쪽어깨랑 불편햇단말이야",
          "speaker_id": "1",
          "time": "20210904 18:49"
        },
        {
          "id": "MDRW2100040409.1.4",
          "form": "그니깐 주사땜시 왼쪽아픈게 어깨까지 더괴로운거같아",
          "original_form": "그니깐 주사땜시 왼쪽아픈게 어깨까지 더괴로운거같아",
          "speaker_id": "1",
          "time": "20210904 18:49"
        },
        {
          "id": "MDRW2100040409.1.5",
          "form": "오른쪽은 상대적으로 왼쪽보다 아픈적이 적엇으니깐 오른쪽을 맞으면 더 나앗을까싶지만 며칠불편햇겟쥬ㅜㅋㅋㅋㅋ에라이",
          "original_form": "오른쪽은 상대적으로 왼쪽보다 아픈적이 적엇으니깐 오른쪽을 맞으면 더 나앗을까싶지만 며칠불편햇겟쥬ㅜㅋㅋㅋㅋ에라이",
          "speaker_id": "1",
          "time": "20210904 18:50"
        },
        {
          "id": "MDRW2100040409.1.6",
          "form": "헉ㅠㅠㅠㅠ 낼 되면 좀 괜찮아질라나 ㅠㅠㅠ 큰일",
          "original_form": "헉ㅠㅠㅠㅠ 낼 되면 좀 괜찮아질라나 ㅠㅠㅠ 큰일",
          "speaker_id": "2",
          "time": "20210904 19:01"
        },
        {
          "id": "MDRW2100040409.1.7",
          "form": "양쪽 다 아픈 것보다 한 쪽만 아픈게 더 나을지도 몰라..!",
          "original_form": "양쪽 다 아픈 것보다 한 쪽만 아픈게 더 나을지도 몰라..!",
          "speaker_id": "2",
          "time": "20210904 19:02"
        },
        {
          "id": "MDRW2100040409.1.8",
          "form": "그렇겟주ㅜ또 오른쪽이아프면 사용이불편하니ㅜㅜ",
          "original_form": "그렇겟주ㅜ또 오른쪽이아프면 사용이불편하니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 19:05"
        },
        {
          "id": "MDRW2100040409.1.9",
          "form": "ㅠㅜ그래도 다른 증상 없어서 또 다행이야",
          "original_form": "ㅠㅜ그래도 다른 증상 없어서 또 다행이야",
          "speaker_id": "2",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100040409.1.10",
          "form": "너도",
          "original_form": "너도",
          "speaker_id": "1",
          "time": "20210904 19:12"
        },
        {
          "id": "MDRW2100040409.1.11",
          "form": "별일 없을꺼야",
          "original_form": "별일 없을꺼야",
          "speaker_id": "1",
          "time": "20210904 19:12"
        },
        {
          "id": "MDRW2100040409.1.12",
          "form": "몇번까임 ㅋㅋㅋ",
          "original_form": "몇번까임 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:12"
        },
        {
          "id": "MDRW2100040409.1.13",
          "form": "ㅋㅋㅋㅋㅋ슬퍼 ㅋㅋㅋㅋ까일때마다",
          "original_form": "ㅋㅋㅋㅋㅋ슬퍼 ㅋㅋㅋㅋ까일때마다",
          "speaker_id": "2",
          "time": "20210904 19:29"
        },
        {
          "id": "MDRW2100040409.1.14",
          "form": "난 아빠가 자꾸 맞지말래 ㅋㅋㅋ",
          "original_form": "난 아빠가 자꾸 맞지말래 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:29"
        },
        {
          "id": "MDRW2100040409.1.15",
          "form": "ㅠㅠㅠㅠ 그러게",
          "original_form": "ㅠㅠㅠㅠ 그러게",
          "speaker_id": "1",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100040409.1.16",
          "form": "진짜 무슨일이 어찌생길지모르니",
          "original_form": "진짜 무슨일이 어찌생길지모르니",
