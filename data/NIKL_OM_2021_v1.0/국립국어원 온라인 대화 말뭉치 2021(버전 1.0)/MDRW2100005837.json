      "utterance": [
        {
          "id": "MDRW2100005837.1.1",
          "form": "요즘 커뮤해?",
          "original_form": "요즘 커뮤해?",
          "speaker_id": "1",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100005837.1.2",
          "form": "응 해 여시",
          "original_form": "응 해 여시",
          "speaker_id": "2",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100005837.1.3",
          "form": "잼나??",
          "original_form": "잼나??",
          "speaker_id": "1",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100005837.1.4",
          "form": "정보글만봐",
          "original_form": "정보글만봐",
          "speaker_id": "2",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100005837.1.5",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.6",
          "form": "얼마나해?",
          "original_form": "얼마나해?",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.7",
          "form": "나도 하는데 댓글 안달아",
          "original_form": "나도 하는데 댓글 안달아",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.8",
          "form": "반나절?",
          "original_form": "반나절?",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.9",
          "form": "그냥 정보글만 캐온다",
          "original_form": "그냥 정보글만 캐온다",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.10",
          "form": "맞아 네페이런거",
          "original_form": "맞아 네페이런거",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.11",
          "form": "마니하네",
          "original_form": "마니하네",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.12",
          "form": "마자 그리고 주식소모임가입해서",
          "original_form": "마자 그리고 주식소모임가입해서",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.13",
          "form": "틈틈히봐",
          "original_form": "틈틈히봐",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.14",
          "form": "정보얻어옴",
          "original_form": "정보얻어옴",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.15",
          "form": "네페 소중하지..",
          "original_form": "네페 소중하지..",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.16",
          "form": "오 나는 여시 공모주달글봐",
          "original_form": "오 나는 여시 공모주달글봐",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100005837.1.17",
          "form": "나도 가끔 봐 ㅋㅋㅋ",
          "original_form": "나도 가끔 봐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005837.1.18",
          "form": "소중하지",
          "original_form": "소중하지",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005837.1.19",
          "form": "공모주 해야지..",
          "original_form": "공모주 해야지..",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005837.1.20",
          "form": "알아서 정보줘서좋아",
          "original_form": "알아서 정보줘서좋아",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005837.1.21",
          "form": "낼하자",
          "original_form": "낼하자",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100005837.1.22",
          "form": "정보얻기는 커뮤만한데앖어",
          "original_form": "정보얻기는 커뮤만한데앖어",
          "speaker_id": "1",
