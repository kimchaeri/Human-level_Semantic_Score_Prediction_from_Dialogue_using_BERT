      "utterance": [
        {
          "id": "MDRW2100021955.1.1",
          "form": "차이나타운이나 중국 가보셨어요?",
          "original_form": "차이나타운이나 중국 가보셨어요?",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021955.1.2",
          "form": "넵 둘다 가봤어요",
          "original_form": "넵 둘다 가봤어요",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021955.1.3",
          "form": "거기 그 단콩물에 빵 담가먹는 아침식사 드셔보셨나요",
          "original_form": "거기 그 단콩물에 빵 담가먹는 아침식사 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021955.1.4",
          "form": "아 저그거 엄청 좋아해요",
          "original_form": "아 저그거 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021955.1.5",
          "form": "약간 그 느낌인가요 설탕 넣은 콩국수가?",
          "original_form": "약간 그 느낌인가요 설탕 넣은 콩국수가?",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021955.1.6",
          "form": "중국거가 묽기는 한데",
          "original_form": "중국거가 묽기는 한데",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021955.1.7",
          "form": "그거 비슷한 거 경주에 콩국이라고 있거든요",
          "original_form": "그거 비슷한 거 경주에 콩국이라고 있거든요",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021955.1.8",
          "form": "것두 맛있어요",
          "original_form": "것두 맛있어요",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021955.1.9",
          "form": "아 경주에도요? 그것도 단가욪?",
          "original_form": "아 경주에도요? 그것도 단가욪?",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021955.1.10",
          "form": "설탕 넣은 콩국수 그거의 시원한 버전이라고 보셔도 되겠네요",
          "original_form": "설탕 넣은 콩국수 그거의 시원한 버전이라고 보셔도 되겠네요",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021955.1.11",
          "form": "경주 콩국도 면이나 빵을 넣어 먹나요? 아님 음료수로?",
          "original_form": "경주 콩국도 면이나 빵을 넣어 먹나요? 아님 음료수로?",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021955.1.12",
          "form": "빵 들어간것도 있고 안 들어간 것도 있어요",
          "original_form": "빵 들어간것도 있고 안 들어간 것도 있어요",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021955.1.13",
          "form": "뜨뜻해서 아침으로 먹기 좋더라구요",
          "original_form": "뜨뜻해서 아침으로 먹기 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021955.1.14",
          "form": "오오 경주가면 먹어야겠어요",
          "original_form": "오오 경주가면 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021955.1.15",
          "form": "차이나타운 가서 먹은 그 맛이 안 잊혀지는데",
          "original_form": "차이나타운 가서 먹은 그 맛이 안 잊혀지는데",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021955.1.16",
          "form": "백종원이 나온 프로에서 그집을 소개한 이후론 붐벼서 못갔었거든요ㅜ",
          "original_form": "백종원이 나온 프로에서 그집을 소개한 이후론 붐벼서 못갔었거든요ㅜ",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021955.1.17",
          "form": "아 거기 유명해서 가보고 싶었는데 항상 사람 많더라구요ㅜㅜ",
          "original_form": "아 거기 유명해서 가보고 싶었는데 항상 사람 많더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021955.1.18",
          "form": "앗 아시는군요 신발원ㅜ",
          "original_form": "앗 아시는군요 신발원ㅜ",
          "speaker_id": "1",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021955.1.19",
          "form": "저 첨 갔을때는 한산했는데",
          "original_form": "저 첨 갔을때는 한산했는데",
          "speaker_id": "1",
          "time": "20210829 00:23"
        }
      ]
