      "utterance": [
        {
          "id": "MDRW2100021141.1.1",
          "form": "종교",
          "original_form": "종교",
          "speaker_id": "1",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100021141.1.2",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100021141.1.3",
          "form": "친정은 기독교라 어릴때는 다녔었는데",
          "original_form": "친정은 기독교라 어릴때는 다녔었는데",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100021141.1.4",
          "form": "지금은 안다녀요",
          "original_form": "지금은 안다녀요",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100021141.1.5",
          "form": "요즘은 종교활동은 못하잔아요?",
          "original_form": "요즘은 종교활동은 못하잔아요?",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021141.1.6",
          "form": "하긴요",
          "original_form": "하긴요",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021141.1.7",
          "form": "저도 친구랑둘이 교회간 적 읶2",
          "original_form": "저도 친구랑둘이 교회간 적 읶2",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100021141.1.8",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100021141.1.9",
          "form": "종교는 있으세요?",
          "original_form": "종교는 있으세요?",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100021141.1.10",
          "form": "전 없어요",
          "original_form": "전 없어요",
          "speaker_id": "1",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100021141.1.11",
          "form": "부모님은 교회에서 활동도 많이하시고 매주  인터넷예배도 보시는데",
          "original_form": "부모님은 교회에서 활동도 많이하시고 매주  인터넷예배도 보시는데",
          "speaker_id": "2",
          "time": "20210829 14:52"
        },
        {
          "id": "MDRW2100021141.1.12",
          "form": "아프시면서 종교에 의지를 더하시더라구요",
          "original_form": "아프시면서 종교에 의지를 더하시더라구요",
          "speaker_id": "2",
          "time": "20210829 14:52"
        },
        {
          "id": "MDRW2100021141.1.13",
          "form": "신앙이깊으시네여",
          "original_form": "신앙이깊으시네여",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100021141.1.14",
          "form": "종교모임이 건전해서 딸을 교회에 보내보기도 했어요",
          "original_form": "종교모임이 건전해서 딸을 교회에 보내보기도 했어요",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100021141.1.15",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100021141.1.16",
          "form": "요즘도요?",
          "original_form": "요즘도요?",
          "speaker_id": "1",
          "time": "20210901 09:34"
        },
        {
          "id": "MDRW2100021141.1.17",
          "form": "요즘은 안가더라구요",
          "original_form": "요즘은 안가더라구요",
          "speaker_id": "2",
          "time": "20210901 10:03"
        },
        {
          "id": "MDRW2100021141.1.18",
          "form": "한참 해외봉사도 다녀오고 열심히 다니더니  안가더라구요",
          "original_form": "한참 해외봉사도 다녀오고 열심히 다니더니  안가더라구요",
          "speaker_id": "2",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100021141.1.19",
          "form": "시기가 그러니",
          "original_form": "시기가 그러니",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100021141.1.20",
          "form": "가지고 싶은 종교는 있으세요?",
          "original_form": "가지고 싶은 종교는 있으세요?",
          "speaker_id": "2",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100021141.1.21",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210901 10:17"
        }
      ]
