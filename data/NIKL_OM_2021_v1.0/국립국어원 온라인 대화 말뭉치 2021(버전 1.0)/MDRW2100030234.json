      "utterance": [
        {
          "id": "MDRW2100030234.1.1",
          "form": "좋아하는 가수 있으신가요",
          "original_form": "좋아하는 가수 있으신가요",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030234.1.2",
          "form": "네 저 엔시티 좋아해요 ㅋㅋㅋㅌ",
          "original_form": "네 저 엔시티 좋아해요 ㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030234.1.3",
          "form": "님은용??",
          "original_form": "님은용??",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030234.1.4",
          "form": "저는 투바투 좋아해요ㅎㅎ",
          "original_form": "저는 투바투 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030234.1.5",
          "form": "우와 저 투바투 노래들 타팬인데도 갠적으로 좋아해가지고요!!",
          "original_form": "우와 저 투바투 노래들 타팬인데도 갠적으로 좋아해가지고요!!",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030234.1.6",
          "form": "그 러브쏭 그거 진짜 좋아했어요ㅋㅋㅋㅋ",
          "original_form": "그 러브쏭 그거 진짜 좋아했어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030234.1.7",
          "form": "ㅎㅎ감사합니다",
          "original_form": "ㅎㅎ감사합니다",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030234.1.8",
          "form": "저도 엔시티 노래 몇번 들었었는데",
          "original_form": "저도 엔시티 노래 몇번 들었었는데",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030234.1.9",
          "form": "최근에 칠감 듣고 꽂혀써요",
          "original_form": "최근에 칠감 듣고 꽂혀써요",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030234.1.10",
          "form": "꽂혔어요",
          "original_form": "꽂혔어요",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030234.1.11",
          "form": "칠감 진짜..개좋죠.. 그 고급진 사운드..ㅡ크",
          "original_form": "칠감 진짜..개좋죠.. 그 고급진 사운드..ㅡ크",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030234.1.12",
          "form": "뭘 좀 아시네요ㅠㅠㅠㅠ",
          "original_form": "뭘 좀 아시네요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030234.1.13",
          "form": "혹시 투바투 최애는 누군가여?",
          "original_form": "혹시 투바투 최애는 누군가여?",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030234.1.14",
          "form": "최애가 따로 없어요",
          "original_form": "최애가 따로 없어요",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030234.1.15",
          "form": "입덕멤은 수빈입니다",
          "original_form": "입덕멤은 수빈입니다",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030234.1.16",
          "form": "수빈 그친구 참 잘생겼더라고요.. 알아요,,,",
          "original_form": "수빈 그친구 참 잘생겼더라고요.. 알아요,,,",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030234.1.17",
          "form": "역시 그룹덕질하시다보면 최애가 누군지 모르게돼죸ㅋㅋ",
          "original_form": "역시 그룹덕질하시다보면 최애가 누군지 모르게돼죸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030234.1.18",
          "form": "그쵸ㅋㅋ 오래 파다보면 어느새",
          "original_form": "그쵸ㅋㅋ 오래 파다보면 어느새",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030234.1.19",
          "form": "모두 안고 가고 있더라구요",
          "original_form": "모두 안고 가고 있더라구요",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030234.1.20",
          "form": "name1님은 최애가 누구세요?",
          "original_form": "&name1&님은 최애가 누구세요?",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030234.1.21",
          "form": "저 진짜 다 좋아하는데 마크랑 태용이랑 도영이한테 마음이 조금 더 많이 가요ㅋㅋㅋㅋ",
          "original_form": "저 진짜 다 좋아하는데 마크랑 태용이랑 도영이한테 마음이 조금 더 많이 가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030234.1.22",
          "form": "입덕멤은 근데 재민입니다 !ㅎㅎ",
          "original_form": "입덕멤은 근데 재민입니다 !ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030234.1.23",
          "form": "태용님은 스우파에서 봤어요",
          "original_form": "태용님은 스우파에서 봤어요",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030234.1.24",
          "form": "마크님은 음중에서 봤었고",
          "original_form": "마크님은 음중에서 봤었고",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030234.1.25",
          "form": "도영님이랑 재민님은 얼굴만 알아요ㅠ",
          "original_form": "도영님이랑 재민님은 얼굴만 알아요ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030234.1.26",
          "form": "헐스우파 보시는규나!!! 그거 진짜 재밌죸ㅋㅋㅋ저 태용이땜에 봤는데 그냥 재밌더라고요 프로자체가",
          "original_form": "헐스우파 보시는규나!!! 그거 진짜 재밌죸ㅋㅋㅋ저 태용이땜에 봤는데 그냥 재밌더라고요 프로자체가",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030234.1.27",
          "form": "네ㅋㅋ 처음에는 관심 없었는데 리정님 보고 반해서 보고 있어요",
          "original_form": "네ㅋㅋ 처음에는 관심 없었는데 리정님 보고 반해서 보고 있어요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030234.1.28",
          "form": "..저랑 취향이 같으시네요 ㅠㅠ 리정님 진짜 멋있어요..ㅠ",
          "original_form": "..저랑 취향이 같으시네요 ㅠㅠ 리정님 진짜 멋있어요..ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030234.1.29",
          "form": "걍 근데 거기 나오는 분들 다 멋있어요",
          "original_form": "걍 근데 거기 나오는 분들 다 멋있어요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030234.1.30",
          "form": "맞아요오",
          "original_form": "맞아요오",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030234.1.31",
          "form": "배틀 볼때 숨죽이고 봤잖아요",
          "original_form": "배틀 볼때 숨죽이고 봤잖아요",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030234.1.32",
          "form": "제가 거기 출연자면 스티커 오만군데 다붙여지고 울면서 배틀했을거예요ㅠ",
          "original_form": "제가 거기 출연자면 스티커 오만군데 다붙여지고 울면서 배틀했을거예요ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030234.1.33",
          "form": "엠넷 진짜 서바이벌 프로 하나는 잘 만들어요..",
          "original_form": "엠넷 진짜 서바이벌 프로 하나는 잘 만들어요..",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030234.1.34",
          "form": "맞아요... 근데 이번에도 악편 얘기 나오더라구요",
          "original_form": "맞아요... 근데 이번에도 악편 얘기 나오더라구요",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030234.1.35",
          "form": "긍까요 악편 진짜 한숨나오는데 또 재밌어서 보는 저를 발견..",
          "original_form": "긍까요 악편 진짜 한숨나오는데 또 재밌어서 보는 저를 발견..",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030234.1.36",
          "form": "진짜 밉상이에욬ㅋㅋ",
          "original_form": "진짜 밉상이에욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030234.1.37",
          "form": "이번엔 악편 적당히 거르고 보려구요",
          "original_form": "이번엔 악편 적당히 거르고 보려구요",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030234.1.38",
