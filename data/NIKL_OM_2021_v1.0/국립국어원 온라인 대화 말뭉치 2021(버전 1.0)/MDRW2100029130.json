      "utterance": [
        {
          "id": "MDRW2100029130.1.1",
          "form": "보라카이는 필리핀이에요",
          "original_form": "보라카이는 필리핀이에요",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029130.1.2",
          "form": "아하 필리핀이구나ㅎㅎ",
          "original_form": "아하 필리핀이구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029130.1.3",
          "form": "맨날 보라카이라고 부르니까 헷갈려서 검색해봤어요 ㅋㅋㅋ",
          "original_form": "맨날 보라카이라고 부르니까 헷갈려서 검색해봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029130.1.4",
          "form": "저도 태국이랑 필리핀은 가봤어요",
          "original_form": "저도 태국이랑 필리핀은 가봤어요",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029130.1.5",
          "form": "방콕이랑 세부",
          "original_form": "방콕이랑 세부",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029130.1.6",
          "form": "마사지가 싸서 너무 좋았어요",
          "original_form": "마사지가 싸서 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029130.1.7",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029130.1.8",
          "form": "여행가서 마사지 받으면 좋더라구요",
          "original_form": "여행가서 마사지 받으면 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029130.1.9",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029130.1.10",
          "form": "피곤한 몸이 노곤노곤",
          "original_form": "피곤한 몸이 노곤노곤",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029130.1.11",
          "form": "아빠가 건너편에서 코고셨던게 생각나네요 ㅎㅎㅎ",
          "original_form": "아빠가 건너편에서 코고셨던게 생각나네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029130.1.12",
          "form": "여행지 음식은 잘드시나요?",
          "original_form": "여행지 음식은 잘드시나요?",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029130.1.13",
          "form": "아 저도 민사지 받다가ㅋㅋㅋ",
          "original_form": "아 저도 민사지 받다가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029130.1.14",
          "form": "태국갔을때 빼고는 대체로 잘 먹었던 것 같아요",
          "original_form": "태국갔을때 빼고는 대체로 잘 먹었던 것 같아요",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029130.1.15",
          "form": "제 코고는 소리에 놀라서 깼는데",
          "original_form": "제 코고는 소리에 놀라서 깼는데",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029130.1.16",
          "form": "저는 물놀이하고 마사지 갔는데, 막 누웠는데 끝났으니까 일어나라고해서",
          "original_form": "저는 물놀이하고 마사지 갔는데, 막 누웠는데 끝났으니까 일어나라고해서",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029130.1.17",
          "form": "놀랐었어요.",
          "original_form": "놀랐었어요.",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029130.1.18",
          "form": "잠들었구나",
          "original_form": "잠들었구나",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029130.1.19",
          "form": "같이 간 일행은 저 자는줄도 몰랐다고 하더라구요",
          "original_form": "같이 간 일행은 저 자는줄도 몰랐다고 하더라구요",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029130.1.20",
          "form": "다행히 코는 안골았나봐요",
          "original_form": "다행히 코는 안골았나봐요",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
