      "utterance": [
        {
          "id": "MDRW2100022782.1.1",
          "form": "오늘 번개 오지게 쳤어요",
          "original_form": "오늘 번개 오지게 쳤어요",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100022782.1.2",
          "form": "정말 무섭게 쳤어요",
          "original_form": "정말 무섭게 쳤어요",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100022782.1.3",
          "form": "오 제가있는지역엔 ㅋㅋ번개없엇어요",
          "original_form": "오 제가있는지역엔 ㅋㅋ번개없엇어요",
          "speaker_id": "1",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100022782.1.4",
          "form": "소나기 잠깐 왓어요",
          "original_form": "소나기 잠깐 왓어요",
          "speaker_id": "1",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100022782.1.5",
          "form": "오늘은 맑네요!",
          "original_form": "오늘은 맑네요!",
          "speaker_id": "1",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100022782.1.6",
          "form": "남부지방은 흐리네요",
          "original_form": "남부지방은 흐리네요",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100022782.1.7",
          "form": "창가에서 보니까 구름이 가득하네요",
          "original_form": "창가에서 보니까 구름이 가득하네요",
          "speaker_id": "2",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100022782.1.8",
          "form": "어떤 계절을 좋아하시나요?",
          "original_form": "어떤 계절을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100022782.1.9",
          "form": "저는 여름에 쥐약이라서 여름빼고 싹다 좋아욤",
          "original_form": "저는 여름에 쥐약이라서 여름빼고 싹다 좋아욤",
          "speaker_id": "1",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100022782.1.10",
          "form": "저도 여름이 싫어요",
          "original_form": "저도 여름이 싫어요",
          "speaker_id": "2",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100022782.1.11",
          "form": "그래서 이제 여름끝이라서 점점 살아나고있어요",
          "original_form": "그래서 이제 여름끝이라서 점점 살아나고있어요",
          "speaker_id": "1",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100022782.1.12",
          "form": "가을존버!!!!",
          "original_form": "가을존버!!!!",
          "speaker_id": "1",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100022782.1.13",
          "form": "오 맞습니다 ㅎㅎ",
          "original_form": "오 맞습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100022782.1.14",
          "form": "가을이 오기를 바라고 있어요",
          "original_form": "가을이 오기를 바라고 있어요",
          "speaker_id": "2",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100022782.1.15",
          "form": "가을에 피는 코스모스가 보고싶네요",
          "original_form": "가을에 피는 코스모스가 보고싶네요",
          "speaker_id": "2",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100022782.1.16",
          "form": "추석엔 이러다 춥겟어요 ㅋㅋㅋ",
          "original_form": "추석엔 이러다 춥겟어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100022782.1.17",
          "form": "추석이 9월인가요?",
          "original_form": "추석이 9월인가요?",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100022782.1.18",
          "form": "추석때 쌀쌀한데 낮에는 정장입고 성묘가니 더워요",
          "original_form": "추석때 쌀쌀한데 낮에는 정장입고 성묘가니 더워요",
          "speaker_id": "2",
          "time": "20210830 11:56"
        },
        {
          "id": "MDRW2100022782.1.19",
          "form": "아 매년그랫던거같아요",
          "original_form": "아 매년그랫던거같아요",
          "speaker_id": "1",
          "time": "20210830 11:57"
        },
        {
          "id": "MDRW2100022782.1.20",
          "form": "전땀쟁이라 왠만하믄 덥슴다ㅋㅋ",
          "original_form": "전땀쟁이라 왠만하믄 덥슴다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:57"
        },
        {
          "id": "MDRW2100022782.1.21",
          "form": "좀있으면 아침 저녁으로 쌀쌀하니 건강관리 잘하세요",
          "original_form": "좀있으면 아침 저녁으로 쌀쌀하니 건강관리 잘하세요",
          "speaker_id": "2",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100022782.1.22",
          "form": "여름철 옷차림은 어떤걸 선호하시나요?",
          "original_form": "여름철 옷차림은 어떤걸 선호하시나요?",
          "speaker_id": "2",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100022782.1.23",
          "form": "저는 원피스",
          "original_form": "저는 원피스",
          "speaker_id": "1",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100022782.1.24",
          "form": "반팔반바지욥",
          "original_form": "반팔반바지욥",
          "speaker_id": "1",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100022782.1.25",
          "form": "원피스 입으면 시원하겠습니다",
          "original_form": "원피스 입으면 시원하겠습니다",
