      "utterance": [
        {
          "id": "MDRW2100046393.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211020 16:28"
        },
        {
          "id": "MDRW2100046393.1.2",
          "form": "안녕하세요! 요즘 좋아하는 취미활동 있으신가요?",
          "original_form": "안녕하세요! 요즘 좋아하는 취미활동 있으신가요?",
          "speaker_id": "2",
          "time": "20211020 16:34"
        },
        {
          "id": "MDRW2100046393.1.3",
          "form": "취미는 모바일게임요",
          "original_form": "취미는 모바일게임요",
          "speaker_id": "1",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046393.1.4",
          "form": "어떤 게임하시나요?",
          "original_form": "어떤 게임하시나요?",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046393.1.5",
          "form": "야구게임해요",
          "original_form": "야구게임해요",
          "speaker_id": "1",
          "time": "20211020 16:38"
        },
        {
          "id": "MDRW2100046393.1.6",
          "form": "직접 야구하는건 안좋아하시나요?",
          "original_form": "직접 야구하는건 안좋아하시나요?",
          "speaker_id": "2",
          "time": "20211020 16:38"
        },
        {
          "id": "MDRW2100046393.1.7",
          "form": "네 직접하는건 별로요 ㅋㅋ",
          "original_form": "네 직접하는건 별로요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:39"
        },
        {
          "id": "MDRW2100046393.1.8",
          "form": "게임만 좋아해요",
          "original_form": "게임만 좋아해요",
          "speaker_id": "1",
          "time": "20211020 16:39"
        },
        {
          "id": "MDRW2100046393.1.9",
          "form": "ㅋㅋ직접하는건 왜 별로인가요?",
          "original_form": "ㅋㅋ직접하는건 왜 별로인가요?",
          "speaker_id": "2",
          "time": "20211020 16:39"
        },
        {
          "id": "MDRW2100046393.1.10",
          "form": "몸이 안따라줘서요 ㅋㅋ",
          "original_form": "몸이 안따라줘서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:39"
        },
        {
          "id": "MDRW2100046393.1.11",
          "form": "직관하는것도 별로이신가요?",
          "original_form": "직관하는것도 별로이신가요?",
          "speaker_id": "2",
          "time": "20211020 16:39"
        },
        {
          "id": "MDRW2100046393.1.12",
          "form": "직관은 좋아해요",
          "original_form": "직관은 좋아해요",
          "speaker_id": "1",
          "time": "20211020 16:39"
        },
        {
          "id": "MDRW2100046393.1.13",
          "form": "아 몸이 안따라줘서...ㅜㅜ..",
          "original_form": "아 몸이 안따라줘서...ㅜㅜ..",
          "speaker_id": "2",
          "time": "20211020 16:40"
        },
        {
          "id": "MDRW2100046393.1.14",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20211020 16:40"
        },
        {
          "id": "MDRW2100046393.1.15",
          "form": "야구 게임은 하신지 얼마나 됐어요?",
          "original_form": "야구 게임은 하신지 얼마나 됐어요?",
          "speaker_id": "2",
          "time": "20211020 16:40"
        },
        {
          "id": "MDRW2100046393.1.16",
          "form": "꾸준히 해왔어요",
          "original_form": "꾸준히 해왔어요",
          "speaker_id": "1",
          "time": "20211020 16:40"
        },
        {
          "id": "MDRW2100046393.1.17",
          "form": "새로운 겜 나올때마다",
          "original_form": "새로운 겜 나올때마다",
          "speaker_id": "1",
          "time": "20211020 16:40"
        },
        {
          "id": "MDRW2100046393.1.18",
          "form": "오 한가지 게임하는게 아니라 다 다른게임으로 즐기시는거군요?!",
          "original_form": "오 한가지 게임하는게 아니라 다 다른게임으로 즐기시는거군요?!",
          "speaker_id": "2",
          "time": "20211020 16:41"
        },
        {
          "id": "MDRW2100046393.1.19",
          "form": "네 그렇죠^^",
          "original_form": "네 그렇죠^^",
          "speaker_id": "1",
          "time": "20211020 16:41"
        },
        {
          "id": "MDRW2100046393.1.20",
          "form": "얼른 코로나가 끝나서 야구 직관하는 날이 왔으면 좋겠어요! 날씨가 추워졌는데 감기 조심하시고 오늘 남은 하루도 화이팅하시길 바랄게요:)",
          "original_form": "얼른 코로나가 끝나서 야구 직관하는 날이 왔으면 좋겠어요! 날씨가 추워졌는데 감기 조심하시고 오늘 남은 하루도 화이팅하시길 바랄게요:)",
          "speaker_id": "2",
          "time": "20211020 16:42"
        }
