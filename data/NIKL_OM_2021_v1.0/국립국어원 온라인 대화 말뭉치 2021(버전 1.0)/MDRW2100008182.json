      "utterance": [
        {
          "id": "MDRW2100008182.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008182.1.2",
          "form": "안해요?",
          "original_form": "안해요?",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008182.1.3",
          "form": "네 지금 설거지 하려고요",
          "original_form": "네 지금 설거지 하려고요",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008182.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008182.1.5",
          "form": "헉 아까랑 넘 똑같아서 답이 순간 ㅜㅜ",
          "original_form": "헉 아까랑 넘 똑같아서 답이 순간 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100008182.1.6",
          "form": "네 오늘 청소 힘드셨죠",
          "original_form": "네 오늘 청소 힘드셨죠",
          "speaker_id": "2",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100008182.1.7",
          "form": "네 창문닦아요",
          "original_form": "네 창문닦아요",
          "speaker_id": "1",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008182.1.8",
          "form": "창문고요 와 제대로 하시네요 ㅎㅎ",
          "original_form": "창문고요 와 제대로 하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008182.1.9",
          "form": "전 창문 생각도 못했네요",
          "original_form": "전 창문 생각도 못했네요",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008182.1.10",
          "form": "진짜 청소하고 힘드실만 해요",
          "original_form": "진짜 청소하고 힘드실만 해요",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008182.1.11",
          "form": "비가 많이 와서 얼룩이 장난 아니에요",
          "original_form": "비가 많이 와서 얼룩이 장난 아니에요",
          "speaker_id": "1",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008182.1.12",
          "form": "아 전 얼룩 보고 청소 생각 못했어요",
          "original_form": "아 전 얼룩 보고 청소 생각 못했어요",
          "speaker_id": "2",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008182.1.13",
          "form": "창문도 해야겠네요",
          "original_form": "창문도 해야겠네요",
          "speaker_id": "2",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008182.1.14",
          "form": "남자분들이 더 꼼꼼하더라구요",
          "original_form": "남자분들이 더 꼼꼼하더라구요",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008182.1.15",
          "form": "하고나면 딴일 못하겠어요",
          "original_form": "하고나면 딴일 못하겠어요",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008182.1.16",
          "form": "전 집안일중 설거지가 젤싫어요",
          "original_form": "전 집안일중 설거지가 젤싫어요",
          "speaker_id": "2",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008182.1.17",
          "form": "설거지는 안하세요?",
          "original_form": "설거지는 안하세요?",
          "speaker_id": "2",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008182.1.18",
          "form": "설거지 하죠",
          "original_form": "설거지 하죠",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008182.1.19",
          "form": "요리를 못하니 그거라도",
          "original_form": "요리를 못하니 그거라도",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008182.1.20",
          "form": "네 와 그럼 설거지 청소 다하시네요 ㅎㅎ",
          "original_form": "네 와 그럼 설거지 청소 다하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008182.1.21",
          "form": "좋은 남편이신데요",
          "original_form": "좋은 남편이신데요",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008182.1.22",
          "form": "이거라도 해야 분란이 없죠 ㅎㅎ",
          "original_form": "이거라도 해야 분란이 없죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008182.1.23",
          "form": "네 잘하셨어요",
          "original_form": "네 잘하셨어요",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008182.1.24",
          "form": "가화만사성이죠",
          "original_form": "가화만사성이죠",
          "speaker_id": "1",
          "time": "20210822 15:31"
        }
      ]
