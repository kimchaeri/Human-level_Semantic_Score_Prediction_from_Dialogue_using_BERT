      "utterance": [
        {
          "id": "MDRW2100030381.1.1",
          "form": "어떤 시간이요",
          "original_form": "어떤 시간이요",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.2",
          "form": "독서는 어려워요",
          "original_form": "독서는 어려워요",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.3",
          "form": "공부 관련 책도 밀리에서 보고 있어요",
          "original_form": "공부 관련 책도 밀리에서 보고 있어요",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.4",
          "form": "단순히 읽기만 해서는 안 되죠",
          "original_form": "단순히 읽기만 해서는 안 되죠",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.5",
          "form": "지독하게 안 일기도 해요",
          "original_form": "지독하게 안 일기도 해요",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.6",
          "form": "미라클 모닝이랑 비슷한 책 같아요",
          "original_form": "미라클 모닝이랑 비슷한 책 같아요",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.7",
          "form": "주식 공부 재밌나요",
          "original_form": "주식 공부 재밌나요",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.8",
          "form": "주식 정보도 보고 있습니다",
          "original_form": "주식 정보도 보고 있습니다",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.9",
          "form": "청약은 잘 하던데요",
          "original_form": "청약은 잘 하던데요",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.10",
          "form": "아는 회사가 늘어나니 관심이 많이 가네요",
          "original_form": "아는 회사가 늘어나니 관심이 많이 가네요",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100030381.1.11",
          "form": "시장의 마법사들",
          "original_form": "시장의 마법사들",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.12",
          "form": "주식 기본서에요",
          "original_form": "주식 기본서에요",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.13",
          "form": "아는 회사가 없을 때는 무슨 말인지 모르겠더라고요",
          "original_form": "아는 회사가 없을 때는 무슨 말인지 모르겠더라고요",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.14",
          "form": "이것도 캡쳐했습니다",
          "original_form": "이것도 캡쳐했습니다",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.15",
          "form": "트레이더들의 의견을 모아서 책자로 발행했어요",
          "original_form": "트레이더들의 의견을 모아서 책자로 발행했어요",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.16",
          "form": "좋은 책 추천 감사해요",
          "original_form": "좋은 책 추천 감사해요",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.17",
          "form": "책 두꺼워요",
          "original_form": "책 두꺼워요",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.18",
          "form": "두꺼비",
          "original_form": "두꺼비",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.19",
          "form": "청약도 유튜브 보고합니다",
          "original_form": "청약도 유튜브 보고합니다",
          "speaker_id": "2",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.20",
          "form": "유튜브만 봐도 도움 되죠",
          "original_form": "유튜브만 봐도 도움 되죠",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.21",
          "form": "용돈 벌이 솔솔 하죠",
          "original_form": "용돈 벌이 솔솔 하죠",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100030381.1.22",
          "form": "IPO 관련 책도 있더라고요",
          "original_form": "IPO 관련 책도 있더라고요",
          "speaker_id": "2",
          "time": "20210904 00:22"
        }
