      "utterance": [
        {
          "id": "MDRW2100012903.1.1",
          "form": "강제 새벽형인간",
          "original_form": "강제 새벽형인간",
          "speaker_id": "2",
          "time": "20210825 07:50"
        },
        {
          "id": "MDRW2100012903.1.2",
          "form": "저도 직장이 멀었어서\r\n강제 새벽형이긴 했는데\r\n그래도 여섯시에 일어났거든용",
          "original_form": "저도 직장이 멀었어서\r\n강제 새벽형이긴 했는데\r\n그래도 여섯시에 일어났거든용",
          "speaker_id": "1",
          "time": "20210825 07:51"
        },
        {
          "id": "MDRW2100012903.1.3",
          "form": "그리고 대중교통 이용했어서\r\n출퇴근길에 조금씩 자기도 했는데\r\n운전하시면 그러지도 못해서\r\n더 피곤하시겠어여ㅠ",
          "original_form": "그리고 대중교통 이용했어서\r\n출퇴근길에 조금씩 자기도 했는데\r\n운전하시면 그러지도 못해서\r\n더 피곤하시겠어여ㅠ",
          "speaker_id": "1",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012903.1.4",
          "form": "네ㅜㅜ 수험생때보다 더 일찍 일어나요 ㅋㅋ",
          "original_form": "네ㅜㅜ 수험생때보다 더 일찍 일어나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:17"
        },
        {
          "id": "MDRW2100012903.1.5",
          "form": "오늘도 어서 아메 수혈해주셔야겠네요 ㅋㅋㅋ",
          "original_form": "오늘도 어서 아메 수혈해주셔야겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:21"
        },
        {
          "id": "MDRW2100012903.1.6",
          "form": "네 ㅋㅋ 아메 마셔야죠",
          "original_form": "네 ㅋㅋ 아메 마셔야죠",
          "speaker_id": "2",
          "time": "20210825 08:34"
        },
        {
          "id": "MDRW2100012903.1.7",
          "form": "아이들 다 등원했나요?",
          "original_form": "아이들 다 등원했나요?",
          "speaker_id": "2",
          "time": "20210825 08:34"
        },
        {
          "id": "MDRW2100012903.1.8",
          "form": "아니용ㅠ 안했습니당",
          "original_form": "아니용ㅠ 안했습니당",
          "speaker_id": "1",
          "time": "20210825 08:39"
        },
        {
          "id": "MDRW2100012903.1.9",
          "form": "출근길에 아메 테이크아웃 해서\r\n가시나요? ㅋㅋ",
          "original_form": "출근길에 아메 테이크아웃 해서\r\n가시나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:40"
        },
        {
          "id": "MDRW2100012903.1.10",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 10:52"
        },
        {
          "id": "MDRW2100012903.1.11",
          "form": "가는길에 사가지고 가기가 애매해서",
          "original_form": "가는길에 사가지고 가기가 애매해서",
          "speaker_id": "2",
          "time": "20210825 10:52"
        },
        {
          "id": "MDRW2100012903.1.12",
          "form": "그냥회사에서 내려먹거나",
          "original_form": "그냥회사에서 내려먹거나",
          "speaker_id": "2",
          "time": "20210825 10:53"
        },
        {
          "id": "MDRW2100012903.1.13",
          "form": "아님 카누로 때워요 ㅋㅋㅋ",
          "original_form": "아님 카누로 때워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:53"
        },
        {
          "id": "MDRW2100012903.1.14",
          "form": "설탕들어간건 안마시거든요",
          "original_form": "설탕들어간건 안마시거든요",
          "speaker_id": "2",
          "time": "20210825 10:53"
        },
        {
          "id": "MDRW2100012903.1.15",
          "form": "오 저희 남편도 카누 좋아해요",
          "original_form": "오 저희 남편도 카누 좋아해요",
          "speaker_id": "1",
          "time": "20210825 11:08"
        },
        {
          "id": "MDRW2100012903.1.16",
          "form": "마일드로스트? 그거 먹어요ㅋㅋ",
          "original_form": "마일드로스트? 그거 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:08"
        },
        {
          "id": "MDRW2100012903.1.17",
          "form": "아메리카노보다 맛은 없지만",
          "original_form": "아메리카노보다 맛은 없지만",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100012903.1.18",
          "form": "커피는 마셔야하니까...",
          "original_form": "커피는 마셔야하니까...",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100012903.1.19",
          "form": "오늘도 어김없이 수혈해주셨습니꽈 ㅋㅋ",
          "original_form": "오늘도 어김없이 수혈해주셨습니꽈 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100012903.1.20",
          "form": "네 점심먹었으니",
          "original_form": "네 점심먹었으니",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012903.1.21",
          "form": "또 먹어줘야죠",
          "original_form": "또 먹어줘야죠",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012903.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012903.1.23",
          "form": "ㅋㅋ회사에서 몇 잔이나 드세용?",
          "original_form": "ㅋㅋ회사에서 몇 잔이나 드세용?",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012903.1.24",
          "form": "저 세잔 ㅋㅋ",
          "original_form": "저 세잔 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100012903.1.25",
          "form": "출근전에 집에서 한잔",
          "original_form": "출근전에 집에서 한잔",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100012903.1.26",
          "form": "하루에 4~5잔 먹는듯요",
          "original_form": "하루에 4~5잔 먹는듯요",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100012903.1.27",
          "form": "너무 많이 드시는거 아니에요?",
          "original_form": "너무 많이 드시는거 아니에요?",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100012903.1.28",
          "form": "심장 괜찮으세요?",
          "original_form": "심장 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100012903.1.29",
          "form": "심장요?",
          "original_form": "심장요?",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100012903.1.30",
          "form": "전혀.....",
          "original_form": "전혀.....",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100012903.1.31",
          "form": "에이트가 부릅니다...",
          "original_form": "에이트가 부릅니다...",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
