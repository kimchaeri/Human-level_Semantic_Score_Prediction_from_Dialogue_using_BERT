      "utterance": [
        {
          "id": "MDRW2100004108.1.1",
          "form": "저 템플스테이 순천에 송광사갔었어요",
          "original_form": "저 템플스테이 순천에 송광사갔었어요",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004108.1.2",
          "form": "여기 템플스테이숙소를 싹리모델링해서 진짜 좋더라구야",
          "original_form": "여기 템플스테이숙소를 싹리모델링해서 진짜 좋더라구야",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004108.1.3",
          "form": "여",
          "original_form": "여",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004108.1.4",
          "form": "오..나중에 한번 가봐야겠는데요",
          "original_form": "오..나중에 한번 가봐야겠는데요",
          "speaker_id": "2",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004108.1.5",
          "form": "거기서 막 무슨 체험같은것도 했나요?",
          "original_form": "거기서 막 무슨 체험같은것도 했나요?",
          "speaker_id": "2",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004108.1.6",
          "form": "네 아 작년이었나봐요 코로나라 힐링테마로해서",
          "original_form": "네 아 작년이었나봐요 코로나라 힐링테마로해서",
          "speaker_id": "1",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004108.1.7",
          "form": "강제로 하는체험은없었고 다 자율이었어요",
          "original_form": "강제로 하는체험은없었고 다 자율이었어요",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004108.1.8",
          "form": "스님과함께하는 차담시간 108배 이런거 근데 다신청해서 체험은했어요 ㅋㅋㅋ",
          "original_form": "스님과함께하는 차담시간 108배 이런거 근데 다신청해서 체험은했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004108.1.9",
          "form": "아하 그렇군요ㅋㅋ",
          "original_form": "아하 그렇군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004108.1.10",
          "form": "이왕 간거 해보는게 좋죠",
          "original_form": "이왕 간거 해보는게 좋죠",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004108.1.11",
          "form": "차담시간엔 무슨얘기하셨어요?",
          "original_form": "차담시간엔 무슨얘기하셨어요?",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004108.1.12",
          "form": "저라면 스님이랑 어색할거같은뎈ㅋㅋㅠㅠ",
          "original_form": "저라면 스님이랑 어색할거같은뎈ㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004108.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ스님혼자 가운데앉아계시고 참여자들이 한10명은돼서 어색하진않았는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ스님혼자 가운데앉아계시고 참여자들이 한10명은돼서 어색하진않았는데",
          "speaker_id": "1",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004108.1.14",
          "form": "앉아있는게 힘들었어요 ㅋㅋㅋㅋ",
          "original_form": "앉아있는게 힘들었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004108.1.15",
          "form": "아하ㅋㅋㅋ 그렇겠네요",
          "original_form": "아하ㅋㅋㅋ 그렇겠네요",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004108.1.16",
          "form": "다리에 쥐날거같은데ㅋㅋ",
          "original_form": "다리에 쥐날거같은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004108.1.17",
          "form": "무릎꿇고있어야되는건아니죠?",
          "original_form": "무릎꿇고있어야되는건아니죠?",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004108.1.18",
          "form": "네 앞자리에앉았다가 스님눈치보면서 다리왔다리갔다리했어요",
          "original_form": "네 앞자리에앉았다가 스님눈치보면서 다리왔다리갔다리했어요",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004108.1.19",
          "form": "네네 양반다리로 ㅋㅋㅋ양반다리 피안통해여",
          "original_form": "네네 양반다리로 ㅋㅋㅋ양반다리 피안통해여",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004108.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ 그런거 할일있으면 뒷자리에 앉아야겠는데요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그런거 할일있으면 뒷자리에 앉아야겠는데요",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004108.1.21",
          "form": "저도 가만히앉아있는거 못견뎌갖고",
          "original_form": "저도 가만히앉아있는거 못견뎌갖고",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004108.1.22",
          "form": "108배는어땠어요? 안힘들어요?",
          "original_form": "108배는어땠어요? 안힘들어요?",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004108.1.23",
          "form": "108도 할생각은없었는데 새벽에 일찍눈떠져서 구경갔다가 얼결에 같이 했었는데",
          "original_form": "108도 할생각은없었는데 새벽에 일찍눈떠져서 구경갔다가 얼결에 같이 했었는데",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004108.1.24",
          "form": "생각보다 금방하고 어렵지않았어요 ㅋㅋㅋㅋ",
          "original_form": "생각보다 금방하고 어렵지않았어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.25",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.26",
          "form": "108배 다이어트도있길래",
          "original_form": "108배 다이어트도있길래",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.27",
          "form": "힘든줄알았는데",
          "original_form": "힘든줄알았는데",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.28",
          "form": "네 삼천배가힘들것같아요 ㅋㅋㅋㅋ",
          "original_form": "네 삼천배가힘들것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.29",
          "form": "괜찮았나봐요ㅋㅋ",
          "original_form": "괜찮았나봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.30",
          "form": "와 삼천배",
          "original_form": "와 삼천배",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.31",
          "form": "어케해요ㄹㅇ",
          "original_form": "어케해요ㄹㅇ",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.32",
          "form": "저108배할때",
          "original_form": "저108배할때",
          "speaker_id": "1",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004108.1.33",
          "form": "젊은스님 삼천배하셨는데",
          "original_form": "젊은스님 삼천배하셨는데",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004108.1.34",
          "form": "거의 절하는기계처럼 스피드장난아니셨어요",
          "original_form": "거의 절하는기계처럼 스피드장난아니셨어요",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004108.1.35",
          "form": "와우ㅋㅋ스님 체력이 보통이아닌데요ㅋㅋ",
          "original_form": "와우ㅋㅋ스님 체력이 보통이아닌데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004108.1.36",
          "form": "뭔가 웃기네요 상상하니까..절하는기계..",
          "original_form": "뭔가 웃기네요 상상하니까..절하는기계..",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004108.1.37",
          "form": "네 ㅋㅋㅋㅋㅋㅋ뒤에서 웃으면안되는데 넘 대단하고 스피드가 웃음이나서 참느라힘들었던",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋ뒤에서 웃으면안되는데 넘 대단하고 스피드가 웃음이나서 참느라힘들었던",
