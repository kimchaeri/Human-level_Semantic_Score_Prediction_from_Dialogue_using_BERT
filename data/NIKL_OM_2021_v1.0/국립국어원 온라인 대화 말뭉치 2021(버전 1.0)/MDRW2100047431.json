        },
        {
          "id": "MDRW2100047431.1.2",
          "form": "네 빵 좋아해요ㅎㅎ 디저트 좋아하세요?",
          "original_form": "네 빵 좋아해요ㅎㅎ 디저트 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047431.1.3",
          "form": "네 저도 빵 좋아해요",
          "original_form": "네 저도 빵 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100047431.1.4",
          "form": "어떤빵 좋아하세요?",
          "original_form": "어떤빵 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100047431.1.5",
          "form": "저 그냥 다좋아해요 팥이랑 야채빵 빼구요 ㅋㅋ",
          "original_form": "저 그냥 다좋아해요 팥이랑 야채빵 빼구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100047431.1.6",
          "form": "저도 팥빵 빼고 다 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 팥빵 빼고 다 좋아해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100047431.1.7",
          "form": "아 저 건포도도ㅠㅠ보니까 많이 안좋아하네요ㅜㅜㅋㅋㅋ",
          "original_form": "아 저 건포도도ㅠㅠ보니까 많이 안좋아하네요ㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100047431.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ팥빵이랑 건포도빵은 원래 호불호가 갈리잖아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ팥빵이랑 건포도빵은 원래 호불호가 갈리잖아요",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100047431.1.9",
          "form": "그쵸...먹었는데 건포도 씹히면 ....ㅋㅋㅋ",
          "original_form": "그쵸...먹었는데 건포도 씹히면 ....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047431.1.10",
          "form": "밤식빵은 괜찮으세요? ㅋㅋ",
          "original_form": "밤식빵은 괜찮으세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047431.1.11",
          "form": "네..고구마같고 맛있더라구요 ㅋㅋ안좋아하세요?",
          "original_form": "네..고구마같고 맛있더라구요 ㅋㅋ안좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047431.1.12",
          "form": "저는 굉장히 좋아해요",
          "original_form": "저는 굉장히 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100047431.1.13",
          "form": "밤을 좋아해서 ㅋㅋㅋㅋㅋ",
          "original_form": "밤을 좋아해서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100047431.1.14",
          "form": "저두 밤좋아요ㅋㅋㅋ군밤도 ㅎㅎㅎ",
          "original_form": "저두 밤좋아요ㅋㅋㅋ군밤도 ㅎㅎㅎ",
