      "utterance": [
        {
          "id": "MDRW2100041615.1.1",
          "form": "이제 돌아옴 ㅋㅋ 점심 유부초밥 싸갔었어!",
          "original_form": "이제 돌아옴 ㅋㅋ 점심 유부초밥 싸갔었어!",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041615.1.2",
          "form": "삼계탕 맛있었겠다 삼계탕 먹은지 오래됐네",
          "original_form": "삼계탕 맛있었겠다 삼계탕 먹은지 오래됐네",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041615.1.3",
          "form": "도시락ㅎ좋았겠다",
          "original_form": "도시락ㅎ좋았겠다",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041615.1.4",
          "form": "삼계탕 잘 만드셨더라\n맛있었오ㅎ",
          "original_form": "삼계탕 잘 만드셨더라\n맛있었오ㅎ",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041615.1.5",
          "form": "유부초밥 맛 없드라 유부초밥패키지? 그게 별로였나봐",
          "original_form": "유부초밥 맛 없드라 유부초밥패키지? 그게 별로였나봐",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041615.1.6",
          "form": "엄마가 씨제이 샀다했는데",
          "original_form": "엄마가 씨제이 샀다했는데",
          "speaker_id": "2",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100041615.1.7",
          "form": "오랜만에 나들이 였는데 아쉽",
          "original_form": "오랜만에 나들이 였는데 아쉽",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100041615.1.8",
          "form": "기억했다 담에 안사야겠다",
          "original_form": "기억했다 담에 안사야겠다",
          "speaker_id": "1",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100041615.1.9",
          "form": "ㅇ거기랑 동원꺼 맛없음",
          "original_form": "ㅇ거기랑 동원꺼 맛없음",
          "speaker_id": "2",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041615.1.10",
          "form": "송정가서 문토스트도 먹었다",
          "original_form": "송정가서 문토스트도 먹었다",
          "speaker_id": "2",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041615.1.11",
          "form": "문토스트 안먹어봄",
          "original_form": "문토스트 안먹어봄",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100041615.1.12",
          "form": "어떤거길래?",
          "original_form": "어떤거길래?",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100041615.1.13",
          "form": "오~~",
          "original_form": "오~~",
          "speaker_id": "1",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100041615.1.14",
          "form": "유튜브에 치면 바로 나와! 내가 설명한거보다 더 이해편할거야",
          "original_form": "유튜브에 치면 바로 나와! 내가 설명한거보다 더 이해편할거야",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100041615.1.15",
          "form": "내가 먹어본거에는 걍 계란이랑 소스 치즈듬뿍",
          "original_form": "내가 먹어본거에는 걍 계란이랑 소스 치즈듬뿍",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100041615.1.16",
          "form": "이런느낌",
          "original_form": "이런느낌",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100041615.1.17",
          "form": "응 보고 오~~한거ㅋ",
          "original_form": "응 보고 오~~한거ㅋ",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041615.1.18",
          "form": "맛있겠더라ㅎ",
          "original_form": "맛있겠더라ㅎ",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100041615.1.19",
          "form": "ㅇㅇ 맛 괜찮았어 한번 먹어보는거 괜찮을듯해",
          "original_form": "ㅇㅇ 맛 괜찮았어 한번 먹어보는거 괜찮을듯해",
          "speaker_id": "2",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100041615.1.20",
          "form": "근데 난 같은가격이면 다른 재료 많이 들어가는 이삭이 나은듯",
          "original_form": "근데 난 같은가격이면 다른 재료 많이 들어가는 이삭이 나은듯",
          "speaker_id": "2",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100041615.1.21",
          "form": "이삭 좋다",
          "original_form": "이삭 좋다",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100041615.1.22",
          "form": "나 예전에 이삭 대표님 유퀴즈 나온거보고는 이미지 좋아졌어",
          "original_form": "나 예전에 이삭 대표님 유퀴즈 나온거보고는 이미지 좋아졌어",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100041615.1.23",
          "form": "ㅇㅇ 이삭은 여러가지가 많이드가는 느낌이고",
          "original_form": "ㅇㅇ 이삭은 여러가지가 많이드가는 느낌이고",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100041615.1.24",
          "form": "문토스트는 치즈랑 계란이 많이들어가는느낌",
          "original_form": "문토스트는 치즈랑 계란이 많이들어가는느낌",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100041615.1.25",
          "form": "ㅇㅇ 나도 유퀴즈서봤당",
          "original_form": "ㅇㅇ 나도 유퀴즈서봤당",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100041615.1.26",
          "form": "응 이삭 좋아졌는데 근거리 이삭이 없어ㅜㅜㅋ",
          "original_form": "응 이삭 좋아졌는데 근거리 이삭이 없어ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100041615.1.27",
          "form": "난 소계동에 있는데 귀찮....",
          "original_form": "난 소계동에 있는데 귀찮....",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100041615.1.28",
          "form": "낼 백신 맞으러 갖다오면서 살까 싶기도해",
          "original_form": "낼 백신 맞으러 갖다오면서 살까 싶기도해",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100041615.1.29",
          "form": "이삭토스트",
          "original_form": "이삭토스트",
