      "utterance": [
        {
          "id": "MDRW2100024415.1.1",
          "form": "갖게 되실거에요",
          "original_form": "갖게 되실거에요",
          "speaker_id": "2",
          "time": "20210831 07:19"
        },
        {
          "id": "MDRW2100024415.1.2",
          "form": "급여같은거엔 만족하시나요?",
          "original_form": "급여같은거엔 만족하시나요?",
          "speaker_id": "1",
          "time": "20210831 07:19"
        },
        {
          "id": "MDRW2100024415.1.3",
          "form": "네 업무량 비해서는 만족해요",
          "original_form": "네 업무량 비해서는 만족해요",
          "speaker_id": "2",
          "time": "20210831 07:19"
        },
        {
          "id": "MDRW2100024415.1.4",
          "form": "name1님은 무슨 알바하세요?",
          "original_form": "&name1&님은 무슨 알바하세요?",
          "speaker_id": "2",
          "time": "20210831 07:19"
        },
        {
          "id": "MDRW2100024415.1.5",
          "form": "전 학원알바하고 있어요!",
          "original_form": "전 학원알바하고 있어요!",
          "speaker_id": "1",
          "time": "20210831 07:20"
        },
        {
          "id": "MDRW2100024415.1.6",
          "form": "이것저것 많이 하다가",
          "original_form": "이것저것 많이 하다가",
          "speaker_id": "1",
          "time": "20210831 07:20"
        },
        {
          "id": "MDRW2100024415.1.7",
          "form": "코로나때문에 이거 하나 빼고는 안해요ㅎㅎ",
          "original_form": "코로나때문에 이거 하나 빼고는 안해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 07:21"
        },
        {
          "id": "MDRW2100024415.1.8",
          "form": "어떤걸 가르치세요?",
          "original_form": "어떤걸 가르치세요?",
          "speaker_id": "2",
          "time": "20210831 07:21"
        },
        {
          "id": "MDRW2100024415.1.9",
          "form": "아 학원알바라고",
          "original_form": "아 학원알바라고",
          "speaker_id": "2",
          "time": "20210831 07:21"
        },
        {
          "id": "MDRW2100024415.1.10",
          "form": "뭘 꼭 가르치는건 아닐수도 있겠다",
          "original_form": "뭘 꼭 가르치는건 아닐수도 있겠다",
          "speaker_id": "2",
          "time": "20210831 07:21"
        },
        {
          "id": "MDRW2100024415.1.11",
          "form": "학원에서 어떤일하세요?",
          "original_form": "학원에서 어떤일하세요?",
          "speaker_id": "2",
          "time": "20210831 07:22"
        },
        {
          "id": "MDRW2100024415.1.12",
          "form": "넵 전 그냥 편집해요!",
          "original_form": "넵 전 그냥 편집해요!",
          "speaker_id": "1",
          "time": "20210831 07:22"
        },
        {
          "id": "MDRW2100024415.1.13",
          "form": "편집이요?",
          "original_form": "편집이요?",
          "speaker_id": "2",
          "time": "20210831 07:22"
        },
        {
          "id": "MDRW2100024415.1.14",
          "form": "넵 교재 편집하고",
          "original_form": "넵 교재 편집하고",
          "speaker_id": "1",
          "time": "20210831 07:35"
        },
        {
          "id": "MDRW2100024415.1.15",
          "form": "가끔 애들가르치긴 하는데",
          "original_form": "가끔 애들가르치긴 하는데",
          "speaker_id": "1",
          "time": "20210831 07:35"
        },
        {
          "id": "MDRW2100024415.1.16",
          "form": "제 주된 일은 아니에요",
          "original_form": "제 주된 일은 아니에요",
          "speaker_id": "1",
          "time": "20210831 07:35"
        },
        {
          "id": "MDRW2100024415.1.17",
          "form": "name2님은 회사 인간관계 관련한 걱정같은건 없으신가요?",
          "original_form": "&name2&님은 회사 인간관계 관련한 걱정같은건 없으신가요?",
          "speaker_id": "1",
          "time": "20210831 07:40"
        },
        {
          "id": "MDRW2100024415.1.18",
          "form": "네 특별히 문제는 없어요",
          "original_form": "네 특별히 문제는 없어요",
          "speaker_id": "2",
          "time": "20210831 07:40"
        },
        {
          "id": "MDRW2100024415.1.19",
          "form": "직원들 모두 좋은 사람들이에요",
          "original_form": "직원들 모두 좋은 사람들이에요",
          "speaker_id": "2",
          "time": "20210831 07:40"
        },
        {
          "id": "MDRW2100024415.1.20",
          "form": "오늘도 출글해요?",
          "original_form": "오늘도 출글해요?",
          "speaker_id": "2",
          "time": "20210831 07:41"
        },
        {
          "id": "MDRW2100024415.1.21",
          "form": "오 넘 부럽네요!!",
          "original_form": "오 넘 부럽네요!!",
          "speaker_id": "1",
          "time": "20210831 07:41"
        },
        {
          "id": "MDRW2100024415.1.22",
          "form": "아뇨 월수금 출근해요!",
          "original_form": "아뇨 월수금 출근해요!",
          "speaker_id": "1",
          "time": "20210831 07:41"
        },
        {
          "id": "MDRW2100024415.1.23",
          "form": "오늘 쉬는 날 이시구나",
          "original_form": "오늘 쉬는 날 이시구나",
          "speaker_id": "2",
          "time": "20210831 07:42"
        },
        {
          "id": "MDRW2100024415.1.24",
          "form": "넵ㅎㅎ 근데 너무 일찍일어나서",
          "original_form": "넵ㅎㅎ 근데 너무 일찍일어나서",
          "speaker_id": "1",
          "time": "20210831 07:42"
        },
        {
          "id": "MDRW2100024415.1.25",
          "form": "억울해요ㅋㅋㅋ",
          "original_form": "억울해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 07:42"
        },
        {
          "id": "MDRW2100024415.1.26",
          "form": "name2님은 원래 이 시간에 일어나시나요?",
          "original_form": "&name2&님은 원래 이 시간에 일어나시나요?",
          "speaker_id": "1",
          "time": "20210831 07:42"
        },
        {
          "id": "MDRW2100024415.1.27",
          "form": "ㅎㅎ 일찍 일어나면 좋죠 하루가 길고",
          "original_form": "ㅎㅎ 일찍 일어나면 좋죠 하루가 길고",
          "speaker_id": "2",
          "time": "20210831 07:42"
        },
        {
          "id": "MDRW2100024415.1.28",
          "form": "저는 평소면 6시쯤 일어나요",
          "original_form": "저는 평소면 6시쯤 일어나요",
