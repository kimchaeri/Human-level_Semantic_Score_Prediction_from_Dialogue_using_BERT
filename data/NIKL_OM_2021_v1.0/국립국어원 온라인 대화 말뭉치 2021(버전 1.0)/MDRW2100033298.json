      "utterance": [
        {
          "id": "MDRW2100033298.1.1",
          "form": "대전에서",
          "original_form": "대전에서",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033298.1.2",
          "form": "얼마걸려요?",
          "original_form": "얼마걸려요?",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033298.1.3",
          "form": "대전이랫쥬?",
          "original_form": "대전이랫쥬?",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033298.1.4",
          "form": "오래...",
          "original_form": "오래...",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033298.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033298.1.6",
          "form": "애매해요 ㅋㅋㅋ",
          "original_form": "애매해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033298.1.7",
          "form": "케텍스 있지않나요?",
          "original_form": "케텍스 있지않나요?",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033298.1.8",
          "form": "음 근데 가서 이동하기 불편하지않나요",
          "original_form": "음 근데 가서 이동하기 불편하지않나요",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.9",
          "form": "저 여수 연애할때 갔엇는데",
          "original_form": "저 여수 연애할때 갔엇는데",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.10",
          "form": "여수 오동도!",
          "original_form": "여수 오동도!",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.11",
          "form": "밤바닼ㅋㅋㅋ",
          "original_form": "밤바닼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.12",
          "form": "가서 렌트 하면되지 않겠습니까",
          "original_form": "가서 렌트 하면되지 않겠습니까",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.13",
          "form": "오동도 굿",
          "original_form": "오동도 굿",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.14",
          "form": "아항렌트",
          "original_form": "아항렌트",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.15",
          "form": "저 곧 전화중국어해서",
          "original_form": "저 곧 전화중국어해서",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.16",
          "form": "끝나면",
          "original_form": "끝나면",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.17",
          "form": "ㅋㅋㅋㅋ 와부지런허시다 중국어라니",
          "original_form": "ㅋㅋㅋㅋ 와부지런허시다 중국어라니",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.18",
          "form": "답장드림",
          "original_form": "답장드림",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.19",
          "form": "ㅋㅋ하오",
          "original_form": "ㅋㅋ하오",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033298.1.20",
          "form": "ㅋㅋㅋㅋㅋ 취미신가..",
          "original_form": "ㅋㅋㅋㅋㅋ 취미신가..",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033298.1.21",
          "form": "취미입니다",
          "original_form": "취미입니다",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033298.1.22",
          "form": "언어 하나는 해볼려고요 ㅋㅋ",
          "original_form": "언어 하나는 해볼려고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033298.1.23",
          "form": "니하오",
          "original_form": "니하오",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100033298.1.24",
          "form": "ㅋㅋㅋ 얼마나되셧쥬",
          "original_form": "ㅋㅋㅋ 얼마나되셧쥬",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033298.1.25",
          "form": "음 3일됫구요",
          "original_form": "음 3일됫구요",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033298.1.26",
          "form": "ㅋㅋㅋ사솔",
          "original_form": "ㅋㅋㅋ사솔",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033298.1.27",
          "form": "사실 작년에 9개월했엇구",
          "original_form": "사실 작년에 9개월했엇구",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033298.1.28",
          "form": "기사시험 친다고 잠깐 쉬엇더니만",
          "original_form": "기사시험 친다고 잠깐 쉬엇더니만",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033298.1.29",
          "form": "다까먹어서",
          "original_form": "다까먹어서",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033298.1.30",
          "form": "학사때 어학연수를 중국으로 갔엇네요 ㅋㅋㅋ",
          "original_form": "학사때 어학연수를 중국으로 갔엇네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100033298.1.31",
          "form": "6개월 놀다가 왔는데 하나도 안하고",
          "original_form": "6개월 놀다가 왔는데 하나도 안하고",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100033298.1.32",
          "form": "쳐놀다가와서",
          "original_form": "쳐놀다가와서",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100033298.1.33",
          "form": "사람들이 물어보면 기본이라도 할려고",
          "original_form": "사람들이 물어보면 기본이라도 할려고",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100033298.1.34",
          "form": "하고있네요 ㅋㅋㅋ",
          "original_form": "하고있네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033298.1.35",
          "form": "웃픕니다 아오 ㅋㅋㅋ",
          "original_form": "웃픕니다 아오 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100033298.1.36",
          "form": "앜ㅋㅋㅋ3일ㅋㅋㅋㅋ 왜어학연수까지가셧어요??",
          "original_form": "앜ㅋㅋㅋ3일ㅋㅋㅋㅋ 왜어학연수까지가셧어요??",
          "speaker_id": "1",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033298.1.37",
          "form": "아 무슨 외국인들 교류단체활동 좀 햇더만",
          "original_form": "아 무슨 외국인들 교류단체활동 좀 햇더만",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033298.1.38",
          "form": "가라고 해서",
          "original_form": "가라고 해서",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033298.1.39",
          "form": "갔다왓죠",
          "original_form": "갔다왓죠",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033298.1.40",
          "form": "공짜고 뭐",
          "original_form": "공짜고 뭐",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033298.1.41",
          "form": "그래서 갓는데",
          "original_form": "그래서 갓는데",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033298.1.42",
          "form": "술만 맨날 쳐묵고 ㅋㅋ",
          "original_form": "술만 맨날 쳐묵고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033298.1.43",
          "form": "와 신기하닼ㅋㅋㅋㅋ그래도 잼썻겟네요.. 공짜라니 ㄷㄷ",
          "original_form": "와 신기하닼ㅋㅋㅋㅋ그래도 잼썻겟네요.. 공짜라니 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033298.1.44",
          "form": "맥주만 한달에 만씨씨먹엇서요",
          "original_form": "맥주만 한달에 만씨씨먹엇서요",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033298.1.45",
          "form": "미친자였음",
          "original_form": "미친자였음",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033298.1.46",
          "form": "만씨씨가 아니구나",
          "original_form": "만씨씨가 아니구나",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033298.1.47",
          "form": "하루에 3천씩",
          "original_form": "하루에 3천씩",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033298.1.48",
          "form": "25일을 먹엇으니",
          "original_form": "25일을 먹엇으니",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033298.1.49",
          "form": "암튼 잘놀다왓씁니다 ㅋㅋ",
          "original_form": "암튼 잘놀다왓씁니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033298.1.50",
          "form": "마사지도 정말 쌈",
          "original_form": "마사지도 정말 쌈",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033298.1.51",
          "form": "샘들 오시면 항상 같이 마사지받으러 갓서요",
          "original_form": "샘들 오시면 항상 같이 마사지받으러 갓서요",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033298.1.52",
          "form": "여자애들이 참 좋아했었어요",
          "original_form": "여자애들이 참 좋아했었어요",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100033298.1.53",
          "form": "대박 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대박 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100033298.1.54",
          "form": "중국 어디요?",
          "original_form": "중국 어디요?",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100033298.1.55",
          "form": "하얼빈 ㅋㅋ",
          "original_form": "하얼빈 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100033298.1.56",
          "form": "얼어뒈질뻔",
          "original_form": "얼어뒈질뻔",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100033298.1.57",
          "form": "ㅋㅋㅋㅋㅋㅋ거기가 춥군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ거기가 춥군요",
