      "utterance": [
        {
          "id": "MDRW2100029199.1.1",
          "form": "월급상승률이 장난아님",
          "original_form": "월급상승률이 장난아님",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029199.1.2",
          "form": "휴가 나올때마다",
          "original_form": "휴가 나올때마다",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029199.1.3",
          "form": "피엑스가서",
          "original_form": "피엑스가서",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029199.1.4",
          "form": "군대에서 어나오는 돈이 올랐다는 얘기죠",
          "original_form": "군대에서 어나오는 돈이 올랐다는 얘기죠",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029199.1.5",
          "form": "화장품 싸게 살수있으니까",
          "original_form": "화장품 싸게 살수있으니까",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.6",
          "form": "진짜 요새 군대 많이 좋아졌네",
          "original_form": "진짜 요새 군대 많이 좋아졌네",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.7",
          "form": "부탁하거덩",
          "original_form": "부탁하거덩",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.8",
          "form": "그래 선크림도 많이 산다고 하더라",
          "original_form": "그래 선크림도 많이 산다고 하더라",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.9",
          "form": "크림이랑",
          "original_form": "크림이랑",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.10",
          "form": "우리의 피부는 소중하니까요",
          "original_form": "우리의 피부는 소중하니까요",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.11",
          "form": "클렌저랑 다사와줌",
          "original_form": "클렌저랑 다사와줌",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.12",
          "form": "글고",
          "original_form": "글고",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.13",
          "form": "고객께 훨씬 더 많이 싼가 봐요",
          "original_form": "고객께 훨씬 더 많이 싼가 봐요",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.14",
          "form": "군인이면 통신사 갸ㅓ",
          "original_form": "군인이면 통신사 갸ㅓ",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.15",
          "form": "가서",
          "original_form": "가서",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.16",
          "form": "통신사도 싸요",
          "original_form": "통신사도 싸요",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.17",
          "form": "요금제 따로 신청가능해서",
          "original_form": "요금제 따로 신청가능해서",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.18",
          "form": "싸게 쓸수 있다는듯",
          "original_form": "싸게 쓸수 있다는듯",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029199.1.19",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029199.1.20",
          "form": "할 만하네",
          "original_form": "할 만하네",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029199.1.21",
          "form": "휴가 나오면",
          "original_form": "휴가 나오면",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029199.1.22",
          "form": "영화 할인받을수있어서",
          "original_form": "영화 할인받을수있어서",
          "speaker_id": "2",
