      "utterance": [
        {
          "id": "MDRW2100034260.1.1",
          "form": "먹고픈 음식 마음껏 드시나요",
          "original_form": "먹고픈 음식 마음껏 드시나요",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034260.1.2",
          "form": "네 먹기라도하자",
          "original_form": "네 먹기라도하자",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034260.1.3",
          "form": "주의ㅋㅋ",
          "original_form": "주의ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034260.1.4",
          "form": "어떤걸 좋아하시나요?",
          "original_form": "어떤걸 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034260.1.5",
          "form": "달고 짜고",
          "original_form": "달고 짜고",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034260.1.6",
          "form": "담백한건 별루",
          "original_form": "담백한건 별루",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034260.1.7",
          "form": "ㅋㅋ과자요? 요리요?",
          "original_form": "ㅋㅋ과자요? 요리요?",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034260.1.8",
          "form": "다좋아해요ㅋ",
          "original_form": "다좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100034260.1.9",
          "form": "가리는게없는",
          "original_form": "가리는게없는",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100034260.1.10",
          "form": "저 지금 홈런볼 먹고있어요 ㅠㅠ",
          "original_form": "저 지금 홈런볼 먹고있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100034260.1.11",
          "form": "밤에 먹으면 왜케 맛날까요",
          "original_form": "밤에 먹으면 왜케 맛날까요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100034260.1.12",
          "form": "맞아요 금요일인데 뭐 안드시나요...",
          "original_form": "맞아요 금요일인데 뭐 안드시나요...",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100034260.1.13",
          "form": "지금 메뉴 고민중요",
          "original_form": "지금 메뉴 고민중요",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100034260.1.14",
          "form": "야식드시나요?",
          "original_form": "야식드시나요?",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034260.1.15",
          "form": "냉장고 뒤지는중여",
          "original_form": "냉장고 뒤지는중여",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034260.1.16",
          "form": "뭐가 선택될지 궁금하네요",
          "original_form": "뭐가 선택될지 궁금하네요",
          "speaker_id": "2",
          "time": "20210827 23:06"
        }
      ]
