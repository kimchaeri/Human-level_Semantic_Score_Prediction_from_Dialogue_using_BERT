      "utterance": [
        {
          "id": "MDRW2100040997.1.1",
          "form": "그치 선선해서 청소해도 짜증 안나 ㅋㅋ",
          "original_form": "그치 선선해서 청소해도 짜증 안나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100040997.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ 우산쓰고돌아가는길",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 우산쓰고돌아가는길",
          "speaker_id": "1",
          "time": "20210905 15:02"
        },
        {
          "id": "MDRW2100040997.1.3",
          "form": "아쥐죽은거봐서 욕함",
          "original_form": "아쥐죽은거봐서 욕함",
          "speaker_id": "1",
          "time": "20210905 15:02"
        },
        {
          "id": "MDRW2100040997.1.4",
          "form": "ㅋㅈㅋㅈㄴㄱㅈㄴㄱ",
          "original_form": "ㅋㅈㅋㅈㄴㄱㅈㄴㄱ",
          "speaker_id": "1",
          "time": "20210905 15:02"
        },
        {
          "id": "MDRW2100040997.1.5",
          "form": "아으ㅠㅠ",
          "original_form": "아으ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100040997.1.6",
          "form": "지그 비와?",
          "original_form": "지그 비와?",
          "speaker_id": "2",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100040997.1.7",
          "form": "응응 비쬐끔오넹",
          "original_form": "응응 비쬐끔오넹",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100040997.1.8",
          "form": "지금은 또 안온다 희한",
          "original_form": "지금은 또 안온다 희한",
          "speaker_id": "2",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100040997.1.9",
          "form": "응 근데흐리긴해그칭",
          "original_form": "응 근데흐리긴해그칭",
          "speaker_id": "1",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100040997.1.10",
          "form": "빨래돌리고있어서 해나야하는데",
          "original_form": "빨래돌리고있어서 해나야하는데",
          "speaker_id": "1",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100040997.1.11",
          "form": "근데 저번빨래개는데",
          "original_form": "근데 저번빨래개는데",
          "speaker_id": "1",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100040997.1.12",
          "form": "티가왤케다물들었지..이유를모르겟다",
          "original_form": "티가왤케다물들었지..이유를모르겟다",
          "speaker_id": "1",
          "time": "20210905 18:02"
        },
        {
          "id": "MDRW2100040997.1.13",
          "form": "헐 정말 어떡ㅎㅐㅠㅠ",
          "original_form": "헐 정말 어떡ㅎㅐㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100040997.1.14",
          "form": "망했다ㅋㅋㅋㅋ\n내흰티셔츠들..",
          "original_form": "망했다ㅋㅋㅋㅋ\n내흰티셔츠들..",
          "speaker_id": "1",
          "time": "20210905 18:24"
        },
        {
          "id": "MDRW2100040997.1.15",
          "form": ";-;",
          "original_form": ";-;",
          "speaker_id": "1",
          "time": "20210905 18:24"
        },
        {
          "id": "MDRW2100040997.1.16",
          "form": "어떡해ㅜㅠ 나도 다리미질 하다가 다리미 녹물 텼어 ㅋㅋㅋ",
          "original_form": "어떡해ㅜㅠ 나도 다리미질 하다가 다리미 녹물 텼어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100040997.1.17",
          "form": "악 다리미 녹물ㅋㅋㅋ",
          "original_form": "악 다리미 녹물ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100040997.1.18",
          "form": "옷을 고민해서 사도 이럴때면 후회해 ㅋㅋㅋㅋ",
          "original_form": "옷을 고민해서 사도 이럴때면 후회해 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040997.1.19",
          "form": "오늘 선선하다 내일도 가을옷 입으카?",
          "original_form": "오늘 선선하다 내일도 가을옷 입으카?",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100040997.1.20",
          "form": "내일날씨한번봐야겠는뎅",
          "original_form": "내일날씨한번봐야겠는뎅",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100040997.1.21",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100040997.1.22",
