      "utterance": [
        {
          "id": "MDRW2100032330.1.1",
          "form": "난 원래 평일에 연가쓰고 머리하는데",
          "original_form": "난 원래 평일에 연가쓰고 머리하는데",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032330.1.2",
          "form": "갈수록 둘다 피곤에 쩔어",
          "original_form": "갈수록 둘다 피곤에 쩔어",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032330.1.3",
          "form": "그럼 머릿값이 두배",
          "original_form": "그럼 머릿값이 두배",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032330.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032330.1.5",
          "form": "아웃겨",
          "original_form": "아웃겨",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032330.1.6",
          "form": "슬픈건가",
          "original_form": "슬픈건가",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032330.1.7",
          "form": "파마12만원 날라가는 연가보상비7만원",
          "original_form": "파마12만원 날라가는 연가보상비7만원",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032330.1.8",
          "form": "지금 애기 몇개월이지??",
          "original_form": "지금 애기 몇개월이지??",
          "speaker_id": "2",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032330.1.9",
          "form": "9개월을 향해 가",
          "original_form": "9개월을 향해 가",
          "speaker_id": "1",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032330.1.10",
          "form": "비싼 파마네",
          "original_form": "비싼 파마네",
          "speaker_id": "1",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032330.1.11",
          "form": "이제 슬슬 잡고 서기 시작하면서 이쁜짓 하기 시작하면 키우는 재미도 있을껴",
          "original_form": "이제 슬슬 잡고 서기 시작하면서 이쁜짓 하기 시작하면 키우는 재미도 있을껴",
          "speaker_id": "2",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100032330.1.12",
          "form": "희망의 말 감사... 한줄기 빛...",
          "original_form": "희망의 말 감사... 한줄기 빛...",
          "speaker_id": "1",
          "time": "20210905 15:49"
        },
        {
          "id": "MDRW2100032330.1.13",
          "form": "딸래미는 진짜 돌~두돌까지 미치게 이쁜것같아",
          "original_form": "딸래미는 진짜 돌~두돌까지 미치게 이쁜것같아",
          "speaker_id": "2",
          "time": "20210905 15:50"
        },
        {
          "id": "MDRW2100032330.1.14",
          "form": "우리아들은 너무 늦게 자라서 온가족이 지쳐가는중",
          "original_form": "우리아들은 너무 늦게 자라서 온가족이 지쳐가는중",
          "speaker_id": "2",
          "time": "20210905 15:50"
        },
        {
          "id": "MDRW2100032330.1.15",
          "form": "둘째 몇개월인데?",
          "original_form": "둘째 몇개월인데?",
          "speaker_id": "1",
          "time": "20210905 15:50"
        },
        {
          "id": "MDRW2100032330.1.16",
          "form": "이제 세돌되셨습니다",
          "original_form": "이제 세돌되셨습니다",
          "speaker_id": "2",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032330.1.17",
          "form": "남자애는 크는 게 다른가...",
          "original_form": "남자애는 크는 게 다른가...",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032330.1.18",
          "form": "근데 너무 아가아가해 ㅠㅠ",
          "original_form": "근데 너무 아가아가해 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032330.1.19",
          "form": "아가아가한데 힘은 아가가 아니니",
          "original_form": "아가아가한데 힘은 아가가 아니니",
          "speaker_id": "2",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032330.1.20",
          "form": "막내라 그런가봐",
          "original_form": "막내라 그런가봐",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032330.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032330.1.22",
          "form": "힘쎈 아가",
          "original_form": "힘쎈 아가",
