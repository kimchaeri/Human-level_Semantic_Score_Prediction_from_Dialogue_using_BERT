      "utterance": [
        {
          "id": "MDRW2100037397.1.1",
          "form": "나랑 간거야?",
          "original_form": "나랑 간거야?",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100037397.1.2",
          "form": "나는 중학교때 말곤 가본적이 없는걸..",
          "original_form": "나는 중학교때 말곤 가본적이 없는걸..",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.3",
          "form": "응 키큰친구랑 같이 셋이!",
          "original_form": "응 키큰친구랑 같이 셋이!",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.4",
          "form": "음 서운하다",
          "original_form": "음 서운하다",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.5",
          "form": "캠핑이아니라 여행이었나",
          "original_form": "캠핑이아니라 여행이었나",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.6",
          "form": "어쨌든 무슨 비빔면?",
          "original_form": "어쨌든 무슨 비빔면?",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.7",
          "form": "미안 내가 너무 인싸라서 좀 헷갈렸어",
          "original_form": "미안 내가 너무 인싸라서 좀 헷갈렸어",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.8",
          "form": "아 혹시 배홍동 먹어봤어?",
          "original_form": "아 혹시 배홍동 먹어봤어?",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.9",
          "form": "근데 그 비빔면 이름이 기억이안나",
          "original_form": "근데 그 비빔면 이름이 기억이안나",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.10",
          "form": "아아ㅠ 부럽다. 질투나.",
          "original_form": "아아ㅠ 부럽다. 질투나.",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.11",
          "form": "면이 얇고 밀가루 맛이 안나는",
          "original_form": "면이 얇고 밀가루 맛이 안나는",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.12",
          "form": "그거였나?? 봉지가 파란색이었러",
          "original_form": "그거였나?? 봉지가 파란색이었러",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.13",
          "form": "그거 맞지?",
          "original_form": "그거 맞지?",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.14",
          "form": "나는 근데 진짜쫄면도 좋아해",
          "original_form": "나는 근데 진짜쫄면도 좋아해",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.15",
          "form": "응응 나 그거 마트가서 사와야해",
          "original_form": "응응 나 그거 마트가서 사와야해",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.16",
          "form": "면이 쫄깃하고 상추찢어넣어먹으면 맛있어",
          "original_form": "면이 쫄깃하고 상추찢어넣어먹으면 맛있어",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.17",
          "form": "그거 안먹은지 오래돾어",
          "original_form": "그거 안먹은지 오래돾어",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.18",
          "form": "그리고 진쫄은 매워",
          "original_form": "그리고 진쫄은 매워",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.19",
          "form": "너는 비빔면 차갑게해서 먹니?",
          "original_form": "너는 비빔면 차갑게해서 먹니?",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100037397.1.20",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100037397.1.21",
          "form": "근데 귀찮으면 그냥 먹어",
          "original_form": "근데 귀찮으면 그냥 먹어",
          "speaker_id": "2",
          "time": "20210831 23:16"
        }
      ]
