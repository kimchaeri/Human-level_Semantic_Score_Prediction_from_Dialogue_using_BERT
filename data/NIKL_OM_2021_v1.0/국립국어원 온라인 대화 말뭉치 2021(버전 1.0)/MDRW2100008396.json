        {
          "id": "MDRW2100008396.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008396.1.2",
          "form": "저 오늘 잔치국수 먹었어요",
          "original_form": "저 오늘 잔치국수 먹었어요",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008396.1.3",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008396.1.4",
          "form": "국밥이요",
          "original_form": "국밥이요",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008396.1.5",
          "form": "오 국밥 맛있겠다",
          "original_form": "오 국밥 맛있겠다",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008396.1.6",
          "form": "부추넣으면 최고죠",
          "original_form": "부추넣으면 최고죠",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100008396.1.7",
          "form": "먹잘알이시다",
          "original_form": "먹잘알이시다",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008396.1.8",
          "form": "부추랑 새우젓 많이 넣어야해요",
          "original_form": "부추랑 새우젓 많이 넣어야해요",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008396.1.9",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008396.1.10",
          "form": "아시네요",
          "original_form": "아시네요",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008396.1.11",
          "form": "요새 배달음식도 워낙 잘 되지만",
          "original_form": "요새 배달음식도 워낙 잘 되지만",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008396.1.12",
          "form": "국밥은 가서 먹는게 좋더라고요",
          "original_form": "국밥은 가서 먹는게 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008396.1.13",
          "form": "뚝배기에",
          "original_form": "뚝배기에",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008396.1.14",
          "form": "네 식당갔어요",
          "original_form": "네 식당갔어요",
          "speaker_id": "1",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008396.1.15",
          "form": "저도 국밥 먹고 싶네요ㅋㅋㅋ",
          "original_form": "저도 국밥 먹고 싶네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008396.1.16",
          "form": "맛나요",
          "original_form": "맛나요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008396.1.17",
          "form": "내일 먹어야겠어요",
          "original_form": "내일 먹어야겠어요",
