        {
          "id": "MDRW2100018253.1.1",
          "form": "배달 음식 자주 드시나요?",
          "original_form": "배달 음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018253.1.2",
          "form": "네자주먹어요!",
          "original_form": "네자주먹어요!",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100018253.1.3",
          "form": "주로 뭐시켜 드세요?",
          "original_form": "주로 뭐시켜 드세요?",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100018253.1.4",
          "form": "치느님이요!",
          "original_form": "치느님이요!",
          "speaker_id": "2",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100018253.1.5",
          "form": "치킨 좋죠ㅎ 저는 어제 치킨 시켜먹었어요",
          "original_form": "치킨 좋죠ㅎ 저는 어제 치킨 시켜먹었어요",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100018253.1.6",
          "form": "와 어디꺼요??",
          "original_form": "와 어디꺼요??",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100018253.1.7",
          "form": "비비큐요ㅎㅎ",
          "original_form": "비비큐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100018253.1.8",
          "form": "저는교촌좋아해요ㅎㅎ",
          "original_form": "저는교촌좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100018253.1.9",
          "form": "저도 교촌 좋아해요 주로 뭐시켜 드세요?",
          "original_form": "저도 교촌 좋아해요 주로 뭐시켜 드세요?",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100018253.1.10",
          "form": "허니콤보요!",
          "original_form": "허니콤보요!",
          "speaker_id": "2",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100018253.1.11",
          "form": "저도 허니콤보 최애에요ㅎ",
          "original_form": "저도 허니콤보 최애에요ㅎ",
          "speaker_id": "1",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100018253.1.12",
          "form": "퍽퍽살이 없어서 좋네요",
          "original_form": "퍽퍽살이 없어서 좋네요",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100018253.1.13",
          "form": "레드소스도 찍어먹으면ㅎㅎ",
          "original_form": "레드소스도 찍어먹으면ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100018253.1.14",
          "form": "교촌에 발사믹치킨이 새로 나왔던데 드셔본적 있나요?",
