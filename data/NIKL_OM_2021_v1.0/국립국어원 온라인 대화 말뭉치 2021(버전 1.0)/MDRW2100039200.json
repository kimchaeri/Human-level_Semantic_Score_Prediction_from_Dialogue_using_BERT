      "utterance": [
        {
          "id": "MDRW2100039200.1.1",
          "form": "전도연도 나오는가요?",
          "original_form": "전도연도 나오는가요?",
          "speaker_id": "1",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100039200.1.2",
          "form": "류준열 전도연이 주인공이에요",
          "original_form": "류준열 전도연이 주인공이에요",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100039200.1.3",
          "form": "뭔가 잘 어울리더라구요",
          "original_form": "뭔가 잘 어울리더라구요",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100039200.1.4",
          "form": "기대하고 있습니다",
          "original_form": "기대하고 있습니다",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100039200.1.5",
          "form": "와 엄청난대요",
          "original_form": "와 엄청난대요",
          "speaker_id": "1",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100039200.1.6",
          "form": "기대작이네요",
          "original_form": "기대작이네요",
          "speaker_id": "1",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100039200.1.7",
          "form": "네 ㅎㅎ 오랜만에 류준열 나오는 드라마라..ㅇ",
          "original_form": "네 ㅎㅎ 오랜만에 류준열 나오는 드라마라..ㅇ",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100039200.1.8",
          "form": "요새는 보는거 없으세요??",
          "original_form": "요새는 보는거 없으세요??",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100039200.1.9",
          "form": "원작이 따로 있나요?",
          "original_form": "원작이 따로 있나요?",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100039200.1.10",
          "form": "전 완결나면 몰아서 봐요ㅎㅎ",
          "original_form": "전 완결나면 몰아서 봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100039200.1.11",
          "form": "아니요 원작이 따로 있는건 아닌거같아요",
          "original_form": "아니요 원작이 따로 있는건 아닌거같아요",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100039200.1.12",
          "form": "슬의 안보시겠네요 ㅎㅎ",
          "original_form": "슬의 안보시겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100039200.1.13",
          "form": "요새는 슬의만 보고있어요",
          "original_form": "요새는 슬의만 보고있어요",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100039200.1.14",
          "form": "본방은 못봐서 늘 넷플로",
          "original_form": "본방은 못봐서 늘 넷플로",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100039200.1.15",
          "form": "슬의생기다리고 있어요ㅜㅜ",
          "original_form": "슬의생기다리고 있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100039200.1.16",
          "form": "얼른 완결나면 보려구ㅎㅎ",
          "original_form": "얼른 완결나면 보려구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100039200.1.17",
          "form": "곧 완결날때가 됐는데",
          "original_form": "곧 완결날때가 됐는데",
          "speaker_id": "2",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039200.1.18",
          "form": "어제\r\n결방해서 ㅠ",
          "original_form": "어제\r\n결방해서 ㅠ",
          "speaker_id": "2",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039200.1.19",
          "form": "응답하라 시리즈는 다 보셨어요??",
          "original_form": "응답하라 시리즈는 다 보셨어요??",
          "speaker_id": "2",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039200.1.20",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100039200.1.21",
          "form": "88 너무좋아합니다ㅠㅠ",
          "original_form": "88 너무좋아합니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100039200.1.22",
          "form": "네 저도 88은 몇번 돌려봤어요 ㅎㅎ",
          "original_form": "네 저도 88은 몇번 돌려봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100039200.1.23",
          "form": "88 제일 좋죠",
          "original_form": "88 제일 좋죠",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100039200.1.24",
          "form": "연기못하는 배우도 너무 제옷처럼 연기해서",
          "original_form": "연기못하는 배우도 너무 제옷처럼 연기해서",
          "speaker_id": "1",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100039200.1.25",
          "form": "자주 찾아서 돌려보고 있어요ㅎㅎ",
          "original_form": "자주 찾아서 돌려보고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100039200.1.26",
          "form": "네 ㅎㅎ 저도 종종 다시보고 있어요",
          "original_form": "네 ㅎㅎ 저도 종종 다시보고 있어요",
          "speaker_id": "2",
          "time": "20210903 14:00"
        },
        {
          "id": "MDRW2100039200.1.27",
