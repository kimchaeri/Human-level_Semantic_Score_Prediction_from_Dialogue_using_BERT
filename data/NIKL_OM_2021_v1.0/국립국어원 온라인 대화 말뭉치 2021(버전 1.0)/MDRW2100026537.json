      "utterance": [
        {
          "id": "MDRW2100026537.1.1",
          "form": "법적으로 못하게해야하는데",
          "original_form": "법적으로 못하게해야하는데",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026537.1.2",
          "form": "솔직히 이만큼 온것도 많이온거같음",
          "original_form": "솔직히 이만큼 온것도 많이온거같음",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026537.1.3",
          "form": "더 큰처벌도없을것같고...",
          "original_form": "더 큰처벌도없을것같고...",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026537.1.4",
          "form": "맞어ㅜㅠ 주작 밝혀내고 여기까지 온 것만으로도 대단해",
          "original_form": "맞어ㅜㅠ 주작 밝혀내고 여기까지 온 것만으로도 대단해",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026537.1.5",
          "form": "오타쿠를 쉽게 보지말라",
          "original_form": "오타쿠를 쉽게 보지말라",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026537.1.6",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026537.1.7",
          "form": "진짜여기까지 밝힌것도 대단했어",
          "original_form": "진짜여기까지 밝힌것도 대단했어",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026537.1.8",
          "form": "망하라망해라했던",
          "original_form": "망하라망해라했던",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026537.1.9",
          "form": "안준영이진짜망했고",
          "original_form": "안준영이진짜망했고",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026537.1.10",
          "form": "아무리 꼬리자르기래도",
          "original_form": "아무리 꼬리자르기래도",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026537.1.11",
          "form": "그치만 준영이는 더 혼나야 해",
          "original_form": "그치만 준영이는 더 혼나야 해",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026537.1.12",
          "form": "나한테 걸리지 말아라",
          "original_form": "나한테 걸리지 말아라",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026537.1.13",
          "form": "내 눈에 띄지마",
          "original_form": "내 눈에 띄지마",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026537.1.14",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026537.1.15",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026537.1.16",
          "form": "내눈에띄지마라...",
          "original_form": "내눈에띄지마라...",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026537.1.17",
          "form": "프듀는 그렇게 망령만남기고",
          "original_form": "프듀는 그렇게 망령만남기고",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026537.1.18",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026537.1.19",
          "form": "프듀는 진짜...\n국프라는 이상한 문화로 악개들한테 완장질 핑계 만들어줌",
          "original_form": "프듀는 진짜...\n국프라는 이상한 문화로 악개들한테 완장질 핑계 만들어줌",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026537.1.20",
          "form": "진짜 싫어",
          "original_form": "진짜 싫어",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026537.1.21",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026537.1.22",
          "form": "진짜 그이후로",
          "original_form": "진짜 그이후로",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026537.1.23",
          "form": "돌판분위기 심각하게 망했다고",
          "original_form": "돌판분위기 심각하게 망했다고",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026537.1.24",
          "form": "팬덤이 갑질을 이상하게 하기시작함",
          "original_form": "팬덤이 갑질을 이상하게 하기시작함",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026537.1.25",
          "form": "맞어 국민 프로듀서라니깐 자기가 진짜 뭐라도 되는거라 생각하는지",
          "original_form": "맞어 국민 프로듀서라니깐 자기가 진짜 뭐라도 되는거라 생각하는지",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026537.1.26",
          "form": "진짜 넹글이야...",
          "original_form": "진짜 넹글이야...",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026537.1.27",
          "form": "그것도",
          "original_form": "그것도",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026537.1.28",
          "form": "솔까 일본에서따왔잖아",
          "original_form": "솔까 일본에서따왔잖아",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026537.1.29",
          "form": "48다베껴와놓고",
          "original_form": "48다베껴와놓고",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026537.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아..",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026537.1.31",
          "form": "정병시스템을 따왔어",
          "original_form": "정병시스템을 따왔어",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026537.1.32",
          "form": "뭐 자기가 되게 엄청난거 만든줄 알듯",
          "original_form": "뭐 자기가 되게 엄청난거 만든줄 알듯",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026537.1.33",
          "form": "맞아 결국 시스템 표절해서 방송 만든거면서...",
          "original_form": "맞아 결국 시스템 표절해서 방송 만든거면서...",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026537.1.34",
          "form": "ㅋㅋㄱㄱㄱ ㄹㅇ 완전싹다베껴와놓고",
          "original_form": "ㅋㅋㄱㄱㄱ ㄹㅇ 완전싹다베껴와놓고",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026537.1.35",
          "form": "뭐 아키누구지걔랑 협업하는거",
          "original_form": "뭐 아키누구지걔랑 협업하는거",
