      "utterance": [
        {
          "id": "MDRW2100005320.1.1",
          "form": "키우시는 동물있나요?",
          "original_form": "키우시는 동물있나요?",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005320.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005320.1.3",
          "form": "고양이 1마리 키우고 있어요!",
          "original_form": "고양이 1마리 키우고 있어요!",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005320.1.4",
          "form": "고양이 귀엽죠 새침한매력도  있고",
          "original_form": "고양이 귀엽죠 새침한매력도  있고",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005320.1.5",
          "form": "맞아요 귀여운데 안앵겨주는..그런데 더 끌리는",
          "original_form": "맞아요 귀여운데 안앵겨주는..그런데 더 끌리는",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005320.1.6",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005320.1.7",
          "form": "ㅋㅋ 남자분들이 그래서 고양이를 좋아하나봐요",
          "original_form": "ㅋㅋ 남자분들이 그래서 고양이를 좋아하나봐요",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005320.1.8",
          "form": "부모님이 길냥이 데려다 키우셨는데 저희고양이는 꼬랑지도 흔들어요",
          "original_form": "부모님이 길냥이 데려다 키우셨는데 저희고양이는 꼬랑지도 흔들어요",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005320.1.9",
          "form": "맞아요 ㅋㅋㅌ 이게",
          "original_form": "맞아요 ㅋㅋㅌ 이게",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005320.1.10",
          "form": "매력이 엄청나요",
          "original_form": "매력이 엄청나요",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005320.1.11",
          "form": "저희냥이는 꼬랑지 흔들어줘하면 막 흔들어주거든요ㅎ",
          "original_form": "저희냥이는 꼬랑지 흔들어줘하면 막 흔들어주거든요ㅎ",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005320.1.12",
          "form": "강아지처럼",
          "original_form": "강아지처럼",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005320.1.13",
          "form": "헐 저희집 고양이도",
          "original_form": "헐 저희집 고양이도",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005320.1.14",
          "form": "꼬리 흔들라하면 흔들하더라고요",
          "original_form": "꼬리 흔들라하면 흔들하더라고요",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005320.1.15",
          "form": "대박대밧",
          "original_form": "대박대밧",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005320.1.16",
          "form": "오ㅎ 진짜요? 그런경우 흔치않은데",
          "original_form": "오ㅎ 진짜요? 그런경우 흔치않은데",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005320.1.17",
          "form": "정확히는 이제 꼬리 탁탁! 하면 이제 흔들면서 탁탁치는",
          "original_form": "정확히는 이제 꼬리 탁탁! 하면 이제 흔들면서 탁탁치는",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005320.1.18",
          "form": "발이 젤 귀여워요 냥이는",
          "original_form": "발이 젤 귀여워요 냥이는",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005320.1.19",
          "form": "그런식으로 하더라고요",
          "original_form": "그런식으로 하더라고요",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005320.1.20",
          "form": "헐 고양이 젤리 그쵸",
          "original_form": "헐 고양이 젤리 그쵸",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005320.1.21",
          "form": "막 만지면 싫어하더라구요",
          "original_form": "막 만지면 싫어하더라구요",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005320.1.22",
          "form": "어떤 종류 고양이  키우시는거예요?",
          "original_form": "어떤 종류 고양이  키우시는거예요?",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100005320.1.23",
          "form": "삼색코쇼? 라고 부르던데",
          "original_form": "삼색코쇼? 라고 부르던데",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100005320.1.24",
          "form": "쉽게 말하면 삼색이 키워요 ㅎㅎ",
          "original_form": "쉽게 말하면 삼색이 키워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100005320.1.25",
          "form": "아 삼색이 귀엽겠어요",
          "original_form": "아 삼색이 귀엽겠어요",
          "speaker_id": "1",
          "time": "20210822 11:31"
        }
      ]
    }
  ]
