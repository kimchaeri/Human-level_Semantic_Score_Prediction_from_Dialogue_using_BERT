        },
        {
          "id": "MDRW2100035069.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 01:45"
        },
        {
          "id": "MDRW2100035069.1.3",
          "form": "마스크 안필요하세여",
          "original_form": "마스크 안필요하세여",
          "speaker_id": "1",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100035069.1.4",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100035069.1.5",
          "form": "공째마스크 알려주려고",
          "original_form": "공째마스크 알려주려고",
          "speaker_id": "1",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100035069.1.6",
          "form": "공째?",
          "original_form": "공째?",
          "speaker_id": "2",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100035069.1.7",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210829 01:47"
        },
        {
          "id": "MDRW2100035069.1.8",
          "form": "해줘요",
          "original_form": "해줘요",
          "speaker_id": "2",
          "time": "20210829 01:47"
        },
        {
          "id": "MDRW2100035069.1.9",
          "form": "유투브에 아.홉시 내고향 들어가서\r\n마스.크 영.상.보고 알아서 하슈",
          "original_form": "유투브에 아.홉시 내고향 들어가서\r\n마스.크 영.상.보고 알아서 하슈",
          "speaker_id": "1",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100035069.1.10",
          "form": ".......꼭 봐야해요?",
          "original_form": ".......꼭 봐야해요?",
          "speaker_id": "2",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100035069.1.11",
          "form": "그럼 하지말던가",
          "original_form": "그럼 하지말던가",
          "speaker_id": "1",
          "time": "20210829 01:49"
        },
        {
          "id": "MDRW2100035069.1.12",
          "form": "친절하셨으면 해요",
          "original_form": "친절하셨으면 해요",
          "speaker_id": "2",
          "time": "20210829 01:50"
        },
        {
          "id": "MDRW2100035069.1.13",
