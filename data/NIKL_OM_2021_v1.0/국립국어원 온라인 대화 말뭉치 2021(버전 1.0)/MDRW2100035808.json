          "speaker_id": "2",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100035808.1.3",
          "form": "포르토에 갔던 게 떠올라",
          "original_form": "포르토에 갔던 게 떠올라",
          "speaker_id": "1",
          "time": "20210830 03:12"
        },
        {
          "id": "MDRW2100035808.1.4",
          "form": "너가 사진 보냈던 거 기억나",
          "original_form": "너가 사진 보냈던 거 기억나",
          "speaker_id": "2",
          "time": "20210830 03:14"
        },
        {
          "id": "MDRW2100035808.1.5",
          "form": "거기 정말 예쁘지 않니",
          "original_form": "거기 정말 예쁘지 않니",
          "speaker_id": "1",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.6",
          "form": "나는 포르투갈을 한 번도 안 가봤는데",
          "original_form": "나는 포르투갈을 한 번도 안 가봤는데",
          "speaker_id": "2",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.7",
          "form": "나도 그때 간 게 처음이야",
          "original_form": "나도 그때 간 게 처음이야",
          "speaker_id": "1",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.8",
          "form": "소감이 어때서",
          "original_form": "소감이 어때서",
          "speaker_id": "2",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.9",
          "form": "에그타르트가 맛있었어",
          "original_form": "에그타르트가 맛있었어",
          "speaker_id": "1",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.10",
          "form": "에그타르트의 본고장이지",
          "original_form": "에그타르트의 본고장이지",
          "speaker_id": "2",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.11",
          "form": "그리고 풍경이 무척 이뻐",
          "original_form": "그리고 풍경이 무척 이뻐",
          "speaker_id": "1",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.12",
          "form": "그렇게 보이더라",
          "original_form": "그렇게 보이더라",
          "speaker_id": "2",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.13",
          "form": "강 건너편으로 양조장이 있어",
          "original_form": "강 건너편으로 양조장이 있어",
          "speaker_id": "1",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.14",
          "form": "무척 신기한 비주얼이었다",
          "original_form": "무척 신기한 비주얼이었다",
          "speaker_id": "2",
          "time": "20210830 03:15"
        },
        {
          "id": "MDRW2100035808.1.15",
          "form": "일단 야경도 이쁘더라",
          "original_form": "일단 야경도 이쁘더라",
