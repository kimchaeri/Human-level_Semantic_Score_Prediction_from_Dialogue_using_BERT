          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026385.1.16",
          "form": "안타까운 ㅠ",
          "original_form": "안타까운 ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026385.1.17",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026385.1.18",
          "form": "그나마 봄이랑 가을 둘다 입을 수 있는게",
          "original_form": "그나마 봄이랑 가을 둘다 입을 수 있는게",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026385.1.19",
          "form": "장점아닌 장점이려나요ㅠㅠ",
          "original_form": "장점아닌 장점이려나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026385.1.20",
          "form": "근데 대박이뻐요 트렌치 어디서 사세요?",
          "original_form": "근데 대박이뻐요 트렌치 어디서 사세요?",
