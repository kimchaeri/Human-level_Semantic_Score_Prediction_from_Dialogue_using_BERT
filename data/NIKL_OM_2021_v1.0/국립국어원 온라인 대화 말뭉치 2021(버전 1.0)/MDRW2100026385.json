      "utterance": [
        {
          "id": "MDRW2100026385.1.1",
          "form": "요즘 사고싶은 물건 있어요?",
          "original_form": "요즘 사고싶은 물건 있어요?",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026385.1.2",
          "form": "저 플립이요 ㅋㅋ",
          "original_form": "저 플립이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026385.1.3",
          "form": "이쁘지않나요",
          "original_form": "이쁘지않나요",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026385.1.4",
          "form": "그거 진짜 예쁘더라고요",
          "original_form": "그거 진짜 예쁘더라고요",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026385.1.5",
          "form": "꾸민거 보니까 너무예뻐요!!",
          "original_form": "꾸민거 보니까 너무예뻐요!!",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026385.1.6",
          "form": "맞아요 ㅋㅋ근데 폰바꾼지 얼마안돼서",
          "original_form": "맞아요 ㅋㅋ근데 폰바꾼지 얼마안돼서",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026385.1.7",
          "form": "못바꿀듯요 ㅠ",
          "original_form": "못바꿀듯요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026385.1.8",
          "form": "name2은 사고싶은거있나요?",
          "original_form": "&name2&은 사고싶은거있나요?",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026385.1.9",
          "form": "저는 갑자기 트렌치 코트에 꽂혀서",
          "original_form": "저는 갑자기 트렌치 코트에 꽂혀서",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026385.1.10",
          "form": "코트를 살지 말지 고민하고 있어요",
          "original_form": "코트를 살지 말지 고민하고 있어요",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026385.1.11",
          "form": "오 ㅋㅋ이제 가을이니 사도될거같아요",
          "original_form": "오 ㅋㅋ이제 가을이니 사도될거같아요",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026385.1.12",
          "form": "그쵸 이제 가을이니까 살까봐요",
          "original_form": "그쵸 이제 가을이니까 살까봐요",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026385.1.13",
          "form": "좀 세일만 하면 좋을텐데",
          "original_form": "좀 세일만 하면 좋을텐데",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026385.1.14",
          "form": "트렌치 넘 이쁜데",
          "original_form": "트렌치 넘 이쁜데",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026385.1.15",
