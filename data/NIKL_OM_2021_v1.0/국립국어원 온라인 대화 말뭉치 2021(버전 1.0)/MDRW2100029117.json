      "utterance": [
        {
          "id": "MDRW2100029117.1.1",
          "form": "컴공 전과는 학점만 본다 그랬지?",
          "original_form": "컴공 전과는 학점만 본다 그랬지?",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.2",
          "form": "옛스...",
          "original_form": "옛스...",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.3",
          "form": "온리학점",
          "original_form": "온리학점",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.4",
          "form": "근데 우리과는",
          "original_form": "근데 우리과는",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.5",
          "form": "전출할때 상담해야하더라",
          "original_form": "전출할때 상담해야하더라",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.7",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.8",
          "form": "컴공전입은 학점만잇으면되는데",
          "original_form": "컴공전입은 학점만잇으면되는데",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.9",
          "form": "근데 다 그렇지 않나?",
          "original_form": "근데 다 그렇지 않나?",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.10",
          "form": "ㄴㄴ없는과도 많더라",
          "original_form": "ㄴㄴ없는과도 많더라",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.12",
          "form": "학과 마다 다르구나..",
          "original_form": "학과 마다 다르구나..",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029117.1.13",
          "form": "거의 성적순으로 끊는과가 대부분인데",
          "original_form": "거의 성적순으로 끊는과가 대부분인데",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.14",
          "form": "우리과는 전입전출 다 상담있더라",
          "original_form": "우리과는 전입전출 다 상담있더라",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.15",
          "form": "왜 그렇지",
          "original_form": "왜 그렇지",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.16",
          "form": "무슨 차이일까?",
          "original_form": "무슨 차이일까?",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.17",
          "form": "글게",
          "original_form": "글게",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.18",
          "form": "약간 마이너 공대라",
          "original_form": "약간 마이너 공대라",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.19",
          "form": "사람 나가는거 신경쓰이나ㅋㅋ",
          "original_form": "사람 나가는거 신경쓰이나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.20",
          "form": "아니 전출은 그렇다 쳐도",
          "original_form": "아니 전출은 그렇다 쳐도",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.21",
          "form": "전입은 왜?",
          "original_form": "전입은 왜?",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.22",
          "form": "아님 교수진 차이?",
          "original_form": "아님 교수진 차이?",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029117.1.23",
          "form": "꼰대 많아서??",
          "original_form": "꼰대 많아서??",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029117.1.24",
          "form": "컴공 교수진은",
          "original_form": "컴공 교수진은",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029117.1.25",
          "form": "진짜 젊음",
          "original_form": "진짜 젊음",
          "speaker_id": "1",
