      "utterance": [
        {
          "id": "MDRW2100004172.1.1",
          "form": "최근에 영화 보신거 있나요?",
          "original_form": "최근에 영화 보신거 있나요?",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004172.1.2",
          "form": "블랙위도우 봤어요ㅎㅎ 보신거 있나요?",
          "original_form": "블랙위도우 봤어요ㅎㅎ 보신거 있나요?",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004172.1.3",
          "form": "저도 그거 보고싶었는데",
          "original_form": "저도 그거 보고싶었는데",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004172.1.4",
          "form": "최근엔 영화관을 못갔어요 ㅜㅜ",
          "original_form": "최근엔 영화관을 못갔어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004172.1.5",
          "form": "저도 엄청 고민하다가 조조로 보고왔어요ㅠㅠ",
          "original_form": "저도 엄청 고민하다가 조조로 보고왔어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004172.1.6",
          "form": "근데 영화관에 오히려 사람 없지않나요ㅎㅎ",
          "original_form": "근데 영화관에 오히려 사람 없지않나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004172.1.7",
          "form": "네 맞아요ㅎㅎ 사람보다 먹을수가 없는게 아쉽더라구요",
          "original_form": "네 맞아요ㅎㅎ 사람보다 먹을수가 없는게 아쉽더라구요",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004172.1.8",
          "form": "아 맞네요ㅋㅋ 팝콘...언제쯤 먹을수있을지",
          "original_form": "아 맞네요ㅋㅋ 팝콘...언제쯤 먹을수있을지",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004172.1.9",
          "form": "그래도 영화는 크게 보는게 맞더라구요ㅎㅎ그걸로 만족",
          "original_form": "그래도 영화는 크게 보는게 맞더라구요ㅎㅎ그걸로 만족",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004172.1.10",
          "form": "맞아요 확실히 몰입도가 다르죠",
          "original_form": "맞아요 확실히 몰입도가 다르죠",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004172.1.11",
          "form": "넷플릭스도 정말 좋아하는데 아쉽긴해요ㅎㅎ",
          "original_form": "넷플릭스도 정말 좋아하는데 아쉽긴해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004172.1.12",
          "form": "넷플은 어떤거 재밌게보셨나요??",
          "original_form": "넷플은 어떤거 재밌게보셨나요??",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004172.1.13",
          "form": "미스터리 시리즈 봤는데 으스스하고 좋았어요ㅎㅎ",
          "original_form": "미스터리 시리즈 봤는데 으스스하고 좋았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004172.1.14",
          "form": "무서운건가요??",
          "original_form": "무서운건가요??",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004172.1.15",
          "form": "아뇨 그 해외 그알같은ㅎㅎ",
          "original_form": "아뇨 그 해외 그알같은ㅎㅎ",
          "speaker_id": "2",
