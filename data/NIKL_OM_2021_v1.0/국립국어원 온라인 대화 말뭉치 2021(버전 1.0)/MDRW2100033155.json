      "utterance": [
        {
          "id": "MDRW2100033155.1.1",
          "form": "나 요즘 루미큐브함",
          "original_form": "나 요즘 루미큐브함",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.2",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.4",
          "form": "같이해",
          "original_form": "같이해",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.5",
          "form": "나 루미큐브너무하고싶어",
          "original_form": "나 루미큐브너무하고싶어",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.6",
          "form": "맨날 4등임",
          "original_form": "맨날 4등임",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.7",
          "form": "그거 깔아 앱",
          "original_form": "그거 깔아 앱",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.8",
          "form": "루미큐브앱 있음",
          "original_form": "루미큐브앱 있음",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.9",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.10",
          "form": "애플에잇긔?",
          "original_form": "애플에잇긔?",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100033155.1.11",
          "form": "나 갤럭시자나",
          "original_form": "나 갤럭시자나",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100033155.1.12",
          "form": "근데 애플에도 있음",
          "original_form": "근데 애플에도 있음",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100033155.1.13",
          "form": "애플일때로 루미큐브함",
          "original_form": "애플일때로 루미큐브함",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100033155.1.14",
          "form": "엥그럼 나",
          "original_form": "엥그럼 나",
          "speaker_id": "1",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100033155.1.15",
          "form": "아이패드에깔아서너랑",
          "original_form": "아이패드에깔아서너랑",
          "speaker_id": "1",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100033155.1.16",
          "form": "할수잇음??",
          "original_form": "할수잇음??",
          "speaker_id": "1",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100033155.1.17",
          "form": "아~ 아이패드로 하면 더 크게 보이겠당",
          "original_form": "아~ 아이패드로 하면 더 크게 보이겠당",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100033155.1.18",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100033155.1.19",
          "form": "그래서 10개만채우고 한판갈기려고",
          "original_form": "그래서 10개만채우고 한판갈기려고",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100033155.1.20",
          "form": "ㄴ\n오키 가보자고",
          "original_form": "ㄴ\n오키 가보자고",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100033155.1.21",
          "form": "아싸",
          "original_form": "아싸",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100033155.1.22",
          "form": "나 아이패드켠다",
          "original_form": "나 아이패드켠다",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100033155.1.23",
          "form": "근데 앱이름이뭐야",
          "original_form": "근데 앱이름이뭐야",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100033155.1.24",
          "form": "그냥 루미큐브 치면 나옴",
          "original_form": "그냥 루미큐브 치면 나옴",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100033155.1.25",
          "form": "루미큐브가 재수없게 널 바라보고 있어",
          "original_form": "루미큐브가 재수없게 널 바라보고 있어",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100033155.1.26",
          "form": "ㅇㅋ지금깔러가는중.",
          "original_form": "ㅇㅋ지금깔러가는중.",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100033155.1.27",
          "form": "이따 저녁에 하자 나 강의 좀 듣고",
          "original_form": "이따 저녁에 하자 나 강의 좀 듣고",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100033155.1.28",
          "form": "오키",
          "original_form": "오키",
          "speaker_id": "1",
          "time": "20210906 17:51"
        }
