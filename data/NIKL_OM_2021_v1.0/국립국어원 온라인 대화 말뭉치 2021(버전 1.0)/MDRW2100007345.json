      "utterance": [
        {
          "id": "MDRW2100007345.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007345.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007345.1.3",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007345.1.4",
          "form": "네 저 고양이 좋아해요",
          "original_form": "네 저 고양이 좋아해요",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007345.1.5",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007345.1.6",
          "form": "전 강아지요!",
          "original_form": "전 강아지요!",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007345.1.7",
          "form": "자고있어요 ㅋㅋ",
          "original_form": "자고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007345.1.8",
          "form": "헉 키우시나요? ㅜㅜ너무 귀엽겠어요",
          "original_form": "헉 키우시나요? ㅜㅜ너무 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007345.1.9",
          "form": "네 ㅎㅎ 8년째 키우고 잇어요",
          "original_form": "네 ㅎㅎ 8년째 키우고 잇어요",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007345.1.10",
          "form": "8년을 봐도 매일이 귀여운것같아요 ㅠㅠ",
          "original_form": "8년을 봐도 매일이 귀여운것같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007345.1.11",
          "form": "헐 너무 부러워요...",
          "original_form": "헐 너무 부러워요...",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007345.1.12",
          "form": "강아지도 코로나 백신맞는 경우도 있던데",
          "original_form": "강아지도 코로나 백신맞는 경우도 있던데",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007345.1.13",
          "form": "진짜 맞기도하나요? 그냥 인터넷에 떠도는 말인가했어요!",
          "original_form": "진짜 맞기도하나요? 그냥 인터넷에 떠도는 말인가했어요!",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007345.1.14",
          "form": "헐...전 처음 듣는 말인데",
          "original_form": "헐...전 처음 듣는 말인데",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007345.1.15",
          "form": "잘 모르겠어요",
          "original_form": "잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007345.1.16",
          "form": "일단 저희 강아지는 안맞았아요!",
          "original_form": "일단 저희 강아지는 안맞았아요!",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007345.1.17",
          "form": "아 그렇구나!!",
          "original_form": "아 그렇구나!!",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007345.1.18",
          "form": "강아지 키우면 힘든점이뭐에요?",
          "original_form": "강아지 키우면 힘든점이뭐에요?",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100007345.1.19",
          "form": "음.. 아무래도 털빠짐이요 ㅠㅠ",
          "original_form": "음.. 아무래도 털빠짐이요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007345.1.20",
          "form": "산책도 꾸준히 못시켜줘서 많이 미안하더라구요",
          "original_form": "산책도 꾸준히 못시켜줘서 많이 미안하더라구요",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007345.1.21",
          "form": "아아 ㅜㅜ 털빠짐 많이 심한가요?",
          "original_form": "아아 ㅜㅜ 털빠짐 많이 심한가요?",
          "speaker_id": "2",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007345.1.22",
          "form": "음 저희개는 장모치와와라",
          "original_form": "음 저희개는 장모치와와라",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007345.1.23",
          "form": "아주 심하지는 않지만 그래도 좀 있는 편인데",
          "original_form": "아주 심하지는 않지만 그래도 좀 있는 편인데",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007345.1.24",
          "form": "그래도 8년 사니까 적응돼서 괜찮아요 ㅋㅋㅋ",
          "original_form": "그래도 8년 사니까 적응돼서 괜찮아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007345.1.25",
          "form": "대신 집안에서 검은옷은 못 입구요 ㅋㅋㅋ",
          "original_form": "대신 집안에서 검은옷은 못 입구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007345.1.26",
          "form": "와 그 정도구나.. 저는 고양이만 그런줄알았어요",
          "original_form": "와 그 정도구나.. 저는 고양이만 그런줄알았어요",
          "speaker_id": "2",
          "time": "20210822 12:52"
        }
      ]
    }
