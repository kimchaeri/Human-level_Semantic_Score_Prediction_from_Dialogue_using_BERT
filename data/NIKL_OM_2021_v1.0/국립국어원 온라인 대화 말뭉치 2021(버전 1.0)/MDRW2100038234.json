      "utterance": [
        {
          "id": "MDRW2100038234.1.1",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038234.1.2",
          "form": "팁 받으셨다고 그렇게 행복해 하시네",
          "original_form": "팁 받으셨다고 그렇게 행복해 하시네",
          "speaker_id": "1",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038234.1.3",
          "form": "얼마안되는 돈인데도 팁으로 받아서",
          "original_form": "얼마안되는 돈인데도 팁으로 받아서",
          "speaker_id": "2",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038234.1.4",
          "form": "자랑하신다",
          "original_form": "자랑하신다",
          "speaker_id": "1",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038234.1.5",
          "form": "귀염 신구할배",
          "original_form": "귀염 신구할배",
          "speaker_id": "2",
          "time": "20210902 01:24"
        },
        {
          "id": "MDRW2100038234.1.6",
          "form": "그러게 니들이 팁 맛을 알어 ...패러디",
          "original_form": "그러게 니들이 팁 맛을 알어 ...패러디",
          "speaker_id": "1",
          "time": "20210902 01:24"
        },
        {
          "id": "MDRW2100038234.1.7",
          "form": "너두 웃긴다",
          "original_form": "너두 웃긴다",
          "speaker_id": "2",
          "time": "20210902 01:24"
        },
        {
          "id": "MDRW2100038234.1.8",
          "form": "영업 앞두고 예민해진 윤사장이래",
          "original_form": "영업 앞두고 예민해진 윤사장이래",
          "speaker_id": "1",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038234.1.9",
          "form": "첫날 가게가 그렇게 되었으니",
          "original_form": "첫날 가게가 그렇게 되었으니",
          "speaker_id": "2",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038234.1.10",
          "form": "맞다 그래서 더 예민할수도 있겠네",
          "original_form": "맞다 그래서 더 예민할수도 있겠네",
          "speaker_id": "1",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038234.1.11",
          "form": "저때 이서진도 젊어보인다",
          "original_form": "저때 이서진도 젊어보인다",
          "speaker_id": "2",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038234.1.12",
          "form": "응 윤식당 한지 조금 되었나봐",
          "original_form": "응 윤식당 한지 조금 되었나봐",
          "speaker_id": "1",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038234.1.13",
          "form": "그런가보네",
          "original_form": "그런가보네",
          "speaker_id": "2",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038234.1.14",
