      "utterance": [
        {
          "id": "MDRW2100031401.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031401.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031401.1.3",
          "form": "집안일 중 좋아하시는 것 있나요?",
          "original_form": "집안일 중 좋아하시는 것 있나요?",
          "speaker_id": "2",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031401.1.4",
          "form": "전 정리랑 버리기요 ㅋㅋ",
          "original_form": "전 정리랑 버리기요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031401.1.5",
          "form": "저는 요리 좋아해요",
          "original_form": "저는 요리 좋아해요",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031401.1.6",
          "form": "버릴때 기준이 있으세요?",
          "original_form": "버릴때 기준이 있으세요?",
          "speaker_id": "1",
          "time": "20210904 21:21"
        },
        {
          "id": "MDRW2100031401.1.7",
          "form": "버릴 때 좋아하지 않거나 쓸모가 없으면 버려요",
          "original_form": "버릴 때 좋아하지 않거나 쓸모가 없으면 버려요",
          "speaker_id": "2",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031401.1.8",
          "form": "요리 자주 해 드시나요?",
          "original_form": "요리 자주 해 드시나요?",
          "speaker_id": "2",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100031401.1.9",
          "form": "네 하루 한끼정도요",
          "original_form": "네 하루 한끼정도요",
          "speaker_id": "1",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100031401.1.10",
          "form": "전 큰 물건은 잘 버리는데 작은 잡동사시를 잘 못버리겠어요",
          "original_form": "전 큰 물건은 잘 버리는데 작은 잡동사시를 잘 못버리겠어요",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031401.1.11",
          "form": "원래 소품이 어려워요 ㅠ 분류를 잘 해 놓아야 되서요",
          "original_form": "원래 소품이 어려워요 ㅠ 분류를 잘 해 놓아야 되서요",
          "speaker_id": "2",
          "time": "20210904 21:28"
        },
        {
          "id": "MDRW2100031401.1.12",
          "form": "분류할때 플라스틱통이나 다른 어떤거 쓰는게 나을까요",
          "original_form": "분류할때 플라스틱통이나 다른 어떤거 쓰는게 나을까요",
          "speaker_id": "1",
          "time": "20210905 11:14"
        },
        {
          "id": "MDRW2100031401.1.13",
          "form": "저는 칸막이 있으면 좋더라구요 아니면 작은 서랍이요",
          "original_form": "저는 칸막이 있으면 좋더라구요 아니면 작은 서랍이요",
          "speaker_id": "2",
          "time": "20210905 11:17"
        },
        {
          "id": "MDRW2100031401.1.14",
          "form": "그러고보니 분류도 잘 못하네요 ㅎㅎ",
          "original_form": "그러고보니 분류도 잘 못하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 11:17"
        },
        {
          "id": "MDRW2100031401.1.15",
          "form": "카테고리 만드시면 좋아요 요리도구 카테고리 이런 식으로요",
          "original_form": "카테고리 만드시면 좋아요 요리도구 카테고리 이런 식으로요",
          "speaker_id": "2",
          "time": "20210905 11:19"
        },
        {
          "id": "MDRW2100031401.1.16",
          "form": "그렇군요~",
          "original_form": "그렇군요~",
          "speaker_id": "1",
          "time": "20210905 11:24"
        },
        {
          "id": "MDRW2100031401.1.17",
          "form": "청소 빨래 요리는 크게크게 하는건데",
          "original_form": "청소 빨래 요리는 크게크게 하는건데",
          "speaker_id": "1",
          "time": "20210905 11:25"
        },
        {
          "id": "MDRW2100031401.1.18",
          "form": "서랍정리는 힘드네영",
          "original_form": "서랍정리는 힘드네영",
          "speaker_id": "1",
          "time": "20210905 11:25"
        },
        {
          "id": "MDRW2100031401.1.19",
          "form": "맞아요 크게 하는 건 그래도 빨리하는데 자잘한 물건이 더 손이 많이가요",
          "original_form": "맞아요 크게 하는 건 그래도 빨리하는데 자잘한 물건이 더 손이 많이가요",
          "speaker_id": "2",
          "time": "20210905 11:30"
        },
        {
          "id": "MDRW2100031401.1.20",
          "form": "저는 요리하는거 냉장고 정리하는 거 잘해요",
          "original_form": "저는 요리하는거 냉장고 정리하는 거 잘해요",
          "speaker_id": "1",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100031401.1.21",
          "form": "설거지는 좀 지겹구요 ㅎㅎㅎ",
          "original_form": "설거지는 좀 지겹구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:19"
        }
      ]
