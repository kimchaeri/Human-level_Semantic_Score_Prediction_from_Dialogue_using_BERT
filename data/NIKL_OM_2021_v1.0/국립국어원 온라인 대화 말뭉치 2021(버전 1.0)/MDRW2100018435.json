      "utterance": [
        {
          "id": "MDRW2100018435.1.1",
          "form": "그럼 대면이랑 비대면 중에 뮤ㅓ가 더 좋으신 것 같아요?",
          "original_form": "그럼 대면이랑 비대면 중에 뮤ㅓ가 더 좋으신 것 같아요?",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018435.1.2",
          "form": "전 솔직히 비대면이라 편하고 좋긴 하더라구요ㅋㅋ",
          "original_form": "전 솔직히 비대면이라 편하고 좋긴 하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018435.1.3",
          "form": "ㅋㅋㅋ 솔직히",
          "original_form": "ㅋㅋㅋ 솔직히",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018435.1.4",
          "form": "편한건 비대면이 편하긴해요",
          "original_form": "편한건 비대면이 편하긴해요",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018435.1.5",
          "form": "근데 이제 자유로워진 20대인데",
          "original_form": "근데 이제 자유로워진 20대인데",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018435.1.6",
          "form": "맘편하게 놀면서 즐기고싶은 대학교를 못가니",
          "original_form": "맘편하게 놀면서 즐기고싶은 대학교를 못가니",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018435.1.7",
          "form": "그게 아쉽긴 하죠ㅠㅠ",
          "original_form": "그게 아쉽긴 하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018435.1.8",
          "form": "답답한건 있어요",
          "original_form": "답답한건 있어요",
          "speaker_id": "1",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018435.1.9",
          "form": "그렇군요 얼른 코로나가 괜찮아졌움 좋겠네요",
          "original_form": "그렇군요 얼른 코로나가 괜찮아졌움 좋겠네요",
          "speaker_id": "2",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018435.1.10",
          "form": "내년에는 대면할 수 있겠죠?ㅎㅎ..",
          "original_form": "내년에는 대면할 수 있겠죠?ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018435.1.11",
          "form": "음 근데 이번 2학기에",
          "original_form": "음 근데 이번 2학기에",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018435.1.12",
          "form": "대면 진행하기로 한 과목들도 있어서",
          "original_form": "대면 진행하기로 한 과목들도 있어서",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018435.1.13",
          "form": "솔직히 모르겠네요 ㅋㅋㅋ",
          "original_form": "솔직히 모르겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018435.1.14",
          "form": "헉 벌써 대면인 과목들도 있네여!",
          "original_form": "헉 벌써 대면인 과목들도 있네여!",
          "speaker_id": "2",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018435.1.15",
          "form": "대면이라 생각하니 뭔가 학교가기 귀찮아지는거같아요ㅋㅋ",
          "original_form": "대면이라 생각하니 뭔가 학교가기 귀찮아지는거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018435.1.16",
          "form": "아ㅋㅋㅠ전 이번에 1교시가 4번이라서 대면하면 망해요...",
          "original_form": "아ㅋㅋㅠ전 이번에 1교시가 4번이라서 대면하면 망해요...",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100018435.1.17",
          "form": "ㅋㅋㅋ 1교시 수업 지옥이죠",
          "original_form": "ㅋㅋㅋ 1교시 수업 지옥이죠",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100018435.1.18",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100018435.1.19",
          "form": "학녀닝 어떻게되세여",
          "original_form": "학녀닝 어떻게되세여",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100018435.1.20",
          "form": "저 1학년이요",
          "original_form": "저 1학년이요",
          "speaker_id": "2",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100018435.1.21",
          "form": "교양은 재밌는걸로 신청하셧나여",
          "original_form": "교양은 재밌는걸로 신청하셧나여",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100018435.1.22",
          "form": "교양 재밌는건 다 경쟁률이 엄청나서 못했어요ㅋㅋ",
          "original_form": "교양 재밌는건 다 경쟁률이 엄청나서 못했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100018435.1.23",
          "form": "이번에 수강신청 좀 망한 것 같아여",
          "original_form": "이번에 수강신청 좀 망한 것 같아여",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100018435.1.24",
          "form": "헉 ㅋㅋㅋ 수강신청 망하면 아쉽죠",
          "original_form": "헉 ㅋㅋㅋ 수강신청 망하면 아쉽죠",
          "speaker_id": "1",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100018435.1.25",
          "form": "허가서는 안받아주시나봐여",
          "original_form": "허가서는 안받아주시나봐여",
          "speaker_id": "1",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100018435.1.26",
          "form": "네ㅠㅠ 아쉽죠",
          "original_form": "네ㅠㅠ 아쉽죠",
          "speaker_id": "2",
          "time": "20210828 19:16"
        }
      ]
