      "utterance": [
        {
          "id": "MDRW2100036612.1.1",
          "form": "그래서 알고보면 성격 진짜 더 별로일거같은.. ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래서 알고보면 성격 진짜 더 별로일거같은.. ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:04"
        },
        {
          "id": "MDRW2100036612.1.2",
          "form": "맞아여...그냥 딱봐도 별로에용 ㅠ",
          "original_form": "맞아여...그냥 딱봐도 별로에용 ㅠ",
          "speaker_id": "1",
          "time": "20210831 02:04"
        },
        {
          "id": "MDRW2100036612.1.3",
          "form": "아니 이혜영 스토리도 듣는데 슬프네여...",
          "original_form": "아니 이혜영 스토리도 듣는데 슬프네여...",
          "speaker_id": "2",
          "time": "20210831 02:06"
        },
        {
          "id": "MDRW2100036612.1.4",
          "form": "와 재열 직진ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 재열 직진ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:08"
        },
        {
          "id": "MDRW2100036612.1.5",
          "form": "아 더 비호감",
          "original_form": "아 더 비호감",
          "speaker_id": "2",
          "time": "20210831 02:08"
        },
        {
          "id": "MDRW2100036612.1.6",
          "form": "비호감 직진!!!",
          "original_form": "비호감 직진!!!",
          "speaker_id": "2",
          "time": "20210831 02:08"
        },
        {
          "id": "MDRW2100036612.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ직진하는애들 다 비호감",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ직진하는애들 다 비호감",
          "speaker_id": "1",
          "time": "20210831 02:12"
        },
        {
          "id": "MDRW2100036612.1.8",
          "form": "아니 이거 모에요?",
          "original_form": "아니 이거 모에요?",
          "speaker_id": "2",
          "time": "20210831 02:15"
        },
        {
          "id": "MDRW2100036612.1.9",
          "form": "3박4일하고 끝?",
          "original_form": "3박4일하고 끝?",
          "speaker_id": "2",
          "time": "20210831 02:15"
        },
        {
          "id": "MDRW2100036612.1.10",
          "form": "그리고 동거라니..??",
          "original_form": "그리고 동거라니..??",
          "speaker_id": "2",
          "time": "20210831 02:16"
        },
        {
          "id": "MDRW2100036612.1.11",
          "form": "너무 짧다 8명모여있는게 더 잼날거같은데",
          "original_form": "너무 짧다 8명모여있는게 더 잼날거같은데",
          "speaker_id": "2",
          "time": "20210831 02:16"
        },
        {
          "id": "MDRW2100036612.1.12",
          "form": "아..어디갔어요 빨리와요...",
          "original_form": "아..어디갔어요 빨리와요...",
          "speaker_id": "2",
          "time": "20210831 02:24"
        },
        {
          "id": "MDRW2100036612.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036612.1.14",
          "form": "8명안모입니다",
          "original_form": "8명안모입니다",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036612.1.15",
          "form": "동거시작해요",
          "original_form": "동거시작해요",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036612.1.16",
          "form": "재미덜하긴한데 그래도 ㅋㅋㅋ웃김",
          "original_form": "재미덜하긴한데 그래도 ㅋㅋㅋ웃김",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100036612.1.17",
          "form": "아..그럼 파트너 정해지면",
          "original_form": "아..그럼 파트너 정해지면",
          "speaker_id": "2",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100036612.1.18",
          "form": "바꾸는 기회 0??",
          "original_form": "바꾸는 기회 0??",
          "speaker_id": "2",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100036612.1.19",
          "form": "네업습니다",
          "original_form": "네업습니다",
          "speaker_id": "1",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036612.1.20",
          "form": "걍 동거하고 ...더만날거냐 물어보고끝",
          "original_form": "걍 동거하고 ...더만날거냐 물어보고끝",
          "speaker_id": "1",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036612.1.21",
          "form": "다같이잇는데 존잼인데 진짜.ㅠㅋㅋ",
          "original_form": "다같이잇는데 존잼인데 진짜.ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:48"
        },
        {
          "id": "MDRW2100036612.1.22",
          "form": "아... 아니... 너무 마지막 데이트 정하는 방식도 별로고",
          "original_form": "아... 아니... 너무 마지막 데이트 정하는 방식도 별로고",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036612.1.23",
          "form": "좀 돌아가면서 1대1 더해봐야지...ㅡㅡ",
          "original_form": "좀 돌아가면서 1대1 더해봐야지...ㅡㅡ",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036612.1.24",
          "form": "별로네요",
          "original_form": "별로네요",
          "speaker_id": "2",
          "time": "20210831 02:49"
        },
        {
          "id": "MDRW2100036612.1.25",
          "form": "4커플 다 동거해요?",
          "original_form": "4커플 다 동거해요?",
          "speaker_id": "2",
          "time": "20210831 02:55"
        },
        {
          "id": "MDRW2100036612.1.26",
          "form": "ㅋㅋㅋㅋ 4컵 불가능하죠",
          "original_form": "ㅋㅋㅋㅋ 4컵 불가능하죠",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036612.1.27",
          "form": "그럼 몇커플이에요 ㄴㅋㅋ커플안되면 이제 안나오는거겠네요??",
          "original_form": "그럼 몇커플이에요 ㄴㅋㅋ커플안되면 이제 안나오는거겠네요??",
          "speaker_id": "2",
          "time": "20210831 13:07"
        },
        {
          "id": "MDRW2100036612.1.28",
          "form": "지금 점심먹으면서도 봤는데",
          "original_form": "지금 점심먹으면서도 봤는데",
          "speaker_id": "2",
          "time": "20210831 13:07"
        },
        {
          "id": "MDRW2100036612.1.29",
          "form": "배타고 재열ㅋㅋㅋㅋㅋㅌㅋㅋ아 ㅋㅋㅋㅋㅋ진짜",
          "original_form": "배타고 재열ㅋㅋㅋㅋㅋㅌㅋㅋ아 ㅋㅋㅋㅋㅋ진짜",
          "speaker_id": "2",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100036612.1.30",
          "form": "한대줘패고싶네욬ㅋㅋㅋㅋㅌㅌ",
          "original_form": "한대줘패고싶네욬ㅋㅋㅋㅋㅌㅌ",
          "speaker_id": "2",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100036612.1.31",
          "form": "ㅋㅋㅋ 효정이 사이다라 다행",
          "original_form": "ㅋㅋㅋ 효정이 사이다라 다행",
          "speaker_id": "1",
          "time": "20210831 13:30"
        },
        {
          "id": "MDRW2100036612.1.32",
          "form": "3커플댐니다",
          "original_form": "3커플댐니다",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100036612.1.33",
          "form": "헐 그럼 효정 재열 커플 가긴 가나요...??",
          "original_form": "헐 그럼 효정 재열 커플 가긴 가나요...??",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036612.1.34",
          "form": "제발 도망쳐...",
          "original_form": "제발 도망쳐...",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036612.1.35",
