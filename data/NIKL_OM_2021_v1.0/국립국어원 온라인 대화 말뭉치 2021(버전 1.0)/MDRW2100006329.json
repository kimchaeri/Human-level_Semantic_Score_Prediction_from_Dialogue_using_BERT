      "utterance": [
        {
          "id": "MDRW2100006329.1.1",
          "form": "책 읽으시나요",
          "original_form": "책 읽으시나요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006329.1.2",
          "form": "늘",
          "original_form": "늘",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006329.1.3",
          "form": "읽으려 하는데",
          "original_form": "읽으려 하는데",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006329.1.4",
          "form": "한달에 한권꼴로만",
          "original_form": "한달에 한권꼴로만",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006329.1.5",
          "form": "간신히 읽네요",
          "original_form": "간신히 읽네요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006329.1.6",
          "form": "그래도 책을.좋아하시나봅니다",
          "original_form": "그래도 책을.좋아하시나봅니다",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006329.1.7",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006329.1.8",
          "form": "영상 매체보단",
          "original_form": "영상 매체보단",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006329.1.9",
          "form": "글을 더 좋아해요",
          "original_form": "글을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006329.1.10",
          "form": "그럼 주로 사서 읽으시나요",
          "original_form": "그럼 주로 사서 읽으시나요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100006329.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006329.1.12",
          "form": "참 사람이 소장하는걸",
          "original_form": "참 사람이 소장하는걸",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006329.1.13",
          "form": "좋아하는지라",
          "original_form": "좋아하는지라",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006329.1.14",
          "form": "집에 책이 많겠네요",
          "original_form": "집에 책이 많겠네요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006329.1.15",
          "form": "적다고는 하기 힘들 정도로는 있어요",
          "original_form": "적다고는 하기 힘들 정도로는 있어요",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006329.1.16",
          "form": "그러시군요 아이짐이 많아지니 어른 책은 강제 처분하게 됐어요ㅠㅠ",
          "original_form": "그러시군요 아이짐이 많아지니 어른 책은 강제 처분하게 됐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006329.1.17",
          "form": "아아...",
          "original_form": "아아...",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006329.1.18",
          "form": "그건 어쩔 수 없죠",
          "original_form": "그건 어쩔 수 없죠",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006329.1.19",
          "form": "전자책/오디오북도 이용하시나요?",
          "original_form": "전자책/오디오북도 이용하시나요?",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006329.1.20",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100006329.1.21",
          "form": "전자책은",
          "original_form": "전자책은",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100006329.1.22",
          "form": "제법 이용하는데",
          "original_form": "제법 이용하는데",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006329.1.23",
          "form": "오디오북은",
          "original_form": "오디오북은",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006329.1.24",
          "form": "거의 안 쓰네요",
          "original_form": "거의 안 쓰네요",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006329.1.25",
          "form": "오디오북 어플 받아보셨나요",
          "original_form": "오디오북 어플 받아보셨나요",
          "speaker_id": "1",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100006329.1.26",
          "form": "오디오북이요?",
