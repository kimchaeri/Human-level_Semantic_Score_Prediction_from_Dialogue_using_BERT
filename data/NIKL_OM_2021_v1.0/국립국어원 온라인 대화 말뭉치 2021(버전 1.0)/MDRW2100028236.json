      "utterance": [
        {
          "id": "MDRW2100028236.1.1",
          "form": "안녕하세요 저녁 드셨어요?",
          "original_form": "안녕하세요 저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100028236.1.2",
          "form": "네 간단히 먹었어요",
          "original_form": "네 간단히 먹었어요",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028236.1.3",
          "form": "저도 입맛이 없어 볶음밥 간단히 먹었는데",
          "original_form": "저도 입맛이 없어 볶음밥 간단히 먹었는데",
          "speaker_id": "2",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028236.1.4",
          "form": "김치볶음밥?",
          "original_form": "김치볶음밥?",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028236.1.5",
          "form": "참치랑 야채 조금 넣었어요",
          "original_form": "참치랑 야채 조금 넣었어요",
          "speaker_id": "2",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028236.1.6",
          "form": "맛있었겠네요",
          "original_form": "맛있었겠네요",
          "speaker_id": "1",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028236.1.7",
          "form": "별로였어요",
          "original_form": "별로였어요",
          "speaker_id": "2",
          "time": "20210902 18:29"
        },
        {
          "id": "MDRW2100028236.1.8",
          "form": "약 먹으려고 억지로 먹었어요",
          "original_form": "약 먹으려고 억지로 먹었어요",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028236.1.9",
          "form": "잘 챙겨 먹어야 건강해요",
          "original_form": "잘 챙겨 먹어야 건강해요",
          "speaker_id": "1",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028236.1.10",
          "form": "식탁 김 없었으면 못 먹고 다 남겼을 거 같아요",
          "original_form": "식탁 김 없었으면 못 먹고 다 남겼을 거 같아요",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028236.1.11",
          "form": "그쵸 조미김 정말 맛있죠",
          "original_form": "그쵸 조미김 정말 맛있죠",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028236.1.12",
          "form": "조미김도 맛있고 재래식 돌김 구워서 기름장 곁들이는 것도 좋아요",
          "original_form": "조미김도 맛있고 재래식 돌김 구워서 기름장 곁들이는 것도 좋아요",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028236.1.13",
          "form": "먹을 줄 아네요",
          "original_form": "먹을 줄 아네요",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028236.1.14",
          "form": "원초가 좋으면 정말 맛있더라고요",
          "original_form": "원초가 좋으면 정말 맛있더라고요",
          "speaker_id": "2",
          "time": "20210902 18:32"
