      "utterance": [
        {
          "id": "MDRW2100039343.1.1",
          "form": "안녕하세요 요즘 보시는 티비프로 있으세요?",
          "original_form": "안녕하세요 요즘 보시는 티비프로 있으세요?",
          "speaker_id": "1",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100039343.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039343.1.3",
          "form": "저는 펜트하우스 보고있어요ㅎㅎ",
          "original_form": "저는 펜트하우스 보고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039343.1.4",
          "form": "요즘 보시는거 있으세요?",
          "original_form": "요즘 보시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039343.1.5",
          "form": "저도 펜트하우스ㅎㅎㅎ 담주부턴 안봐야지라고 쭉 보고있네요ㅋㅋ",
          "original_form": "저도 펜트하우스ㅎㅎㅎ 담주부턴 안봐야지라고 쭉 보고있네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100039343.1.6",
          "form": "ㅋㅋㅋㅋㅋ맞아요 넘 막장이고 현실성이 없는데도",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 넘 막장이고 현실성이 없는데도",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039343.1.7",
          "form": "계속 보게되는 순옥킴 드라마...",
          "original_form": "계속 보게되는 순옥킴 드라마...",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039343.1.8",
          "form": "맞아여ㅋㅋㅋ그리고 여기까지 온이상 결말을 봐야겠어요ㅋㅋ",
          "original_form": "맞아여ㅋㅋㅋ그리고 여기까지 온이상 결말을 봐야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039343.1.9",
          "form": "ㅋㅋㅋㅋ저도요 이제 진짜 마지막이잖아요",
          "original_form": "ㅋㅋㅋㅋ저도요 이제 진짜 마지막이잖아요",
          "speaker_id": "2",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100039343.1.10",
          "form": "퀸수련 복수 마무리 잘되길...!",
          "original_form": "퀸수련 복수 마무리 잘되길...!",
          "speaker_id": "2",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100039343.1.11",
          "form": "네 이젠 진짜 복수했으면...ㅋㅋㅋ지치네요ㅠㅠ",
          "original_form": "네 이젠 진짜 복수했으면...ㅋㅋㅋ지치네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039343.1.12",
          "form": "저번주부터 복수하고 있지만 완전 후련한건 아닌거같아요ㅋㅋㅋㅋㅋ",
          "original_form": "저번주부터 복수하고 있지만 완전 후련한건 아닌거같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100039343.1.13",
          "form": "마무리 깔끔하길...!!",
          "original_form": "마무리 깔끔하길...!!",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039343.1.14",
          "form": "그니까요ㅋㅋ 특히 주단태ㅜㅜ너무 끈질겨요ㅠㅠ",
          "original_form": "그니까요ㅋㅋ 특히 주단태ㅜㅜ너무 끈질겨요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039343.1.15",
          "form": "하지만 단탱이 이제 아웃!!",
          "original_form": "하지만 단탱이 이제 아웃!!",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039343.1.16",
          "form": "진짜 엄기준 배우님 악역 많이 하시는거 같아요ㅋㅋ",
          "original_form": "진짜 엄기준 배우님 악역 많이 하시는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039343.1.17",
          "form": "네ㅋㅋㅋ이번엔 진짜 끝판왕격..ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ이번엔 진짜 끝판왕격..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039343.1.18",
          "form": "혹시 피고인도 보셨을까요??",
          "original_form": "혹시 피고인도 보셨을까요??",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039343.1.19",
          "form": "피고인때도 엄배우님 넘 연기잘하셔서",
          "original_form": "피고인때도 엄배우님 넘 연기잘하셔서",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039343.1.20",
          "form": "욕하면서 봤던 기억이 있어요ㅋㅋㅋㅋ",
          "original_form": "욕하면서 봤던 기억이 있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039343.1.21",
          "form": "저 그거 얘기한거에요ㅋㅋ왜자꾸 이런 역할ㅋㅋㅋㅋ",
          "original_form": "저 그거 얘기한거에요ㅋㅋ왜자꾸 이런 역할ㅋㅋㅋㅋ",
