      "utterance": [
        {
          "id": "MDRW2100018342.1.1",
          "form": "최근에 산 물건 있으세요?",
          "original_form": "최근에 산 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018342.1.2",
          "form": "아 저는 남방 샀어요 날이 쌀쌀해지니까 걸칠게 필요해서요",
          "original_form": "아 저는 남방 샀어요 날이 쌀쌀해지니까 걸칠게 필요해서요",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100018342.1.3",
          "form": "오 이제 곧 가을이네요",
          "original_form": "오 이제 곧 가을이네요",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100018342.1.4",
          "form": "저도 가을옷 준비해야겠어요",
          "original_form": "저도 가을옷 준비해야겠어요",
          "speaker_id": "1",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100018342.1.5",
          "form": "그쵸 시간이 너무 빠르게 지나가는것 같아요",
          "original_form": "그쵸 시간이 너무 빠르게 지나가는것 같아요",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100018342.1.6",
          "form": "깅엄체크 아세요?",
          "original_form": "깅엄체크 아세요?",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100018342.1.7",
          "form": "저는 이번에  처음 알았어요!",
          "original_form": "저는 이번에  처음 알았어요!",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100018342.1.8",
          "form": "아마 보시면 아실거에요ㅋㅋㅋ",
          "original_form": "아마 보시면 아실거에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100018342.1.9",
          "form": "깅엄체크요?",
          "original_form": "깅엄체크요?",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100018342.1.10",
          "form": "한번 찾아볼게요! 이름이 특이하네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "한번 찾아볼게요! 이름이 특이하네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100018342.1.11",
          "form": "아하 이 작은 체크가 깅엄체크였구나",
          "original_form": "아하 이 작은 체크가 깅엄체크였구나",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100018342.1.12",
          "form": "무슨색사셨어요?",
          "original_form": "무슨색사셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100018342.1.13",
          "form": "무난무난한 검정에 흰색이요ㅋㅋㅋ 보니까 딱 아시겠죠 이름이 있는게 신기했어요",
          "original_form": "무난무난한 검정에 흰색이요ㅋㅋㅋ 보니까 딱 아시겠죠 이름이 있는게 신기했어요",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100018342.1.14",
          "form": "네 이름도 특이해서 이제 안까먹을거 같아요ㅋㅋㅋㅋ",
          "original_form": "네 이름도 특이해서 이제 안까먹을거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100018342.1.15",
          "form": "오늘 날씨 정말 쌀쌀하네요 저도 긴팔 좀 사야겠어요",
          "original_form": "오늘 날씨 정말 쌀쌀하네요 저도 긴팔 좀 사야겠어요",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100018342.1.16",
          "form": "그니까요 지금 완전 쌀쌀 무슨일이죠",
          "original_form": "그니까요 지금 완전 쌀쌀 무슨일이죠",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100018342.1.17",
          "form": "ㅋㅋㅋㅋ바람이 챠가워요",
          "original_form": "ㅋㅋㅋㅋ바람이 챠가워요",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100018342.1.18",
          "form": "차가운거아니고 ㅋㅋ",
          "original_form": "차가운거아니고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100018342.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗 어떤 느낌인지 알겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗 어떤 느낌인지 알겠어요",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100018342.1.20",
          "form": "뭐라도 걸치고 나가야할 날씨ㅋㅋㅋ",
          "original_form": "뭐라도 걸치고 나가야할 날씨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100018342.1.21",
          "form": "저는 트렌치를 사볼까 싶네요 한달 쯤 뒤에 입게",
          "original_form": "저는 트렌치를 사볼까 싶네요 한달 쯤 뒤에 입게",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100018342.1.22",
          "form": "그니까요 감기 걸리지 않게 조심하세요",
          "original_form": "그니까요 감기 걸리지 않게 조심하세요",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100018342.1.23",
          "form": "아 저는 비추에요",
          "original_form": "아 저는 비추에요",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100018342.1.24",
          "form": "이제 봄가을이 진짜 너무 짧아가지구",
          "original_form": "이제 봄가을이 진짜 너무 짧아가지구",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100018342.1.25",
          "form": "앗... 그렇죠 ㅠㅠㅠ",
          "original_form": "앗... 그렇죠 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100018342.1.26",
          "form": "네 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "네 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100018342.1.27",
          "form": "스테디긴한데",
          "original_form": "스테디긴한데",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100018342.1.28",
          "form": "그래도 매년 약간씩 디테일이 달라지잖아요",
          "original_form": "그래도 매년 약간씩 디테일이 달라지잖아요",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100018342.1.29",
          "form": "가을 왤케 짧아진건지 몰겠어요",
          "original_form": "가을 왤케 짧아진건지 몰겠어요",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100018342.1.30",
          "form": "그냥 맨투맨을 사야하나 ㅠ",
          "original_form": "그냥 맨투맨을 사야하나 ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018342.1.31",
          "form": "너무 아시워요 ㅠ",
          "original_form": "너무 아시워요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018342.1.32",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018342.1.33",
          "form": "어느 계절 좋아하세요?",
          "original_form": "어느 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018342.1.34",
          "form": "저는 겨울 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 겨울 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018342.1.35",
          "form": "역시즌으로 롱패딩이나 살까요?",
          "original_form": "역시즌으로 롱패딩이나 살까요?",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018342.1.36",
          "form": "오! 겨울 좋아하시는 구나",
          "original_form": "오! 겨울 좋아하시는 구나",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100018342.1.37",
          "form": "롱패딩 좋죠 쌀때 구매하세요",
          "original_form": "롱패딩 좋죠 쌀때 구매하세요",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100018342.1.38",
          "form": "ㅋㅋㅋㅋㅋ 롱퍄딩은 이제 생존템이자나요",
          "original_form": "ㅋㅋㅋㅋㅋ 롱퍄딩은 이제 생존템이자나요",
          "speaker_id": "2",
          "time": "20210827 20:22"
        }
      ]
