      "utterance": [
        {
          "id": "MDRW2100013636.1.1",
          "form": "엄마 내가 편의점에서 사왔자나",
          "original_form": "엄마 내가 편의점에서 사왔자나",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013636.1.2",
          "form": "과자",
          "original_form": "과자",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013636.1.3",
          "form": "엄마가 밀가루냐고 했던거",
          "original_form": "엄마가 밀가루냐고 했던거",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013636.1.4",
          "form": "그거곰표",
          "original_form": "그거곰표",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013636.1.5",
          "form": "과잔데밀가룬거",
          "original_form": "과잔데밀가룬거",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013636.1.6",
          "form": "요즘 그런게 유행이래요",
          "original_form": "요즘 그런게 유행이래요",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013636.1.7",
          "form": "전에봤는데 맥주도 나왔더라구요",
          "original_form": "전에봤는데 맥주도 나왔더라구요",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013636.1.8",
          "form": "곰표맥주???",
          "original_form": "곰표맥주???",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013636.1.9",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013636.1.10",
          "form": "맛있대요 다음에 제가 한번사갈겡6",
          "original_form": "맛있대요 다음에 제가 한번사갈겡6",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013636.1.11",
          "form": "말표도있대요",
          "original_form": "말표도있대요",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013636.1.12",
          "form": "말표구두약아니냐",
          "original_form": "말표구두약아니냐",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013636.1.13",
          "form": "브랜드만그거고 먹는거ㅋㅋㅋ",
          "original_form": "브랜드만그거고 먹는거ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013636.1.14",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013636.1.15",
          "form": "먹을수있는거맞나",
          "original_form": "먹을수있는거맞나",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013636.1.16",
          "form": "우리과자도먹었자나요",
          "original_form": "우리과자도먹었자나요",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013636.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013636.1.18",
          "form": "그냥 먹어도 될듯",
          "original_form": "그냥 먹어도 될듯",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013636.1.19",
          "form": "그래요즘신기한거만타",
          "original_form": "그래요즘신기한거만타",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013636.1.20",
          "form": "신기한거많이사올게요",
          "original_form": "신기한거많이사올게요",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013636.1.21",
          "form": "맛있는거만사와",
          "original_form": "맛있는거만사와",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013636.1.22",
          "form": "알았어요ㅋㅋㅋㅋㅋ",
          "original_form": "알았어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013636.1.23",
          "form": "다른건없냐",
          "original_form": "다른건없냐",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013636.1.24",
          "form": "근데저도잘몰라요",
          "original_form": "근데저도잘몰라요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013636.1.25",
          "form": "인터넷에ㅋㅋㅋㅋ",
          "original_form": "인터넷에ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013636.1.26",
          "form": "그거보고",
          "original_form": "그거보고",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013636.1.27",
          "form": "가끔사오는거ㅋㅋㅋ",
          "original_form": "가끔사오는거ㅋㅋㅋ",
