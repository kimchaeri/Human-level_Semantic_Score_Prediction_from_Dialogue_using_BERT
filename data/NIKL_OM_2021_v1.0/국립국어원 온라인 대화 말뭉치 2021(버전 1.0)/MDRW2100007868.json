      "utterance": [
        {
          "id": "MDRW2100007868.1.1",
          "form": "요즘 날씨가 좀 갑자기 바뀌었죠?",
          "original_form": "요즘 날씨가 좀 갑자기 바뀌었죠?",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007868.1.2",
          "form": "비가와서 그런가봐요",
          "original_form": "비가와서 그런가봐요",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007868.1.3",
          "form": "여기는 아직도 흐리고 추워요",
          "original_form": "여기는 아직도 흐리고 추워요",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007868.1.4",
          "form": "아 여기도 축축하고 서늘하네요",
          "original_form": "아 여기도 축축하고 서늘하네요",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007868.1.5",
          "form": "날씨가 빨리 갰으면 좋겠네요",
          "original_form": "날씨가 빨리 갰으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007868.1.6",
          "form": "태풍도 온다구 하구..벌써 가을인거 같은ㅎㅎ",
          "original_form": "태풍도 온다구 하구..벌써 가을인거 같은ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100007868.1.7",
          "form": "여름에는 겨울을 바랬는데",
          "original_form": "여름에는 겨울을 바랬는데",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007868.1.8",
          "form": "막상 다가오니까 기분이 뒤숭숭하네요",
          "original_form": "막상 다가오니까 기분이 뒤숭숭하네요",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007868.1.9",
          "form": "ㅋㅋ그러니까요 추워지는거 또 싫으네요ㅎㅎ",
          "original_form": "ㅋㅋ그러니까요 추워지는거 또 싫으네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007868.1.10",
          "form": "환절기가 여러모로 애매한거 같아요",
          "original_form": "환절기가 여러모로 애매한거 같아요",
          "speaker_id": "2",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100007868.1.11",
          "form": "옷 입기도 애매하고",
          "original_form": "옷 입기도 애매하고",
          "speaker_id": "2",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100007868.1.12",
          "form": "네 낮에는 또 더우니까..땀날때두 있구ㅎㅎ",
          "original_form": "네 낮에는 또 더우니까..땀날때두 있구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100007868.1.13",
          "form": "막상 밤되면 추워서 겉옷을 찾게되는",
          "original_form": "막상 밤되면 추워서 겉옷을 찾게되는",
          "speaker_id": "2",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007868.1.14",
          "form": "마성의 날씨...ㅎㅎ",
          "original_form": "마성의 날씨...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007868.1.15",
          "form": "낮에 겉옷들고 다니면 또 짜증나구요ㅠㅠㅋㅋㅋ",
          "original_form": "낮에 겉옷들고 다니면 또 짜증나구요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007868.1.16",
          "form": "낮에는 짐이죠ㅋㅋ",
          "original_form": "낮에는 짐이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007868.1.17",
          "form": "아예 낮밤 둘중에 하나에만 외출을 하면",
          "original_form": "아예 낮밤 둘중에 하나에만 외출을 하면",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007868.1.18",
          "form": "편할텐데 그런것도 아니니까...ㅠ",
          "original_form": "편할텐데 그런것도 아니니까...ㅠ",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007868.1.19",
          "form": "그럼 좋을텐데ㅠㅠ",
          "original_form": "그럼 좋을텐데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 11:38"
