      "utterance": [
        {
          "id": "MDRW2100010611.1.1",
          "form": "무슨 공연가고 싶어?",
          "original_form": "무슨 공연가고 싶어?",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100010611.1.2",
          "form": "가고 싶은 전시회있어?",
          "original_form": "가고 싶은 전시회있어?",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100010611.1.3",
          "form": "없어 가고 싶은 전시회 있어?",
          "original_form": "없어 가고 싶은 전시회 있어?",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100010611.1.4",
          "form": "나는 공연",
          "original_form": "나는 공연",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100010611.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100010611.1.6",
          "form": "무슨 공연 가고싶어?",
          "original_form": "무슨 공연 가고싶어?",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100010611.1.7",
          "form": "난 모르겠다",
          "original_form": "난 모르겠다",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100010611.1.8",
          "form": "아예 공연없으니까 아무 공연이라도 다 좋아",
          "original_form": "아예 공연없으니까 아무 공연이라도 다 좋아",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100010611.1.9",
          "form": "혹시 콘서트 아냐?",
          "original_form": "혹시 콘서트 아냐?",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100010611.1.10",
          "form": "콘서트 좋지",
          "original_form": "콘서트 좋지",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100010611.1.11",
          "form": "가봤어?",
          "original_form": "가봤어?",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100010611.1.12",
          "form": "아니 한번도 가본적이 없어",
          "original_form": "아니 한번도 가본적이 없어",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100010611.1.13",
          "form": "가봤지?",
          "original_form": "가봤지?",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100010611.1.14",
          "form": "코로나끝나고 같이 가자",
          "original_form": "코로나끝나고 같이 가자",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100010611.1.15",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100010611.1.16",
          "form": "내년에 가자고?",
          "original_form": "내년에 가자고?",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100010611.1.17",
          "form": "코로나 끝나고",
          "original_form": "코로나 끝나고",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100010611.1.18",
          "form": "누구 콘서트 가고싶어?",
          "original_form": "누구 콘서트 가고싶어?",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100010611.1.19",
          "form": "exo",
          "original_form": "exo",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100010611.1.20",
          "form": "그래 가자",
          "original_form": "그래 가자",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100010611.1.21",
          "form": "근데 티켓팅이 어려워",
          "original_form": "근데 티켓팅이 어려워",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100010611.1.22",
          "form": "응 같이가자",
          "original_form": "응 같이가자",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100010611.1.23",
          "form": "티켓팅이 뭐야?",
          "original_form": "티켓팅이 뭐야?",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100010611.1.24",
          "form": "티켓예약하는거",
          "original_form": "티켓예약하는거",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
