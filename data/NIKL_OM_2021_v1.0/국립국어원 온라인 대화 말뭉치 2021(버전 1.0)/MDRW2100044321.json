      "utterance": [
        {
          "id": "MDRW2100044321.1.1",
          "form": "그냥모 넣고 싶은거 넣어봐요ㅋㅋㅋ",
          "original_form": "그냥모 넣고 싶은거 넣어봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044321.1.2",
          "form": "우아 직접 만드시는구나!!",
          "original_form": "우아 직접 만드시는구나!!",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044321.1.3",
          "form": "양배추 쌈에 먹으면 맛나요",
          "original_form": "양배추 쌈에 먹으면 맛나요",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044321.1.4",
          "form": "와 맞아요 양배추쌈 ㅜㅜ 너무 좋아요",
          "original_form": "와 맞아요 양배추쌈 ㅜㅜ 너무 좋아요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044321.1.5",
          "form": "진짜 다른 반찬 필요없을거같아요",
          "original_form": "진짜 다른 반찬 필요없을거같아요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044321.1.6",
          "form": "맛아요 전 밑반찬은 잘안먹고 하나만 파는 스탸일이라ㅋㅋ",
          "original_form": "맛아요 전 밑반찬은 잘안먹고 하나만 파는 스탸일이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044321.1.7",
          "form": "메인메뉴 하나있으면 충분하죠!!!",
          "original_form": "메인메뉴 하나있으면 충분하죠!!!",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044321.1.8",
          "form": "네 맞아요 그래서 한가지만 해서먹어요ㅋㅋ",
          "original_form": "네 맞아요 그래서 한가지만 해서먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044321.1.9",
          "form": "낼 아침 머먹을지 고민이네용!!",
          "original_form": "낼 아침 머먹을지 고민이네용!!",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044321.1.10",
          "form": "전아침은 토스트요",
          "original_form": "전아침은 토스트요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044321.1.11",
          "form": "오오 토스트 !! 안에 뭐 넣으실건가요?,",
          "original_form": "오오 토스트 !! 안에 뭐 넣으실건가요?,",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044321.1.12",
          "form": "계란이랑 양상추 토마토요\n마요네즈뿌려서요",
          "original_form": "계란이랑 양상추 토마토요\n마요네즈뿌려서요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044321.1.13",
          "form": "잇는거로 대충",
          "original_form": "잇는거로 대충",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044321.1.14",
          "form": "우아 맛있을거같아요!!!",
          "original_form": "우아 맛있을거같아요!!!",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044321.1.15",
          "form": "토스트랑 음료도 드실건가요?",
          "original_form": "토스트랑 음료도 드실건가요?",
