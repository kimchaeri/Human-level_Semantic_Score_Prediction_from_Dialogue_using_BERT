      "utterance": [
        {
          "id": "MDRW2100004474.1.1",
          "form": "요새 책 많이 보잖아",
          "original_form": "요새 책 많이 보잖아",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004474.1.2",
          "form": "보려고 노력하지",
          "original_form": "보려고 노력하지",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004474.1.3",
          "form": "진득하게 읽지는 못하고 있어",
          "original_form": "진득하게 읽지는 못하고 있어",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004474.1.4",
          "form": "최근에 본것중 재밌는거 뭐야?",
          "original_form": "최근에 본것중 재밌는거 뭐야?",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004474.1.5",
          "form": "인상깊었던 책 소개해주십셔",
          "original_form": "인상깊었던 책 소개해주십셔",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004474.1.6",
          "form": "이번에 앞으로 올 사랑 빌려왔는데",
          "original_form": "이번에 앞으로 올 사랑 빌려왔는데",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004474.1.7",
          "form": "이것 좋다는 사람이 많대서",
          "original_form": "이것 좋다는 사람이 많대서",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004474.1.8",
          "form": "기대하고 있는중",
          "original_form": "기대하고 있는중",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004474.1.9",
          "form": "청소년 소설이야?",
          "original_form": "청소년 소설이야?",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004474.1.10",
          "form": "아니 정혜윤 작가 에세이?",
          "original_form": "아니 정혜윤 작가 에세이?",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004474.1.11",
          "form": "못 들어봤는데",
          "original_form": "못 들어봤는데",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004474.1.12",
          "form": "읽고 재미있음 알려줘",
          "original_form": "읽고 재미있음 알려줘",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004474.1.13",
          "form": "난 어제 도서관서 빌려온 <유리와 철의 계절> 생각보다 재미있어서 만족",
          "original_form": "난 어제 도서관서 빌려온 <유리와 철의 계절> 생각보다 재미있어서 만족",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004474.1.14",
          "form": "그렇구만~",
          "original_form": "그렇구만~",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004474.1.15",
          "form": "책 선택을 잘했네!",
          "original_form": "책 선택을 잘했네!",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004474.1.16",
          "form": "집근처에 도서관이 있으니 좋아",
          "original_form": "집근처에 도서관이 있으니 좋아",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004474.1.17",
          "form": "이거 신문에서 추천글 보고 산 창비 책인데",
          "original_form": "이거 신문에서 추천글 보고 산 창비 책인데",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004474.1.18",
          "form": "익숙한 동화를 비틀어서 이야기하는 게 재미있네",
          "original_form": "익숙한 동화를 비틀어서 이야기하는 게 재미있네",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004474.1.19",
          "form": "맞아 우리집 근처에 도서관이 있는 건 행운",
          "original_form": "맞아 우리집 근처에 도서관이 있는 건 행운",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004474.1.20",
          "form": "각종 이야기의 공주 둘이 만나서 이야기하면서 서로 위로하고 행동하는 부분이 좋아",
          "original_form": "각종 이야기의 공주 둘이 만나서 이야기하면서 서로 위로하고 행동하는 부분이 좋아",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004474.1.21",
          "form": "오 흥미로운 주제네",
          "original_form": "오 흥미로운 주제네",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004474.1.22",
          "form": "최근에는 업무 관련 책만 너무 많이 본 것 같아",
          "original_form": "최근에는 업무 관련 책만 너무 많이 본 것 같아",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004474.1.23",
          "form": "좀 더 다양한 분야의 책을 봐야지 싶은데",
          "original_form": "좀 더 다양한 분야의 책을 봐야지 싶은데",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004474.1.24",
          "form": "바쁜 시즌 좀 지나면ㅋㅋ",
          "original_form": "바쁜 시즌 좀 지나면ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004474.1.25",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004474.1.26",
          "form": "난 업무관련 책은 잘 안보게되더라고",
          "original_form": "난 업무관련 책은 잘 안보게되더라고",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004474.1.27",
          "form": "책은.. 끌리는 걸로 읽는다",
          "original_form": "책은.. 끌리는 걸로 읽는다",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004474.1.28",
          "form": "수요일에 빌린 책들도 조금 들춰보다 말다 하고 있어",
          "original_form": "수요일에 빌린 책들도 조금 들춰보다 말다 하고 있어",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004474.1.29",
          "form": "다시 휴가 좀 주어졌으면",
          "original_form": "다시 휴가 좀 주어졌으면",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004474.1.30",
          "form": "아 청소년 문학에 관심 있다며ㆍ",
          "original_form": "아 청소년 문학에 관심 있다며ㆍ",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004474.1.31",
          "form": "있다면",
          "original_form": "있다면",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004474.1.32",
          "form": "내 책상 옆에 있는 5번 레인 재미있음",
          "original_form": "내 책상 옆에 있는 5번 레인 재미있음",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004474.1.33",
          "form": "여름 느낌이야",
          "original_form": "여름 느낌이야",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004474.1.34",
          "form": "수영부 초딩의 성장이야기",
          "original_form": "수영부 초딩의 성장이야기",
          "speaker_id": "2",
          "time": "20210821 18:18"
        }
      ]
    }
