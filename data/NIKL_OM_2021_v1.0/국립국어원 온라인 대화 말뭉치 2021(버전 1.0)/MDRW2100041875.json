      "utterance": [
        {
          "id": "MDRW2100041875.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041875.1.2",
          "form": "오늘 저녁머먹을껀가요?",
          "original_form": "오늘 저녁머먹을껀가요?",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041875.1.3",
          "form": "집밥 먹을거같아요",
          "original_form": "집밥 먹을거같아요",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041875.1.4",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041875.1.5",
          "form": "저는 홈플 치킨먹을예정이에요",
          "original_form": "저는 홈플 치킨먹을예정이에요",
          "speaker_id": "1",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041875.1.6",
          "form": "소스는 KFC꺼먹구요",
          "original_form": "소스는 KFC꺼먹구요",
          "speaker_id": "1",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041875.1.7",
          "form": "간식은 뭐좋아하세요??",
          "original_form": "간식은 뭐좋아하세요??",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041875.1.8",
          "form": "간식은 군것질되게잘하는데",
          "original_form": "간식은 군것질되게잘하는데",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041875.1.9",
          "form": "너무잘먹어서",
          "original_form": "너무잘먹어서",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041875.1.10",
          "form": "패스트푸드로할께요ㅋㅋㅋ",
          "original_form": "패스트푸드로할께요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041875.1.11",
          "form": "ㅋㅋ 햄버거가 간식은 아니신거죠? ㅋㅋ",
          "original_form": "ㅋㅋ 햄버거가 간식은 아니신거죠? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041875.1.12",
          "form": "햄버거 간식이에요!!",
          "original_form": "햄버거 간식이에요!!",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041875.1.13",
          "form": "그것까지고는 배가 안부르죠",
          "original_form": "그것까지고는 배가 안부르죠",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041875.1.14",
          "form": "저는 먹방찍는사람이 부럽더라구요",
          "original_form": "저는 먹방찍는사람이 부럽더라구요",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041875.1.15",
          "form": "ㅋㅋ 세트도요? ㅋㅋ",
          "original_form": "ㅋㅋ 세트도요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041875.1.16",
          "form": "세트 간단하게 먹어요",
          "original_form": "세트 간단하게 먹어요",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041875.1.17",
          "form": "ㅋㅋ 맞아요! 뷔페가면 늘  아쉽",
          "original_form": "ㅋㅋ 맞아요! 뷔페가면 늘  아쉽",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041875.1.18",
          "form": "그니깐요ㅜㅜ",
          "original_form": "그니깐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041875.1.19",
          "form": "뷔펴에있는거 다 먹고싶은데",
          "original_form": "뷔펴에있는거 다 먹고싶은데",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041875.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041875.1.21",
          "form": "그냥 막상 단품시키면 아쉽고요!",
          "original_form": "그냥 막상 단품시키면 아쉽고요!",
          "speaker_id": "2",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041875.1.22",
          "form": "양이 너무적어요ㅜㅜ",
          "original_form": "양이 너무적어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041875.1.23",
          "form": "양이 너무적어요ㅜㅜ",
