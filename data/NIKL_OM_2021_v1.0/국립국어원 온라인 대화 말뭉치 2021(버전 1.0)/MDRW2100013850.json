      "utterance": [
        {
          "id": "MDRW2100013850.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013850.1.2",
          "form": "여행 다니는거 좋아해요! 코로나 땜에 지금은ㅁ살짝 힘들지만",
          "original_form": "여행 다니는거 좋아해요! 코로나 땜에 지금은ㅁ살짝 힘들지만",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013850.1.3",
          "form": "저도요 아쉬워요 ㅜ",
          "original_form": "저도요 아쉬워요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013850.1.4",
          "form": "어디 다녀오셨어요?",
          "original_form": "어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013850.1.5",
          "form": "저는 스페인에 다녀온게 가장 최근의 해외여행이었어요",
          "original_form": "저는 스페인에 다녀온게 가장 최근의 해외여행이었어요",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013850.1.6",
          "form": "우와 거기 좋나요?",
          "original_form": "우와 거기 좋나요?",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013850.1.7",
          "form": "음식이 맛있고 풍경이 예쁘더라구요!",
          "original_form": "음식이 맛있고 풍경이 예쁘더라구요!",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013850.1.8",
          "form": "뭐 드셨나요",
          "original_form": "뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013850.1.9",
          "form": "개인적으로 기억에남았던건 감바스 새우요리였어요",
          "original_form": "개인적으로 기억에남았던건 감바스 새우요리였어요",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013850.1.10",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013850.1.11",
          "form": "헐 감바스",
          "original_form": "헐 감바스",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013850.1.12",
          "form": "스페인서 먹음 대박이겠네요",
          "original_form": "스페인서 먹음 대박이겠네요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013850.1.13",
          "form": "네 가고싶어요",
          "original_form": "네 가고싶어요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013850.1.14",
          "form": "얼른 코로나만 끝나면 진짜 여행가고싶어요",
          "original_form": "얼른 코로나만 끝나면 진짜 여행가고싶어요",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013850.1.15",
          "form": "전 제주도도 좋아해요",
          "original_form": "전 제주도도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013850.1.16",
          "form": "언제 가셨어요",
          "original_form": "언제 가셨어요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013850.1.17",
          "form": "제주도는 6개월전에 친구랑 짧게 다녀왔는데 풍경이 너무좋더라구요",
          "original_form": "제주도는 6개월전에 친구랑 짧게 다녀왔는데 풍경이 너무좋더라구요",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013850.1.18",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013850.1.19",
          "form": "얼마안됐네요",
          "original_form": "얼마안됐네요",
          "speaker_id": "1",
          "time": "20210825 16:18"
        }
      ]
