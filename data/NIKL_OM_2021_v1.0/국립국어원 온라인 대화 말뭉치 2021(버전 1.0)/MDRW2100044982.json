      "utterance": [
        {
          "id": "MDRW2100044982.1.1",
          "form": "색이랑 향은 어때?",
          "original_form": "색이랑 향은 어때?",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044982.1.2",
          "form": "살짝 꽃향나는데 색깔은 누리끼리하더라 ㅋㅋㅋ",
          "original_form": "살짝 꽃향나는데 색깔은 누리끼리하더라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044982.1.3",
          "form": "근데 가성비 좋은거같아!",
          "original_form": "근데 가성비 좋은거같아!",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044982.1.4",
          "form": "엥? ㅋㅋㅋ 그래?",
          "original_form": "엥? ㅋㅋㅋ 그래?",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044982.1.5",
          "form": "응 3천원밖에안행",
          "original_form": "응 3천원밖에안행",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044982.1.6",
          "form": "누리끼리면 쓰기 좀 그러겠다...",
          "original_form": "누리끼리면 쓰기 좀 그러겠다...",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044982.1.7",
          "form": "ㅋㅋㅋㅋㅋ맞아 그래도 거품이 되게 잘나서 거품때문에 잘 안보이드라 색은",
          "original_form": "ㅋㅋㅋㅋㅋ맞아 그래도 거품이 되게 잘나서 거품때문에 잘 안보이드라 색은",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044982.1.8",
          "form": "앜ㅋㅋㅋ 다행이네",
          "original_form": "앜ㅋㅋㅋ 다행이네",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044982.1.9",
          "form": "다른 색상은 없었어?",
          "original_form": "다른 색상은 없었어?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044982.1.10",
          "form": "다른색 한 4개정도 더 있던거같아!",
          "original_form": "다른색 한 4개정도 더 있던거같아!",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044982.1.11",
          "form": "오 대략 어떤 색이 있었는지 기억나?",
          "original_form": "오 대략 어떤 색이 있었는지 기억나?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044982.1.12",
          "form": "그건자세히 기억안난다 ㅜ",
          "original_form": "그건자세히 기억안난다 ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044982.1.13",
          "form": "힝.. 아쉽다",
          "original_form": "힝.. 아쉽다",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044982.1.14",
          "form": "그러게 담에 같이 다이소갈때 함뵈보자",
          "original_form": "그러게 담에 같이 다이소갈때 함뵈보자",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044982.1.15",
          "form": "그래!!",
          "original_form": "그래!!",
