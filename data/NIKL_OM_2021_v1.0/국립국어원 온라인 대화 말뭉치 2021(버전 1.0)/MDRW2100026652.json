      "utterance": [
        {
          "id": "MDRW2100026652.1.1",
          "form": "그럼 취업준비중이세요?",
          "original_form": "그럼 취업준비중이세요?",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026652.1.2",
          "form": "저는 네 취준중이에요",
          "original_form": "저는 네 취준중이에요",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026652.1.3",
          "form": "알바랑 병행하고 공부하고있슴니다",
          "original_form": "알바랑 병행하고 공부하고있슴니다",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026652.1.4",
          "form": "우오아",
          "original_form": "우오아",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026652.1.5",
          "form": "저는 상반기에 친구회사에서 일하다",
          "original_form": "저는 상반기에 친구회사에서 일하다",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026652.1.6",
          "form": "지금은 실업급여 받아여",
          "original_form": "지금은 실업급여 받아여",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026652.1.7",
          "form": "아 맞아요 실업급여 좋더라구요",
          "original_form": "아 맞아요 실업급여 좋더라구요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026652.1.8",
          "form": "저는 그 뭐지 국비지원받아서 공부하고 있는데 약간 애매~하더라구요",
          "original_form": "저는 그 뭐지 국비지원받아서 공부하고 있는데 약간 애매~하더라구요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026652.1.9",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026652.1.10",
          "form": "계좌제 인가 무슨 카드 발급받으셨어요?",
          "original_form": "계좌제 인가 무슨 카드 발급받으셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026652.1.11",
          "form": "네그 내일배움카드라고 수업료 지원받는거 있어요",
          "original_form": "네그 내일배움카드라고 수업료 지원받는거 있어요",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026652.1.12",
          "form": "근데 취업 연계해주는건 좋은데 생각보다 수업의 질은 기대하면 안되겠더라구요",
          "original_form": "근데 취업 연계해주는건 좋은데 생각보다 수업의 질은 기대하면 안되겠더라구요",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026652.1.13",
          "form": "저 바리스타 과정 관심있어서 봤었어요",
          "original_form": "저 바리스타 과정 관심있어서 봤었어요",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026652.1.14",
          "form": "세금이 제대로 안쓰여지내요",
          "original_form": "세금이 제대로 안쓰여지내요",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026652.1.15",
          "form": "마자요... 기간은 정해져있고 진도도 너무 빠르고",
          "original_form": "마자요... 기간은 정해져있고 진도도 너무 빠르고",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026652.1.16",
          "form": "졸업전에 취직하는게 유리한테",
          "original_form": "졸업전에 취직하는게 유리한테",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026652.1.17",
          "form": "코시국이라 만만치.않죠 ㅜ",
          "original_form": "코시국이라 만만치.않죠 ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026652.1.18",
          "form": "그렇더라구요...ㅠㅠ",
          "original_form": "그렇더라구요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026652.1.19",
          "form": "스트레스는 어떻게 해소해요 그럼",
          "original_form": "스트레스는 어떻게 해소해요 그럼",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026652.1.20",
          "form": "전 작년에 일 너무 많이해서 번아웃와서",
          "original_form": "전 작년에 일 너무 많이해서 번아웃와서",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026652.1.21",
          "form": "제주살이중인데",
          "original_form": "제주살이중인데",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026652.1.22",
          "form": "요새는 그냥 운동하고 공부하고 반복인거같아요 그냥",
          "original_form": "요새는 그냥 운동하고 공부하고 반복인거같아요 그냥",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026652.1.23",
          "form": "그리고 사진찍는거 좋아해서 돌아다니구 가끔",
          "original_form": "그리고 사진찍는거 좋아해서 돌아다니구 가끔",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026652.1.24",
          "form": "아 맞다 여행동아리 ㅋㅋ",
          "original_form": "아 맞다 여행동아리 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026652.1.25",
          "form": "아르바이트는 어떤거 하세요",
          "original_form": "아르바이트는 어떤거 하세요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        }
      ]
    }
