      "utterance": [
        {
          "id": "MDRW2100003240.1.1",
          "form": "혹시 해외여행 가보신 곳 있으세요?",
          "original_form": "혹시 해외여행 가보신 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003240.1.2",
          "form": "대만 태구규일본이요",
          "original_form": "대만 태구규일본이요",
          "speaker_id": "2",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003240.1.3",
          "form": "태국",
          "original_form": "태국",
          "speaker_id": "2",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003240.1.4",
          "form": "오 거기서 어느나라가 제일 좋으셨어요?",
          "original_form": "오 거기서 어느나라가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003240.1.5",
          "form": "대만이요",
          "original_form": "대만이요",
          "speaker_id": "2",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003240.1.6",
          "form": "대만 어디가보셨어요?",
          "original_form": "대만 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 21:01"
        },
        {
          "id": "MDRW2100003240.1.7",
          "form": "타이페이 근처투어했어요",
          "original_form": "타이페이 근처투어했어요",
          "speaker_id": "2",
          "time": "20210820 21:01"
        },
        {
          "id": "MDRW2100003240.1.8",
          "form": "예스진지 센과치히로 행방불명 모티브된곳 거긴 뭐",
          "original_form": "예스진지 센과치히로 행방불명 모티브된곳 거긴 뭐",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003240.1.9",
          "form": "구경도 못했어요 사람에 치여서",
          "original_form": "구경도 못했어요 사람에 치여서",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003240.1.10",
          "form": "요샌 이름도 생각 안나네요",
          "original_form": "요샌 이름도 생각 안나네요",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003240.1.11",
          "form": "다 오래전 일이네요",
          "original_form": "다 오래전 일이네요",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003240.1.12",
          "form": "작년에 제주도 때만해도 이렇게 심하지 않아서 잘놀다왔는데 올해는 여건이 안되네요",
          "original_form": "작년에 제주도 때만해도 이렇게 심하지 않아서 잘놀다왔는데 올해는 여건이 안되네요",
          "speaker_id": "2",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003240.1.13",
          "form": "요즘엔 코로나때문에 느낌도 잘 안날거같아요",
          "original_form": "요즘엔 코로나때문에 느낌도 잘 안날거같아요",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003240.1.14",
          "form": "태국이랑 일본중엔 어디가 더 좋았어요?",
          "original_form": "태국이랑 일본중엔 어디가 더 좋았어요?",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003240.1.15",
          "form": "태국은 패키리로 가서 그냥 그랬어요",
          "original_form": "태국은 패키리로 가서 그냥 그랬어요",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003240.1.16",
          "form": "일본은 쇼핑만했어요 ㅋㅋ",
          "original_form": "일본은 쇼핑만했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003240.1.17",
          "form": "느낌 안나죠",
          "original_form": "느낌 안나죠",
          "speaker_id": "2",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003240.1.18",
          "form": "대만이 그나마 자유 여행으로 좋았어요",
          "original_form": "대만이 그나마 자유 여행으로 좋았어요",
          "speaker_id": "2",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003240.1.19",
          "form": "중간에 하루를 버스 투어로 15000원주고 끼워 넣었는데 싸고 좋았어요 근데 힘들어요",
          "original_form": "중간에 하루를 버스 투어로 15000원주고 끼워 넣었는데 싸고 좋았어요 근데 힘들어요",
          "speaker_id": "2",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003240.1.20",
          "form": "대만은 위험한건 잘 없었어요?",
          "original_form": "대만은 위험한건 잘 없었어요?",
          "speaker_id": "1",
          "time": "20210820 21:26"
        },
        {
          "id": "MDRW2100003240.1.21",
          "form": "밤에 아이들과 다녀도 괜찮았어요",
          "original_form": "밤에 아이들과 다녀도 괜찮았어요",
          "speaker_id": "2",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003240.1.22",
          "form": "대만가면 사람들은 어땠어요?",
          "original_form": "대만가면 사람들은 어땠어요?",
          "speaker_id": "1",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003240.1.23",
          "form": "친절했어요",
          "original_form": "친절했어요",
          "speaker_id": "2",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003240.1.24",
          "form": "대만이 자유여행가기 좋은 나라고해서 카페가입해서 정보 얻고 갔어요",
          "original_form": "대만이 자유여행가기 좋은 나라고해서 카페가입해서 정보 얻고 갔어요",
          "speaker_id": "2",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003240.1.25",
          "form": "보험 따로들고 특가 비행기권사고해서 갔어요",
          "original_form": "보험 따로들고 특가 비행기권사고해서 갔어요",
          "speaker_id": "2",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003240.1.26",
          "form": "아무래도 아시아쪽이 싸고 편해서 좋은거같아요",
          "original_form": "아무래도 아시아쪽이 싸고 편해서 좋은거같아요",
          "speaker_id": "1",
          "time": "20210820 21:34"
        },
        {
          "id": "MDRW2100003240.1.27",
          "form": "네 싸요 음식도 잘맞았어요",
          "original_form": "네 싸요 음식도 잘맞았어요",
          "speaker_id": "2",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003240.1.28",
          "form": "저희집은 고수나 향신료 거부감이 없어서 음식도 잘맞았어요",
          "original_form": "저희집은 고수나 향신료 거부감이 없어서 음식도 잘맞았어요",
          "speaker_id": "2",
          "time": "20210820 21:35"
        }
      ]
