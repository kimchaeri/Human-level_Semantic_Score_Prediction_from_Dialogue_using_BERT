      "utterance": [
        {
          "id": "MDRW2100019173.1.1",
          "form": "유럽에 가면",
          "original_form": "유럽에 가면",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019173.1.2",
          "form": "젤라또랑 디저트를",
          "original_form": "젤라또랑 디저트를",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019173.1.3",
          "form": "먹고싶어",
          "original_form": "먹고싶어",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019173.1.4",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019173.1.5",
          "form": "니가 행복해하면",
          "original_form": "니가 행복해하면",
          "speaker_id": "1",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019173.1.6",
          "form": "나도좋단다",
          "original_form": "나도좋단다",
          "speaker_id": "1",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019173.1.7",
          "form": "유럽 남부가 좋은거니 그럼?",
          "original_form": "유럽 남부가 좋은거니 그럼?",
          "speaker_id": "1",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019173.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 1일 1젤라또",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 1일 1젤라또",
          "speaker_id": "2",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019173.1.9",
          "form": "남부쪽 먼저 찍고",
          "original_form": "남부쪽 먼저 찍고",
          "speaker_id": "2",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019173.1.10",
          "form": "거기가 디저트뿐만 아니라 다른 음식도 좀 풍부하지 않나?",
          "original_form": "거기가 디저트뿐만 아니라 다른 음식도 좀 풍부하지 않나?",
          "speaker_id": "2",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019173.1.11",
          "form": "ㅋㅋㅋㅋㅋ꿈이 크구만",
          "original_form": "ㅋㅋㅋㅋㅋ꿈이 크구만",
          "speaker_id": "1",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019173.1.12",
          "form": "맞아 남부유럽이 식문화가 대박이지",
          "original_form": "맞아 남부유럽이 식문화가 대박이지",
          "speaker_id": "1",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019173.1.13",
          "form": "와인도 먹어보고싶어",
          "original_form": "와인도 먹어보고싶어",
          "speaker_id": "2",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019173.1.14",
          "form": "아이스와인",
          "original_form": "아이스와인",
          "speaker_id": "2",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019173.1.15",
          "form": "오오 그렇지",
          "original_form": "오오 그렇지",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019173.1.16",
          "form": "일조량높아서 올리브 포도",
          "original_form": "일조량높아서 올리브 포도",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019173.1.17",
          "form": "이런계열 다맛있다잖아",
          "original_form": "이런계열 다맛있다잖아",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019173.1.18",
          "form": "난 유럽 토마토가궁금ㅋㅋㅋ",
          "original_form": "난 유럽 토마토가궁금ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019173.1.19",
          "form": "ㅁㅈㅁㅈ 그런거랑 요리랑 곁들여 먹음 진짜 기분 좋을듯",
          "original_form": "ㅁㅈㅁㅈ 그런거랑 요리랑 곁들여 먹음 진짜 기분 좋을듯",
          "speaker_id": "2",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019173.1.20",
          "form": "토마토가 우리 토마토랑 달라?",
          "original_form": "토마토가 우리 토마토랑 달라?",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019173.1.21",
          "form": "걔네걸로 통조림만들지않나",
          "original_form": "걔네걸로 통조림만들지않나",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019173.1.22",
          "form": "수분감이 좀 없다고들음",
          "original_form": "수분감이 좀 없다고들음",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019173.1.23",
          "form": "그걸 마늘이랑 빵에발라먹는",
          "original_form": "그걸 마늘이랑 빵에발라먹는",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019173.1.24",
          "form": "오...난 토마토는 물 많은게 좋은데",
          "original_form": "오...난 토마토는 물 많은게 좋은데",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019173.1.25",
          "form": "뭐그런음식이 스페인에있을걸",
          "original_form": "뭐그런음식이 스페인에있을걸",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019173.1.26",
          "form": "헐 그건 맛있을거 같다",
          "original_form": "헐 그건 맛있을거 같다",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019173.1.27",
          "form": "존맛이라는 후기가있어",
          "original_form": "존맛이라는 후기가있어",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019173.1.28",
          "form": "햐 진짜가보고싶다",
          "original_form": "햐 진짜가보고싶다",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.29",
          "form": "식도락 짱이야",
          "original_form": "식도락 짱이야",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.30",
          "form": "프랑스는 빵이 존맛인데 겁나싸대",
          "original_form": "프랑스는 빵이 존맛인데 겁나싸대",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.31",
          "form": "영국은 음식은좀그렇고",
          "original_form": "영국은 음식은좀그렇고",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.32",
          "form": "헐 맞아..유럽 갔다온 사람들 하나같이",
          "original_form": "헐 맞아..유럽 갔다온 사람들 하나같이",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.33",
          "form": "남자들이 친절하다네",
          "original_form": "남자들이 친절하다네",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.35",
          "form": "프랑스 빵 극찬하더라",
          "original_form": "프랑스 빵 극찬하더라",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.36",
          "form": "걍 동네 아무빵집 들어가서 먹어도 다 맛있대",
          "original_form": "걍 동네 아무빵집 들어가서 먹어도 다 맛있대",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.37",
          "form": "특히 바게뜨 강추하던데",
          "original_form": "특히 바게뜨 강추하던데",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.38",
          "form": "크으..",
          "original_form": "크으..",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.39",
          "form": "그니까 프랑스갔다온애들은",
          "original_form": "그니까 프랑스갔다온애들은",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019173.1.40",
          "form": "크로와상이 그리고 글케...환상적이랴",
          "original_form": "크로와상이 그리고 글케...환상적이랴",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019173.1.41",
          "form": "빵 한국거 못먹겠다그러고",
          "original_form": "빵 한국거 못먹겠다그러고",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019173.1.42",
          "form": "동남아갔다온애들은",
          "original_form": "동남아갔다온애들은",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019173.1.43",
          "form": "ㅁㅈㅁㅈ 하..궁금하다 대체 얼마나 존맛이길래",
          "original_form": "ㅁㅈㅁㅈ 하..궁금하다 대체 얼마나 존맛이길래",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019173.1.44",
          "form": "열대과일ㅋㅋㅋㅋ",
          "original_form": "열대과일ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019173.1.45",
          "form": "ㅠㅠ 과일 진짜 싸고 원없이 먹는다던데",
          "original_form": "ㅠㅠ 과일 진짜 싸고 원없이 먹는다던데",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019173.1.46",
          "form": "가지고 이맛이아니라고 맨날그러잖아",
          "original_form": "가지고 이맛이아니라고 맨날그러잖아",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019173.1.47",
          "form": "꼭가봅시다ㅜㅜ",
          "original_form": "꼭가봅시다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019173.1.48",
          "form": "대만가서 1일 3망고 해야해",
          "original_form": "대만가서 1일 3망고 해야해",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019173.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋ대체 그놈의망고는",
          "original_form": "ㅋㅋㅋㅋㅋㅋ대체 그놈의망고는",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
