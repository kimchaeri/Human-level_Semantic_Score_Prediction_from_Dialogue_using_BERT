      "utterance": [
        {
          "id": "MDRW2100046034.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ코기는 아주 쬐금만 넣어야 해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ코기는 아주 쬐금만 넣어야 해요",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046034.1.2",
          "form": "야채향이 고기에 묻혀버린다구여",
          "original_form": "야채향이 고기에 묻혀버린다구여",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100046034.1.3",
          "form": "name1님 오늘 아침식사 제가 맞췄나요?",
          "original_form": "&name1&님 오늘 아침식사 제가 맞췄나요?",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046034.1.4",
          "form": "아녀!!",
          "original_form": "아녀!!",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046034.1.5",
          "form": "혹시 아침 거르셨나요",
          "original_form": "혹시 아침 거르셨나요",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046034.1.6",
          "form": "왜냐면 전 아침을 먹지못해ㅛ어요",
          "original_form": "왜냐면 전 아침을 먹지못해ㅛ어요",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046034.1.7",
          "form": "네네..",
          "original_form": "네네..",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046034.1.8",
          "form": "그럼 점심을 맞춰주세요!",
          "original_form": "그럼 점심을 맞춰주세요!",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046034.1.9",
          "form": "아이구 저런...",
          "original_form": "아이구 저런...",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046034.1.10",
          "form": "점심 끝내주는 걸로 드셔요",
          "original_form": "점심 끝내주는 걸로 드셔요",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046034.1.11",
          "form": "고기 드세요! 고기 드시나요?",
          "original_form": "고기 드세요! 고기 드시나요?",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046034.1.12",
          "form": "음 고기가 들어거긴해요!!",
          "original_form": "음 고기가 들어거긴해요!!",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046034.1.13",
          "form": "앗 고기가 메인이 아닌가요?",
          "original_form": "앗 고기가 메인이 아닌가요?",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046034.1.14",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046034.1.15",
          "form": "이럴수가 아침도 거르셨는데ㅠㅜ든든히 드시지",
          "original_form": "이럴수가 아침도 거르셨는데ㅠㅜ든든히 드시지",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046034.1.16",
          "form": "면이 들어가나요?",
          "original_form": "면이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046034.1.17",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046034.1.18",
          "form": "오늘은 이미 친구와 약속한 점심이어서",
          "original_form": "오늘은 이미 친구와 약속한 점심이어서",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046034.1.19",
          "form": "저번주부터 정해져있던 메뉴였어요 ㅋㅋ",
          "original_form": "저번주부터 정해져있던 메뉴였어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046034.1.20",
          "form": "그럼 쌀 요리인가요!",
          "original_form": "그럼 쌀 요리인가요!",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046034.1.21",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046034.1.22",
          "form": "저도 이렇게 접근했어야했는제!",
          "original_form": "저도 이렇게 접근했어야했는제!",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046034.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ근데 지금 다 아니래서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ근데 지금 다 아니래서",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046034.1.24",
          "form": "약간 시무룩해요",
          "original_form": "약간 시무룩해요",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046034.1.25",
          "form": "내 돗자리 너덜너덜하다",
          "original_form": "내 돗자리 너덜너덜하다",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046034.1.26",
          "form": "엉엉엉",
          "original_form": "엉엉엉",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046034.1.27",
          "form": "너덜너덜",
          "original_form": "너덜너덜",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046034.1.28",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㄱㅋㅋㄱㄱㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㄱㅋㅋㄱㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046034.1.29",
          "form": "돗자리 새로 사오세요..",
          "original_form": "돗자리 새로 사오세요..",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046034.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ눼 사올게요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ눼 사올게요",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046034.1.31",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046034.1.32",
          "form": "밥도 면도 아니면!!!",
          "original_form": "밥도 면도 아니면!!!",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046034.1.33",
          "form": "답은 심플해요",
          "original_form": "답은 심플해요",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046034.1.34",
