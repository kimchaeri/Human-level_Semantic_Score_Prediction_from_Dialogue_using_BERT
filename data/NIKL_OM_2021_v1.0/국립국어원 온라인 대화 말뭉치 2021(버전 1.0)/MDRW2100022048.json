      "utterance": [
        {
          "id": "MDRW2100022048.1.1",
          "form": "첫번짼  유아교육학과 갔는데 ㅋ",
          "original_form": "첫번짼  유아교육학과 갔는데 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022048.1.2",
          "form": "ㅋㅋㅋ뭐가요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ뭐가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022048.1.3",
          "form": "뭔가 저랑 안맞아서요 ㅋ",
          "original_form": "뭔가 저랑 안맞아서요 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022048.1.4",
          "form": "실기평가  교육학 연대 외우는거보고ㅠ뭐야 고등학교야ㅠㅋㅋㅋ",
          "original_form": "실기평가  교육학 연대 외우는거보고ㅠ뭐야 고등학교야ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100022048.1.5",
          "form": "열심히 공부해서 왓더니 대학교에서도 외우기라니 ㅋㅋ",
          "original_form": "열심히 공부해서 왓더니 대학교에서도 외우기라니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022048.1.6",
          "form": "에요ㅎ",
          "original_form": "에요ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022048.1.7",
          "form": "다 외울꺼에요",
          "original_form": "다 외울꺼에요",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022048.1.8",
          "form": "저 로망이 좀 컷어요",
          "original_form": "저 로망이 좀 컷어요",
          "speaker_id": "1",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022048.1.9",
          "form": "저희도 대학교때 맨날외움ㅠ",
          "original_form": "저희도 대학교때 맨날외움ㅠ",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022048.1.10",
          "form": "외우는건 문제가 아닌데 심지어 하나도 안틀렸죠 ㅋㅋ",
          "original_form": "외우는건 문제가 아닌데 심지어 하나도 안틀렸죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022048.1.11",
          "form": "대학가면  열정적인 토론과 자유로움 ㅋㅋㅋ",
          "original_form": "대학가면  열정적인 토론과 자유로움 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100022048.1.12",
          "form": "은 없엇다 ㅋ",
          "original_form": "은 없엇다 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100022048.1.13",
          "form": "그럼 공부도 잘하셨는데",
          "original_form": "그럼 공부도 잘하셨는데",
          "speaker_id": "2",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100022048.1.14",
          "form": "없었죠ㅠ",
          "original_form": "없었죠ㅠ",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.15",
          "form": "진짜 없었죠ㅠ 대학교ㅠ",
          "original_form": "진짜 없었죠ㅠ 대학교ㅠ",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.16",
          "form": "그래서 아 내길이 아닌거 같앙서",
          "original_form": "그래서 아 내길이 아닌거 같앙서",
          "speaker_id": "1",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.17",
          "form": "왜요 그래도 남자친구분있으셨으니깐ㅎ",
          "original_form": "왜요 그래도 남자친구분있으셨으니깐ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.18",
          "form": "자유로웠잖아요ㅎ",
          "original_form": "자유로웠잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.19",
          "form": "아 그거랑 별개 ㅎㅎ",
          "original_form": "아 그거랑 별개 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.20",
          "form": "다른 대학인가요?ㅎㅎ",
          "original_form": "다른 대학인가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.21",
          "form": "그럼,다음엔 어딜 가셨어요?",
          "original_form": "그럼,다음엔 어딜 가셨어요?",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.22",
          "form": "아뇨 남친은 남친이규 대락은 대학 ㅋ",
          "original_form": "아뇨 남친은 남친이규 대락은 대학 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100022048.1.23",
          "form": "글긴해요ㅎ 나에게 맞아야하는거죠ㅎ",
          "original_form": "글긴해요ㅎ 나에게 맞아야하는거죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100022048.1.24",
          "form": "사복과요 근데 일하다가  학교 들어가니",
          "original_form": "사복과요 근데 일하다가  학교 들어가니",
          "speaker_id": "1",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100022048.1.25",
          "form": "왜 더 일이 하고 싶던지 후훕 ㅋ",
          "original_form": "왜 더 일이 하고 싶던지 후훕 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100022048.1.26",
          "form": "사회복지학과ㅎ",
          "original_form": "사회복지학과ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100022048.1.27",
          "form": "어쩌다보니 일하고 잇더라구요 ㅎㅎ",
          "original_form": "어쩌다보니 일하고 잇더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100022048.1.28",
          "form": "그럴수있져ㅎㅎ",
          "original_form": "그럴수있져ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100022048.1.29",
          "form": "일하는건 그럼 어떤일해요?",
          "original_form": "일하는건 그럼 어떤일해요?",
          "speaker_id": "2",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100022048.1.30",
          "form": "학교 전공도 아닐껀데ㅎㅎ",
          "original_form": "학교 전공도 아닐껀데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100022048.1.31",
          "form": "지금은 그냥 사무직요 ㅎ",
          "original_form": "지금은 그냥 사무직요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100022048.1.32",
          "form": "아아ㅎ 좋네요ㅎ",
          "original_form": "아아ㅎ 좋네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100022048.1.33",
          "form": "현장에서 사무직온 독특한 케이스죠",
          "original_form": "현장에서 사무직온 독특한 케이스죠",
          "speaker_id": "1",
          "time": "20210829 01:34"
        }
      ]
