      "utterance": [
        {
          "id": "MDRW2100040863.1.1",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210905 10:21"
        },
        {
          "id": "MDRW2100040863.1.2",
          "form": "회를 시켜 먹으면 맛있어요?",
          "original_form": "회를 시켜 먹으면 맛있어요?",
          "speaker_id": "1",
          "time": "20210905 10:22"
        },
        {
          "id": "MDRW2100040863.1.3",
          "form": "네 전 회를 정말 좋아하거든요",
          "original_form": "네 전 회를 정말 좋아하거든요",
          "speaker_id": "2",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100040863.1.4",
          "form": "횟집을 혼자가기에 부담이라 배달로 시켜먹기도 하는것같아요 ㅎㅎ",
          "original_form": "횟집을 혼자가기에 부담이라 배달로 시켜먹기도 하는것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100040863.1.5",
          "form": "네 저도 좋아하긴 하는데",
          "original_form": "네 저도 좋아하긴 하는데",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100040863.1.6",
          "form": "혼자서 시켜 먹어 본 적이 없었어요",
          "original_form": "혼자서 시켜 먹어 본 적이 없었어요",
          "speaker_id": "1",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100040863.1.7",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100040863.1.8",
          "form": "근데 항상 양이 너무 적어서 부족한것같아요 ㅠㅠ",
          "original_form": "근데 항상 양이 너무 적어서 부족한것같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100040863.1.9",
          "form": "비싸면서 양도적더라구요",
          "original_form": "비싸면서 양도적더라구요",
          "speaker_id": "2",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100040863.1.10",
          "form": "네 ㅠㅠ 회가 그렇죠 ㅠㅠㅠ",
          "original_form": "네 ㅠㅠ 회가 그렇죠 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100040863.1.11",
          "form": "배달도 그렇군요...",
          "original_form": "배달도 그렇군요...",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100040863.1.12",
          "form": "네 그래요 ㅠ 진짜 회는비싸고 양이적죠",
          "original_form": "네 그래요 ㅠ 진짜 회는비싸고 양이적죠",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100040863.1.13",
          "form": "수산시장에서 사 오는게 제일 싸긴 한데요...",
          "original_form": "수산시장에서 사 오는게 제일 싸긴 한데요...",
          "speaker_id": "1",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100040863.1.14",
          "form": "맞아요 근데.저는 근처에 없어서 아쉽네요",
          "original_form": "맞아요 근데.저는 근처에 없어서 아쉽네요",
          "speaker_id": "2",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100040863.1.15",
          "form": "근처에 수산시장이 있나요?",
          "original_form": "근처에 수산시장이 있나요?",
          "speaker_id": "2",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100040863.1.16",
          "form": "네 저는 마산살아서요",
          "original_form": "네 저는 마산살아서요",
          "speaker_id": "1",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100040863.1.17",
          "form": "근처에 있어요",
          "original_form": "근처에 있어요",
          "speaker_id": "1",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100040863.1.18",
          "form": "와 그러시군요",
          "original_form": "와 그러시군요",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040863.1.19",
          "form": "좋은곳에 사시네야",
          "original_form": "좋은곳에 사시네야",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040863.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040863.1.21",
          "form": "회가 먹고싶네요",
          "original_form": "회가 먹고싶네요",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040863.1.22",
          "form": "저녁은 뭐 드실건가요?",
          "original_form": "저녁은 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040863.1.23",
          "form": "옥수수 먹을거같아요",
          "original_form": "옥수수 먹을거같아요",
          "speaker_id": "1",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040863.1.24",
          "form": "이웃분이 방금 주셨어요",
          "original_form": "이웃분이 방금 주셨어요",
          "speaker_id": "1",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100040863.1.25",
          "form": "옥수수 맛있죠~",
          "original_form": "옥수수 맛있죠~",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100040863.1.26",
          "form": "이웃분이 너무 친절하시네요 ㅎㅎ",
          "original_form": "이웃분이 너무 친절하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100040863.1.27",
          "form": "정답네요",
          "original_form": "정답네요",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100040863.1.28",
          "form": "맛있게먹었어요 ㅎㅎㅎ",
          "original_form": "맛있게먹었어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100040863.1.29",
          "form": "맛있더라구요",
