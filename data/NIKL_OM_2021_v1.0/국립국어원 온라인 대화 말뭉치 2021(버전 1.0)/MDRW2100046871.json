      "utterance": [
        {
          "id": "MDRW2100046871.1.1",
          "form": "오징어게임 재미있다고 하던데",
          "original_form": "오징어게임 재미있다고 하던데",
          "speaker_id": "1",
          "time": "20211108 09:35"
        },
        {
          "id": "MDRW2100046871.1.2",
          "form": "혹시 보셨나요!",
          "original_form": "혹시 보셨나요!",
          "speaker_id": "1",
          "time": "20211108 09:35"
        },
        {
          "id": "MDRW2100046871.1.3",
          "form": "네! 저는 다 보았습니다 ㅎㅎ",
          "original_form": "네! 저는 다 보았습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046871.1.4",
          "form": "명성대로 재밌더라구요",
          "original_form": "명성대로 재밌더라구요",
          "speaker_id": "2",
          "time": "20211108 09:38"
        },
        {
          "id": "MDRW2100046871.1.5",
          "form": "저도 이번주말에 한번 봐야겠어요",
          "original_form": "저도 이번주말에 한번 봐야겠어요",
          "speaker_id": "1",
          "time": "20211108 09:55"
        },
        {
          "id": "MDRW2100046871.1.6",
          "form": "오징어게임은 어디에서 볼수있어요?",
          "original_form": "오징어게임은 어디에서 볼수있어요?",
          "speaker_id": "1",
          "time": "20211108 09:55"
        },
        {
          "id": "MDRW2100046871.1.7",
          "form": "넷플릭스에서 전편 다 볼 수 있습니다!",
          "original_form": "넷플릭스에서 전편 다 볼 수 있습니다!",
          "speaker_id": "2",
          "time": "20211108 09:56"
        },
        {
          "id": "MDRW2100046871.1.8",
          "form": "넷플릭스에서 볼수있군요 감사합니가",
          "original_form": "넷플릭스에서 볼수있군요 감사합니가",
          "speaker_id": "1",
          "time": "20211108 09:58"
        },
        {
          "id": "MDRW2100046871.1.9",
          "form": "다",
          "original_form": "다",
          "speaker_id": "1",
          "time": "20211108 09:58"
        },
        {
          "id": "MDRW2100046871.1.10",
          "form": "오징어게임 말고 다른 재미있는것도 있을까요?",
          "original_form": "오징어게임 말고 다른 재미있는것도 있을까요?",
          "speaker_id": "1",
          "time": "20211108 09:59"
        },
        {
          "id": "MDRW2100046871.1.11",
          "form": "유튜브에서도 요약된거 있으니까 찾으면 볼 수 있을겁니다!",
          "original_form": "유튜브에서도 요약된거 있으니까 찾으면 볼 수 있을겁니다!",
          "speaker_id": "2",
          "time": "20211108 09:59"
        },
        {
          "id": "MDRW2100046871.1.12",
          "form": "음.. 넷플릭스는 다 재미있는 콘텐츠 뿐이라..",
          "original_form": "음.. 넷플릭스는 다 재미있는 콘텐츠 뿐이라..",
          "speaker_id": "2",
          "time": "20211108 09:59"
        },
        {
          "id": "MDRW2100046871.1.13",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20211108 09:59"
        },
        {
          "id": "MDRW2100046871.1.14",
          "form": "판타지?",
          "original_form": "판타지?",
          "speaker_id": "1",
          "time": "20211108 09:59"
        },
        {
          "id": "MDRW2100046871.1.15",
          "form": "같은거 좋아해요",
          "original_form": "같은거 좋아해요",
          "speaker_id": "1",
          "time": "20211108 10:00"
        },
        {
          "id": "MDRW2100046871.1.16",
          "form": "판타지라... 제가 판타지를 많이 안봐서 추천드리기가 애매하네요..",
          "original_form": "판타지라... 제가 판타지를 많이 안봐서 추천드리기가 애매하네요..",
          "speaker_id": "2",
          "time": "20211108 10:01"
        },
        {
          "id": "MDRW2100046871.1.17",
          "form": "넵 ㅋㅋ",
          "original_form": "넵 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211108 10:02"
        },
        {
          "id": "MDRW2100046871.1.18",
          "form": "요즘 오징어게임 열풍으로",
          "original_form": "요즘 오징어게임 열풍으로",
          "speaker_id": "1",
          "time": "20211108 10:02"
        },
        {
          "id": "MDRW2100046871.1.19",
          "form": "달고나도",
          "original_form": "달고나도",
          "speaker_id": "1",
          "time": "20211108 10:02"
        },
        {
          "id": "MDRW2100046871.1.20",
          "form": "인기많아졌다고하던데",
          "original_form": "인기많아졌다고하던데",
          "speaker_id": "1",
          "time": "20211108 10:02"
        },
        {
          "id": "MDRW2100046871.1.21",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "1",
          "time": "20211108 10:02"
        },
        {
          "id": "MDRW2100046871.1.22",
          "form": "달고나는 어릴 때 많이 먹어보고",
          "original_form": "달고나는 어릴 때 많이 먹어보고",
          "speaker_id": "2",
          "time": "20211108 10:03"
        },
        {
          "id": "MDRW2100046871.1.23",
          "form": "요즘은 너무 달아서 잘 안먹어요",
          "original_form": "요즘은 너무 달아서 잘 안먹어요",
          "speaker_id": "2",
          "time": "20211108 10:03"
        },
        {
          "id": "MDRW2100046871.1.24",
          "form": "오징어게임 열풍으로 달고나 요즘 비싸게 팔리니",
          "original_form": "오징어게임 열풍으로 달고나 요즘 비싸게 팔리니",
          "speaker_id": "1",
          "time": "20211108 10:03"
        },
        {
          "id": "MDRW2100046871.1.25",
          "form": "ㅋㅋ달고나는 집에서 국자 좀 태우면서 만들어 먹어야 제맛이죠!",
          "original_form": "ㅋㅋ달고나는 집에서 국자 좀 태우면서 만들어 먹어야 제맛이죠!",
          "speaker_id": "2",
          "time": "20211108 10:04"
        }
      ]
    }
  ]
