      "utterance": [
        {
          "id": "MDRW2100033904.1.1",
          "form": "이제 밥먹으려구ㅋㅋ",
          "original_form": "이제 밥먹으려구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100033904.1.2",
          "form": "또 저녁시간이 다가옴",
          "original_form": "또 저녁시간이 다가옴",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100033904.1.3",
          "form": "그러니깐 ㅋㅋ첫째 시댁에 있었는데 데리고왔다.",
          "original_form": "그러니깐 ㅋㅋ첫째 시댁에 있었는데 데리고왔다.",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100033904.1.4",
          "form": "피부는 어때 괜찮아졌어?",
          "original_form": "피부는 어때 괜찮아졌어?",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100033904.1.5",
          "form": "아닝 ㅠ 내일 병원가보려고",
          "original_form": "아닝 ㅠ 내일 병원가보려고",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100033904.1.6",
          "form": "아 둘째안잔다 진짜 화난다",
          "original_form": "아 둘째안잔다 진짜 화난다",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100033904.1.7",
          "form": "여기도ㅠㅠ",
          "original_form": "여기도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100033904.1.8",
          "form": "근데 채원이도 버짐같이",
          "original_form": "근데 채원이도 버짐같이",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100033904.1.9",
          "form": "건조하니까 피던데ㄱ",
          "original_form": "건조하니까 피던데ㄱ",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100033904.1.10",
          "form": "그거랑은 다른가",
          "original_form": "그거랑은 다른가",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100033904.1.11",
          "form": "눈 옆있자나 코사이에 거기가",
          "original_form": "눈 옆있자나 코사이에 거기가",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100033904.1.12",
          "form": "눈도 막 만지고 건조하니 하얗게일어나던디",
          "original_form": "눈도 막 만지고 건조하니 하얗게일어나던디",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100033904.1.13",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210826 07:29"
        },
        {
          "id": "MDRW2100033904.1.14",
          "form": "알러지야먹고 약바르니 좀 낫다.",
          "original_form": "알러지야먹고 약바르니 좀 낫다.",
          "speaker_id": "2",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100033904.1.15",
          "form": "병원을 갔다가 첫째를 보낼지, 아니면은 첫째 보내놓고 병원을 다녀올지가 고민이다.",
          "original_form": "병원을 갔다가 첫째를 보낼지, 아니면은 첫째 보내놓고 병원을 다녀올지가 고민이다.",
          "speaker_id": "2",
          "time": "20210826 07:57"
        },
        {
          "id": "MDRW2100033904.1.16",
          "form": "학교가기전에 갔다오는게 낮지 않나ㅜㅜ하필 개학날ㅠㅠ",
          "original_form": "학교가기전에 갔다오는게 낮지 않나ㅜㅜ하필 개학날ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100033904.1.17",
          "form": "응응 그러니깐ㅜㅋㅋ",
          "original_form": "응응 그러니깐ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100033904.1.18",
          "form": "아따 디다",
          "original_form": "아따 디다",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100033904.1.19",
          "form": "응응 ㅋㅋ 고생했다ㅋ",
          "original_form": "응응 ㅋㅋ 고생했다ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100033904.1.20",
          "form": "지금 쉬나ㅋ",
          "original_form": "지금 쉬나ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100033904.1.21",
          "form": "밥먹고 커피 먹고 쉬는중",
          "original_form": "밥먹고 커피 먹고 쉬는중",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100033904.1.22",
          "form": "좋구만ㅋㅋ",
          "original_form": "좋구만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100033904.1.23",
          "form": "나도 커피한잔 하고 쉰다ㅋㅋ",
          "original_form": "나도 커피한잔 하고 쉰다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:40"
        }
      ]
