        {
          "id": "MDRW2100024926.1.1",
          "form": "신상?",
          "original_form": "신상?",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024926.1.2",
          "form": "뭐어떤음식?",
          "original_form": "뭐어떤음식?",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024926.1.3",
          "form": "이번에스벅음료ㅋㄱ새로나온거",
          "original_form": "이번에스벅음료ㅋㄱ새로나온거",
          "speaker_id": "1",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024926.1.4",
          "form": "궁금해",
          "original_form": "궁금해",
          "speaker_id": "1",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024926.1.5",
          "form": "오ㅋㅋ어떤거야?",
          "original_form": "오ㅋㅋ어떤거야?",
          "speaker_id": "2",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100024926.1.6",
          "form": "시즌음로인가?",
          "original_form": "시즌음로인가?",
          "speaker_id": "2",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100024926.1.7",
          "form": "얼마전미니랑콜라보하던데 그거야?",
          "original_form": "얼마전미니랑콜라보하던데 그거야?",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024926.1.8",
          "form": "그건끝났음",
          "original_form": "그건끝났음",
          "speaker_id": "1",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024926.1.9",
          "form": "이번엔파인애플이야",
          "original_form": "이번엔파인애플이야",
          "speaker_id": "1",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024926.1.10",
          "form": "파인애플쥬스?ㅋㅋㅋ",
          "original_form": "파인애플쥬스?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024926.1.11",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024926.1.12",
          "form": "기대됨ㅋ",
          "original_form": "기대됨ㅋ",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024926.1.13",
          "form": "파인애플푸라프치노ㅋㅋ",
          "original_form": "파인애플푸라프치노ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024926.1.14",
          "form": "ㅋㅋ블랙티함유래",
          "original_form": "ㅋㅋ블랙티함유래",
          "speaker_id": "1",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024926.1.15",
          "form": "카페인이좀있겠군ㅋㅋ",
          "original_form": "카페인이좀있겠군ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024926.1.16",
          "form": "아침에마셔ㅋㅋ",
          "original_form": "아침에마셔ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024926.1.17",
          "form": "오후에는마시면잠못잘듯ㅋㅋ",
          "original_form": "오후에는마시면잠못잘듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024926.1.18",
          "form": "응ㅋㅋㅋ나도한번마셔봐야겠어ㅋㅋㅋ",
          "original_form": "응ㅋㅋㅋ나도한번마셔봐야겠어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024926.1.19",
          "form": "기프트콘있거든",
          "original_form": "기프트콘있거든",
          "speaker_id": "2",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024926.1.20",
          "form": "상큼하면좋겠다",
          "original_form": "상큼하면좋겠다",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024926.1.21",
          "form": "기대되ㅋㅋ",
          "original_form": "기대되ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024926.1.22",
          "form": "스벅음료 대부분 맛있어",
          "original_form": "스벅음료 대부분 맛있어",
          "speaker_id": "2",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024926.1.23",
          "form": "기대된다ㅋㅋ",
