      "utterance": [
        {
          "id": "MDRW2100008219.1.1",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008219.1.2",
          "form": "과자랑 간식을 잔뜩먹었어요",
          "original_form": "과자랑 간식을 잔뜩먹었어요",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008219.1.3",
          "form": "삼각김밥도 샀고요 ㅋㅋ",
          "original_form": "삼각김밥도 샀고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008219.1.4",
          "form": "전 아직인데 삼각김밥은 무슨맛이요?",
          "original_form": "전 아직인데 삼각김밥은 무슨맛이요?",
          "speaker_id": "1",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008219.1.5",
          "form": "두개 붙어있는데 매콤 간장 불고기 세트요 ㅋㅋ",
          "original_form": "두개 붙어있는데 매콤 간장 불고기 세트요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008219.1.6",
          "form": "불고기  맛있겠어요",
          "original_form": "불고기  맛있겠어요",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008219.1.7",
          "form": "전주비빔불고기? 그것도 맛있던데",
          "original_form": "전주비빔불고기? 그것도 맛있던데",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008219.1.8",
          "form": "근데 생각보다 가격이 많이 올랐더라고요 ㅋㅋ",
          "original_form": "근데 생각보다 가격이 많이 올랐더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008219.1.9",
          "form": "얼마해요? 삼김은 잘안사먹어서ㅎ",
          "original_form": "얼마해요? 삼김은 잘안사먹어서ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100008219.1.10",
          "form": "제 추억속 삼김은 700원이었는데",
          "original_form": "제 추억속 삼김은 700원이었는데",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008219.1.11",
          "form": "요샌 1100  1200  1500  1800다양하더라고요ㅋㅋㄱ",
          "original_form": "요샌 1100  1200  1500  1800다양하더라고요ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008219.1.12",
          "form": "그래도 비싸!ㅋㅋㅋ",
          "original_form": "그래도 비싸!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008219.1.13",
          "form": "오 비싸네요 큰거라 그런가봐요",
          "original_form": "오 비싸네요 큰거라 그런가봐요",
          "speaker_id": "1",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008219.1.14",
          "form": "재료가 그래도 예전보단 많이들어가있죠?",
          "original_form": "재료가 그래도 예전보단 많이들어가있죠?",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008219.1.15",
          "form": "음 별차이 못느꼈어요 사실",
          "original_form": "음 별차이 못느꼈어요 사실",
          "speaker_id": "2",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008219.1.16",
          "form": "단품 안사고 세트사서 1900원짜리인데",
          "original_form": "단품 안사고 세트사서 1900원짜리인데",
          "speaker_id": "2",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008219.1.17",
          "form": "쪼끔 싸네여 단가가 ㅋㅋ",
          "original_form": "쪼끔 싸네여 단가가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008219.1.18",
          "form": "요즘 편의점 도시락도 잘나오나보던데요",
          "original_form": "요즘 편의점 도시락도 잘나오나보던데요",
          "speaker_id": "1",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008219.1.19",
          "form": "뭔가 그 맛이나요 공장의맛?",
          "original_form": "뭔가 그 맛이나요 공장의맛?",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008219.1.20",
          "form": "보존재의맛?",
          "original_form": "보존재의맛?",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008219.1.21",
          "form": "뭔지알것같아요ㅎ",
          "original_form": "뭔지알것같아요ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008219.1.22",
          "form": "밥은 집밥이 최고죠",
          "original_form": "밥은 집밥이 최고죠",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008219.1.23",
          "form": "그러게요 월욜부터는 진짜 건강 메뉴로 도시락 싸 다니려고요",
          "original_form": "그러게요 월욜부터는 진짜 건강 메뉴로 도시락 싸 다니려고요",
          "speaker_id": "2",
          "time": "20210822 14:37"
        }
      ]
    }
