      "utterance": [
        {
          "id": "MDRW2100031856.1.1",
          "form": "입시가끊이질않는사회네요",
          "original_form": "입시가끊이질않는사회네요",
          "speaker_id": "2",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.2",
          "form": "공부는평생하는거네요",
          "original_form": "공부는평생하는거네요",
          "speaker_id": "2",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.3",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.4",
          "form": "그래서 피트를 오랫동안 했는데",
          "original_form": "그래서 피트를 오랫동안 했는데",
          "speaker_id": "1",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.5",
          "form": "헐네네",
          "original_form": "헐네네",
          "speaker_id": "2",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.6",
          "form": "이건 또 과목이 4개인데",
          "original_form": "이건 또 과목이 4개인데",
          "speaker_id": "1",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.8",
          "form": "시험 칠 때마다",
          "original_form": "시험 칠 때마다",
          "speaker_id": "1",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.9",
          "form": "상대평가겠죠",
          "original_form": "상대평가겠죠",
          "speaker_id": "2",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.10",
          "form": "잘 본 과목이랑 못 본 과목이",
          "original_form": "잘 본 과목이랑 못 본 과목이",
          "speaker_id": "1",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.11",
          "form": "달라져서",
          "original_form": "달라져서",
          "speaker_id": "1",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.12",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "1",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.13",
          "form": "헐 ㅠ",
          "original_form": "헐 ㅠ",
          "speaker_id": "2",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.14",
          "form": "결국 때려치우고",
          "original_form": "결국 때려치우고",
          "speaker_id": "1",
          "time": "20210905 03:06"
        },
        {
          "id": "MDRW2100031856.1.15",
          "form": "뒤늦게 대학원 온거예요ㅠ",
          "original_form": "뒤늦게 대학원 온거예요ㅠ",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.16",
          "form": "헉 ㅠㅠㅠ",
          "original_form": "헉 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.17",
          "form": "근데너무멋지시네요",
          "original_form": "근데너무멋지시네요",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.18",
          "form": "그래도 피트 공부한거 덕분에",
          "original_form": "그래도 피트 공부한거 덕분에",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.19",
          "form": "일반편입은 성공했어요!!",
          "original_form": "일반편입은 성공했어요!!",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.20",
          "form": "오아아아앙",
          "original_form": "오아아아앙",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.21",
          "form": "너무멋져요ㅠㅠ",
          "original_form": "너무멋져요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.22",
          "form": "피트 아니었으면 예전대학에서 머물렀을텐데",
          "original_form": "피트 아니었으면 예전대학에서 머물렀을텐데",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.23",
          "form": "그래도 남는 장사예요 흐흐",
          "original_form": "그래도 남는 장사예요 흐흐",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.24",
          "form": "크으....",
          "original_form": "크으....",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.25",
          "form": "긍정적이신모습",
          "original_form": "긍정적이신모습",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.26",
          "form": "너무멋져요",
          "original_form": "너무멋져요",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.27",
          "form": "감사합니다 (--)(__)",
          "original_form": "감사합니다 (--)(__)",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.28",
          "form": "ㅋ큐ㅠ",
          "original_form": "ㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.29",
          "form": "제가 또",
          "original_form": "제가 또",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.30",
          "form": "긍정 빼면 시체거든요",
          "original_form": "긍정 빼면 시체거든요",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.31",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.32",
          "form": "근데 너무 제 이야기만 했네요",
          "original_form": "근데 너무 제 이야기만 했네요",
          "speaker_id": "1",
          "time": "20210905 03:07"
        },
        {
          "id": "MDRW2100031856.1.33",
          "form": "대단하세요",
