        },
        {
          "id": "MDRW2100029863.1.2",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029863.1.3",
          "form": "어떤 거 제일 좋아하세요?",
          "original_form": "어떤 거 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100029863.1.4",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029863.1.5",
          "form": "저도 치킨 좋아해요 저는 어제 땅땅치킨 먹었어요",
          "original_form": "저도 치킨 좋아해요 저는 어제 땅땅치킨 먹었어요",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029863.1.6",
          "form": "전 네네",
          "original_form": "전 네네",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029863.1.7",
          "form": "오 네네 저는 네네에서는 파닭 좋아해요",
          "original_form": "오 네네 저는 네네에서는 파닭 좋아해요",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029863.1.8",
          "form": "전 후라이드 먹습니다",
          "original_form": "전 후라이드 먹습니다",
          "speaker_id": "1",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029863.1.9",
          "form": "오늘 저녁메뉴는 먼가요",
          "original_form": "오늘 저녁메뉴는 먼가요",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029863.1.10",
          "form": "짜장이요",
          "original_form": "짜장이요",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029863.1.11",
          "form": "탕수육도 먹나요",
          "original_form": "탕수육도 먹나요",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029863.1.12",
          "form": "같이 먹어줘야죠 ㅋ",
          "original_form": "같이 먹어줘야죠 ㅋ",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029863.1.13",
          "form": "부먹? 찍먹?",
