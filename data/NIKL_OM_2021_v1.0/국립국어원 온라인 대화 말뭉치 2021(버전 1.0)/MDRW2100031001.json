      "utterance": [
        {
          "id": "MDRW2100031001.1.1",
          "form": "당근마켓 이용해보셨나요",
          "original_form": "당근마켓 이용해보셨나요",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100031001.1.2",
          "form": "네..어플있네요",
          "original_form": "네..어플있네요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100031001.1.3",
          "form": "두번팔아봤어요",
          "original_form": "두번팔아봤어요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100031001.1.4",
          "form": "아 어떤거 파셨어요?",
          "original_form": "아 어떤거 파셨어요?",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100031001.1.5",
          "form": "삼성 이어폰. 트램폴린요",
          "original_form": "삼성 이어폰. 트램폴린요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100031001.1.6",
          "form": "와 트램폴린은 왜 파셨어요?",
          "original_form": "와 트램폴린은 왜 파셨어요?",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100031001.1.7",
          "form": "애들이 안해서요",
          "original_form": "애들이 안해서요",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031001.1.8",
          "form": "처음엔엄청하더니",
          "original_form": "처음엔엄청하더니",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031001.1.9",
          "form": "10만원주고샀는데 오만원에",
          "original_form": "10만원주고샀는데 오만원에",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031001.1.10",
          "form": "3개월만에팔았네요",
          "original_form": "3개월만에팔았네요",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031001.1.11",
          "form": "앗 안하게 되는군요 저도 운동목적으로 하나 살려고 고민중인데",
          "original_form": "앗 안하게 되는군요 저도 운동목적으로 하나 살려고 고민중인데",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100031001.1.12",
          "form": "안하면 짐만될까봐 갈등이 생기네요",
          "original_form": "안하면 짐만될까봐 갈등이 생기네요",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100031001.1.13",
          "form": "거실많이 차지해서",
          "original_form": "거실많이 차지해서",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100031001.1.14",
          "form": "답답하고..",
          "original_form": "답답하고..",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100031001.1.15",
          "form": "너무약한건 뛰다부서질것같고해서튼튼한거샀거든요",
          "original_form": "너무약한건 뛰다부서질것같고해서튼튼한거샀거든요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100031001.1.16",
          "form": "그랬더니 답답해서 하지도않고",
          "original_form": "그랬더니 답답해서 하지도않고",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100031001.1.17",
          "form": "그렇군요 가족이 자꾸 사달래는데 말려야겠네요",
          "original_form": "그렇군요 가족이 자꾸 사달래는데 말려야겠네요",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100031001.1.18",
          "form": "런닝도있거든요",
          "original_form": "런닝도있거든요",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100031001.1.19",
          "form": "그것도 잘안하고",
          "original_form": "그것도 잘안하고",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100031001.1.20",
          "form": "앗 그건 저희집도ㅠ",
          "original_form": "앗 그건 저희집도ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100031001.1.21",
          "form": "옷걸이로 쓴답니다",
          "original_form": "옷걸이로 쓴답니다",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100031001.1.22",
          "form": "저희두요",
          "original_form": "저희두요",
          "speaker_id": "2",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100031001.1.23",
          "form": "렌탈이라 3년써야해서",
          "original_form": "렌탈이라 3년써야해서",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100031001.1.24",
          "form": "앗 렌탈이군요 한달에 나가는돈 아깝겠어요",
          "original_form": "앗 렌탈이군요 한달에 나가는돈 아깝겠어요",
          "speaker_id": "1",
          "time": "20210904 15:29"
        }
      ]
