      "utterance": [
        {
          "id": "MDRW2100045430.1.1",
          "form": "악 마지막 너무 호로록 넘어가서 못 봤어요 ㅋㅋㅋ",
          "original_form": "악 마지막 너무 호로록 넘어가서 못 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045430.1.2",
          "form": "점심메뉴 당연히 기억하고 있어요!",
          "original_form": "점심메뉴 당연히 기억하고 있어요!",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045430.1.3",
          "form": "좋아요 그럼 맞춰볼게요 ㅎㅎ",
          "original_form": "좋아요 그럼 맞춰볼게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045430.1.4",
          "form": "고기가 들어가나요?",
          "original_form": "고기가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045430.1.5",
          "form": "아니요! 들어가지 않아요!",
          "original_form": "아니요! 들어가지 않아요!",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045430.1.6",
          "form": "근데 생각해보면 들어가는 것도 있어요!",
          "original_form": "근데 생각해보면 들어가는 것도 있어요!",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045430.1.7",
          "form": "앗 그렇담 고기가 필수는 아니군요",
          "original_form": "앗 그렇담 고기가 필수는 아니군요",
          "speaker_id": "2",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045430.1.8",
          "form": "빵이 들어가나요?",
          "original_form": "빵이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045430.1.9",
          "form": "네! 필수는 아니에요!",
          "original_form": "네! 필수는 아니에요!",
          "speaker_id": "1",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045430.1.10",
          "form": "빵은 들어가지 않아요!",
          "original_form": "빵은 들어가지 않아요!",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045430.1.11",
          "form": "양식인가요?",
          "original_form": "양식인가요?",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045430.1.12",
          "form": "아뇨! 한식이에요!",
          "original_form": "아뇨! 한식이에요!",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045430.1.13",
          "form": "으으으음 야채가 주된 재료인가요?",
          "original_form": "으으으음 야채가 주된 재료인가요?",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045430.1.14",
          "form": "야채가 주로 됐다고 해야할까요? 속에 들어가는 메인에 따라 주가 다른것같아요.",
          "original_form": "야채가 주로 됐다고 해야할까요? 속에 들어가는 메인에 따라 주가 다른것같아요.",
          "speaker_id": "1",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045430.1.15",
          "form": "앗 속에 뭘 채워넣는 조리인건가",
          "original_form": "앗 속에 뭘 채워넣는 조리인건가",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045430.1.16",
          "form": "혹시 유부초밥인가요?",
          "original_form": "혹시 유부초밥인가요?",
          "speaker_id": "2",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045430.1.17",
          "form": "아뇨! 유부초밥이랑 비슷한? 느낌인거 같아요",
          "original_form": "아뇨! 유부초밥이랑 비슷한? 느낌인거 같아요",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045430.1.18",
          "form": "유부랑 비슷한 으음...",
          "original_form": "유부랑 비슷한 으음...",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045430.1.19",
          "form": "야채를 볶나요?",
          "original_form": "야채를 볶나요?",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045430.1.20",
          "form": "들어가는 재료중에 볶는 것도 있어요!",
          "original_form": "들어가는 재료중에 볶는 것도 있어요!",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045430.1.21",
          "form": "김이 들어가나요?",
          "original_form": "김이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045430.1.22",
          "form": "필수 입니다.",
          "original_form": "필수 입니다.",
          "speaker_id": "1",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045430.1.23",
          "form": "앗 정답 정답 김밥!",
          "original_form": "앗 정답 정답 김밥!",
          "speaker_id": "2",
          "time": "20210908 02:41"
        },
        {
          "id": "MDRW2100045430.1.24",
          "form": "네!! 정답이에요!!!",
          "original_form": "네!! 정답이에요!!!",
          "speaker_id": "1",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045430.1.25",
          "form": "와아 정답이다",
          "original_form": "와아 정답이다",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045430.1.26",
          "form": "속에 재료가 들어간다는 게 핵심 힌트였어요",
          "original_form": "속에 재료가 들어간다는 게 핵심 힌트였어요",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045430.1.27",
          "form": "ㅎㅎ 속에 재료를 넣어하는게 생각보다 많지 않죠",
          "original_form": "ㅎㅎ 속에 재료를 넣어하는게 생각보다 많지 않죠",
          "speaker_id": "1",
          "time": "20210908 02:43"
        }
      ]
    }
