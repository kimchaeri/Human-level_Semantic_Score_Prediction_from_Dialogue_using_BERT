      "utterance": [
        {
          "id": "MDRW2100031881.1.1",
          "form": "하와이입니다",
          "original_form": "하와이입니다",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031881.1.2",
          "form": "말안해도 알겠죠?",
          "original_form": "말안해도 알겠죠?",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100031881.1.3",
          "form": "하와이요?",
          "original_form": "하와이요?",
          "speaker_id": "1",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100031881.1.4",
          "form": "우와~",
          "original_form": "우와~",
          "speaker_id": "1",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100031881.1.5",
          "form": "하와이 거의 10년전에 갔었는데 진짜 좋았거든요",
          "original_form": "하와이 거의 10년전에 갔었는데 진짜 좋았거든요",
          "speaker_id": "1",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100031881.1.6",
          "form": "지금도 여전히 좋겠죠?",
          "original_form": "지금도 여전히 좋겠죠?",
          "speaker_id": "1",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100031881.1.7",
          "form": "네  여전합니다",
          "original_form": "네  여전합니다",
          "speaker_id": "2",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100031881.1.8",
          "form": "혹시 이런 여행으로 가셨나요",
          "original_form": "혹시 이런 여행으로 가셨나요",
          "speaker_id": "2",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100031881.1.9",
          "form": "신혼여행?",
          "original_form": "신혼여행?",
          "speaker_id": "2",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031881.1.10",
          "form": "하와이는 날씨가 너무 좋습니다",
          "original_form": "하와이는 날씨가 너무 좋습니다",
          "speaker_id": "2",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031881.1.11",
          "form": "아뇨 해외여행을 종종 했어서 그냥 여행으로 갔었어요",
          "original_form": "아뇨 해외여행을 종종 했어서 그냥 여행으로 갔었어요",
          "speaker_id": "1",
          "time": "20210905 09:14"
        },
        {
          "id": "MDRW2100031881.1.12",
          "form": "하와이는 항상 날씨가 좋다고 그러더라구요",
          "original_form": "하와이는 항상 날씨가 좋다고 그러더라구요",
          "speaker_id": "1",
          "time": "20210905 09:14"
        },
        {
          "id": "MDRW2100031881.1.13",
          "form": "와우 멋지십니다",
          "original_form": "와우 멋지십니다",
          "speaker_id": "2",
          "time": "20210905 09:14"
        },
        {
          "id": "MDRW2100031881.1.14",
          "form": "노스쇼어",
          "original_form": "노스쇼어",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100031881.1.15",
          "form": "새우 기억나시나요",
          "original_form": "새우 기억나시나요",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100031881.1.16",
          "form": "음.. 10년도 더 전이라.. 아마도 먹었을텐데..",
          "original_form": "음.. 10년도 더 전이라.. 아마도 먹었을텐데..",
          "speaker_id": "1",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100031881.1.17",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031881.1.18",
          "form": "이런 날씨에는 놀러가야 됩니다",
          "original_form": "이런 날씨에는 놀러가야 됩니다",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031881.1.19",
          "form": "놀아야 되죠 아주 좋은 날씨죠",
          "original_form": "놀아야 되죠 아주 좋은 날씨죠",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031881.1.20",
          "form": "그런데 간만에 딸이 와서 일단 집에 있을 계획이에요..ㅎㅎ",
          "original_form": "그런데 간만에 딸이 와서 일단 집에 있을 계획이에요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031881.1.21",
          "form": "오  딸이 어디에서 온 건가요",
          "original_form": "오  딸이 어디에서 온 건가요",
          "speaker_id": "2",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100031881.1.22",
          "form": "따로 살고 있거든요 한시간 거리에",
          "original_form": "따로 살고 있거든요 한시간 거리에",
          "speaker_id": "1",
          "time": "20210905 09:31"
        },
        {
          "id": "MDRW2100031881.1.23",
          "form": "그동안 거리두기 때문에 못왔었죠",
          "original_form": "그동안 거리두기 때문에 못왔었죠",
          "speaker_id": "1",
          "time": "20210905 09:31"
        },
        {
          "id": "MDRW2100031881.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100031881.1.25",
          "form": "정말 보고 싶으셨겠습니다",
          "original_form": "정말 보고 싶으셨겠습니다",
          "speaker_id": "2",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100031881.1.26",
          "form": "따님은 결혼하셨나요",
          "original_form": "따님은 결혼하셨나요",
          "speaker_id": "2",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100031881.1.27",
          "form": "날씨 좋은 데 따님이랑 같이 밖에 나가서 꽃구경  구경하고",
          "original_form": "날씨 좋은 데 따님이랑 같이 밖에 나가서 꽃구경  구경하고",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100031881.1.28",
          "form": "맛있는 식사도 하면 좋으실 것 같습니다",
          "original_form": "맛있는 식사도 하면 좋으실 것 같습니다",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100031881.1.29",
          "form": "제가 집에서 요리하는 걸 좋아해서요",
          "original_form": "제가 집에서 요리하는 걸 좋아해서요",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031881.1.30",
          "form": "열심히 장봐와서 맛있는 요리 해주고 있어요",
          "original_form": "열심히 장봐와서 맛있는 요리 해주고 있어요",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031881.1.31",
          "form": "저희 장모님과 비슷하시네요",
          "original_form": "저희 장모님과 비슷하시네요",
          "speaker_id": "2",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031881.1.32",
          "form": "손자 온다고 하면",
          "original_form": "손자 온다고 하면",
          "speaker_id": "2",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031881.1.33",
          "form": "한여름에도 닭을 쌈 습니다",
          "original_form": "한여름에도 닭을 쌈 습니다",
          "speaker_id": "2",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100031881.1.34",
          "form": "나가서 먹어도 좋지만 왠지 집에서 정성드린 음식 해먹이고 싶어요",
          "original_form": "나가서 먹어도 좋지만 왠지 집에서 정성드린 음식 해먹이고 싶어요",
          "speaker_id": "1",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100031881.1.35",
          "form": "아버지 신가요",
          "original_form": "아버지 신가요",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031881.1.36",
          "form": "엄마죠~",
          "original_form": "엄마죠~",
          "speaker_id": "1",
          "time": "20210905 09:45"
        }
      ]
    }
