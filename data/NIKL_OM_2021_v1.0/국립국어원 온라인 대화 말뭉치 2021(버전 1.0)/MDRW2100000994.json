      "utterance": [
        {
          "id": "MDRW2100000994.1.1",
          "form": "콘서트 같은 거 가본 적 있으신가요?",
          "original_form": "콘서트 같은 거 가본 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100000994.1.2",
          "form": "저는 가본 적이 없어서",
          "original_form": "저는 가본 적이 없어서",
          "speaker_id": "2",
          "time": "20210820 11:28"
        },
        {
          "id": "MDRW2100000994.1.3",
          "form": "아니요 연극이나 뮤지컬은 몇번 가봤는데 콘서트는 아직 못가봤어요",
          "original_form": "아니요 연극이나 뮤지컬은 몇번 가봤는데 콘서트는 아직 못가봤어요",
          "speaker_id": "1",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100000994.1.4",
          "form": "문화생활은 요즘 영화가 전부",
          "original_form": "문화생활은 요즘 영화가 전부",
          "speaker_id": "1",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100000994.1.5",
          "form": "기억에 남는 뮤지컬이 있나요?",
          "original_form": "기억에 남는 뮤지컬이 있나요?",
          "speaker_id": "2",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100000994.1.6",
          "form": "그리스 재밌게봤어요 비싼공연은 못가봐서",
          "original_form": "그리스 재밌게봤어요 비싼공연은 못가봐서",
          "speaker_id": "1",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100000994.1.7",
          "form": "저는 어릴 때 어린이 뮤지컬만 봐서 커서는 기회가 안닿아 못봤던거같아요",
          "original_form": "저는 어릴 때 어린이 뮤지컬만 봐서 커서는 기회가 안닿아 못봤던거같아요",
          "speaker_id": "2",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100000994.1.8",
          "form": "그리스요? 어떤 내용인가요?",
          "original_form": "그리스요? 어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20210820 11:30"
        },
        {
          "id": "MDRW2100000994.1.9",
          "form": "아마 노래들으심 아 이거 하실텐데 내용은 저도 기억이 잘안나네요ㅎ 젊은이들의 사랑얘기였던듯",
          "original_form": "아마 노래들으심 아 이거 하실텐데 내용은 저도 기억이 잘안나네요ㅎ 젊은이들의 사랑얘기였던듯",
          "speaker_id": "1",
          "time": "20210820 11:30"
        },
        {
          "id": "MDRW2100000994.1.10",
          "form": "안그래도 찾아던 중이었는데 재밌을거 같아요",
          "original_form": "안그래도 찾아던 중이었는데 재밌을거 같아요",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000994.1.11",
          "form": "뮤지컬은 그 현장에서 보는 그게 정말 좋은 거 같아요",
          "original_form": "뮤지컬은 그 현장에서 보는 그게 정말 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000994.1.12",
          "form": "코로나인데 요즘 공연하나요?",
          "original_form": "코로나인데 요즘 공연하나요?",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000994.1.13",
          "form": "거리두기하고 하는거같기도하던데",
          "original_form": "거리두기하고 하는거같기도하던데",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000994.1.14",
          "form": "해당공연은 일정 없는 거 같구 다른 공연은 하긴 하는데 관객은 많이 안받는다더라구요",
          "original_form": "해당공연은 일정 없는 거 같구 다른 공연은 하긴 하는데 관객은 많이 안받는다더라구요",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000994.1.15",
          "form": "코로나로 예술계 타격이 크다들었어요",
          "original_form": "코로나로 예술계 타격이 크다들었어요",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000994.1.16",
          "form": "요즘 다힘든거 같아요 영화도 보러가면 자리도 많이 비었더라구요",
          "original_form": "요즘 다힘든거 같아요 영화도 보러가면 자리도 많이 비었더라구요",
          "speaker_id": "1",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000994.1.17",
          "form": "최근에 어떤영화 보셨나요?",
          "original_form": "최근에 어떤영화 보셨나요?",
          "speaker_id": "2",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000994.1.18",
          "form": "일욜에 모가디슈봤는데 잼나요",
          "original_form": "일욜에 모가디슈봤는데 잼나요",
          "speaker_id": "1",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000994.1.19",
          "form": "반응 좋더라구요. 신파없이 잘 만들었다고",
          "original_form": "반응 좋더라구요. 신파없이 잘 만들었다고",
          "speaker_id": "2",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000994.1.20",
          "form": "스트레스도 풀리고 ㅎ 총을 마구쏴서",
          "original_form": "스트레스도 풀리고 ㅎ 총을 마구쏴서",
          "speaker_id": "1",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000994.1.21",
          "form": "저도 기회가 된다면 보러가야겠어요",
          "original_form": "저도 기회가 된다면 보러가야겠어요",
          "speaker_id": "2",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000994.1.22",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210820 11:35"
        },
        {
          "id": "MDRW2100000994.1.23",
          "form": "시간날때 함보고오세요ㅎㅎ",
          "original_form": "시간날때 함보고오세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:35"
        }
      ]
    }
