      "utterance": [
        {
          "id": "MDRW2100006813.1.1",
          "form": "가사분담은 어떻게 하세요?-",
          "original_form": "가사분담은 어떻게 하세요?-",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100006813.1.2",
          "form": "맞벌이라 반반 해요",
          "original_form": "맞벌이라 반반 해요",
          "speaker_id": "2",
          "time": "20210908 10:00"
        },
        {
          "id": "MDRW2100006813.1.3",
          "form": "주로 무엇을 하시나요?-",
          "original_form": "주로 무엇을 하시나요?-",
          "speaker_id": "1",
          "time": "20210908 10:00"
        },
        {
          "id": "MDRW2100006813.1.4",
          "form": "어떻게 가사분담을 하시나요",
          "original_form": "어떻게 가사분담을 하시나요",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100006813.1.5",
          "form": "전 요리 청소해요",
          "original_form": "전 요리 청소해요",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100006813.1.6",
          "form": "배우자분은요?",
          "original_form": "배우자분은요?",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100006813.1.7",
          "form": "빨래 설거지 청소요",
          "original_form": "빨래 설거지 청소요",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100006813.1.8",
          "form": "어떤것 하시나요",
          "original_form": "어떤것 하시나요",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100006813.1.9",
          "form": "저는 대부분 같이 하고 화장실청소 전담이요 ㅎㅎ",
          "original_form": "저는 대부분 같이 하고 화장실청소 전담이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100006813.1.10",
          "form": "화장실 진짜 하기 싫은곳중 하나죠",
          "original_form": "화장실 진짜 하기 싫은곳중 하나죠",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100006813.1.11",
          "form": "잘하시고 계시네요",
          "original_form": "잘하시고 계시네요",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100006813.1.12",
          "form": "네 또 어떤게 싫으신가요",
          "original_form": "네 또 어떤게 싫으신가요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100006813.1.13",
          "form": "청소가 싫긴해요",
          "original_form": "청소가 싫긴해요",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100006813.1.14",
          "form": "해도 표도 안나죠",
          "original_form": "해도 표도 안나죠",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100006813.1.15",
          "form": "ㅎㅎ 아이는 있나요??",
          "original_form": "ㅎㅎ 아이는 있나요??",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100006813.1.16",
          "form": "네 저희는 애들이 대학생이예요",
          "original_form": "네 저희는 애들이 대학생이예요",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100006813.1.17",
          "form": "다 키우셨군요",
          "original_form": "다 키우셨군요",
          "speaker_id": "1",
          "time": "20210908 10:15"
        }
