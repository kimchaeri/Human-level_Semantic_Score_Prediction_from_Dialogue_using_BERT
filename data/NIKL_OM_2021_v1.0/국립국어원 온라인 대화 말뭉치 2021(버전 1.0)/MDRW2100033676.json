      "utterance": [
        {
          "id": "MDRW2100033676.1.1",
          "form": "하숙을 하는 기러기 아빠 얘기로 이야기를 시작하네",
          "original_form": "하숙을 하는 기러기 아빠 얘기로 이야기를 시작하네",
          "speaker_id": "2",
          "time": "20210824 22:38"
        },
        {
          "id": "MDRW2100033676.1.2",
          "form": "얘기가 좀 불길하다",
          "original_form": "얘기가 좀 불길하다",
          "speaker_id": "1",
          "time": "20210824 22:38"
        },
        {
          "id": "MDRW2100033676.1.3",
          "form": "이 사건으로 인해서 주민등록증이 생긴건가?",
          "original_form": "이 사건으로 인해서 주민등록증이 생긴건가?",
          "speaker_id": "2",
          "time": "20210824 22:39"
        },
        {
          "id": "MDRW2100033676.1.4",
          "form": "뭐지?  민증이 생겼다고?",
          "original_form": "뭐지?  민증이 생겼다고?",
          "speaker_id": "1",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100033676.1.5",
          "form": "갑자기 의문의 바바리무리가 나옴 ㅋㅋ타자 치는 사이에",
          "original_form": "갑자기 의문의 바바리무리가 나옴 ㅋㅋ타자 치는 사이에",
          "speaker_id": "2",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100033676.1.6",
          "form": "뭔 얘긴지 모르겠다ㅋ",
          "original_form": "뭔 얘긴지 모르겠다ㅋ",
          "speaker_id": "1",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100033676.1.7",
          "form": "검문중이던 사람들이 이 바바리 무리를 발견",
          "original_form": "검문중이던 사람들이 이 바바리 무리를 발견",
          "speaker_id": "2",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100033676.1.8",
          "form": "기러기 아빠는 뭐하는데?",
          "original_form": "기러기 아빠는 뭐하는데?",
          "speaker_id": "1",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100033676.1.9",
          "form": "방첩부대가 지금의 기무사래",
          "original_form": "방첩부대가 지금의 기무사래",
          "speaker_id": "2",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100033676.1.10",
          "form": "방첩부대 군대 아니가?",
          "original_form": "방첩부대 군대 아니가?",
          "speaker_id": "1",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100033676.1.11",
          "form": "3개월동안 산에서 훈련하고 내려오는거라고 그 무리가 얘기하는데",
          "original_form": "3개월동안 산에서 훈련하고 내려오는거라고 그 무리가 얘기하는데",
          "speaker_id": "2",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100033676.1.12",
          "form": "기무사가 원래 뭐하는 거지?",
          "original_form": "기무사가 원래 뭐하는 거지?",
          "speaker_id": "1",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100033676.1.13",
          "form": "무장을 했는데도 검색에서 통과됨 헐~",
          "original_form": "무장을 했는데도 검색에서 통과됨 헐~",
          "speaker_id": "2",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100033676.1.14",
          "form": "뭔 얘긴지 도통 모르겠다ㅋㅋ",
          "original_form": "뭔 얘긴지 도통 모르겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100033676.1.15",
          "form": "갑첩이나 첩보활동을 막는 특무부대라네",
          "original_form": "갑첩이나 첩보활동을 막는 특무부대라네",
          "speaker_id": "2",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100033676.1.16",
          "form": "기러기 아빠는 끝난거? ㅋㅋ",
          "original_form": "기러기 아빠는 끝난거? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100033676.1.17",
          "form": "아까 기러기 아빠가 탄 버스를 이 바바리부대가 군부태차량인줄 알고 총을 쏘았다네",
          "original_form": "아까 기러기 아빠가 탄 버스를 이 바바리부대가 군부태차량인줄 알고 총을 쏘았다네",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100033676.1.18",
          "form": "아 우째 그래서 기러기 아빠 죽었어?",
          "original_form": "아 우째 그래서 기러기 아빠 죽었어?",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100033676.1.19",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100033676.1.20",
          "form": "그럼 나라에서 보상해 줘야지",
          "original_form": "그럼 나라에서 보상해 줘야지",
          "speaker_id": "1",
          "time": "20210824 22:45"
        }
      ]
