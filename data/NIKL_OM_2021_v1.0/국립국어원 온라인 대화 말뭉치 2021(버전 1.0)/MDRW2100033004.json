      "utterance": [
        {
          "id": "MDRW2100033004.1.1",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100033004.1.2",
          "form": "가끔 모바일게임 이것저것 해요",
          "original_form": "가끔 모바일게임 이것저것 해요",
          "speaker_id": "1",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100033004.1.3",
          "form": "ㅋㅋ저는 요새 흥미가 없어서",
          "original_form": "ㅋㅋ저는 요새 흥미가 없어서",
          "speaker_id": "2",
          "time": "20210906 12:50"
        },
        {
          "id": "MDRW2100033004.1.4",
          "form": "프렌즈샷 이벤트 하길래 깔아봤는데 나름 재밌더라구요",
          "original_form": "프렌즈샷 이벤트 하길래 깔아봤는데 나름 재밌더라구요",
          "speaker_id": "1",
          "time": "20210906 13:28"
        },
        {
          "id": "MDRW2100033004.1.5",
          "form": "무슨 이벤트하죵",
          "original_form": "무슨 이벤트하죵",
          "speaker_id": "2",
          "time": "20210906 14:19"
        },
        {
          "id": "MDRW2100033004.1.6",
          "form": "새로 런칭하는 게임이라 이모티콘 주더라구요ㅎㅎ",
          "original_form": "새로 런칭하는 게임이라 이모티콘 주더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:56"
        },
        {
          "id": "MDRW2100033004.1.7",
          "form": "한번 무슨 이모티콘인지 봐야겠네요",
          "original_form": "한번 무슨 이모티콘인지 봐야겠네요",
          "speaker_id": "2",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100033004.1.8",
          "form": "튜토리얼 하면 준다고 하더라구요. 내일부터 순차 지급",
          "original_form": "튜토리얼 하면 준다고 하더라구요. 내일부터 순차 지급",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033004.1.9",
          "form": "오 한번 알아보겠습니다ㅎㅎ",
          "original_form": "오 한번 알아보겠습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100033004.1.10",
          "form": "참고로 골프게임이에요ㅎㅎ",
          "original_form": "참고로 골프게임이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033004.1.11",
          "form": "어라 골프요?ㅎㅎ",
          "original_form": "어라 골프요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100033004.1.12",
          "form": "기본은 랜덤유저랑 일대일로 붙는 방식이더라구요",
          "original_form": "기본은 랜덤유저랑 일대일로 붙는 방식이더라구요",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033004.1.13",
          "form": "ㅋㅋ진행 빠른가요",
          "original_form": "ㅋㅋ진행 빠른가요",
