          "form": "대화",
          "original_form": "대화",
          "speaker_id": "2",
          "time": "20210831 01:02"
        },
        {
          "id": "MDRW2100024315.1.3",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210831 01:02"
        },
        {
          "id": "MDRW2100024315.1.4",
          "form": "9월1일 쇼핑기대됩니다",
          "original_form": "9월1일 쇼핑기대됩니다",
          "speaker_id": "1",
          "time": "20210831 01:03"
        },
        {
          "id": "MDRW2100024315.1.5",
          "form": "리스트 작성하셨나요?",
          "original_form": "리스트 작성하셨나요?",
          "speaker_id": "1",
          "time": "20210831 01:03"
        },
        {
          "id": "MDRW2100024315.1.6",
          "form": "가격 비교 빅스마일데이 전꺼랑 미리하셔야해요ㅋㅋㅋ",
          "original_form": "가격 비교 빅스마일데이 전꺼랑 미리하셔야해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.7",
          "form": "저는 애기엄마라 분유 기저귀",
          "original_form": "저는 애기엄마라 분유 기저귀",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.8",
          "form": "시크릿코인",
          "original_form": "시크릿코인",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.9",
          "form": "초반에달려야하드라구요",
          "original_form": "초반에달려야하드라구요",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.10",
          "form": "저는ㅋㅋ접시랑ㄲ",
          "original_form": "저는ㅋㅋ접시랑ㄲ",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.11",
          "form": "담날가격올림",
          "original_form": "담날가격올림",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.12",
          "form": "가습기요ㅋ",
          "original_form": "가습기요ㅋ",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.13",
          "form": "오ㅋㅋㄲ꿀팁감사감사",
          "original_form": "오ㅋㅋㄲ꿀팁감사감사",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.14",
          "form": "화장품도사야해요ㅋㅋㅋ",
          "original_form": "화장품도사야해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100024315.1.15",
          "form": "설화수꺼ㅋㅋㅋ",
          "original_form": "설화수꺼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100024315.1.16",
          "form": "새벽에사야해요",
          "original_form": "새벽에사야해요",
          "speaker_id": "2",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100024315.1.17",
          "form": "기저귀뭐쓰세요?",
          "original_form": "기저귀뭐쓰세요?",
          "speaker_id": "1",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100024315.1.18",
          "form": "저도친정엄마물어봐야지요",
          "original_form": "저도친정엄마물어봐야지요",
          "speaker_id": "2",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100024315.1.19",
          "form": "보솜이 하기스 마미포코",
          "original_form": "보솜이 하기스 마미포코",
          "speaker_id": "2",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100024315.1.20",
          "form": "다양하게 활용해요",
          "original_form": "다양하게 활용해요",
          "speaker_id": "2",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100024315.1.21",
          "form": "다양하시네요",
          "original_form": "다양하시네요",
          "speaker_id": "1",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100024315.1.22",
          "form": "핫딜뜨면 사는건가요?",
          "original_form": "핫딜뜨면 사는건가요?",
          "speaker_id": "1",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100024315.1.23",
          "form": "그냥사도 핫딜가격이드라구요",
          "original_form": "그냥사도 핫딜가격이드라구요",
          "speaker_id": "2",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024315.1.24",
          "form": "공기청정기사야하는데...",
          "original_form": "공기청정기사야하는데...",
          "speaker_id": "1",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024315.1.25",
          "form": "지금쯤뒤집을줄안고",
          "original_form": "지금쯤뒤집을줄안고",
          "speaker_id": "2",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024315.1.26",
          "form": "뒤집질않으셔요",
          "original_form": "뒤집질않으셔요",
          "speaker_id": "2",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024315.1.27",
          "form": "안잘수있겠지요?ㅋㅋ",
          "original_form": "안잘수있겠지요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024315.1.28",
          "form": "대형밴드사게요ㅋㅋㅋ",
          "original_form": "대형밴드사게요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024315.1.29",
          "form": "저는 자신있어요",
          "original_form": "저는 자신있어요",
          "speaker_id": "2",
          "time": "20210831 01:06"
        },
        {
          "id": "MDRW2100024315.1.30",
          "form": "밴드기저귀ㅋㅋㅋ귀여워요ㅋㅋㅋㅋ",
          "original_form": "밴드기저귀ㅋㅋㅋ귀여워요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:07"
        },
        {
          "id": "MDRW2100024315.1.31",
          "form": "도대체왜 ㄷ뒤집지않는걸까요",
          "original_form": "도대체왜 ㄷ뒤집지않는걸까요",
          "speaker_id": "2",
          "time": "20210831 01:07"
        },
        {
          "id": "MDRW2100024315.1.32",
          "form": "9월1일자로카드풀리니ㅋㅋ미친듯사보아요",
          "original_form": "9월1일자로카드풀리니ㅋㅋ미친듯사보아요",
