      "utterance": [
        {
          "id": "MDRW2100014600.1.1",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014600.1.2",
          "form": "방금 먹었어요",
          "original_form": "방금 먹었어요",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014600.1.3",
          "form": "늦은 저녁이라 좀 더부룩하네요",
          "original_form": "늦은 저녁이라 좀 더부룩하네요",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014600.1.4",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014600.1.5",
          "form": "오리고기랑 김치 먹었어요",
          "original_form": "오리고기랑 김치 먹었어요",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014600.1.6",
          "form": "name1님은 저녁 드셨나요?",
          "original_form": "&name1&님은 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014600.1.7",
          "form": "네 저는 미소시루 끓여먹었어요",
          "original_form": "네 저는 미소시루 끓여먹었어요",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014600.1.8",
          "form": "오 미소된장국인가요?",
          "original_form": "오 미소된장국인가요?",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014600.1.9",
          "form": "한끼 든든하게 드셨겠어요",
          "original_form": "한끼 든든하게 드셨겠어요",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014600.1.10",
          "form": "아녀 대충 먹었어요ㅠ",
          "original_form": "아녀 대충 먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014600.1.11",
          "form": "내일은 잘 챙겨먹으려구요",
          "original_form": "내일은 잘 챙겨먹으려구요",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014600.1.12",
          "form": "한끼 챙겨먹는거 중요한데",
          "original_form": "한끼 챙겨먹는거 중요한데",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014600.1.13",
          "form": "힘들죠",
          "original_form": "힘들죠",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014600.1.14",
          "form": "저도 잘 안챙겨먹게 되더라구요",
          "original_form": "저도 잘 안챙겨먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014600.1.15",
          "form": "오늘만해도 늦게 먹어서",
          "original_form": "오늘만해도 늦게 먹어서",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014600.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014600.1.17",
          "form": "소화 잘 시키고 주무세요ㅋㅋ",
          "original_form": "소화 잘 시키고 주무세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014600.1.18",
          "form": "전 내일 고기 구워먹을 거예요 ㅋㅋ",
          "original_form": "전 내일 고기 구워먹을 거예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014600.1.19",
          "form": "고기!",
          "original_form": "고기!",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014600.1.20",
          "form": "단백질 맛있겠네요",
          "original_form": "단백질 맛있겠네요",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014600.1.21",
          "form": "고기 생갇하면 힘이나죠!",
          "original_form": "고기 생갇하면 힘이나죠!",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014600.1.22",
          "form": "네 소고기 부채살이 절 기다리고 있어요",
          "original_form": "네 소고기 부채살이 절 기다리고 있어요",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014600.1.23",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014600.1.24",
          "form": "부채살",
          "original_form": "부채살",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014600.1.25",
          "form": "전 토시살 좋아해요",
          "original_form": "전 토시살 좋아해요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014600.1.26",
          "form": "부채살이 기다리고 있다니",
          "original_form": "부채살이 기다리고 있다니",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014600.1.27",
          "form": "토시살! 특수부위가 확실히 맛있어요",
          "original_form": "토시살! 특수부위가 확실히 맛있어요",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014600.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014600.1.29",
          "form": "사실 소고기는 다 좋지만",
          "original_form": "사실 소고기는 다 좋지만",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014600.1.30",
          "form": "고기는 다 정답!",
          "original_form": "고기는 다 정답!",
          "speaker_id": "2",
          "time": "20210825 21:16"
        }
      ]
    }
  ]
