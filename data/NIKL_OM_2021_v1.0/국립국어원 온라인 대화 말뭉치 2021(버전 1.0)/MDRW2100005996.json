      "utterance": [
        {
          "id": "MDRW2100005996.1.1",
          "form": "이마트에서 밀키트 샀어요!",
          "original_form": "이마트에서 밀키트 샀어요!",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005996.1.2",
          "form": "밀키트도 요즘 많이 나오죠",
          "original_form": "밀키트도 요즘 많이 나오죠",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005996.1.3",
          "form": "네 잘 나옵니다 ㅎㅎ",
          "original_form": "네 잘 나옵니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100005996.1.4",
          "form": "가성비!",
          "original_form": "가성비!",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100005996.1.5",
          "form": "맛도 괜찮나요",
          "original_form": "맛도 괜찮나요",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100005996.1.6",
          "form": "요즘 밀키트 무인점포도 생기던데요",
          "original_form": "요즘 밀키트 무인점포도 생기던데요",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100005996.1.7",
          "form": "네 배달도 있어요",
          "original_form": "네 배달도 있어요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100005996.1.8",
          "form": "밀키트배달",
          "original_form": "밀키트배달",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100005996.1.9",
          "form": "아 배달은 잘 모랐네요",
          "original_form": "아 배달은 잘 모랐네요",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100005996.1.10",
          "form": "요새 틈새시장이 ㅎㅎ",
          "original_form": "요새 틈새시장이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100005996.1.11",
          "form": "전 아직 한번도 구매를 해보지 않았어요",
          "original_form": "전 아직 한번도 구매를 해보지 않았어요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100005996.1.12",
          "form": "한번 찾아보세요 ㅎㅎ",
          "original_form": "한번 찾아보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100005996.1.13",
          "form": "음식하기 싫을때 시켜서 해 먹으면 좋을것 같네요",
          "original_form": "음식하기 싫을때 시켜서 해 먹으면 좋을것 같네요",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100005996.1.14",
          "form": "아 조리는 직접입니다만ㅎㅎ",
          "original_form": "아 조리는 직접입니다만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100005996.1.15",
          "form": "그래도 끓이기만 하면 되는것 아닌가요",
          "original_form": "그래도 끓이기만 하면 되는것 아닌가요",
