      "utterance": [
        {
          "id": "MDRW2100007275.1.1",
          "form": "앗 좋아요ㅋㅋㅋㅋㅋ",
          "original_form": "앗 좋아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007275.1.2",
          "form": "요즘 영화보신거 있어요?",
          "original_form": "요즘 영화보신거 있어요?",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007275.1.3",
          "form": "하아아... 사실 제가 문화생활 문외한이긴 한데",
          "original_form": "하아아... 사실 제가 문화생활 문외한이긴 한데",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007275.1.4",
          "form": "최근에 친구가 저 꼭 보여주고싶다고 해서ㅋㅋㅋㅋ",
          "original_form": "최근에 친구가 저 꼭 보여주고싶다고 해서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007275.1.5",
          "form": "강제로 앉아서 레미제라블 시청하다 왔어요!",
          "original_form": "강제로 앉아서 레미제라블 시청하다 왔어요!",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007275.1.6",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007275.1.7",
          "form": "오 뮤지컬이요?",
          "original_form": "오 뮤지컬이요?",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007275.1.8",
          "form": "좋은친구네요 ㅋ",
          "original_form": "좋은친구네요 ㅋ",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007275.1.9",
          "form": "영화요! 뮤지컬은 그 친구도 보고싶어했는데 아직도 못 봤다고 하더라고요ㅋㅋㅋㅋ",
          "original_form": "영화요! 뮤지컬은 그 친구도 보고싶어했는데 아직도 못 봤다고 하더라고요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007275.1.10",
          "form": "진짜... 그 친구덕분에 부족한 문화생활 채우는 느낌이에요ㅋㅋㅋㅋ",
          "original_form": "진짜... 그 친구덕분에 부족한 문화생활 채우는 느낌이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007275.1.11",
          "form": "전 뮤지컬보고싶은데 기회가 없네요",
          "original_form": "전 뮤지컬보고싶은데 기회가 없네요",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007275.1.12",
          "form": "지킬앤하이드 보고싶어요 조승우나오는걸로",
          "original_form": "지킬앤하이드 보고싶어요 조승우나오는걸로",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007275.1.13",
          "form": "헉 지금 현재 공연중에 있는건가요?!",
          "original_form": "헉 지금 현재 공연중에 있는건가요?!",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007275.1.14",
          "form": "하는거같기도 하고 끝난건지ㅎ",
          "original_form": "하는거같기도 하고 끝난건지ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007275.1.15",
          "form": "조승우껀 다매진이래요",
          "original_form": "조승우껀 다매진이래요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007275.1.16",
          "form": "헉... 역시 유명배우쪽은 특히나 인기가 많은걸까요...",
          "original_form": "헉... 역시 유명배우쪽은 특히나 인기가 많은걸까요...",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007275.1.17",
          "form": "조승우 좋아하시나요?!",
          "original_form": "조승우 좋아하시나요?!",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007275.1.18",
          "form": "그런건아닌데 그건 꼭 조승우꺼보라더라구요ㅎ",
          "original_form": "그런건아닌데 그건 꼭 조승우꺼보라더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007275.1.19",
          "form": "헉... 맞아요 캐스팅 중에서",
          "original_form": "헉... 맞아요 캐스팅 중에서",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007275.1.20",
          "form": "유난히 잘하는 사람이 꼭 있는 것 같더라고요",
          "original_form": "유난히 잘하는 사람이 꼭 있는 것 같더라고요",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007275.1.21",
          "form": "티켓예매가 넘 힘들어요 그런 배우들은",
          "original_form": "티켓예매가 넘 힘들어요 그런 배우들은",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007275.1.22",
          "form": "맞아요 나만 좋다고 생각한게 아니고",
          "original_form": "맞아요 나만 좋다고 생각한게 아니고",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007275.1.23",
          "form": "나만 좋은 이야기 들은게 아니라서 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나만 좋은 이야기 들은게 아니라서 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007275.1.24",
          "form": "다들 똑같은 걸 노리고 티켓팅하죠ㅋㅋㅋㅋㅋ",
          "original_form": "다들 똑같은 걸 노리고 티켓팅하죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007275.1.25",
          "form": "맞아요 ㅎ 치열한 티켓팅 ㅎ 열리는순간 끝나더라구요",
          "original_form": "맞아요 ㅎ 치열한 티켓팅 ㅎ 열리는순간 끝나더라구요",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007275.1.26",
          "form": "다들대단",
          "original_form": "다들대단",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007275.1.27",
          "form": "맞아요... 진짜 티켓팅 시간 되면",
          "original_form": "맞아요... 진짜 티켓팅 시간 되면",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007275.1.28",
          "form": "서버타임 웹사이트 가면 나랑 똑같은거 대기타는 사람들이 수두룩이에요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "서버타임 웹사이트 가면 나랑 똑같은거 대기타는 사람들이 수두룩이에요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007275.1.29",
          "form": "접때 어머니 미스터트롯 콘서트 티켓팅 도와준다고 서버시계 켜고 기다리고있었는데",
          "original_form": "접때 어머니 미스터트롯 콘서트 티켓팅 도와준다고 서버시계 켜고 기다리고있었는데",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007275.1.30",
          "form": "저랑 상황 똑같은 남의 집 딸들도 다 와서 대기타고있더라고요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "저랑 상황 똑같은 남의 집 딸들도 다 와서 대기타고있더라고요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007275.1.31",
          "form": "전 시도할 엄두도 안나네요",
          "original_form": "전 시도할 엄두도 안나네요",
          "speaker_id": "1",
          "time": "20210822 01:41"
        }
      ]
    }
  ]
