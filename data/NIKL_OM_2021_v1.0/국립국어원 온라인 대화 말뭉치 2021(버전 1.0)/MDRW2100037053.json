      "utterance": [
        {
          "id": "MDRW2100037053.1.1",
          "form": "name1님 잘하시는 요리 있으신가요?!",
          "original_form": "&name1&님 잘하시는 요리 있으신가요?!",
          "speaker_id": "2",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100037053.1.2",
          "form": "저 파스타나 볶음밥 자주 해먹어요",
          "original_form": "저 파스타나 볶음밥 자주 해먹어요",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100037053.1.3",
          "form": "냉장고 야채 처리용입니다 ㅋㅋㅋ",
          "original_form": "냉장고 야채 처리용입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100037053.1.4",
          "form": "헛헛 저도 자주 냉장고 파먹기를... ㅎ.ㅎ !!!!!!!",
          "original_form": "헛헛 저도 자주 냉장고 파먹기를... ㅎ.ㅎ !!!!!!!",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100037053.1.5",
          "form": "파스타!! 어떤 소스를 제일 좋아하세요?",
          "original_form": "파스타!! 어떤 소스를 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100037053.1.6",
          "form": "냉장고 파먹으면 뿌듯해요 ㅋㅋㅋ 식비 아낀거같고",
          "original_form": "냉장고 파먹으면 뿌듯해요 ㅋㅋㅋ 식비 아낀거같고",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100037053.1.7",
          "form": "저 오일파스타 좋아해요!",
          "original_form": "저 오일파스타 좋아해요!",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100037053.1.8",
          "form": "마늘 엄청넣고 알리오올리오를 해먹는데 맛잇어요",
          "original_form": "마늘 엄청넣고 알리오올리오를 해먹는데 맛잇어요",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100037053.1.9",
          "form": "헐헐 저도요 저 진짜 한국인답게",
          "original_form": "헐헐 저도요 저 진짜 한국인답게",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037053.1.10",
          "form": "마늘 엄첯 때려넣어서 먹어요",
          "original_form": "마늘 엄첯 때려넣어서 먹어요",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037053.1.11",
          "form": "마늘에 면넣고 먹기 ㅋㅋㅋㅋㅋ",
          "original_form": "마늘에 면넣고 먹기 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037053.1.12",
          "form": "거의 그 정도죠 ㅎㅎㅎ !!!!!!!!",
          "original_form": "거의 그 정도죠 ㅎㅎㅎ !!!!!!!!",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037053.1.13",
          "form": "크림 파스타도 좋아하세요?!",
          "original_form": "크림 파스타도 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037053.1.14",
          "form": "네! 그것도 마늘 많이넣ㅇ서 먹어요 ㅋㅋㅋㅋ 꾸덕하고 매콤하면 맛있더라구요",
          "original_form": "네! 그것도 마늘 많이넣ㅇ서 먹어요 ㅋㅋㅋㅋ 꾸덕하고 매콤하면 맛있더라구요",
          "speaker_id": "1",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100037053.1.15",
          "form": "애슐리 크림떡볶이 아시나요",
          "original_form": "애슐리 크림떡볶이 아시나요",
          "speaker_id": "1",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100037053.1.16",
          "form": "헐 아뇨!?!? 맛있나요?!!??@@",
          "original_form": "헐 아뇨!?!? 맛있나요?!!??@@",
          "speaker_id": "2",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100037053.1.17",
          "form": "저 그거먹으러 애슐리갓엇거든요 요즘은 안가지만..",
          "original_form": "저 그거먹으러 애슐리갓엇거든요 요즘은 안가지만..",
          "speaker_id": "1",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100037053.1.18",
          "form": "고소하고 꾸덕하고 짭짤하고 존맛이에요",
          "original_form": "고소하고 꾸덕하고 짭짤하고 존맛이에요",
          "speaker_id": "1",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100037053.1.19",
          "form": "헐 저 한 번도 안 먹어봤는데",
          "original_form": "헐 저 한 번도 안 먹어봤는데",
          "speaker_id": "2",
          "time": "20210901 13:26"
        },
        {
          "id": "MDRW2100037053.1.20",
          "form": "다음에 기회거 된다면 꼭 먹어봐여겠어요",
          "original_form": "다음에 기회거 된다면 꼭 먹어봐여겠어요",
          "speaker_id": "2",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100037053.1.21",
          "form": "저 꾸덕한 거 좋아하거든요",
          "original_form": "저 꾸덕한 거 좋아하거든요",
          "speaker_id": "2",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100037053.1.22",
          "form": "추천합니다 ㅋㅋㅋㅋ",
          "original_form": "추천합니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100037053.1.23",
          "form": "요새 코로나때문에 애슐리같은곳 갈 일이 없긴 하지만..",
