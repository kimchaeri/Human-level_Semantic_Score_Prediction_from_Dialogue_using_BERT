      "utterance": [
        {
          "id": "MDRW2100043148.1.1",
          "form": "지금 기억에 남는 단어 오겹살",
          "original_form": "지금 기억에 남는 단어 오겹살",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043148.1.2",
          "form": "ㅋㅋㅋㅋㅋ 했습니다",
          "original_form": "ㅋㅋㅋㅋㅋ 했습니다",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043148.1.3",
          "form": "비 살짝 오니까 기름진거 좋네요",
          "original_form": "비 살짝 오니까 기름진거 좋네요",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043148.1.4",
          "form": "저도 오겹살 ! 킬포",
          "original_form": "저도 오겹살 ! 킬포",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043148.1.5",
          "form": "이번주 주말에는 저도 돼지 좀 구워먹을까봐요",
          "original_form": "이번주 주말에는 저도 돼지 좀 구워먹을까봐요",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043148.1.6",
          "form": "오늘은 뭘 먹어야 되나 고민좀 해봐야겠어여 ㅋㅋㅋ",
          "original_form": "오늘은 뭘 먹어야 되나 고민좀 해봐야겠어여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100043148.1.7",
          "form": "요즘 고기가 너무 비싸여ㅠ",
          "original_form": "요즘 고기가 너무 비싸여ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043148.1.8",
          "form": "아 그 국민재난지원금 나와서 고기값 오를거라고 했어요",
          "original_form": "아 그 국민재난지원금 나와서 고기값 오를거라고 했어요",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043148.1.9",
          "form": "이런 ㅠㅠ 그전에도 근데 비싸긴 했는데",
          "original_form": "이런 ㅠㅠ 그전에도 근데 비싸긴 했는데",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043148.1.10",
          "form": "또 올랐나보네요 ㅜㅜ",
          "original_form": "또 올랐나보네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043148.1.11",
          "form": "소고 돼지고 다 오른다고 그러더라고요",
          "original_form": "소고 돼지고 다 오른다고 그러더라고요",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043148.1.12",
          "form": "하아ㅠㅠㅠ 싫으네여ㅠㅠㅠ",
          "original_form": "하아ㅠㅠㅠ 싫으네여ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043148.1.13",
          "form": "주면 뭐해요 또 올려서 받으니 또이또이",
          "original_form": "주면 뭐해요 또 올려서 받으니 또이또이",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043148.1.14",
          "form": "온라인 안되니 좀 불편해여ㅠ 지역상생도 일리는 ㄴ있는데 ㅠㅠ",
          "original_form": "온라인 안되니 좀 불편해여ㅠ 지역상생도 일리는 ㄴ있는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043148.1.15",
          "form": "요즘같은 시국에 배달 자주 먹는데 만나서 결제할라니 찝찝",
          "original_form": "요즘같은 시국에 배달 자주 먹는데 만나서 결제할라니 찝찝",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043148.1.16",
          "form": "맞아요 온라인이 좋은물건에 더 싸기도 해서 자주 쓰거든요",
          "original_form": "맞아요 온라인이 좋은물건에 더 싸기도 해서 자주 쓰거든요",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043148.1.17",
          "form": "그냥 안경같은걸로 결제하는게 제일 좋을거 같기도 해요",
          "original_form": "그냥 안경같은걸로 결제하는게 제일 좋을거 같기도 해요",
          "speaker_id": "2",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043148.1.18",
          "form": "이랬는데 안경도 시세올려서 받을거 같기도 해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이랬는데 안경도 시세올려서 받을거 같기도 해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043148.1.19",
          "form": "그니까여 ㅠ 이런",
          "original_form": "그니까여 ㅠ 이런",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043148.1.20",
          "form": "그래서 병원 많이들 가시는거 같아요",
          "original_form": "그래서 병원 많이들 가시는거 같아요",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043148.1.21",
          "form": "시장에서 쓰면 좋긴한데 전 사실 시장도 자주 안가서ㅠㅠ",
          "original_form": "시장에서 쓰면 좋긴한데 전 사실 시장도 자주 안가서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043148.1.22",
          "form": "근데 또 튀김같은거 사먹으러는 가여 ㅎㅎㅎㅎㅎㅎ",
          "original_form": "근데 또 튀김같은거 사먹으러는 가여 ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043148.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ 아 튀김 맛있져",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아 튀김 맛있져",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043148.1.24",
          "form": "저도 시장은 잘 안가게되더라고요",
          "original_form": "저도 시장은 잘 안가게되더라고요",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043148.1.25",
          "form": "농산물은 좀 괜찮은 집도 있는데",
          "original_form": "농산물은 좀 괜찮은 집도 있는데",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043148.1.26",
          "form": "시장가면 빨간오뎅 사먹고 튀김 사먹고",
          "original_form": "시장가면 빨간오뎅 사먹고 튀김 사먹고",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043148.1.27",
          "form": "괜찮으면 또 그만큼 비싸요 ㅋㅋㅋ",
          "original_form": "괜찮으면 또 그만큼 비싸요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043148.1.28",
          "form": "캬 빨간오뎅 맛있겠어요",
          "original_form": "캬 빨간오뎅 맛있겠어요",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043148.1.29",
          "form": "노나츠 사먹고 그재민데ㅠ 코로나땜에 뭐 ㅠㅠ",
          "original_form": "노나츠 사먹고 그재민데ㅠ 코로나땜에 뭐 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043148.1.30",
          "form": "아니 오뎅이 하나에 800원해여ㅠ",
          "original_form": "아니 오뎅이 하나에 800원해여ㅠ",
