      "utterance": [
        {
          "id": "MDRW2100003821.1.1",
          "form": "애가 둘이면 결혼생활이 길었겠네요?",
          "original_form": "애가 둘이면 결혼생활이 길었겠네요?",
          "speaker_id": "1",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003821.1.2",
          "form": "아뇨 애 하나에요",
          "original_form": "아뇨 애 하나에요",
          "speaker_id": "2",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.3",
          "form": "이제 6살인가",
          "original_form": "이제 6살인가",
          "speaker_id": "2",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.4",
          "form": "아 하나구나",
          "original_form": "아 하나구나",
          "speaker_id": "1",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.5",
          "form": "총 12년 만났대요",
          "original_form": "총 12년 만났대요",
          "speaker_id": "2",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.6",
          "form": "name1님이랑 비슷해요",
          "original_form": "&name1&님이랑 비슷해요",
          "speaker_id": "2",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.7",
          "form": "앗 길게 만났네요",
          "original_form": "앗 길게 만났네요",
          "speaker_id": "1",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.8",
          "form": "대학때 만나서 연애하고 결혼했는데",
          "original_form": "대학때 만나서 연애하고 결혼했는데",
          "speaker_id": "2",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.9",
          "form": "진짜 비슷하네요",
          "original_form": "진짜 비슷하네요",
          "speaker_id": "1",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.10",
          "form": "결혼기간 포함 12년이요ㅋㅋ",
          "original_form": "결혼기간 포함 12년이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.11",
          "form": "와 비슷하다 저랑",
          "original_form": "와 비슷하다 저랑",
          "speaker_id": "1",
          "time": "20210821 12:31"
        },
        {
          "id": "MDRW2100003821.1.12",
          "form": "부모님 때문에 이혼 안하려다가",
          "original_form": "부모님 때문에 이혼 안하려다가",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.13",
          "form": "소송까지 가게됐다그러는거예요",
          "original_form": "소송까지 가게됐다그러는거예요",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.14",
          "form": "앗 그것까지",
          "original_form": "앗 그것까지",
          "speaker_id": "1",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.15",
          "form": "지금도 얘기하면 부모님한테 너무 죄송하다고",
          "original_form": "지금도 얘기하면 부모님한테 너무 죄송하다고",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.16",
          "form": "네 맞아요ㅠㅠ",
          "original_form": "네 맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.17",
          "form": "이혼 소송중에 암까지 걸리셨대요",
          "original_form": "이혼 소송중에 암까지 걸리셨대요",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.18",
          "form": "너무 스트레스받으셔서",
          "original_form": "너무 스트레스받으셔서",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.19",
          "form": "누가요?",
          "original_form": "누가요?",
          "speaker_id": "1",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.20",
          "form": "남친 부모님이요",
          "original_form": "남친 부모님이요",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.21",
          "form": "어머니가",
          "original_form": "어머니가",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.22",
          "form": "헐 맙소사",
          "original_form": "헐 맙소사",
          "speaker_id": "1",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.23",
          "form": "엎친데 덮친격이죠",
          "original_form": "엎친데 덮친격이죠",
          "speaker_id": "2",
          "time": "20210821 12:32"
        },
        {
          "id": "MDRW2100003821.1.24",
          "form": "아 이혼 소송중은 아니다",
          "original_form": "아 이혼 소송중은 아니다",
