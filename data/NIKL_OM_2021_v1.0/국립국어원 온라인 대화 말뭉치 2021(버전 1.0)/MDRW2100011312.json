      "utterance": [
        {
          "id": "MDRW2100011312.1.1",
          "form": "가고 싶은 여행지 있으신가요",
          "original_form": "가고 싶은 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210824 11:02"
        },
        {
          "id": "MDRW2100011312.1.2",
          "form": "저는 미국가고싶어여ㅠㅠ",
          "original_form": "저는 미국가고싶어여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100011312.1.3",
          "form": "미국 어디요?",
          "original_form": "미국 어디요?",
          "speaker_id": "2",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100011312.1.4",
          "form": "아 참고로 저는 해외를 나가본 적이 없어서 ...",
          "original_form": "아 참고로 저는 해외를 나가본 적이 없어서 ...",
          "speaker_id": "2",
          "time": "20210824 11:03"
        },
        {
          "id": "MDRW2100011312.1.5",
          "form": "뉴욕 가보고싶네요 언제쯤 갈수있을지ㅠ",
          "original_form": "뉴욕 가보고싶네요 언제쯤 갈수있을지ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:04"
        },
        {
          "id": "MDRW2100011312.1.6",
          "form": "뉴욕 너무 멋있는 도시죠",
          "original_form": "뉴욕 너무 멋있는 도시죠",
          "speaker_id": "2",
          "time": "20210824 11:05"
        },
        {
          "id": "MDRW2100011312.1.7",
          "form": "타임스퀘어 너무 멋있어요",
          "original_form": "타임스퀘어 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210824 11:10"
        },
        {
          "id": "MDRW2100011312.1.8",
          "form": "맞아요 타임스퀘어 너무 멋지더라구요",
          "original_form": "맞아요 타임스퀘어 너무 멋지더라구요",
          "speaker_id": "2",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100011312.1.9",
          "form": "전광판이 24시가 꺼지지 않는다고 하더라구요",
          "original_form": "전광판이 24시가 꺼지지 않는다고 하더라구요",
          "speaker_id": "2",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100011312.1.10",
          "form": "와 정말요?? 역시 미국이네요",
          "original_form": "와 정말요?? 역시 미국이네요",
          "speaker_id": "1",
          "time": "20210824 11:14"
        },
        {
          "id": "MDRW2100011312.1.11",
          "form": "저한테도 한번 물어봐 주시겠어요 어디 가고 싶은지",
          "original_form": "저한테도 한번 물어봐 주시겠어요 어디 가고 싶은지",
          "speaker_id": "2",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100011312.1.12",
          "form": "버킷리스트 있으세요? 여행지",
          "original_form": "버킷리스트 있으세요? 여행지",
          "speaker_id": "1",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100011312.1.13",
          "form": "저는 스페인에 가고 싶어요",
          "original_form": "저는 스페인에 가고 싶어요",
          "speaker_id": "2",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100011312.1.14",
          "form": "가우디 보고 싶어서요",
          "original_form": "가우디 보고 싶어서요",
          "speaker_id": "2",
          "time": "20210824 11:21"
        },
        {
          "id": "MDRW2100011312.1.15",
          "form": "오 스페인 좋네요 바르셀로나",
          "original_form": "오 스페인 좋네요 바르셀로나",
