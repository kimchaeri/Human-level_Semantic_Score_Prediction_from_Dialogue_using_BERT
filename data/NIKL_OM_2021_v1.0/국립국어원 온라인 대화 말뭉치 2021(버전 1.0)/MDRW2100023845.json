      "utterance": [
        {
          "id": "MDRW2100023845.1.1",
          "form": "안녕하세용 저녁은 드셨어요?",
          "original_form": "안녕하세용 저녁은 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100023845.1.2",
          "form": "아직 안먹었네요 !",
          "original_form": "아직 안먹었네요 !",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023845.1.3",
          "form": "저녁 드셨나요.?~",
          "original_form": "저녁 드셨나요.?~",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023845.1.4",
          "form": "넹넹 방금 카레 먹고왔어용",
          "original_form": "넹넹 방금 카레 먹고왔어용",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023845.1.5",
          "form": "카레 좋아하세요?",
          "original_form": "카레 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023845.1.6",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023845.1.7",
          "form": "카레 맛있죠",
          "original_form": "카레 맛있죠",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023845.1.8",
          "form": "해드셨나요? 저는 할줄은 몰라요",
          "original_form": "해드셨나요? 저는 할줄은 몰라요",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023845.1.9",
          "form": "넹넹 저는 해먹었어요ㅎㅎ",
          "original_form": "넹넹 저는 해먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023845.1.10",
          "form": "저녁은 어떤거 드실 예정이세요~?",
          "original_form": "저녁은 어떤거 드실 예정이세요~?",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023845.1.11",
          "form": "오 요리 잘하시나봐요ㅎㅎ",
          "original_form": "오 요리 잘하시나봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023845.1.12",
          "form": "김치찌개 먹을 생각입니다ㅎ",
          "original_form": "김치찌개 먹을 생각입니다ㅎ",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023845.1.13",
          "form": "김치찌개에 참치를 더 좋아하세요",
          "original_form": "김치찌개에 참치를 더 좋아하세요",
          "speaker_id": "1",
          "time": "20210830 18:52"
        },
        {
          "id": "MDRW2100023845.1.14",
          "form": "돼지고기를 더 좋아하세요?",
          "original_form": "돼지고기를 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 18:52"
        },
        {
          "id": "MDRW2100023845.1.15",
          "form": "돼지고기요ㅋㅋㅋ",
          "original_form": "돼지고기요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023845.1.16",
          "form": "오 고기파이시군용!",
          "original_form": "오 고기파이시군용!",
          "speaker_id": "1",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023845.1.17",
          "form": "김치찌개는 직접 요리하시는거에용?",
          "original_form": "김치찌개는 직접 요리하시는거에용?",
          "speaker_id": "1",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023845.1.18",
          "form": "고기가 맛있죠! 넵 이제 하려고요",
          "original_form": "고기가 맛있죠! 넵 이제 하려고요",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023845.1.19",
          "form": "김치만 맛있어도 김치찌개 반은 이미 된거더라구요ㅋㅋㅋ",
          "original_form": "김치만 맛있어도 김치찌개 반은 이미 된거더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023845.1.20",
          "form": "그쳐 김치찌개 말고 전 요리 못해여ㅎㅎ",
          "original_form": "그쳐 김치찌개 말고 전 요리 못해여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100023845.1.21",
          "form": "김치찌개도 못하는사람 요즘 많아요!ㅎㅎ 저 포함ㅋㅋㅋㅋ 전 김치찌개는 못한답니당",
          "original_form": "김치찌개도 못하는사람 요즘 많아요!ㅎㅎ 저 포함ㅋㅋㅋㅋ 전 김치찌개는 못한답니당",
