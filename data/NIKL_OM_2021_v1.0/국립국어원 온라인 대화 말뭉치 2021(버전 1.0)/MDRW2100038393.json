      "utterance": [
        {
          "id": "MDRW2100038393.1.1",
          "form": "네네 자주 보는 편인 것 같은데",
          "original_form": "네네 자주 보는 편인 것 같은데",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100038393.1.2",
          "form": "우리나라 드라마는 최근에 시그널 봤어요!",
          "original_form": "우리나라 드라마는 최근에 시그널 봤어요!",
          "speaker_id": "2",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038393.1.3",
          "form": "name1님은요~?",
          "original_form": "&name1&님은요~?",
          "speaker_id": "2",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038393.1.4",
          "form": "저는 로스쿨이랑 마인! 봤어요!",
          "original_form": "저는 로스쿨이랑 마인! 봤어요!",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100038393.1.5",
          "form": "아시나요!?",
          "original_form": "아시나요!?",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100038393.1.6",
          "form": "오 알죠알죠",
          "original_form": "오 알죠알죠",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038393.1.7",
          "form": "완전 핫한 드라마!!!",
          "original_form": "완전 핫한 드라마!!!",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038393.1.8",
          "form": "넷플릭스에서 볼 수 있는 !!!",
          "original_form": "넷플릭스에서 볼 수 있는 !!!",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038393.1.9",
          "form": "맞아요!!!!!!!!!! 혹시 보셨어요??!?!?!?!",
          "original_form": "맞아요!!!!!!!!!! 혹시 보셨어요??!?!?!?!",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038393.1.10",
          "form": "사실 저는 안봤지만!!!",
          "original_form": "사실 저는 안봤지만!!!",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100038393.1.11",
          "form": "그그그 배우가 좋아서 응원하고 있어요",
          "original_form": "그그그 배우가 좋아서 응원하고 있어요",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100038393.1.12",
          "form": "김서형 배우요!!!!",
          "original_form": "김서형 배우요!!!!",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100038393.1.13",
          "form": "헐 저두요!!! 스카이캐슬에사 보고 완전 반했걸랑요",
          "original_form": "헐 저두요!!! 스카이캐슬에사 보고 완전 반했걸랑요",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100038393.1.14",
          "form": "스카이캐슬 보셨어요!?",
          "original_form": "스카이캐슬 보셨어요!?",
          "speaker_id": "1",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100038393.1.15",
          "form": "사실... 그것도 뜨문뜨문",
          "original_form": "사실... 그것도 뜨문뜨문",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038393.1.16",
          "form": "영상클립으로만 접하고",
          "original_form": "영상클립으로만 접하고",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038393.1.17",
          "form": "제대로 보진 않았네요 ㅠㅠ",
          "original_form": "제대로 보진 않았네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038393.1.18",
          "form": "왜이렇게... 요즘 사람들 보는 걸 안보는지 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "왜이렇게... 요즘 사람들 보는 걸 안보는지 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038393.1.19",
          "form": "앗!! 그럴 수 잇죠 ㅎ.ㅎ !",
          "original_form": "앗!! 그럴 수 잇죠 ㅎ.ㅎ !",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100038393.1.20",
          "form": "name2님 드라마 보신 거 뭐 있어요?!?!!",
          "original_form": "&name2&님 드라마 보신 거 뭐 있어요?!?!!",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100038393.1.21",
          "form": "앜ㅋㅋㅋ 한국 드라마는 진짜 거의 안봤고",
          "original_form": "앜ㅋㅋㅋ 한국 드라마는 진짜 거의 안봤고",
          "speaker_id": "2",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100038393.1.22",
          "form": "넷플릭스에서 블랙미러 이제야 보고 있어요 ",
          "original_form": "넷플릭스에서 블랙미러 이제야 보고 있어요 ",
          "speaker_id": "2",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100038393.1.23",
          "form": "아 브리저튼은 다 봤어요!!!!",
          "original_form": "아 브리저튼은 다 봤어요!!!!",
          "speaker_id": "2",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100038393.1.24",
          "form": "헛 브리저튼 저 드믄드믄 봣는데!!",
          "original_form": "헛 브리저튼 저 드믄드믄 봣는데!!",
          "speaker_id": "1",
          "time": "20210902 12:58"
        },
        {
          "id": "MDRW2100038393.1.25",
          "form": "재밌더라고요! 계약 연애하다가",
          "original_form": "재밌더라고요! 계약 연애하다가",
          "speaker_id": "1",
          "time": "20210902 12:58"
        },
        {
          "id": "MDRW2100038393.1.26",
          "form": "진짜 눈맞는 거 아닌가요?!!",
          "original_form": "진짜 눈맞는 거 아닌가요?!!",
          "speaker_id": "1",
          "time": "20210902 12:58"
        },
        {
