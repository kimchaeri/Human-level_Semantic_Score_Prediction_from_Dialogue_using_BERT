      "utterance": [
        {
          "id": "MDRW2100007018.1.1",
          "form": "안녕하세요 ",
          "original_form": "안녕하세요 ",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007018.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007018.1.3",
          "form": "종교가 뭐에요? 천주교? 기독교?",
          "original_form": "종교가 뭐에요? 천주교? 기독교?",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007018.1.4",
          "form": "전 천주교요",
          "original_form": "전 천주교요",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007018.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007018.1.6",
          "form": "전 교회 다녀봤는데 안맞아서 안다닌지 오래 됐어요",
          "original_form": "전 교회 다녀봤는데 안맞아서 안다닌지 오래 됐어요",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007018.1.7",
          "form": "아 저도 친구따라 가본적 있긴 있어요",
          "original_form": "아 저도 친구따라 가본적 있긴 있어요",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007018.1.8",
          "form": "한국 교회는 부정부패 이미지도 있어서 뭔가 믿음이 잘 안가더라구요",
          "original_form": "한국 교회는 부정부패 이미지도 있어서 뭔가 믿음이 잘 안가더라구요",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007018.1.9",
          "form": "아 그런 부분도 없지않아 있죠",
          "original_form": "아 그런 부분도 없지않아 있죠",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007018.1.10",
          "form": "이슬람교는 진짜 미스테리하지 않아요?",
          "original_form": "이슬람교는 진짜 미스테리하지 않아요?",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007018.1.11",
          "form": "이슬람교인들? 겪어본적이 없어요",
          "original_form": "이슬람교인들? 겪어본적이 없어요",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007018.1.12",
          "form": "맹신하는 종교가 있다는게 어쩌면 기대고싶은 마음의 투영일수 있잖아요",
          "original_form": "맹신하는 종교가 있다는게 어쩌면 기대고싶은 마음의 투영일수 있잖아요",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007018.1.13",
          "form": "대부분 심적으로 힘든시기에 종교에 푹 빠지게 되는 케이스 많이 봤어요",
          "original_form": "대부분 심적으로 힘든시기에 종교에 푹 빠지게 되는 케이스 많이 봤어요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007018.1.14",
          "form": "불교나 알라신에 대해 얼마나 아세요?",
          "original_form": "불교나 알라신에 대해 얼마나 아세요?",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007018.1.15",
          "form": "불교요? 절에 가본적 있는데",
          "original_form": "불교요? 절에 가본적 있는데",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007018.1.16",
          "form": "확실히 불교는 무소유 개념이 있어서",
          "original_form": "확실히 불교는 무소유 개념이 있어서",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007018.1.17",
          "form": "그쵸 불교는 심적으로 편안해지는 무언가가 있어요",
          "original_form": "그쵸 불교는 심적으로 편안해지는 무언가가 있어요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007018.1.18",
          "form": "기독교가 큰 개념이고 천주교도 기독교에 포함되는거 맞죠?",
          "original_form": "기독교가 큰 개념이고 천주교도 기독교에 포함되는거 맞죠?",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007018.1.19",
          "form": "네 기독교에 포함되는 개념 맞아요",
          "original_form": "네 기독교에 포함되는 개념 맞아요",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007018.1.20",
          "form": "레바논이 기독교였는데 이슬람으로 바뀌게 된 이유 아세요?",
          "original_form": "레바논이 기독교였는데 이슬람으로 바뀌게 된 이유 아세요?",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007018.1.21",
          "form": "아뇨 뭔데요?",
          "original_form": "아뇨 뭔데요?",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007018.1.22",
          "form": "무슬림들을 받아들였기때문이래요",
          "original_form": "무슬림들을 받아들였기때문이래요",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007018.1.23",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007018.1.24",
          "form": "레바논은 인도주의적으로 무슬림을 받아들였는데",
          "original_form": "레바논은 인도주의적으로 무슬림을 받아들였는데",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007018.1.25",
          "form": "그 사람들이 아이를 5명이싱 낳게하고 중혼도 가능해서 아이를 많이 낳은거죠",
          "original_form": "그 사람들이 아이를 5명이싱 낳게하고 중혼도 가능해서 아이를 많이 낳은거죠",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007018.1.26",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007018.1.27",
          "form": "근데 레바논은 인도주의적인 차원으로 팔레스타인도 받아들였는데 무슬림들끼리 합쳐서 전쟁일으키고 레바논은 나라가 망가진거죠",
          "original_form": "근데 레바논은 인도주의적인 차원으로 팔레스타인도 받아들였는데 무슬림들끼리 합쳐서 전쟁일으키고 레바논은 나라가 망가진거죠",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007018.1.28",
          "form": "와 소름이네요 무슬림들끼리 그랬다니",
          "original_form": "와 소름이네요 무슬림들끼리 그랬다니",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007018.1.29",
          "form": "태어나면서부터 세뇌당해서 어린애고 여자고 다를거 없겠네요 안그래도 살기 무서운데 휴",
          "original_form": "태어나면서부터 세뇌당해서 어린애고 여자고 다를거 없겠네요 안그래도 살기 무서운데 휴",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007018.1.30",
          "form": "네 종교전쟁이 괜히 있던게 아니죠",
          "original_form": "네 종교전쟁이 괜히 있던게 아니죠",
          "speaker_id": "1",
          "time": "20210822 00:32"
        }
      ]
    }
