      "utterance": [
        {
          "id": "MDRW2100042499.1.1",
          "form": "너 니가 생각했을때 젤 맛없는 음식이 뭐야",
          "original_form": "너 니가 생각했을때 젤 맛없는 음식이 뭐야",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042499.1.2",
          "form": "내가 야매로 만들었다가 실패한 거 말고",
          "original_form": "내가 야매로 만들었다가 실패한 거 말고",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042499.1.3",
          "form": "정식 메뉴 중에서?",
          "original_form": "정식 메뉴 중에서?",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042499.1.4",
          "form": "응 궁금해",
          "original_form": "응 궁금해",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042499.1.5",
          "form": "삭힌 홍어...",
          "original_form": "삭힌 홍어...",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042499.1.6",
          "form": "도대체가 얼마나 맛없길래",
          "original_form": "도대체가 얼마나 맛없길래",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042499.1.7",
          "form": "냄새가 너무 괴로워서 그거 먹은 사람 곁에 못 가겠어",
          "original_form": "냄새가 너무 괴로워서 그거 먹은 사람 곁에 못 가겠어",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042499.1.8",
          "form": "어휴 세상에",
          "original_form": "어휴 세상에",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042499.1.9",
          "form": "이전에 우리집 부가 회식으로 먹고왔는데",
          "original_form": "이전에 우리집 부가 회식으로 먹고왔는데",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042499.1.10",
          "form": "일주일동안 곁에 가지도 않았어",
          "original_form": "일주일동안 곁에 가지도 않았어",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042499.1.11",
          "form": "원체 안 가지만",
          "original_form": "원체 안 가지만",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042499.1.12",
          "form": "더 거리두기 했어",
          "original_form": "더 거리두기 했어",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042499.1.13",
          "form": "냄새 독하나봐",
          "original_form": "냄새 독하나봐",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042499.1.14",
          "form": "오줌밭에 뒹굴고 안 씻은 냄새 나",
          "original_form": "오줌밭에 뒹굴고 안 씻은 냄새 나",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042499.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042499.1.16",
          "form": "그럼 만들어 먹은 괴식중에 최악인건?",
          "original_form": "그럼 만들어 먹은 괴식중에 최악인건?",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042499.1.17",
          "form": "바나나랑 딸기요거트 갈은 거",
          "original_form": "바나나랑 딸기요거트 갈은 거",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042499.1.18",
          "form": "딸기바나나와 달라",
          "original_form": "딸기바나나와 달라",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042499.1.19",
          "form": "발냄새 나",
          "original_form": "발냄새 나",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042499.1.20",
          "form": "ㅋㅋㅋ듣기에는 정상인데",
          "original_form": "ㅋㅋㅋ듣기에는 정상인데",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042499.1.21",
          "form": "맛있는 거끼리 섞으면 맛있을 줄 알았어",
          "original_form": "맛있는 거끼리 섞으면 맛있을 줄 알았어",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042499.1.22",
          "form": "내 손으로 만든 건 웬만하면 다 먹는 편인데 그건 최초로 한 입 먹고 다 버렸어",
          "original_form": "내 손으로 만든 건 웬만하면 다 먹는 편인데 그건 최초로 한 입 먹고 다 버렸어",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042499.1.23",
          "form": "나는 오이넣은 볶음밥",
          "original_form": "나는 오이넣은 볶음밥",
          "speaker_id": "2",
          "time": "20210906 22:33"
        }
      ]
    }
