      "utterance": [
        {
          "id": "MDRW2100004124.1.1",
          "form": "반려동물을 기르시나요",
          "original_form": "반려동물을 기르시나요",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004124.1.2",
          "form": "제가 키우는 건 아니지만 동생이 키워요",
          "original_form": "제가 키우는 건 아니지만 동생이 키워요",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004124.1.3",
          "form": "가끔 같이 놀러와요",
          "original_form": "가끔 같이 놀러와요",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004124.1.4",
          "form": "name1님은 반려동물 있나요?",
          "original_form": "&name1&님은 반려동물 있나요?",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004124.1.5",
          "form": "고양이 키우고있어요",
          "original_form": "고양이 키우고있어요",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004124.1.6",
          "form": "고양이 좋아하시나요?",
          "original_form": "고양이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004124.1.7",
          "form": "저희집은 개 키워요",
          "original_form": "저희집은 개 키워요",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004124.1.8",
          "form": "고양이도 좋아해요",
          "original_form": "고양이도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004124.1.9",
          "form": "강아지 이름이 뭐에요?",
          "original_form": "강아지 이름이 뭐에요?",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004124.1.10",
          "form": "제 고양이는 name3에요",
          "original_form": "제 고양이는 &name3&에요",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004124.1.11",
          "form": "요즘 유튜브로 고양이 많이 봐서 관심 있어요",
          "original_form": "요즘 유튜브로 고양이 많이 봐서 관심 있어요",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004124.1.12",
          "form": "강아지 이름은 name4예요 갈색 푸들이거든요",
          "original_form": "강아지 이름은 &name4&예요 갈색 푸들이거든요",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004124.1.13",
          "form": "귀여워라",
          "original_form": "귀여워라",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004124.1.14",
          "form": "name3요? 포실포실한가봐요",
          "original_form": "&name3&요? 포실포실한가봐요",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004124.1.15",
          "form": "아니요 그건 아니고",
          "original_form": "아니요 그건 아니고",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004124.1.16",
          "form": "주워왔거든요",
          "original_form": "주워왔거든요",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004124.1.17",
          "form": "냥줍이었구나",
          "original_form": "냥줍이었구나",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004124.1.18",
          "form": "겨울에 얼어죽을것 같아 데려왔는데 제가 작명엔 자신이없어서ㅋㅋ",
          "original_form": "겨울에 얼어죽을것 같아 데려왔는데 제가 작명엔 자신이없어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004124.1.19",
          "form": "name3 먹다 name3가 되다니 신기하네요",
          "original_form": "&name3& 먹다 &name3&가 되다니 신기하네요",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004124.1.20",
          "form": "다행이네요 구해져서...",
          "original_form": "다행이네요 구해져서...",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004124.1.21",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004124.1.22",
          "form": "푸들은 똑똑하다던데 진짜인가요?",
          "original_form": "푸들은 똑똑하다던데 진짜인가요?",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004124.1.23",
          "form": "똑똑하긴한데 모르는 척 잘해요",
          "original_form": "똑똑하긴한데 모르는 척 잘해요",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004124.1.24",
          "form": "예를들면?",
          "original_form": "예를들면?",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004124.1.25",
          "form": "고양이는 몇살이에요?",
          "original_form": "고양이는 몇살이에요?",
          "speaker_id": "2",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004124.1.26",
          "form": "올해 4살이요",
          "original_form": "올해 4살이요",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004124.1.27",
          "form": "사람같아요",
          "original_form": "사람같아요",
          "speaker_id": "2",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004124.1.28",
          "form": "4살 애기네요 ㅋㅋㅋㅋㅋ",
          "original_form": "4살 애기네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004124.1.29",
          "form": "푸들은 몇살이에요?",
          "original_form": "푸들은 몇살이에요?",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004124.1.30",
          "form": "9살쯤 되었어요 능글 맞은 아재예요 그래도 얼굴은 애기지만 ㅋㅋㅋ",
          "original_form": "9살쯤 되었어요 능글 맞은 아재예요 그래도 얼굴은 애기지만 ㅋㅋㅋ",
