      "utterance": [
        {
          "id": "MDRW2100038781.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100038781.1.2",
          "form": "저녁식사 하셨나요?",
          "original_form": "저녁식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038781.1.3",
          "form": "네 김밥 먹었어요",
          "original_form": "네 김밥 먹었어요",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038781.1.4",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038781.1.5",
          "form": "네 저도 식사했어요ㅎㅎ",
          "original_form": "네 저도 식사했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038781.1.6",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038781.1.7",
          "form": "저는 된장찌개를 먹었어요ㅎㅎ",
          "original_form": "저는 된장찌개를 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038781.1.8",
          "form": "직접 만드신건가요?",
          "original_form": "직접 만드신건가요?",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038781.1.9",
          "form": "엄마가 만들어주셨어요",
          "original_form": "엄마가 만들어주셨어요",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038781.1.10",
          "form": "와 맛있겠네요",
          "original_form": "와 맛있겠네요",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038781.1.11",
          "form": "전 그냥 사다먹었어요 ㅋㅋㅋ",
          "original_form": "전 그냥 사다먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038781.1.12",
          "form": "네 맛있었어요",
          "original_form": "네 맛있었어요",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038781.1.13",
          "form": "사먹는게 편하긴하죠ㅋㅋㅋ",
          "original_form": "사먹는게 편하긴하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038781.1.14",
          "form": "김밥싸는거 은근 귀찮아요ㅜㅜ",
          "original_form": "김밥싸는거 은근 귀찮아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038781.1.15",
          "form": "그렇긴 한데 최근에 난리나서",
          "original_form": "그렇긴 한데 최근에 난리나서",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038781.1.16",
          "form": "좀 미심쩍은 기분으로 사먹게 되더라규요 ㅋㅋㅋ ㅠ",
          "original_form": "좀 미심쩍은 기분으로 사먹게 되더라규요 ㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038781.1.17",
          "form": "아 맞아요!",
          "original_form": "아 맞아요!",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038781.1.18",
          "form": "올 여름에 김밥으로 인한 일들이 있었죠ㅜㅜ",
          "original_form": "올 여름에 김밥으로 인한 일들이 있었죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038781.1.19",
          "form": "네네 ㅠ 그거 보고나니까",
          "original_form": "네네 ㅠ 그거 보고나니까",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038781.1.20",
          "form": "사먹기가 찝찝해지더라구요..",
          "original_form": "사먹기가 찝찝해지더라구요..",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038781.1.21",
          "form": "맞아요 그런 소식 듣고나면 좀 찝찝한 느낌이 있어요ㅜㅜ 조심스러워지고..",
          "original_form": "맞아요 그런 소식 듣고나면 좀 찝찝한 느낌이 있어요ㅜㅜ 조심스러워지고..",
          "speaker_id": "1",
          "time": "20210902 21:31"
        }
