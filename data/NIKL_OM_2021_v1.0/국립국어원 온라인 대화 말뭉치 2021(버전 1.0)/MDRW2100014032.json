      "utterance": [
        {
          "id": "MDRW2100014032.1.1",
          "form": "님은 뭐키우나요",
          "original_form": "님은 뭐키우나요",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.2",
          "form": "근데 원래 닭은 뭘로 씻겨요?",
          "original_form": "근데 원래 닭은 뭘로 씻겨요?",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.3",
          "form": "넘 내얘기만햇네",
          "original_form": "넘 내얘기만햇네",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.4",
          "form": "닭은 원래 모래요",
          "original_form": "닭은 원래 모래요",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.5",
          "form": "아 저는 키우는게 없습니다",
          "original_form": "아 저는 키우는게 없습니다",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.6",
          "form": "모래?",
          "original_form": "모래?",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.7",
          "form": "모래 주면 지가 혼자 씻는댜요",
          "original_form": "모래 주면 지가 혼자 씻는댜요",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.9",
          "form": "똑똑하네",
          "original_form": "똑똑하네",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.10",
          "form": "근데 그아이를 엘라스틴으로..",
          "original_form": "근데 그아이를 엘라스틴으로..",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.11",
          "form": "조금 더 향긋해졌군요",
          "original_form": "조금 더 향긋해졌군요",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014032.1.12",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014032.1.13",
          "form": "글고 원래 닭 가슴 털은",
          "original_form": "글고 원래 닭 가슴 털은",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014032.1.14",
          "form": "가슴이랑 궁댕이 털은 보숭보숭하답니다",
          "original_form": "가슴이랑 궁댕이 털은 보숭보숭하답니다",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014032.1.15",
          "form": "보숭보숭",
          "original_form": "보숭보숭",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014032.1.16",
          "form": "너무 귀여워 닭",
          "original_form": "너무 귀여워 닭",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014032.1.17",
          "form": "넘 귀엽다",
          "original_form": "넘 귀엽다",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014032.1.18",
          "form": "그 닭",
          "original_form": "그 닭",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014032.1.19",
          "form": "머리랑 턱에",
          "original_form": "머리랑 턱에",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014032.1.20",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014032.1.21",
          "form": "벼슬이요",
          "original_form": "벼슬이요",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014032.1.22",
          "form": "촉감 어때요",
          "original_form": "촉감 어때요",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014032.1.23",
          "form": "맞아 벼슬이요",
          "original_form": "맞아 벼슬이요",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014032.1.24",
          "form": "벼슬이 턱에 달린건 말랑말랑하고 벼슬은 딱딱해요",
          "original_form": "벼슬이 턱에 달린건 말랑말랑하고 벼슬은 딱딱해요",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014032.1.25",
          "form": "그래서 둘이 만지면 재밋답니다",
          "original_form": "그래서 둘이 만지면 재밋답니다",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014032.1.26",
          "form": "혼자만 재미보다니",
          "original_form": "혼자만 재미보다니",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014032.1.27",
          "form": "턱 벼슬 만지면 닭 입이 벌어진답니다",
          "original_form": "턱 벼슬 만지면 닭 입이 벌어진답니다",
          "speaker_id": "2",
          "time": "20210825 16:58"
        }
