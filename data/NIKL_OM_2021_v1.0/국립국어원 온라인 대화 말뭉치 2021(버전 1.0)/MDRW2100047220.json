      "utterance": [
        {
          "id": "MDRW2100047220.1.1",
          "form": "맞아요 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100047220.1.2",
          "form": "코로나땜에 배달이나 이런걸",
          "original_form": "코로나땜에 배달이나 이런걸",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100047220.1.3",
          "form": "더 많이 이용하는 것 같아요",
          "original_form": "더 많이 이용하는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100047220.1.4",
          "form": "ㅜㅜ쇼핑을어케해야 쓰레기가 덜나오는지 고민하는데 쉽지가않아요",
          "original_form": "ㅜㅜ쇼핑을어케해야 쓰레기가 덜나오는지 고민하는데 쉽지가않아요",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047220.1.5",
          "form": "ㅠㅠㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요... 특히 요즘같은때는",
          "original_form": "ㅠㅠㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요... 특히 요즘같은때는",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047220.1.6",
          "form": "마스크도 맨날 쓰레기로 나오니까...",
          "original_form": "마스크도 맨날 쓰레기로 나오니까...",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100047220.1.7",
          "form": "코로나로 쓰레기가 늘어나는데 바이러스문제라 이게 겉잡을수없네요ㅜ",
          "original_form": "코로나로 쓰레기가 늘어나는데 바이러스문제라 이게 겉잡을수없네요ㅜ",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100047220.1.8",
          "form": "그니까요 ㅠㅠㅠ...",
          "original_form": "그니까요 ㅠㅠㅠ...",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100047220.1.9",
          "form": "그래두 제로웨이스트샵이 은근 늘었어요",
          "original_form": "그래두 제로웨이스트샵이 은근 늘었어요",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100047220.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100047220.1.11",
          "form": "생수병에도 라벨지 다 사라졌잖아요",
          "original_form": "생수병에도 라벨지 다 사라졌잖아요",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100047220.1.12",
          "form": "저희동네 카페는 반찬통가져오면 오백원할인해줘용",
          "original_form": "저희동네 카페는 반찬통가져오면 오백원할인해줘용",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100047220.1.13",
          "form": "와 진짜요?",
          "original_form": "와 진짜요?",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100047220.1.14",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100047220.1.15",
          "form": "브라우니 사러갈때 좋아요",
          "original_form": "브라우니 사러갈때 좋아요",
          "speaker_id": "1",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100047220.1.16",
          "form": "와 진짜로요",
          "original_form": "와 진짜로요",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100047220.1.17",
          "form": "칫솔도 대나무칫솔 요새 나오더래요",
          "original_form": "칫솔도 대나무칫솔 요새 나오더래요",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100047220.1.18",
          "form": "대나무칫솔은 그냥 칫솔이랑",
          "original_form": "대나무칫솔은 그냥 칫솔이랑",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100047220.1.19",
          "form": "뭐가 다른가요??",
          "original_form": "뭐가 다른가요??",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100047220.1.20",
          "form": "플라스틱이 안썩어서",
          "original_form": "플라스틱이 안썩어서",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100047220.1.21",
          "form": "제가 버린 칫솔이 아마 저보다 더 오래존재할꺼에요ㅋㅋ",
          "original_form": "제가 버린 칫솔이 아마 저보다 더 오래존재할꺼에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100047220.1.22",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100047220.1.23",
          "form": "칫솔모는 같아요?",
          "original_form": "칫솔모는 같아요?",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100047220.1.24",
          "form": "고거는 제품마다 달라용",
          "original_form": "고거는 제품마다 달라용",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100047220.1.25",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100047220.1.26",
          "form": "대나무칫솔은 처음들어봤어요",
          "original_form": "대나무칫솔은 처음들어봤어요",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100047220.1.27",
          "form": "한 번 알아봐야겠네용",
          "original_form": "한 번 알아봐야겠네용",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100047220.1.28",
          "form": "저희동네에는 강아지 빵집이 있어요",
          "original_form": "저희동네에는 강아지 빵집이 있어요",
          "speaker_id": "1",
          "time": "20210826 00:56"
        }
      ]
    }
