      "utterance": [
        {
          "id": "MDRW2100026307.1.1",
          "form": "82년생 김지영. 이거사실 남자가 봐야하는데",
          "original_form": "82년생 김지영. 이거사실 남자가 봐야하는데",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026307.1.2",
          "form": "와와 ㅋㅋ 좋은 팁입니다",
          "original_form": "와와 ㅋㅋ 좋은 팁입니다",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026307.1.3",
          "form": "음... 전남자친구가 매우 거부감이 심하더라구요",
          "original_form": "음... 전남자친구가 매우 거부감이 심하더라구요",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026307.1.4",
          "form": "뭔가 과장되었다는둥",
          "original_form": "뭔가 과장되었다는둥",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026307.1.5",
          "form": "음..  제입장에선",
          "original_form": "음..  제입장에선",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026307.1.6",
          "form": "제가 안당했지만",
          "original_form": "제가 안당했지만",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026307.1.7",
          "form": "정말 내가 보는것만해도 저렇게 사는 사람 많은데 ㅠ",
          "original_form": "정말 내가 보는것만해도 저렇게 사는 사람 많은데 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026307.1.8",
          "form": "왜 이렇게 사고차이가 나는지 알고싶었는데",
          "original_form": "왜 이렇게 사고차이가 나는지 알고싶었는데",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026307.1.9",
          "form": "또 답을 찾으면... 권리를 잃고싶지않아서..라고만하니까",
          "original_form": "또 답을 찾으면... 권리를 잃고싶지않아서..라고만하니까",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026307.1.10",
          "form": "네. 남자들은 그런생각을 해볼필요가 없었죠",
          "original_form": "네. 남자들은 그런생각을 해볼필요가 없었죠",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026307.1.11",
          "form": "할말이없더라구요",
          "original_form": "할말이없더라구요",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026307.1.12",
          "form": "사람은 자리유리한대로 사는게 당연하니까 ㅠ",
          "original_form": "사람은 자리유리한대로 사는게 당연하니까 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026307.1.13",
          "form": "자기유리..",
          "original_form": "자기유리..",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026307.1.14",
          "form": "우린 고작 체육시간 피구만했지",
          "original_form": "우린 고작 체육시간 피구만했지",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026307.1.15",
          "form": "축구.야구도 안시켜주고",
          "original_form": "축구.야구도 안시켜주고",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026307.1.16",
          "form": "김지영 봐도 모를거에요. 당사자가 아니면",
          "original_form": "김지영 봐도 모를거에요. 당사자가 아니면",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026307.1.17",
          "form": "그쵸오 피구만 했어요 축구 시켜주는 체육쌤없어요..",
          "original_form": "그쵸오 피구만 했어요 축구 시켜주는 체육쌤없어요..",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026307.1.18",
          "form": "아..그쵸 본인이 당하지않을테니",
          "original_form": "아..그쵸 본인이 당하지않을테니",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026307.1.19",
          "form": "여자가 깨어있으면 돼요. 안만나주면 되고ㅎㅎ",
          "original_form": "여자가 깨어있으면 돼요. 안만나주면 되고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026307.1.20",
          "form": "이런저런책 읽으면서 많이 배우자구요",
          "original_form": "이런저런책 읽으면서 많이 배우자구요",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026307.1.21",
          "form": "경제도 알고",
          "original_form": "경제도 알고",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026307.1.22",
          "form": "세상이치도 알고",
          "original_form": "세상이치도 알고",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026307.1.23",
          "form": "책도 많이 읽고",
          "original_form": "책도 많이 읽고",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026307.1.24",
          "form": "그렇게 결혼 안한다 애 안 낳는다면서 다시 비난하죠 ㅠ",
          "original_form": "그렇게 결혼 안한다 애 안 낳는다면서 다시 비난하죠 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026307.1.25",
          "form": "4차산업책도 보세요",
          "original_form": "4차산업책도 보세요",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.26",
          "form": "아무튼 자기중심을 잘 잡고 사는게 중요한듯해요",
          "original_form": "아무튼 자기중심을 잘 잡고 사는게 중요한듯해요",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.27",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.28",
          "form": "정해진 미래가있거든요",
          "original_form": "정해진 미래가있거든요",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.29",
          "form": "정해진 미래가 있어요?????",
          "original_form": "정해진 미래가 있어요?????",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.30",
          "form": "전기차도 수소차도 정해진미래",
          "original_form": "전기차도 수소차도 정해진미래",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.31",
          "form": "메타버스도",
          "original_form": "메타버스도",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.32",
          "form": "그런공부하다보면",
          "original_form": "그런공부하다보면",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.33",
          "form": "투자로 연결돼요",
          "original_form": "투자로 연결돼요",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026307.1.34",
          "form": "도서잡지인가 메타버스가ㅎ",
          "original_form": "도서잡지인가 메타버스가ㅎ",
          "speaker_id": "1",
          "time": "20210901 21:35"
        }
      ]
    }
