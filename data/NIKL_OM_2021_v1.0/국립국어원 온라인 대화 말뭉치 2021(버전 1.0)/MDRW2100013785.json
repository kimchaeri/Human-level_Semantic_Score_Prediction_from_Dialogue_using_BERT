      "utterance": [
        {
          "id": "MDRW2100013785.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013785.1.2",
          "form": "네 안녕하세요 ^^",
          "original_form": "네 안녕하세요 ^^",
          "speaker_id": "2",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013785.1.3",
          "form": "단무지없이 자장면 먹기 vs 김치없이 라면먹기 어떠세요?",
          "original_form": "단무지없이 자장면 먹기 vs 김치없이 라면먹기 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013785.1.4",
          "form": "우와...진짜 난제네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우와...진짜 난제네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013785.1.5",
          "form": "그래도 김치없이 라면먹을래용",
          "original_form": "그래도 김치없이 라면먹을래용",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013785.1.6",
          "form": "단무지없이 자장면은 시러요 ㅋㅋㅋ",
          "original_form": "단무지없이 자장면은 시러요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013785.1.7",
          "form": "ㅋㅋㅋ저랑같군요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저랑같군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013785.1.8",
          "form": "저는 원래 김치없이 라면을먹거든요!",
          "original_form": "저는 원래 김치없이 라면을먹거든요!",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013785.1.9",
          "form": "같이먹으면 더 맛있지만 없어도 괜찮을듯욬ㄱㅋ",
          "original_form": "같이먹으면 더 맛있지만 없어도 괜찮을듯욬ㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013785.1.10",
          "form": "생수 사서 드시나요? 요즘 생수 부적합판정때문에",
          "original_form": "생수 사서 드시나요? 요즘 생수 부적합판정때문에",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013785.1.11",
          "form": "난리네요",
          "original_form": "난리네요",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013785.1.12",
          "form": "저 사서먹는데ㅠ",
          "original_form": "저 사서먹는데ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013785.1.13",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013785.1.14",
          "form": "저도봤어요",
          "original_form": "저도봤어요",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013785.1.15",
          "form": "전 보기전에 삼다수 샀었는데 보고나서 삼다수 한번 더 샀네요ㅜ",
          "original_form": "전 보기전에 삼다수 샀었는데 보고나서 삼다수 한번 더 샀네요ㅜ",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013785.1.16",
          "form": "오늘 저녁 메뉴는 뭘 드시나요^^",
          "original_form": "오늘 저녁 메뉴는 뭘 드시나요^^",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013785.1.17",
          "form": "삼다수 근데너무비싸요ㅠㅠ",
          "original_form": "삼다수 근데너무비싸요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013785.1.18",
          "form": "아직고민중이랍니다",
          "original_form": "아직고민중이랍니다",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013785.1.19",
          "form": "뭐드시나요?!",
          "original_form": "뭐드시나요?!",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013785.1.20",
          "form": "저도 고민중인데",
          "original_form": "저도 고민중인데",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013785.1.21",
          "form": "카레를 해볼까 하고있어요",
          "original_form": "카레를 해볼까 하고있어요",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013785.1.22",
          "form": "카레 잘안먹는데",
          "original_form": "카레 잘안먹는데",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013785.1.23",
          "form": "양파로 카라멜라이징해서 만드니까",
          "original_form": "양파로 카라멜라이징해서 만드니까",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013785.1.24",
          "form": "너무너무 맛있더라구요!ㅎ",
          "original_form": "너무너무 맛있더라구요!ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013785.1.25",
          "form": "와우 집에서요? 본격적이시네요",
          "original_form": "와우 집에서요? 본격적이시네요",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013785.1.26",
          "form": "단어만들었는데 배고프네요ㅠ저녁시간이라",
          "original_form": "단어만들었는데 배고프네요ㅠ저녁시간이라",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013785.1.27",
          "form": "ㅠㅠ 저도배고프네요 점심을 너무 소식했더니..ㅋㅋ",
          "original_form": "ㅠㅠ 저도배고프네요 점심을 너무 소식했더니..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013785.1.28",
          "form": "어제밤엔 저녁을 소식했더니  배고파서 못자겠고",
          "original_form": "어제밤엔 저녁을 소식했더니  배고파서 못자겠고",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013785.1.29",
          "form": "소식하는걸 때려쳐야겠어요",
          "original_form": "소식하는걸 때려쳐야겠어요",
          "speaker_id": "2",
