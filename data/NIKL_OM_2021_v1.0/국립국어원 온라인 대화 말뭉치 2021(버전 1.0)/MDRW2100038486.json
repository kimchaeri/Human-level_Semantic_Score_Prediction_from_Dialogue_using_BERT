      "utterance": [
        {
          "id": "MDRW2100038486.1.1",
          "form": "맞아요.. 아이스초코 이런거",
          "original_form": "맞아요.. 아이스초코 이런거",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038486.1.2",
          "form": "맞아요... 저도 만쥬 귀신이 됐어요",
          "original_form": "맞아요... 저도 만쥬 귀신이 됐어요",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038486.1.3",
          "form": "달고 지방 빵빵한...",
          "original_form": "달고 지방 빵빵한...",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038486.1.4",
          "form": "아 만쥬 좋죵",
          "original_form": "아 만쥬 좋죵",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100038486.1.5",
          "form": "갑자기 버스 터미널에 온 기분이 드네여",
          "original_form": "갑자기 버스 터미널에 온 기분이 드네여",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100038486.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ델리만쥬 말고요! 밤만쥬 좋아해요 저는",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ델리만쥬 말고요! 밤만쥬 좋아해요 저는",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038486.1.7",
          "form": "어릴때부터 약간 소울푸드였는데",
          "original_form": "어릴때부터 약간 소울푸드였는데",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038486.1.8",
          "form": "취직하고부터 거의 체력포션처럼 먹어요",
          "original_form": "취직하고부터 거의 체력포션처럼 먹어요",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038486.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038486.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038486.1.11",
          "form": "체력포션ㅋㅋㅋㅋㅋㅋㅋㅋ 넘 귀여운 단어에요",
          "original_form": "체력포션ㅋㅋㅋㅋㅋㅋㅋㅋ 넘 귀여운 단어에요",
          "speaker_id": "2",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038486.1.12",
          "form": "ㅋㅋㅋㅋㅋ포션이 없으면... 체력이 떨어져서 죽어버려여...",
          "original_form": "ㅋㅋㅋㅋㅋ포션이 없으면... 체력이 떨어져서 죽어버려여...",
          "speaker_id": "1",
          "time": "20210903 00:52"
        },
        {
          "id": "MDRW2100038486.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 06:33"
        },
        {
          "id": "MDRW2100038486.1.14",
          "form": "커피어떤거좋아하세요?",
          "original_form": "커피어떤거좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 06:33"
        },
        {
          "id": "MDRW2100038486.1.15",
          "form": "커피는 아메 아니면 라떼! 클래식하고 안 단게 좋아요",
          "original_form": "커피는 아메 아니면 라떼! 클래식하고 안 단게 좋아요",
          "speaker_id": "1",
          "time": "20210904 10:39"
        },
        {
          "id": "MDRW2100038486.1.16",
          "form": "갑자기 따뜻한 라떼 마시고싶네요 ㅎㅎ",
          "original_form": "갑자기 따뜻한 라떼 마시고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100038486.1.17",
          "form": "식사는 하셨어요?",
          "original_form": "식사는 하셨어요?",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100038486.1.18",
          "form": "저 오늘 오전에 일정이 있어서ㅠ 지금 고구마에 우유 먹고 있어요",
          "original_form": "저 오늘 오전에 일정이 있어서ㅠ 지금 고구마에 우유 먹고 있어요",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100038486.1.19",
          "form": "헉 아침부터 바쁘셨군요",
          "original_form": "헉 아침부터 바쁘셨군요",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100038486.1.20",
          "form": "내 줌으로 원격 연수가 있네요ㅠㅠ",
          "original_form": "내 줌으로 원격 연수가 있네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100038486.1.21",
          "form": "그래서 아침 대신 고구마를...ㅋㅋ",
          "original_form": "그래서 아침 대신 고구마를...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100038486.1.22",
          "form": "어머 저는 줌으로 강의 들었어요 ㅋㅋㅋㅋ",
          "original_form": "어머 저는 줌으로 강의 들었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100038486.1.23",
          "form": "원격 연수요?!",
          "original_form": "원격 연수요?!",
