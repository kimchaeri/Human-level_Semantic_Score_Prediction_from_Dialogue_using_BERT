      "utterance": [
        {
          "id": "MDRW2100000517.1.1",
          "form": "반려동물 키우세요? ㅎㅎ",
          "original_form": "반려동물 키우세요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000517.1.2",
          "form": "네네 강아지한마리 키워요ㅋㅋ",
          "original_form": "네네 강아지한마리 키워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000517.1.3",
          "form": "포메인데 털이 엄청빠져서 고민이네요..",
          "original_form": "포메인데 털이 엄청빠져서 고민이네요..",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000517.1.4",
          "form": "앗 저두 포메 두마리 키워요!ㅎㅎ",
          "original_form": "앗 저두 포메 두마리 키워요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000517.1.5",
          "form": "털 진짜 많이빠지죠 ㅠ ㅋㅋ",
          "original_form": "털 진짜 많이빠지죠 ㅠ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000517.1.6",
          "form": "헉 두마리나ㅋㅋㅋ",
          "original_form": "헉 두마리나ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000517.1.7",
          "form": "대단하시네욬ㅋㅋㅋㅋ",
          "original_form": "대단하시네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000517.1.8",
          "form": "미용도시켜요??",
          "original_form": "미용도시켜요??",
          "speaker_id": "2",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000517.1.9",
          "form": "아니요! ㅠㅋㅋㅋ 가족이랑 같이 사는데~ 부모님이 미용을 반대하셔서 ㅋㅋ",
          "original_form": "아니요! ㅠㅋㅋㅋ 가족이랑 같이 사는데~ 부모님이 미용을 반대하셔서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000517.1.10",
          "form": "ㅋㅋㅋ 그럼 털관리 열심히하셔야겠네욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 그럼 털관리 열심히하셔야겠네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000517.1.11",
          "form": "미용도 넘 비싸서 시키기 부담스럽기도하공ㅋㅋ",
          "original_form": "미용도 넘 비싸서 시키기 부담스럽기도하공ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000517.1.12",
          "form": "네 포메는 가위컷으로하니 더 비싸다더라구요 ㅠㅎ",
          "original_form": "네 포메는 가위컷으로하니 더 비싸다더라구요 ㅠㅎ",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000517.1.13",
          "form": "집에서 틈틈이 빗어줘요 ㅎ",
          "original_form": "집에서 틈틈이 빗어줘요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000517.1.14",
          "form": "ㅋㅋㅋ 자주빗어주는것밖에는 답이없네요ㅋㅋ",
          "original_form": "ㅋㅋㅋ 자주빗어주는것밖에는 답이없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000517.1.15",
          "form": "맞아요 ㅠ 넘 잘 엉키죠 ~ㅎㅎ",
          "original_form": "맞아요 ㅠ 넘 잘 엉키죠 ~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000517.1.16",
          "form": "ㅋㅋㅋ부지런해야하는거같아요 강아지키우려면",
          "original_form": "ㅋㅋㅋ부지런해야하는거같아요 강아지키우려면",
          "speaker_id": "2",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000517.1.17",
          "form": "산책도 매일 시켜줘야하고ㅋ",
          "original_form": "산책도 매일 시켜줘야하고ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000517.1.18",
          "form": "맞아요~ ㅠㅠㅋㅋ 책임감 있게~ 잘 키워야하는~",
          "original_form": "맞아요~ ㅠㅠㅋㅋ 책임감 있게~ 잘 키워야하는~",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000517.1.19",
          "form": "ㅋㅋ요즘 파양문제도 넘 심각해서... 잘키워야해요정말ㅠㅠ",
          "original_form": "ㅋㅋ요즘 파양문제도 넘 심각해서... 잘키워야해요정말ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:51"
        },
