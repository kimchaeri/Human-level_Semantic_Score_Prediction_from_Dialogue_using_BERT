      "utterance": [
        {
          "id": "MDRW2100046244.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211018 13:23"
        },
        {
          "id": "MDRW2100046244.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211018 14:05"
        },
        {
          "id": "MDRW2100046244.1.3",
          "form": "운동 좋아하시나요",
          "original_form": "운동 좋아하시나요",
          "speaker_id": "2",
          "time": "20211018 14:05"
        },
        {
          "id": "MDRW2100046244.1.4",
          "form": "넵 운동 좋아하는 편입니다",
          "original_form": "넵 운동 좋아하는 편입니다",
          "speaker_id": "1",
          "time": "20211018 14:06"
        },
        {
          "id": "MDRW2100046244.1.5",
          "form": "님도 운동좋아하세요?",
          "original_form": "님도 운동좋아하세요?",
          "speaker_id": "1",
          "time": "20211018 14:06"
        },
        {
          "id": "MDRW2100046244.1.6",
          "form": "저는 구기종목들만 좋아하는편이에요 ㅎㅎ",
          "original_form": "저는 구기종목들만 좋아하는편이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211018 14:06"
        },
        {
          "id": "MDRW2100046244.1.7",
          "form": "헬스 달리기 이런건 정말 싫은데....",
          "original_form": "헬스 달리기 이런건 정말 싫은데....",
          "speaker_id": "2",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046244.1.8",
          "form": "울며 겨자먹기식으로 가끔 합니다 ㅎㅎㅎ",
          "original_form": "울며 겨자먹기식으로 가끔 합니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211018 14:07"
        },
        {
          "id": "MDRW2100046244.1.9",
          "form": "아하 ㅋㅋ 축구나 농구 같은 거 말씀이시죠?",
          "original_form": "아하 ㅋㅋ 축구나 농구 같은 거 말씀이시죠?",
          "speaker_id": "1",
          "time": "20211018 14:08"
        },
        {
          "id": "MDRW2100046244.1.10",
          "form": "네네 ㅋㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211018 14:11"
        },
        {
          "id": "MDRW2100046244.1.11",
          "form": "일주일에 몇 번 정도 운동하세요?",
          "original_form": "일주일에 몇 번 정도 운동하세요?",
          "speaker_id": "2",
          "time": "20211018 14:11"
        },
        {
          "id": "MDRW2100046244.1.12",
          "form": "2,3번 정도 운동하고 저는 달리기 위주로 하는편입니다. ",
          "original_form": "2,3번 정도 운동하고 저는 달리기 위주로 하는편입니다. ",
          "speaker_id": "1",
          "time": "20211018 14:13"
        },
        {
          "id": "MDRW2100046244.1.13",
          "form": "아하 러닝하시는구나",
          "original_form": "아하 러닝하시는구나",
          "speaker_id": "2",
          "time": "20211018 14:13"
        },
        {
          "id": "MDRW2100046244.1.14",
          "form": "어제도 밤에 러닝크루 봤는데",
          "original_form": "어제도 밤에 러닝크루 봤는데",
          "speaker_id": "2",
          "time": "20211018 14:13"
        },
        {
          "id": "MDRW2100046244.1.15",
          "form": "멋지더라구요 ㅎㅎ",
          "original_form": "멋지더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211018 14:13"
        },
        {
          "id": "MDRW2100046244.1.16",
          "form": "넵 맞아요",
          "original_form": "넵 맞아요",
          "speaker_id": "1",
          "time": "20211018 14:14"
        },
        {
          "id": "MDRW2100046244.1.17",
          "form": "요즘 많이들 해서 달리기 잘 못하시거나 안좋아하시더라도 하다보면 괜찮아요!",
          "original_form": "요즘 많이들 해서 달리기 잘 못하시거나 안좋아하시더라도 하다보면 괜찮아요!",
          "speaker_id": "1",
          "time": "20211018 14:14"
        },
        {
          "id": "MDRW2100046244.1.18",
          "form": "저는 한 100미터 뛰면 헥헥댈거같은데",
          "original_form": "저는 한 100미터 뛰면 헥헥댈거같은데",
          "speaker_id": "2",
          "time": "20211018 14:14"
        },
        {
          "id": "MDRW2100046244.1.19",
          "form": "다들 정말 존경스러워요 ㅋㅋㅋㅋ",
          "original_form": "다들 정말 존경스러워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.20",
          "form": "러닝하시는 모든분들...",
          "original_form": "러닝하시는 모든분들...",
          "speaker_id": "2",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.21",
          "form": "ㅋㅋㅋㅋ 존경할 것까지는 아니죠 다들 자기가 좋아하는 운동하는건데",
          "original_form": "ㅋㅋㅋㅋ 존경할 것까지는 아니죠 다들 자기가 좋아하는 운동하는건데",
          "speaker_id": "1",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.22",
          "form": "그럼 님은 ",
          "original_form": "그럼 님은 ",
          "speaker_id": "1",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.23",
          "form": "게으른 저는 대단한 분들이라고 생각이 들더라구욬ㅋㅋㅋㅋ",
          "original_form": "게으른 저는 대단한 분들이라고 생각이 들더라구욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.24",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.26",
          "form": "말씀하신 구기 중에 당구나 볼링 골프?  하실 것 같은 느낌이.",
          "original_form": "말씀하신 구기 중에 당구나 볼링 골프?  하실 것 같은 느낌이.",
          "speaker_id": "1",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.27",
          "form": "오 그 중에는 하나도 즐기는게 별로 없고...",
          "original_form": "오 그 중에는 하나도 즐기는게 별로 없고...",
          "speaker_id": "2",
          "time": "20211018 14:15"
        },
        {
          "id": "MDRW2100046244.1.28",
          "form": "저는 축구 농구 배구 이런게 재밌더라구요 ㅎㅎ\r\n",
          "original_form": "저는 축구 농구 배구 이런게 재밌더라구요 ㅎㅎ\r\n",
