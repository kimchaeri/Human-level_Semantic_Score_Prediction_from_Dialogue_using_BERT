      "utterance": [
        {
          "id": "MDRW2100034095.1.1",
          "form": "오늘 계신 곳의 날씨는 어때요?",
          "original_form": "오늘 계신 곳의 날씨는 어때요?",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100034095.1.2",
          "form": "오늘 쪼금식 비오네요",
          "original_form": "오늘 쪼금식 비오네요",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100034095.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100034095.1.4",
          "form": "오전에는 하늘만 어둡더니\r\n오후되니 여기도 비가 와요",
          "original_form": "오전에는 하늘만 어둡더니\r\n오후되니 여기도 비가 와요",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100034095.1.5",
          "form": "비오는날 좋아하세요?",
          "original_form": "비오는날 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100034095.1.6",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100034095.1.7",
          "form": "집에있을땐좋은데",
          "original_form": "집에있을땐좋은데",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100034095.1.8",
          "form": "길걸을땐 싫어요",
          "original_form": "길걸을땐 싫어요",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100034095.1.9",
          "form": "저도 창밖으로 보는것만\r\n좋은 거 같아요 ^^^^",
          "original_form": "저도 창밖으로 보는것만\r\n좋은 거 같아요 ^^^^",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100034095.1.10",
          "form": "계절은 어느 계절 좋아하세요?",
          "original_form": "계절은 어느 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100034095.1.11",
          "form": "농사지을땐 좋아요 비오는거",
          "original_form": "농사지을땐 좋아요 비오는거",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100034095.1.12",
          "form": "겨울빼고 다 좋아요 ㅎㅎ",
          "original_form": "겨울빼고 다 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100034095.1.13",
          "form": "겨울빼고...^^",
          "original_form": "겨울빼고...^^",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034095.1.14",
          "form": "ㅋㅋ name1님두 그러신가요",
          "original_form": "ㅋㅋ &name1&님두 그러신가요",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100034095.1.15",
          "form": "저는 여름이랑 겨울은\r\n별로 안좋아해요 ㅜㅜㅋㅋ",
          "original_form": "저는 여름이랑 겨울은\r\n별로 안좋아해요 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034095.1.16",
          "form": "여름도 추가되셨군요 ㅎㅎ전 여름은 낮이 길고 빨래가 가벼워서 좋아요",
          "original_form": "여름도 추가되셨군요 ㅎㅎ전 여름은 낮이 길고 빨래가 가벼워서 좋아요",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100034095.1.17",
          "form": "저는 아이 출산하고 나니\r\n땀이 많아져서 여름이\r\n힘들더라구요 ㅠㅠㅋ",
          "original_form": "저는 아이 출산하고 나니\r\n땀이 많아져서 여름이\r\n힘들더라구요 ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100034095.1.18",
          "form": "아ㅜ 체질이 바뀐다더라구요",
          "original_form": "아ㅜ 체질이 바뀐다더라구요",
