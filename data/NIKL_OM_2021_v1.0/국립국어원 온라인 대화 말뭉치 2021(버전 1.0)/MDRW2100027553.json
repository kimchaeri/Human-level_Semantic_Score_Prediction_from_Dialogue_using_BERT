      "utterance": [
        {
          "id": "MDRW2100027553.1.1",
          "form": "초반은 약간 다큐같았는데 재밌게봤어요",
          "original_form": "초반은 약간 다큐같았는데 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027553.1.2",
          "form": "그쵸 잠올뻔하다가",
          "original_form": "그쵸 잠올뻔하다가",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027553.1.3",
          "form": "뒤로갈수록 후덜덜",
          "original_form": "뒤로갈수록 후덜덜",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027553.1.4",
          "form": "여배우 매력적이에요",
          "original_form": "여배우 매력적이에요",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027553.1.5",
          "form": "그러게요 연기도 잘하시고 인상적이었어요",
          "original_form": "그러게요 연기도 잘하시고 인상적이었어요",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027553.1.6",
          "form": "태국어",
          "original_form": "태국어",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027553.1.7",
          "form": "왠지 무서워요",
          "original_form": "왠지 무서워요",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027553.1.8",
          "form": "목소리톤도 한층 높아서 공포영화보는데 더 무서웠던것같긴하네요",
          "original_form": "목소리톤도 한층 높아서 공포영화보는데 더 무서웠던것같긴하네요",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027553.1.9",
          "form": "글자도 라면 같이 생겨서 ㅋㅋ",
          "original_form": "글자도 라면 같이 생겨서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027553.1.10",
          "form": "태국영화 또 보고싶네요",
          "original_form": "태국영화 또 보고싶네요",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027553.1.11",
          "form": "공포영화로보고싶네요ㅋ",
          "original_form": "공포영화로보고싶네요ㅋ",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027553.1.12",
          "form": "태국 공포 뭐 없을까요",
          "original_form": "태국 공포 뭐 없을까요",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027553.1.13",
          "form": "잘모르겠네요 예전에 셔터 엄청 무서웠던것같아요",
          "original_form": "잘모르겠네요 예전에 셔터 엄청 무서웠던것같아요",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027553.1.14",
          "form": "오 그거",
          "original_form": "오 그거",
          "speaker_id": "1",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027553.1.15",
          "form": "재밌지 않나요",
          "original_form": "재밌지 않나요",
          "speaker_id": "1",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027553.1.16",
          "form": "갑툭티 무서워요",
          "original_form": "갑툭티 무서워요",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027553.1.17",
          "form": "맞아요 무섭더라구요",
          "original_form": "맞아요 무섭더라구요",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027553.1.18",
          "form": "일본도 무서운게 많던데",
          "original_form": "일본도 무서운게 많던데",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027553.1.19",
          "form": "링 주온 착신아리 공포영화재밌었죠",
          "original_form": "링 주온 착신아리 공포영화재밌었죠",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027553.1.20",
          "form": "귀신영화 자주개봉햇으면",
          "original_form": "귀신영화 자주개봉햇으면",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027553.1.21",
          "form": "귀신영화가 잘안나오네요",
          "original_form": "귀신영화가 잘안나오네요",
