      "utterance": [
        {
          "id": "MDRW2100025385.1.1",
          "form": "혹시 중고거래 해보셨나요?",
          "original_form": "혹시 중고거래 해보셨나요?",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100025385.1.2",
          "form": "중고거래는 잘 안해봤어요",
          "original_form": "중고거래는 잘 안해봤어요",
          "speaker_id": "2",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025385.1.3",
          "form": "제가 시간맞춰가는게 힘들어서요",
          "original_form": "제가 시간맞춰가는게 힘들어서요",
          "speaker_id": "2",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025385.1.4",
          "form": "보통 온라인샵이나 홈쇼핑을 많이해묘",
          "original_form": "보통 온라인샵이나 홈쇼핑을 많이해묘",
          "speaker_id": "2",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025385.1.5",
          "form": "그렇군요.. 요즘 비대면  쇼핑이 많아져서 많이들 온라인 쇼핖도 하던데.. 저는 아날로그세대인지.. 쉽지 않ㄴ에ㅛ..ㅎㅎ",
          "original_form": "그렇군요.. 요즘 비대면  쇼핑이 많아져서 많이들 온라인 쇼핖도 하던데.. 저는 아날로그세대인지.. 쉽지 않ㄴ에ㅛ..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025385.1.6",
          "form": "그쵸 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025385.1.7",
          "form": "최근에 가방을 하나 샀는데",
          "original_form": "최근에 가방을 하나 샀는데",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025385.1.8",
          "form": "출석 체크 이벤트 있어서",
          "original_form": "출석 체크 이벤트 있어서",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025385.1.9",
          "form": "매일 출석체크해서 포인트 모아서 싸게 샀어요 ㅋㅋㅋ",
          "original_form": "매일 출석체크해서 포인트 모아서 싸게 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025385.1.10",
          "form": "뭔가 뿌듯하더라구요",
          "original_form": "뭔가 뿌듯하더라구요",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025385.1.11",
          "form": "와.. 역시.. 온라인은 이벤트가 많아요",
          "original_form": "와.. 역시.. 온라인은 이벤트가 많아요",
          "speaker_id": "1",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025385.1.12",
          "form": "들어보면 행사도 하고 할인도 하고",
          "original_form": "들어보면 행사도 하고 할인도 하고",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025385.1.13",
          "form": "안하는 저만  손해보는 느낌?",
          "original_form": "안하는 저만  손해보는 느낌?",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025385.1.14",
          "form": "엄청 귀찮긴해요 그래도 싸게 사면 엄청 뿌듯해요",
          "original_form": "엄청 귀찮긴해요 그래도 싸게 사면 엄청 뿌듯해요",
          "speaker_id": "2",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025385.1.15",
          "form": "요즘 관심있는 물건 있으세요?",
          "original_form": "요즘 관심있는 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025385.1.16",
          "form": "저는 이제 뭔가를 안사려구요..ㅋㅋㅋㅋ",
          "original_form": "저는 이제 뭔가를 안사려구요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100025385.1.17",
          "form": "코로나로 집에 있다보니 쌓여져 있는 물건들이 많네요..",
          "original_form": "코로나로 집에 있다보니 쌓여져 있는 물건들이 많네요..",
          "speaker_id": "1",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100025385.1.18",
          "form": "저도 항상 그렇게 마음은 먹는데",
          "original_form": "저도 항상 그렇게 마음은 먹는데",
          "speaker_id": "2",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100025385.1.19",
          "form": "잘안되네요 ㅠㅠ",
          "original_form": "잘안되네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100025385.1.20",
          "form": "안나가다보니 옷이랑 화장품은 확실히 덜 사게 되더라구요",
          "original_form": "안나가다보니 옷이랑 화장품은 확실히 덜 사게 되더라구요",
          "speaker_id": "2",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100025385.1.21",
          "form": "대신 집에서 내려마신다고 커피는 많이 샀어요",
          "original_form": "대신 집에서 내려마신다고 커피는 많이 샀어요",
          "speaker_id": "2",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100025385.1.22",
          "form": "맞아요.. 필요한게 왜이리 많은지..",
          "original_form": "맞아요.. 필요한게 왜이리 많은지..",
          "speaker_id": "1",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025385.1.23",
          "form": "전 그래도 어느정도 정리한 후에 사려고 엄청 자제중이에요",
          "original_form": "전 그래도 어느정도 정리한 후에 사려고 엄청 자제중이에요",
          "speaker_id": "1",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025385.1.24",
          "form": "미니멀리즘 하시는건가요?",
          "original_form": "미니멀리즘 하시는건가요?",
          "speaker_id": "2",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025385.1.25",
          "form": "전 그게 어렵더라구요 ㅠㅠ",
          "original_form": "전 그게 어렵더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025385.1.26",
          "form": "미니멀하겠다고 거의 1년을 당근하고 있는데 여전히 맥시멈입니다..",
          "original_form": "미니멀하겠다고 거의 1년을 당근하고 있는데 여전히 맥시멈입니다..",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025385.1.27",
          "form": "왠짐들이 이렇게 많은지.. 오래된 것들 잘 못버리는 성격이라 다 모아놔요",
          "original_form": "왠짐들이 이렇게 많은지.. 오래된 것들 잘 못버리는 성격이라 다 모아놔요",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025385.1.28",
          "form": "앗 저희 엄마도 그러세요!",
          "original_form": "앗 저희 엄마도 그러세요!",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100025385.1.29",
          "form": "제가 몰래 버려요 ㅋㅋㄱㄱㅋ",
          "original_form": "제가 몰래 버려요 ㅋㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100025385.1.30",
          "form": "없어져도 모르시더라구요 ㅋㅋㅋㅋ",
          "original_form": "없어져도 모르시더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100025385.1.31",
          "form": "필요할때 사는게 나은거 같아요 ㅋㅋㅋ",
          "original_form": "필요할때 사는게 나은거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025385.1.32",
          "form": "그게 왠지 꼭 언젠간 쓸일 있을거 같아서 못버리겟더라구요",
          "original_form": "그게 왠지 꼭 언젠간 쓸일 있을거 같아서 못버리겟더라구요",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025385.1.33",
          "form": "그래서 모아놨던거.. 이번 코로나로 집 한번 뒤집으면서 정리하고 있죠",
          "original_form": "그래서 모아놨던거.. 이번 코로나로 집 한번 뒤집으면서 정리하고 있죠",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025385.1.34",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025385.1.35",
          "form": "그런데 찾을때 어딨는지 몰라서",
          "original_form": "그런데 찾을때 어딨는지 몰라서",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025385.1.36",
          "form": "못쓰게되더라구요 ㅠ",
          "original_form": "못쓰게되더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025385.1.37",
          "form": "그래서 이번에 정리하면서 위치 잘 기억해두고 있어요",
          "original_form": "그래서 이번에 정리하면서 위치 잘 기억해두고 있어요",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025385.1.38",
          "form": "그리고 버릴건 버리고..",
          "original_form": "그리고 버릴건 버리고..",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025385.1.39",
          "form": "중고로 팔건 팔고",
          "original_form": "중고로 팔건 팔고",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025385.1.40",
          "form": "좋으신 생각이에요! 꼭 성공하실거에요~",
          "original_form": "좋으신 생각이에요! 꼭 성공하실거에요~",
