      "utterance": [
        {
          "id": "MDRW2100005881.1.1",
          "form": "화이자가 그나마 나은거 맞을까",
          "original_form": "화이자가 그나마 나은거 맞을까",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100005881.1.2",
          "form": "모더나가 더 좋아보여",
          "original_form": "모더나가 더 좋아보여",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100005881.1.3",
          "form": "그래서 이게 ㅠㅠ 그게맞나.아닌가 싶어서ㅠㅠ",
          "original_form": "그래서 이게 ㅠㅠ 그게맞나.아닌가 싶어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100005881.1.4",
          "form": "모더나가 혈전이 적다고 하던데 이미 맞은거니ㅠ",
          "original_form": "모더나가 혈전이 적다고 하던데 이미 맞은거니ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100005881.1.5",
          "form": "하필..며칠차이로.ㅠㅠ",
          "original_form": "하필..며칠차이로.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100005881.1.6",
          "form": "그치 나도 1차 맞아놓고는 후회했어",
          "original_form": "그치 나도 1차 맞아놓고는 후회했어",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100005881.1.7",
          "form": "모더나가 그나마 부작용 덜 보고 되는거 같지???",
          "original_form": "모더나가 그나마 부작용 덜 보고 되는거 같지???",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100005881.1.8",
          "form": "근데 또 생각해보면. 맞은 사람들이 적어서일수도",
          "original_form": "근데 또 생각해보면. 맞은 사람들이 적어서일수도",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005881.1.9",
          "form": "모더나는 사망자가 업는감요",
          "original_form": "모더나는 사망자가 업는감요",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005881.1.10",
          "form": "좀 적은거같아요 부작용 기사도 적고",
          "original_form": "좀 적은거같아요 부작용 기사도 적고",
          "speaker_id": "1",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005881.1.11",
          "form": "아 맞은 사람이 적어서 그럴수도 있겠어요",
          "original_form": "아 맞은 사람이 적어서 그럴수도 있겠어요",
          "speaker_id": "1",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005881.1.12",
          "form": "뭐가 맞는지 참",
          "original_form": "뭐가 맞는지 참",
          "speaker_id": "1",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005881.1.13",
          "form": "그니까여ㅠ뭐 일단 모더나는 교차접종이안되니 물건너갔구",
          "original_form": "그니까여ㅠ뭐 일단 모더나는 교차접종이안되니 물건너갔구",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005881.1.14",
          "form": "화이자로 잘 맞으셔야죠",
          "original_form": "화이자로 잘 맞으셔야죠",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005881.1.15",
