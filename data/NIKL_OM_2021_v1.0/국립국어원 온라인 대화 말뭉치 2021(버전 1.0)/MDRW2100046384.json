        },
        {
          "id": "MDRW2100046384.1.3",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046384.1.4",
          "form": "강아지 좋아해요",
          "original_form": "강아지 좋아해요",
          "speaker_id": "1",
          "time": "20211020 16:35"
        },
        {
          "id": "MDRW2100046384.1.5",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046384.1.6",
          "form": "키우세요??",
          "original_form": "키우세요??",
          "speaker_id": "1",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046384.1.7",
          "form": "요즘 고양이가 대세인듯한데",
          "original_form": "요즘 고양이가 대세인듯한데",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046384.1.8",
          "form": "전 예전부터",
          "original_form": "전 예전부터",
          "speaker_id": "2",
          "time": "20211020 16:36"
        },
        {
          "id": "MDRW2100046384.1.9",
          "form": "강경 댕댕이파였습니다 ㅋㅋ",
          "original_form": "강경 댕댕이파였습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046384.1.10",
          "form": "아니요 ㅠㅠ 너무 키우고싶은데",
          "original_form": "아니요 ㅠㅠ 너무 키우고싶은데",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046384.1.11",
          "form": "현실적인 여건이 ㅎㅎ",
          "original_form": "현실적인 여건이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046384.1.12",
          "form": "여건이 안되시구나 ㅋㅋ",
          "original_form": "여건이 안되시구나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046384.1.13",
          "form": "맞아요 좋아하는거랑 실제로 키우는거랑 달라서",
          "original_form": "맞아요 좋아하는거랑 실제로 키우는거랑 달라서",
          "speaker_id": "1",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046384.1.14",
          "form": "매일 산책도 시켜줘야되고",
          "original_form": "매일 산책도 시켜줘야되고",
          "speaker_id": "1",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046384.1.15",
          "form": "맞아요...ㅠㅠ 집에 혼자 냅두기도 너무 미안할거같고",
          "original_form": "맞아요...ㅠㅠ 집에 혼자 냅두기도 너무 미안할거같고",
          "speaker_id": "2",
          "time": "20211020 16:37"
        },
        {
          "id": "MDRW2100046384.1.16",
          "form": "귀엽긴 엄청 귀여워요",
          "original_form": "귀엽긴 엄청 귀여워요",
          "speaker_id": "1",
          "time": "20211020 16:38"
        },
        {
          "id": "MDRW2100046384.1.17",
          "form": "ㅋㅋㅋㅋ 마자요 ..",
          "original_form": "ㅋㅋㅋㅋ 마자요 ..",
          "speaker_id": "2",
          "time": "20211020 16:38"
        },
        {
          "id": "MDRW2100046384.1.18",
          "form": "솔직히 아파트 단지에서",
          "original_form": "솔직히 아파트 단지에서",
          "speaker_id": "2",
          "time": "20211020 16:38"
        },
        {
          "id": "MDRW2100046384.1.19",
