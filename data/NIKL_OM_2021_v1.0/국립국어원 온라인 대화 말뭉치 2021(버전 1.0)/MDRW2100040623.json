      "utterance": [
        {
          "id": "MDRW2100040623.1.1",
          "form": "뭐 최근에는",
          "original_form": "뭐 최근에는",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100040623.1.2",
          "form": "셋이서 name3이방에서 놀다가 피아노대회놀이를 하는거야",
          "original_form": "셋이서 &name3&이방에서 놀다가 피아노대회놀이를 하는거야",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100040623.1.3",
          "form": "ㅋㅋㅋㅋㄱ 대회\r\nㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ 대회\r\nㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100040623.1.4",
          "form": "name4가 스타트했지. 근데 애들이 정한룰보다는 길게했나봐",
          "original_form": "&name4&가 스타트했지. 근데 애들이 정한룰보다는 길게했나봐",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100040623.1.5",
          "form": "그건 뭔데 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "그건 뭔데 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040623.1.6",
          "form": "한곡만치기",
          "original_form": "한곡만치기",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040623.1.7",
          "form": "근데 name4가 한번더치고싶다 이래됐지",
          "original_form": "근데 &name4&가 한번더치고싶다 이래됐지",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040623.1.8",
          "form": "ㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040623.1.9",
          "form": "미치겠다 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "미치겠다 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040623.1.10",
          "form": "아 큰 피아노가 있으니까",
          "original_form": "아 큰 피아노가 있으니까",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040623.1.11",
          "form": "name3이랑 name5이가 한번씩 돌아가면서",
          "original_form": "&name3&이랑 &name5&이가 한번씩 돌아가면서",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040623.1.12",
          "form": "애들이 또 치고싶은가보구나",
          "original_form": "애들이 또 치고싶은가보구나",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040623.1.13",
          "form": "쳐야되잖아",
          "original_form": "쳐야되잖아",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
