      "utterance": [
        {
          "id": "MDRW2100009268.1.1",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009268.1.2",
          "form": "아니요 그냥 길가다 보는거 좋아해요",
          "original_form": "아니요 그냥 길가다 보는거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009268.1.3",
          "form": "저는 막 엄청나게 좋아하진않는데",
          "original_form": "저는 막 엄청나게 좋아하진않는데",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009268.1.4",
          "form": "가끔 귀여운애들보면 좋더라구요",
          "original_form": "가끔 귀여운애들보면 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009268.1.5",
          "form": "맞아요 지칠때 보면 좀 힐링도 되고요!",
          "original_form": "맞아요 지칠때 보면 좀 힐링도 되고요!",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009268.1.6",
          "form": "고양이를 더 좋아하세요? 아님 강아지?",
          "original_form": "고양이를 더 좋아하세요? 아님 강아지?",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009268.1.7",
          "form": "저는 강아지요 님은요?",
          "original_form": "저는 강아지요 님은요?",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009268.1.8",
          "form": "저도 강아지요!",
          "original_form": "저도 강아지요!",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009268.1.9",
          "form": "고양이는",
          "original_form": "고양이는",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009268.1.10",
          "form": "아직 무서워영 ..",
          "original_form": "아직 무서워영 ..",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009268.1.11",
          "form": "ㅋㅋㅋㅋ왜요 개냥이들도 많은데요",
          "original_form": "ㅋㅋㅋㅋ왜요 개냥이들도 많은데요",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009268.1.12",
          "form": "뭔가 눈이 날공격할거같은 ...ㅋㅋ",
          "original_form": "뭔가 눈이 날공격할거같은 ...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009268.1.13",
          "form": "ㅋㅋㅋㅋㅋ사실 동물 별로 안좋아하시는 것 아니에요",
          "original_form": "ㅋㅋㅋㅋㅋ사실 동물 별로 안좋아하시는 것 아니에요",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009268.1.14",
          "form": "ㅋㅋㅋㅋ그런가요 저도 긴가민가하네요",
          "original_form": "ㅋㅋㅋㅋ그런가요 저도 긴가민가하네요",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009268.1.15",
          "form": "전 예전에 강아지 한번 키워 봤어서 고양이도 한번 키워보고 싶어요",
          "original_form": "전 예전에 강아지 한번 키워 봤어서 고양이도 한번 키워보고 싶어요",
          "speaker_id": "1",
