      "utterance": [
        {
          "id": "MDRW2100044029.1.1",
          "form": "오늘 강아지들 양치 시켰나",
          "original_form": "오늘 강아지들 양치 시켰나",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044029.1.2",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044029.1.3",
          "form": "두 마리 다",
          "original_form": "두 마리 다",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044029.1.4",
          "form": "아니 온유만~",
          "original_form": "아니 온유만~",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044029.1.5",
          "form": "지우는",
          "original_form": "지우는",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044029.1.6",
          "form": "시켜줘야지..!근데 넘 싫어해서 하기 힘들다",
          "original_form": "시켜줘야지..!근데 넘 싫어해서 하기 힘들다",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044029.1.7",
          "form": "웅 ㅜㅜ 이따 시도해볼게",
          "original_form": "웅 ㅜㅜ 이따 시도해볼게",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044029.1.8",
          "form": "달래가면서 해 봐라",
          "original_form": "달래가면서 해 봐라",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044029.1.9",
          "form": "웅웅..ㅋㅋ 잘 달래줘야한다",
          "original_form": "웅웅..ㅋㅋ 잘 달래줘야한다",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044029.1.10",
          "form": "먹을 거로 유인 해라",
          "original_form": "먹을 거로 유인 해라",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044029.1.11",
          "form": "웅웅그렇게 해야겠다 ㅋㅋ",
          "original_form": "웅웅그렇게 해야겠다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044029.1.12",
          "form": "이빨 썩으면 안 된다",
          "original_form": "이빨 썩으면 안 된다",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044029.1.13",
          "form": "웅 ㅜㅜ 입냄새도 나고",
          "original_form": "웅 ㅜㅜ 입냄새도 나고",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044029.1.14",
          "form": "그래 치아가 건강해야 한다",
          "original_form": "그래 치아가 건강해야 한다",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044029.1.15",
          "form": "울 온유 계속 눈을 긁는다",
