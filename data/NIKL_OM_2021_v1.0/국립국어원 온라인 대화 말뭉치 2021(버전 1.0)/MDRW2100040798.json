      "utterance": [
        {
          "id": "MDRW2100040798.1.1",
          "form": "아침은 챙겨먹는 편인가요?",
          "original_form": "아침은 챙겨먹는 편인가요?",
          "speaker_id": "2",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100040798.1.2",
          "form": "네 부모님과 살고 있어서",
          "original_form": "네 부모님과 살고 있어서",
          "speaker_id": "1",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100040798.1.3",
          "form": "출근 전에 아침식사 하고 나서요",
          "original_form": "출근 전에 아침식사 하고 나서요",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040798.1.4",
          "form": "부모님이 챙겨주시는군요",
          "original_form": "부모님이 챙겨주시는군요",
          "speaker_id": "2",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040798.1.5",
          "form": "네 감사하게도요",
          "original_form": "네 감사하게도요",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040798.1.6",
          "form": "챙겨줘도 안먹는 사람들도 있던데 name1님은 잘 챙겨먹어서 부모님인 좋아하시겠어요 :)",
          "original_form": "챙겨줘도 안먹는 사람들도 있던데 &name1&님은 잘 챙겨먹어서 부모님인 좋아하시겠어요 :)",
          "speaker_id": "2",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100040798.1.7",
          "form": "아침식사 드셨어요?",
          "original_form": "아침식사 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100040798.1.8",
          "form": "네 저희집도 아침은 꼭 먹는 집이라서",
          "original_form": "네 저희집도 아침은 꼭 먹는 집이라서",
          "speaker_id": "2",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100040798.1.9",
          "form": "오늘은 출근 안할텐데 일찍 일어났네요",
          "original_form": "오늘은 출근 안할텐데 일찍 일어났네요",
          "speaker_id": "2",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100040798.1.10",
          "form": "식사했나요?",
          "original_form": "식사했나요?",
          "speaker_id": "2",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100040798.1.11",
          "form": "네 어제 일찍 잤더니ㅎㅎ",
          "original_form": "네 어제 일찍 잤더니ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100040798.1.12",
          "form": "아침식사는 아직이요",
          "original_form": "아침식사는 아직이요",
          "speaker_id": "1",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100040798.1.13",
          "form": "그렇죠.. 아직 8시도 안됐으니까",
          "original_form": "그렇죠.. 아직 8시도 안됐으니까",
          "speaker_id": "2",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100040798.1.14",
          "form": "아침은 주로 어떤 거 드세요?",
          "original_form": "아침은 주로 어떤 거 드세요?",
          "speaker_id": "2",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100040798.1.15",
          "form": "그때그때 다른데",
          "original_form": "그때그때 다른데",
          "speaker_id": "1",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100040798.1.16",
          "form": "보통 샐러드와 식빵 정도 먹어요",
          "original_form": "보통 샐러드와 식빵 정도 먹어요",
          "speaker_id": "1",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100040798.1.17",
          "form": "간단히 먹나요?",
          "original_form": "간단히 먹나요?",
          "speaker_id": "2",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100040798.1.18",
          "form": "오호.. 그렇군요",
          "original_form": "오호.. 그렇군요",
          "speaker_id": "2",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100040798.1.19",
          "form": "그게 오늘 우리집 아침 메뉸데..ㅎㅎ",
          "original_form": "그게 오늘 우리집 아침 메뉸데..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040798.1.20",
          "form": "보통 밥 국 먹거든요 우리집은",
          "original_form": "보통 밥 국 먹거든요 우리집은",
          "speaker_id": "2",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040798.1.21",
          "form": "오 한식으로 챙겨드시는군요",
          "original_form": "오 한식으로 챙겨드시는군요",
          "speaker_id": "1",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040798.1.22",
          "form": "저희도 날 추워지면 밥 먹어요 아침식사로",
          "original_form": "저희도 날 추워지면 밥 먹어요 아침식사로",
          "speaker_id": "1",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040798.1.23",
          "form": "아무래도 추울땐 따근한 국이 좋죠",
          "original_form": "아무래도 추울땐 따근한 국이 좋죠",
          "speaker_id": "2",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040798.1.24",
          "form": "주로 한식으로 먹다가 요새 조금씩 간편하게 바뀌고 있어요",
          "original_form": "주로 한식으로 먹다가 요새 조금씩 간편하게 바뀌고 있어요",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040798.1.25",
          "form": "요거트에 견과류랑 과일 채소 넣은 것도 아침으로 먹기도 해요",
          "original_form": "요거트에 견과류랑 과일 채소 넣은 것도 아침으로 먹기도 해요",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040798.1.26",
          "form": "요거트도 좋죠~",
          "original_form": "요거트도 좋죠~",
          "speaker_id": "1",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100040798.1.27",
          "form": "저는 카페인에 약해서 커피 잘 못마셔요ㅜ",
