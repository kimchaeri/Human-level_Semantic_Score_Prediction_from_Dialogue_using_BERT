        {
          "id": "MDRW2100046314.1.2",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "2",
          "time": "20211019 18:28"
        },
        {
          "id": "MDRW2100046314.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20211019 18:28"
        },
        {
          "id": "MDRW2100046314.1.4",
          "form": "곧 먹으려고 합니다",
          "original_form": "곧 먹으려고 합니다",
          "speaker_id": "1",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046314.1.5",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046314.1.6",
          "form": "저는 오늘 저녁은 안먹으러구요 ㅠ",
          "original_form": "저는 오늘 저녁은 안먹으러구요 ㅠ",
          "speaker_id": "2",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046314.1.7",
          "form": "점심에 많이먹어서 ㅎㅎ",
          "original_form": "점심에 많이먹어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046314.1.8",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "2",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046314.1.9",
          "form": "전 기숙사 살아서",
          "original_form": "전 기숙사 살아서",
          "speaker_id": "1",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046314.1.10",
          "form": "기숙사 급식 먹을 거 같아요",
          "original_form": "기숙사 급식 먹을 거 같아요",
          "speaker_id": "1",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046314.1.11",
          "form": "아하 오늘 메뉴 뭔지 아세요?",
          "original_form": "아하 오늘 메뉴 뭔지 아세요?",
          "speaker_id": "2",
          "time": "20211019 18:29"
        },
        {
          "id": "MDRW2100046314.1.12",
          "form": "스파게티로 알고 있어요",
          "original_form": "스파게티로 알고 있어요",
          "speaker_id": "1",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046314.1.13",
          "form": "저녁 안 드시면 배 많이 고프시겠네요..",
          "original_form": "저녁 안 드시면 배 많이 고프시겠네요..",
          "speaker_id": "1",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046314.1.14",
          "form": "우와 잘나오네요 ㅎㅎ 맛있겠다",
          "original_form": "우와 잘나오네요 ㅎㅎ 맛있겠다",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046314.1.15",
          "form": "아 근데 지금 배불러서....",
          "original_form": "아 근데 지금 배불러서....",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046314.1.16",
          "form": "괜찮아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "괜찮아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 18:30"
        },
        {
          "id": "MDRW2100046314.1.17",
          "form": "점심도 급식 드셨어요?",
          "original_form": "점심도 급식 드셨어요?",
          "speaker_id": "2",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046314.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046314.1.19",
          "form": "근데 가끔 맛없으면",
          "original_form": "근데 가끔 맛없으면",
          "speaker_id": "1",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046314.1.20",
          "form": "사먹기도 해요",
          "original_form": "사먹기도 해요",
          "speaker_id": "1",
          "time": "20211019 18:31"
        },
        {
          "id": "MDRW2100046314.1.21",
          "form": "가끔은 또 사먹어줘야죠 ㅎㅎ",
          "original_form": "가끔은 또 사먹어줘야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 18:32"
        },
        {
          "id": "MDRW2100046314.1.22",
          "form": "저녁 먹고 뭐하실 예정이세요?",
          "original_form": "저녁 먹고 뭐하실 예정이세요?",
          "speaker_id": "2",
          "time": "20211019 18:32"
        },
        {
          "id": "MDRW2100046314.1.23",
          "form": "책 빌려야 해서",
          "original_form": "책 빌려야 해서",
          "speaker_id": "1",
          "time": "20211019 18:33"
        },
        {
          "id": "MDRW2100046314.1.24",
          "form": "도서관 갈 거 같아요",
          "original_form": "도서관 갈 거 같아요",
          "speaker_id": "1",
          "time": "20211019 18:33"
        },
        {
          "id": "MDRW2100046314.1.25",
          "form": "오 책..이랑 정말 안친한 저로써는",
          "original_form": "오 책..이랑 정말 안친한 저로써는",
          "speaker_id": "2",
          "time": "20211019 18:34"
        },
        {
          "id": "MDRW2100046314.1.26",
          "form": "특이한 일정이라고 생각이 드네욬ㅋㅋㅋㅋ",
          "original_form": "특이한 일정이라고 생각이 드네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 18:34"
        },
        {
          "id": "MDRW2100046314.1.27",
          "form": "책 좋아하시나봐요!",
          "original_form": "책 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20211019 18:35"
        },
        {
          "id": "MDRW2100046314.1.28",
          "form": "저도 책 안 읽는데",
          "original_form": "저도 책 안 읽는데",
          "speaker_id": "1",
          "time": "20211019 18:35"
        },
        {
          "id": "MDRW2100046314.1.29",
          "form": "학교 과제에",
          "original_form": "학교 과제에",
          "speaker_id": "1",
          "time": "20211019 18:35"
        },
        {
          "id": "MDRW2100046314.1.30",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20211019 18:36"
        },
        {
          "id": "MDRW2100046314.1.31",
          "form": "책 요약하는 게 있어서",
          "original_form": "책 요약하는 게 있어서",
          "speaker_id": "1",
          "time": "20211019 18:36"
        },
        {
          "id": "MDRW2100046314.1.32",
          "form": "아하 그래서 가시는거구나",
          "original_form": "아하 그래서 가시는거구나",
          "speaker_id": "2",
          "time": "20211019 18:36"
        },
        {
          "id": "MDRW2100046314.1.33",
          "form": "살짝 거리감 생겼다가 다시 가까워진 느낌 ㅋㅋ",
          "original_form": "살짝 거리감 생겼다가 다시 가까워진 느낌 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 18:36"
        },
        {
          "id": "MDRW2100046314.1.34",
          "form": "몇 년 만에 읽은 거라",
          "original_form": "몇 년 만에 읽은 거라",
          "speaker_id": "1",
          "time": "20211019 18:36"
        },
        {
          "id": "MDRW2100046314.1.35",
          "form": "두렵네요..",
          "original_form": "두렵네요..",
          "speaker_id": "1",
          "time": "20211019 18:36"
        },
        {
          "id": "MDRW2100046314.1.36",
          "form": "잘 하실 수 있을거라 믿습니다..",
          "original_form": "잘 하실 수 있을거라 믿습니다..",
          "speaker_id": "2",
          "time": "20211019 18:36"
        }
      ]
