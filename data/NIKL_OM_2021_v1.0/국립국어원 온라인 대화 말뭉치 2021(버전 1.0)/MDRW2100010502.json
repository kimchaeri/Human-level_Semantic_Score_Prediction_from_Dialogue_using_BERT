      "utterance": [
        {
          "id": "MDRW2100010502.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100010502.1.2",
          "form": "좋아하는 게임 있으신가요?",
          "original_form": "좋아하는 게임 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010502.1.3",
          "form": "안녕하세여 저 쿠키런 하다가 접구 지금은 없어요ㅠㅜ하시는 게임 있으세요?",
          "original_form": "안녕하세여 저 쿠키런 하다가 접구 지금은 없어요ㅠㅜ하시는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100010502.1.4",
          "form": "저도 심즈하다가 요새는 안하고 있어요",
          "original_form": "저도 심즈하다가 요새는 안하고 있어요",
          "speaker_id": "1",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100010502.1.5",
          "form": "쿠키런이 되게 핫한가봐요",
          "original_form": "쿠키런이 되게 핫한가봐요",
          "speaker_id": "1",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100010502.1.6",
          "form": "아 심즈 저도 좋아했는데ㅎㅎ",
          "original_form": "아 심즈 저도 좋아했는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100010502.1.7",
          "form": "쿠키런 재밌긴한데 현질이 심해서ㅠㅠ",
          "original_form": "쿠키런 재밌긴한데 현질이 심해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100010502.1.8",
          "form": "아 현질 하는 방식으로 바꼈나요????",
          "original_form": "아 현질 하는 방식으로 바꼈나요????",
          "speaker_id": "1",
          "time": "20210823 19:01"
        },
        {
          "id": "MDRW2100010502.1.9",
          "form": "음 쿠키사려면 현질이 꼭 필요하더라구요",
          "original_form": "음 쿠키사려면 현질이 꼭 필요하더라구요",
          "speaker_id": "2",
          "time": "20210823 19:01"
        },
        {
          "id": "MDRW2100010502.1.10",
          "form": "아하 옛날에 모두의마블 같은 겅가요 ㅎ",
          "original_form": "아하 옛날에 모두의마블 같은 겅가요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010502.1.11",
          "form": "그건 제가 안해봐서 모르겠네요ㅜㅜㅎㅎ",
          "original_form": "그건 제가 안해봐서 모르겠네요ㅜㅜㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010502.1.12",
          "form": "아하 ㅎ ㅎ ㅎㅎ 심즈는 왜 이제 그만하셨어요?",
          "original_form": "아하 ㅎ ㅎ ㅎㅎ 심즈는 왜 이제 그만하셨어요?",
          "speaker_id": "1",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010502.1.13",
          "form": "뭔가 반복되는 느낌?ㅎㅎ 급 질리더라구요ㅎㅎ",
          "original_form": "뭔가 반복되는 느낌?ㅎㅎ 급 질리더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100010502.1.14",
          "form": "아 뭔지 알아요",
          "original_form": "아 뭔지 알아요",
          "speaker_id": "1",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100010502.1.15",
          "form": "다른 사람이 하는 플레이는 엄청 재밌어보이는데",
          "original_form": "다른 사람이 하는 플레이는 엄청 재밌어보이는데",
          "speaker_id": "1",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100010502.1.16",
          "form": "막상 제가하면 재미없더라구요",
          "original_form": "막상 제가하면 재미없더라구요",
          "speaker_id": "1",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100010502.1.17",
          "form": "맞아요ㅋㅋ심즈 재밌게 하는 분들은 엄청 재밌게 하던데ㅠㅜ",
          "original_form": "맞아요ㅋㅋ심즈 재밌게 하는 분들은 엄청 재밌게 하던데ㅠㅜ",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010502.1.18",
          "form": "그러니까여 ㅎ ㅎ 하ㅏ 저한테 소질이 없나봐요",
          "original_form": "그러니까여 ㅎ ㅎ 하ㅏ 저한테 소질이 없나봐요",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010502.1.19",
          "form": "심즈 유투버들만 봐도 재밌어보여요",
          "original_form": "심즈 유투버들만 봐도 재밌어보여요",
