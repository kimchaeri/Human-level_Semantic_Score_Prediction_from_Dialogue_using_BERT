      "utterance": [
        {
          "id": "MDRW2100041355.1.1",
          "form": "오 맛나겠어용 전 통닭이랑 피자요!",
          "original_form": "오 맛나겠어용 전 통닭이랑 피자요!",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100041355.1.2",
          "form": "어디거 드셨어료??",
          "original_form": "어디거 드셨어료??",
          "speaker_id": "1",
          "time": "20210906 08:16"
        },
        {
          "id": "MDRW2100041355.1.3",
          "form": "도미노피자랑 통닭은 집앞에있는 통닭집요 ㅋ.ㅋ",
          "original_form": "도미노피자랑 통닭은 집앞에있는 통닭집요 ㅋ.ㅋ",
          "speaker_id": "2",
          "time": "20210906 08:50"
        },
        {
          "id": "MDRW2100041355.1.4",
          "form": "도미노 뭐 드셨어요?",
          "original_form": "도미노 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210906 12:04"
        },
        {
          "id": "MDRW2100041355.1.5",
          "form": "저도 도미노 좋아해요",
          "original_form": "저도 도미노 좋아해요",
          "speaker_id": "1",
          "time": "20210906 12:04"
        },
        {
          "id": "MDRW2100041355.1.6",
          "form": "어제는 기프티콘 쓰느라 포테이토피자용 ㅎㅎ",
          "original_form": "어제는 기프티콘 쓰느라 포테이토피자용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041355.1.7",
          "form": "아하 감자피자 좋아하세요?",
          "original_form": "아하 감자피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041355.1.8",
          "form": "전 그냥 그래요!",
          "original_form": "전 그냥 그래요!",
          "speaker_id": "1",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041355.1.9",
          "form": "저도 있으면 먹는정도 ㅋㅋ",
          "original_form": "저도 있으면 먹는정도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041355.1.10",
          "form": "ㅋㅋㅋㅋ비슷하군요",
          "original_form": "ㅋㅋㅋㅋ비슷하군요",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041355.1.11",
          "form": "뭐 좋아하세용",
          "original_form": "뭐 좋아하세용",
          "speaker_id": "2",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100041355.1.12",
          "form": "전 그.거기 블랙타이거인가",
          "original_form": "전 그.거기 블랙타이거인가",
          "speaker_id": "1",
          "time": "20210906 12:41"
        },
        {
          "id": "MDRW2100041355.1.13",
          "form": "그거 좋아해요",
          "original_form": "그거 좋아해요",
          "speaker_id": "1",
          "time": "20210906 12:41"
        },
        {
          "id": "MDRW2100041355.1.14",
          "form": "오 저두요 그거맛나죠",
          "original_form": "오 저두요 그거맛나죠",
          "speaker_id": "2",
          "time": "20210906 13:52"
        },
        {
          "id": "MDRW2100041355.1.15",
          "form": "ㅋㅋㅋㅋㅋㅠ 네 도미노",
