      "utterance": [
        {
          "id": "MDRW2100000524.1.1",
          "form": "오늘 점심 뭐드셨나요?",
          "original_form": "오늘 점심 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000524.1.2",
          "form": "오늘 맘스터치 싸이버거요!!",
          "original_form": "오늘 맘스터치 싸이버거요!!",
          "speaker_id": "1",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000524.1.3",
          "form": "와 마싯는거 드셨네용",
          "original_form": "와 마싯는거 드셨네용",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000524.1.4",
          "form": "근데 감자튀김이랑 콜라만 마시고 버거는 아직 손 안댔어요 ㅋㅋㄱ",
          "original_form": "근데 감자튀김이랑 콜라만 마시고 버거는 아직 손 안댔어요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000524.1.5",
          "form": "헉 왜안드셨어요??",
          "original_form": "헉 왜안드셨어요??",
          "speaker_id": "2",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000524.1.6",
          "form": "먹기전에 바로 콘푸라이트 먹었거든요 ㅠ",
          "original_form": "먹기전에 바로 콘푸라이트 먹었거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000524.1.7",
          "form": "배가 불러서 ㅎㅎ",
          "original_form": "배가 불러서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000524.1.8",
          "form": "앗~~그럼 저녁으로는 남은 버거 드실생각인가요??",
          "original_form": "앗~~그럼 저녁으로는 남은 버거 드실생각인가요??",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000524.1.9",
          "form": "딩동댕ㅋㅋㅋㄱ",
          "original_form": "딩동댕ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000524.1.10",
          "form": "더워서 아직 입맛이없네요",
          "original_form": "더워서 아직 입맛이없네요",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000524.1.11",
          "form": "ㅋㅋㅋ전 오늘 점심을 너무 배부르게 먹어서 생각이없네룡..",
          "original_form": "ㅋㅋㅋ전 오늘 점심을 너무 배부르게 먹어서 생각이없네룡..",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000524.1.12",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000524.1.13",
          "form": "분식 먹었어요!",
          "original_form": "분식 먹었어요!",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000524.1.14",
          "form": "제 최애음식인데 떡볶이랑 튀김 순대 드셨겠네요 ㅎㅈ",
          "original_form": "제 최애음식인데 떡볶이랑 튀김 순대 드셨겠네요 ㅎㅈ",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000524.1.15",
          "form": "튀김빼고",
          "original_form": "튀김빼고",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000524.1.16",
          "form": "떡볶이랑 김밥이랑 라면이랑 순대랑 오뎅먹었어요!!",
          "original_form": "떡볶이랑 김밥이랑 라면이랑 순대랑 오뎅먹었어요!!",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000524.1.17",
          "form": "많이먹었죠..",
          "original_form": "많이먹었죠..",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000524.1.18",
          "form": "와 혼자서 다 드신건 아니겠죠?",
          "original_form": "와 혼자서 다 드신건 아니겠죠?",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000524.1.19",
          "form": "다같이 먹었어요!!",
          "original_form": "다같이 먹었어요!!",
          "speaker_id": "2",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000524.1.20",
          "form": "ㅋㅋㄱㅋㄱ전 방금 막걸리에 두부김치 먹었어요",
