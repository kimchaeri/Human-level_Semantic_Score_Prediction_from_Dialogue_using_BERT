      "utterance": [
        {
          "id": "MDRW2100008392.1.1",
          "form": "부업 가끔하죠!",
          "original_form": "부업 가끔하죠!",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008392.1.2",
          "form": "본업은요?",
          "original_form": "본업은요?",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008392.1.3",
          "form": "본업은 군문제땜에",
          "original_form": "본업은 군문제땜에",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008392.1.4",
          "form": "군문제 해결후에 가질 생각입니다!",
          "original_form": "군문제 해결후에 가질 생각입니다!",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008392.1.5",
          "form": "부업 돈되요?",
          "original_form": "부업 돈되요?",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008392.1.6",
          "form": "조금씩 버는 맛으로 하는거같아요 ㅎㅎ",
          "original_form": "조금씩 버는 맛으로 하는거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008392.1.7",
          "form": "그래더 돈이 되니 하겠죠",
          "original_form": "그래더 돈이 되니 하겠죠",
          "speaker_id": "1",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008392.1.8",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008392.1.9",
          "form": "가만히 있는거보단",
          "original_form": "가만히 있는거보단",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008392.1.10",
          "form": "조금이라도 버는게 좋으니까요 ㅎㅎ",
          "original_form": "조금이라도 버는게 좋으니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008392.1.11",
          "form": "개인시간은 있어요?",
          "original_form": "개인시간은 있어요?",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008392.1.12",
          "form": "개인시간은 늘 가지고 있죠!",
          "original_form": "개인시간은 늘 가지고 있죠!",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008392.1.13",
          "form": "여유롭게요",
          "original_form": "여유롭게요",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008392.1.14",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008392.1.15",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008392.1.16",
          "form": "편해야 일이 수월하죠!",
