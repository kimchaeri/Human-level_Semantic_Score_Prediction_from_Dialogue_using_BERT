      "utterance": [
        {
          "id": "MDRW2100011060.1.1",
          "form": "오호 메모해두겠습니다 ㅎㅎㅎ",
          "original_form": "오호 메모해두겠습니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011060.1.2",
          "form": "앱테크용 게임말고 다른 관심맀는 게임 있으세요??",
          "original_form": "앱테크용 게임말고 다른 관심맀는 게임 있으세요??",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011060.1.3",
          "form": "다른건 잘안해요^^",
          "original_form": "다른건 잘안해요^^",
          "speaker_id": "1",
          "time": "20210824 00:17"
        },
        {
          "id": "MDRW2100011060.1.4",
          "form": "플레이랑은 함검색해보세요",
          "original_form": "플레이랑은 함검색해보세요",
          "speaker_id": "1",
          "time": "20210824 00:17"
        },
        {
          "id": "MDRW2100011060.1.5",
          "form": "아아 ㅎㅎ 네 감사합니다",
          "original_form": "아아 ㅎㅎ 네 감사합니다",
          "speaker_id": "2",
          "time": "20210824 00:17"
        },
        {
          "id": "MDRW2100011060.1.6",
          "form": "진짜열심히 하시는분은 하루 3천원씩도벌고",
          "original_form": "진짜열심히 하시는분은 하루 3천원씩도벌고",
          "speaker_id": "1",
          "time": "20210824 00:18"
        },
        {
          "id": "MDRW2100011060.1.7",
          "form": "ㅎㅎ 좋네용 다음에 한번 검색해볼게요",
          "original_form": "ㅎㅎ 좋네용 다음에 한번 검색해볼게요",
          "speaker_id": "2",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011060.1.8",
          "form": "저도 잠시빠져있을땐 40일정도해서  5만원 찾았거든요",
          "original_form": "저도 잠시빠져있을땐 40일정도해서  5만원 찾았거든요",
          "speaker_id": "1",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011060.1.9",
          "form": "ㅎㅎ 많이 벌으셨네요!!네네",
          "original_form": "ㅎㅎ 많이 벌으셨네요!!네네",
          "speaker_id": "2",
          "time": "20210824 00:21"
        },
        {
          "id": "MDRW2100011060.1.10",
          "form": "게임은 그러면 컴퓨터로도 아예 안하시는거죵",
          "original_form": "게임은 그러면 컴퓨터로도 아예 안하시는거죵",
          "speaker_id": "2",
          "time": "20210824 00:21"
        },
        {
          "id": "MDRW2100011060.1.11",
          "form": "광고도 없고 좋답니다",
          "original_form": "광고도 없고 좋답니다",
          "speaker_id": "1",
          "time": "20210824 00:21"
        },
        {
          "id": "MDRW2100011060.1.12",
          "form": "네네 컴터겜은 안해요",
          "original_form": "네네 컴터겜은 안해요",
          "speaker_id": "1",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011060.1.13",
          "form": "20년전에 포트리스 이런건해봤네요ㅋ",
          "original_form": "20년전에 포트리스 이런건해봤네요ㅋ",
          "speaker_id": "1",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011060.1.14",
          "form": "컴터겜도 하세요?",
          "original_form": "컴터겜도 하세요?",
          "speaker_id": "1",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011060.1.15",
          "form": "아 ㅎㅎ 전가끔 맞고도 쳐용",
          "original_form": "아 ㅎㅎ 전가끔 맞고도 쳐용",
          "speaker_id": "2",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011060.1.16",
          "form": "컴터로는 메이플 했었어요",
          "original_form": "컴터로는 메이플 했었어요",
          "speaker_id": "2",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011060.1.17",
          "form": "아 맞고 잼나죠ㅎㅎ",
          "original_form": "아 맞고 잼나죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:23"
        },
        {
          "id": "MDRW2100011060.1.18",
          "form": "그것도 많이 했었는데",
          "original_form": "그것도 많이 했었는데",
          "speaker_id": "1",
          "time": "20210824 00:23"
        },
        {
          "id": "MDRW2100011060.1.19",
          "form": "가끔 하면 엄청 재밌어요 ㅎㅎ시간 빨리가고",
          "original_form": "가끔 하면 엄청 재밌어요 ㅎㅎ시간 빨리가고",
          "speaker_id": "2",
          "time": "20210824 00:25"
        },
        {
          "id": "MDRW2100011060.1.20",
          "form": "중독되죠 맞고는ㅎㅎ",
          "original_form": "중독되죠 맞고는ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:25"
        },
        {
          "id": "MDRW2100011060.1.21",
          "form": "3고하고 따면 기쁘고",
          "original_form": "3고하고 따면 기쁘고",
          "speaker_id": "1",
          "time": "20210824 00:25"
        },
        {
          "id": "MDRW2100011060.1.22",
          "form": "맞아요 ㅋㅋㅋㅋ그러다 맨날 파산하지만 ㅠㅠㅠ",
          "original_form": "맞아요 ㅋㅋㅋㅋ그러다 맨날 파산하지만 ㅠㅠㅠ",
