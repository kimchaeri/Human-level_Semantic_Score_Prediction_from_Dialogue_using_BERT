      "utterance": [
        {
          "id": "MDRW2100009538.1.1",
          "form": "그러면 진짜 친화력이 필수겠어요",
          "original_form": "그러면 진짜 친화력이 필수겠어요",
          "speaker_id": "1",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100009538.1.2",
          "form": "코로나가 빨리 잠잠해져야할텐디",
          "original_form": "코로나가 빨리 잠잠해져야할텐디",
          "speaker_id": "1",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100009538.1.3",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009538.1.4",
          "form": "게임은 좋아하셔요,?",
          "original_form": "게임은 좋아하셔요,?",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009538.1.5",
          "form": "네! 좋아합니다",
          "original_form": "네! 좋아합니다",
          "speaker_id": "2",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009538.1.6",
          "form": "근데 하는 것보다 보는걸 좋아해요",
          "original_form": "근데 하는 것보다 보는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009538.1.7",
          "form": "아 진짜요? 유튜브로 많이봐요?",
          "original_form": "아 진짜요? 유튜브로 많이봐요?",
          "speaker_id": "1",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009538.1.8",
          "form": "유튜브로도 보고 스트리머 찾아보기도 해요",
          "original_form": "유튜브로도 보고 스트리머 찾아보기도 해요",
          "speaker_id": "2",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009538.1.9",
          "form": "name1님은 좋아하는 게임 있으신가요?",
          "original_form": "&name1&님은 좋아하는 게임 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009538.1.10",
          "form": "전 농작물 키우는 건 좋아해요",
          "original_form": "전 농작물 키우는 건 좋아해요",
          "speaker_id": "1",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009538.1.11",
          "form": "다른 사람들이릉흐는 군 좀 불편하더라고요",
          "original_form": "다른 사람들이릉흐는 군 좀 불편하더라고요",
          "speaker_id": "1",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009538.1.12",
          "form": "아 솔로 게임요?",
          "original_form": "아 솔로 게임요?",
          "speaker_id": "2",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009538.1.13",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100009538.1.14",
          "form": "그냥 애니팡 같은건 자주 했어요",
          "original_form": "그냥 애니팡 같은건 자주 했어요",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100009538.1.15",
          "form": "애니팡이 고전겜이지만 시간해소용으로 좋은 것같아요",
          "original_form": "애니팡이 고전겜이지만 시간해소용으로 좋은 것같아요",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100009538.1.16",
          "form": "마자요!!",
          "original_form": "마자요!!",
          "speaker_id": "1",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100009538.1.17",
          "form": "name2님은 오버워치같은거 해봤어요?",
          "original_form": "&name2&님은 오버워치같은거 해봤어요?",
          "speaker_id": "1",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100009538.1.18",
          "form": "쿠킹덤이 한창 유행이었잔ㄹ아요",
          "original_form": "쿠킹덤이 한창 유행이었잔ㄹ아요",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100009538.1.19",
          "form": "앗 아니용",
          "original_form": "앗 아니용",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100009538.1.20",
          "form": "앜ㅋㅋㅋ 너무 추억이네요",
          "original_form": "앜ㅋㅋㅋ 너무 추억이네요",
          "speaker_id": "1",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100009538.1.21",
          "form": "오버워치 하면 어지럽더라구요ㅋㅋㅋㅋ",
          "original_form": "오버워치 하면 어지럽더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100009538.1.22",
