      "utterance": [
        {
          "id": "MDRW2100018024.1.1",
          "form": "카페 좋아하세요?",
          "original_form": "카페 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018024.1.2",
          "form": "카페 좋아하죠",
          "original_form": "카페 좋아하죠",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018024.1.3",
          "form": "스타벅스를 제일 좋아해요~",
          "original_form": "스타벅스를 제일 좋아해요~",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018024.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018024.1.5",
          "form": "저는 투썸이요!",
          "original_form": "저는 투썸이요!",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018024.1.6",
          "form": "케이크 좋아하거든요~",
          "original_form": "케이크 좋아하거든요~",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018024.1.7",
          "form": "투썸이 디저트로 유명하죠 ㅎㅎ",
          "original_form": "투썸이 디저트로 유명하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018024.1.8",
          "form": "어떤 케이크 좋아하세요?",
          "original_form": "어떤 케이크 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018024.1.9",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018024.1.10",
          "form": "티라미수요!",
          "original_form": "티라미수요!",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018024.1.11",
          "form": "케이크 좋아하세요?",
          "original_form": "케이크 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018024.1.12",
          "form": "네 ㅜㅜ 저 케이크 미쳐요",
          "original_form": "네 ㅜㅜ 저 케이크 미쳐요",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018024.1.13",
          "form": "근데 기프티콘은 스타벅스가 많다보니 주로 스벅 가네요",
          "original_form": "근데 기프티콘은 스타벅스가 많다보니 주로 스벅 가네요",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018024.1.14",
          "form": "아ㅜ 스벅은 되게 무난히 가기 좋은거같아요",
          "original_form": "아ㅜ 스벅은 되게 무난히 가기 좋은거같아요",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100018024.1.15",
          "form": "ㅎㅎㅎ투썸 케이크는 가격대가 얼마 정도에요?",
          "original_form": "ㅎㅎㅎ투썸 케이크는 가격대가 얼마 정도에요?",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100018024.1.16",
          "form": "6천원정도가 평균이에요!",
          "original_form": "6천원정도가 평균이에요!",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100018024.1.17",
          "form": "투썸 싸게 먹는 법 있어요?",
          "original_form": "투썸 싸게 먹는 법 있어요?",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018024.1.18",
          "form": "거기 커피는 제 취향 아니더라구요 ㅜㅜ",
          "original_form": "거기 커피는 제 취향 아니더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018024.1.19",
          "form": "근처에 가까운곳이 거기뿐인지라..",
          "original_form": "근처에 가까운곳이 거기뿐인지라..",
          "speaker_id": "2",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018024.1.20",
          "form": "저도 거기 음료는 별로라서 케이크만 사가는편이에요",
          "original_form": "저도 거기 음료는 별로라서 케이크만 사가는편이에요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018024.1.21",
          "form": "사고 다음 날 먹어도 돼요?",
          "original_form": "사고 다음 날 먹어도 돼요?",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018024.1.22",
          "form": "유통기한이 짧나?",
          "original_form": "유통기한이 짧나?",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018024.1.23",
          "form": "거의 바로 먹는 편입니다!",
          "original_form": "거의 바로 먹는 편입니다!",
          "speaker_id": "2",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018024.1.24",
          "form": "남지 않아욧",
          "original_form": "남지 않아욧",
          "speaker_id": "2",
          "time": "20210826 22:26"
        }
      ]
    }
