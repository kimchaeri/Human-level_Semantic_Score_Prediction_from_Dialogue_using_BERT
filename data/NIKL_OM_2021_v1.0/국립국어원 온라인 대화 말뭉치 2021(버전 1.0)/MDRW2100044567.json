      "utterance": [
        {
          "id": "MDRW2100044567.1.1",
          "form": "티비는 자주 보시나요?",
          "original_form": "티비는 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044567.1.2",
          "form": "네 주말에 자주봐요",
          "original_form": "네 주말에 자주봐요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044567.1.3",
          "form": "예능도보구요",
          "original_form": "예능도보구요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044567.1.4",
          "form": "드라마도보구요ㅋㅋ",
          "original_form": "드라마도보구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044567.1.5",
          "form": "예능 뭐보시낭：?ㅡ",
          "original_form": "예능 뭐보시낭：?ㅡ",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044567.1.6",
          "form": "저는 아는형님도 보고",
          "original_form": "저는 아는형님도 보고",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044567.1.7",
          "form": "노는언니도 가끔봐요ㅋㅋ",
          "original_form": "노는언니도 가끔봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044567.1.8",
          "form": "문명특급도요!",
          "original_form": "문명특급도요!",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044567.1.9",
          "form": "아 놀뭐 보시나요?",
          "original_form": "아 놀뭐 보시나요?",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044567.1.10",
          "form": "아 놀뭐 저번에 지미유때 봤어요",
          "original_form": "아 놀뭐 저번에 지미유때 봤어요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044567.1.11",
          "form": "노래부르는거",
          "original_form": "노래부르는거",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044567.1.12",
          "form": "진짜좋던데요ㅋㅋ",
          "original_form": "진짜좋던데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044567.1.13",
          "form": "ㅋㅋ 아 노래 좋아하시는군요!",
          "original_form": "ㅋㅋ 아 노래 좋아하시는군요!",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044567.1.14",
          "form": "이번주에 정준하나왔거든요",
          "original_form": "이번주에 정준하나왔거든요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044567.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044567.1.16",
          "form": "오 주나형",
          "original_form": "오 주나형",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044567.1.17",
          "form": "나와서 뭐했어요?",
          "original_form": "나와서 뭐했어요?",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044567.1.18",
          "form": "예전 무도처럼 할거같아요",
          "original_form": "예전 무도처럼 할거같아요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044567.1.19",
          "form": "예전 무도 좋아하시면 추천!",
          "original_form": "예전 무도 좋아하시면 추천!",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044567.1.20",
          "form": "오 추천감사합니당ㅎㅎ",
          "original_form": "오 추천감사합니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044567.1.21",
          "form": "무도 좋아하셨나요?",
          "original_form": "무도 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044567.1.22",
          "form": "네 어린때 정말많이봤죠!",
          "original_form": "네 어린때 정말많이봤죠!",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044567.1.23",
          "form": "저도 팬이어서 좋았어욪ㅋㅈㅋ",
          "original_form": "저도 팬이어서 좋았어욪ㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210907 23:31"
        }
      ]
