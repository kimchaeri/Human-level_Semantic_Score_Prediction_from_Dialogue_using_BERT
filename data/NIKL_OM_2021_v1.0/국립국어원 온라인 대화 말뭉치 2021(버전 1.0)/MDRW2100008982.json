      "utterance": [
        {
          "id": "MDRW2100008982.1.1",
          "form": "곧 개학인데 어떠세요?",
          "original_form": "곧 개학인데 어떠세요?",
          "speaker_id": "1",
          "time": "20210822 20:09"
        },
        {
          "id": "MDRW2100008982.1.2",
          "form": "내일이요ㅠ",
          "original_form": "내일이요ㅠ",
          "speaker_id": "2",
          "time": "20210822 20:10"
        },
        {
          "id": "MDRW2100008982.1.3",
          "form": "언제세요?",
          "original_form": "언제세요?",
          "speaker_id": "2",
          "time": "20210822 20:10"
        },
        {
          "id": "MDRW2100008982.1.4",
          "form": "악!!!! 전 31일입니다^-^",
          "original_form": "악!!!! 전 31일입니다^-^",
          "speaker_id": "1",
          "time": "20210822 20:10"
        },
        {
          "id": "MDRW2100008982.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008982.1.6",
          "form": "스트레스는 없으세요?",
          "original_form": "스트레스는 없으세요?",
          "speaker_id": "2",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008982.1.7",
          "form": "이거 조삼모사인거 아시죠? 어짜피 수업일수는 190일로 동일합니다 ㅋㅋ",
          "original_form": "이거 조삼모사인거 아시죠? 어짜피 수업일수는 190일로 동일합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008982.1.8",
          "form": "개학하면 스트레스가 엄청 나겠죠? ㅠ",
          "original_form": "개학하면 스트레스가 엄청 나겠죠? ㅠ",
          "speaker_id": "1",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008982.1.9",
          "form": "그렇긴한데 당장ㅠ",
          "original_form": "그렇긴한데 당장ㅠ",
          "speaker_id": "2",
          "time": "20210822 20:12"
        },
        {
          "id": "MDRW2100008982.1.10",
          "form": "겨울 방학이 짧겠죠? ㅠ",
          "original_form": "겨울 방학이 짧겠죠? ㅠ",
          "speaker_id": "1",
          "time": "20210822 20:13"
        },
        {
          "id": "MDRW2100008982.1.11",
          "form": "그럴것같아요",
          "original_form": "그럴것같아요",
          "speaker_id": "2",
          "time": "20210822 20:14"
        },
        {
          "id": "MDRW2100008982.1.12",
          "form": "개학준비는 좀 하셨어요?",
          "original_form": "개학준비는 좀 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 20:15"
        },
        {
          "id": "MDRW2100008982.1.13",
          "form": "그냥 부딧히기요",
          "original_form": "그냥 부딧히기요",
          "speaker_id": "2",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008982.1.14",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "2",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008982.1.15",
          "form": "맞아요 미리 준비해도 제 생각과는 달라지는게 많으니까요 ㅠ",
          "original_form": "맞아요 미리 준비해도 제 생각과는 달라지는게 많으니까요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100008982.1.16",
          "form": "내일이 두려워요",
          "original_form": "내일이 두려워요",
          "speaker_id": "2",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100008982.1.17",
          "form": "진상인 학부모는 없으신가요?",
          "original_form": "진상인 학부모는 없으신가요?",
          "speaker_id": "1",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100008982.1.18",
          "form": "올해는 없는데 예전엔 좀 있었어요",
          "original_form": "올해는 없는데 예전엔 좀 있었어요",
          "speaker_id": "2",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100008982.1.19",
          "form": "복 받으셨네요. 전 제 운이 나쁜가 진상 학부모가 매년 있네요 ㅜ 이게 제일 스트레스 ㅠ",
          "original_form": "복 받으셨네요. 전 제 운이 나쁜가 진상 학부모가 매년 있네요 ㅜ 이게 제일 스트레스 ㅠ",
          "speaker_id": "1",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008982.1.20",
          "form": "마져요",
          "original_form": "마져요",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008982.1.21",
          "form": "완전 운에 맡겨진",
          "original_form": "완전 운에 맡겨진",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100008982.1.22",
          "form": "사실 우리 일이 학부모 스트레스가 제일 크지 않아요?",
          "original_form": "사실 우리 일이 학부모 스트레스가 제일 크지 않아요?",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008982.1.23",
          "form": "마져요",
          "original_form": "마져요",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100008982.1.24",
          "form": "애들은 애니까 그렇지만 학부모는ㅠ",
          "original_form": "애들은 애니까 그렇지만 학부모는ㅠ",
          "speaker_id": "2",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100008982.1.25",
          "form": "올해 학부모는 교사들 배식 줄이고 애들 많이 주라고 하질 않나 5분만 늦어도 전화하질 않나 ㅠ",
          "original_form": "올해 학부모는 교사들 배식 줄이고 애들 많이 주라고 하질 않나 5분만 늦어도 전화하질 않나 ㅠ",
          "speaker_id": "1",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100008982.1.26",
          "form": "읭?",
          "original_form": "읭?",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008982.1.27",
          "form": "하 말만 들어도",
          "original_form": "하 말만 들어도",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008982.1.28",
          "form": "안내를 5번이 나갔는데 이게 그건지 몰랐다는 학부모도 있고요 난리부르스예요",
          "original_form": "안내를 5번이 나갔는데 이게 그건지 몰랐다는 학부모도 있고요 난리부르스예요",
          "speaker_id": "1",
          "time": "20210822 20:28"
        }
      ]
    }
  ]
