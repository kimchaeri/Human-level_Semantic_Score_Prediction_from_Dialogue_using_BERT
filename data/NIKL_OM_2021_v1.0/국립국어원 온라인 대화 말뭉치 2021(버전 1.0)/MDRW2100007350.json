      "utterance": [
        {
          "id": "MDRW2100007350.1.1",
          "form": "al가 뭔지 알아?",
          "original_form": "al가 뭔지 알아?",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007350.1.2",
          "form": "나는 몰라",
          "original_form": "나는 몰라",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007350.1.3",
          "form": "걍 인공지능 아녀?",
          "original_form": "걍 인공지능 아녀?",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007350.1.4",
          "form": "아니 ai학과가 있어?",
          "original_form": "아니 ai학과가 있어?",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007350.1.5",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007350.1.6",
          "form": "똑똑",
          "original_form": "똑똑",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007350.1.7",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007350.1.8",
          "form": "습관이 무섭네",
          "original_form": "습관이 무섭네",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007350.1.9",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007350.1.10",
          "form": "모르겠어",
          "original_form": "모르겠어",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007350.1.11",
          "form": "요즘시대 어렵다",
          "original_form": "요즘시대 어렵다",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007350.1.12",
          "form": "저게 대세인가봐",
          "original_form": "저게 대세인가봐",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007350.1.13",
          "form": "뒤쳐지기 전에 다 알아놔야하는데",
          "original_form": "뒤쳐지기 전에 다 알아놔야하는데",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007350.1.14",
          "form": "유튜브 알고리즘도 ai인가?",
          "original_form": "유튜브 알고리즘도 ai인가?",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007350.1.15",
          "form": "인공지능",
          "original_form": "인공지능",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007350.1.16",
          "form": "인터넷 서치하는것도 몇분있으면 팝업창에 뜨더라",
          "original_form": "인터넷 서치하는것도 몇분있으면 팝업창에 뜨더라",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007350.1.17",
          "form": "무서워",
          "original_form": "무서워",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007350.1.18",
          "form": "그치만 돈이 되겠다 대세니깐",
          "original_form": "그치만 돈이 되겠다 대세니깐",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007350.1.19",
          "form": "그 영화생각난다",
          "original_form": "그 영화생각난다",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007350.1.20",
          "form": "아이로봇?",
          "original_form": "아이로봇?",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007350.1.21",
          "form": "마이너리리포트",
          "original_form": "마이너리리포트",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007350.1.22",
          "form": "로봇이 지배하는거야",
          "original_form": "로봇이 지배하는거야",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007350.1.23",
          "form": "아 존잼",
          "original_form": "아 존잼",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007350.1.24",
          "form": "뭔가 점점 인공지능에 의지하는 느낌",
          "original_form": "뭔가 점점 인공지능에 의지하는 느낌",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007350.1.25",
          "form": "세상이 너무 빠르게 돌아가",
          "original_form": "세상이 너무 빠르게 돌아가",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007350.1.26",
          "form": "나보다 똑똑해질거 같아",
          "original_form": "나보다 똑똑해질거 같아",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007350.1.27",
          "form": "시리나",
          "original_form": "시리나",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007350.1.28",
          "form": "이미 똑똑한걸? 걔네는 그림도 그림",
          "original_form": "이미 똑똑한걸? 걔네는 그림도 그림",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007350.1.29",
          "form": "빅스비도 그런걸까",
          "original_form": "빅스비도 그런걸까",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007350.1.30",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210822 01:38"
        }
      ]
