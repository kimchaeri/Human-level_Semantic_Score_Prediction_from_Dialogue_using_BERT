      "utterance": [
        {
          "id": "MDRW2100041890.1.1",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041890.1.2",
          "form": "친구 하나가 늘었엉",
          "original_form": "친구 하나가 늘었엉",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041890.1.3",
          "form": "혹시 그",
          "original_form": "혹시 그",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041890.1.4",
          "form": "지령감실이라고",
          "original_form": "지령감실이라고",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041890.1.5",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041890.1.6",
          "form": "음...모르겠는데",
          "original_form": "음...모르겠는데",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.7",
          "form": "혹시 가방에",
          "original_form": "혹시 가방에",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.8",
          "form": "어디 들어가기에 넘 랩이 낮아서 빠꾸먹는 경우가",
          "original_form": "어디 들어가기에 넘 랩이 낮아서 빠꾸먹는 경우가",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.9",
          "form": "가방이었어",
          "original_form": "가방이었어",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.10",
          "form": "청록색 열쇄같은거",
          "original_form": "청록색 열쇄같은거",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.11",
          "form": "있어?",
          "original_form": "있어?",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.12",
          "form": "아니...",
          "original_form": "아니...",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.13",
          "form": "그 들어가고 싶어도 못들어가는 그곳이니",
          "original_form": "그 들어가고 싶어도 못들어가는 그곳이니",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.14",
          "form": "함",
          "original_form": "함",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.15",
          "form": "민트빛 빛이 나오는",
          "original_form": "민트빛 빛이 나오는",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.16",
          "form": "가방 찾아봐",
          "original_form": "가방 찾아봐",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.17",
          "form": "방같이 생긴",
          "original_form": "방같이 생긴",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.18",
          "form": "ㅇㅇ그거",
          "original_form": "ㅇㅇ그거",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.19",
          "form": "그걸 까면",
          "original_form": "그걸 까면",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.20",
          "form": "못들어갔어..ㅠ",
          "original_form": "못들어갔어..ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041890.1.21",
          "form": "원석을 40개씩",
          "original_form": "원석을 40개씩",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041890.1.22",
          "form": "주거든",
          "original_form": "주거든",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041890.1.23",
          "form": "혹시 지도 얼마나 열었어?",
          "original_form": "혹시 지도 얼마나 열었어?",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041890.1.24",
          "form": "열쇠없다고 빠꾸먹음",
          "original_form": "열쇠없다고 빠꾸먹음",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041890.1.25",
          "form": "어제 뭐볼땐 11퍼였는데",
          "original_form": "어제 뭐볼땐 11퍼였는데",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041890.1.26",
          "form": "그게 맞는지 헷갈리네",
          "original_form": "그게 맞는지 헷갈리네",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041890.1.27",
          "form": "접속 될때 스샷 찍어서 보여줘바",
          "original_form": "접속 될때 스샷 찍어서 보여줘바",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041890.1.28",
          "form": "보냈엉 방금 신상 하나 열어서 넓어졌다",
          "original_form": "보냈엉 방금 신상 하나 열어서 넓어졌다",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041890.1.29",
          "form": "비경에서 감실열쇠도 나오고",
          "original_form": "비경에서 감실열쇠도 나오고",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041890.1.30",
          "form": "난 13랩이지..",
          "original_form": "난 13랩이지..",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041890.1.31",
          "form": "원소시련은 튜토야",
          "original_form": "원소시련은 튜토야",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041890.1.32",
          "form": "저거 비경 레벨 기준이",
          "original_form": "저거 비경 레벨 기준이",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041890.1.33",
          "form": "모험렙 아니고",
          "original_form": "모험렙 아니고",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041890.1.34",
          "form": "캐릭터들 평균 레벨일걸?",
          "original_form": "캐릭터들 평균 레벨일걸?",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041890.1.35",
          "form": "원소시련 그 과부하 융해 증발 감전 그런거",
          "original_form": "원소시련 그 과부하 융해 증발 감전 그런거",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041890.1.36",
          "form": "이해하기 힘든사람을 위한",
          "original_form": "이해하기 힘든사람을 위한",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041890.1.37",
          "form": "튜토리얼이고",
          "original_form": "튜토리얼이고",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041890.1.38",
          "form": "저거는 캐를 지급해줘",
          "original_form": "저거는 캐를 지급해줘",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041890.1.39",
          "form": "아 그래? 근데 못 가던데 흠... 다시 도전을 해보겠습니다",
          "original_form": "아 그래? 근데 못 가던데 흠... 다시 도전을 해보겠습니다",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041890.1.40",
          "form": "아 그거 모험렙 제한이 있나??",
          "original_form": "아 그거 모험렙 제한이 있나??",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041890.1.41",
          "form": "안가본지 너무 오래돼서 기억이 안나는군.....",
          "original_form": "안가본지 너무 오래돼서 기억이 안나는군.....",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041890.1.42",
          "form": "여튼 여기저기 다녀봐",
          "original_form": "여튼 여기저기 다녀봐",
          "speaker_id": "2",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100041890.1.43",
          "form": "ㅇㅇ 제한있어 지금 보니가 16랩이야",
          "original_form": "ㅇㅇ 제한있어 지금 보니가 16랩이야",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041890.1.44",
          "form": "일단 열심히 키우고 갈게ㅠ",
          "original_form": "일단 열심히 키우고 갈게ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041890.1.45",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210906 18:01"
        }
      ]
    }
  ]
