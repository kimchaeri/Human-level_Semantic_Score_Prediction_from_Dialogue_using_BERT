      "utterance": [
        {
          "id": "MDRW2100047519.1.1",
          "form": "보시는 영상 있으세료??",
          "original_form": "보시는 영상 있으세료??",
          "speaker_id": "2",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047519.1.2",
          "form": "유튜브 혹시 보세요?",
          "original_form": "유튜브 혹시 보세요?",
          "speaker_id": "1",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100047519.1.3",
          "form": "네 봐요!",
          "original_form": "네 봐요!",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047519.1.4",
          "form": "어떤거 보세요?ㅋㅋ",
          "original_form": "어떤거 보세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100047519.1.5",
          "form": "ㅋㅋ 저 다양하게 다 봐요",
          "original_form": "ㅋㅋ 저 다양하게 다 봐요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100047519.1.6",
          "form": "저는 다이어트 먹방 뷰티 요렇게 봐요ㅋㅋ",
          "original_form": "저는 다이어트 먹방 뷰티 요렇게 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047519.1.7",
          "form": "ㅋㅋㅋ 다이어트 누구 보세오?",
          "original_form": "ㅋㅋㅋ 다이어트 누구 보세오?",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100047519.1.8",
          "form": "땅끄부부님거요!",
          "original_form": "땅끄부부님거요!",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047519.1.9",
          "form": "헉 저도요 ㅎ",
          "original_form": "헉 저도요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047519.1.10",
          "form": "효과 많이 보셨나요?",
          "original_form": "효과 많이 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047519.1.11",
          "form": "와우ㅋㅋㅋ",
          "original_form": "와우ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047519.1.12",
          "form": "저는 매일 하는게 아니라 시간 날 때만 잠깐 하는거라 효과보는데 좀 걸렸어요",
          "original_form": "저는 매일 하는게 아니라 시간 날 때만 잠깐 하는거라 효과보는데 좀 걸렸어요",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047519.1.13",
          "form": "몇개월 걸렸는데 4키로정도 뺐어요ㅋㅋ물론 군것질 줄이구요ㅜ",
          "original_form": "몇개월 걸렸는데 4키로정도 뺐어요ㅋㅋ물론 군것질 줄이구요ㅜ",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047519.1.14",
          "form": "와.. 4개월..",
          "original_form": "와.. 4개월..",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100047519.1.15",
          "form": "저도 주말만 하거등요 효과 보고싶어요 ㅠㅠ",
          "original_form": "저도 주말만 하거등요 효과 보고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100047519.1.16",
          "form": "저는 개인적으로 운동도 운동이지만 군것질이 줄어드니까 효과가 더 컸어요ㅋㅋㅋㅋㅋ",
          "original_form": "저는 개인적으로 운동도 운동이지만 군것질이 줄어드니까 효과가 더 컸어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100047519.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 아 원래 그런 소리듀 있잖아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아 원래 그런 소리듀 있잖아요",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047519.1.18",
          "form": "먹는 걸 반절로만 줄어도 빠진다는..",
          "original_form": "먹는 걸 반절로만 줄어도 빠진다는..",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047519.1.19",
          "form": "ㅋㅋㅋㅋㅜㅠ 맛있는 거 먹는 맛에 사는건데ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅜㅠ 맛있는 거 먹는 맛에 사는건데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047519.1.20",
          "form": "뷰티유튜브도 보세요?",
          "original_form": "뷰티유튜브도 보세요?",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100047519.1.21",
          "form": "네 긍데 자주는 안봐요ㅋㅋㅋㅋ",
          "original_form": "네 긍데 자주는 안봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:40"
