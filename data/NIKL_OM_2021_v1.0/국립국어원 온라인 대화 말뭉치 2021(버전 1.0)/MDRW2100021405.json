          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021405.1.6",
          "form": "알바가서도",
          "original_form": "알바가서도",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021405.1.7",
          "form": "오늘은 뭐먹었어",
          "original_form": "오늘은 뭐먹었어",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021405.1.8",
          "form": "언니들이name2 먹고 싶은거 다 먹어~~",
          "original_form": "언니들이&name2& 먹고 싶은거 다 먹어~~",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021405.1.9",
          "form": "이래서",
          "original_form": "이래서",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021405.1.10",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100021405.1.11",
          "form": "말그대로 다 먹는중",
          "original_form": "말그대로 다 먹는중",
          "speaker_id": "2",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100021405.1.12",
          "form": "그건진짜 부럽다",
