          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100021405.1.19",
          "form": "진짜 이해 안가는거 많네",
          "original_form": "진짜 이해 안가는거 많네",
          "speaker_id": "2",
          "time": "20210828 18:35"
        },
