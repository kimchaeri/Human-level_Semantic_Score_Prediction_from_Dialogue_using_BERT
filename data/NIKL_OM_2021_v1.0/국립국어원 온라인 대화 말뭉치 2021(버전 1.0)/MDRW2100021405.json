          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021405.1.4",
          "form": "그러고 있어",
          "original_form": "그러고 있어",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021405.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
