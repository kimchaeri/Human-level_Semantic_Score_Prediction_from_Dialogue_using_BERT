      "utterance": [
        {
          "id": "MDRW2100026600.1.1",
          "form": "요즘 특히 신경쓰는 부분 있으세요?",
          "original_form": "요즘 특히 신경쓰는 부분 있으세요?",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026600.1.2",
          "form": "발목이 얼른 나아서 런닝뛰고싶어요ㅋㅋ",
          "original_form": "발목이 얼른 나아서 런닝뛰고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026600.1.3",
          "form": "백신맞으셨어요?",
          "original_form": "백신맞으셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026600.1.4",
          "form": "아 그러시겠구나",
          "original_form": "아 그러시겠구나",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026600.1.5",
          "form": "저 백신 일차는 맞고 이차 기다리고 있어요",
          "original_form": "저 백신 일차는 맞고 이차 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026600.1.6",
          "form": "맞으셨어요??",
          "original_form": "맞으셨어요??",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026600.1.7",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026600.1.8",
          "form": "화이자 맞았는데 name1도??",
          "original_form": "화이자 맞았는데 &name1&도??",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026600.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026600.1.10",
          "form": "근육통때문에 며칠 힘들었어요ㅠ",
          "original_form": "근육통때문에 며칠 힘들었어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026600.1.11",
          "form": "그러셨구나 저는 팔만 좀 아프고 괜찮았어요 근데 이차때는 심할까봐 좀 걱정돼요",
          "original_form": "그러셨구나 저는 팔만 좀 아프고 괜찮았어요 근데 이차때는 심할까봐 좀 걱정돼요",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026600.1.12",
          "form": "맞아요 이차는 더 아플거라던데",
          "original_form": "맞아요 이차는 더 아플거라던데",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026600.1.13",
          "form": "걱정돼요",
          "original_form": "걱정돼요",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026600.1.14",
          "form": "아직 화이자 이차 맞은 사람이 가까이엔 없는데 부작용 심하다는 얘기를 건너건너서 들어서요",
          "original_form": "아직 화이자 이차 맞은 사람이 가까이엔 없는데 부작용 심하다는 얘기를 건너건너서 들어서요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026600.1.15",
          "form": "뭐 심한 사람은 심하고 아닌사람은 괜찮고 그렇겠지만요 ㅋㅋㅋ",
          "original_form": "뭐 심한 사람은 심하고 아닌사람은 괜찮고 그렇겠지만요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026600.1.16",
          "form": "오,..",
          "original_form": "오,..",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026600.1.17",
          "form": "2차접종언제세요?!",
          "original_form": "2차접종언제세요?!",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026600.1.18",
          "form": "저 구월 마지막주요",
          "original_form": "저 구월 마지막주요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026600.1.19",
          "form": "밀려서 이주후더라고요",
          "original_form": "밀려서 이주후더라고요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026600.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026600.1.21",
          "form": "저도에요",
          "original_form": "저도에요",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026600.1.22",
          "form": "똑같네요!",
          "original_form": "똑같네요!",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026600.1.23",
          "form": "밀려서막주됐어요",
          "original_form": "밀려서막주됐어요",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026600.1.24",
          "form": "빨리 맞아서 끝내버리고 싶었는데 ㅋㅋㅋ",
          "original_form": "빨리 맞아서 끝내버리고 싶었는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026600.1.25",
          "form": "아쉽더라고요ㅋㅋ 추석전이라 좋아했거든욬ㅋㅋ",
          "original_form": "아쉽더라고요ㅋㅋ 추석전이라 좋아했거든욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026600.1.26",
          "form": "그쵸ㅠ 매도먼저맞는게좋은데",
          "original_form": "그쵸ㅠ 매도먼저맞는게좋은데",
          "speaker_id": "1",
