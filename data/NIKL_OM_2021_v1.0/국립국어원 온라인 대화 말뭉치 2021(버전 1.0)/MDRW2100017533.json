      "utterance": [
        {
          "id": "MDRW2100017533.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017533.1.2",
          "form": "요즘 미세먼지 없어서 좋은 것 같아요~",
          "original_form": "요즘 미세먼지 없어서 좋은 것 같아요~",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017533.1.3",
          "form": "안녕하세요~ 네 요새 날도 선선해지고",
          "original_form": "안녕하세요~ 네 요새 날도 선선해지고",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017533.1.4",
          "form": "날씨 되게 좋은것같아요",
          "original_form": "날씨 되게 좋은것같아요",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017533.1.5",
          "form": "그쵸? 이렇게 하늘이 오랫동안 맑은 게 오랜만이네요",
          "original_form": "그쵸? 이렇게 하늘이 오랫동안 맑은 게 오랜만이네요",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017533.1.6",
          "form": "요새 비가 와서 우중충하긴 하지만..ㅜㅜ",
          "original_form": "요새 비가 와서 우중충하긴 하지만..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017533.1.7",
          "form": "지금도 약간 어두컴컴하긴 한데",
          "original_form": "지금도 약간 어두컴컴하긴 한데",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017533.1.8",
          "form": "그래도 대체로 날이 좋은편이라ㅎㅎ",
          "original_form": "그래도 대체로 날이 좋은편이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017533.1.9",
          "form": "날씨가 좋으면 기분도 좋아지는 느낌 듥ᆢ",
          "original_form": "날씨가 좋으면 기분도 좋아지는 느낌 듥ᆢ",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017533.1.10",
          "form": "들고 좋은것같아요!",
          "original_form": "들고 좋은것같아요!",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017533.1.11",
          "form": "그쵸 ㅎ 날씨는 잘 챙겨보는 편이에요?",
          "original_form": "그쵸 ㅎ 날씨는 잘 챙겨보는 편이에요?",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017533.1.12",
          "form": "최근에 소나기 갑자기 오고 그래서 우산 챙기느라 챙겨보긴했어요ㅎㅎ",
          "original_form": "최근에 소나기 갑자기 오고 그래서 우산 챙기느라 챙겨보긴했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017533.1.13",
          "form": "어 맞아요 저 최근에 식당 가다가 갑자기 비 쏟아져서",
          "original_form": "어 맞아요 저 최근에 식당 가다가 갑자기 비 쏟아져서",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017533.1.14",
          "form": "비 다맞고 그랬는데 그 뒤로 우산 작은거 챙겨다녀요 ㅜㅜ",
          "original_form": "비 다맞고 그랬는데 그 뒤로 우산 작은거 챙겨다녀요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017533.1.15",
          "form": "하늘에 구멍난듯이 엄청 쏟아지더라구요",
          "original_form": "하늘에 구멍난듯이 엄청 쏟아지더라구요",
          "speaker_id": "2",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017533.1.16",
          "form": "쫄딱 젖었던 기억ㅠㅠ",
          "original_form": "쫄딱 젖었던 기억ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017533.1.17",
          "form": "맞아요 갑자기 소나기 내리면 진짜 당황스럽더라구요.",
          "original_form": "맞아요 갑자기 소나기 내리면 진짜 당황스럽더라구요.",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100017533.1.18",
          "form": "소나기 갑자기 내리니 당황스럽더라구요.",
          "original_form": "소나기 갑자기 내리니 당황스럽더라구요.",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017533.1.19",
          "form": "그뒤로는 햇빛 가릴겸 우양산 들고다녀요ㅎㅎ",
          "original_form": "그뒤로는 햇빛 가릴겸 우양산 들고다녀요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017533.1.20",
          "form": "오~ 저도 우양산 새로 샀는데 너무 무거워서 창고행..",
          "original_form": "오~ 저도 우양산 새로 샀는데 너무 무거워서 창고행..",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017533.1.21",
          "form": "앗ㅜㅜㅜ",
          "original_form": "앗ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017533.1.22",
          "form": "주말 날씨 혹시 확인해보셨어요?",
          "original_form": "주말 날씨 혹시 확인해보셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017533.1.23",
          "form": "역시 휴대하기엔 무게가 꽤 중요한것같아요",
          "original_form": "역시 휴대하기엔 무게가 꽤 중요한것같아요",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017533.1.24",
          "form": "나무 손잡이로 된건 사지마세요..ㅠㅠ",
          "original_form": "나무 손잡이로 된건 사지마세요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017533.1.25",
          "form": "아직요!",
          "original_form": "아직요!",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017533.1.26",
          "form": "이름만 들어도 무게가 있을것같은ㅠ",
          "original_form": "이름만 들어도 무게가 있을것같은ㅠ",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017533.1.27",
          "form": "비 내리는 날씨 좋아해요?",
          "original_form": "비 내리는 날씨 좋아해요?",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017533.1.28",
          "form": "비..는 집에서 창밖 소리를 듣는게 좋아요ㅎㅎ",
          "original_form": "비..는 집에서 창밖 소리를 듣는게 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017533.1.29",
          "form": "직접 맞는거 말구..",
          "original_form": "직접 맞는거 말구..",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017533.1.30",
          "form": "아 ㅎㅎ 비 오는 날엔 이상하게 파전, 김치전이 생각나요.",
          "original_form": "아 ㅎㅎ 비 오는 날엔 이상하게 파전, 김치전이 생각나요.",
          "speaker_id": "1",
          "time": "20210826 20:19"
        }
      ]
