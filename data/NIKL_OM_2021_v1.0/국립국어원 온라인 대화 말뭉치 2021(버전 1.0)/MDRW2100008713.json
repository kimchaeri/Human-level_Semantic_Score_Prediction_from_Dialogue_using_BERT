      "utterance": [
        {
          "id": "MDRW2100008713.1.1",
          "form": "알바해본적 있어요?",
          "original_form": "알바해본적 있어요?",
          "speaker_id": "2",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008713.1.2",
          "form": "앗 저 빵집 알바 고인물이랍니다",
          "original_form": "앗 저 빵집 알바 고인물이랍니다",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008713.1.3",
          "form": "개인빵집부터 프랜차이즈 빵집은 다 해봤습니다",
          "original_form": "개인빵집부터 프랜차이즈 빵집은 다 해봤습니다",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008713.1.4",
          "form": "우와 혹시 파바에서도 하셨나요??",
          "original_form": "우와 혹시 파바에서도 하셨나요??",
          "speaker_id": "2",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100008713.1.5",
          "form": "헉 맞아요",
          "original_form": "헉 맞아요",
          "speaker_id": "1",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008713.1.6",
          "form": "방금도 파바 마감 기깔나게 하고왔슴다",
          "original_form": "방금도 파바 마감 기깔나게 하고왔슴다",
          "speaker_id": "1",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008713.1.7",
          "form": "우와~ 파바 알바 하고 계셨어요!?",
          "original_form": "우와~ 파바 알바 하고 계셨어요!?",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008713.1.8",
          "form": "수고하셨어요~",
          "original_form": "수고하셨어요~",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008713.1.9",
          "form": "네네넹",
          "original_form": "네네넹",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008713.1.10",
          "form": "감삼다 감삼다",
          "original_form": "감삼다 감삼다",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008713.1.11",
          "form": "알바하시는거 있으신가요?",
          "original_form": "알바하시는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.12",
          "form": "저도 작년엔가 파바 실습? 하루했다가 그 이후로 연락이 안왔었죠...",
          "original_form": "저도 작년엔가 파바 실습? 하루했다가 그 이후로 연락이 안왔었죠...",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.13",
          "form": "저 편의점해요!",
          "original_form": "저 편의점해요!",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.15",
          "form": "씨유",
          "original_form": "씨유",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.16",
          "form": "?!",
          "original_form": "?!",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.17",
          "form": "왘",
          "original_form": "왘",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.20",
          "form": "와 촉 오늘 좋다",
          "original_form": "와 촉 오늘 좋다",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.21",
          "form": "그니까요 깜짝놀랐어요 ㅋㅋㅋ",
          "original_form": "그니까요 깜짝놀랐어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.22",
          "form": "편의점 포스 넘 복잡해요",
          "original_form": "편의점 포스 넘 복잡해요",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008713.1.23",
          "form": "파바놈들 인재를 놓쳤네요",
          "original_form": "파바놈들 인재를 놓쳤네요",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008713.1.24",
          "form": "네~?? 파바가 훨씬 힘들지않아요?",
          "original_form": "네~?? 파바가 훨씬 힘들지않아요?",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008713.1.25",
          "form": "빵 이름도 외워야되고",
          "original_form": "빵 이름도 외워야되고",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008713.1.26",
          "form": "빵 이름은 먹으면 외워지더라고요",
          "original_form": "빵 이름은 먹으면 외워지더라고요",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008713.1.27",
          "form": "편의점 무슨페이가 엄청 많이 되더라고요",
          "original_form": "편의점 무슨페이가 엄청 많이 되더라고요",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008713.1.28",
          "form": "씨유는 티멤버십만 할인되죠?!",
          "original_form": "씨유는 티멤버십만 할인되죠?!",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008713.1.29",
          "form": "그냥 바코드 찍으면 돼요 ㅋㅋ",
          "original_form": "그냥 바코드 찍으면 돼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008713.1.30",
          "form": "저 그런거 다 못외워요 대신 포스기가 알아서 해주죠",
          "original_form": "저 그런거 다 못외워요 대신 포스기가 알아서 해주죠",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008713.1.31",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008713.1.32",
          "form": "알바는 그래도 됩니다",
          "original_form": "알바는 그래도 됩니다",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008713.1.33",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.34",
          "form": "마대로 오늘 바닥 광나게 닦았는데",
          "original_form": "마대로 오늘 바닥 광나게 닦았는데",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.35",
          "form": "편의점 알바가 정말 편하더라고요",
          "original_form": "편의점 알바가 정말 편하더라고요",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.36",
          "form": "손님이 킥보드 타고 들어오셔서",
          "original_form": "손님이 킥보드 타고 들어오셔서",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.37",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.38",
          "form": "하..(이깍물)",
          "original_form": "하..(이깍물)",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.39",
          "form": "편의점 그래도",
          "original_form": "편의점 그래도",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.40",
          "form": "점바점이라더라고요",
          "original_form": "점바점이라더라고요",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.41",
          "form": "하지만 편한곳이다?",
          "original_form": "하지만 편한곳이다?",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.42",
          "form": "놓치지 말아야한다.",
          "original_form": "놓치지 말아야한다.",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008713.1.43",
          "form": "ㅋㅋㅋ 일 저체가 단순하고 편해요 손님도 별로 없어요 굿",
          "original_form": "ㅋㅋㅋ 일 저체가 단순하고 편해요 손님도 별로 없어요 굿",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008713.1.44",
          "form": "굿 사장님이 나가라할 때까지",
          "original_form": "굿 사장님이 나가라할 때까지",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008713.1.45",
          "form": "안나가보자구요",
          "original_form": "안나가보자구요",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008713.1.46",
          "form": "그러려구요 지금까지 했던 알바 중에 제일 편해요",
          "original_form": "그러려구요 지금까지 했던 알바 중에 제일 편해요",
          "speaker_id": "2",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100008713.1.47",
          "form": "일주일에 한두번 정도만 하지만 아예 수입이 없는 것보다는 백배낫죠!!",
          "original_form": "일주일에 한두번 정도만 하지만 아예 수입이 없는 것보다는 백배낫죠!!",
          "speaker_id": "2",
