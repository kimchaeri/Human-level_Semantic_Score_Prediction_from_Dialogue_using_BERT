      "utterance": [
        {
          "id": "MDRW2100036663.1.1",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210831 06:26"
        },
        {
          "id": "MDRW2100036663.1.2",
          "form": "저는 1차 맞았고 2차 기다리고있어요 ㅎㅎ",
          "original_form": "저는 1차 맞았고 2차 기다리고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100036663.1.3",
          "form": "와 빠르네요.. 전 추석전에 1차 맞아요.. 2차는 10월 말이나 돼야...;;;",
          "original_form": "와 빠르네요.. 전 추석전에 1차 맞아요.. 2차는 10월 말이나 돼야...;;;",
          "speaker_id": "1",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100036663.1.4",
          "form": "1차 뭐 맞았어요?",
          "original_form": "1차 뭐 맞았어요?",
          "speaker_id": "1",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100036663.1.5",
          "form": "저는 둘다 모더나요!",
          "original_form": "저는 둘다 모더나요!",
          "speaker_id": "2",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036663.1.6",
          "form": "근데ㅜ 원래 8/23이었는데ㅜㅜ",
          "original_form": "근데ㅜ 원래 8/23이었는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036663.1.7",
          "form": "오~ 듣지하니 모더나가 좋다던데 좋은거 맞았네요",
          "original_form": "오~ 듣지하니 모더나가 좋다던데 좋은거 맞았네요",
          "speaker_id": "1",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036663.1.8",
          "form": "전 모더나 아니면 화이지라는데 아무래도 화이자 맞을 듯해요",
          "original_form": "전 모더나 아니면 화이지라는데 아무래도 화이자 맞을 듯해요",
          "speaker_id": "1",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036663.1.9",
          "form": "저는 잔여백신 맞은거라서요",
          "original_form": "저는 잔여백신 맞은거라서요",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036663.1.10",
          "form": "잔여백신 어렵던데",
          "original_form": "잔여백신 어렵던데",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036663.1.11",
          "form": "며칠 시도하다가 포기했어요",
          "original_form": "며칠 시도하다가 포기했어요",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036663.1.12",
          "form": "또래중 좀 빨리 맞은편이긴해요",
          "original_form": "또래중 좀 빨리 맞은편이긴해요",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036663.1.13",
          "form": "저는 60대이신 엄마가 본인 맞고 저 신청해주셨는데",
          "original_form": "저는 60대이신 엄마가 본인 맞고 저 신청해주셨는데",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036663.1.14",
          "form": "바로 5시에 연락와서 ㅋㅋ",
          "original_form": "바로 5시에 연락와서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036663.1.15",
          "form": "아.. 잔여 어플로 잡은게 아니군요..",
          "original_form": "아.. 잔여 어플로 잡은게 아니군요..",
          "speaker_id": "1",
          "time": "20210831 10:02"
        },
        {
          "id": "MDRW2100036663.1.16",
          "form": "어플에서 막 잡아보려고 했는데 어찌나 다들 빠르게 채가던지",
          "original_form": "어플에서 막 잡아보려고 했는데 어찌나 다들 빠르게 채가던지",
          "speaker_id": "1",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100036663.1.17",
          "form": "여긴 전화로 받더라고요",
          "original_form": "여긴 전화로 받더라고요",
          "speaker_id": "2",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100036663.1.18",
          "form": "서울이 아니라그런가",
          "original_form": "서울이 아니라그런가",
          "speaker_id": "2",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100036663.1.19",
          "form": "그냥 해당병원에 전화로? ㅋㅋ",
          "original_form": "그냥 해당병원에 전화로? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:03"
        },
        {
          "id": "MDRW2100036663.1.20",
          "form": "아.. 전화하지 말래요..ㅋ 명단 안받는다고.. 어플로 하래요",
          "original_form": "아.. 전화하지 말래요..ㅋ 명단 안받는다고.. 어플로 하래요",
          "speaker_id": "1",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100036663.1.21",
          "form": "여기 시골이라서 가능했나봐요 ㄷㄷㄷ",
          "original_form": "여기 시골이라서 가능했나봐요 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100036663.1.22",
          "form": "그래서 암튼 아직 백신 못받았죠..ㅎㅎ",
          "original_form": "그래서 암튼 아직 백신 못받았죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100036663.1.23",
          "form": "백신도 몸 약하면 못맞던데.. 건강 관리를 위해 먹는 영양제 같은 건 있나요?",
          "original_form": "백신도 몸 약하면 못맞던데.. 건강 관리를 위해 먹는 영양제 같은 건 있나요?",
          "speaker_id": "1",
          "time": "20210831 11:05"
        }
      ]
    }
