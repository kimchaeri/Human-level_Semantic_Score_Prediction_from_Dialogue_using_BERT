      "utterance": [
        {
          "id": "MDRW2100041135.1.1",
          "form": "그러게요ㅋㅋㅋ 추억이 새록새록",
          "original_form": "그러게요ㅋㅋㅋ 추억이 새록새록",
          "speaker_id": "2",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100041135.1.2",
          "form": "ㅋㅋㅋ샤이니 노래중엔 어떤거 가장 좋아하셨나요?",
          "original_form": "ㅋㅋㅋ샤이니 노래중엔 어떤거 가장 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210905 17:48"
        },
        {
          "id": "MDRW2100041135.1.3",
          "form": "저 타이틀은 셜록 뷰 악개고 수록곡은 알람시계라든가 좋아합니다",
          "original_form": "저 타이틀은 셜록 뷰 악개고 수록곡은 알람시계라든가 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041135.1.4",
          "form": "너무 많아서 앨범 트랙 봐야 돼요",
          "original_form": "너무 많아서 앨범 트랙 봐야 돼요",
          "speaker_id": "2",
          "time": "20210905 17:49"
        },
        {
          "id": "MDRW2100041135.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 앨범트랙봐야되는거 진짜 공감이요 ㅋㅋㅋㅋ하나만 꼽기 힘든ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 앨범트랙봐야되는거 진짜 공감이요 ㅋㅋㅋㅋ하나만 꼽기 힘든ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100041135.1.6",
          "form": "저도 셜록 뷰 젤 좋아해요 ㅠㅠㅠㅠㅠ 셜록진짜...크레이지 명곡",
          "original_form": "저도 셜록 뷰 젤 좋아해요 ㅠㅠㅠㅠㅠ 셜록진짜...크레이지 명곡",
          "speaker_id": "1",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100041135.1.7",
          "form": "앨범별로 최소 한곡씩은 있거든요",
          "original_form": "앨범별로 최소 한곡씩은 있거든요",
          "speaker_id": "2",
          "time": "20210905 17:50"
        },
        {
          "id": "MDRW2100041135.1.8",
          "form": "아...진짜 참 각막과 고막의 소유자시네요",
          "original_form": "아...진짜 참 각막과 고막의 소유자시네요",
          "speaker_id": "2",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041135.1.9",
          "form": "쌍막",
          "original_form": "쌍막",
          "speaker_id": "2",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041135.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041135.1.11",
          "form": "맞습니다 앨범별로 명곡들이 줄줄줄있죠",
          "original_form": "맞습니다 앨범별로 명곡들이 줄줄줄있죠",
          "speaker_id": "1",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041135.1.12",
          "form": "name1님은 최애 가수 누구라고 하셨죠? 아니면 요즘 듣는 곡은 뭐예요?",
          "original_form": "&name1&님은 최애 가수 누구라고 하셨죠? 아니면 요즘 듣는 곡은 뭐예요?",
          "speaker_id": "2",
          "time": "20210905 17:55"
        },
        {
          "id": "MDRW2100041135.1.13",
          "form": "아 예전에는 엑소 좋아했는데.... 아버지와 열명 사태를 겪고....",
          "original_form": "아 예전에는 엑소 좋아했는데.... 아버지와 열명 사태를 겪고....",
          "speaker_id": "1",
          "time": "20210905 17:57"
        },
        {
          "id": "MDRW2100041135.1.14",
          "form": "지금은 그냥 가벼운 마음으로 두루두루 좋아해요ㅠㅋㅋㅋㅋ",
          "original_form": "지금은 그냥 가벼운 마음으로 두루두루 좋아해요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:57"
        },
        {
          "id": "MDRW2100041135.1.15",
          "form": "아버지와 열명사탴ㅋㅋㅋ 김종대디요?",
          "original_form": "아버지와 열명사탴ㅋㅋㅋ 김종대디요?",
          "speaker_id": "2",
          "time": "20210905 17:57"
        },
        {
          "id": "MDRW2100041135.1.16",
          "form": "행복인지 축복이 애비",
          "original_form": "행복인지 축복이 애비",
          "speaker_id": "2",
          "time": "20210905 17:57"
        },
        {
          "id": "MDRW2100041135.1.17",
          "form": "네 김종대디와... 박찬열명...",
          "original_form": "네 김종대디와... 박찬열명...",
          "speaker_id": "1",
          "time": "20210905 17:57"
        },
        {
          "id": "MDRW2100041135.1.18",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210905 17:57"
        },
        {
          "id": "MDRW2100041135.1.19",
          "form": "박찬열명ㅋㅋㅋ 이 별명 처음 들어요",
          "original_form": "박찬열명ㅋㅋㅋ 이 별명 처음 들어요",
          "speaker_id": "2",
          "time": "20210905 17:58"
        },
        {
          "id": "MDRW2100041135.1.20",
          "form": "엑소는 아니지만 루카스가 박찬열명 뒤를 이었다던데",
          "original_form": "엑소는 아니지만 루카스가 박찬열명 뒤를 이었다던데",
          "speaker_id": "2",
          "time": "20210905 17:58"
        },
        {
          "id": "MDRW2100041135.1.21",
          "form": "인재가 많네요",
          "original_form": "인재가 많네요",
          "speaker_id": "2",
          "time": "20210905 17:58"
        },
        {
          "id": "MDRW2100041135.1.22",
          "form": "ㅋㅋㅋㅋㅋ찐 열명인지는 모르겠는데 엄청 만났나봐요 .... 어휴 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ찐 열명인지는 모르겠는데 엄청 만났나봐요 .... 어휴 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:59"
        },
        {
          "id": "MDRW2100041135.1.23",
          "form": "남자들은 역시 .... 다 그런가봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "남자들은 역시 .... 다 그런가봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:59"
        },
        {
          "id": "MDRW2100041135.1.24",
          "form": "그냥 연옌한테는 기대도 하지말고 깊게 좋아하면 안될 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "그냥 연옌한테는 기대도 하지말고 깊게 좋아하면 안될 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:59"
        },
        {
          "id": "MDRW2100041135.1.25",
          "form": "맞아요, 덕질은 라이트하게",
          "original_form": "맞아요, 덕질은 라이트하게",
          "speaker_id": "2",
          "time": "20210905 18:00"
        },
        {
          "id": "MDRW2100041135.1.26",
          "form": "어덕행덕...",
          "original_form": "어덕행덕...",
          "speaker_id": "2",
          "time": "20210905 18:00"
        },
        {
          "id": "MDRW2100041135.1.27",
