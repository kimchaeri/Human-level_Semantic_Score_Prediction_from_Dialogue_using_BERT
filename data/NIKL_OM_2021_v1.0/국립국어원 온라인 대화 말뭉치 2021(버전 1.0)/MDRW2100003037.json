      "utterance": [
        {
          "id": "MDRW2100003037.1.1",
          "form": "요새 보는 드라마 있으세요?",
          "original_form": "요새 보는 드라마 있으세요?",
          "speaker_id": "1",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003037.1.2",
          "form": "요즘 술의생이랑 펜트하우스요",
          "original_form": "요즘 술의생이랑 펜트하우스요",
          "speaker_id": "2",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003037.1.3",
          "form": "슬의생이랑 펜하는\r\n너무 극과극 아닌가요?ㅋㅋ",
          "original_form": "슬의생이랑 펜하는\r\n너무 극과극 아닌가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003037.1.4",
          "form": "펜하는 안봐서 모르지만.ㅋㅋㅋ",
          "original_form": "펜하는 안봐서 모르지만.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003037.1.5",
          "form": "근데 잘 안 챙겨봐요. 지나고나서 생각나서",
          "original_form": "근데 잘 안 챙겨봐요. 지나고나서 생각나서",
          "speaker_id": "2",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003037.1.6",
          "form": "극과극이네요. 그러고보니.ㅋㅋㅋ",
          "original_form": "극과극이네요. 그러고보니.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003037.1.7",
          "form": "저는 슬의생은 보는데\r\n매주 감동과 웃음이 ",
          "original_form": "저는 슬의생은 보는데\r\n매주 감동과 웃음이 {emoji:♡}",
          "speaker_id": "1",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003037.1.8",
          "form": "그러고보니 저번주 이번주 다 못 봤네요",
          "original_form": "그러고보니 저번주 이번주 다 못 봤네요",
          "speaker_id": "2",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003037.1.9",
          "form": "오늘 펜하는 챙겨볼려나?ㅋㅋㅋ",
          "original_form": "오늘 펜하는 챙겨볼려나?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003037.1.10",
          "form": "저도 제 시간에는 못보고\r\n항상 담날 넷플로 봐요ㅠ",
          "original_form": "저도 제 시간에는 못보고\r\n항상 담날 넷플로 봐요ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003037.1.11",
          "form": "펜하는 몇 시에 해요?",
          "original_form": "펜하는 몇 시에 해요?",
          "speaker_id": "1",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003037.1.12",
          "form": "10시인가 하는거 같아요",
          "original_form": "10시인가 하는거 같아요",
          "speaker_id": "2",
          "time": "20210820 17:51"
        },
        {
          "id": "MDRW2100003037.1.13",
          "form": "슬의생 보다는 늦게 하네요^^",
          "original_form": "슬의생 보다는 늦게 하네요^^",
          "speaker_id": "1",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003037.1.14",
          "form": "정확한 시간도 잘 몰라요.ㅋㅋㅋ",
          "original_form": "정확한 시간도 잘 몰라요.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003037.1.15",
          "form": "그냥 tv키고 채널 돌리다\r\n나옴 보는 각이신데요?ㅋㅋ",
          "original_form": "그냥 tv키고 채널 돌리다\r\n나옴 보는 각이신데요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003037.1.16",
          "form": "맞아요. 티비도 잘 안 봐요. 밥 먹을 때나 틀어보고",
          "original_form": "맞아요. 티비도 잘 안 봐요. 밥 먹을 때나 틀어보고",
          "speaker_id": "2",
          "time": "20210820 17:55"
        },
        {
          "id": "MDRW2100003037.1.17",
          "form": "그럼 주로 집에서 뭐하세요?",
          "original_form": "그럼 주로 집에서 뭐하세요?",
          "speaker_id": "1",
          "time": "20210820 18:00"
        },
        {
          "id": "MDRW2100003037.1.18",
          "form": "음악듣는건 좋아하시나용?",
          "original_form": "음악듣는건 좋아하시나용?",
          "speaker_id": "1",
          "time": "20210820 18:00"
        },
        {
          "id": "MDRW2100003037.1.19",
          "form": "빈둥거려요ㅋㅋㅋ. 요즘은 음악도 잘 안 들어요",
          "original_form": "빈둥거려요ㅋㅋㅋ. 요즘은 음악도 잘 안 들어요",
          "speaker_id": "2",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003037.1.20",
          "form": "요즘 노래는 너무 빠르고 정신없어요.",
          "original_form": "요즘 노래는 너무 빠르고 정신없어요.",
          "speaker_id": "2",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003037.1.21",
          "form": "좋아하는 노래 있으세요?",
          "original_form": "좋아하는 노래 있으세요?",
          "speaker_id": "2",
          "time": "20210820 18:01"
        },
        {
          "id": "MDRW2100003037.1.22",
          "form": "저는 요즘 슬의생에 나왔던\r\n이무진 비와 당신",
          "original_form": "저는 요즘 슬의생에 나왔던\r\n이무진 비와 당신",
          "speaker_id": "1",
          "time": "20210820 18:03"
        },
        {
          "id": "MDRW2100003037.1.23",
          "form": "윤종신이랑 성시경이 부른 한번더 이별",
          "original_form": "윤종신이랑 성시경이 부른 한번더 이별",
          "speaker_id": "1",
          "time": "20210820 18:03"
        },
        {
          "id": "MDRW2100003037.1.24",
          "form": "많이 듣는거 같아요",
          "original_form": "많이 듣는거 같아요",
          "speaker_id": "1",
          "time": "20210820 18:04"
        },
        {
          "id": "MDRW2100003037.1.25",
          "form": "한번 더 이별은 제목 처음 듣네요. 찾아서 들어봐야겠어요",
          "original_form": "한번 더 이별은 제목 처음 듣네요. 찾아서 들어봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 18:04"
        },
        {
          "id": "MDRW2100003037.1.26",
          "form": "슬의생에 나오는 노래 다 좋죠",
          "original_form": "슬의생에 나오는 노래 다 좋죠",
          "speaker_id": "2",
          "time": "20210820 18:04"
        },
        {
          "id": "MDRW2100003037.1.27",
          "form": "전에 응답하라 시리즈 노래도 좋았고",
          "original_form": "전에 응답하라 시리즈 노래도 좋았고",
          "speaker_id": "2",
          "time": "20210820 18:05"
        },
        {
          "id": "MDRW2100003037.1.28",
          "form": "가을도 아닌데\r\n벌써부터 센치해지는\r\n노래들을 찾아듣네요ㅋ",
          "original_form": "가을도 아닌데\r\n벌써부터 센치해지는\r\n노래들을 찾아듣네요ㅋ",
          "speaker_id": "1",
          "time": "20210820 18:05"
        },
        {
          "id": "MDRW2100003037.1.29",
          "form": "오오 맞아요 맞아요",
          "original_form": "오오 맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210820 18:05"
        },
        {
          "id": "MDRW2100003037.1.30",
          "form": "둘다 ost들이 너무 좋아서",
          "original_form": "둘다 ost들이 너무 좋아서",
          "speaker_id": "1",
          "time": "20210820 18:05"
        },
        {
          "id": "MDRW2100003037.1.31",
          "form": "보고나면 다음화 할때까지\r\n그 노래들 듣고 또 듣고 했어요ㅋ",
          "original_form": "보고나면 다음화 할때까지\r\n그 노래들 듣고 또 듣고 했어요ㅋ",
