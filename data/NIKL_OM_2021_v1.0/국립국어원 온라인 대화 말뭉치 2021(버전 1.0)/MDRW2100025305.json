      "utterance": [
        {
          "id": "MDRW2100025305.1.1",
          "form": "건강을 위해 하고계신게 있나요??",
          "original_form": "건강을 위해 하고계신게 있나요??",
          "speaker_id": "1",
          "time": "20210901 08:44"
        },
        {
          "id": "MDRW2100025305.1.2",
          "form": "걷기 자주하려구",
          "original_form": "걷기 자주하려구",
          "speaker_id": "2",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100025305.1.3",
          "form": "노력중이에요 ㅎㅎ",
          "original_form": "노력중이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025305.1.4",
          "form": "저도 산책 자주해요!",
          "original_form": "저도 산책 자주해요!",
          "speaker_id": "1",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025305.1.5",
          "form": "비만 안오면 날씨 선선해져서 걷기 좋더라구요",
          "original_form": "비만 안오면 날씨 선선해져서 걷기 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100025305.1.6",
          "form": "맞아요 걷기 몸에 좋대요",
          "original_form": "맞아요 걷기 몸에 좋대요",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025305.1.7",
          "form": "매일 따로 시간내서 걸으세요??",
          "original_form": "매일 따로 시간내서 걸으세요??",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025305.1.8",
          "form": "음 보통 일할때 좀 걷고~ 집에 있을때도 좀 걸으려고 해요 ㅎㅎ",
          "original_form": "음 보통 일할때 좀 걷고~ 집에 있을때도 좀 걸으려고 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025305.1.9",
          "form": "오호 그러시구나~",
          "original_form": "오호 그러시구나~",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025305.1.10",
          "form": "근력 운동은 하시나요 ㅎㅎ",
          "original_form": "근력 운동은 하시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100025305.1.11",
          "form": "저는 가끔 홈트로 근력운동 하고 있어요",
          "original_form": "저는 가끔 홈트로 근력운동 하고 있어요",
          "speaker_id": "1",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025305.1.12",
          "form": "오 홈트 좋아보이던데",
          "original_form": "오 홈트 좋아보이던데",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025305.1.13",
          "form": "귀찮은게 단점이죠",
          "original_form": "귀찮은게 단점이죠",
          "speaker_id": "2",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025305.1.14",
          "form": "맞아요 꾸준히 하는게 여럽죠ㅜ",
          "original_form": "맞아요 꾸준히 하는게 여럽죠ㅜ",
          "speaker_id": "1",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025305.1.15",
          "form": "식단도 관리하시나여",
          "original_form": "식단도 관리하시나여",
          "speaker_id": "2",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025305.1.16",
          "form": "식단은 안해요ㅎㅎㅎ",
          "original_form": "식단은 안해요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025305.1.17",
          "form": "맛있는게 너무 많아서 식단은 힘들더라구요",
          "original_form": "맛있는게 너무 많아서 식단은 힘들더라구요",
          "speaker_id": "1",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025305.1.18",
          "form": "식단관리 하세요??",
          "original_form": "식단관리 하세요??",
          "speaker_id": "1",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025305.1.19",
          "form": "저도 뭐",
          "original_form": "저도 뭐",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025305.1.20",
          "form": "닭가 슴살",
          "original_form": "닭가 슴살",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025305.1.21",
          "form": "챙겨먹는정도",
          "original_form": "챙겨먹는정도",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025305.1.22",
          "form": "아 되게 불편하네",
          "original_form": "아 되게 불편하네",
          "speaker_id": "2",
          "time": "20210901 12:23"
        }
