      "utterance": [
        {
          "id": "MDRW2100029032.1.1",
          "form": "영화같은거 좋아하세요?",
          "original_form": "영화같은거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029032.1.2",
          "form": "저는 집에서 티비로 봐요!!",
          "original_form": "저는 집에서 티비로 봐요!!",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029032.1.3",
          "form": "영화관은 요즘 무서워서 못가구 ㅜ",
          "original_form": "영화관은 요즘 무서워서 못가구 ㅜ",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029032.1.4",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029032.1.5",
          "form": "최근에 뭐 보셨어요,",
          "original_form": "최근에 뭐 보셨어요,",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029032.1.6",
          "form": "최근은 못봣어요 ㅠㅠ",
          "original_form": "최근은 못봣어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029032.1.7",
          "form": "모가지슈 보셧나요?",
          "original_form": "모가지슈 보셧나요?",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029032.1.8",
          "form": "앗 아뇨ㅠㅠ 그거 보고싶었는데 시간이 안맞아소",
          "original_form": "앗 아뇨ㅠㅠ 그거 보고싶었는데 시간이 안맞아소",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029032.1.9",
          "form": "서",
          "original_form": "서",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029032.1.10",
          "form": "싱크홀 봤어요! 재밌더라구요",
          "original_form": "싱크홀 봤어요! 재밌더라구요",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029032.1.11",
          "form": "우아 누구 나오는영화인가요?",
          "original_form": "우아 누구 나오는영화인가요?",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029032.1.12",
          "form": "이광수 씨랑 김성균 씨랑 차승원 씨요!",
          "original_form": "이광수 씨랑 김성균 씨랑 차승원 씨요!",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029032.1.13",
          "form": "너무 재밌었어요 티비 플랫폼에 나오면 꼭 보세요",
          "original_form": "너무 재밌었어요 티비 플랫폼에 나오면 꼭 보세요",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029032.1.14",
          "form": "오오 차승원!!!!",
          "original_form": "오오 차승원!!!!",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029032.1.15",
          "form": "다들 연기 잘하시져?",
          "original_form": "다들 연기 잘하시져?",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029032.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029032.1.17",
          "form": "이광수씨가 너어무 재밌었어요",
          "original_form": "이광수씨가 너어무 재밌었어요",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029032.1.18",
          "form": "저도 티비로 나오면 봐야겟어요 !!!",
          "original_form": "저도 티비로 나오면 봐야겟어요 !!!",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029032.1.19",
          "form": "영화관 가서 보셧나요?",
          "original_form": "영화관 가서 보셧나요?",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029032.1.20",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029032.1.21",
          "form": "근데 코로나때문에 팝콘을 못먹었어요ㅠ",
          "original_form": "근데 코로나때문에 팝콘을 못먹었어요ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029032.1.22",
          "form": "모가디슈 재밌나요??",
          "original_form": "모가디슈 재밌나요??",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029032.1.23",
          "form": "저 모가디슈도 못봣어요 ㅠㅠ 저빼고 다봣드라구요",
          "original_form": "저 모가디슈도 못봣어요 ㅠㅠ 저빼고 다봣드라구요",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029032.1.24",
          "form": "볼게 엄청 많이 생겼어요 ㅋㅋ",
          "original_form": "볼게 엄청 많이 생겼어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029032.1.25",
          "form": "그러게요ㅋㅋ",
          "original_form": "그러게요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029032.1.26",
          "form": "블랙위도우 보셨나요?",
          "original_form": "블랙위도우 보셨나요?",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029032.1.27",
          "form": "저 마블시리즈 처음 본건데 암것도 몰라두 재밌더라구요",
          "original_form": "저 마블시리즈 처음 본건데 암것도 몰라두 재밌더라구요",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029032.1.28",
          "form": "우아 블랙위도우 ㅜㅜㅜ 이것도 못봣네요 ㅋㅋ",
          "original_form": "우아 블랙위도우 ㅜㅜㅜ 이것도 못봣네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029032.1.29",
          "form": "세가지 꼭 볼거예요 ㅠㅠ",
          "original_form": "세가지 꼭 볼거예요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029032.1.30",
          "form": "저는 마블 다 보긴 했는데 기억이 가물가물 하네요",
          "original_form": "저는 마블 다 보긴 했는데 기억이 가물가물 하네요",
          "speaker_id": "1",
          "time": "20210902 23:57"
        }
