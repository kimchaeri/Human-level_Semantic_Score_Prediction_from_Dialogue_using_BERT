      "utterance": [
        {
          "id": "MDRW2100017495.1.1",
          "form": "엄청나게",
          "original_form": "엄청나게",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017495.1.2",
          "form": "호화로운 저녁이네",
          "original_form": "호화로운 저녁이네",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017495.1.3",
          "form": "알코올을 곁들여야 할거같은 느낌인데?",
          "original_form": "알코올을 곁들여야 할거같은 느낌인데?",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017495.1.4",
          "form": "알콜이 있으면 정말 좋겠지만",
          "original_form": "알콜이 있으면 정말 좋겠지만",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017495.1.5",
          "form": "집에서는 술 안마시기로 했어",
          "original_form": "집에서는 술 안마시기로 했어",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017495.1.6",
          "form": "웃긴건 내가 술을 안마시기로 결심하자마자 엄마가",
          "original_form": "웃긴건 내가 술을 안마시기로 결심하자마자 엄마가",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017495.1.7",
          "form": "맨날 나한테 오늘 맥주 드실?막걸리 드실?",
          "original_form": "맨날 나한테 오늘 맥주 드실?막걸리 드실?",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.8",
          "form": "이러면서 물어봐",
          "original_form": "이러면서 물어봐",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.10",
          "form": "난 꿋꿋하게 술은   마시지 않겠다고 하고있지",
          "original_form": "난 꿋꿋하게 술은   마시지 않겠다고 하고있지",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.11",
          "form": "유혹에 안넘어간지",
          "original_form": "유혹에 안넘어간지",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.12",
          "form": "얼마나됐어",
          "original_form": "얼마나됐어",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.13",
          "form": "일주일정도",
          "original_form": "일주일정도",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.14",
          "form": "오래참았다 그치",
          "original_form": "오래참았다 그치",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.15",
          "form": "아니 일주일보다는 오래된거같은데?!",
          "original_form": "아니 일주일보다는 오래된거같은데?!",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.16",
          "form": "한달이라고 하자",
          "original_form": "한달이라고 하자",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.17",
          "form": "(그런건가..?)",
          "original_form": "(그런건가..?)",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.18",
          "form": "나는 집에서 술을",
          "original_form": "나는 집에서 술을",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.19",
          "form": "거의 안먹어",
          "original_form": "거의 안먹어",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017495.1.20",
          "form": "그럴거같아 넌",
          "original_form": "그럴거같아 넌",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017495.1.21",
          "form": "원래도 술 잘 안마시잖아",
          "original_form": "원래도 술 잘 안마시잖아",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017495.1.22",
          "form": "술자리에서 나에게 메인은",
          "original_form": "술자리에서 나에게 메인은",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017495.1.23",
          "form": "술이아니라",
          "original_form": "술이아니라",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017495.1.24",
          "form": "사람이다!",
          "original_form": "사람이다!",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017495.1.25",
          "form": "그치 분위기를 마시는거지",
          "original_form": "그치 분위기를 마시는거지",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017495.1.26",
          "form": "집에서는 마셔도",
          "original_form": "집에서는 마셔도",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017495.1.27",
          "form": "맛있는 와인이나 맥주같은거만",
          "original_form": "맛있는 와인이나 맥주같은거만",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.28",
          "form": "아빠랑 같이 마셔",
          "original_form": "아빠랑 같이 마셔",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.29",
          "form": "그건 술이라기보다",
          "original_form": "그건 술이라기보다",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.30",
          "form": "소주는 집에서 금지야",
          "original_form": "소주는 집에서 금지야",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.31",
          "form": "알코올이 함유된 음료수같은 느낌이야",
          "original_form": "알코올이 함유된 음료수같은 느낌이야",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.32",
          "form": "아빠가 항상 소주까고",
          "original_form": "아빠가 항상 소주까고",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.33",
          "form": "엄마한테 등짝맞아",
          "original_form": "엄마한테 등짝맞아",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.34",
          "form": "거의 맥주만 드셔 이제..",
          "original_form": "거의 맥주만 드셔 이제..",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017495.1.36",
          "form": "아 금지된게 어머니의 영향",
          "original_form": "아 금지된게 어머니의 영향",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017495.1.37",
          "form": "야 근데 그나저나 문제가 조금 있어",
          "original_form": "야 근데 그나저나 문제가 조금 있어",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017495.1.38",
          "form": "베이컨이 좀 많이 얼어있어",
          "original_form": "베이컨이 좀 많이 얼어있어",
          "speaker_id": "1",
          "time": "20210826 19:50"
        }
      ]
    }
