      "utterance": [
        {
          "id": "MDRW2100031913.1.1",
          "form": "애기가 생선파라 거의 날마다 생선 굽는거ㅠ같아요 ㅎㅎ",
          "original_form": "애기가 생선파라 거의 날마다 생선 굽는거ㅠ같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100031913.1.2",
          "form": "우와우와ㅎ",
          "original_form": "우와우와ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:30"
        },
        {
          "id": "MDRW2100031913.1.3",
          "form": "생선을 좋아하는군요ㅎ 저희아이도그런데ㅎ",
          "original_form": "생선을 좋아하는군요ㅎ 저희아이도그런데ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:30"
        },
        {
          "id": "MDRW2100031913.1.4",
          "form": "근데 생선보다 고기를 더좋아해요~^^",
          "original_form": "근데 생선보다 고기를 더좋아해요~^^",
          "speaker_id": "2",
          "time": "20210905 08:30"
        },
        {
          "id": "MDRW2100031913.1.5",
          "form": "반찬은 보통 뭐에 먹어요?",
          "original_form": "반찬은 보통 뭐에 먹어요?",
          "speaker_id": "2",
          "time": "20210905 08:30"
        },
        {
          "id": "MDRW2100031913.1.6",
          "form": "생선 기본에",
          "original_form": "생선 기본에",
          "speaker_id": "1",
          "time": "20210905 08:31"
        },
        {
          "id": "MDRW2100031913.1.7",
          "form": "먹는걸 안좋아해서 먹는게 별루 없어요",
          "original_form": "먹는걸 안좋아해서 먹는게 별루 없어요",
          "speaker_id": "1",
          "time": "20210905 08:31"
        },
        {
          "id": "MDRW2100031913.1.8",
          "form": "된장국 고기국 달걀국 만먹어오",
          "original_form": "된장국 고기국 달걀국 만먹어오",
          "speaker_id": "1",
          "time": "20210905 08:31"
        },
        {
          "id": "MDRW2100031913.1.9",
          "form": "에고에고ㅠ",
          "original_form": "에고에고ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:31"
        },
        {
          "id": "MDRW2100031913.1.10",
          "form": "다양하게 먹어야하는데",
          "original_form": "다양하게 먹어야하는데",
          "speaker_id": "2",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031913.1.11",
          "form": "맛없으며누안먹는 ㅋ",
          "original_form": "맛없으며누안먹는 ㅋ",
          "speaker_id": "1",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031913.1.12",
          "form": "저희 애도그래요ㅎ",
          "original_form": "저희 애도그래요ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031913.1.13",
          "form": "고기도 아이가 안먹으면 안싱싱한거에요",
          "original_form": "고기도 아이가 안먹으면 안싱싱한거에요",
          "speaker_id": "1",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031913.1.14",
          "form": "어제 엄마가 생선구이해줫는데ㅎ",
          "original_form": "어제 엄마가 생선구이해줫는데ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031913.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 08:32"
        },
        {
          "id": "MDRW2100031913.1.16",
          "form": "아 민감하군요",
          "original_form": "아 민감하군요",
          "speaker_id": "2",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031913.1.17",
          "form": "안먹더라구요ㅎ 과자를 많이먹어서ㅎㅎ",
          "original_form": "안먹더라구요ㅎ 과자를 많이먹어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031913.1.18",
          "form": "민망해가지고",
          "original_form": "민망해가지고",
          "speaker_id": "2",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031913.1.19",
          "form": "과자도 잘안먹는 ㅋ",
          "original_form": "과자도 잘안먹는 ㅋ",
          "speaker_id": "1",
          "time": "20210905 08:33"
        },
        {
          "id": "MDRW2100031913.1.20",
          "form": "아.. 그럼 또 뭐먹어요?",
          "original_form": "아.. 그럼 또 뭐먹어요?",
          "speaker_id": "2",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031913.1.21",
          "form": "햄",
          "original_form": "햄",
          "speaker_id": "1",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031913.1.22",
          "form": "달걀",
          "original_form": "달걀",
          "speaker_id": "1",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031913.1.23",
          "form": "김",
          "original_form": "김",
          "speaker_id": "1",
          "time": "20210905 08:34"
        },
        {
          "id": "MDRW2100031913.1.24",
          "form": "음음ㅎ 소시지는요?ㅎ",
          "original_form": "음음ㅎ 소시지는요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031913.1.25",
          "form": "어묵은 바로 튀겨주는데 있죠",
          "original_form": "어묵은 바로 튀겨주는데 있죠",
          "speaker_id": "1",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031913.1.26",
          "form": "그것만 먹구여 \n소세지도 먹죠 ㅎㅎ",
          "original_form": "그것만 먹구여 \n소세지도 먹죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031913.1.27",
          "form": "우리애도 다른건안먹어도 소시지 좋아하던데ㅎㅎ",
          "original_form": "우리애도 다른건안먹어도 소시지 좋아하던데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031913.1.28",
          "form": "아니면 케챱에 찍어줘보세요 ㅎ",
          "original_form": "아니면 케챱에 찍어줘보세요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031913.1.29",
          "form": "한마디로 야채 노놉 ㅋ",
          "original_form": "한마디로 야채 노놉 ㅋ",
          "speaker_id": "1",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031913.1.30",
          "form": "우와 똑같네요ㅎ",
          "original_form": "우와 똑같네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100031913.1.31",
          "form": "저희 애도 야채를 싫어해요",
          "original_form": "저희 애도 야채를 싫어해요",
          "speaker_id": "2",
          "time": "20210905 08:36"
        },
