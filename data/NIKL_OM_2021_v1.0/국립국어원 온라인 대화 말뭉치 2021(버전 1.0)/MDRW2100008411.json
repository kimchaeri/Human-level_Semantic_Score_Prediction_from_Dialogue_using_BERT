      "utterance": [
        {
          "id": "MDRW2100008411.1.1",
          "form": "여기는 딱히 취미랑 관련된 주제가 없네요",
          "original_form": "여기는 딱히 취미랑 관련된 주제가 없네요",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008411.1.2",
          "form": "전시회 자주 가시나요?",
          "original_form": "전시회 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008411.1.3",
          "form": "가끔씩 가요!",
          "original_form": "가끔씩 가요!",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008411.1.4",
          "form": "마음같아서는 자주가고싶은데",
          "original_form": "마음같아서는 자주가고싶은데",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008411.1.5",
          "form": "은근히 비싸더라고요",
          "original_form": "은근히 비싸더라고요",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008411.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008411.1.7",
          "form": "그림 좋아하시나요?",
          "original_form": "그림 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100008411.1.8",
          "form": "저는 그림 작품 위주로 전시회 갔었어요",
          "original_form": "저는 그림 작품 위주로 전시회 갔었어요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008411.1.9",
          "form": "그림 좋아해요!",
          "original_form": "그림 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008411.1.10",
          "form": "취미로 하고있거든요",
          "original_form": "취미로 하고있거든요",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008411.1.11",
          "form": "최근에 좋았던 곳 있으세요?",
          "original_form": "최근에 좋았던 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008411.1.12",
          "form": "최근에는 저도 잘 안 다녀서",
          "original_form": "최근에는 저도 잘 안 다녀서",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008411.1.13",
          "form": "근데 최근에 앨리스 달튼 브라운전 추천받았어요",
          "original_form": "근데 최근에 앨리스 달튼 브라운전 추천받았어요",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008411.1.14",
          "form": "와 저도 그거 가고싶어요",
          "original_form": "와 저도 그거 가고싶어요",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008411.1.15",
          "form": "소개글 읽어봤는대",
          "original_form": "소개글 읽어봤는대",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008411.1.16",
          "form": "엄청 예쁘더라고요",
          "original_form": "엄청 예쁘더라고요",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008411.1.17",
          "form": "그러니까요 시간 맞으면 한번 다녀오려고요",
          "original_form": "그러니까요 시간 맞으면 한번 다녀오려고요",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008411.1.18",
          "form": "그림 그리는 거 좋아하시는 군요!",
          "original_form": "그림 그리는 거 좋아하시는 군요!",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008411.1.19",
          "form": "네네 어릴적부터 좋아했어요 ㅎㅎ",
          "original_form": "네네 어릴적부터 좋아했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008411.1.20",
          "form": "저도 요즘에는 자주 못 그리지만 취미로 디지털 그림 그렸어요",
          "original_form": "저도 요즘에는 자주 못 그리지만 취미로 디지털 그림 그렸어요",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008411.1.21",
          "form": "오 테블릿 그런걸로요?",
          "original_form": "오 테블릿 그런걸로요?",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008411.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008411.1.23",
          "form": "중학교 때 사달라고 졸라서 그 뒤로 종종 그렀어요",
          "original_form": "중학교 때 사달라고 졸라서 그 뒤로 종종 그렀어요",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008411.1.24",
          "form": "퐁퐁님은 손 그림이랑 디지털 중에 어느 쪽을 더 좋아하시나요?",
          "original_form": "퐁퐁님은 손 그림이랑 디지털 중에 어느 쪽을 더 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008411.1.25",
          "form": "저는 손그림을 잘하고 싶은데",
          "original_form": "저는 손그림을 잘하고 싶은데",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008411.1.26",
          "form": "디지털쪽이 편해서 디지털 그림을 많이그려요",
          "original_form": "디지털쪽이 편해서 디지털 그림을 많이그려요",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008411.1.27",
          "form": "프로그램 뭐쓰세요?",
          "original_form": "프로그램 뭐쓰세요?",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008411.1.28",
          "form": "예전에는 사이툴 썼는데 올해 초부터는 클튜 써요!",
          "original_form": "예전에는 사이툴 썼는데 올해 초부터는 클튜 써요!",
          "speaker_id": "1",
          "time": "20210822 15:59"
        }
      ]
    }
