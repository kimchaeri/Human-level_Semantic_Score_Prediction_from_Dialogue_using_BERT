      "utterance": [
        {
          "id": "MDRW2100037201.1.1",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037201.1.2",
          "form": "네 하루종일 오네요",
          "original_form": "네 하루종일 오네요",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037201.1.3",
          "form": "그쪽도 비 오나요?",
          "original_form": "그쪽도 비 오나요?",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037201.1.4",
          "form": "여기는 아까 소나기 내리고 안오고있어요!",
          "original_form": "여기는 아까 소나기 내리고 안오고있어요!",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037201.1.5",
          "form": "새벽에는 비 많이 온다고 안내문자왔네요",
          "original_form": "새벽에는 비 많이 온다고 안내문자왔네요",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037201.1.6",
          "form": "창 문 잘 닫고 주무세요!",
          "original_form": "창 문 잘 닫고 주무세요!",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100037201.1.7",
          "form": "네! name1님도요!",
          "original_form": "네! &name1&님도요!",
          "speaker_id": "2",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037201.1.8",
          "form": "선풍기 틀고 자야하나 싶어요",
          "original_form": "선풍기 틀고 자야하나 싶어요",
          "speaker_id": "2",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037201.1.9",
          "form": "저는 기온이 많이 떨어져서 안 틀고 자려고요",
          "original_form": "저는 기온이 많이 떨어져서 안 틀고 자려고요",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037201.1.10",
          "form": "더우시면 약하게 틀어야겠네요",
          "original_form": "더우시면 약하게 틀어야겠네요",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037201.1.11",
          "form": "네ㅠ 더위를 많이타서그런지 조금만 더워도 못자겠더라구요ㅠ",
          "original_form": "네ㅠ 더위를 많이타서그런지 조금만 더워도 못자겠더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037201.1.12",
          "form": "한동안 선선해져서 좋았는데 비가온다니 슬퍼요",
          "original_form": "한동안 선선해져서 좋았는데 비가온다니 슬퍼요",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100037201.1.13",
          "form": "이번주까지는 올 거 같아요",
          "original_form": "이번주까지는 올 거 같아요",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037201.1.14",
          "form": "가을이 비오다가 끝나면 너무 아쉬울 거 같아요...",
          "original_form": "가을이 비오다가 끝나면 너무 아쉬울 거 같아요...",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037201.1.15",
          "form": "저두요.. 이렇게 보내기엔 가을이 넘 소중해요",
          "original_form": "저두요.. 이렇게 보내기엔 가을이 넘 소중해요",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037201.1.16",
          "form": "가을이 워낙 짧아서... 빨리 비가 그쳤으면 좋겠어요",
          "original_form": "가을이 워낙 짧아서... 빨리 비가 그쳤으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037201.1.17",
          "form": "가을 좋아하시나요?",
          "original_form": "가을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037201.1.18",
          "form": "넹! 봄 가을 좋아해요~~",
          "original_form": "넹! 봄 가을 좋아해요~~",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037201.1.19",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037201.1.20",
          "form": "젇도 가을 제일 좋아해요~ 시원해서 너무 좋아요",
          "original_form": "젇도 가을 제일 좋아해요~ 시원해서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100037201.1.21",
          "form": "맞아용 비만안오면 딱인데 말이죠",
