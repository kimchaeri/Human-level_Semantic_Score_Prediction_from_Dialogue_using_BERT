      "utterance": [
        {
          "id": "MDRW2100018127.1.1",
          "form": "동호회나 봉사 하시나요?",
          "original_form": "동호회나 봉사 하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018127.1.2",
          "form": "딱히 없어요 ㅠㅠ 하시는 거 있으세요?",
          "original_form": "딱히 없어요 ㅠㅠ 하시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018127.1.3",
          "form": "저도.. 없어요ㅠ",
          "original_form": "저도.. 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018127.1.4",
          "form": "어떤 모임 가지고 싶으세요?",
          "original_form": "어떤 모임 가지고 싶으세요?",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018127.1.5",
          "form": "글쎄요 음.. 맛집가는 모임..? ㅋㅋㅋㅋㅋ",
          "original_form": "글쎄요 음.. 맛집가는 모임..? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018127.1.6",
          "form": "ㅋㅋ거기서 만날수도 있겠어요",
          "original_form": "ㅋㅋ거기서 만날수도 있겠어요",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018127.1.7",
          "form": "ㅋㅋㅋㅋ 하시는 모임이나 봉사있으세요?",
          "original_form": "ㅋㅋㅋㅋ 하시는 모임이나 봉사있으세요?",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018127.1.8",
          "form": "아뇨ㅋㅋ",
          "original_form": "아뇨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018127.1.9",
          "form": "공예 모임에는 관심 있어요!",
          "original_form": "공예 모임에는 관심 있어요!",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018127.1.10",
          "form": "맛집이랑",
          "original_form": "맛집이랑",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018127.1.11",
          "form": "공예 어떤거 하세요?",
          "original_form": "공예 어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018127.1.12",
          "form": "요즘에는 비즈에 빠졌어요",
          "original_form": "요즘에는 비즈에 빠졌어요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018127.1.13",
          "form": "아 어렸을 때 많이 했었어요 저는",
          "original_form": "아 어렸을 때 많이 했었어요 저는",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018127.1.14",
          "form": "오 공예 좋아하세요?",
          "original_form": "오 공예 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018127.1.15",
          "form": "요새는 안하구 어렸을때 스킬 아세요? 그런거 했었어요 ㅋㅋㅋㅋ",
          "original_form": "요새는 안하구 어렸을때 스킬 아세요? 그런거 했었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018127.1.16",
          "form": "아ㅋㅋ 저도 자주 했어요",
          "original_form": "아ㅋㅋ 저도 자주 했어요",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018127.1.17",
          "form": "유행이였죠",
          "original_form": "유행이였죠",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
