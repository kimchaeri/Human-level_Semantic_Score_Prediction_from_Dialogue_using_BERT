      "utterance": [
        {
          "id": "MDRW2100045314.1.1",
          "form": "좋은팁이죠 전 그렇게 마법사 했어요",
          "original_form": "좋은팁이죠 전 그렇게 마법사 했어요",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045314.1.2",
          "form": "인신좋아요",
          "original_form": "인신좋아요",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045314.1.3",
          "form": "최고는 은신 쌍수지ㅏㄴ",
          "original_form": "최고는 은신 쌍수지ㅏㄴ",
          "speaker_id": "1",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045314.1.4",
          "form": "아 은신은 너무 비겁하잖아요",
          "original_form": "아 은신은 너무 비겁하잖아요",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045314.1.5",
          "form": "그래서 좀 별로에요",
          "original_form": "그래서 좀 별로에요",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045314.1.6",
          "form": "제가 도망치더라도 은신? 절대 못해",
          "original_form": "제가 도망치더라도 은신? 절대 못해",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045314.1.7",
          "form": "도망치는거 가오상해요",
          "original_form": "도망치는거 가오상해요",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045314.1.8",
          "form": "아니 은신도 가오 상해요",
          "original_form": "아니 은신도 가오 상해요",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045314.1.9",
          "form": "이상한 사람이네",
          "original_form": "이상한 사람이네",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045314.1.10",
          "form": "은신은 간지에요",
          "original_form": "은신은 간지에요",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045314.1.11",
          "form": "닌자간지",
          "original_form": "닌자간지",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045314.1.12",
          "form": "은신간지",
          "original_form": "은신간지",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045314.1.13",
          "form": "아 뭔 닌자간지예요",
          "original_form": "아 뭔 닌자간지예요",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045314.1.14",
          "form": "탈주하게 생겼구만",
          "original_form": "탈주하게 생겼구만",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045314.1.15",
          "form": "다른 방벚은 없나요?",
          "original_form": "다른 방벚은 없나요?",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045314.1.16",
          "form": "마법사는 힘들죠..",
          "original_form": "마법사는 힘들죠..",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045314.1.17",
          "form": "소환쪽이 후반에 갓이라고 들었어요",
          "original_form": "소환쪽이 후반에 갓이라고 들었어요",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045314.1.18",
          "form": "하지만.. 은신말고. 방법이 있을 거 아니예요",
          "original_form": "하지만.. 은신말고. 방법이 있을 거 아니예요",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045314.1.19",
          "form": "소화...ㄴ...",
          "original_form": "소화...ㄴ...",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045314.1.20",
          "form": "그리고 지팡이 쓰세요 마나 떨어지면",
          "original_form": "그리고 지팡이 쓰세요 마나 떨어지면",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045314.1.21",
          "form": "아 다 팔았는데",
          "original_form": "아 다 팔았는데",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045314.1.22",
          "form": "그런 방법이 있군요",
          "original_form": "그런 방법이 있군요",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045314.1.23",
          "form": "내 마나안씀 지팡이는",
          "original_form": "내 마나안씀 지팡이는",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045314.1.24",
          "form": "그건 몰랐네요.. 에잉.. 좀 아껴줄걸 그랬네",
          "original_form": "그건 몰랐네요.. 에잉.. 좀 아껴줄걸 그랬네",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045314.1.25",
          "form": "근대 스킬도 오르냐면 모르겠어요..",
          "original_form": "근대 스킬도 오르냐면 모르겠어요..",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.26",
          "form": "소환무기 써봐요",
          "original_form": "소환무기 써봐요",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.27",
          "form": "나중에는 드래곤 비늘로",
          "original_form": "나중에는 드래곤 비늘로",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.28",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.29",
          "form": "방어구도 만들고 싶은데",
          "original_form": "방어구도 만들고 싶은데",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.30",
          "form": "근대 마법사면서 갑옷재료를..",
          "original_form": "근대 마법사면서 갑옷재료를..",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.31",
          "form": "갑옷밖에 못 만들어요?",
          "original_form": "갑옷밖에 못 만들어요?",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.32",
          "form": "갑옷만드는거에요 로브 아니고",
          "original_form": "갑옷만드는거에요 로브 아니고",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.33",
          "form": "왜죠..?",
          "original_form": "왜죠..?",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.34",
          "form": "예날겜이라?",
          "original_form": "예날겜이라?",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.35",
          "form": "로브는...뭐가 강하죠...?",
          "original_form": "로브는...뭐가 강하죠...?",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045314.1.36",
          "form": "ㅏ검사하세요 간지임",
          "original_form": "ㅏ검사하세요 간지임",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045314.1.37",
          "form": "마검사",
          "original_form": "마검사",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045314.1.38",
          "form": "아니 정통 마법사라니까요",
          "original_form": "아니 정통 마법사라니까요",
          "speaker_id": "2",
          "time": "20210908 01:57"
        }
      ]
    }
  ]
