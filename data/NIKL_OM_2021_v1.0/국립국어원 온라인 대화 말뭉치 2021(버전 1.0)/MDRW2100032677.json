      "utterance": [
        {
          "id": "MDRW2100032677.1.1",
          "form": "네 종종먹어요",
          "original_form": "네 종종먹어요",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032677.1.2",
          "form": "피자 치킨중에 뭐가더좋으세요?",
          "original_form": "피자 치킨중에 뭐가더좋으세요?",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032677.1.3",
          "form": "저는 피자",
          "original_form": "저는 피자",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032677.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032677.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032677.1.6",
          "form": "저는좀 우열을 가리기힘들어요ㅋㅋ",
          "original_form": "저는좀 우열을 가리기힘들어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032677.1.7",
          "form": "둘다좋아하는군요 ㅎㅎ",
          "original_form": "둘다좋아하는군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032677.1.8",
          "form": "넵 돼지라서ㅋㅋ",
          "original_form": "넵 돼지라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032677.1.9",
          "form": "어디피자좋아하세요?",
          "original_form": "어디피자좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032677.1.10",
          "form": "딱히 가리지않아요 다 맛있더라고요",
          "original_form": "딱히 가리지않아요 다 맛있더라고요",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032677.1.11",
          "form": "그쵸그쵸ㅠ",
          "original_form": "그쵸그쵸ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032677.1.12",
          "form": "하와이안피자도 좋아하세요?!",
          "original_form": "하와이안피자도 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032677.1.13",
          "form": "아 파인애플올라간?",
          "original_form": "아 파인애플올라간?",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032677.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032677.1.15",
          "form": "특이해서 그냥 먹을만 하더라고요",
          "original_form": "특이해서 그냥 먹을만 하더라고요",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032677.1.16",
          "form": "맞아요 특이하게 달콤하더라고요",
          "original_form": "맞아요 특이하게 달콤하더라고요",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100032677.1.17",
          "form": "맛있어요!",
          "original_form": "맛있어요!",
          "speaker_id": "1",
          "time": "20210905 22:12"
        }
