      "utterance": [
        {
          "id": "MDRW2100046760.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 12:30"
        },
        {
          "id": "MDRW2100046760.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20211104 12:31"
        },
        {
          "id": "MDRW2100046760.1.3",
          "form": "네ㅎ",
          "original_form": "네ㅎ",
          "speaker_id": "1",
          "time": "20211104 12:31"
        },
        {
          "id": "MDRW2100046760.1.4",
          "form": "밤인데 왜 아직 안주무세요!",
          "original_form": "밤인데 왜 아직 안주무세요!",
          "speaker_id": "2",
          "time": "20211104 12:32"
        },
        {
          "id": "MDRW2100046760.1.5",
          "form": "롤하다 져서 잠이 안옵니다",
          "original_form": "롤하다 져서 잠이 안옵니다",
          "speaker_id": "1",
          "time": "20211104 12:32"
        },
        {
          "id": "MDRW2100046760.1.6",
          "form": "왜안자세요?",
          "original_form": "왜안자세요?",
          "speaker_id": "1",
          "time": "20211104 12:33"
        },
        {
          "id": "MDRW2100046760.1.7",
          "form": "앗 잠들었네요 ㅋㅋ",
          "original_form": "앗 잠들었네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 07:02"
        },
        {
          "id": "MDRW2100046760.1.8",
          "form": "저는 게임을 요즘 잘 안해서 무슨 게임이 있는지도 잘 몰라요 ㅠ",
          "original_form": "저는 게임을 요즘 잘 안해서 무슨 게임이 있는지도 잘 몰라요 ㅠ",
          "speaker_id": "2",
          "time": "20211104 07:03"
        },
        {
          "id": "MDRW2100046760.1.9",
          "form": "저도 롤만 해요ㅋㅋ",
          "original_form": "저도 롤만 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 07:07"
        },
        {
          "id": "MDRW2100046760.1.10",
          "form": "ㅋㅋㅋ옛날에는 핸드폰으로 게임 자주 했는데 요즘은 시간이 안나네요 ㅠ",
          "original_form": "ㅋㅋㅋ옛날에는 핸드폰으로 게임 자주 했는데 요즘은 시간이 안나네요 ㅠ",
          "speaker_id": "2",
          "time": "20211104 07:57"
        },
        {
          "id": "MDRW2100046760.1.11",
          "form": "롤 재미있나요?",
          "original_form": "롤 재미있나요?",
          "speaker_id": "2",
          "time": "20211104 07:57"
        },
        {
          "id": "MDRW2100046760.1.12",
          "form": "딱히요ㅋㅋ",
          "original_form": "딱히요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 18:56"
        },
        {
          "id": "MDRW2100046760.1.13",
          "form": "랭크는 어느정도이신가요",
          "original_form": "랭크는 어느정도이신가요",
          "speaker_id": "2",
          "time": "20211104 20:01"
        },
        {
          "id": "MDRW2100046760.1.14",
          "form": "다이아요ㅋㅋ",
          "original_form": "다이아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 20:01"
        },
        {
          "id": "MDRW2100046760.1.15",
          "form": "얼 높으신데요",
          "original_form": "얼 높으신데요",
          "speaker_id": "2",
          "time": "20211104 20:02"
        },
        {
          "id": "MDRW2100046760.1.16",
          "form": "아뇨 그닥...",
          "original_form": "아뇨 그닥...",
          "speaker_id": "1",
          "time": "20211104 20:02"
        },
        {
          "id": "MDRW2100046760.1.17",
          "form": "곧 시즌마감이라",
          "original_form": "곧 시즌마감이라",
          "speaker_id": "1",
          "time": "20211104 20:02"
        },
        {
          "id": "MDRW2100046760.1.18",
          "form": "ㅋㅋㅋ그래도 제 기준 높으십니다..",
          "original_form": "ㅋㅋㅋ그래도 제 기준 높으십니다..",
          "speaker_id": "2",
          "time": "20211104 20:09"
        }
      ]
