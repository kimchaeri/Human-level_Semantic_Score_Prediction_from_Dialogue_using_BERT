      "utterance": [
        {
          "id": "MDRW2100015704.1.1",
          "form": "이제 슬슬 선선하군",
          "original_form": "이제 슬슬 선선하군",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015704.1.2",
          "form": "운동타이밍이 온다",
          "original_form": "운동타이밍이 온다",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015704.1.3",
          "form": "런데이 뛰려고 전철타는거 오바인가?",
          "original_form": "런데이 뛰려고 전철타는거 오바인가?",
          "speaker_id": "1",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015704.1.4",
          "form": "집근처 뛸곳이 없어",
          "original_form": "집근처 뛸곳이 없어",
          "speaker_id": "1",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015704.1.5",
          "form": "아 그거 오바",
          "original_form": "아 그거 오바",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015704.1.6",
          "form": "차라리 3만원짜리 헬스정을 끊겠다",
          "original_form": "차라리 3만원짜리 헬스정을 끊겠다",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015704.1.7",
          "form": "아니 이게 또 런닝머신뛰는거랑은 달라",
          "original_form": "아니 이게 또 런닝머신뛰는거랑은 달라",
          "speaker_id": "1",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015704.1.8",
          "form": "그 바람 나무냄새맡으면서 뛰는게 힐링",
          "original_form": "그 바람 나무냄새맡으면서 뛰는게 힐링",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015704.1.9",
          "form": "답정너였다",
          "original_form": "답정너였다",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015704.1.10",
          "form": "일단 전철타고 한번해본다",
          "original_form": "일단 전철타고 한번해본다",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015704.1.11",
          "form": "어차피 일주일에 3번정도 뛰는거라",
          "original_form": "어차피 일주일에 3번정도 뛰는거라",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015704.1.12",
          "form": "홈트도 도전해봐야지",
          "original_form": "홈트도 도전해봐야지",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015704.1.13",
          "form": "나도 이번주말부터 자기관리 다시 시작합니다",
          "original_form": "나도 이번주말부터 자기관리 다시 시작합니다",
          "speaker_id": "2",
          "time": "20210826 09:53"
        },
        {
          "id": "MDRW2100015704.1.14",
          "form": "루틴 돌려놔야지",
          "original_form": "루틴 돌려놔야지",
          "speaker_id": "2",
          "time": "20210826 09:54"
        },
        {
          "id": "MDRW2100015704.1.15",
          "form": "진짜 밤낮 바꿔야지",
          "original_form": "진짜 밤낮 바꿔야지",
          "speaker_id": "1",
          "time": "20210826 09:59"
        },
        {
          "id": "MDRW2100015704.1.16",
          "form": "무병장수해야하는데",
          "original_form": "무병장수해야하는데",
          "speaker_id": "1",
          "time": "20210826 09:59"
        },
        {
          "id": "MDRW2100015704.1.17",
          "form": "아 난 이제 밤에 활동 못해",
          "original_form": "아 난 이제 밤에 활동 못해",
          "speaker_id": "2",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100015704.1.18",
          "form": "너어어무 피곤하더라",
          "original_form": "너어어무 피곤하더라",
          "speaker_id": "2",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100015704.1.19",
          "form": "차라리 일직일어나서 움직이는게 더 괜츈해짐",
          "original_form": "차라리 일직일어나서 움직이는게 더 괜츈해짐",
          "speaker_id": "2",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100015704.1.20",
          "form": "아침에는 집중이 안된다고~~~",
          "original_form": "아침에는 집중이 안된다고~~~",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100015704.1.21",
          "form": "아직 런데이 뛰기엔 덥군",
          "original_form": "아직 런데이 뛰기엔 덥군",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100015704.1.22",
          "form": "밤에 뛰면좋은데 위험하니 패스",
          "original_form": "밤에 뛰면좋은데 위험하니 패스",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100015704.1.23",
          "form": "진짜 나도 헬스하고싶다",
          "original_form": "진짜 나도 헬스하고싶다",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100015704.1.24",
          "form": "돈벌어서 개인피티 받는다",
          "original_form": "돈벌어서 개인피티 받는다",
          "speaker_id": "1",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100015704.1.25",
          "form": "아 돈벌어서 그 맛에 쓰는거지~~~",
          "original_form": "아 돈벌어서 그 맛에 쓰는거지~~~",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100015704.1.26",
          "form": "이게 비올때 주택살면 운동하기애매",
          "original_form": "이게 비올때 주택살면 운동하기애매",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100015704.1.27",
          "form": "아파트 살면 계단이라도 오르는ㄷ니ㅣ",
          "original_form": "아파트 살면 계단이라도 오르는ㄷ니ㅣ",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100015704.1.28",
          "form": "헬스도안다니고",
          "original_form": "헬스도안다니고",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100015704.1.29",
          "form": "홈트 쉽지않어",
          "original_form": "홈트 쉽지않어",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100015704.1.30",
          "form": "아 진짜 아파트 장점 하나 더 추가요",
          "original_form": "아 진짜 아파트 장점 하나 더 추가요",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100015704.1.31",
          "form": "그것만 4번하면 벌써 하루치일텐데",
          "original_form": "그것만 4번하면 벌써 하루치일텐데",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100015704.1.32",
          "form": "진짜 건강도 돈이 있어야 챙기는거야",
          "original_form": "진짜 건강도 돈이 있어야 챙기는거야",
          "speaker_id": "1",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100015704.1.33",
          "form": "사람이 여유가없으니까 건강 잃어",
          "original_form": "사람이 여유가없으니까 건강 잃어",
          "speaker_id": "1",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100015704.1.34",
          "form": "그 내가 지금 신청해서 하고있는 서울프로그램에서",
          "original_form": "그 내가 지금 신청해서 하고있는 서울프로그램에서",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100015704.1.35",
          "form": "세라밴드라고 운동용 밴드줬는데",
          "original_form": "세라밴드라고 운동용 밴드줬는데",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100015704.1.36",
          "form": "이걸로 홈트나 해봐야겠다",
          "original_form": "이걸로 홈트나 해봐야겠다",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100015704.1.37",
          "form": "기계?",
          "original_form": "기계?",
          "speaker_id": "2",
