      "utterance": [
        {
          "id": "MDRW2100030036.1.1",
          "form": "전 잔여백신이요!!",
          "original_form": "전 잔여백신이요!!",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100030036.1.2",
          "form": "10시부터 새로고침 해서",
          "original_form": "10시부터 새로고침 해서",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100030036.1.3",
          "form": "겨우 잡았네요ㅋㅋㅋ",
          "original_form": "겨우 잡았네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100030036.1.4",
          "form": "오 완전 대단하신데요 ㅎㅎ",
          "original_form": "오 완전 대단하신데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100030036.1.5",
          "form": "저는 시도조차도 안해봤어요 ㅋㅋㅋ",
          "original_form": "저는 시도조차도 안해봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100030036.1.6",
          "form": "그래도 잡았으면 된 거죠!!ㅋㅋㅋ",
          "original_form": "그래도 잡았으면 된 거죠!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:13"
        },
        {
          "id": "MDRW2100030036.1.7",
          "form": "맞아요 ㅎㅎㅎ 10부제 덕분에 9월말이어도 잡았네여 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅎㅎㅎ 10부제 덕분에 9월말이어도 잡았네여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100030036.1.8",
          "form": "백신맞고는 푹 쉬셨나요?",
          "original_form": "백신맞고는 푹 쉬셨나요?",
          "speaker_id": "1",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100030036.1.9",
          "form": "네! 전 다행히 삼일쯤 되니까",
          "original_form": "네! 전 다행히 삼일쯤 되니까",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030036.1.10",
          "form": "근육통도 사라졌어요ㅋㅋㅋ",
          "original_form": "근육통도 사라졌어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030036.1.11",
          "form": "2차 맞고는 더 열심히 쉴 거에요!!",
          "original_form": "2차 맞고는 더 열심히 쉴 거에요!!",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030036.1.12",
          "form": "저도 주사 목요일에 맞구 금요일은 재택하랴그요 ㅋㅋㅋㅋ",
          "original_form": "저도 주사 목요일에 맞구 금요일은 재택하랴그요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030036.1.13",
          "form": "맞아요 무리하시면 안돼요!!",
          "original_form": "맞아요 무리하시면 안돼요!!",
          "speaker_id": "2",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030036.1.14",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030036.1.15",
          "form": "저도 화이자맞았으면 좋겠네요",
          "original_form": "저도 화이자맞았으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100030036.1.16",
          "form": "ㅋㅋㅋ맞으실 수 있을 거에요!!",
          "original_form": "ㅋㅋㅋ맞으실 수 있을 거에요!!",
          "speaker_id": "2",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100030036.1.17",
          "form": "ㅎㅎ네네 백신맞구 돼지고기도 먹구요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅎㅎ네네 백신맞구 돼지고기도 먹구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:51"
        },
        {
          "id": "MDRW2100030036.1.18",
          "form": "ㅋㅋㅋ맞아요!! 삼겹살은 좀 기대돼용ㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요!! 삼겹살은 좀 기대돼용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:59"
        },
        {
          "id": "MDRW2100030036.1.19",
          "form": "전 백신도 안맞았는데 고기먹으러왔네요 ㅎㅎ",
          "original_form": "전 백신도 안맞았는데 고기먹으러왔네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:01"
        },
        {
          "id": "MDRW2100030036.1.20",
          "form": "앜ㅋㅋㅋㅋㅋㅋ맛있겠어요!!",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ맛있겠어요!!",
