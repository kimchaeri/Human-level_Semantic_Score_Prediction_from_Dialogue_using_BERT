      "utterance": [
        {
          "id": "MDRW2100017497.1.1",
          "form": "오 오늘은 말 잘들었나?",
          "original_form": "오 오늘은 말 잘들었나?",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017497.1.2",
          "form": "숙제도 열심히 했고",
          "original_form": "숙제도 열심히 했고",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017497.1.3",
          "form": "ㅎㅎ 착하네",
          "original_form": "ㅎㅎ 착하네",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017497.1.4",
          "form": "놀라운 건 쓸데없는 소리를 거의 내지 않았어 ㅋㅋ",
          "original_form": "놀라운 건 쓸데없는 소리를 거의 내지 않았어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017497.1.5",
          "form": "오오 아침에 얘기한 게 좀 효과가 있었나?",
          "original_form": "오오 아침에 얘기한 게 좀 효과가 있었나?",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017497.1.6",
          "form": "나름 노력한 것 같아 ㅎㅎ",
          "original_form": "나름 노력한 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017497.1.7",
          "form": "과하게 행동한 게 없었나보네",
          "original_form": "과하게 행동한 게 없었나보네",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017497.1.8",
          "form": "ㅇㅇ 그리고 눈은 알러지가 맞는지 숟가락 차게 해서 눈에 댔더니 많이 좋아졌어",
          "original_form": "ㅇㅇ 그리고 눈은 알러지가 맞는지 숟가락 차게 해서 눈에 댔더니 많이 좋아졌어",
          "speaker_id": "1",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017497.1.9",
          "form": "ㅎㅎㅎ 최고의 하루였네",
          "original_form": "ㅎㅎㅎ 최고의 하루였네",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017497.1.10",
          "form": "지금처럼만 쭉 차분하게 눈 깜빡임 없이 잘 지내면 좋겠다",
          "original_form": "지금처럼만 쭉 차분하게 눈 깜빡임 없이 잘 지내면 좋겠다",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017497.1.11",
          "form": "ㅇㅇ 점점 좋아지겠지",
          "original_form": "ㅇㅇ 점점 좋아지겠지",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017497.1.12",
          "form": "작은 아들은 대화가 잘 통해서 참 고맙고 예뻐",
          "original_form": "작은 아들은 대화가 잘 통해서 참 고맙고 예뻐",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017497.1.13",
          "form": "ㅎㅎ 맞아~그게 최고의 장점인 것 같아",
          "original_form": "ㅎㅎ 맞아~그게 최고의 장점인 것 같아",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017497.1.14",
          "form": "얘기해도 안고쳐지면 엄청 속상할텐데",
          "original_form": "얘기해도 안고쳐지면 엄청 속상할텐데",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017497.1.15",
          "form": "그래도 우리 막내는 항상 노력하니까",
          "original_form": "그래도 우리 막내는 항상 노력하니까",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017497.1.16",
          "form": "칭찬 많이 해줘야겠어",
          "original_form": "칭찬 많이 해줘야겠어",
          "speaker_id": "1",
