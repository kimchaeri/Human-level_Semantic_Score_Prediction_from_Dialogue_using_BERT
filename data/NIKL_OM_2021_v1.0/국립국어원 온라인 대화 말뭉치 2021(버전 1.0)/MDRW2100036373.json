      "utterance": [
        {
          "id": "MDRW2100036373.1.1",
          "form": "어제 저녁 와플팬에 인절미 구워먹었어요 ㅎㅎ",
          "original_form": "어제 저녁 와플팬에 인절미 구워먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 06:11"
        },
        {
          "id": "MDRW2100036373.1.2",
          "form": "와 그거 실패하면 참사던데 성공하셨군요ㅎ",
          "original_form": "와 그거 실패하면 참사던데 성공하셨군요ㅎ",
          "speaker_id": "1",
          "time": "20210831 06:42"
        },
        {
          "id": "MDRW2100036373.1.3",
          "form": "ㅎㅎㅎ 진짜 맛있는게 대참사 같아요",
          "original_form": "ㅎㅎㅎ 진짜 맛있는게 대참사 같아요",
          "speaker_id": "2",
          "time": "20210831 06:44"
        },
        {
          "id": "MDRW2100036373.1.4",
          "form": "계속 먹어요",
          "original_form": "계속 먹어요",
          "speaker_id": "2",
          "time": "20210831 06:44"
        },
        {
          "id": "MDRW2100036373.1.5",
          "form": "칼로리가 ㅜ",
          "original_form": "칼로리가 ㅜ",
          "speaker_id": "2",
          "time": "20210831 06:44"
        },
        {
          "id": "MDRW2100036373.1.6",
          "form": "전 그거 한번도 안 먹어봐서 궁금하네요",
          "original_form": "전 그거 한번도 안 먹어봐서 궁금하네요",
          "speaker_id": "1",
          "time": "20210831 06:53"
        },
        {
          "id": "MDRW2100036373.1.7",
          "form": "바삭 쫀득하다던데",
          "original_form": "바삭 쫀득하다던데",
          "speaker_id": "1",
          "time": "20210831 06:53"
        },
        {
          "id": "MDRW2100036373.1.8",
          "form": "겉바속촉",
          "original_form": "겉바속촉",
          "speaker_id": "2",
          "time": "20210831 07:11"
        },
        {
          "id": "MDRW2100036373.1.9",
          "form": "있으면 좋은데 추천까진 아니에요",
          "original_form": "있으면 좋은데 추천까진 아니에요",
          "speaker_id": "2",
          "time": "20210831 07:11"
        },
        {
          "id": "MDRW2100036373.1.10",
          "form": "에어프라이어처럼 살찌기 딱이라서요 ㅋ",
          "original_form": "에어프라이어처럼 살찌기 딱이라서요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 07:11"
        },
        {
          "id": "MDRW2100036373.1.11",
          "form": "아앗 살찌는것=JMT",
          "original_form": "아앗 살찌는것=JMT",
          "speaker_id": "1",
          "time": "20210831 07:25"
        },
        {
          "id": "MDRW2100036373.1.12",
          "form": "즉 무지 맛있다는 거군요!",
          "original_form": "즉 무지 맛있다는 거군요!",
          "speaker_id": "1",
          "time": "20210831 07:25"
        },
        {
          "id": "MDRW2100036373.1.13",
          "form": "요알못이라 두려워 시도 안했는데 갑자기 도전의식이 ㅎㅎ",
          "original_form": "요알못이라 두려워 시도 안했는데 갑자기 도전의식이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 07:25"
        },
        {
          "id": "MDRW2100036373.1.14",
          "form": "하아 맛있어요 다이어트 빠이빠이 해야해요",
          "original_form": "하아 맛있어요 다이어트 빠이빠이 해야해요",
          "speaker_id": "2",
          "time": "20210831 07:25"
        },
        {
          "id": "MDRW2100036373.1.15",
          "form": "그냥 와플팬에 구우면 되나요?",
          "original_form": "그냥 와플팬에 구우면 되나요?",
          "speaker_id": "1",
          "time": "20210831 07:31"
        },
        {
          "id": "MDRW2100036373.1.16",
          "form": "다른 소스를 찍어 먹나요",
          "original_form": "다른 소스를 찍어 먹나요",
          "speaker_id": "1",
          "time": "20210831 07:31"
        },
        {
          "id": "MDRW2100036373.1.17",
          "form": "꿀 찍어 먹어요 ㅎㅎ",
          "original_form": "꿀 찍어 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:34"
        },
        {
          "id": "MDRW2100036373.1.18",
          "form": "헉 맛있겠다ㄷㄷㄷ",
          "original_form": "헉 맛있겠다ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210831 07:34"
        },
        {
          "id": "MDRW2100036373.1.19",
          "form": "맛은 보장합니다 ㅋㅋㅋ",
          "original_form": "맛은 보장합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 07:35"
        },
        {
          "id": "MDRW2100036373.1.20",
          "form": "듣기만 해도 느낌이 와요ㅎㅎ 중독될 느낌이",
          "original_form": "듣기만 해도 느낌이 와요ㅎㅎ 중독될 느낌이",
