      },
      "utterance": [
        {
          "id": "MDRW2100031127.1.1",
          "form": "이 주제는 많이 말 안해본 것 같아",
          "original_form": "이 주제는 많이 말 안해본 것 같아",
          "speaker_id": "1",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100031127.1.2",
          "form": "해야할 집안일 있?",
          "original_form": "해야할 집안일 있?",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031127.1.3",
          "form": "가사..?",
          "original_form": "가사..?",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031127.1.4",
          "form": "집안일은 매일 있져",
          "original_form": "집안일은 매일 있져",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031127.1.5",
          "form": "매일 해도 매일 생겨",
          "original_form": "매일 해도 매일 생겨",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031127.1.6",
          "form": "집안일에 많은 협조를 하는편인지 ㅋㅋㅋㅋ",
          "original_form": "집안일에 많은 협조를 하는편인지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100031127.1.7",
          "form": "주로 어떤거 해?",
          "original_form": "주로 어떤거 해?",
          "speaker_id": "1",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031127.1.8",
          "form": "객관적으로 봐도 많이는 아닌것 같은데",
          "original_form": "객관적으로 봐도 많이는 아닌것 같은데",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031127.1.9",
          "form": "하면 청소기 돌리기 빨래널기 수건개기 밥짓기 설거지...",
          "original_form": "하면 청소기 돌리기 빨래널기 수건개기 밥짓기 설거지...",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031127.1.10",
          "form": "다 하고 있는거 아냐?",
          "original_form": "다 하고 있는거 아냐?",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031127.1.11",
          "form": "거의 다 하긴 하네",
          "original_form": "거의 다 하긴 하네",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031127.1.12",
          "form": "근데 매일 저걸 다 하는건 아니니깐ㅋㅋㅋㅋ",
          "original_form": "근데 매일 저걸 다 하는건 아니니깐ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031127.1.13",
          "form": "역시 밥 얻어먹으러 가야한다",
          "original_form": "역시 밥 얻어먹으러 가야한다",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100031127.1.14",
          "form": "감자떡 만들때 부터 알아봤어",
          "original_form": "감자떡 만들때 부터 알아봤어",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100031127.1.15",
          "form": "오시등가",
          "original_form": "오시등가",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.16",
          "form": "와볼테면 와바라",
          "original_form": "와볼테면 와바라",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.17",
          "form": "허... 진짜 갑니다?",
          "original_form": "허... 진짜 갑니다?",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.19",
          "form": "아주 겁이 없구만",
          "original_form": "아주 겁이 없구만",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.20",
          "form": "막 오라구 그러고",
          "original_form": "막 오라구 그러고",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.21",
          "form": "뭐왜뭐",
          "original_form": "뭐왜뭐",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.22",
          "form": "와서 구세즈 델꼬 가",
          "original_form": "와서 구세즈 델꼬 가",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.23",
          "form": "흐엉.. 가서 진짜 먹을거 다 털어먹어야지",
          "original_form": "흐엉.. 가서 진짜 먹을거 다 털어먹어야지",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100031127.1.24",
          "form": "전에 긱사 쓸때 음식 이것 저것 시켜볼것을",
          "original_form": "전에 긱사 쓸때 음식 이것 저것 시켜볼것을",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100031127.1.25",
          "form": "내줄수 있는건 굿즈뿐..",
          "original_form": "내줄수 있는건 굿즈뿐..",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100031127.1.26",
          "form": "이익....",
          "original_form": "이익....",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031127.1.27",
          "form": "룸메이트를 장장 3년 이상하고도",
          "original_form": "룸메이트를 장장 3년 이상하고도",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031127.1.28",
          "form": "내줄수 있는게 저거라니 ㅠㅠ",
          "original_form": "내줄수 있는게 저거라니 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:17"
        }
      ]
    }
  ]
