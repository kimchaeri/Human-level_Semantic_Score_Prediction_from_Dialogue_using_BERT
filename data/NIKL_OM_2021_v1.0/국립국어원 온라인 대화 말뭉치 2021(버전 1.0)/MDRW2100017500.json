      "utterance": [
        {
          "id": "MDRW2100017500.1.1",
          "form": "저 반쎄오 좋아해요",
          "original_form": "저 반쎄오 좋아해요",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017500.1.2",
          "form": "name1님은 좋아하시는 해외음식 있으신가요?",
          "original_form": "&name1&님은 좋아하시는 해외음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100017500.1.3",
          "form": "와우 반쎄오~!",
          "original_form": "와우 반쎄오~!",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017500.1.4",
          "form": "저는 마라샹궈에 빠졌어요 ㅜㅜ",
          "original_form": "저는 마라샹궈에 빠졌어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017500.1.5",
          "form": "마라샹궈 한번도 먹어본적 없는데 맛있나요..?",
          "original_form": "마라샹궈 한번도 먹어본적 없는데 맛있나요..?",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017500.1.6",
          "form": "네! 저 중국에서 먹어봤는데 진짜 맛있었어요!",
          "original_form": "네! 저 중국에서 먹어봤는데 진짜 맛있었어요!",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017500.1.7",
          "form": "처음 먹어보는 맛이였어요 ㅎㅎ",
          "original_form": "처음 먹어보는 맛이였어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017500.1.8",
          "form": "우와... 궁금하네요!",
          "original_form": "우와... 궁금하네요!",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100017500.1.9",
          "form": "분짜도 맛있더라구요!",
          "original_form": "분짜도 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017500.1.10",
          "form": "어디 음식이었지 베트남이었던가?",
          "original_form": "어디 음식이었지 베트남이었던가?",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017500.1.11",
          "form": "마자요!",
          "original_form": "마자요!",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017500.1.12",
          "form": "분짜도 맛있죠",
          "original_form": "분짜도 맛있죠",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017500.1.13",
          "form": "또 어떤 음식 좋아해요?",
          "original_form": "또 어떤 음식 좋아해요?",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017500.1.14",
          "form": "저 양꼬치도 좋아해요!",
          "original_form": "저 양꼬치도 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017500.1.15",
          "form": "오 양꼬치~",
          "original_form": "오 양꼬치~",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017500.1.16",
          "form": "그럼 양갈비도 먹어봤어요?",
          "original_form": "그럼 양갈비도 먹어봤어요?",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017500.1.17",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017500.1.18",
          "form": "양갈비도 맛있죠",
          "original_form": "양갈비도 맛있죠",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017500.1.19",
          "form": "양갈비 맛집 아시는 데 있어요?",
          "original_form": "양갈비 맛집 아시는 데 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017500.1.20",
          "form": "아뇨... 전 그냥 항상 따라가는 역할이라 잘 몰라요",
          "original_form": "아뇨... 전 그냥 항상 따라가는 역할이라 잘 몰라요",
