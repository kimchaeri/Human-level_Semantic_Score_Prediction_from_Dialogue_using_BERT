      "utterance": [
        {
          "id": "MDRW2100022857.1.1",
          "form": "안녕하세용 저녁 드셨어용?",
          "original_form": "안녕하세용 저녁 드셨어용?",
          "speaker_id": "1",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022857.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022857.1.3",
          "form": "네 먹었습니다",
          "original_form": "네 먹었습니다",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022857.1.4",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022857.1.5",
          "form": "저는 아직이에요ㅎㅎ",
          "original_form": "저는 아직이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022857.1.6",
          "form": "뭐 먹을까 고민하면서 배달어플 뒤적이고있네요",
          "original_form": "뭐 먹을까 고민하면서 배달어플 뒤적이고있네요",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022857.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022857.1.8",
          "form": "저는 피자먹었습니다~",
          "original_form": "저는 피자먹었습니다~",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022857.1.9",
          "form": "오 피자 어느 브랜드 선호하세요?",
          "original_form": "오 피자 어느 브랜드 선호하세요?",
          "speaker_id": "1",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022857.1.10",
          "form": "저도 얼마전에 도미노피자 먹었어요!",
          "original_form": "저도 얼마전에 도미노피자 먹었어요!",
          "speaker_id": "1",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022857.1.11",
          "form": "지금은 간단하게 냉동제품 먹었는데",
          "original_form": "지금은 간단하게 냉동제품 먹었는데",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022857.1.12",
          "form": "파파존스 좋아합니다!",
          "original_form": "파파존스 좋아합니다!",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022857.1.13",
          "form": "거기 넘 맛있더라구요ㅋㅋㅋ",
          "original_form": "거기 넘 맛있더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022857.1.14",
          "form": "토마토 소스가",
          "original_form": "토마토 소스가",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022857.1.15",
          "form": "제일 진하다해야할까용",
          "original_form": "제일 진하다해야할까용",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022857.1.16",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022857.1.17",
          "form": "도미노는 뭐드셨나요?",
          "original_form": "도미노는 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022857.1.18",
          "form": "뉴욕 오리진? 그거 먹었는대",
          "original_form": "뉴욕 오리진? 그거 먹었는대",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022857.1.19",
          "form": "점바점일수도있는데 맛있었어용!",
          "original_form": "점바점일수도있는데 맛있었어용!",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022857.1.20",
          "form": "파파존스 아이리쉬 포테이토랑",
          "original_form": "파파존스 아이리쉬 포테이토랑",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022857.1.21",
          "form": "슈퍼파파스 맛있어요ㅋㅋ",
          "original_form": "슈퍼파파스 맛있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022857.1.22",
          "form": "오 뉴욕오리진 다음에먹어봐야겠어요",
          "original_form": "오 뉴욕오리진 다음에먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022857.1.23",
          "form": "저도 아이리쉬 좋아해요!",
          "original_form": "저도 아이리쉬 좋아해요!",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022857.1.24",
          "form": "파파존스는 피자 크기가 패밀리인가? 파티 사이즈가 있어서",
          "original_form": "파파존스는 피자 크기가 패밀리인가? 파티 사이즈가 있어서",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022857.1.25",
          "form": "맨날 큰걸로 시켜요ㅋㅋㅋ",
          "original_form": "맨날 큰걸로 시켜요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022857.1.26",
          "form": "작은건 너무 작아서ㅜㅜ",
          "original_form": "작은건 너무 작아서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022857.1.27",
          "form": "브라우니도 꼭 시키구요ㅋㅋ",
          "original_form": "브라우니도 꼭 시키구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022857.1.28",
          "form": "오 드실줄 아시네요!ㅋㅋㅋ",
          "original_form": "오 드실줄 아시네요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022857.1.29",
          "form": "저 얼마전에 뚜레쥬르 갔는데",
          "original_form": "저 얼마전에 뚜레쥬르 갔는데",
          "speaker_id": "1",
          "time": "20210829 18:44"
        }
      ]
    }
  ]
