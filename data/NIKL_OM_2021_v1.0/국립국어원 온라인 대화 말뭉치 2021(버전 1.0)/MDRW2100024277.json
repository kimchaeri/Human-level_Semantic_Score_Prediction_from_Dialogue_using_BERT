      "utterance": [
        {
          "id": "MDRW2100024277.1.1",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024277.1.2",
          "form": "네 근데 스파게티면이 그냥봐서는 잘 안보이네요",
          "original_form": "네 근데 스파게티면이 그냥봐서는 잘 안보이네요",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024277.1.3",
          "form": "그리고 가격 대박이네요 만원 ㅋㅋ",
          "original_form": "그리고 가격 대박이네요 만원 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024277.1.4",
          "form": "네 ㅋㅋ 치즈 속에 숨겨져있어요",
          "original_form": "네 ㅋㅋ 치즈 속에 숨겨져있어요",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024277.1.5",
          "form": "그리고 과하게 많지도 않고 적당해요",
          "original_form": "그리고 과하게 많지도 않고 적당해요",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024277.1.6",
          "form": "치즈도 푸짐하고 맛있겠어요",
          "original_form": "치즈도 푸짐하고 맛있겠어요",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024277.1.7",
          "form": "그 이탈리아 식당에 가서 파스타랑 피자 먹고 싶을 때 먹는 그런 느낌",
          "original_form": "그 이탈리아 식당에 가서 파스타랑 피자 먹고 싶을 때 먹는 그런 느낌",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024277.1.8",
          "form": "아 뭔맛인지 조금 상상이가네요 ㅋㅋ",
          "original_form": "아 뭔맛인지 조금 상상이가네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024277.1.9",
          "form": "예전엔 까르보네 피자 8천원하던 시절이 있었는데 ㅜㅜ",
          "original_form": "예전엔 까르보네 피자 8천원하던 시절이 있었는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024277.1.10",
          "form": "지금 가격이 많이 올랐죵..",
          "original_form": "지금 가격이 많이 올랐죵..",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024277.1.11",
          "form": "까르보나라 파스타 좋아하시면 이 피자 좋아하실거에요!",
          "original_form": "까르보나라 파스타 좋아하시면 이 피자 좋아하실거에요!",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024277.1.12",
          "form": "팔천원ㅜㅜ 대박 그때 먹었어야됐는데",
          "original_form": "팔천원ㅜㅜ 대박 그때 먹었어야됐는데",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024277.1.13",
          "form": "네네 제 입맛에도 맞을것 같네요",
          "original_form": "네네 제 입맛에도 맞을것 같네요",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024277.1.14",
          "form": "ㅋㅋㅋ 피자스쿨 깐쇼피자도 있더라구요",
          "original_form": "ㅋㅋㅋ 피자스쿨 깐쇼피자도 있더라구요",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024277.1.15",
          "form": "근데 맛있긴한데 칠리소스 맛이 너무 강해요 ㅜ",
          "original_form": "근데 맛있긴한데 칠리소스 맛이 너무 강해요 ㅜ",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024277.1.16",
          "form": "깐쇼피자 그건 깐쇼새우 + 피자 인가요?",
          "original_form": "깐쇼피자 그건 깐쇼새우 + 피자 인가요?",
          "speaker_id": "2",
          "time": "20210831 00:47"
        },
        {
          "id": "MDRW2100024277.1.17",
          "form": "헐 맞아요 아주 정확해요!!",
          "original_form": "헐 맞아요 아주 정확해요!!",
          "speaker_id": "1",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024277.1.18",
          "form": "근데 칠리소스가 맛있어서 피자가 맛있게 느껴지는게 신기해요",
          "original_form": "근데 칠리소스가 맛있어서 피자가 맛있게 느껴지는게 신기해요",
          "speaker_id": "1",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100024277.1.19",
          "form": "깐쇼피자도 땡기네요 피자스쿨은 메뉴개발을 정말 열심히 하네요 ㅋㅋ",
          "original_form": "깐쇼피자도 땡기네요 피자스쿨은 메뉴개발을 정말 열심히 하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100024277.1.20",
          "form": "놀랍게도 이 메뉴들은 한참 옛날부터 있었던거에요 ㅋㅋㅋ",
          "original_form": "놀랍게도 이 메뉴들은 한참 옛날부터 있었던거에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024277.1.21",
          "form": "근데 제가 느끼기엔 점바점이 엄청 컸어요",
          "original_form": "근데 제가 느끼기엔 점바점이 엄청 컸어요",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024277.1.22",
          "form": "어느 곳은 토핑 충실히 해주고 어느곳은 너무 아끼더라구요",
          "original_form": "어느 곳은 토핑 충실히 해주고 어느곳은 너무 아끼더라구요",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024277.1.23",
          "form": "아 점포도 잘 만나야겠네요ㅜㅜ",
          "original_form": "아 점포도 잘 만나야겠네요ㅜㅜ",
