        },
        {
          "id": "MDRW2100006212.1.24",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100006212.1.25",
          "form": "어떤 부분에서 좋아하시나요?",
          "original_form": "어떤 부분에서 좋아하시나요?",
          "speaker_id": "2",
