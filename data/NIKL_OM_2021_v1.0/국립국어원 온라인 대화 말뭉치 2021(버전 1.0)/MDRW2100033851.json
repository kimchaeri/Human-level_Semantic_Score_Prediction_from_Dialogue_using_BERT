      "utterance": [
        {
          "id": "MDRW2100033851.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 12:32"
        },
        {
          "id": "MDRW2100033851.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100033851.1.3",
          "form": "치킨좋아하세요?",
          "original_form": "치킨좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100033851.1.4",
          "form": "치킨 좋아하죠! 어제도 먹었어요ㅎㅎ 어떤스타일 치킨좋아하시나요?",
          "original_form": "치킨 좋아하죠! 어제도 먹었어요ㅎㅎ 어떤스타일 치킨좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100033851.1.5",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100033851.1.6",
          "form": "무조건 양녕파입니다ㅋㅋㅋ",
          "original_form": "무조건 양녕파입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033851.1.7",
          "form": "양념도 다양하잖아요~ 오리지널양념좋아하세요?",
          "original_form": "양념도 다양하잖아요~ 오리지널양념좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033851.1.8",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033851.1.9",
          "form": "저는 처갓집슈프림이랑",
          "original_form": "저는 처갓집슈프림이랑",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033851.1.10",
          "form": "페리카나양념 좋아해요",
          "original_form": "페리카나양념 좋아해요",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033851.1.11",
          "form": "감이 오시나요?ㅋㅋㅋㅋ",
          "original_form": "감이 오시나요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033851.1.12",
          "form": "네ㅋㅋ 저는 바삭한거 좋아요 가마치통닭 같은 튀김옷 앒은거요! 치킨드실때 마시는거 뭐드세요?",
          "original_form": "네ㅋㅋ 저는 바삭한거 좋아요 가마치통닭 같은 튀김옷 앒은거요! 치킨드실때 마시는거 뭐드세요?",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033851.1.13",
          "form": "ㅋㅋㅋ 저는 콜라요",
          "original_form": "ㅋㅋㅋ 저는 콜라요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033851.1.14",
          "form": "가끔 맥주요ㅋㅋㅋ",
          "original_form": "가끔 맥주요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033851.1.15",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033851.1.16",
          "form": "저도 콜라요ㅎㅎ 탄산수나!ㅎ",
          "original_form": "저도 콜라요ㅎㅎ 탄산수나!ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100033851.1.17",
          "form": "순살로 드시나요?",
          "original_form": "순살로 드시나요?",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100033851.1.18",
          "form": "ㅋㅋㅋ치킨에따라",
          "original_form": "ㅋㅋㅋ치킨에따라",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100033851.1.19",
          "form": "다르게갑니다!",
          "original_form": "다르게갑니다!",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100033851.1.20",
          "form": "어울리는 치킨들이 있죠잉",
          "original_form": "어울리는 치킨들이 있죠잉",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100033851.1.21",
          "form": "전문가 같아요ㅋ",
          "original_form": "전문가 같아요ㅋ",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100033851.1.22",
          "form": "ㅋㅋㅋ 파닭같은건",
          "original_form": "ㅋㅋㅋ 파닭같은건",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100033851.1.23",
          "form": "아묻따순살이죠",
          "original_form": "아묻따순살이죠",
          "speaker_id": "1",
          "time": "20210825 15:49"
        }
      ]
