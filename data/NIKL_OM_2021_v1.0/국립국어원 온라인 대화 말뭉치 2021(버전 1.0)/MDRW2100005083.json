      "utterance": [
        {
          "id": "MDRW2100005083.1.1",
          "form": "학종 두개인데 두개다 면접이에요!",
          "original_form": "학종 두개인데 두개다 면접이에요!",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005083.1.2",
          "form": "그럼 면접 연습도 하겠네요",
          "original_form": "그럼 면접 연습도 하겠네요",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005083.1.3",
          "form": "네.. 담달에 할거 같아요",
          "original_form": "네.. 담달에 할거 같아요",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005083.1.4",
          "form": "요즘에도 비대면으로 면접 보나요?",
          "original_form": "요즘에도 비대면으로 면접 보나요?",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005083.1.5",
          "form": "음.. 코로나 백신 맞아서 대면으로 할거같아요",
          "original_form": "음.. 코로나 백신 맞아서 대면으로 할거같아요",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005083.1.6",
          "form": "아!!",
          "original_form": "아!!",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005083.1.7",
          "form": "저는 대학교에서 노트북으로도 면접 봤었어요",
          "original_form": "저는 대학교에서 노트북으로도 면접 봤었어요",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005083.1.8",
          "form": "전 6개 다 면접전형이었어요ㅋㅋ",
          "original_form": "전 6개 다 면접전형이었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005083.1.9",
          "form": "우왕... 자소서 대박 이셧겠어요....",
          "original_form": "우왕... 자소서 대박 이셧겠어요....",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005083.1.10",
          "form": "많이 썼었죠....",
          "original_form": "많이 썼었죠....",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005083.1.11",
          "form": "저는 두개쓰는거도 넘 힘든데",
          "original_form": "저는 두개쓰는거도 넘 힘든데",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005083.1.12",
          "form": "주변에 학종 쓰는 친구들이 얼마없어서",
          "original_form": "주변에 학종 쓰는 친구들이 얼마없어서",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005083.1.13",
          "form": "더 힘든거 같아요",
          "original_form": "더 힘든거 같아요",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005083.1.14",
          "form": "아 하긴 학교마다 다르죠",
          "original_form": "아 하긴 학교마다 다르죠",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005083.1.15",
          "form": "저희 학교는 학종 많이 썼거든요",
          "original_form": "저희 학교는 학종 많이 썼거든요",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005083.1.16",
          "form": "물론 6학종 쓰는 애는 못 들어봤지만",
          "original_form": "물론 6학종 쓰는 애는 못 들어봤지만",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005083.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저희는 교과가 더 많은더 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저희는 교과가 더 많은더 같아요",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005083.1.18",
          "form": "학교가 세특을 잘 못써주는걸로 유명해서..",
          "original_form": "학교가 세특을 잘 못써주는걸로 유명해서..",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005083.1.19",
          "form": "아 그렇구나....",
          "original_form": "아 그렇구나....",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005083.1.20",
          "form": "대학생되면 뭐가 제일 하고 싶으세요?",
          "original_form": "대학생되면 뭐가 제일 하고 싶으세요?",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005083.1.21",
          "form": "저는 친구들이랑 다른지역으로 놀러가는거요!",
          "original_form": "저는 친구들이랑 다른지역으로 놀러가는거요!",
          "speaker_id": "2",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005083.1.22",
          "form": "그땐 코로나 없어졌으면 좋겠네요",
          "original_form": "그땐 코로나 없어졌으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005083.1.23",
          "form": "대학생활 제대로 못 즐기니까 아쉬워요",
          "original_form": "대학생활 제대로 못 즐기니까 아쉬워요",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005083.1.24",
          "form": "그러실거같아요..ㅠ",
          "original_form": "그러실거같아요..ㅠ",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005083.1.25",
          "form": "다 싸강 이셧어요?",
          "original_form": "다 싸강 이셧어요?",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005083.1.26",
          "form": "전 실습과목이 두 개 있어서 그거 빼곤 싸강이었어요",
          "original_form": "전 실습과목이 두 개 있어서 그거 빼곤 싸강이었어요",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005083.1.27",
          "form": "나중에 전부 대면으로 돌아오면 학교생활 하기 더 힘들어질 것 같아요ㅋㅋ 비대면으로 해도 너무 바쁘고 힘들었어서요....",
          "original_form": "나중에 전부 대면으로 돌아오면 학교생활 하기 더 힘들어질 것 같아요ㅋㅋ 비대면으로 해도 너무 바쁘고 힘들었어서요....",
