      "utterance": [
        {
          "id": "MDRW2100035385.1.1",
          "form": "오늘 늦잠자서 아침운동 조금밖에 못했어요ㅠ",
          "original_form": "오늘 늦잠자서 아침운동 조금밖에 못했어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100035385.1.2",
          "form": "아침운동 하시는군요",
          "original_form": "아침운동 하시는군요",
          "speaker_id": "2",
          "time": "20210830 12:11"
        },
        {
          "id": "MDRW2100035385.1.3",
          "form": "저는 평일엔 퇴근하고 월수금 운동하는데",
          "original_form": "저는 평일엔 퇴근하고 월수금 운동하는데",
          "speaker_id": "2",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100035385.1.4",
          "form": "그나마도 이번주는 백신 맞을 예정이라 조금 쉬려고요",
          "original_form": "그나마도 이번주는 백신 맞을 예정이라 조금 쉬려고요",
          "speaker_id": "2",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100035385.1.5",
          "form": "저녁에는 안나가게 되더라고요",
          "original_form": "저녁에는 안나가게 되더라고요",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100035385.1.6",
          "form": "어휴 그럼 더 쉬셔야죠",
          "original_form": "어휴 그럼 더 쉬셔야죠",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100035385.1.7",
          "form": "백신맞기전에는 좋은 상태를 유지해야된다더라구요",
          "original_form": "백신맞기전에는 좋은 상태를 유지해야된다더라구요",
          "speaker_id": "1",
          "time": "20210830 12:38"
        },
        {
          "id": "MDRW2100035385.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035385.1.9",
          "form": "맛있는것도 많이 드시고 푹 쉬셔요!",
          "original_form": "맛있는것도 많이 드시고 푹 쉬셔요!",
          "speaker_id": "1",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100035385.1.10",
          "form": "하필 지금 기온차가 커서 더 관리해줘야하잖아요ㅠ",
          "original_form": "하필 지금 기온차가 커서 더 관리해줘야하잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100035385.1.11",
          "form": "네네 환절기 건강 관리 잘해야죠",
          "original_form": "네네 환절기 건강 관리 잘해야죠",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100035385.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100035385.1.13",
          "form": "유산균 챙겨드시나요?",
          "original_form": "유산균 챙겨드시나요?",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100035385.1.14",
          "form": "요즘 유산균은 못챙겨 먹었어요",
          "original_form": "요즘 유산균은 못챙겨 먹었어요",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100035385.1.15",
          "form": "비타민이랑 밀크시슬 먹고 있습니다",
          "original_form": "비타민이랑 밀크시슬 먹고 있습니다",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100035385.1.16",
          "form": "밀크시슬은 어디에 좋은거에요??",
          "original_form": "밀크시슬은 어디에 좋은거에요??",
          "speaker_id": "1",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100035385.1.17",
          "form": "간보호에 좋대요",
          "original_form": "간보호에 좋대요",
          "speaker_id": "2",
          "time": "20210830 15:17"
        },
        {
          "id": "MDRW2100035385.1.18",
          "form": "제가 원래 간 쪽이 약하고 얼굴에 열이 잘 올라서 몇 년 전부터 먹고 있어요",
          "original_form": "제가 원래 간 쪽이 약하고 얼굴에 열이 잘 올라서 몇 년 전부터 먹고 있어요",
          "speaker_id": "2",
          "time": "20210830 15:17"
        },
        {
          "id": "MDRW2100035385.1.19",
          "form": "효과 좀 보셨나요??",
          "original_form": "효과 좀 보셨나요??",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100035385.1.20",
          "form": "네 저는 효과가 좀 있었어요",
          "original_form": "네 저는 효과가 좀 있었어요",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100035385.1.21",
          "form": "간 건강은 사실 잘 실감이 안나는데",
          "original_form": "간 건강은 사실 잘 실감이 안나는데",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100035385.1.22",
          "form": "얼굴에 열 오르는 건 한결 나아졌거든요",
          "original_form": "얼굴에 열 오르는 건 한결 나아졌거든요",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100035385.1.23",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100035385.1.24",
          "form": "저 얼굴에 열 진짜 잘오르는데..",
          "original_form": "저 얼굴에 열 진짜 잘오르는데..",
          "speaker_id": "1",
          "time": "20210830 22:32"
        }
      ]
    }
