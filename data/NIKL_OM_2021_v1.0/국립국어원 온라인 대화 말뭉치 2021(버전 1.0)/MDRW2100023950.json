      "utterance": [
        {
          "id": "MDRW2100023950.1.1",
          "form": "계란 파 기름만 있으면",
          "original_form": "계란 파 기름만 있으면",
          "speaker_id": "2",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023950.1.2",
          "form": "볶음밥 쌉가능한데요",
          "original_form": "볶음밥 쌉가능한데요",
          "speaker_id": "2",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023950.1.3",
          "form": "심플이즈 베스트이비다",
          "original_form": "심플이즈 베스트이비다",
          "speaker_id": "2",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023950.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 마저요 근데 전 김치볶음밥 좋아해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 마저요 근데 전 김치볶음밥 좋아해서",
          "speaker_id": "1",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023950.1.5",
          "form": "김치 썰고 볶고 하자니 귀찮더라고여",
          "original_form": "김치 썰고 볶고 하자니 귀찮더라고여",
          "speaker_id": "1",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023950.1.6",
          "form": "글서 걍 입맛에 맞는 냉동볶음밥 사먹습니다ㅋㅋ",
          "original_form": "글서 걍 입맛에 맞는 냉동볶음밥 사먹습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023950.1.7",
          "form": "훔 김치를 칼로 써세요?",
          "original_form": "훔 김치를 칼로 써세요?",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023950.1.8",
          "form": "넹ㅋㅋㅋ 근데 김치도 죄다 사먹는지라...",
          "original_form": "넹ㅋㅋㅋ 근데 김치도 죄다 사먹는지라...",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023950.1.9",
          "form": "볶음밥에 넣긴 아까워요 밥반찬임ㅎㅎ",
          "original_form": "볶음밥에 넣긴 아까워요 밥반찬임ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023950.1.10",
          "form": "우와 엄마찬스 안되나요?",
          "original_form": "우와 엄마찬스 안되나요?",
          "speaker_id": "2",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023950.1.11",
          "form": "되긴하는데 대중교통 타고 김치 들고 오는 것도 일이예요ㅠㅠ",
          "original_form": "되긴하는데 대중교통 타고 김치 들고 오는 것도 일이예요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100023950.1.12",
          "form": "김치 가지러 가면 김치 말고도 반찬 너무 많이 주셔서ㅋㅋㅋ",
          "original_form": "김치 가지러 가면 김치 말고도 반찬 너무 많이 주셔서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023950.1.13",
          "form": "1시간을... 반찬을 이고지고... 걷고... 힘들어줍니다ㅠㅠ",
          "original_form": "1시간을... 반찬을 이고지고... 걷고... 힘들어줍니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023950.1.14",
          "form": "택배찬스도 안되나여?",
          "original_form": "택배찬스도 안되나여?",
          "speaker_id": "2",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023950.1.15",
          "form": "본가로 가셔서 잘 포장해서 택배로 슝",
          "original_form": "본가로 가셔서 잘 포장해서 택배로 슝",
          "speaker_id": "2",
          "time": "20210830 20:37"
        },
        {
          "id": "MDRW2100023950.1.16",
          "form": "음식이라서 택배는 안 시키는 게 좋아요.",
          "original_form": "음식이라서 택배는 안 시키는 게 좋아요.",
          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023950.1.17",
          "form": "스티로폴 바스에 잘 포장하심",
          "original_form": "스티로폴 바스에 잘 포장하심",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023950.1.18",
          "form": "괜찮을 것 같아요",
          "original_form": "괜찮을 것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023950.1.19",
          "form": "포장도 일이고ㅋㅋ 게다가 물기 있는 반찬이 많잖아요.",
          "original_form": "포장도 일이고ㅋㅋ 게다가 물기 있는 반찬이 많잖아요.",
          "speaker_id": "1",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023950.1.20",
          "form": "걍 들고 가는 게 낫습니다...",
          "original_form": "걍 들고 가는 게 낫습니다...",
          "speaker_id": "1",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023950.1.21",
          "form": "핸드카트에 끌고가세요 ㅠ",
          "original_form": "핸드카트에 끌고가세요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023950.1.22",
          "form": "버스에선 핸드카트가 더 힘들어여ㅠㅠ 오르락내리락",
          "original_form": "버스에선 핸드카트가 더 힘들어여ㅠㅠ 오르락내리락",
          "speaker_id": "1",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023950.1.23",
          "form": "김치 얻어먹기가 이렇게 힘들다니",
          "original_form": "김치 얻어먹기가 이렇게 힘들다니",
          "speaker_id": "2",
          "time": "20210830 20:41"
        }
      ]
    }
  ]
