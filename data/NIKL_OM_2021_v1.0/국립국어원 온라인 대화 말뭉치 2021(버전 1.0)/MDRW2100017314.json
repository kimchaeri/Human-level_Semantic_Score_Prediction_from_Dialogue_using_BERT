      "utterance": [
        {
          "id": "MDRW2100017314.1.1",
          "form": "독서 좋아하시나요?",
          "original_form": "독서 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017314.1.2",
          "form": "네 최근에 좀 취미삼아보려고 읽어요",
          "original_form": "네 최근에 좀 취미삼아보려고 읽어요",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017314.1.3",
          "form": "소크라테스 익스프레스 읽고있어요",
          "original_form": "소크라테스 익스프레스 읽고있어요",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017314.1.4",
          "form": "어려워요ㅋㅋㅋ무슨 책 좋아하ㅛㅔ요",
          "original_form": "어려워요ㅋㅋㅋ무슨 책 좋아하ㅛㅔ요",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017314.1.5",
          "form": "제목부터가 너무 어렵다......",
          "original_form": "제목부터가 너무 어렵다......",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017314.1.6",
          "form": "책 읽는게 너무 어려워서   베스트셀러에서 끌리는 제목 골라읽는 편이에요ㅋㅋㅋ",
          "original_form": "책 읽는게 너무 어려워서   베스트셀러에서 끌리는 제목 골라읽는 편이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017314.1.7",
          "form": "최근엔 범죄관련 책 읽었는데 이런건 안 어렵더라구요 술술읽히고",
          "original_form": "최근엔 범죄관련 책 읽었는데 이런건 안 어렵더라구요 술술읽히고",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017314.1.8",
          "form": "우와 그 책은 제목이 뭐에요",
          "original_form": "우와 그 책은 제목이 뭐에요",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017314.1.9",
          "form": "범죄심리 해부노트?",
          "original_form": "범죄심리 해부노트?",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017314.1.10",
          "form": "와 뭔가 흥미진진하네뇨",
          "original_form": "와 뭔가 흥미진진하네뇨",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017314.1.11",
          "form": "이수정교수님껀데 신작이라길래 바로 읽어봤어요ㅋㅋㅋ",
          "original_form": "이수정교수님껀데 신작이라길래 바로 읽어봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017314.1.12",
          "form": "직업상 읽는건가요? 관심사?",
          "original_form": "직업상 읽는건가요? 관심사?",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017314.1.13",
          "form": "아 저자가 좋으신 분이시네요",
          "original_form": "아 저자가 좋으신 분이시네요",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017314.1.14",
          "form": "관심이요!",
          "original_form": "관심이요!",
          "speaker_id": "1",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017314.1.15",
          "form": "책은 주로 구매해서 읽으세요?",
          "original_form": "책은 주로 구매해서 읽으세요?",
          "speaker_id": "1",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017314.1.16",
          "form": "네 주로 구매하는데 책값이 너무 들어서 도서관 가야겠어요",
          "original_form": "네 주로 구매하는데 책값이 너무 들어서 도서관 가야겠어요",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017314.1.17",
          "form": "근데 신작 보는 즐거움이 사서 읽을 때 젤 크니깐",
          "original_form": "근데 신작 보는 즐거움이 사서 읽을 때 젤 크니깐",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017314.1.18",
          "form": "저희지역 도서관엔 바로대출이라고 신작도서 요청하면 서점에서 빌려서 볼수있게 해주더라고요!",
          "original_form": "저희지역 도서관엔 바로대출이라고 신작도서 요청하면 서점에서 빌려서 볼수있게 해주더라고요!",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017314.1.19",
          "form": "그걸 도서관에서 나중에 구매하는 방식으로 대신 신작이나 베스트셀러는 나오자마자 신청하는 사람이 많아서",
          "original_form": "그걸 도서관에서 나중에 구매하는 방식으로 대신 신작이나 베스트셀러는 나오자마자 신청하는 사람이 많아서",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017314.1.20",
          "form": "어 저희 지역도 있긴 있던데 괜히 죄송해서 못하겠더라구요ㅋㅋ",
          "original_form": "어 저희 지역도 있긴 있던데 괜히 죄송해서 못하겠더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017314.1.21",
          "form": "다 도서관에서 구매하는거였규나..",
          "original_form": "다 도서관에서 구매하는거였규나..",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017314.1.22",
          "form": "어차피 베스트셀러같은건 다 구매할거라 먼저 요청하는거 뿐이라 어때용!",
          "original_form": "어차피 베스트셀러같은건 다 구매할거라 먼저 요청하는거 뿐이라 어때용!",
          "speaker_id": "1",
          "time": "20210826 18:48"
        }
      ]
    }
