      "utterance": [
        {
          "id": "MDRW2100034905.1.1",
          "form": "휴..",
          "original_form": "휴..",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034905.1.2",
          "form": "저오믈 배터지게 먹고왔어요",
          "original_form": "저오믈 배터지게 먹고왔어요",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034905.1.3",
          "form": "뭐 드셨나요?!",
          "original_form": "뭐 드셨나요?!",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034905.1.4",
          "form": "마라탕과 아이스크림입니다..",
          "original_form": "마라탕과 아이스크림입니다..",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034905.1.5",
          "form": "정말 행복하게 건강을 놓고왔어요",
          "original_form": "정말 행복하게 건강을 놓고왔어요",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034905.1.6",
          "form": "와 너무 맛있었을 거 같아요!",
          "original_form": "와 너무 맛있었을 거 같아요!",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034905.1.7",
          "form": "배가 빵빵해서 러닝하는데 버겁더라고요 ㅋㅋㅋㅋ",
          "original_form": "배가 빵빵해서 러닝하는데 버겁더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034905.1.8",
          "form": "전 이번달에 벌써 5번이나 먹어서ㅋㅋ큐ㅠㅜ 그만 먹기 위해 요 며칠 뇌에 힘주고 참고 있어요",
          "original_form": "전 이번달에 벌써 5번이나 먹어서ㅋㅋ큐ㅠㅜ 그만 먹기 위해 요 며칠 뇌에 힘주고 참고 있어요",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034905.1.9",
          "form": "아이고 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아이고 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100034905.1.10",
          "form": "마라탕이 중독성이 장난 아니죠",
          "original_form": "마라탕이 중독성이 장난 아니죠",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100034905.1.11",
          "form": "통장도 아야하는 음식 ㅠㅠ",
          "original_form": "통장도 아야하는 음식 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034905.1.12",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034905.1.13",
          "form": "너무 중독적이에요...",
          "original_form": "너무 중독적이에요...",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100034905.1.14",
          "form": "아이스크림은 어떤 아이스크림 드셨나요?",
          "original_form": "아이스크림은 어떤 아이스크림 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100034905.1.15",
          "form": "그 롯데리아",
          "original_form": "그 롯데리아",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034905.1.16",
          "form": "토네이도요 ㅎㅎ!",
          "original_form": "토네이도요 ㅎㅎ!",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034905.1.17",
          "form": "토네이도 진짜 맛있죠!",
          "original_form": "토네이도 진짜 맛있죠!",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034905.1.18",
          "form": "냉 ㅠㅠ 진짜 달달하고 맛나더라고요",
          "original_form": "냉 ㅠㅠ 진짜 달달하고 맛나더라고요",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034905.1.19",
          "form": "마라탕에 뭐넣어드새요?",
          "original_form": "마라탕에 뭐넣어드새요?",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034905.1.20",
          "form": "저도 가끔 토네이도만 먹으러 롯데리아 가요ㅋㅋㅋㅋ",
          "original_form": "저도 가끔 토네이도만 먹으러 롯데리아 가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034905.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실 햄버거집이아니라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실 햄버거집이아니라",
          "speaker_id": "2",
          "time": "20210829 07:03"
        },
