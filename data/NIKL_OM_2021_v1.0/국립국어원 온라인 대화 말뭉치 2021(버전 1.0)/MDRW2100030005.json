      "utterance": [
        {
          "id": "MDRW2100030005.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100030005.1.2",
          "form": "사랑하죠 저는 여행의 남자 입니다",
          "original_form": "사랑하죠 저는 여행의 남자 입니다",
          "speaker_id": "2",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100030005.1.3",
          "form": "어디 여행 다녀오셨나요 최근에",
          "original_form": "어디 여행 다녀오셨나요 최근에",
          "speaker_id": "2",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100030005.1.4",
          "form": "해외로는 2년전에 태국 갔다 왔습니다",
          "original_form": "해외로는 2년전에 태국 갔다 왔습니다",
          "speaker_id": "1",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100030005.1.5",
          "form": "국내는 여름 휴가로 경기도 펜션 두 군데 다녀왔어요.. 그냥 펜션 수영장 밖에 이용 못했지만요..",
          "original_form": "국내는 여름 휴가로 경기도 펜션 두 군데 다녀왔어요.. 그냥 펜션 수영장 밖에 이용 못했지만요..",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100030005.1.6",
          "form": "싸와티캄",
          "original_form": "싸와티캄",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030005.1.7",
          "form": "ㅋㅋ 태국 가보셨나요",
          "original_form": "ㅋㅋ 태국 가보셨나요",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030005.1.8",
          "form": "아니오",
          "original_form": "아니오",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030005.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030005.1.10",
          "form": "가보고싶어영",
          "original_form": "가보고싶어영",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030005.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030005.1.12",
          "form": "태국..바다가 아름답죠",
          "original_form": "태국..바다가 아름답죠",
          "speaker_id": "1",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100030005.1.13",
          "form": "가보셨던 곳중에 다시 가고 싶은 여행지 있나요",
          "original_form": "가보셨던 곳중에 다시 가고 싶은 여행지 있나요",
          "speaker_id": "1",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100030005.1.14",
          "form": "음 두고 지 있는데",
          "original_form": "음 두고 지 있는데",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100030005.1.15",
          "form": "두 곳",
          "original_form": "두 곳",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100030005.1.16",
          "form": "되나봐",
          "original_form": "되나봐",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100030005.1.17",
          "form": "캐나다 뱀프",
          "original_form": "캐나다 뱀프",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100030005.1.18",
          "form": "스페인 바르셀로나",
          "original_form": "스페인 바르셀로나",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100030005.1.19",
          "form": "밴프입니다 밴프",
          "original_form": "밴프입니다 밴프",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100030005.1.20",
          "form": "캐나다 밴프 라는 곳은 처음 들어봤네요!",
          "original_form": "캐나다 밴프 라는 곳은 처음 들어봤네요!",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030005.1.21",
          "form": "어떤 곳이었나요?",
          "original_form": "어떤 곳이었나요?",
          "speaker_id": "1",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030005.1.22",
          "form": "록키 산맥",
          "original_form": "록키 산맥",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030005.1.23",
          "form": "근교 이구요",
          "original_form": "근교 이구요",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030005.1.24",
          "form": "곰이 살아요 곰",
          "original_form": "곰이 살아요 곰",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030005.1.25",
          "form": "아하 록키 산맥",
          "original_form": "아하 록키 산맥",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100030005.1.26",
          "form": "곰...을 마주치셨었나요....?",
          "original_form": "곰...을 마주치셨었나요....?",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100030005.1.27",
          "form": "봐로 앞에서 봤습니다",
          "original_form": "봐로 앞에서 봤습니다",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100030005.1.28",
          "form": "진짜 무서워요",
          "original_form": "진짜 무서워요",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100030005.1.29",
          "form": "차를 안타고 있었으면 이미 고기 밥 아닌데",
          "original_form": "차를 안타고 있었으면 이미 고기 밥 아닌데",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100030005.1.30",
          "form": "어 곤 밥",
          "original_form": "어 곤 밥",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100030005.1.31",
          "form": "곰 밥",
          "original_form": "곰 밥",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100030005.1.32",
          "form": "허이고,,,,,진짜 위험한 상황이었네요....",
          "original_form": "허이고,,,,,진짜 위험한 상황이었네요....",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100030005.1.33",
          "form": "네 그래도 정말 좋앗서요",
          "original_form": "네 그래도 정말 좋앗서요",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100030005.1.34",
          "form": "유월인데도 눈이  펑펑 왔어요",
          "original_form": "유월인데도 눈이  펑펑 왔어요",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100030005.1.35",
          "form": "헐 진짜요? 여름이 없는 지역인가봐요",
          "original_form": "헐 진짜요? 여름이 없는 지역인가봐요",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100030005.1.36",
          "form": "위도가 높아서 눈이 와요",
          "original_form": "위도가 높아서 눈이 와요",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100030005.1.37",
          "form": "그리고 빙하 녹은 물이 내려와서 물색깔이 애 맷이 에메랄드빛 입니다",
          "original_form": "그리고 빙하 녹은 물이 내려와서 물색깔이 애 맷이 에메랄드빛 입니다",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100030005.1.38",
          "form": "에메랄드빛 물..진귀한 경험이었겠어요",
          "original_form": "에메랄드빛 물..진귀한 경험이었겠어요",
          "speaker_id": "1",
          "time": "20210903 19:23"
        }
      ]
    }
