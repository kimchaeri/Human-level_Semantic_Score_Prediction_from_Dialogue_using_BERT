      "utterance": [
        {
          "id": "MDRW2100015922.1.1",
          "form": "우리 올해는 여행안가?",
          "original_form": "우리 올해는 여행안가?",
          "speaker_id": "1",
          "time": "20210826 02:59"
        },
        {
          "id": "MDRW2100015922.1.2",
          "form": "어디가고싶은 곳 있어?",
          "original_form": "어디가고싶은 곳 있어?",
          "speaker_id": "2",
          "time": "20210826 02:59"
        },
        {
          "id": "MDRW2100015922.1.3",
          "form": "코로나때문에 고민이네",
          "original_form": "코로나때문에 고민이네",
          "speaker_id": "1",
          "time": "20210826 02:59"
        },
        {
          "id": "MDRW2100015922.1.4",
          "form": "어디가고싶은데?",
          "original_form": "어디가고싶은데?",
          "speaker_id": "2",
          "time": "20210826 02:59"
        },
        {
          "id": "MDRW2100015922.1.5",
          "form": "가고싶은건 해외여행 ㅋㅋ",
          "original_form": "가고싶은건 해외여행 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015922.1.6",
          "form": "해외는 지금 힘들지",
          "original_form": "해외는 지금 힘들지",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015922.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015922.1.8",
          "form": "국내에는 없어?",
          "original_form": "국내에는 없어?",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015922.1.9",
          "form": "국내는 다 거기서 거기라 잘 모르겟네",
          "original_form": "국내는 다 거기서 거기라 잘 모르겟네",
          "speaker_id": "1",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015922.1.10",
          "form": "호캉스 한번 갈까?",
          "original_form": "호캉스 한번 갈까?",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015922.1.11",
          "form": "오 호캉스 좋지",
          "original_form": "오 호캉스 좋지",
          "speaker_id": "1",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015922.1.12",
          "form": "지역은 상관없어?",
          "original_form": "지역은 상관없어?",
          "speaker_id": "2",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015922.1.13",
          "form": "가평?",
          "original_form": "가평?",
          "speaker_id": "1",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015922.1.14",
          "form": "나는 풀빌라도 좋아",
          "original_form": "나는 풀빌라도 좋아",
          "speaker_id": "1",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015922.1.15",
          "form": "풀빌라보다는 호텔이지",
          "original_form": "풀빌라보다는 호텔이지",
          "speaker_id": "2",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015922.1.16",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015922.1.17",
          "form": "더 깔끔하고 시설이 좋아",
          "original_form": "더 깔끔하고 시설이 좋아",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015922.1.18",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015922.1.19",
          "form": "근데 풀빌라 한번도 안가봐서",
          "original_form": "근데 풀빌라 한번도 안가봐서",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015922.1.20",
          "form": "한번쯤 가보는것도 괜찮겠지",
          "original_form": "한번쯤 가보는것도 괜찮겠지",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015922.1.21",
          "form": "아니면 캠핑은 어때?",
          "original_form": "아니면 캠핑은 어때?",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015922.1.22",
          "form": "캠핑같은곳은 벌레가 많아",
          "original_form": "캠핑같은곳은 벌레가 많아",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015922.1.23",
          "form": "아 그럼 안되겠다",
          "original_form": "아 그럼 안되겠다",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015922.1.24",
          "form": "그냥 서울로 호캉스 한번가자",
          "original_form": "그냥 서울로 호캉스 한번가자",
          "speaker_id": "2",
          "time": "20210826 03:03"
        },
        {
          "id": "MDRW2100015922.1.25",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210826 03:03"
        }
      ]
