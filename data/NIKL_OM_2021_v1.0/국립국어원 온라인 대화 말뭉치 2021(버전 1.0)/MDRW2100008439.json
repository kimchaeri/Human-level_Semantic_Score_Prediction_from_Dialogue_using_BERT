      "utterance": [
        {
          "id": "MDRW2100008439.1.1",
          "form": "오늘 비 오나요?",
          "original_form": "오늘 비 오나요?",
          "speaker_id": "1",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008439.1.2",
          "form": "거기 비 오냐?",
          "original_form": "거기 비 오냐?",
          "speaker_id": "1",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008439.1.3",
          "form": "여기는 해 쨍쨍",
          "original_form": "여기는 해 쨍쨍",
          "speaker_id": "1",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008439.1.4",
          "form": "어제는 퍼붓던데",
          "original_form": "어제는 퍼붓던데",
          "speaker_id": "1",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008439.1.5",
          "form": "여기도 비온다면서 안와",
          "original_form": "여기도 비온다면서 안와",
          "speaker_id": "2",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008439.1.6",
          "form": "내가 꼭 약속 안나갈때 비 안오드라",
          "original_form": "내가 꼭 약속 안나갈때 비 안오드라",
          "speaker_id": "2",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008439.1.7",
          "form": "인생은 불공평해",
          "original_form": "인생은 불공평해",
          "speaker_id": "2",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008439.1.8",
          "form": "인생은 원래 불공평한겁니자",
          "original_form": "인생은 원래 불공평한겁니자",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008439.1.9",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008439.1.10",
          "form": "오늘 그래도 햇빛 나는거 치고는",
          "original_form": "오늘 그래도 햇빛 나는거 치고는",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.11",
          "form": "많이 안 더운거 같아",
          "original_form": "많이 안 더운거 같아",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.12",
          "form": "이제 가을이니까",
          "original_form": "이제 가을이니까",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.13",
          "form": "가을인데 왤케 더워..",
          "original_form": "가을인데 왤케 더워..",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.14",
          "form": "여름도 다 갔군..",
          "original_form": "여름도 다 갔군..",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.15",
          "form": "가을엔 원래 더워",
          "original_form": "가을엔 원래 더워",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.16",
          "form": "여름 빠빠이",
          "original_form": "여름 빠빠이",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.17",
          "form": "가을 웰컴",
          "original_form": "가을 웰컴",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.18",
          "form": "지구 온난화때문에",
          "original_form": "지구 온난화때문에",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.19",
          "form": "가을도 더워",
          "original_form": "가을도 더워",
          "speaker_id": "2",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.20",
          "form": "다음주엔 내내 비온다는데",
          "original_form": "다음주엔 내내 비온다는데",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.21",
          "form": "낮에는 덥고 밤에는 춥고",
          "original_form": "낮에는 덥고 밤에는 춥고",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.22",
          "form": "이런 날씨엔 뭐입냐",
          "original_form": "이런 날씨엔 뭐입냐",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.23",
          "form": "나는 반팔 반바지",
          "original_form": "나는 반팔 반바지",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008439.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008439.1.25",
          "form": "긴팔 입기엔 아직 너무 덥다",
          "original_form": "긴팔 입기엔 아직 너무 덥다",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008439.1.26",
          "form": "날씨가 애매해",
          "original_form": "날씨가 애매해",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008439.1.27",
          "form": "저녁엔 추우니까 그래도",
          "original_form": "저녁엔 추우니까 그래도",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008439.1.28",
          "form": "걸칠거 하나 챙겨야해",
          "original_form": "걸칠거 하나 챙겨야해",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008439.1.29",
          "form": "나는 강하기 때문에 그냥 걷지",
          "original_form": "나는 강하기 때문에 그냥 걷지",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008439.1.30",
          "form": "강한 사람은 위에 안 걸쳐?",
          "original_form": "강한 사람은 위에 안 걸쳐?",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008439.1.31",
          "form": "ㅎㅎ농담~~",
          "original_form": "ㅎㅎ농담~~",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008439.1.32",
          "form": "날 쌀쌀해져서 뭐라도 걸쳐야지",
          "original_form": "날 쌀쌀해져서 뭐라도 걸쳐야지",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008439.1.33",
          "form": "에어컨은 조심해",
          "original_form": "에어컨은 조심해",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008439.1.34",
          "form": "이제 에어컨 아낄 날씨여",
          "original_form": "이제 에어컨 아낄 날씨여",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008439.1.35",
          "form": "감기 걸렸어",
          "original_form": "감기 걸렸어",
          "speaker_id": "1",
          "time": "20210822 15:54"
        }
      ]
    }
