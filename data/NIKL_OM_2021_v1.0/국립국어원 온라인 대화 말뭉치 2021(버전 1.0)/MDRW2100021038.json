      "utterance": [
        {
          "id": "MDRW2100021038.1.1",
          "form": "곰표 맥주 드셔보셨어요?",
          "original_form": "곰표 맥주 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100021038.1.2",
          "form": "네 한 번 먹어봤는데 괜찮더라구요",
          "original_form": "네 한 번 먹어봤는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100021038.1.3",
          "form": "저도 한번 먹어봤는데 괜찮았어요",
          "original_form": "저도 한번 먹어봤는데 괜찮았어요",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021038.1.4",
          "form": "요즘 말표 양표 이런 것도 나오던데",
          "original_form": "요즘 말표 양표 이런 것도 나오던데",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021038.1.5",
          "form": "엄청 다양해요ㅋㅋ 맥주 말고도 별 게 다 나오던데요",
          "original_form": "엄청 다양해요ㅋㅋ 맥주 말고도 별 게 다 나오던데요",
          "speaker_id": "1",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021038.1.6",
          "form": "네네 곰표 무늬 있는 것 많더라고요",
          "original_form": "네네 곰표 무늬 있는 것 많더라고요",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021038.1.7",
          "form": "최근엔 베스킨라빈스도 콜라보 많이 하더라고요",
          "original_form": "최근엔 베스킨라빈스도 콜라보 많이 하더라고요",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021038.1.8",
          "form": "이번달은 맥심커피 아이스크림이 나왔어요",
          "original_form": "이번달은 맥심커피 아이스크림이 나왔어요",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021038.1.9",
          "form": "커피맛이면 맛있을 것 같네요ㅋㅋ",
          "original_form": "커피맛이면 맛있을 것 같네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021038.1.10",
          "form": "네 근데 더위사냥 비슷하지 않을까...",
          "original_form": "네 근데 더위사냥 비슷하지 않을까...",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021038.1.11",
          "form": "ㅋㅋ 그럴 것 같기도 하네요",
          "original_form": "ㅋㅋ 그럴 것 같기도 하네요",
          "speaker_id": "1",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100021038.1.12",
          "form": "진로 두꺼비 인형 너무 귀엽더라구요",
          "original_form": "진로 두꺼비 인형 너무 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100021038.1.13",
          "form": "네 인형 갖고싶어서 사는 사람도 많을 것 같아요",
          "original_form": "네 인형 갖고싶어서 사는 사람도 많을 것 같아요",
          "speaker_id": "2",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100021038.1.14",
          "form": "야구장에도 인형탈들 가서 시구도 하고 그러는 거 봤어요ㅋㅋ",
          "original_form": "야구장에도 인형탈들 가서 시구도 하고 그러는 거 봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100021038.1.15",
          "form": "ㅋㅋㅋ마케팅 확실히 하네요",
          "original_form": "ㅋㅋㅋ마케팅 확실히 하네요",
          "speaker_id": "2",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100021038.1.16",
          "form": "근데 말표는 구두약 브랜드라 그런가 식품 콜라보 하니까 좀 이상하긴 하더라구요",
          "original_form": "근데 말표는 구두약 브랜드라 그런가 식품 콜라보 하니까 좀 이상하긴 하더라구요",
          "speaker_id": "1",
          "time": "20210828 15:38"
        },
        {
          "id": "MDRW2100021038.1.17",
          "form": "ㅋㅋㅋ그렇네요",
          "original_form": "ㅋㅋㅋ그렇네요",
