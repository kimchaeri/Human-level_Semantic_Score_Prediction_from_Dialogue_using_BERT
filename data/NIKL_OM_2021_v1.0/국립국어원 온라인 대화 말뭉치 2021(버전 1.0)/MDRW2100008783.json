      "utterance": [
        {
          "id": "MDRW2100008783.1.1",
          "form": "괜찮은 운동화 사고싶어요",
          "original_form": "괜찮은 운동화 사고싶어요",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008783.1.2",
          "form": "워킹화로요",
          "original_form": "워킹화로요",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008783.1.3",
          "form": "저도 런닝화보고있는데 종류가 너무 많네요ㅎㅎ",
          "original_form": "저도 런닝화보고있는데 종류가 너무 많네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008783.1.4",
          "form": "막 걸으면 살 빠지는 신발이라고 광고하는거있던데",
          "original_form": "막 걸으면 살 빠지는 신발이라고 광고하는거있던데",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008783.1.5",
          "form": "오 그런게 있구나..진짜면 살텐데ㅎㅎ",
          "original_form": "오 그런게 있구나..진짜면 살텐데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008783.1.6",
          "form": "저도 그 광고 보면서 저거 너무 과장 광고인데",
          "original_form": "저도 그 광고 보면서 저거 너무 과장 광고인데",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008783.1.7",
          "form": "심의 안드가나 싶었어요 ㅋㅋ",
          "original_form": "심의 안드가나 싶었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008783.1.8",
          "form": "혹하긴 하네요ㅠㅠ왜사는지 알거같은ㅋㅋ",
          "original_form": "혹하긴 하네요ㅠㅠ왜사는지 알거같은ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008783.1.9",
          "form": "사실 샌들도 좀사고싶어요",
          "original_form": "사실 샌들도 좀사고싶어요",
          "speaker_id": "2",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008783.1.10",
          "form": "비올때 걷기운동할때 신을",
          "original_form": "비올때 걷기운동할때 신을",
          "speaker_id": "2",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100008783.1.11",
          "form": "아 저도 그래서 아쿠아슈즈샀는데 많이 신을 일이 없었네요ㅠ",
          "original_form": "아 저도 그래서 아쿠아슈즈샀는데 많이 신을 일이 없었네요ㅠ",
          "speaker_id": "1",
          "time": "20210822 18:39"
        },
        {
          "id": "MDRW2100008783.1.12",
          "form": "아쿠아슈즈ㅜ 저도 샀는데 전좀 실망했어요",
          "original_form": "아쿠아슈즈ㅜ 저도 샀는데 전좀 실망했어요",
          "speaker_id": "2",
          "time": "20210822 18:43"
        },
        {
          "id": "MDRW2100008783.1.13",
          "form": "저도ㅎㅎ 이쁜가 했는데 안신다보니 이쁜것도 모르겠구..",
          "original_form": "저도ㅎㅎ 이쁜가 했는데 안신다보니 이쁜것도 모르겠구..",
          "speaker_id": "1",
          "time": "20210822 18:44"
        },
        {
          "id": "MDRW2100008783.1.14",
          "form": "발수 잘된다더니 그것도모르겠고 ㅋㅋ 잘마르지도 않고 ㅋㅋ",
          "original_form": "발수 잘된다더니 그것도모르겠고 ㅋㅋ 잘마르지도 않고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008783.1.15",
          "form": "무엇보가 발이 너무 아파서ㅠㅠ",
          "original_form": "무엇보가 발이 너무 아파서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100008783.1.16",
          "form": "아 맞아요 결론적으로 발이 아프죠ㅜㅜ",
          "original_form": "아 맞아요 결론적으로 발이 아프죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008783.1.17",
          "form": "아치형깔창을 사고싶네요",
          "original_form": "아치형깔창을 사고싶네요",
          "speaker_id": "2",
          "time": "20210822 18:54"
        },
        {
          "id": "MDRW2100008783.1.18",
          "form": "아 그런게 좋은가봐요",
          "original_form": "아 그런게 좋은가봐요",
          "speaker_id": "1",
          "time": "20210822 18:55"
