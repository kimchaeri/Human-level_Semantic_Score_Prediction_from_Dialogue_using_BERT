      "utterance": [
        {
          "id": "MDRW2100015134.1.1",
          "form": "대학원생님이세요?",
          "original_form": "대학원생님이세요?",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015134.1.2",
          "form": "네......랩 슬레이브입니다",
          "original_form": "네......랩 슬레이브입니다",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015134.1.3",
          "form": "개강이 얼마 남지 않아 슬픈 대학원생이에여",
          "original_form": "개강이 얼마 남지 않아 슬픈 대학원생이에여",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015134.1.4",
          "form": "앗...ㅠㅠ 저는 학교 안다니는데 데헷..",
          "original_form": "앗...ㅠㅠ 저는 학교 안다니는데 데헷..",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015134.1.5",
          "form": "동생이 수강신청을 하더라고요",
          "original_form": "동생이 수강신청을 하더라고요",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015134.1.6",
          "form": "너무 오래전이라 기억이 안나요",
          "original_form": "너무 오래전이라 기억이 안나요",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015134.1.7",
          "form": "수강신청 늦게하네",
          "original_form": "수강신청 늦게하네",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015134.1.8",
          "form": "그러고보니 이번에 우리학교 수강신청 서버 터진거 레전드였어",
          "original_form": "그러고보니 이번에 우리학교 수강신청 서버 터진거 레전드였어",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015134.1.9",
          "form": "아물론 저번주에..",
          "original_form": "아물론 저번주에..",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015134.1.10",
          "form": "??아직도 그런일이 있냐고",
          "original_form": "??아직도 그런일이 있냐고",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015134.1.11",
          "form": "여튼 수강신청이 8시 반이었는데",
          "original_form": "여튼 수강신청이 8시 반이었는데",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015134.1.12",
          "form": "8시 반까지 로그인이 안되는거야ㅅ",
          "original_form": "8시 반까지 로그인이 안되는거야ㅅ",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015134.1.13",
          "form": "사람들 어리둥절 하면서 봤더니",
          "original_form": "사람들 어리둥절 하면서 봤더니",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015134.1.14",
          "form": "8시 반까지^^ 서버점검을 한대",
          "original_form": "8시 반까지^^ 서버점검을 한대",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015134.1.15",
          "form": "그래서 사람들이 아니 이게 뭐람 하면서",
          "original_form": "그래서 사람들이 아니 이게 뭐람 하면서",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015134.1.16",
          "form": "두고보자 했는데",
          "original_form": "두고보자 했는데",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015134.1.17",
          "form": "아니 그럼 수강신청 언제하라고요",
          "original_form": "아니 그럼 수강신청 언제하라고요",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015134.1.18",
          "form": "8시 반이 되도 로그인이 안됨",
          "original_form": "8시 반이 되도 로그인이 안됨",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015134.1.19",
          "form": "정보본부에서 수강신청 미룬다고 공지 했는데",
          "original_form": "정보본부에서 수강신청 미룬다고 공지 했는데",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015134.1.20",
          "form": "개그네 서버를 진작 좀 늘리던가 하지",
          "original_form": "개그네 서버를 진작 좀 늘리던가 하지",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015134.1.21",
          "form": "문제는 그걸 터진 수강신청 사이트에 올려서",
          "original_form": "문제는 그걸 터진 수강신청 사이트에 올려서",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015134.1.22",
          "form": "공지도 못 봄....",
          "original_form": "공지도 못 봄....",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015134.1.23",
          "form": "뭐야.. 바보야..?",
          "original_form": "뭐야.. 바보야..?",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015134.1.24",
          "form": "그리고 그렇게 밀린 수강신청날",
          "original_form": "그리고 그렇게 밀린 수강신청날",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015134.1.25",
          "form": "수강신청이 시작되고 10분이 지날때까지 로그인이 안 되는 사태가 발생햄",
          "original_form": "수강신청이 시작되고 10분이 지날때까지 로그인이 안 되는 사태가 발생햄",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015134.1.26",
          "form": ".......",
          "original_form": ".......",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015134.1.27",
          "form": ".......약간 그 부서가 학습능력이 없는거같아",
          "original_form": ".......약간 그 부서가 학습능력이 없는거같아",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015134.1.28",
          "form": "시간 좀 지나니까 일부는 수강신청이 됐는데",
          "original_form": "시간 좀 지나니까 일부는 수강신청이 됐는데",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015134.1.29",
          "form": "일부는 아직도 로그인도 안 돼가지구",
          "original_form": "일부는 아직도 로그인도 안 돼가지구",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015134.1.30",
          "form": "수강신청을 취소하니 마니 시끄러웠지만",
          "original_form": "수강신청을 취소하니 마니 시끄러웠지만",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015134.1.31",
          "form": "취소 해줄리가 없지",
          "original_form": "취소 해줄리가 없지",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015134.1.32",
          "form": "자기들이 듣는 수업 아니다 이거지 뭐",
          "original_form": "자기들이 듣는 수업 아니다 이거지 뭐",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015134.1.33",
          "form": "양아치들이네",
          "original_form": "양아치들이네",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015134.1.34",
          "form": "그래서 넌 성공함?",
          "original_form": "그래서 넌 성공함?",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015134.1.35",
          "form": "성공하고 말고 할게 있나?",
          "original_form": "성공하고 말고 할게 있나?",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015134.1.36",
          "form": "나는 성공하고 말고 할게 없어서",
          "original_form": "나는 성공하고 말고 할게 없어서",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015134.1.37",
          "form": "듣는거 다 정원 안 차서",
          "original_form": "듣는거 다 정원 안 차서",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015134.1.38",
          "form": "그냥 장바구니에서 끝남",
          "original_form": "그냥 장바구니에서 끝남",
