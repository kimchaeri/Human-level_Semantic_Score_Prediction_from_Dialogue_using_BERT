      "utterance": [
        {
          "id": "MDRW2100018710.1.1",
          "form": "너의 가정생활은 어떠니??",
          "original_form": "너의 가정생활은 어떠니??",
          "speaker_id": "2",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018710.1.2",
          "form": "가정생활은 요즘 눈코뜰새가 없는듯요~",
          "original_form": "가정생활은 요즘 눈코뜰새가 없는듯요~",
          "speaker_id": "1",
          "time": "20210827 11:23"
        },
        {
          "id": "MDRW2100018710.1.3",
          "form": "요즘 집 정리도 내려놨음 ㅋㅋㅋㅋ",
          "original_form": "요즘 집 정리도 내려놨음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:23"
        },
        {
          "id": "MDRW2100018710.1.4",
          "form": "ㅋㅋㅋㅋ 많이 내려놨네",
          "original_form": "ㅋㅋㅋㅋ 많이 내려놨네",
          "speaker_id": "2",
          "time": "20210827 11:24"
        },
        {
          "id": "MDRW2100018710.1.5",
          "form": "어제name3 친구네 명 데려왔음",
          "original_form": "어제&name3& 친구네 명 데려왔음",
          "speaker_id": "2",
          "time": "20210827 11:24"
        },
        {
          "id": "MDRW2100018710.1.6",
          "form": "아 정신 사나와서",
          "original_form": "아 정신 사나와서",
          "speaker_id": "2",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.7",
          "form": "집에가면 암것도허기싫어여~",
          "original_form": "집에가면 암것도허기싫어여~",
          "speaker_id": "1",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.8",
          "form": "네명요.??",
          "original_form": "네명요.??",
          "speaker_id": "1",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.9",
          "form": "name3한테 이제 친구 되려고 집에 오지 말자고",
          "original_form": "&name3&한테 이제 친구 되려고 집에 오지 말자고",
          "speaker_id": "2",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.10",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "1",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.11",
          "form": "어 name3포함 넷 엄마 넷",
          "original_form": "어 &name3&포함 넷 엄마 넷",
          "speaker_id": "2",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.12",
          "form": "여덟 명에서 놀았음",
          "original_form": "여덟 명에서 놀았음",
          "speaker_id": "2",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.13",
          "form": "언니ㅠㅠ친구는 1대1이라고 얘기해줘요ㅠ",
          "original_form": "언니ㅠㅠ친구는 1대1이라고 얘기해줘요ㅠ",
          "speaker_id": "1",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.14",
          "form": "근데 그 와중에 우리 신랑은 출근 준비한다고",
          "original_form": "근데 그 와중에 우리 신랑은 출근 준비한다고",
          "speaker_id": "2",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.15",
          "form": "언니집이니 가능하다 ㅋㅋㅋㅋㅋ",
          "original_form": "언니집이니 가능하다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.16",
          "form": "아직 집에 있는 상태였는데",
          "original_form": "아직 집에 있는 상태였는데",
          "speaker_id": "2",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.17",
          "form": "아니 사실 일대일로 초대했거든",
          "original_form": "아니 사실 일대일로 초대했거든",
          "speaker_id": "2",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ신랑 얼마나 황당했을꼬 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ신랑 얼마나 황당했을꼬 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.19",
          "form": "근데 가는 길에 친구를 만났어 우리 집 가는 줄 아는데",
          "original_form": "근데 가는 길에 친구를 만났어 우리 집 가는 줄 아는데",
          "speaker_id": "2",
          "time": "20210827 11:25"
        },
        {
          "id": "MDRW2100018710.1.20",
          "form": "우리끼리 가기 뭐해서 같이 갈래 했더니 응",
          "original_form": "우리끼리 가기 뭐해서 같이 갈래 했더니 응",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.21",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.22",
          "form": "그래서 세 명이서 가는데",
          "original_form": "그래서 세 명이서 가는데",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.23",
          "form": "친구를 또 만났어",
          "original_form": "친구를 또 만났어",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.24",
          "form": "나도 꼭 가 기 뭐해서 우리 집 갈래",
          "original_form": "나도 꼭 가 기 뭐해서 우리 집 갈래",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.25",
          "form": "언니ㅠㅠ",
          "original_form": "언니ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.26",
          "form": "그래서 그렇게 네 명이서 집으로 감",
          "original_form": "그래서 그렇게 네 명이서 집으로 감",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.27",
          "form": "남자 둘 여자 둘이었는데",
          "original_form": "남자 둘 여자 둘이었는데",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.28",
          "form": "내가 엄마였음 담에 가께요 캤을껀데..",
          "original_form": "내가 엄마였음 담에 가께요 캤을껀데..",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.29",
          "form": "남자 둘이서 싸우고 브루스",
          "original_form": "남자 둘이서 싸우고 브루스",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.31",
          "form": "둘은 절대 같이 부르지 않아야겠다고 생각함",
          "original_form": "둘은 절대 같이 부르지 않아야겠다고 생각함",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.32",
          "form": "다신 온다 안카겠네여 ㅋㅋㅋㅋㅋ",
          "original_form": "다신 온다 안카겠네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.33",
          "form": "근데 하필",
          "original_form": "근데 하필",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018710.1.34",
          "form": "맞아여~애들도 싸우고 엄마도 정신없고 ㅋㅋㅋㅋ",
          "original_form": "맞아여~애들도 싸우고 엄마도 정신없고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018710.1.35",
          "form": "name3도 놀이터를 안 가고 집으로 바로 왔어",
          "original_form": "&name3&도 놀이터를 안 가고 집으로 바로 왔어",
          "speaker_id": "2",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018710.1.36",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "1",
          "time": "20210827 11:27"
        },
        {
          "id": "MDRW2100018710.1.37",
