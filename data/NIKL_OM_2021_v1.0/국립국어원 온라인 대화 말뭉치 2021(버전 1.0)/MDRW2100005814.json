      "utterance": [
        {
          "id": "MDRW2100005814.1.1",
          "form": "최근에 구매하신건 있으세요?",
          "original_form": "최근에 구매하신건 있으세요?",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005814.1.2",
          "form": "구매쪽은 제 아내 담당이라서 저는 뭘 사진 않았지만",
          "original_form": "구매쪽은 제 아내 담당이라서 저는 뭘 사진 않았지만",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005814.1.3",
          "form": "아내가 최근에 와플팬을 샀더군요",
          "original_form": "아내가 최근에 와플팬을 샀더군요",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005814.1.4",
          "form": "오! 해드셨나요?ㅋㅋㅋ",
          "original_form": "오! 해드셨나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005814.1.5",
          "form": "인절미를 구워서 주더군요",
          "original_form": "인절미를 구워서 주더군요",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005814.1.6",
          "form": "맛이 있었습니다",
          "original_form": "맛이 있었습니다",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005814.1.7",
          "form": "헐 천재시네요",
          "original_form": "헐 천재시네요",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100005814.1.8",
          "form": "바삭 쫀득한가요?",
          "original_form": "바삭 쫀득한가요?",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100005814.1.9",
          "form": "맞아요 딱 그맛입니다",
          "original_form": "맞아요 딱 그맛입니다",
          "speaker_id": "2",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100005814.1.10",
          "form": "ㅋㅋㅋㅋ 두분다 와플기계  만족하시나요?",
          "original_form": "ㅋㅋㅋㅋ 두분다 와플기계  만족하시나요?",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100005814.1.11",
          "form": "제 아내가 좋아합니다",
          "original_form": "제 아내가 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100005814.1.12",
          "form": "전 그걸 왜사나 싶었는데",
          "original_form": "전 그걸 왜사나 싶었는데",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100005814.1.13",
          "form": "막상 먹어보니 좋더군요",
          "original_form": "막상 먹어보니 좋더군요",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100005814.1.14",
          "form": "ㅋㅋㅋ 그럼 두 분 다 만족이시네요 아내분의 선견지명이었어요",
          "original_form": "ㅋㅋㅋ 그럼 두 분 다 만족이시네요 아내분의 선견지명이었어요",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100005814.1.15",
          "form": "저는 신발 사고 싶어요",
          "original_form": "저는 신발 사고 싶어요",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100005814.1.16",
          "form": "편한 런닝화같응거요",
          "original_form": "편한 런닝화같응거요",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100005814.1.17",
          "form": "달리기 좋은 계절이 시작됐지요",
          "original_form": "달리기 좋은 계절이 시작됐지요",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100005814.1.18",
          "form": "앗 달리려는건 아니고 그냥 가벼운 신발이 사고 싶어요 ㅋㅋㅋ",
          "original_form": "앗 달리려는건 아니고 그냥 가벼운 신발이 사고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100005814.1.19",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100005814.1.20",
          "form": "또 사고 싶은거 있나요?",
          "original_form": "또 사고 싶은거 있나요?",
          "speaker_id": "2",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100005814.1.21",
          "form": "화장지워주는 기계요....",
          "original_form": "화장지워주는 기계요....",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100005814.1.22",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210908 07:39"
        }
