      "utterance": [
        {
          "id": "MDRW2100029805.1.1",
          "form": "요즘 뭐사고싶은거있으세요?",
          "original_form": "요즘 뭐사고싶은거있으세요?",
          "speaker_id": "2",
          "time": "20210903 14:01"
        },
        {
          "id": "MDRW2100029805.1.2",
          "form": "속.옷 샀어요ㅋㅋ",
          "original_form": "속.옷 샀어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:35"
        },
        {
          "id": "MDRW2100029805.1.3",
          "form": "빤쮸",
          "original_form": "빤쮸",
          "speaker_id": "1",
          "time": "20210903 19:35"
        },
        {
          "id": "MDRW2100029805.1.4",
          "form": "ㄲㅋㅋㅋㅋ 개비싸서 눈물나지만",
          "original_form": "ㄲㅋㅋㅋㅋ 개비싸서 눈물나지만",
          "speaker_id": "1",
          "time": "20210903 19:35"
        },
        {
          "id": "MDRW2100029805.1.5",
          "form": "ㅋㅋㅋ 사야져사야져 저도얼마전에 부라샀어여",
          "original_form": "ㅋㅋㅋ 사야져사야져 저도얼마전에 부라샀어여",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100029805.1.6",
          "form": "생각보다자주사야하는거같아여",
          "original_form": "생각보다자주사야하는거같아여",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100029805.1.7",
          "form": "ㅋㅋㅋㅋㅋ어디꺼샀어요",
          "original_form": "ㅋㅋㅋㅋㅋ어디꺼샀어요",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100029805.1.8",
          "form": "러브미모스트? 요즘인스타에서많이나오길래",
          "original_form": "러브미모스트? 요즘인스타에서많이나오길래",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100029805.1.9",
          "form": "한번사봤어여",
          "original_form": "한번사봤어여",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100029805.1.10",
          "form": "오~ 검색해봄",
          "original_form": "오~ 검색해봄",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100029805.1.11",
          "form": "입어봤어요?",
          "original_form": "입어봤어요?",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100029805.1.12",
          "form": "아직이여 인제와가지고 씻고입어볼라고여",
          "original_form": "아직이여 인제와가지고 씻고입어볼라고여",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100029805.1.13",
          "form": "내가편하면말해줄께여ㅋㅋㅋ",
          "original_form": "내가편하면말해줄께여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100029805.1.14",
          "form": "ㅋㅋ좋아요 부라는 정말 고르기힘들어여",
          "original_form": "ㅋㅋ좋아요 부라는 정말 고르기힘들어여",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100029805.1.15",
          "form": "최근에 옛날 도시락도 샀어요",
          "original_form": "최근에 옛날 도시락도 샀어요",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100029805.1.16",
          "form": "먹는도시락?",
          "original_form": "먹는도시락?",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100029805.1.17",
          "form": "내가아는 옛날도시락?",
          "original_form": "내가아는 옛날도시락?",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100029805.1.18",
          "form": "그 양은도시락이요ㅋ",
          "original_form": "그 양은도시락이요ㅋ",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100029805.1.19",
          "form": "와우 ㅋㅋㅋㅋㅋㅋ그걸왜사요",
          "original_form": "와우 ㅋㅋㅋㅋㅋㅋ그걸왜사요",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100029805.1.20",
          "form": "ㅋㅋ갑자기 옛날 도시락 해먹고싶어서요",
          "original_form": "ㅋㅋ갑자기 옛날 도시락 해먹고싶어서요",
          "speaker_id": "1",
          "time": "20210903 19:56"
        }
