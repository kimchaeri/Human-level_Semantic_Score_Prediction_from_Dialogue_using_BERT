        {
          "id": "MDRW2100038898.1.1",
          "form": "여행가고싶네요",
          "original_form": "여행가고싶네요",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038898.1.2",
          "form": "맞아요 저 케이티엑스 마일리지 많은데ㅜㅠ",
          "original_form": "맞아요 저 케이티엑스 마일리지 많은데ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038898.1.3",
          "form": "다 소멸되게 생겼어요ㅜ",
          "original_form": "다 소멸되게 생겼어요ㅜ",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038898.1.4",
          "form": "여행가고시퍼요",
          "original_form": "여행가고시퍼요",
          "speaker_id": "1",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100038898.1.5",
          "form": "저도요!!! 유럽ㅜㅠ",
          "original_form": "저도요!!! 유럽ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100038898.1.6",
          "form": "유럽 어디요ㅠ",
          "original_form": "유럽 어디요ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100038898.1.7",
          "form": "포르투갈!!! ㅋㅋ",
          "original_form": "포르투갈!!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100038898.1.8",
          "form": "어디가고 싶으세요?",
          "original_form": "어디가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100038898.1.9",
          "form": "헐 저도",
          "original_form": "헐 저도",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100038898.1.10",
          "form": "영어 잘하세요??? 내년 목표로 회화!!!!",
          "original_form": "영어 잘하세요??? 내년 목표로 회화!!!!",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100038898.1.11",
          "form": "해외갈때마다 절실하게 느껴요ㅜ",
          "original_form": "해외갈때마다 절실하게 느껴요ㅜ",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100038898.1.12",
          "form": "ㅋㅋㅋㅋㅋ 영어...유럽에서 별로 쓸모없던뎈ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 영어...유럽에서 별로 쓸모없던뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100038898.1.13",
          "form": "혼자 갈거라..ㅋㅋ",
          "original_form": "혼자 갈거라..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100038898.1.14",
          "form": "그럼 어쩌죠!! ㅜㅠ",
          "original_form": "그럼 어쩌죠!! ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100038898.1.15",
          "form": "ㅋㅋㅋㅋㅋ ㅋ그냥.... 어케든 되던디요",
          "original_form": "ㅋㅋㅋㅋㅋ ㅋ그냥.... 어케든 되던디요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100038898.1.16",
          "form": "저도ㅋㅋ 어찌어찌 다녀는 왔는데ㅋㅋ",
          "original_form": "저도ㅋㅋ 어찌어찌 다녀는 왔는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100038898.1.17",
          "form": "터키는 패키지갈거였는데요ㅜㅜ",
          "original_form": "터키는 패키지갈거였는데요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100038898.1.18",
          "form": "ㅌㅋㅋㅋㅋ헐 취소댓나요",
          "original_form": "ㅌㅋㅋㅋㅋ헐 취소댓나요",
          "speaker_id": "1",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100038898.1.19",
          "form": "예약했다 취소했었는데 다신 못갈 느낌이죠? ㅋㅋ",
          "original_form": "예약했다 취소했었는데 다신 못갈 느낌이죠? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100038898.1.20",
          "form": "ㅠㅠㅠ 글게여ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ 글게여ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100038898.1.21",
