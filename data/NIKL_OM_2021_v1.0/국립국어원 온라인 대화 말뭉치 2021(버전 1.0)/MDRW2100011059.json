      "utterance": [
        {
          "id": "MDRW2100011059.1.1",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011059.1.2",
          "form": "좋아ㅘ는건아닌데",
          "original_form": "좋아ㅘ는건아닌데",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011059.1.3",
          "form": "자주해요",
          "original_form": "자주해요",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011059.1.4",
          "form": "컴퓨터로 하시나요?",
          "original_form": "컴퓨터로 하시나요?",
          "speaker_id": "1",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011059.1.5",
          "form": "아니여",
          "original_form": "아니여",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011059.1.6",
          "form": "아이패드로 해요 모바일겜만",
          "original_form": "아이패드로 해요 모바일겜만",
          "speaker_id": "2",
          "time": "20210824 00:17"
        },
        {
          "id": "MDRW2100011059.1.7",
          "form": "아 저도 아이패드로 해요",
          "original_form": "아 저도 아이패드로 해요",
          "speaker_id": "1",
          "time": "20210824 00:17"
        },
        {
          "id": "MDRW2100011059.1.8",
          "form": "저는 컴퓨터도 가끔해요.",
          "original_form": "저는 컴퓨터도 가끔해요.",
          "speaker_id": "1",
          "time": "20210824 00:17"
        },
        {
          "id": "MDRW2100011059.1.9",
          "form": "컴터로는 무슨게임하시나요",
          "original_form": "컴터로는 무슨게임하시나요",
          "speaker_id": "2",
          "time": "20210824 00:17"
        },
        {
          "id": "MDRW2100011059.1.10",
          "form": "패드로는요??",
          "original_form": "패드로는요??",
          "speaker_id": "2",
          "time": "20210824 00:18"
        },
        {
          "id": "MDRW2100011059.1.11",
          "form": "메이플 아시나요? 종종해요 ㅎㅎ",
          "original_form": "메이플 아시나요? 종종해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:18"
        },
        {
          "id": "MDRW2100011059.1.12",
          "form": "패드로는 고스톱칩니다",
          "original_form": "패드로는 고스톱칩니다",
          "speaker_id": "1",
          "time": "20210824 00:18"
        },
        {
          "id": "MDRW2100011059.1.13",
          "form": "오 알죠 메이플",
          "original_form": "오 알죠 메이플",
          "speaker_id": "2",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011059.1.14",
          "form": "옛날에 많이했었어요",
          "original_form": "옛날에 많이했었어요",
          "speaker_id": "2",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011059.1.15",
          "form": "고스톱은 넷마블??피망??인가요??",
          "original_form": "고스톱은 넷마블??피망??인가요??",
          "speaker_id": "2",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011059.1.16",
          "form": "피망이요!",
          "original_form": "피망이요!",
          "speaker_id": "1",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011059.1.17",
          "form": "가끔 심심할때 치면 재밌어요",
          "original_form": "가끔 심심할때 치면 재밌어요",
          "speaker_id": "1",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011059.1.18",
          "form": "무슨게임 하세요?",
          "original_form": "무슨게임 하세요?",
          "speaker_id": "1",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011059.1.19",
          "form": "피망 저두 가끔해요 ㅎㅎ",
          "original_form": "피망 저두 가끔해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100011059.1.20",
          "form": "모바일로는 타이쿤류를 즐겨핮니다",
          "original_form": "모바일로는 타이쿤류를 즐겨핮니다",
          "speaker_id": "2",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100011059.1.21",
          "form": "요즘은 비행기 서빙게임 즐겨해요",
          "original_form": "요즘은 비행기 서빙게임 즐겨해요",
          "speaker_id": "2",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100011059.1.22",
          "form": "비행기 서빙은 어떤 게임이에요?",
          "original_form": "비행기 서빙은 어떤 게임이에요?",
          "speaker_id": "1",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100011059.1.23",
          "form": "에어플레인쉐프라고",
          "original_form": "에어플레인쉐프라고",
          "speaker_id": "2",
          "time": "20210824 00:21"
        },
        {
          "id": "MDRW2100011059.1.24",
          "form": "승무원체험하는느낌으로",
          "original_form": "승무원체험하는느낌으로",
          "speaker_id": "2",
          "time": "20210824 00:21"
        },
        {
          "id": "MDRW2100011059.1.25",
          "form": "기내식 서빙해주는 게임이에요 ㅎㅎ",
          "original_form": "기내식 서빙해주는 게임이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011059.1.26",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011059.1.27",
          "form": "제가 승무원이 되는거에요?",
          "original_form": "제가 승무원이 되는거에요?",
          "speaker_id": "1",
          "time": "20210824 00:23"
        },
        {
          "id": "MDRW2100011059.1.28",
          "form": "네네 ㅎㅎㅎ 나중에 해보세요",
          "original_form": "네네 ㅎㅎㅎ 나중에 해보세요",
          "speaker_id": "2",
          "time": "20210824 00:24"
        },
        {
          "id": "MDRW2100011059.1.29",
          "form": "완전 재밌어용",
          "original_form": "완전 재밌어용",
          "speaker_id": "2",
          "time": "20210824 00:24"
        }
      ]
