      "utterance": [
        {
          "id": "MDRW2100008448.1.1",
          "form": "저는 일단 필요한거..? 근데",
          "original_form": "저는 일단 필요한거..? 근데",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100008448.1.2",
          "form": "꽂히면 고민하다가 사요ㅋㅋㅋ",
          "original_form": "꽂히면 고민하다가 사요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100008448.1.3",
          "form": "저도 그러는 거 같아요ㅎ",
          "original_form": "저도 그러는 거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210823 21:12"
        },
        {
          "id": "MDRW2100008448.1.4",
          "form": "위시리스트가 있나요",
          "original_form": "위시리스트가 있나요",
          "speaker_id": "1",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100008448.1.5",
          "form": "음 가끔 정리해두긴 하는데",
          "original_form": "음 가끔 정리해두긴 하는데",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100008448.1.6",
          "form": "또 거기에서 벗어나더라고욬ㅋㅋㅋㅋ",
          "original_form": "또 거기에서 벗어나더라고욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100008448.1.7",
          "form": "앗.. 하긴 담아뒀다 지나고 보면 마음이 바뀌기도 하니까요",
          "original_form": "앗.. 하긴 담아뒀다 지나고 보면 마음이 바뀌기도 하니까요",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100008448.1.8",
          "form": "그리고 충동적으로 할인하는거 보면",
          "original_form": "그리고 충동적으로 할인하는거 보면",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100008448.1.9",
          "form": "오 괜찮은데? 하고 사고 그래요ㅋㅋㅋ",
          "original_form": "오 괜찮은데? 하고 사고 그래요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100008448.1.10",
          "form": "맞아요ㅋㅋ 약간  할인하면",
          "original_form": "맞아요ㅋㅋ 약간  할인하면",
          "speaker_id": "1",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100008448.1.11",
          "form": "굳이 필요없어도 사게되는게 있더라구요",
          "original_form": "굳이 필요없어도 사게되는게 있더라구요",
          "speaker_id": "1",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100008448.1.12",
          "form": "엇 싸다!  하고 충동적인 구매로 이어지는..",
          "original_form": "엇 싸다!  하고 충동적인 구매로 이어지는..",
          "speaker_id": "1",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100008448.1.13",
          "form": "맞아요 원플원 이런거 하면",
          "original_form": "맞아요 원플원 이런거 하면",
          "speaker_id": "2",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100008448.1.14",
          "form": "괜히 사게 되고 그래요ㅋㅋㅋ",
          "original_form": "괜히 사게 되고 그래요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100008448.1.15",
          "form": "맞아요 뭔가 더 이득인 기분",
          "original_form": "맞아요 뭔가 더 이득인 기분",
          "speaker_id": "1",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100008448.1.16",
          "form": "사실 안 사면 0원인데",
          "original_form": "사실 안 사면 0원인데",
          "speaker_id": "2",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100008448.1.17",
          "form": "굳이굳이 사게 되는ㅋㅋㅋ",
          "original_form": "굳이굳이 사게 되는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100008448.1.18",
          "form": "공감합니다ㅎ",
          "original_form": "공감합니다ㅎ",
          "speaker_id": "1",
          "time": "20210823 21:57"
        },
        {
          "id": "MDRW2100008448.1.19",
          "form": "돈쓰는거 제일 재밌어",
          "original_form": "돈쓰는거 제일 재밌어",
          "speaker_id": "1",
          "time": "20210823 21:57"
        },
        {
          "id": "MDRW2100008448.1.20",
          "form": "최근에 구매한거 있으세요?",
          "original_form": "최근에 구매한거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 21:58"
        },
        {
          "id": "MDRW2100008448.1.21",
          "form": "저 완전 최근은 아닌데 카메라 샀어요!",
          "original_form": "저 완전 최근은 아닌데 카메라 샀어요!",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100008448.1.22",
          "form": "사진찍는거 좋아하시나봐요",
          "original_form": "사진찍는거 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100008448.1.23",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210824 11:42"
        }
      ]
