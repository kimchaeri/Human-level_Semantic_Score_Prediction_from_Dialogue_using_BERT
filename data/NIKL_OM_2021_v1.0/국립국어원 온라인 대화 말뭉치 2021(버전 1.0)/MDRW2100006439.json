      "utterance": [
        {
          "id": "MDRW2100006439.1.1",
          "form": "요즘 사람들 투자많이하죠",
          "original_form": "요즘 사람들 투자많이하죠",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006439.1.2",
          "form": "그니까요 제 주변도 해요",
          "original_form": "그니까요 제 주변도 해요",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006439.1.3",
          "form": "뭐 하는거 있어요?",
          "original_form": "뭐 하는거 있어요?",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006439.1.4",
          "form": "아뇨 그래도 공부중이죠",
          "original_form": "아뇨 그래도 공부중이죠",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006439.1.5",
          "form": "저는 주식 공부중임",
          "original_form": "저는 주식 공부중임",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006439.1.6",
          "form": "저는 비트코인",
          "original_form": "저는 비트코인",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006439.1.7",
          "form": "소식많이들려와요",
          "original_form": "소식많이들려와요",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006439.1.8",
          "form": "위험하다고요?",
          "original_form": "위험하다고요?",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006439.1.9",
          "form": "네 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006439.1.10",
          "form": "실제로도 그런가요?",
          "original_form": "실제로도 그런가요?",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006439.1.11",
          "form": "공부중이지만",
          "original_form": "공부중이지만",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006439.1.12",
          "form": "저보단 잘알잖아요",
          "original_form": "저보단 잘알잖아요",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006439.1.13",
          "form": "비교적 위험해요",
          "original_form": "비교적 위험해요",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006439.1.14",
          "form": "등락이 커서",
          "original_form": "등락이 커서",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006439.1.15",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006439.1.16",
          "form": "주식은 좀 정해진게 있으니까",
          "original_form": "주식은 좀 정해진게 있으니까",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006439.1.17",
          "form": "덜하겠죠",
          "original_form": "덜하겠죠",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006439.1.18",
          "form": "그럼편이예요",
          "original_form": "그럼편이예요",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006439.1.19",
          "form": "삼전은 안망할꺼구",
          "original_form": "삼전은 안망할꺼구",
          "speaker_id": "1",
          "time": "20210908 00:15"
        }
      ]
