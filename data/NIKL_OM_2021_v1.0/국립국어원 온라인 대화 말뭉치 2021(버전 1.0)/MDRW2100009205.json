      "utterance": [
        {
          "id": "MDRW2100009205.1.1",
          "form": "티비 보세요?",
          "original_form": "티비 보세요?",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009205.1.2",
          "form": "미우새 보고있어요 ㅋㅋ",
          "original_form": "미우새 보고있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009205.1.3",
          "form": "아 재밌어요?",
          "original_form": "아 재밌어요?",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009205.1.4",
          "form": "원래 뭉쳐야찬다 보다가",
          "original_form": "원래 뭉쳐야찬다 보다가",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009205.1.5",
          "form": "끝나서 10시부터 미우새 ㅎㅎ",
          "original_form": "끝나서 10시부터 미우새 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009205.1.6",
          "form": "네 재밌어요",
          "original_form": "네 재밌어요",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009205.1.7",
          "form": "아 저도 아까 잠깐 뭉쳐야찬다 봤어오",
          "original_form": "아 저도 아까 잠깐 뭉쳐야찬다 봤어오",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009205.1.8",
          "form": "아 보셨구나 ㅋㅋ",
          "original_form": "아 보셨구나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009205.1.9",
          "form": "그냥저냥 볼만하더라구룡",
          "original_form": "그냥저냥 볼만하더라구룡",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009205.1.10",
          "form": "네 근데 몇주째 똑같은 내용이라",
          "original_form": "네 근데 몇주째 똑같은 내용이라",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009205.1.11",
          "form": "전 중간에 껐어오",
          "original_form": "전 중간에 껐어오",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009205.1.12",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009205.1.13",
          "form": "저도 좀 지루해서",
          "original_form": "저도 좀 지루해서",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009205.1.14",
          "form": "중간에 게임하면서 봤어요 ㅋㅋ",
          "original_form": "중간에 게임하면서 봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009205.1.15",
          "form": "네 저는 그냥 껐어요 ㅎㅎ",
          "original_form": "네 저는 그냥 껐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009205.1.16",
          "form": "맞아요 ㅋㅋ 저도 그냥 오디오 안 비게만",
          "original_form": "맞아요 ㅋㅋ 저도 그냥 오디오 안 비게만",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009205.1.17",
          "form": "미우새가 더 재밌긴 해요 ㅋㅋㅋ",
          "original_form": "미우새가 더 재밌긴 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009205.1.18",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009205.1.19",
          "form": "저도 봐봐야겠어요",
          "original_form": "저도 봐봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009205.1.20",
          "form": "네 근데 임원희편은 별로 재미없어요..",
          "original_form": "네 근데 임원희편은 별로 재미없어요..",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009205.1.21",
          "form": "저는 김희철 편이 그나마 낫더라구요",
          "original_form": "저는 김희철 편이 그나마 낫더라구요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009205.1.22",
          "form": "네 맞아요 슈쥬 나올때!! ㅋㅋㅋ",
          "original_form": "네 맞아요 슈쥬 나올때!! ㅋㅋㅋ",
