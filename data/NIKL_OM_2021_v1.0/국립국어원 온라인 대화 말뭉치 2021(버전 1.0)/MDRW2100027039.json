      "utterance": [
        {
          "id": "MDRW2100027039.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210902 07:53"
        },
        {
          "id": "MDRW2100027039.1.2",
          "form": "살기 위해",
          "original_form": "살기 위해",
          "speaker_id": "2",
          "time": "20210902 07:56"
        },
        {
          "id": "MDRW2100027039.1.3",
          "form": "살기 위해서 하고 있어요",
          "original_form": "살기 위해서 하고 있어요",
          "speaker_id": "2",
          "time": "20210902 07:56"
        },
        {
          "id": "MDRW2100027039.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100027039.1.5",
          "form": "저는 혼자서 다 하고 있어요",
          "original_form": "저는 혼자서 다 하고 있어요",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027039.1.6",
          "form": "어떤 청소를 제일 좋아?하시나요?",
          "original_form": "어떤 청소를 제일 좋아?하시나요?",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027039.1.7",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100027039.1.8",
          "form": "그쵸ㅠㅠ 모든 청소가 편한 자세로 못하지만\n화장실 청소는 더 그런 걱 같아요",
          "original_form": "그쵸ㅠㅠ 모든 청소가 편한 자세로 못하지만\n화장실 청소는 더 그런 걱 같아요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027039.1.9",
          "form": "같아요",
          "original_form": "같아요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027039.1.10",
          "form": "저도 화장실 청소랑 음식물쓰레기 버리는 게 제일 싫더라구요",
          "original_form": "저도 화장실 청소랑 음식물쓰레기 버리는 게 제일 싫더라구요",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027039.1.11",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210902 17:01"
        },
        {
          "id": "MDRW2100027039.1.12",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100027039.1.13",
          "form": "맞아요. 룸메이트님이 그럼 요리나 다른 일을 맡으시나요?!",
          "original_form": "맞아요. 룸메이트님이 그럼 요리나 다른 일을 맡으시나요?!",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027039.1.14",
          "form": "요리를못해서요",
          "original_form": "요리를못해서요",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100027039.1.15",
          "form": "룸메이트님이 요리를 잘하시나봐요",
          "original_form": "룸메이트님이 요리를 잘하시나봐요",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100027039.1.16",
          "form": "저보다는 ㅋㅋ",
          "original_form": "저보다는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100027039.1.17",
          "form": "저도 요리를 잘 못하고 싫어해서",
          "original_form": "저도 요리를 잘 못하고 싫어해서",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100027039.1.18",
          "form": "같이 사는 사람이 요리를 하면",
          "original_form": "같이 사는 사람이 요리를 하면",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100027039.1.19",
          "form": "설거지를 즐겁게 하는 편입니다ㅋㅋ",
          "original_form": "설거지를 즐겁게 하는 편입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100027039.1.20",
          "form": "그게 정답이조",
          "original_form": "그게 정답이조",
          "speaker_id": "1",
