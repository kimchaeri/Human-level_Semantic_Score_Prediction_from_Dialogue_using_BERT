      "utterance": [
        {
          "id": "MDRW2100025852.1.1",
          "form": "jk가 이제 미래이야기한다",
          "original_form": "jk가 이제 미래이야기한다",
          "speaker_id": "1",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100025852.1.2",
          "form": "헐 님 결혼각이야?",
          "original_form": "헐 님 결혼각이야?",
          "speaker_id": "2",
          "time": "20210902 13:02"
        },
        {
          "id": "MDRW2100025852.1.3",
          "form": "만나보니꺼 자기어떠녀고해서",
          "original_form": "만나보니꺼 자기어떠녀고해서",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100025852.1.4",
          "form": "재미있고 좋더했더니",
          "original_form": "재미있고 좋더했더니",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100025852.1.5",
          "form": "그랬더니 머래",
          "original_form": "그랬더니 머래",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100025852.1.6",
          "form": "프로포즈각이야?",
          "original_form": "프로포즈각이야?",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100025852.1.7",
          "form": "그게 다냐 그러더라 그래서 너는 어떠냐고물었는데",
          "original_form": "그게 다냐 그러더라 그래서 너는 어떠냐고물었는데",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100025852.1.8",
          "form": "오그런데요.왜말을끊어여",
          "original_form": "오그런데요.왜말을끊어여",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100025852.1.9",
          "form": "아직 백퍼 확신은 아니지만 나랑 같이 잘 살 수 있겠다고 생각이든대",
          "original_form": "아직 백퍼 확신은 아니지만 나랑 같이 잘 살 수 있겠다고 생각이든대",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100025852.1.10",
          "form": "근데 이제 남자 말 안믿고요^^",
          "original_form": "근데 이제 남자 말 안믿고요^^",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100025852.1.11",
          "form": "ㅋㅋㅋㅋ마자 말로 말고 딱봐도 알걸 그러면...",
          "original_form": "ㅋㅋㅋㅋ마자 말로 말고 딱봐도 알걸 그러면...",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100025852.1.12",
          "form": "강한확신이어도 힘드러",
          "original_form": "강한확신이어도 힘드러",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100025852.1.13",
          "form": "맞앜ㅋㅋㅋㅋㅋ",
          "original_form": "맞앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100025852.1.14",
          "form": "저는 진짜 해도 33이하로는 할 생각이없어",
          "original_form": "저는 진짜 해도 33이하로는 할 생각이없어",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100025852.1.15",
          "form": "한 35쯤좋겠다",
          "original_form": "한 35쯤좋겠다",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100025852.1.16",
