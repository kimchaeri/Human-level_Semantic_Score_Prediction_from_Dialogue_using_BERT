      "utterance": [
        {
          "id": "MDRW2100009502.1.1",
          "form": "결혼 하지말걸",
          "original_form": "결혼 하지말걸",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009502.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 빵터졌네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 빵터졌네",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009502.1.3",
          "form": "아님 고아랑 결혼할걸",
          "original_form": "아님 고아랑 결혼할걸",
          "speaker_id": "2",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009502.1.4",
          "form": "ㅋㅋㅋ극단적이다 너 시댁도 멀잖아",
          "original_form": "ㅋㅋㅋ극단적이다 너 시댁도 멀잖아",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100009502.1.5",
          "form": "저번학교 실뭔쌤이 40대 미혼이었거든",
          "original_form": "저번학교 실뭔쌤이 40대 미혼이었거든",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009502.1.6",
          "form": "오 부러운 삶",
          "original_form": "오 부러운 삶",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009502.1.7",
          "form": "나랑 교무쌤이 결혼 조건표 작성해줬어",
          "original_form": "나랑 교무쌤이 결혼 조건표 작성해줬어",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009502.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 그게 뭔데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 그게 뭔데",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009502.1.9",
          "form": "연하 건강 고아 고소득",
          "original_form": "연하 건강 고아 고소득",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009502.1.10",
          "form": "크으 좋다",
          "original_form": "크으 좋다",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009502.1.11",
          "form": "나이 먹을 수록 연하가 좋지",
          "original_form": "나이 먹을 수록 연하가 좋지",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009502.1.12",
          "form": "이조건을 모두 충족하지않으면 결혼하면 무조건 여자가 손해",
          "original_form": "이조건을 모두 충족하지않으면 결혼하면 무조건 여자가 손해",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009502.1.13",
          "form": "아냐 다 충족해도 여자 손해야",
          "original_form": "아냐 다 충족해도 여자 손해야",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009502.1.14",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009502.1.15",
          "form": "역시 똘똘이",
          "original_form": "역시 똘똘이",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009502.1.16",
          "form": "고등학교 단톡방에서 얘기나왔는데",
          "original_form": "고등학교 단톡방에서 얘기나왔는데",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009502.1.17",
          "form": "남편은 다 논문 쓴다고 애 안보고",
          "original_form": "남편은 다 논문 쓴다고 애 안보고",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100009502.1.18",
          "form": "자기도 석박사 하고 싶은데 애보느라 못한대",
          "original_form": "자기도 석박사 하고 싶은데 애보느라 못한대",
          "speaker_id": "1",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009502.1.19",
          "form": "아오 그니까",
          "original_form": "아오 그니까",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009502.1.20",
          "form": "나도 학교에서 빌빌대다가 승진도 동기중에 라스트 4에서 대기중",
          "original_form": "나도 학교에서 빌빌대다가 승진도 동기중에 라스트 4에서 대기중",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100009502.1.21",
          "form": "짜증나 이거 진짜 나이먹고 애 낳기 전엔 실감 못했다",
          "original_form": "짜증나 이거 진짜 나이먹고 애 낳기 전엔 실감 못했다",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009502.1.22",
          "form": "젊은 미혼 여자일때는 에이 옛날 얘기지 했어 바보같이",
          "original_form": "젊은 미혼 여자일때는 에이 옛날 얘기지 했어 바보같이",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009502.1.23",
          "form": "하여튼 난 회식이나 야근하려면 오빠의 일정을 먼저 물어보고 동의를 받아야되는데 오빠는 통보야",
          "original_form": "하여튼 난 회식이나 야근하려면 오빠의 일정을 먼저 물어보고 동의를 받아야되는데 오빠는 통보야",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009502.1.24",
          "form": "내가 애데리러 가는게 디폴트거근",
          "original_form": "내가 애데리러 가는게 디폴트거근",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009502.1.25",
          "form": "내 미랜가...",
          "original_form": "내 미랜가...",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009502.1.26",
          "form": "노는 것도 아니고 회식이나 야근인데",
          "original_form": "노는 것도 아니고 회식이나 야근인데",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009502.1.27",
          "form": "심지어 나는 학교라서 4시나 4시반에 퇴근하잖아",
          "original_form": "심지어 나는 학교라서 4시나 4시반에 퇴근하잖아",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009502.1.28",
          "form": "그니까 완전 내가 육아 전담이여",
          "original_form": "그니까 완전 내가 육아 전담이여",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009502.1.29",
          "form": "등하원 100프로 책임제",
          "original_form": "등하원 100프로 책임제",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009502.1.30",
          "form": "여자직업으로 선생 좋다는게 다 굴려먹으려고 하는 말이었어",
          "original_form": "여자직업으로 선생 좋다는게 다 굴려먹으려고 하는 말이었어",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009502.1.31",
          "form": "요정도 억울한거는 그래도 오빠가 돈 잘모아와서 한번에 몇천씩 갖다주면 싹 풀리거든?",
          "original_form": "요정도 억울한거는 그래도 오빠가 돈 잘모아와서 한번에 몇천씩 갖다주면 싹 풀리거든?",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009502.1.32",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009502.1.33",
          "form": "근데 시댁한번 갔다오면 내가 미쳐",
          "original_form": "근데 시댁한번 갔다오면 내가 미쳐",
