      "utterance": [
        {
          "id": "MDRW2100037721.1.1",
          "form": "다른 음식은 또 뭐 좋아하세요",
          "original_form": "다른 음식은 또 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100037721.1.2",
          "form": "음 .. 저 대략 다 좋아하는데..",
          "original_form": "음 .. 저 대략 다 좋아하는데..",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037721.1.3",
          "form": "과일 좋아하세요?",
          "original_form": "과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037721.1.4",
          "form": "넹 ㅎㅎ 자주 사먹어용",
          "original_form": "넹 ㅎㅎ 자주 사먹어용",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037721.1.5",
          "form": "최애 과일이 뭔가요????",
          "original_form": "최애 과일이 뭔가요????",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037721.1.6",
          "form": "메론요 님은?",
          "original_form": "메론요 님은?",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037721.1.7",
          "form": "메론 좋아요",
          "original_form": "메론 좋아요",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037721.1.8",
          "form": "전 귤이요!!!!",
          "original_form": "전 귤이요!!!!",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037721.1.9",
          "form": "오 이제 곧 귤 엄청먹는 시기잖아용 ㅋㅋ",
          "original_form": "오 이제 곧 귤 엄청먹는 시기잖아용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037721.1.10",
          "form": "네 ㅠ 너무 설레요",
          "original_form": "네 ㅠ 너무 설레요",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100037721.1.11",
          "form": "겨울에는 그게 좋죠",
          "original_form": "겨울에는 그게 좋죠",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100037721.1.12",
          "form": "오늘은 인터넷으루 사과시켯어용",
          "original_form": "오늘은 인터넷으루 사과시켯어용",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037721.1.13",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037721.1.14",
          "form": "맛있겠오요",
          "original_form": "맛있겠오요",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037721.1.15",
          "form": "어디서 시켰나요",
          "original_form": "어디서 시켰나요",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037721.1.16",
          "form": "마켓컬리에서 다른거시키면서 같이샀오용 ㅎㅎ",
          "original_form": "마켓컬리에서 다른거시키면서 같이샀오용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037721.1.17",
          "form": "오 거기꺼 드샤보셨어요??",
          "original_form": "오 거기꺼 드샤보셨어요??",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037721.1.18",
          "form": "마켓컬리 물건 괜찮아용 ~!",
          "original_form": "마켓컬리 물건 괜찮아용 ~!",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100037721.1.19",
          "form": "귤 철 되면",
          "original_form": "귤 철 되면",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100037721.1.20",
          "form": "마켓컬리에서",
          "original_form": "마켓컬리에서",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100037721.1.21",
          "form": "시켜봐야겠어요 ㅎ",
          "original_form": "시켜봐야겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100037721.1.22",
