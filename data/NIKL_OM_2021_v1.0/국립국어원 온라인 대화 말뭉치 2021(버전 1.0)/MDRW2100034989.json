      "utterance": [
        {
          "id": "MDRW2100034989.1.1",
          "form": "디즈니 영화 좋아하시죠 !",
          "original_form": "디즈니 영화 좋아하시죠 !",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100034989.1.2",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034989.1.3",
          "form": "개봉하면 꼭꼭 챙겨 보려고 노력해요 ㅎㅎ",
          "original_form": "개봉하면 꼭꼭 챙겨 보려고 노력해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034989.1.4",
          "form": "칩 데일 말고도 디즈니 좋아하세요?",
          "original_form": "칩 데일 말고도 디즈니 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034989.1.5",
          "form": "네 좋아해요 ㅎㅎ !!!",
          "original_form": "네 좋아해요 ㅎㅎ !!!",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034989.1.6",
          "form": "저도 거의 다 챙겨보려고 해요 ㅎㅎㅎ",
          "original_form": "저도 거의 다 챙겨보려고 해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100034989.1.7",
          "form": "최근에 개봉한 루카는 못 봤는데 루카 되게 재밌었다고 하더라구요 ㅎㅎ",
          "original_form": "최근에 개봉한 루카는 못 봤는데 루카 되게 재밌었다고 하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100034989.1.8",
          "form": "혹시 보셨나요?",
          "original_form": "혹시 보셨나요?",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100034989.1.9",
          "form": "봤어요 !",
          "original_form": "봤어요 !",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034989.1.10",
          "form": "재밌어요 ㅎㅎㅎ !!!",
          "original_form": "재밌어요 ㅎㅎㅎ !!!",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034989.1.11",
          "form": "어떻게든 시간 내볼껄 그랬어요ㅠㅠ",
          "original_form": "어떻게든 시간 내볼껄 그랬어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034989.1.12",
          "form": "마키아밸리 고양이 귀엽더라구요 ㅎㅋㅋㅋ",
          "original_form": "마키아밸리 고양이 귀엽더라구요 ㅎㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034989.1.13",
          "form": "아 맞아욬ㅋㄱㅋ 어찌나 의심을 하던지 ㅋㅋㅋ",
          "original_form": "아 맞아욬ㅋㄱㅋ 어찌나 의심을 하던지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100034989.1.14",
          "form": "그 눈이 참 귀여워욬ㅋㄱㅋ",
          "original_form": "그 눈이 참 귀여워욬ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100034989.1.15",
          "form": "영화에 자주 나오나요 마키아밸리 ㅎㅎ",
          "original_form": "영화에 자주 나오나요 마키아밸리 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034989.1.16",
          "form": "코코랑 비슷한 느낌이에요 ㅎㅎ 내용은 다르지만 ! 코코는 보셨죠 ??!",
          "original_form": "코코랑 비슷한 느낌이에요 ㅎㅎ 내용은 다르지만 ! 코코는 보셨죠 ??!",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034989.1.17",
          "form": "음 자주는 아니지만",
          "original_form": "음 자주는 아니지만",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034989.1.18",
          "form": "꽤 나와요 ㅋㅋㅋ",
          "original_form": "꽤 나와요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034989.1.19",
          "form": "마음 놓고 봐야겠네요 ㅋㅋㅋㅋㅋ",
          "original_form": "마음 놓고 봐야겠네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034989.1.20",
          "form": "디플 얼른 서비스해주면 좋겠어요...ㅎㅎ",
          "original_form": "디플 얼른 서비스해주면 좋겠어요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100034989.1.21",
          "form": "11월쯤에 한다던데 맞나요???",
          "original_form": "11월쯤에 한다던데 맞나요???",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034989.1.22",
          "form": "저도 그렇게 알고있어요!",
          "original_form": "저도 그렇게 알고있어요!",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100034989.1.23",
