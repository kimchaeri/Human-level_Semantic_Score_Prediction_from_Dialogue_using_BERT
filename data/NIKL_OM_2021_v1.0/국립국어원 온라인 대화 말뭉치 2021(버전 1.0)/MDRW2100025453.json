      "utterance": [
        {
          "id": "MDRW2100025453.1.1",
          "form": "혹시 결혼하셨나요",
          "original_form": "혹시 결혼하셨나요",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025453.1.2",
          "form": "미혼이신가요",
          "original_form": "미혼이신가요",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025453.1.3",
          "form": "비혼입니다...ㅎㅎㅎ",
          "original_form": "비혼입니다...ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100025453.1.4",
          "form": "저도 비혼이요",
          "original_form": "저도 비혼이요",
          "speaker_id": "1",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100025453.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100025453.1.6",
          "form": "예전부터 쭉  비혼 생각이었머요",
          "original_form": "예전부터 쭉  비혼 생각이었머요",
          "speaker_id": "1",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025453.1.7",
          "form": "남자친구는 있는데",
          "original_form": "남자친구는 있는데",
          "speaker_id": "1",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025453.1.8",
          "form": "비혼이라고 말하고 사귀고 있죠 ㅎㅎ",
          "original_form": "비혼이라고 말하고 사귀고 있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025453.1.9",
          "form": "아항.. 전 코로나이후로 연애도 쉬고 있어요",
          "original_form": "아항.. 전 코로나이후로 연애도 쉬고 있어요",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025453.1.10",
          "form": "사람 만나는 거가 스트레스라",
          "original_form": "사람 만나는 거가 스트레스라",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025453.1.11",
          "form": "그죠 ㅠㅠ",
          "original_form": "그죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025453.1.12",
          "form": "이상형은 어떻게 되시나요",
          "original_form": "이상형은 어떻게 되시나요",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025453.1.13",
          "form": "저도 코로나 심해지고 나선 안만나고 있어요 ㅋㅋㅋ",
          "original_form": "저도 코로나 심해지고 나선 안만나고 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025453.1.14",
          "form": "이상형은 존경할 수 있는 연하남이요...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이상형은 존경할 수 있는 연하남이요...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025453.1.15",
          "form": "앗 저랑 같으시네요!",
          "original_form": "앗 저랑 같으시네요!",
          "speaker_id": "1",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025453.1.16",
          "form": "연하최고",
          "original_form": "연하최고",
          "speaker_id": "1",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025453.1.17",
          "form": "존경심 가질수 있는것두요",
          "original_form": "존경심 가질수 있는것두요",
          "speaker_id": "1",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025453.1.18",
          "form": "맞아요.. 근데 찾기 쉽지 않네요...ㅎㅎ 그래서 비혼됐나??ㅋ",
          "original_form": "맞아요.. 근데 찾기 쉽지 않네요...ㅎㅎ 그래서 비혼됐나??ㅋ",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025453.1.19",
          "form": "세상이 저희를 글케 만드네요 ㅠㅠ",
          "original_form": "세상이 저희를 글케 만드네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025453.1.20",
          "form": "코로나라서 밖에서 만나는게 쉽지 않네요",
          "original_form": "코로나라서 밖에서 만나는게 쉽지 않네요",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025453.1.21",
          "form": "그니까요..",
          "original_form": "그니까요..",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025453.1.22",
          "form": "저 비대면 데이트도 해봤어요..ㅎㅎㅎㅎ",
          "original_form": "저 비대면 데이트도 해봤어요..ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025453.1.23",
          "form": "앗 신기해요",
          "original_form": "앗 신기해요",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025453.1.24",
          "form": "어떻게 하셨어요?",
          "original_form": "어떻게 하셨어요?",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025453.1.25",
          "form": "소개 받아서 화상어플로 대화했죠",
          "original_form": "소개 받아서 화상어플로 대화했죠",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025453.1.26",
          "form": "우와 어떤어플로요.ㅣ",
          "original_form": "우와 어떤어플로요.ㅣ",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025453.1.27",
          "form": "근데 역시.. 연애까지 가긴 힘들더라구요",
          "original_form": "근데 역시.. 연애까지 가긴 힘들더라구요",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025453.1.28",
          "form": "줌이요",
          "original_form": "줌이요",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025453.1.29",
          "form": "영상통화한느 느낌이었어요",
          "original_form": "영상통화한느 느낌이었어요",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025453.1.30",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025453.1.31",
          "form": "영상 소개팅이라니 진짜 신기해요!",
          "original_form": "영상 소개팅이라니 진짜 신기해요!",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025453.1.32",
          "form": "만나는것만 못하겠지만 그래도 특별한 경험이네요!",
          "original_form": "만나는것만 못하겠지만 그래도 특별한 경험이네요!",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025453.1.33",
          "form": "네.. 이시국이라 가능한가봐요...ㅎㅎㅎ",
          "original_form": "네.. 이시국이라 가능한가봐요...ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:56"
        }
      ]
