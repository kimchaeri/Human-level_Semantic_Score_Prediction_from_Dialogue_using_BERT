      "utterance": [
        {
          "id": "MDRW2100023162.1.1",
          "form": "ㅇㅇ.....아깝다",
          "original_form": "ㅇㅇ.....아깝다",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.2",
          "form": "아니 근데 대체 상할 일이 뭐가 있다도",
          "original_form": "아니 근데 대체 상할 일이 뭐가 있다도",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.3",
          "form": "냉장고 안에서 곰창이가 피지...",
          "original_form": "냉장고 안에서 곰창이가 피지...",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.4",
          "form": "옮기는 와중에",
          "original_form": "옮기는 와중에",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.5",
          "form": "좀 뜨거웠나?",
          "original_form": "좀 뜨거웠나?",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.6",
          "form": "나름 염장한 보관식품 아니었냐고...",
          "original_form": "나름 염장한 보관식품 아니었냐고...",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.7",
          "form": "날이 좀 덥긴 했는데...",
          "original_form": "날이 좀 덥긴 했는데...",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.8",
          "form": "가방 안에서 안꺼냈는데...",
          "original_form": "가방 안에서 안꺼냈는데...",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.9",
          "form": "날 더우면 진짜",
          "original_form": "날 더우면 진짜",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.10",
          "form": "순식간에 상하긴 하더라...",
          "original_form": "순식간에 상하긴 하더라...",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.11",
          "form": "아까운 내 하몽...",
          "original_form": "아까운 내 하몽...",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023162.1.12",
          "form": "어쩔수업지.....",
          "original_form": "어쩔수업지.....",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023162.1.13",
          "form": "근데 맛이 좀 실망스럽긴 했음",
          "original_form": "근데 맛이 좀 실망스럽긴 했음",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023162.1.14",
          "form": "베이컨이나 육포느낌일줄 알았는데",
          "original_form": "베이컨이나 육포느낌일줄 알았는데",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023162.1.15",
          "form": "그냥 햄만 먹었어?",
          "original_form": "그냥 햄만 먹었어?",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023162.1.16",
          "form": "하몽은 샌드위치겉은거에",
          "original_form": "하몽은 샌드위치겉은거에",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023162.1.17",
          "form": "같이 먹는다고 하긴 하더라",
          "original_form": "같이 먹는다고 하긴 하더라",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023162.1.18",
          "form": "난 크래커에 얹어먹으려고 아이비 사갔었어",
          "original_form": "난 크래커에 얹어먹으려고 아이비 사갔었어",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023162.1.19",
          "form": "야채나 후추같은거 뿌리면 더 나았을까?",
          "original_form": "야채나 후추같은거 뿌리면 더 나았을까?",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023162.1.20",
          "form": "무화과 먹어?",
          "original_form": "무화과 먹어?",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100023162.1.21",
          "form": "나?",
          "original_form": "나?",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023162.1.22",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023162.1.23",
          "form": "있으면 먹지",
          "original_form": "있으면 먹지",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023162.1.24",
          "form": "영암이 무화과 재배지라",
          "original_form": "영암이 무화과 재배지라",
          "speaker_id": "2",
          "time": "20210829 22:24"
        }
      ]
