      "utterance": [
        {
          "id": "MDRW2100018242.1.1",
          "form": "그래도 여름 자나서 다행이다",
          "original_form": "그래도 여름 자나서 다행이다",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018242.1.2",
          "form": "지나서",
          "original_form": "지나서",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018242.1.3",
          "form": "그러게 말야 걱정많이 했는데",
          "original_form": "그러게 말야 걱정많이 했는데",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018242.1.4",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018242.1.5",
          "form": "올해가 특별히 많이 덥지는 않은 해였나?",
          "original_form": "올해가 특별히 많이 덥지는 않은 해였나?",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018242.1.6",
          "form": "어 그런 것 같아",
          "original_form": "어 그런 것 같아",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018242.1.7",
          "form": "그래도 꽤 덥기는 했는데",
          "original_form": "그래도 꽤 덥기는 했는데",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018242.1.8",
          "form": "그런데 잘 안나가서 그런거 아닌가?",
          "original_form": "그런데 잘 안나가서 그런거 아닌가?",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018242.1.9",
          "form": "그것도 크지",
          "original_form": "그것도 크지",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018242.1.10",
          "form": "내년이 걱정이네",
          "original_form": "내년이 걱정이네",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018242.1.11",
          "form": "벌써 내년 걱정돼?",
          "original_form": "벌써 내년 걱정돼?",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018242.1.12",
          "form": "응 그렇지",
          "original_form": "응 그렇지",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018242.1.13",
          "form": "난 겨울이 걱정돼ㅋㅋㅋㅋ",
          "original_form": "난 겨울이 걱정돼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018242.1.14",
          "form": "ㅋㅋㅋㅋ 그래?",
          "original_form": "ㅋㅋㅋㅋ 그래?",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018242.1.15",
          "form": "응 난 추위를 더 많이 타",
          "original_form": "응 난 추위를 더 많이 타",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018242.1.16",
          "form": "오늘 날씨 어땠어?",
          "original_form": "오늘 날씨 어땠어?",
