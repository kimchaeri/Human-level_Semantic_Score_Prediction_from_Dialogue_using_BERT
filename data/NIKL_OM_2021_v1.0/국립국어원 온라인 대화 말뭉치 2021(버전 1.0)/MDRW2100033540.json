      "utterance": [
        {
          "id": "MDRW2100033540.1.1",
          "form": "최근에 사신것있나요?",
          "original_form": "최근에 사신것있나요?",
          "speaker_id": "1",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100033540.1.2",
          "form": "바디워시 구매했어요",
          "original_form": "바디워시 구매했어요",
          "speaker_id": "2",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100033540.1.3",
          "form": "저는 운동화샀어요ㅋㅋㅋ",
          "original_form": "저는 운동화샀어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100033540.1.4",
          "form": "운동화 어디꺼 사셨나요",
          "original_form": "운동화 어디꺼 사셨나요",
          "speaker_id": "2",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100033540.1.5",
          "form": "나이키꺼요ㅋㅋㅋ",
          "original_form": "나이키꺼요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100033540.1.6",
          "form": "오즈위고\r\n아시나요?",
          "original_form": "오즈위고\r\n아시나요?",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100033540.1.7",
          "form": "처음 들어보는데 찾아보니 이쁘네요",
          "original_form": "처음 들어보는데 찾아보니 이쁘네요",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100033540.1.8",
          "form": "편해보여요",
          "original_form": "편해보여요",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100033540.1.9",
          "form": "ㅋㅋㅋ네네 편해요!",
          "original_form": "ㅋㅋㅋ네네 편해요!",
          "speaker_id": "1",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100033540.1.10",
          "form": "구매하시고싶으신거있나요?",
          "original_form": "구매하시고싶으신거있나요?",
          "speaker_id": "1",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100033540.1.11",
          "form": "저.. 선풍기가 고장나서",
          "original_form": "저.. 선풍기가 고장나서",
          "speaker_id": "2",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100033540.1.12",
          "form": "선풍기를 알아보고있어요",
          "original_form": "선풍기를 알아보고있어요",
          "speaker_id": "2",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100033540.1.13",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100033540.1.14",
          "form": "여름이 끝나가지않나요?",
          "original_form": "여름이 끝나가지않나요?",
          "speaker_id": "1",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100033540.1.15",
          "form": "그렇긴 한데 아직 좀 더워서..ㅋㅋㅋ",
          "original_form": "그렇긴 한데 아직 좀 더워서..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100033540.1.16",
          "form": "하필 끝나 갈 이 시기에 고장이 나더라구요",
          "original_form": "하필 끝나 갈 이 시기에 고장이 나더라구요",
          "speaker_id": "2",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100033540.1.17",
          "form": "ㅠㅠ지금사긴 좀 아깝겠어요",
          "original_form": "ㅠㅠ지금사긴 좀 아깝겠어요",
          "speaker_id": "1",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100033540.1.18",
          "form": "장마인지 부쩍추워졌더라구요",
          "original_form": "장마인지 부쩍추워졌더라구요",
