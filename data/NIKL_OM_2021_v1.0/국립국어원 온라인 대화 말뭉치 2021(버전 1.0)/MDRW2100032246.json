        {
          "id": "MDRW2100032246.1.1",
          "form": "어치ㅡ피안할청소지만ㅋㅋㅋ",
          "original_form": "어치ㅡ피안할청소지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032246.1.2",
          "form": "저는원래안하지만요^^",
          "original_form": "저는원래안하지만요^^",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032246.1.3",
          "form": "그거있자나요매직블럭",
          "original_form": "그거있자나요매직블럭",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032246.1.4",
          "form": "그걸로살살살닦아야한대요",
          "original_form": "그걸로살살살닦아야한대요",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032246.1.5",
          "form": "ㅋㅋ솔로팍팍뭉대야지",
          "original_form": "ㅋㅋ솔로팍팍뭉대야지",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032246.1.6",
          "form": "청소한기분인데...ㅋ",
          "original_form": "청소한기분인데...ㅋ",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100032246.1.7",
          "form": "청라한양사시는분이강추하셨는데",
          "original_form": "청라한양사시는분이강추하셨는데",
          "speaker_id": "2",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032246.1.8",
          "form": "그분은 청소하면 기분이상쾌해진대요ㅋㅋ",
          "original_form": "그분은 청소하면 기분이상쾌해진대요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032246.1.9",
          "form": "태생이다르니 난하지말아야짘ㅋ",
          "original_form": "태생이다르니 난하지말아야짘ㅋ",
          "speaker_id": "2",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032246.1.10",
          "form": "ㅋㅋㅋ저랑다른분^^",
          "original_form": "ㅋㅋㅋ저랑다른분^^",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032246.1.11",
          "form": "성향이다르네요ㅋㅋ",
          "original_form": "성향이다르네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100032246.1.12",
