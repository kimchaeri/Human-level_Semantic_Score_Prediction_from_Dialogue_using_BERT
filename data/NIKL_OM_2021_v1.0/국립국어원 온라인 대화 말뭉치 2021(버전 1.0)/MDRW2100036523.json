      "utterance": [
        {
          "id": "MDRW2100036523.1.1",
          "form": "500년 엄청 오래되었네",
          "original_form": "500년 엄청 오래되었네",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036523.1.2",
          "form": "엉 스페인의 최고 벼룩시장 라스트로라네",
          "original_form": "엉 스페인의 최고 벼룩시장 라스트로라네",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036523.1.3",
          "form": "가면 볼게 엄청 많겠다",
          "original_form": "가면 볼게 엄청 많겠다",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036523.1.4",
          "form": "엉 우리도 거기서 산 물건이 막 진품이고 ㅋㅋ하느거 아니가",
          "original_form": "엉 우리도 거기서 산 물건이 막 진품이고 ㅋㅋ하느거 아니가",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036523.1.5",
          "form": "ㅋㅋ꿈은 꿀수 있으니",
          "original_form": "ㅋㅋ꿈은 꿀수 있으니",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036523.1.6",
          "form": "매주 일요일하고 공휴일 아침 9시부터 3시까지",
          "original_form": "매주 일요일하고 공휴일 아침 9시부터 3시까지",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036523.1.7",
          "form": "일찍 문닫네",
          "original_form": "일찍 문닫네",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036523.1.8",
          "form": "외국이라서 그럴듯",
          "original_form": "외국이라서 그럴듯",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036523.1.9",
          "form": "그렇겠다",
          "original_form": "그렇겠다",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036523.1.10",
          "form": "거기다가 스페인이 낮잠을 자는 시간이 있잖아",
          "original_form": "거기다가 스페인이 낮잠을 자는 시간이 있잖아",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036523.1.11",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036523.1.12",
          "form": "응 11시 이후에는 사람이 많아서 일찍 가는게 좋데",
          "original_form": "응 11시 이후에는 사람이 많아서 일찍 가는게 좋데",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036523.1.13",
          "form": "사람많으면구경하기도 힝들지",
          "original_form": "사람많으면구경하기도 힝들지",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036523.1.14",
          "form": "엉 그리고 사람이 엄청 많아서 소매치기도 조심",
          "original_form": "엉 그리고 사람이 엄청 많아서 소매치기도 조심",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036523.1.15",
          "form": "어디가든 소매치기는 조심",
          "original_form": "어디가든 소매치기는 조심",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036523.1.16",
          "form": "엉 교통편은 좋다 메트로 5호선타고 역에서 3분",
          "original_form": "엉 교통편은 좋다 메트로 5호선타고 역에서 3분",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036523.1.17",
          "form": "접근하기도 싶네",
          "original_form": "접근하기도 싶네",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036523.1.18",
          "form": "마요르 광장 동쪽에도 시장이 잇어",
          "original_form": "마요르 광장 동쪽에도 시장이 잇어",
          "speaker_id": "1",
          "time": "20210831 00:12"
        }
