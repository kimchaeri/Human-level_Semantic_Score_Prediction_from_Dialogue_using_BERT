      "utterance": [
        {
          "id": "MDRW2100039078.1.1",
          "form": "오늘날씨짱이에요",
          "original_form": "오늘날씨짱이에요",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100039078.1.2",
          "form": "그러게요 날씨가 진짜 뜨겁네요",
          "original_form": "그러게요 날씨가 진짜 뜨겁네요",
          "speaker_id": "2",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100039078.1.3",
          "form": "오랜만에 해가 엄청 쨍쨍해요 ㅋㅋㅋ",
          "original_form": "오랜만에 해가 엄청 쨍쨍해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100039078.1.4",
          "form": "하늘이파래요",
          "original_form": "하늘이파래요",
          "speaker_id": "1",
          "time": "20210903 09:22"
        },
        {
          "id": "MDRW2100039078.1.5",
          "form": "바람도 선선하고 ㅋㅋㅋ",
          "original_form": "바람도 선선하고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:23"
        },
        {
          "id": "MDRW2100039078.1.6",
          "form": "맞아요 ㅋㅋㅋ 해는 뜨거운데 바람이 몇주 전과는 다르네요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 해는 뜨거운데 바람이 몇주 전과는 다르네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:23"
        },
        {
          "id": "MDRW2100039078.1.7",
          "form": "외출하셧나요",
          "original_form": "외출하셧나요",
          "speaker_id": "1",
          "time": "20210903 09:23"
        },
        {
          "id": "MDRW2100039078.1.8",
          "form": "네 잠시 나왔어요 ㅋㅋㅋ",
          "original_form": "네 잠시 나왔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100039078.1.9",
          "form": "아직 걸어다니기는 힘든 날씨 같아요 ㅋㅋㅋ해가 너무 쨍하네요",
          "original_form": "아직 걸어다니기는 힘든 날씨 같아요 ㅋㅋㅋ해가 너무 쨍하네요",
          "speaker_id": "2",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100039078.1.10",
          "form": "ㅋㅋㅋ 전 아직 안이라",
          "original_form": "ㅋㅋㅋ 전 아직 안이라",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100039078.1.11",
          "form": "머입으셧어요",
          "original_form": "머입으셧어요",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100039078.1.12",
          "form": "여름용 니트원피스 입었어요 ㅋㅋㅋ",
          "original_form": "여름용 니트원피스 입었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100039078.1.13",
          "form": "요새 날씨에 뭐 입으세요???",
          "original_form": "요새 날씨에 뭐 입으세요???",
          "speaker_id": "2",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100039078.1.14",
          "form": "아직은 반팔이고 남방 걸쳐요 ㅋㅋ",
          "original_form": "아직은 반팔이고 남방 걸쳐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100039078.1.15",
          "form": "남방 좋네요!! 해 가리기도 좋고 추울 때 보온도 되고요 ㅋㅋㅋ",
          "original_form": "남방 좋네요!! 해 가리기도 좋고 추울 때 보온도 되고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100039078.1.16",
          "form": "마자용 ~ 조아하는 가을 옷차림 잇으세요?",
          "original_form": "마자용 ~ 조아하는 가을 옷차림 잇으세요?",
          "speaker_id": "1",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100039078.1.17",
          "form": "가을옷이면 역시 트렌치 밖에 안떠올라요",
          "original_form": "가을옷이면 역시 트렌치 밖에 안떠올라요",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100039078.1.18",
          "form": "name1님은 좋아하시는 가을패션 있으세요??",
          "original_form": "&name1&님은 좋아하시는 가을패션 있으세요??",
