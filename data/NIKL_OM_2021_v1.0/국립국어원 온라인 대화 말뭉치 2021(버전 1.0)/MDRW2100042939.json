      "utterance": [
        {
          "id": "MDRW2100042939.1.1",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100042939.1.2",
          "form": "저 이제 먹으려고 합니다 name1님은 드셨어요??",
          "original_form": "저 이제 먹으려고 합니다 &name1&님은 드셨어요??",
          "speaker_id": "2",
          "time": "20210907 11:55"
        },
        {
          "id": "MDRW2100042939.1.3",
          "form": "ㅎㅎ저는단호박이랑사과요",
          "original_form": "ㅎㅎ저는단호박이랑사과요",
          "speaker_id": "1",
          "time": "20210907 11:57"
        },
        {
          "id": "MDRW2100042939.1.4",
          "form": "오 건강식이네여!!",
          "original_form": "오 건강식이네여!!",
          "speaker_id": "2",
          "time": "20210907 11:57"
        },
        {
          "id": "MDRW2100042939.1.5",
          "form": "저는 계란간장밥 먹으려고요",
          "original_form": "저는 계란간장밥 먹으려고요",
          "speaker_id": "2",
          "time": "20210907 11:57"
        },
        {
          "id": "MDRW2100042939.1.6",
          "form": "오저그거좋아해요",
          "original_form": "오저그거좋아해요",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042939.1.7",
          "form": "반숙인가요",
          "original_form": "반숙인가요",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042939.1.8",
          "form": "간단하게 되니까 좋아요",
          "original_form": "간단하게 되니까 좋아요",
          "speaker_id": "2",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042939.1.9",
          "form": "네 저는 반숙",
          "original_form": "네 저는 반숙",
          "speaker_id": "2",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042939.1.10",
          "form": "저도반숙파에오ㅡ",
          "original_form": "저도반숙파에오ㅡ",
          "speaker_id": "1",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042939.1.11",
          "form": "커피도좋아하시나요",
          "original_form": "커피도좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042939.1.12",
          "form": "요즘엔 잘 못마시는데 아메리카노도 좋아하고 라떼도 좋아해요!!",
          "original_form": "요즘엔 잘 못마시는데 아메리카노도 좋아하고 라떼도 좋아해요!!",
          "speaker_id": "2",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100042939.1.13",
          "form": "요새줄이시나요",
          "original_form": "요새줄이시나요",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042939.1.14",
          "form": "저도줄여야하는데..",
          "original_form": "저도줄여야하는데..",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042939.1.15",
          "form": "마시면 잠을 잘 못자서요",
          "original_form": "마시면 잠을 잘 못자서요",
          "speaker_id": "2",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042939.1.16",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042939.1.17",
          "form": "좋아해욯ㅅㄴ",
          "original_form": "좋아해욯ㅅㄴ",
          "speaker_id": "1",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042939.1.18",
          "form": "디저트는어떠세요",
          "original_form": "디저트는어떠세요",
          "speaker_id": "1",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042939.1.19",
          "form": "종류 가리지 않으시나여?",
          "original_form": "종류 가리지 않으시나여?",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042939.1.20",
          "form": "물론 디저트는 가리지 않습니다 ㅋㅋ",
          "original_form": "물론 디저트는 가리지 않습니다 ㅋㅋ",
