      "utterance": [
        {
          "id": "MDRW2100021365.1.1",
          "form": "저도 여름휴가 안 갔는데",
          "original_form": "저도 여름휴가 안 갔는데",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021365.1.2",
          "form": "다들 가시는건지ㅜㅜ",
          "original_form": "다들 가시는건지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100021365.1.3",
          "form": "제주도도 난리난리 속초 부산도 그렇고",
          "original_form": "제주도도 난리난리 속초 부산도 그렇고",
          "speaker_id": "2",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021365.1.4",
          "form": "여름은 바닷가쪽으로 많이 가시는거 같아요",
          "original_form": "여름은 바닷가쪽으로 많이 가시는거 같아요",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021365.1.5",
          "form": "다 가고싶죠ㅠㅠ",
          "original_form": "다 가고싶죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021365.1.6",
          "form": "전 계곡가서 놀고싶었는ㄷㅔㅠ",
          "original_form": "전 계곡가서 놀고싶었는ㄷㅔㅠ",
          "speaker_id": "2",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021365.1.7",
          "form": "그래서 피서철에는 나가지를 못 하겠어요ㅜㅜ",
          "original_form": "그래서 피서철에는 나가지를 못 하겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100021365.1.8",
          "form": "그쳐 저도 그냥 차에서 구경만하고왓어요ㅠ 바다가서ㅠ",
          "original_form": "그쳐 저도 그냥 차에서 구경만하고왓어요ㅠ 바다가서ㅠ",
          "speaker_id": "2",
          "time": "20210829 03:41"
        },
        {
          "id": "MDRW2100021365.1.9",
          "form": "ㅠㅠ 계절은 어느 계절 좋아하세요?",
          "original_form": "ㅠㅠ 계절은 어느 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100021365.1.10",
          "form": "전 가을겨울이요~~ 더운건 너무싫어요",
          "original_form": "전 가을겨울이요~~ 더운건 너무싫어요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100021365.1.11",
          "form": "저도 더운건 정말 싫어해요ㅠㅠ",
          "original_form": "저도 더운건 정말 싫어해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100021365.1.12",
          "form": "더운건 너무 견디기가 힘들더라구여ㅠㅠ 추운건 참겠는데ㅠㅠ",
          "original_form": "더운건 너무 견디기가 힘들더라구여ㅠㅠ 추운건 참겠는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100021365.1.13",
          "form": "저도 생각이 같아요\n추우면 옷 껴입고 이불 속에 들어가고 하면 나은데, 더울때는 예민하고 짜증도 잘 나고 지치고...ㅠㅠ",
          "original_form": "저도 생각이 같아요\n추우면 옷 껴입고 이불 속에 들어가고 하면 나은데, 더울때는 예민하고 짜증도 잘 나고 지치고...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100021365.1.14",
          "form": "ㅠㅠㅠ예민짜증 너무공감해여ㅠㅠ",
          "original_form": "ㅠㅠㅠ예민짜증 너무공감해여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100021365.1.15",
          "form": "전 그래서 가을을 좋아해요",
          "original_form": "전 그래서 가을을 좋아해요",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100021365.1.16",
          "form": "저듀요ㅠㅠ 짧아서아쉬워여ㅠㅠ",
          "original_form": "저듀요ㅠㅠ 짧아서아쉬워여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100021365.1.17",
          "form": "맞아요ㅜㅜ\n가을이 해가 지날수록 짧아지고 있어요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ\n가을이 해가 지날수록 짧아지고 있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100021365.1.18",
          "form": "늦여름이 길어지고 겨울이 빨라지고ㅠㅠ",
          "original_form": "늦여름이 길어지고 겨울이 빨라지고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100021365.1.19",
          "form": "맞아요ㅠㅠ\n여름만 점점 길어지고 있어요..",
          "original_form": "맞아요ㅠㅠ\n여름만 점점 길어지고 있어요..",
          "speaker_id": "1",
          "time": "20210829 15:17"
        }
      ]
