      "utterance": [
        {
          "id": "MDRW2100041391.1.1",
          "form": "name1님 좋아하는 티비 프로 있으신지용",
          "original_form": "&name1&님 좋아하는 티비 프로 있으신지용",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041391.1.2",
          "form": "저 티비엔 예능 좋아해요",
          "original_form": "저 티비엔 예능 좋아해요",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100041391.1.3",
          "form": "어떤 프로요?",
          "original_form": "어떤 프로요?",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100041391.1.4",
          "form": "유퀴즈랑 나영석피디 예능도 거의 봐요",
          "original_form": "유퀴즈랑 나영석피디 예능도 거의 봐요",
          "speaker_id": "1",
          "time": "20210905 23:30"
        },
        {
          "id": "MDRW2100041391.1.5",
          "form": "지금은 대탈출 보고있어요",
          "original_form": "지금은 대탈출 보고있어요",
          "speaker_id": "1",
          "time": "20210905 23:30"
        },
        {
          "id": "MDRW2100041391.1.6",
          "form": "오 저도 유퀴즈 좋아해요",
          "original_form": "오 저도 유퀴즈 좋아해요",
          "speaker_id": "2",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041391.1.7",
          "form": "시간을 못맞춰서 본방으론 잘 못 보지만 유튜브로 조금씩 챙겨봐요 ㅋㅋ",
          "original_form": "시간을 못맞춰서 본방으론 잘 못 보지만 유튜브로 조금씩 챙겨봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041391.1.8",
          "form": "대탈출 좋아하시는 분들 진짜 많더라구요",
          "original_form": "대탈출 좋아하시는 분들 진짜 많더라구요",
          "speaker_id": "2",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041391.1.9",
          "form": "추리하시는거 좋아하시나요ㅡ",
          "original_form": "추리하시는거 좋아하시나요ㅡ",
          "speaker_id": "2",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041391.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041391.1.11",
          "form": "좋아하는건 아닌데ㅋㅋㅋ 매번 몰입감있게 만들어서 재밌더라구요",
          "original_form": "좋아하는건 아닌데ㅋㅋㅋ 매번 몰입감있게 만들어서 재밌더라구요",
          "speaker_id": "1",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100041391.1.12",
          "form": "오 그러시군요 저 대탈출 한 두편 정도 봤는데 그때마다 어떻게 저렇게 잘하지? 싶어요",
          "original_form": "오 그러시군요 저 대탈출 한 두편 정도 봤는데 그때마다 어떻게 저렇게 잘하지? 싶어요",
          "speaker_id": "2",
          "time": "20210905 23:41"
        },
        {
          "id": "MDRW2100041391.1.13",
          "form": "name1님은 보시면서 무슨 생각하세요?",
          "original_form": "&name1&님은 보시면서 무슨 생각하세요?",
          "speaker_id": "2",
          "time": "20210905 23:41"
        },
        {
          "id": "MDRW2100041391.1.14",
          "form": "전 멤버들 삽질하는게 웃기더라구요ㅋㅋ",
          "original_form": "전 멤버들 삽질하는게 웃기더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:42"
        },
        {
          "id": "MDRW2100041391.1.15",
          "form": "제작진들이 설계해둔 내용은 감탄하고요",
          "original_form": "제작진들이 설계해둔 내용은 감탄하고요",
          "speaker_id": "1",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100041391.1.16",
          "form": "요새 보시는 프로그램 있으세요?",
          "original_form": "요새 보시는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100041391.1.17",
          "form": "아ㅋㅋㅋㅋㅋ 음 저는 슬의생이요! 본방으로는 잘 못보고 넾플로 다음날 봐요",
          "original_form": "아ㅋㅋㅋㅋㅋ 음 저는 슬의생이요! 본방으로는 잘 못보고 넾플로 다음날 봐요",
          "speaker_id": "2",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100041391.1.18",
          "form": "슬의생 재밌죠",
          "original_form": "슬의생 재밌죠",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100041391.1.19",
          "form": "큰 갈등없이도 스토리 재밌는거 같아요",
          "original_form": "큰 갈등없이도 스토리 재밌는거 같아요",
          "speaker_id": "1",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100041391.1.20",
          "form": "네네 맞아요 그리고 무엇보다 제가 모르는 분야를 잘 설명하면서 풀어주니가 그게 재밌어요",
          "original_form": "네네 맞아요 그리고 무엇보다 제가 모르는 분야를 잘 설명하면서 풀어주니가 그게 재밌어요",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100041391.1.21",
          "form": "드라마가 친절해요",
          "original_form": "드라마가 친절해요",
          "speaker_id": "2",
          "time": "20210905 23:55"
        },
        {
          "id": "MDRW2100041391.1.22",
          "form": "맞아요. 병동마다 다양한 사건들 다뤄주는거 재밌어요.",
          "original_form": "맞아요. 병동마다 다양한 사건들 다뤄주는거 재밌어요.",
          "speaker_id": "1",
          "time": "20210906 00:02"
        }
      ]
    }
