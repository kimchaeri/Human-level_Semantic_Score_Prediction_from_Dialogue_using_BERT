      "utterance": [
        {
          "id": "MDRW2100024840.1.1",
          "form": "이건희 컬렉션 가보고 싶어요",
          "original_form": "이건희 컬렉션 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024840.1.2",
          "form": "관심있으세요?",
          "original_form": "관심있으세요?",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024840.1.3",
          "form": "오 저도 경쟁 빡세지않나요",
          "original_form": "오 저도 경쟁 빡세지않나요",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024840.1.4",
          "form": "그렇다고 하더라구요",
          "original_form": "그렇다고 하더라구요",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024840.1.5",
          "form": "ㅜㅜ일찌감치포기...",
          "original_form": "ㅜㅜ일찌감치포기...",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024840.1.6",
          "form": "최근 본영화있으세요?",
          "original_form": "최근 본영화있으세요?",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024840.1.7",
          "form": "앗 아뇨!!ㅠ",
          "original_form": "앗 아뇨!!ㅠ",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024840.1.8",
          "form": "추천해주실거있나요",
          "original_form": "추천해주실거있나요",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024840.1.9",
          "form": "극장에서 본건 분노의질주가 마지막이었어요",
          "original_form": "극장에서 본건 분노의질주가 마지막이었어요",
          "speaker_id": "1",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100024840.1.10",
          "form": "연말되면 친구들과 뮤지컬도 보러 다녔었는데 가서보면 현장감때문에 더 좋긴하더라구요",
          "original_form": "연말되면 친구들과 뮤지컬도 보러 다녔었는데 가서보면 현장감때문에 더 좋긴하더라구요",
          "speaker_id": "1",
          "time": "20210831 19:37"
        },
        {
          "id": "MDRW2100024840.1.11",
          "form": "ㅋㅋㅋ 저도 오래됏어요ㅠㅠ뮤지컬!",
          "original_form": "ㅋㅋㅋ 저도 오래됏어요ㅠㅠ뮤지컬!",
          "speaker_id": "2",
          "time": "20210831 19:39"
        },
        {
          "id": "MDRW2100024840.1.12",
          "form": "추천착품 잇으신가요",
          "original_form": "추천착품 잇으신가요",
          "speaker_id": "2",
          "time": "20210831 19:39"
        },
        {
          "id": "MDRW2100024840.1.13",
          "form": "조승우꺼는 추천해요ㆍ맨오브라반차른 봤거든요",
          "original_form": "조승우꺼는 추천해요ㆍ맨오브라반차른 봤거든요",
          "speaker_id": "1",
          "time": "20210831 19:41"
        },
        {
          "id": "MDRW2100024840.1.14",
          "form": "조승우꺼 티켓팅 안빡센가요!!",
          "original_form": "조승우꺼 티켓팅 안빡센가요!!",
          "speaker_id": "2",
          "time": "20210831 19:46"
        },
        {
          "id": "MDRW2100024840.1.15",
          "form": "앞쪽만 아니면 티켓구하기 어렵지않을거예요",
          "original_form": "앞쪽만 아니면 티켓구하기 어렵지않을거예요",
          "speaker_id": "1",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100024840.1.16",
          "form": "요즘은 더 관람하기 쉬울것같아요",
          "original_form": "요즘은 더 관람하기 쉬울것같아요",
          "speaker_id": "1",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100024840.1.17",
          "form": "코로나때문에 좌석이 줄어서 더 어려울줄 알았는데!!",
          "original_form": "코로나때문에 좌석이 줄어서 더 어려울줄 알았는데!!",
          "speaker_id": "2",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100024840.1.18",
          "form": "추천해주실만한 공연이나 영화있으실까요?",
          "original_form": "추천해주실만한 공연이나 영화있으실까요?",
          "speaker_id": "1",
          "time": "20210831 20:47"
        },
        {
          "id": "MDRW2100024840.1.19",
          "form": "전 케이팝 콘서트 많이다녓어요...관심잇으신가요 ㅋㅋㅋ",
          "original_form": "전 케이팝 콘서트 많이다녓어요...관심잇으신가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024840.1.20",
          "form": "젊은나이는 아니어도 케이팝음악은 좋아해요!!",
          "original_form": "젊은나이는 아니어도 케이팝음악은 좋아해요!!",
          "speaker_id": "1",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100024840.1.21",
          "form": "콘서트는 엄두도 못내죠. ㅋ",
          "original_form": "콘서트는 엄두도 못내죠. ㅋ",
