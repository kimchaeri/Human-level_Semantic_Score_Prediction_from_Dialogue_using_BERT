      "utterance": [
        {
          "id": "MDRW2100006011.1.1",
          "form": "기혼/미혼 어느쪽이신가요",
          "original_form": "기혼/미혼 어느쪽이신가요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100006011.1.2",
          "form": "미혼입니당",
          "original_form": "미혼입니당",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100006011.1.3",
          "form": "결혼하셨나요",
          "original_form": "결혼하셨나요",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100006011.1.4",
          "form": "저는 기혼입니다 연애중이신가요",
          "original_form": "저는 기혼입니다 연애중이신가요",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100006011.1.5",
          "form": "넵넵 와 결혼하셨군요",
          "original_form": "넵넵 와 결혼하셨군요",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100006011.1.6",
          "form": "이상형과 만나신건가요",
          "original_form": "이상형과 만나신건가요",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100006011.1.7",
          "form": "음 비슷합니다...ㅎㅎ",
          "original_form": "음 비슷합니다...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100006011.1.8",
          "form": "와 정말 부럽네요",
          "original_form": "와 정말 부럽네요",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100006011.1.9",
          "form": "이상형에 가까운 배우자라니",
          "original_form": "이상형에 가까운 배우자라니",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100006011.1.10",
          "form": "어떻게 만나셨나요",
          "original_form": "어떻게 만나셨나요",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100006011.1.11",
          "form": "소개팅입니다...\r\nname2님은요?",
          "original_form": "소개팅입니다...\r\n&name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100006011.1.12",
          "form": "저도 소개팅이요",
          "original_form": "저도 소개팅이요",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100006011.1.13",
          "form": "자연스러운만남은 어렵죠",
          "original_form": "자연스러운만남은 어렵죠",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100006011.1.14",
          "form": "소개해준 분이 은인이네요",
          "original_form": "소개해준 분이 은인이네요",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100006011.1.15",
          "form": "ㅎㅎ 네 예상치 못한 소개팅인데 잘 만났었죠",
          "original_form": "ㅎㅎ 네 예상치 못한 소개팅인데 잘 만났었죠",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100006011.1.16",
          "form": "와 첨보는순간 느낌이왔나요?",
          "original_form": "와 첨보는순간 느낌이왔나요?",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100006011.1.17",
          "form": "네 저는 좋더라구요...ㅎㅎ name2님은요??",
          "original_form": "네 저는 좋더라구요...ㅎㅎ &name2&님은요??",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100006011.1.18",
          "form": "저는 그닥 느낌이 온상대는 많이못만나본거같아요",
          "original_form": "저는 그닥 느낌이 온상대는 많이못만나본거같아요",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100006011.1.19",
          "form": "그냥 만나다보면 호감이라던가",
          "original_form": "그냥 만나다보면 호감이라던가",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100006011.1.20",
          "form": "좋은사람이라서 만나게 되는거같아요",
          "original_form": "좋은사람이라서 만나게 되는거같아요",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100006011.1.21",
          "form": "그러시군요 지금 만나시는분은 몇 번 만나고 그런 느낌이 오셨나요",
          "original_form": "그러시군요 지금 만나시는분은 몇 번 만나고 그런 느낌이 오셨나요",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100006011.1.22",
          "form": "음 저는 한 3개월 정도는 만나야",
          "original_form": "음 저는 한 3개월 정도는 만나야",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100006011.1.23",
          "form": "그런 생각이 들어요",
          "original_form": "그런 생각이 들어요",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100006011.1.24",
          "form": "그래서 성미급한분과는 연애가 참 힘들어요",
          "original_form": "그래서 성미급한분과는 연애가 참 힘들어요",
          "speaker_id": "2",
          "time": "20210907 19:11"
        }
      ]
    }
