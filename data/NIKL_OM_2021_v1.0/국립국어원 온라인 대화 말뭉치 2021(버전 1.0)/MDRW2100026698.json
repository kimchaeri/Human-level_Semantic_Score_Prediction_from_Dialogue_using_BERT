      "utterance": [
        {
          "id": "MDRW2100026698.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026698.1.2",
          "form": "여행어디가고싶으세요?",
          "original_form": "여행어디가고싶으세요?",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026698.1.3",
          "form": "저는 예전 홍콩이요ㅠㅠ 난리나기 전 홍콩...",
          "original_form": "저는 예전 홍콩이요ㅠㅠ 난리나기 전 홍콩...",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026698.1.4",
          "form": "코로나 직전에 가려고 했는데 갑자기 난리나서 못갔거든요ㅠㅠ",
          "original_form": "코로나 직전에 가려고 했는데 갑자기 난리나서 못갔거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026698.1.5",
          "form": "ㅜㅜㅜ아 ㅠㅠㅠ 그쵸 홍콩 난리났었죠 ㅠㅠ",
          "original_form": "ㅜㅜㅜ아 ㅠㅠㅠ 그쵸 홍콩 난리났었죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026698.1.6",
          "form": "제친구도 그때 홍콩가서...",
          "original_form": "제친구도 그때 홍콩가서...",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026698.1.7",
          "form": "어이가없었지만 잘놀다 왓더라구요",
          "original_form": "어이가없었지만 잘놀다 왓더라구요",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026698.1.8",
          "form": "그때 외국인은 딱히 안건드렸다는 소리 본 것 같아요ㅠㅠ",
          "original_form": "그때 외국인은 딱히 안건드렸다는 소리 본 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026698.1.9",
          "form": "저는 그래도 무서워서 여행지 옮겼지만요",
          "original_form": "저는 그래도 무서워서 여행지 옮겼지만요",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026698.1.10",
          "form": "그래도 조심하는게 옳죠 ㅠ",
          "original_form": "그래도 조심하는게 옳죠 ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026698.1.11",
          "form": "맞아요ㅠㅠ 이젠 다시 못가니까 조금 아쉽네요",
          "original_form": "맞아요ㅠㅠ 이젠 다시 못가니까 조금 아쉽네요",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026698.1.12",
          "form": "제일 최근에 간 여행이 어디에요?*",
          "original_form": "제일 최근에 간 여행이 어디에요?*",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026698.1.13",
          "form": "경주 갔었어요 ㅎㅎ",
          "original_form": "경주 갔었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026698.1.14",
          "form": "해바라기보러 ㅎㅎ",
          "original_form": "해바라기보러 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026698.1.15",
          "form": "와 좋다 해바라기 많은 곳이 경주에 있어요?",
          "original_form": "와 좋다 해바라기 많은 곳이 경주에 있어요?",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026698.1.16",
          "form": "네 ㅋㅋㅋㅋ 카페가 유명해서",
          "original_form": "네 ㅋㅋㅋㅋ 카페가 유명해서",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026698.1.17",
          "form": "갔었는데",
          "original_form": "갔었는데",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026698.1.18",
          "form": "거긴 해바라기 안녕했고...",
          "original_form": "거긴 해바라기 안녕했고...",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026698.1.19",
          "form": "다른 곳갔었는데",
          "original_form": "다른 곳갔었는데",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026698.1.20",
          "form": "월정교!!",
          "original_form": "월정교!!",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026698.1.21",
          "form": "거긴 한가득 피어있더라구요",
          "original_form": "거긴 한가득 피어있더라구요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026698.1.22",
          "form": "와 보고싶다",
          "original_form": "와 보고싶다",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026698.1.23",
          "form": "다음엔 저도 경주가야겠어요",
          "original_form": "다음엔 저도 경주가야겠어요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026698.1.24",
          "form": "ㅋㅋㅋ 좋아요! 바실리카페가 젤 유명하대요",
          "original_form": "ㅋㅋㅋ 좋아요! 바실리카페가 젤 유명하대요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026698.1.25",
          "form": "7월 중반쯤가야하긴하지만",
          "original_form": "7월 중반쯤가야하긴하지만",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026698.1.26",
          "form": "오오 내년을 기약해야겠네요",
          "original_form": "오오 내년을 기약해야겠네요",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026698.1.27",
          "form": "저는 올해는 포항 갔는데",
          "original_form": "저는 올해는 포항 갔는데",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026698.1.28",
          "form": "엄청 덥더라구요 다녀오니 다 탔어요ㅋㅋ",
          "original_form": "엄청 덥더라구요 다녀오니 다 탔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026698.1.29",
          "form": "ㅋㅋㅋㅋㅋ 저도 다리랑 발이 색이 달라욬ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 다리랑 발이 색이 달라욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026698.1.30",
          "form": "정말 다 타욬ㅋㅋㅋㅋㅋ",
          "original_form": "정말 다 타욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026698.1.31",
          "form": "저는 팔 안쪽이랑 밖잌ㅋㅋㅋ 다른 사람이에요",
          "original_form": "저는 팔 안쪽이랑 밖잌ㅋㅋㅋ 다른 사람이에요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026698.1.32",
          "form": "아니 거의 실내만 있었는데 진짜 볕이 센가봐요",
          "original_form": "아니 거의 실내만 있었는데 진짜 볕이 센가봐요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026698.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026698.1.34",
          "form": "여름은 항상 ㅋㅋㅋㅋㅋ 그런게웃긴거같아요",
          "original_form": "여름은 항상 ㅋㅋㅋㅋㅋ 그런게웃긴거같아요",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026698.1.35",
          "form": "색다른거",
          "original_form": "색다른거",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026698.1.36",
          "form": "이번에 펜션을 빌렸는데",
          "original_form": "이번에 펜션을 빌렸는데",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026698.1.37",
          "form": "평일끼워서 2박 했거든요",
          "original_form": "평일끼워서 2박 했거든요",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026698.1.38",
          "form": "그러니까 펜션 사장님이 잘해주셔서 좋았어욬ㅋㅋㅋㅋㅋ",
          "original_form": "그러니까 펜션 사장님이 잘해주셔서 좋았어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026698.1.39",
          "form": "ㅋㅋㅋㅋ 코로나 시국엔 그런게 최고죠!",
          "original_form": "ㅋㅋㅋㅋ 코로나 시국엔 그런게 최고죠!",
          "speaker_id": "1",
