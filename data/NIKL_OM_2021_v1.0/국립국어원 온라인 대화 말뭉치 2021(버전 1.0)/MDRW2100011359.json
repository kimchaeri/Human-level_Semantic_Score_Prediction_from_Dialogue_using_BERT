      "utterance": [
        {
          "id": "MDRW2100011359.1.1",
          "form": "연예인좋아하시나요",
          "original_form": "연예인좋아하시나요",
          "speaker_id": "2",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100011359.1.2",
          "form": "네 좋아해요! 에스파",
          "original_form": "네 좋아해요! 에스파",
          "speaker_id": "1",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100011359.1.3",
          "form": "아하 저도 좋아해요 저는 태민도 좋아요",
          "original_form": "아하 저도 좋아해요 저는 태민도 좋아요",
          "speaker_id": "2",
          "time": "20210824 11:40"
        },
        {
          "id": "MDRW2100011359.1.4",
          "form": "태민 지금 군대에있죠? ㅠㅠ",
          "original_form": "태민 지금 군대에있죠? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100011359.1.5",
          "form": "네ㅎㅎ 군대갔어요ㅜㅜㅜㅜㅜ",
          "original_form": "네ㅎㅎ 군대갔어요ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100011359.1.6",
          "form": "어우 엄청 고생하겠네요",
          "original_form": "어우 엄청 고생하겠네요",
          "speaker_id": "1",
          "time": "20210824 11:48"
        },
        {
          "id": "MDRW2100011359.1.7",
          "form": "샤이니 좋아하시는건가여",
          "original_form": "샤이니 좋아하시는건가여",
          "speaker_id": "1",
          "time": "20210824 11:48"
        },
        {
          "id": "MDRW2100011359.1.8",
          "form": "네 종현이가 가버려서ㅜㅜ슬펐어요 그래도 좋은곡 남겨줘서 유투브로 들어요",
          "original_form": "네 종현이가 가버려서ㅜㅜ슬펐어요 그래도 좋은곡 남겨줘서 유투브로 들어요",
          "speaker_id": "2",
          "time": "20210824 11:49"
        },
        {
          "id": "MDRW2100011359.1.9",
          "form": "샤이니 노래 진짜 다좋은것같아요",
          "original_form": "샤이니 노래 진짜 다좋은것같아요",
          "speaker_id": "1",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100011359.1.10",
          "form": "에스파도 노래 춤 다 실력파라서 즐겨들어요",
          "original_form": "에스파도 노래 춤 다 실력파라서 즐겨들어요",
          "speaker_id": "2",
          "time": "20210824 11:55"
        },
        {
          "id": "MDRW2100011359.1.11",
          "form": "중독성이 진짜 있더라구요 노래ㅎㅎ",
          "original_form": "중독성이 진짜 있더라구요 노래ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100011359.1.12",
          "form": "둘다 에스엠가스 좋아하네요",
          "original_form": "둘다 에스엠가스 좋아하네요",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100011359.1.13",
          "form": "가수",
          "original_form": "가수",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100011359.1.14",
          "form": "아 그러네요ㅎㅎ 에스파 신곡 또 언제나올지",
          "original_form": "아 그러네요ㅎㅎ 에스파 신곡 또 언제나올지",
