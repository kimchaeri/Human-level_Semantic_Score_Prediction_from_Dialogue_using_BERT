      "utterance": [
        {
          "id": "MDRW2100014558.1.1",
          "form": "다이어트 해보신적 있으신가요",
          "original_form": "다이어트 해보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014558.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100014558.1.3",
          "form": "다이어트 해 본적 딱히 없어요",
          "original_form": "다이어트 해 본적 딱히 없어요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100014558.1.4",
          "form": "아파본적있으신가요",
          "original_form": "아파본적있으신가요",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014558.1.5",
          "form": "어디가요? 전 허리가 자주 아파요.",
          "original_form": "어디가요? 전 허리가 자주 아파요.",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100014558.1.6",
          "form": "병원 다니세요?",
          "original_form": "병원 다니세요?",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100014558.1.7",
          "form": "병원을 다니진 않는데 디스크가 좀 있다 그러네요",
          "original_form": "병원을 다니진 않는데 디스크가 좀 있다 그러네요",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100014558.1.8",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100014558.1.9",
          "form": "만보걷기해요",
          "original_form": "만보걷기해요",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100014558.1.10",
          "form": "영양제 드시나요",
          "original_form": "영양제 드시나요",
          "speaker_id": "1",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100014558.1.11",
          "form": "그냥 비타민이랑 마그네슘 먹어요.",
          "original_form": "그냥 비타민이랑 마그네슘 먹어요.",
          "speaker_id": "2",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100014558.1.12",
          "form": "저는 마그네슘,아연,비타민c 먹어요",
          "original_form": "저는 마그네슘,아연,비타민c 먹어요",
          "speaker_id": "1",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100014558.1.13",
          "form": "요즘 다들 많이 드시더라구요",
          "original_form": "요즘 다들 많이 드시더라구요",
          "speaker_id": "2",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100014558.1.14",
          "form": "혹시 크릴 오일 드셔보셨어요?",
          "original_form": "혹시 크릴 오일 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100014558.1.15",
          "form": "아니요 님은요?",
