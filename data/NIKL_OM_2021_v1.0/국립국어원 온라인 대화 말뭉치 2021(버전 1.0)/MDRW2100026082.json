      "utterance": [
        {
          "id": "MDRW2100026082.1.1",
          "form": "그렇겠어요 ㅠㅠ",
          "original_form": "그렇겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026082.1.2",
          "form": "손질법 많이 어렵나요??",
          "original_form": "손질법 많이 어렵나요??",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026082.1.3",
          "form": "살아있어서 쫌 무서웠네요 ㄷㄷ",
          "original_form": "살아있어서 쫌 무서웠네요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026082.1.4",
          "form": "다리를 잘라도 계속 움직였어요",
          "original_form": "다리를 잘라도 계속 움직였어요",
          "speaker_id": "1",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026082.1.5",
          "form": "헉 진짜 무서운데요,,,",
          "original_form": "헉 진짜 무서운데요,,,",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026082.1.6",
          "form": "협조를 잘 해주면 좋을텐데 ㅜㅜ",
          "original_form": "협조를 잘 해주면 좋을텐데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026082.1.7",
          "form": "흑흑 너무 싱싱해도 문제네요",
          "original_form": "흑흑 너무 싱싱해도 문제네요",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026082.1.8",
          "form": "그러게용",
          "original_form": "그러게용",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100026082.1.9",
          "form": "잘 쪄지고 있나요??",
          "original_form": "잘 쪄지고 있나요??",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100026082.1.10",
          "form": "4마리 쪘어요 뱃속으로 들어가는중 ㅎㅎ",
          "original_form": "4마리 쪘어요 뱃속으로 들어가는중 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100026082.1.11",
          "form": "ㅎㅎ 맛있겠어요",
          "original_form": "ㅎㅎ 맛있겠어요",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026082.1.12",
          "form": "살이 꽉 찼나요??",
          "original_form": "살이 꽉 찼나요??",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026082.1.13",
          "form": "오 꽉 찼더라구요",
          "original_form": "오 꽉 찼더라구요",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026082.1.14",
          "form": "옆에서 냥이들이 달라해서 나눠먹었어요",
          "original_form": "옆에서 냥이들이 달라해서 나눠먹었어요",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026082.1.15",
          "form": "헐 고양이 키우시는구나",
          "original_form": "헐 고양이 키우시는구나",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026082.1.16",
          "form": "ㅠㅠ 귀여워요",
          "original_form": "ㅠㅠ 귀여워요",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026082.1.17",
          "form": "치킨 먹으면 맨날 닭가슴살 줘야해요",
          "original_form": "치킨 먹으면 맨날 닭가슴살 줘야해요",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026082.1.18",
          "form": "옆에서  쳐다보면서 울어요",
          "original_form": "옆에서  쳐다보면서 울어요",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026082.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026082.1.20",
          "form": "너무 귀엽네요",
          "original_form": "너무 귀엽네요",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026082.1.21",
          "form": "닭가슴살 좋아해요??",
          "original_form": "닭가슴살 좋아해요??",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026082.1.22",
          "form": "돼지고기는 안먹던데 닭은 좋아하더라구요?",
          "original_form": "돼지고기는 안먹던데 닭은 좋아하더라구요?",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026082.1.23",
          "form": "그래서 제가 싫어하는 퍽퍽살은 울 애들꺼",
          "original_form": "그래서 제가 싫어하는 퍽퍽살은 울 애들꺼",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026082.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026082.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026082.1.26",
          "form": "일석이조네요",
          "original_form": "일석이조네요",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026082.1.27",
          "form": "저는 퍽퍽살도 맛있던뎅 ㅎㅎ",
          "original_form": "저는 퍽퍽살도 맛있던뎅 ㅎㅎ",
