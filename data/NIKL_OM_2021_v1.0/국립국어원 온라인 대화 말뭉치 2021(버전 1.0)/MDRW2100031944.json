      "utterance": [
        {
          "id": "MDRW2100031944.1.1",
          "form": "ㅎㅎ 아고아거 이제청소다해가요ㅎㅎ",
          "original_form": "ㅎㅎ 아고아거 이제청소다해가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100031944.1.2",
          "form": "아참 아내가 차량말했어요ㅎㅎㅎㅎ",
          "original_form": "아참 아내가 차량말했어요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100031944.1.3",
          "form": "전  결혼식 가서 얼굴도장만 찍고 왓어요 ㅎㅎ",
          "original_form": "전  결혼식 가서 얼굴도장만 찍고 왓어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100031944.1.4",
          "form": "스포티지 골랐는데 아직 계약은 안했대요ㅎㅎ",
          "original_form": "스포티지 골랐는데 아직 계약은 안했대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100031944.1.5",
          "form": "네네 어떤거에요 ㄹㄹ",
          "original_form": "네네 어떤거에요 ㄹㄹ",
          "speaker_id": "1",
          "time": "20210905 14:19"
        },
        {
          "id": "MDRW2100031944.1.6",
          "form": "오오ㅎ 결혼식ㅎㅎ",
          "original_form": "오오ㅎ 결혼식ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:19"
        },
        {
