      "utterance": [
        {
          "id": "MDRW2100035694.1.1",
          "form": "재밌는 시간이 찾아왔군요 ㅎㅎ ?",
          "original_form": "재밌는 시간이 찾아왔군요 ㅎㅎ ?",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035694.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035694.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ두근두근 최애 스무고개시간~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ두근두근 최애 스무고개시간~",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035694.1.4",
          "form": "ㅋㅋㅋㅋ하 뭔가 어려울거같지만 ...",
          "original_form": "ㅋㅋㅋㅋ하 뭔가 어려울거같지만 ...",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100035694.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ해봅시다 봅시다!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ해봅시다 봅시다!",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100035694.1.6",
          "form": "음 가장 최근활동 머리길이가 어떤가요 ?!",
          "original_form": "음 가장 최근활동 머리길이가 어떤가요 ?!",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100035694.1.7",
          "form": "긴머리! 잘어울립니다",
          "original_form": "긴머리! 잘어울립니다",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100035694.1.8",
          "form": "오오 ! 음 춤 멤버일까요?",
          "original_form": "오오 ! 음 춤 멤버일까요?",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100035694.1.9",
          "form": "춤멤버는 아니지만 춤도잘춥니다!",
          "original_form": "춤멤버는 아니지만 춤도잘춥니다!",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100035694.1.10",
          "form": "오 ! 혹시 던던댄스 활동기에 밝은 색으로 염색을 했나요?",
          "original_form": "오 ! 혹시 던던댄스 활동기에 밝은 색으로 염색을 했나요?",
          "speaker_id": "1",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100035694.1.11",
          "form": "한 적이 있습니다!",
          "original_form": "한 적이 있습니다!",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100035694.1.12",
          "form": "흠 근데 이건 좀 애매해여 다른 질문도 쥬시져",
          "original_form": "흠 근데 이건 좀 애매해여 다른 질문도 쥬시져",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100035694.1.13",
          "form": "흠 그러면요 !",
          "original_form": "흠 그러면요 !",
          "speaker_id": "1",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100035694.1.14",
          "form": "지금 앞머리가 있나요??!",
          "original_form": "지금 앞머리가 있나요??!",
          "speaker_id": "1",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100035694.1.15",
          "form": "앞머리 없습니나!",
          "original_form": "앞머리 없습니나!",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100035694.1.16",
          "form": "혹시 팀에서 키가 제일 큰가요 !",
          "original_form": "혹시 팀에서 키가 제일 큰가요 !",
          "speaker_id": "1",
