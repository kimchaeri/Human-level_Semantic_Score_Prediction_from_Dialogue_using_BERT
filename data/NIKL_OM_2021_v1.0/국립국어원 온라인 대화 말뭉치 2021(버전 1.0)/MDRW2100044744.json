      "utterance": [
        {
          "id": "MDRW2100044744.1.1",
          "form": "근데 저희동네 오월까지도 붕어빵 팔았어요",
          "original_form": "근데 저희동네 오월까지도 붕어빵 팔았어요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044744.1.2",
          "form": "ㅋㅋㅋㅋ 트럭와서",
          "original_form": "ㅋㅋㅋㅋ 트럭와서",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044744.1.3",
          "form": "생각해보니까 그렇네여 ㅋㅋㅋㅋㅋ",
          "original_form": "생각해보니까 그렇네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044744.1.4",
          "form": "와5월까지..ㅋㅋㅋㅋ",
          "original_form": "와5월까지..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044744.1.5",
          "form": "진짜 늦게까지 팔았죠??",
          "original_form": "진짜 늦게까지 팔았죠??",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044744.1.6",
          "form": "근데 장사도 잘됐어요",
          "original_form": "근데 장사도 잘됐어요",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044744.1.7",
          "form": "더웠을것 같은뎋ㅎ",
          "original_form": "더웠을것 같은뎋ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044744.1.8",
          "form": "사람들이 사먹더라고요 위치가 좋아서",
          "original_form": "사람들이 사먹더라고요 위치가 좋아서",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044744.1.9",
          "form": "와. 먹는장사가 남는장사네여ㅠ",
          "original_form": "와. 먹는장사가 남는장사네여ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044744.1.10",
          "form": "세를 안내는데 꽤 팔거 같았어요",
          "original_form": "세를 안내는데 꽤 팔거 같았어요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044744.1.11",
          "form": "요즘은 염통꼬지 파는곳이 잘 안보여서ㅠ",
          "original_form": "요즘은 염통꼬지 파는곳이 잘 안보여서ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044744.1.12",
          "form": "속상해여",
          "original_form": "속상해여",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044744.1.13",
          "form": "염통!!!",
          "original_form": "염통!!!",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044744.1.14",
          "form": "포장마차같은 분식집에서는 팔았었는데요",
          "original_form": "포장마차같은 분식집에서는 팔았었는데요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044744.1.15",
          "form": "저희동네는 시장에도 잘 안보이고ㅠㅠ",
          "original_form": "저희동네는 시장에도 잘 안보이고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044744.1.16",
          "form": "왜 5일장 같은데 가야있어여ㅠㅠ",
          "original_form": "왜 5일장 같은데 가야있어여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044744.1.17",
          "form": "ㅠㅠ 요즘은 잘 안팔더라고요",
          "original_form": "ㅠㅠ 요즘은 잘 안팔더라고요",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044744.1.18",
          "form": "오일장!! 거긴 꽈배긴데",
          "original_form": "오일장!! 거긴 꽈배긴데",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044744.1.19",
          "form": "캬~ 꽈배기 설탕 솔솔",
          "original_form": "캬~ 꽈배기 설탕 솔솔",
