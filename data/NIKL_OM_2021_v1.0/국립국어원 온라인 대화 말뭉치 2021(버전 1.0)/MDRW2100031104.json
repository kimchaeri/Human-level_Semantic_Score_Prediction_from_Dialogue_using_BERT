      "utterance": [
        {
          "id": "MDRW2100031104.1.1",
          "form": "백신 예약은  하셨나요",
          "original_form": "백신 예약은  하셨나요",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031104.1.2",
          "form": "아 저 우선접종자였는데",
          "original_form": "아 저 우선접종자였는데",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031104.1.3",
          "form": "중간에 취소했더니",
          "original_form": "중간에 취소했더니",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031104.1.4",
          "form": "예약이 안되네요",
          "original_form": "예약이 안되네요",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031104.1.5",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031104.1.6",
          "form": "아니요 7일날 예약 됐어요",
          "original_form": "아니요 7일날 예약 됐어요",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100031104.1.7",
          "form": "좀 두렵네요",
          "original_form": "좀 두렵네요",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031104.1.8",
          "form": "기다림이 무섭네요",
          "original_form": "기다림이 무섭네요",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031104.1.9",
          "form": "아 화이자시죠?",
          "original_form": "아 화이자시죠?",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031104.1.10",
          "form": "괜찮을거예요!",
          "original_form": "괜찮을거예요!",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100031104.1.11",
          "form": "네 화이자",
          "original_form": "네 화이자",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031104.1.12",
          "form": "뉴스에서 부작용 사례가 많이나와서 걱정이되죠",
          "original_form": "뉴스에서 부작용 사례가 많이나와서 걱정이되죠",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031104.1.13",
          "form": "그렇긴 해요 그리고 수급도 모자란다고 접종 간격도 늘이는게 영 맘에 안들어요",
          "original_form": "그렇긴 해요 그리고 수급도 모자란다고 접종 간격도 늘이는게 영 맘에 안들어요",
          "speaker_id": "2",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031104.1.14",
          "form": "2차는 6주후에 접종이죠",
          "original_form": "2차는 6주후에 접종이죠",
          "speaker_id": "1",
          "time": "20210905 08:25"
        },
        {
          "id": "MDRW2100031104.1.15",
          "form": "시간이 다가 올수록 더 긴장이 되네요",
          "original_form": "시간이 다가 올수록 더 긴장이 되네요",
          "speaker_id": "1",
          "time": "20210905 08:25"
        },
        {
          "id": "MDRW2100031104.1.16",
          "form": "미루면 너무 늦어질까 그럴수도 없구요 ?",
          "original_form": "미루면 너무 늦어질까 그럴수도 없구요 ?",
          "speaker_id": "1",
          "time": "20210905 08:26"
        },
        {
          "id": "MDRW2100031104.1.17",
          "form": "네 4주로 땡겨준단말이  있던데",
          "original_form": "네 4주로 땡겨준단말이  있던데",
          "speaker_id": "2",
          "time": "20210906 13:43"
        },
        {
          "id": "MDRW2100031104.1.18",
          "form": "언제 시행될지 모르겠네요",
          "original_form": "언제 시행될지 모르겠네요",
          "speaker_id": "2",
          "time": "20210906 13:43"
        },
        {
          "id": "MDRW2100031104.1.19",
          "form": "약이 부족해서 아직은 미지수 이지 않을까요",
          "original_form": "약이 부족해서 아직은 미지수 이지 않을까요",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100031104.1.20",
          "form": "그쵸 그냥 기대안하는게 나을 거같아요",
          "original_form": "그쵸 그냥 기대안하는게 나을 거같아요",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100031104.1.21",
          "form": "화이자맞으세요?",
          "original_form": "화이자맞으세요?",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100031104.1.22",
          "form": "네 화이자 이네요",
          "original_form": "네 화이자 이네요",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100031104.1.23",
          "form": "모더니 부족으로 변경 됐어요",
          "original_form": "모더니 부족으로 변경 됐어요",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100031104.1.24",
          "form": "아 원래 모더나하셨나보네요",
          "original_form": "아 원래 모더나하셨나보네요",
          "speaker_id": "2",
          "time": "20210906 18:35"
        },
        {
          "id": "MDRW2100031104.1.25",
          "form": "10부제 예약하실때",
          "original_form": "10부제 예약하실때",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100031104.1.26",
          "form": "원하는 날짜에 잘 잡히던가요?",
          "original_form": "원하는 날짜에 잘 잡히던가요?",
          "speaker_id": "2",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100031104.1.27",
          "form": "처음 예약할땐 모더나 라고 문자왔죠",
          "original_form": "처음 예약할땐 모더나 라고 문자왔죠",
          "speaker_id": "1",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100031104.1.28",
          "form": "원하는 날짜 가능해요",
          "original_form": "원하는 날짜 가능해요",
          "speaker_id": "1",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100031104.1.29",
          "form": "아 그렇군요 그럼 내일  직장 쉬시겠네요",
          "original_form": "아 그렇군요 그럼 내일  직장 쉬시겠네요",
          "speaker_id": "2",
          "time": "20210906 18:55"
        }
      ]
