      "utterance": [
        {
          "id": "MDRW2100006010.1.1",
          "form": "영양제 드시나요",
          "original_form": "영양제 드시나요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100006010.1.2",
          "form": "귀찮아서 꾸준히챙겨먹는게 잘 안돼요ㅜ",
          "original_form": "귀찮아서 꾸준히챙겨먹는게 잘 안돼요ㅜ",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100006010.1.3",
          "form": "챙겨드시나용",
          "original_form": "챙겨드시나용",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100006010.1.4",
          "form": "네 저는 챙겨먹습니다",
          "original_form": "네 저는 챙겨먹습니다",
          "speaker_id": "1",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100006010.1.5",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100006010.1.6",
          "form": "비타민b.c.d 오메가3.유산균 이렇게요",
          "original_form": "비타민b.c.d 오메가3.유산균 이렇게요",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100006010.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100006010.1.8",
          "form": "부지런하심다",
          "original_form": "부지런하심다",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100006010.1.9",
          "form": "와이프가 챙겨먹으면서 같이 먹게됐어요 ㅎㅎ",
          "original_form": "와이프가 챙겨먹으면서 같이 먹게됐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100006010.1.10",
          "form": "운동은하시나요",
          "original_form": "운동은하시나요",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100006010.1.11",
          "form": "네 헬스합니다",
          "original_form": "네 헬스합니다",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100006010.1.12",
          "form": "저는 숨쉬기운동",
          "original_form": "저는 숨쉬기운동",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100006010.1.13",
          "form": "바빠요ㅋㅋ",
          "original_form": "바빠요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100006010.1.14",
          "form": "ㅎㅎ 숨쉬기운동... 아직 젊으셔서 그런가봅니다",
          "original_form": "ㅎㅎ 숨쉬기운동... 아직 젊으셔서 그런가봅니다",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100006010.1.15",
          "form": "허리랑 무릎이 안좋아서",
          "original_form": "허리랑 무릎이 안좋아서",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100006010.1.16",
          "form": "피티받고싶다생각만하고잇어요",
          "original_form": "피티받고싶다생각만하고잇어요",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100006010.1.17",
          "form": "와이프가 최근 피티 받더라구요",
