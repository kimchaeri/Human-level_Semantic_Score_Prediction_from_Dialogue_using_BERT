      "utterance": [
        {
          "id": "MDRW2100015244.1.1",
          "form": "바람이 차가워져서 그런지 목이 칼칼하네",
          "original_form": "바람이 차가워져서 그런지 목이 칼칼하네",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015244.1.2",
          "form": "목 아픈 건 아니고?",
          "original_form": "목 아픈 건 아니고?",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015244.1.3",
          "form": "좀 따끔거려.. 감기 걸리면 안되는데ㅠ",
          "original_form": "좀 따끔거려.. 감기 걸리면 안되는데ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015244.1.4",
          "form": "소금물 가글이라도 하고 자",
          "original_form": "소금물 가글이라도 하고 자",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015244.1.5",
          "form": "그럴까?",
          "original_form": "그럴까?",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015244.1.6",
          "form": "베타딘도 뿌리고",
          "original_form": "베타딘도 뿌리고",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015244.1.7",
          "form": "요즘 같은 때에 감기 걸리면 큰일이니",
          "original_form": "요즘 같은 때에 감기 걸리면 큰일이니",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015244.1.8",
          "form": "ㅇㅇ 미리미리 예방해야 돼",
          "original_form": "ㅇㅇ 미리미리 예방해야 돼",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015244.1.9",
          "form": "운동 시작하면 꼭 감기걸린 것처럼 이러더라 ㅎㅎ",
          "original_form": "운동 시작하면 꼭 감기걸린 것처럼 이러더라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015244.1.10",
          "form": "몸이 너무 약해서 그래",
          "original_form": "몸이 너무 약해서 그래",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015244.1.11",
          "form": "내일 일어나면 멀쩡하면 좋겠네",
          "original_form": "내일 일어나면 멀쩡하면 좋겠네",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015244.1.12",
          "form": "혹시 모르니 감기약도 먹고 자던지",
          "original_form": "혹시 모르니 감기약도 먹고 자던지",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015244.1.13",
          "form": "ㅎㅎ 감기약은 증상 더 심해지면 먹을께",
          "original_form": "ㅎㅎ 감기약은 증상 더 심해지면 먹을께",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015244.1.14",
          "form": "그랴.. 일단 소금물 가글부터 고고",
          "original_form": "그랴.. 일단 소금물 가글부터 고고",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015244.1.15",
          "form": "온라인 수업 때문에 그런지 아침부터 심하던데",
          "original_form": "온라인 수업 때문에 그런지 아침부터 심하던데",
          "speaker_id": "1",
          "time": "20210825 22:43"
        }
