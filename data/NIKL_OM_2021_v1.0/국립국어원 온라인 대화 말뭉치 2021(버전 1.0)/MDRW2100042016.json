      "utterance": [
        {
          "id": "MDRW2100042016.1.1",
          "form": "두그룻 먹어여지",
          "original_form": "두그룻 먹어여지",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100042016.1.2",
          "form": "굿 그래야지",
          "original_form": "굿 그래야지",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100042016.1.3",
          "form": "반찬은",
          "original_form": "반찬은",
          "speaker_id": "1",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100042016.1.4",
          "form": "김찌말구",
          "original_form": "김찌말구",
          "speaker_id": "1",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100042016.1.5",
          "form": "아마 할머니가 주신 가지를",
          "original_form": "아마 할머니가 주신 가지를",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100042016.1.6",
          "form": "구워볼까해요",
          "original_form": "구워볼까해요",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100042016.1.7",
          "form": "삼심이가 레시피알려줌",
          "original_form": "삼심이가 레시피알려줌",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100042016.1.8",
          "form": "와 가지요?!!!",
          "original_form": "와 가지요?!!!",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100042016.1.9",
          "form": "가지라니..",
          "original_form": "가지라니..",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100042016.1.10",
          "form": "가지 구우면 맛있대",
          "original_form": "가지 구우면 맛있대",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100042016.1.11",
          "form": "나도 몰라 ㅋㅋ",
          "original_form": "나도 몰라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100042016.1.12",
          "form": "가지맛없져.ㅈ",
          "original_form": "가지맛없져.ㅈ",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100042016.1.13",
          "form": "ㅋㅋㅈㅋ",
          "original_form": "ㅋㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100042016.1.14",
          "form": "맛있으면말해죠..",
          "original_form": "맛있으면말해죠..",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100042016.1.15",
          "form": "맞아 근데 굴소스로ㅜ볶으니",
          "original_form": "맞아 근데 굴소스로ㅜ볶으니",
          "speaker_id": "2",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100042016.1.16",
          "form": "먹을만했어",
          "original_form": "먹을만했어",
          "speaker_id": "2",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100042016.1.17",
          "form": "오 만능굴소스",
          "original_form": "오 만능굴소스",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100042016.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100042016.1.19",
          "form": "맛있으면 가지 줄게",
          "original_form": "맛있으면 가지 줄게",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100042016.1.20",
          "form": "굴소스최고",
          "original_form": "굴소스최고",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100042016.1.21",
          "form": "악ㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100042016.1.22",
          "form": "가지거부",
          "original_form": "가지거부",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100042016.1.23",
          "form": "가지 반사 !",
          "original_form": "가지 반사 !",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042016.1.24",
          "form": "가지도 먹어야지 건강하지ㅠㅠ",
          "original_form": "가지도 먹어야지 건강하지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042016.1.25",
          "form": "ㅂㄱㅋㅋㅋㅋ",
          "original_form": "ㅂㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042016.1.26",
          "form": "가지반사ㅋㄱㅋㅋ",
          "original_form": "가지반사ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042016.1.27",
          "form": "차은우는 뭐먹고 살까",
          "original_form": "차은우는 뭐먹고 살까",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042016.1.28",
          "form": "마카롱 좋아하던데",
          "original_form": "마카롱 좋아하던데",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042016.1.29",
          "form": "가지에 이어",
          "original_form": "가지에 이어",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042016.1.30",
          "form": "차은우는 마카롱 좋아하더라고",
