      "utterance": [
        {
          "id": "MDRW2100023974.1.1",
          "form": "꿀고구마 피자요!",
          "original_form": "꿀고구마 피자요!",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023974.1.2",
          "form": "치즈피자도 맛있어요!!",
          "original_form": "치즈피자도 맛있어요!!",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023974.1.3",
          "form": "아 치즈피자 그거 들어본거 같아요",
          "original_form": "아 치즈피자 그거 들어본거 같아요",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023974.1.4",
          "form": "피자마루에서 만든거 그거 치즈폭탄피자가 정식명이죠?",
          "original_form": "피자마루에서 만든거 그거 치즈폭탄피자가 정식명이죠?",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023974.1.5",
          "form": "아 정식명은 모르겠어요ㅠㅠㅠ",
          "original_form": "아 정식명은 모르겠어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023974.1.6",
          "form": "자동으로 뙇 누르다보니..",
          "original_form": "자동으로 뙇 누르다보니..",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100023974.1.7",
          "form": "저거랑 닭발 같이 팔더라구요ㅋㅋㅋ",
          "original_form": "저거랑 닭발 같이 팔더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100023974.1.8",
          "form": "말씀하시는게 맞으실까요?",
          "original_form": "말씀하시는게 맞으실까요?",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100023974.1.9",
          "form": "앗 잘모르겠어요 ㅋㅋ",
          "original_form": "앗 잘모르겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023974.1.10",
          "form": "예전에 피자마루에서 그거 때문에 한동안 핫했거든요",
          "original_form": "예전에 피자마루에서 그거 때문에 한동안 핫했거든요",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023974.1.11",
          "form": "그랬더니 다른 프렌차이즈도 막 따라하고 그랬어요",
          "original_form": "그랬더니 다른 프렌차이즈도 막 따라하고 그랬어요",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023974.1.12",
          "form": "아아ㅋㅋㅋ 진짜 최고였죠",
          "original_form": "아아ㅋㅋㅋ 진짜 최고였죠",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023974.1.13",
          "form": "역사의 한 부분 같은 전성기..ㅋㅋ",
          "original_form": "역사의 한 부분 같은 전성기..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023974.1.14",
          "form": "ㅋㅋㅋㅋ와 진짶 피자마루 메뉴 개발한걸로",
          "original_form": "ㅋㅋㅋㅋ와 진짶 피자마루 메뉴 개발한걸로",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023974.1.15",
          "form": "이미지도 좋아지고 유명해지고 홍보도 되고",
          "original_form": "이미지도 좋아지고 유명해지고 홍보도 되고",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023974.1.16",
          "form": "그 사람 상 두둑히 받았을듯해요",
          "original_form": "그 사람 상 두둑히 받았을듯해요",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023974.1.17",
          "form": "맞아용!",
          "original_form": "맞아용!",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100023974.1.18",
          "form": "많이 안생긴게 신기할정도에요",
          "original_form": "많이 안생긴게 신기할정도에요",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100023974.1.19",
          "form": "피자마루 프렌차이즈용?",
          "original_form": "피자마루 프렌차이즈용?",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100023974.1.20",
          "form": "아마 자영업자들 매출 보장 때문에 그럴걸요 ㅠㅠ",
          "original_form": "아마 자영업자들 매출 보장 때문에 그럴걸요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100023974.1.21",
          "form": "상권 겹치면 싸움나요!",
          "original_form": "상권 겹치면 싸움나요!",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100023974.1.22",
          "form": "아ㅜㅜ 그런거군욥ㅠ",
          "original_form": "아ㅜㅜ 그런거군욥ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100023974.1.23",
          "form": "왠지 저희집 근처에 하나 있거든오ㅡ",
          "original_form": "왠지 저희집 근처에 하나 있거든오ㅡ",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100023974.1.24",
          "form": "ㅋㅋㅋ 그래서 편의점도 막 서로 싸우고 그러잖아요 ㅜㅜ",
          "original_form": "ㅋㅋㅋ 그래서 편의점도 막 서로 싸우고 그러잖아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100023974.1.25",
          "form": "피자마루는 몇 분거리에 있어요?",
          "original_form": "피자마루는 몇 분거리에 있어요?",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100023974.1.26",
          "form": "이사와서.. 멀어졌어오ㅜㅠ",
          "original_form": "이사와서.. 멀어졌어오ㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100023974.1.27",
          "form": "그래도 예전엔 배달 안하더니 요즘은 해주더라구요!",
          "original_form": "그래도 예전엔 배달 안하더니 요즘은 해주더라구요!",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100023974.1.28",
          "form": "배달비도 없어욧!",
          "original_form": "배달비도 없어욧!",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100023974.1.29",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100023974.1.30",
          "form": "근데 메뉴에 배달비 포함된거 아니에요?ㅠ",
          "original_form": "근데 메뉴에 배달비 포함된거 아니에요?ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100023974.1.31",
          "form": "앗.. 가격은 옛날가격이랑 크게 안변해서요..!",
          "original_form": "앗.. 가격은 옛날가격이랑 크게 안변해서요..!",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100023974.1.32",
          "form": "대신 픽업에 콜라나 소스를 주더라구요",
          "original_form": "대신 픽업에 콜라나 소스를 주더라구요",
          "speaker_id": "2",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100023974.1.33",
          "form": "와 그렇게 파는 거 진짜 다른 가게도 다 배워앻요",
          "original_form": "와 그렇게 파는 거 진짜 다른 가게도 다 배워앻요",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100023974.1.34",
          "form": "배워야해요",
          "original_form": "배워야해요",
          "speaker_id": "1",
          "time": "20210830 23:56"
        }
      ]
