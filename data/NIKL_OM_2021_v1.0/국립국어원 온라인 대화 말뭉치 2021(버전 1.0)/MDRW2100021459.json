      "utterance": [
        {
          "id": "MDRW2100021459.1.1",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021459.1.2",
          "form": "내가 직구로 하이큐 굿즈를 샀거든?",
          "original_form": "내가 직구로 하이큐 굿즈를 샀거든?",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021459.1.3",
          "form": "응응 그런데?",
          "original_form": "응응 그런데?",
          "speaker_id": "2",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021459.1.4",
          "form": "근데 메루카리라고 일본 중고나라에서 구매대행도 했어",
          "original_form": "근데 메루카리라고 일본 중고나라에서 구매대행도 했어",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021459.1.5",
          "form": "응응 무슨일이있었대...",
          "original_form": "응응 무슨일이있었대...",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021459.1.6",
          "form": "근데 직구한걸 배대지에서 결제한걸 까먹고 구매대행 배송신청도 한거야",
          "original_form": "근데 직구한걸 배대지에서 결제한걸 까먹고 구매대행 배송신청도 한거야",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021459.1.7",
          "form": "아이곸ㅋㅋ왜그랬어",
          "original_form": "아이곸ㅋㅋ왜그랬어",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021459.1.8",
          "form": "근데 둘이다 일을 너무 잘해서 같은날 비행기를 탄거야 ㅋㅋ",
          "original_form": "근데 둘이다 일을 너무 잘해서 같은날 비행기를 탄거야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021459.1.9",
          "form": "엄청빠르게 ㅋㅋㅋ칼같이ㅠ",
          "original_form": "엄청빠르게 ㅋㅋㅋ칼같이ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021459.1.10",
          "form": "어앜ㅋㅋㅋㅋㅋㅋㅋ어떡해ㅠㅠㅠ",
          "original_form": "어앜ㅋㅋㅋㅋㅋㅋㅋ어떡해ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021459.1.11",
          "form": "그래서 입항일이 같으면 빼박 관세당첨이라..계산을 해보니까..금액이",
          "original_form": "그래서 입항일이 같으면 빼박 관세당첨이라..계산을 해보니까..금액이",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021459.1.12",
          "form": "응응ㅋㅋㅋㅋ 그래서 어떻게 해써",
          "original_form": "응응ㅋㅋㅋㅋ 그래서 어떻게 해써",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021459.1.13",
          "form": "세상에 관세랑 부가세같이나가서 6만원쯤 나오더라...?ㅠㅠ",
          "original_form": "세상에 관세랑 부가세같이나가서 6만원쯤 나오더라...?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021459.1.14",
          "form": "헐 육만원이 어느 집 개 이름도 아니고ㅠㅠ",
          "original_form": "헐 육만원이 어느 집 개 이름도 아니고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021459.1.15",
          "form": "그래서 그냥 기도했지..",
          "original_form": "그래서 그냥 기도했지..",
          "speaker_id": "1",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021459.1.16",
          "form": "ㅠ관세 안걸리게?",
          "original_form": "ㅠ관세 안걸리게?",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021459.1.17",
          "form": "같은 날 입항 하지 않게 해달라고..응..",
          "original_form": "같은 날 입항 하지 않게 해달라고..응..",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021459.1.18",
          "form": "그래서 어케돼써?",
          "original_form": "그래서 어케돼써?",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021459.1.19",
          "form": "근데 다행히 도쿄에서 탄 비행기가 연착되어서 진짜 딱 30분차이로 입항해서",
          "original_form": "근데 다행히 도쿄에서 탄 비행기가 연착되어서 진짜 딱 30분차이로 입항해서",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021459.1.20",
          "form": "밤 12시 지나서 날짜 다르게 도착함...",
          "original_form": "밤 12시 지나서 날짜 다르게 도착함...",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021459.1.21",
          "form": "우왘ㅋㅋㅋㅋㅋㅋㅋㅋ대박이다 진짜 운 좋다!!",
          "original_form": "우왘ㅋㅋㅋㅋㅋㅋㅋㅋ대박이다 진짜 운 좋다!!",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021459.1.22",
          "form": "울면서 감사의 기도했잖아 나..",
          "original_form": "울면서 감사의 기도했잖아 나..",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021459.1.23",
          "form": "눈물 몇방울 흘렸나요",
          "original_form": "눈물 몇방울 흘렸나요",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021459.1.24",
          "form": "멍청비용치고 6만원은 너무 큰돈이야..ㅠ",
          "original_form": "멍청비용치고 6만원은 너무 큰돈이야..ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021459.1.25",
