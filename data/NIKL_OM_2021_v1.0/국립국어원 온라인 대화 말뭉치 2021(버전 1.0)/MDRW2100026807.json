      "utterance": [
        {
          "id": "MDRW2100026807.1.1",
          "form": "진짜 스벅이",
          "original_form": "진짜 스벅이",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026807.1.2",
          "form": "제일 깨끗한것 같다...",
          "original_form": "제일 깨끗한것 같다...",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.3",
          "form": "비싼값 하는 놈이었네...",
          "original_form": "비싼값 하는 놈이었네...",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.4",
          "form": "아니면 차라리 오픈한지 얼마 안된 개인 카페나 골라 다니거나..",
          "original_form": "아니면 차라리 오픈한지 얼마 안된 개인 카페나 골라 다니거나..",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.5",
          "form": "근데 카페만 그런게아니고",
          "original_form": "근데 카페만 그런게아니고",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.6",
          "form": "나도 어릴때는",
          "original_form": "나도 어릴때는",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.7",
          "form": "스벅만 가는거",
          "original_form": "스벅만 가는거",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.8",
          "form": "이해 잘 못했는데",
          "original_form": "이해 잘 못했는데",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.9",
          "form": "걍 그런 음식점들 개많을거같아서",
          "original_form": "걍 그런 음식점들 개많을거같아서",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.10",
          "form": "진짜싫다",
          "original_form": "진짜싫다",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026807.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나도",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026807.1.12",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026807.1.13",
          "form": "어린 나한테 스벅 가격은",
          "original_form": "어린 나한테 스벅 가격은",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026807.1.14",
          "form": "ㄹㅇ...이해가 안될수준이었음",
          "original_form": "ㄹㅇ...이해가 안될수준이었음",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026807.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅈ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅈ",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026807.1.16",
          "form": "근데 이제 거의 비슷한것 같음",
          "original_form": "근데 이제 거의 비슷한것 같음",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026807.1.17",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026807.1.18",
          "form": "아메리카노 말고는",
          "original_form": "아메리카노 말고는",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026807.1.19",
          "form": "엽떡 설빙 스벅 교촌 얘네가",
          "original_form": "엽떡 설빙 스벅 교촌 얘네가",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026807.1.20",
          "form": "그 분야 음식들 가격 다올려놓음",
          "original_form": "그 분야 음식들 가격 다올려놓음",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026807.1.21",
          "form": "맞아....",
          "original_form": "맞아....",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026807.1.22",
          "form": "교촌 진짜",
          "original_form": "교촌 진짜",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026807.1.23",
          "form": "뭔...테마 주도주냐??",
          "original_form": "뭔...테마 주도주냐??",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
