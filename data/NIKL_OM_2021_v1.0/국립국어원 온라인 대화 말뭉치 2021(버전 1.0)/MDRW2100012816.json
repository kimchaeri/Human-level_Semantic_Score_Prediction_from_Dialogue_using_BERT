      "utterance": [
        {
          "id": "MDRW2100012816.1.1",
          "form": "오 괜찮네요 ㅎㅎ",
          "original_form": "오 괜찮네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012816.1.2",
          "form": "라면 얘기 더해보시죠ㅎㄴㅅ",
          "original_form": "라면 얘기 더해보시죠ㅎㄴㅅ",
          "speaker_id": "1",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012816.1.3",
          "form": "황제라면 왜먹는지 모르는 1인ㅎㅎ",
          "original_form": "황제라면 왜먹는지 모르는 1인ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012816.1.4",
          "form": "라면이 종류가 많잖아요ㅎ",
          "original_form": "라면이 종류가 많잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100012816.1.5",
          "form": "비빔류십니까? 국물류십니까?",
          "original_form": "비빔류십니까? 국물류십니까?",
          "speaker_id": "2",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100012816.1.6",
          "form": "국물파요!",
          "original_form": "국물파요!",
          "speaker_id": "1",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100012816.1.7",
          "form": "황제라면이 뭔가요",
          "original_form": "황제라면이 뭔가요",
          "speaker_id": "1",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100012816.1.8",
          "form": "전복이랑 소고기 새우 등등 몸에좋은건 다들어가는 라면이요ㅎ",
          "original_form": "전복이랑 소고기 새우 등등 몸에좋은건 다들어가는 라면이요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:57"
        },
        {
          "id": "MDRW2100012816.1.9",
          "form": "아...굳이 거기에 왜",
          "original_form": "아...굳이 거기에 왜",
          "speaker_id": "1",
          "time": "20210825 01:57"
        },
        {
          "id": "MDRW2100012816.1.10",
          "form": "전 그때 전복이랑 소고기 넣고 먹었는데",
          "original_form": "전 그때 전복이랑 소고기 넣고 먹었는데",
          "speaker_id": "2",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012816.1.11",
          "form": "그냥 따로 줬으면ㅜ",
          "original_form": "그냥 따로 줬으면ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012816.1.12",
          "form": "그래서 이해가 안됫어요ㅎㅎ",
          "original_form": "그래서 이해가 안됫어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012816.1.13",
          "form": "라면은 어디꺼 사세요?",
          "original_form": "라면은 어디꺼 사세요?",
          "speaker_id": "2",
          "time": "20210825 01:59"
        },
        {
          "id": "MDRW2100012816.1.14",
          "form": "신라면? 진라면?",
          "original_form": "신라면? 진라면?",
          "speaker_id": "2",
          "time": "20210825 01:59"
        },
        {
          "id": "MDRW2100012816.1.15",
          "form": "진매!",
          "original_form": "진매!",
          "speaker_id": "1",
          "time": "20210825 01:59"
        },
        {
          "id": "MDRW2100012816.1.16",
          "form": "신라면은 옛날의 신라면이 아녜요ㅜ",
          "original_form": "신라면은 옛날의 신라면이 아녜요ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:59"
        },
        {
          "id": "MDRW2100012816.1.17",
          "form": "저도 진라면ㅎ",
          "original_form": "저도 진라면ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:00"
        },
        {
          "id": "MDRW2100012816.1.18",
          "form": "어제 저 진라면 매운맛으로 저녁먹었는데",
          "original_form": "어제 저 진라면 매운맛으로 저녁먹었는데",
          "speaker_id": "2",
          "time": "20210825 02:00"
        },
        {
          "id": "MDRW2100012816.1.19",
          "form": "가래떡 넣고 먹으니 정말맛있었어요ㅎ",
          "original_form": "가래떡 넣고 먹으니 정말맛있었어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:00"
        },
        {
          "id": "MDRW2100012816.1.20",
          "form": "가래떡에서 탄수화물이 굴물로 녹았나봐요ㅎ 무슨 사골로 끊인라면 같았음ㅎ",
          "original_form": "가래떡에서 탄수화물이 굴물로 녹았나봐요ㅎ 무슨 사골로 끊인라면 같았음ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:01"
        },
        {
          "id": "MDRW2100012816.1.21",
          "form": "신기한게 가래떡 넣으면 맛이 더 깊어져요",
          "original_form": "신기한게 가래떡 넣으면 맛이 더 깊어져요",
          "speaker_id": "1",
          "time": "20210825 02:05"
        },
        {
          "id": "MDRW2100012816.1.22",
          "form": "매운맛도 더 선명해지고ㅎ",
          "original_form": "매운맛도 더 선명해지고ㅎ",
          "speaker_id": "1",
          "time": "20210825 02:05"
        },
        {
          "id": "MDRW2100012816.1.23",
          "form": "그래서 맛있게 먹었어요ㅎ",
          "original_form": "그래서 맛있게 먹었어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:06"
        },
        {
          "id": "MDRW2100012816.1.24",
          "form": "신라면 비빔면 드셔보셨어요?",
          "original_form": "신라면 비빔면 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210825 02:07"
        },
        {
          "id": "MDRW2100012816.1.25",
          "form": "비빔면도 나왔나요?",
          "original_form": "비빔면도 나왔나요?",
          "speaker_id": "1",
          "time": "20210825 02:07"
        },
        {
          "id": "MDRW2100012816.1.26",
          "form": "와 졸리네요ㅎㅎ 내일 시간될때 해요 ㅣㅎ",
          "original_form": "와 졸리네요ㅎㅎ 내일 시간될때 해요 ㅣㅎ",
          "speaker_id": "2",
          "time": "20210825 02:08"
        },
        {
          "id": "MDRW2100012816.1.27",
          "form": "주무세요~~",
          "original_form": "주무세요~~",
          "speaker_id": "1",
