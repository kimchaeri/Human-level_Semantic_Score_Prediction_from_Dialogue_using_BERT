      "utterance": [
        {
          "id": "MDRW2100023765.1.1",
          "form": "저 체인지데이즈 이제 제일 최근화보고있어여",
          "original_form": "저 체인지데이즈 이제 제일 최근화보고있어여",
          "speaker_id": "1",
          "time": "20210830 16:57"
        },
        {
          "id": "MDRW2100023765.1.2",
          "form": "ㅋㅋㅋㅋ진짜 진도 빠르시네요 ㅋㅋㅋ 어떠신가요 심정이",
          "original_form": "ㅋㅋㅋㅋ진짜 진도 빠르시네요 ㅋㅋㅋ 어떠신가요 심정이",
          "speaker_id": "2",
          "time": "20210830 16:57"
        },
        {
          "id": "MDRW2100023765.1.3",
          "form": "우리 홍주가 어때서",
          "original_form": "우리 홍주가 어때서",
          "speaker_id": "1",
          "time": "20210830 16:57"
        },
        {
          "id": "MDRW2100023765.1.4",
          "form": "웃기네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "웃기네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:57"
        },
        {
          "id": "MDRW2100023765.1.5",
          "form": "첨엔 홍주가 야무지게 챙길거같은 이미지였는데",
          "original_form": "첨엔 홍주가 야무지게 챙길거같은 이미지였는데",
          "speaker_id": "1",
          "time": "20210830 16:58"
        },
        {
          "id": "MDRW2100023765.1.6",
          "form": "볼수록 민선이가 ㄷㄷㄷ 내공이 ㄷㄷㄷ",
          "original_form": "볼수록 민선이가 ㄷㄷㄷ 내공이 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210830 16:58"
        },
        {
          "id": "MDRW2100023765.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023765.1.8",
          "form": "처음에 홍주가 첫데이트때 완전 당차서 ㅋㅋㅋㅋ",
          "original_form": "처음에 홍주가 첫데이트때 완전 당차서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023765.1.9",
          "form": "오빠들 가지고놀줄(?) 알았는데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오빠들 가지고놀줄(?) 알았는데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023765.1.10",
          "form": "민선이 차박하고와서 할말 딱딱해서",
          "original_form": "민선이 차박하고와서 할말 딱딱해서",
          "speaker_id": "1",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023765.1.11",
          "form": "맞어요 민선이 초반에 울고 조용하고 그래서 걍 그런줄알았는데 ㅋㅋㅋ",
          "original_form": "맞어요 민선이 초반에 울고 조용하고 그래서 걍 그런줄알았는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023765.1.12",
          "form": "진록이 벙찌는거 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진록이 벙찌는거 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023765.1.13",
          "form": "긍까요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "긍까요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023765.1.14",
          "form": "마자요 그거 진짴ㅋㅋㅋㅋㅋ",
          "original_form": "마자요 그거 진짴ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023765.1.15",
          "form": "근데 민선이는 진짜 걍 다른사람만나는게 나아보여요",
          "original_form": "근데 민선이는 진짜 걍 다른사람만나는게 나아보여요",
          "speaker_id": "1",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023765.1.16",
          "form": "진록이는 아닌듯...",
          "original_form": "진록이는 아닌듯...",
          "speaker_id": "1",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023765.1.17",
          "form": "ㅋㅋㅋㅋ마자요 진록이 그냥",
          "original_form": "ㅋㅋㅋㅋ마자요 진록이 그냥",
          "speaker_id": "2",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023765.1.18",
          "form": "상대방생각은 안하고 자기 스스로 말만 나 달라질게 나 고칠게",
          "original_form": "상대방생각은 안하고 자기 스스로 말만 나 달라질게 나 고칠게",
          "speaker_id": "2",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023765.1.19",
          "form": "그래놓고 나중에",
          "original_form": "그래놓고 나중에",
          "speaker_id": "2",
          "time": "20210830 17:04"
        },
        {
          "id": "MDRW2100023765.1.20",
          "form": "난 이렇게나 노력했는데 넌뭐했니",
          "original_form": "난 이렇게나 노력했는데 넌뭐했니",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100023765.1.21",
          "form": "이럴 스타일ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이럴 스타일ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100023765.1.22",
          "form": "아니 상미 리액션 너무 기엽다 ㅜㅜ 이뻐요",
          "original_form": "아니 상미 리액션 너무 기엽다 ㅜㅜ 이뻐요",
          "speaker_id": "1",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100023765.1.23",
          "form": "전 거기 진짜 다 깨졌음 조켓어요 ㅋㅋㅋ 성호 상미커플 제발좀 깨졌으면 .....",
          "original_form": "전 거기 진짜 다 깨졌음 조켓어요 ㅋㅋㅋ 성호 상미커플 제발좀 깨졌으면 .....",
          "speaker_id": "2",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100023765.1.24",
          "form": "마자요 상미 ㅋㅋㅋㅋㅋ",
          "original_form": "마자요 상미 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100023765.1.25",
          "form": "넘  상큼해욘",
          "original_form": "넘  상큼해욘",
          "speaker_id": "2",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100023765.1.26",
          "form": "맞아요 차라리 걍 헤어지되 혼자 남겟다 선택하는게 젤 나을듯하네여",
          "original_form": "맞아요 차라리 걍 헤어지되 혼자 남겟다 선택하는게 젤 나을듯하네여",
          "speaker_id": "1",
          "time": "20210830 17:13"
        },
        {
          "id": "MDRW2100023765.1.27",
          "form": "진짜 상미너무 안타까워요.. 성호 가스라이팅진짜ㅡㅡ",
          "original_form": "진짜 상미너무 안타까워요.. 성호 가스라이팅진짜ㅡㅡ",
          "speaker_id": "2",
          "time": "20210830 17:15"
        },
        {
          "id": "MDRW2100023765.1.28",
          "form": "상미 입는 옷들 다 이뻐서 어제 쇼핑몰 찾아들어가봤는데",
          "original_form": "상미 입는 옷들 다 이뻐서 어제 쇼핑몰 찾아들어가봤는데",
          "speaker_id": "1",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100023765.1.29",
          "form": "그런 옷 전혀 안팔대요??",
          "original_form": "그런 옷 전혀 안팔대요??",
          "speaker_id": "1",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100023765.1.30",
          "form": "글고 쇼핑몰 중 하나는 커플잠옷파는데에여...",
          "original_form": "글고 쇼핑몰 중 하나는 커플잠옷파는데에여...",
          "speaker_id": "1",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100023765.1.31",
          "form": "ㅋㅋㅋㅋ헐랭 입은옷들 같은거 팔지 ㅋㅋㅋㅋ 커플잠옷이라니...",
          "original_form": "ㅋㅋㅋㅋ헐랭 입은옷들 같은거 팔지 ㅋㅋㅋㅋ 커플잠옷이라니...",
          "speaker_id": "2",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023765.1.32",
          "form": "그니깐여... 진짜 ㅋㅋ자기가 입은것들 팔지 ㅋㅋㅋㅋㅋ 니트랑 다 이쁘던데",
          "original_form": "그니깐여... 진짜 ㅋㅋ자기가 입은것들 팔지 ㅋㅋㅋㅋㅋ 니트랑 다 이쁘던데",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023765.1.33",
          "form": "이제 돌싱글즈 시작합니다",
          "original_form": "이제 돌싱글즈 시작합니다",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023765.1.34",
          "form": "시작부터 세네요",
          "original_form": "시작부터 세네요",
          "speaker_id": "1",
          "time": "20210830 17:34"
        }
      ]
