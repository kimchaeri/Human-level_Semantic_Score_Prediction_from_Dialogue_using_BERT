        {
          "id": "MDRW2100019717.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019717.1.2",
          "form": "네 싫지만 어쩔수 없이 해요",
          "original_form": "네 싫지만 어쩔수 없이 해요",
          "speaker_id": "2",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019717.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019717.1.4",
          "form": "정리정돈을 잘하시나봐요?",
          "original_form": "정리정돈을 잘하시나봐요?",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100019717.1.5",
          "form": "그렇진 않아요",
          "original_form": "그렇진 않아요",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100019717.1.6",
          "form": "그나마 청소가 나으신거죠?",
          "original_form": "그나마 청소가 나으신거죠?",
          "speaker_id": "2",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100019717.1.7",
          "form": "요리보다는요",
          "original_form": "요리보다는요",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100019717.1.8",
          "form": "저도 요리 망손이라서요 하지만 청소도 싫어요 ㅎ",
          "original_form": "저도 요리 망손이라서요 하지만 청소도 싫어요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100019717.1.9",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100019717.1.10",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100019717.1.11",
          "form": "전 설거지 ,빨래 좋아해요",
          "original_form": "전 설거지 ,빨래 좋아해요",
          "speaker_id": "2",
          "time": "20210829 09:04"
        },
        {
          "id": "MDRW2100019717.1.12",
          "form": "요리를못해서요",
          "original_form": "요리를못해서요",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100019717.1.13",
          "form": "저도 요리를 못해서 설거지를 맡아 해요. 제가 끓이면 라면도 맛이 ㅠ ㅜ",
          "original_form": "저도 요리를 못해서 설거지를 맡아 해요. 제가 끓이면 라면도 맛이 ㅠ ㅜ",
          "speaker_id": "2",
