        {
          "id": "MDRW2100020502.1.1",
          "form": "인절미설빙은 근데 잘 안먹게되더라",
          "original_form": "인절미설빙은 근데 잘 안먹게되더라",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.2",
          "form": "나도...",
          "original_form": "나도...",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.3",
          "form": "목맥혀ㅜ",
          "original_form": "목맥혀ㅜ",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.4",
          "form": "그니까..",
          "original_form": "그니까..",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.5",
          "form": "글고 가격도 뭐ㅜ없는데 비싸졋어",
          "original_form": "글고 가격도 뭐ㅜ없는데 비싸졋어",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.6",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.7",
          "form": "원래5천원아엿어",
          "original_form": "원래5천원아엿어",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.8",
          "form": "그니까ㅡㅡ",
          "original_form": "그니까ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.9",
          "form": "지금은 7천?8천??",
          "original_form": "지금은 7천?8천??",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.10",
          "form": "그정도야 진심",
          "original_form": "그정도야 진심",
          "speaker_id": "1",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.11",
          "form": "너무하자나",
          "original_form": "너무하자나",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020502.1.12",
          "form": "마저",
          "original_form": "마저",
          "speaker_id": "1",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020502.1.13",
          "form": "그럴바에 딴 설빙먹는다",
