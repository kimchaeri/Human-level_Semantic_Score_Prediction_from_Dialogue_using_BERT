        },
        {
          "id": "MDRW2100011548.1.2",
          "form": "name1님 마카롱 좋아하시나요?",
          "original_form": "&name1&님 마카롱 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011548.1.3",
          "form": "닉넴만요ㅎ",
          "original_form": "닉넴만요ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011548.1.4",
          "form": "마카롱은 가끔 먹는데 돈주곤 잘안사먹어요",
          "original_form": "마카롱은 가끔 먹는데 돈주곤 잘안사먹어요",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011548.1.5",
          "form": "마카롱좋아하세요?",
          "original_form": "마카롱좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011548.1.6",
          "form": "저도 한창 유행할때 잠깐 먹어본게 다예요",
          "original_form": "저도 한창 유행할때 잠깐 먹어본게 다예요",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011548.1.7",
          "form": "너무 달아요",
          "original_form": "너무 달아요",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011548.1.8",
          "form": "단거좋아하는데  마카롱은 걍 그렇더라구요",
          "original_form": "단거좋아하는데  마카롱은 걍 그렇더라구요",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011548.1.9",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011548.1.10",
          "form": "카페인 때문에 잘못마셔요ㅎ",
          "original_form": "카페인 때문에 잘못마셔요ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011548.1.11",
          "form": "홍차 좋아합니다",
          "original_form": "홍차 좋아합니다",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011548.1.12",
          "form": "아 홍차도 카페인 있지않아요?",
          "original_form": "아 홍차도 카페인 있지않아요?",
          "speaker_id": "1",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011548.1.13",
          "form": "홍차는 괜찮아요",
          "original_form": "홍차는 괜찮아요",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011548.1.14",
          "form": "커피는 심장이 벌렁거려요ㅋㅋ",
          "original_form": "커피는 심장이 벌렁거려요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011548.1.15",
          "form": "그런분들계시더라구요ㅎㅅㄴ",
          "original_form": "그런분들계시더라구요ㅎㅅㄴ",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011548.1.16",
          "form": "홍차도 종류많던데 과일향나는것도있고",
          "original_form": "홍차도 종류많던데 과일향나는것도있고",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011548.1.17",
          "form": "저는 얼그레이랑 오렌지루이보스티 마십니다",
          "original_form": "저는 얼그레이랑 오렌지루이보스티 마십니다",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011548.1.18",
          "form": "어떤커피 좋아하세요?",
          "original_form": "어떤커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011548.1.19",
          "form": "전 마끼아또나 바닐라라떼요",
          "original_form": "전 마끼아또나 바닐라라떼요",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011548.1.20",
          "form": "카페인중독이라 4샷먹어도 전 멀쩡하더라구요",
          "original_form": "카페인중독이라 4샷먹어도 전 멀쩡하더라구요",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011548.1.21",
          "form": "바닐라라떼 바닐라빈 많으면 맛있죠ㅎㅎ",
