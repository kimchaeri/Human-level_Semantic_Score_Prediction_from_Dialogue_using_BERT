      "utterance": [
        {
          "id": "MDRW2100043283.1.1",
          "form": "가보고 싶은 여행이 있나요",
          "original_form": "가보고 싶은 여행이 있나요",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100043283.1.2",
          "form": "저는 일본 안간지 꽤되어서",
          "original_form": "저는 일본 안간지 꽤되어서",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043283.1.3",
          "form": "가까운 일본가고싶네요",
          "original_form": "가까운 일본가고싶네요",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043283.1.4",
          "form": "먼곳은 이제 힘들어요ㅠㅠ",
          "original_form": "먼곳은 이제 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043283.1.5",
          "form": "일본 괜찮나요",
          "original_form": "일본 괜찮나요",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043283.1.6",
          "form": "그냥 가깝고 저렴한데 외국느낌  나는맛에가는 거같아요",
          "original_form": "그냥 가깝고 저렴한데 외국느낌  나는맛에가는 거같아요",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043283.1.7",
          "form": "그리고 일본어를 조금할줄알아서",
          "original_form": "그리고 일본어를 조금할줄알아서",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043283.1.8",
          "form": "의사소통이 편해서요",
          "original_form": "의사소통이 편해서요",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043283.1.9",
          "form": "일본은 가격이 비싸지 않나요",
          "original_form": "일본은 가격이 비싸지 않나요",
          "speaker_id": "1",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043283.1.10",
          "form": "아 유럽 호주 미국 이런곳 가는것보단 덜 든다는 뜻이었어요",
          "original_form": "아 유럽 호주 미국 이런곳 가는것보단 덜 든다는 뜻이었어요",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043283.1.11",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043283.1.12",
          "form": "일본은 가까워서 가는 시간 많인 안걸리죠",
          "original_form": "일본은 가까워서 가는 시간 많인 안걸리죠",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043283.1.13",
          "form": "어디 선호하는 여행지 있나요?",
          "original_form": "어디 선호하는 여행지 있나요?",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100043283.1.14",
          "form": "아직 선호하는 여행지는 없네요",
          "original_form": "아직 선호하는 여행지는 없네요",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043283.1.15",
          "form": "아 어디 가보고 싶으세요?",
          "original_form": "아 어디 가보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210907 18:35"
        },
        {
          "id": "MDRW2100043283.1.16",
          "form": "발리 푸켓 이런곳은 신혼여행지라 좀 그런가요?",
          "original_form": "발리 푸켓 이런곳은 신혼여행지라 좀 그런가요?",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043283.1.17",
          "form": "아니용 친구들 패키지로 가던걸요",
          "original_form": "아니용 친구들 패키지로 가던걸요",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043283.1.18",
          "form": "휴양하기 좋을거같아요",
          "original_form": "휴양하기 좋을거같아요",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043283.1.19",
          "form": "레포츠같은것도 많이하더라구요",
          "original_form": "레포츠같은것도 많이하더라구요",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043283.1.20",
          "form": "저도 꼭 가서 레포츠도 해보고 싶어요",
          "original_form": "저도 꼭 가서 레포츠도 해보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043283.1.21",
          "form": "스킨스쿠버나 스쿠버다이빙 같은거 많이하더라구요",
          "original_form": "스킨스쿠버나 스쿠버다이빙 같은거 많이하더라구요",
