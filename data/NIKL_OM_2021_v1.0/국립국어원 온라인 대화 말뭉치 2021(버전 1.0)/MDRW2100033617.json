      "utterance": [
        {
          "id": "MDRW2100033617.1.1",
          "form": "점심을 늦게 먹어서 아직 생각 없어요 뭘 먹을지 정하지 못했어요",
          "original_form": "점심을 늦게 먹어서 아직 생각 없어요 뭘 먹을지 정하지 못했어요",
          "speaker_id": "2",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100033617.1.2",
          "form": "아하 점심은 뭐드셨어요??",
          "original_form": "아하 점심은 뭐드셨어요??",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100033617.1.3",
          "form": "라면에 만두 넣어서 먹었어요",
          "original_form": "라면에 만두 넣어서 먹었어요",
          "speaker_id": "2",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100033617.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100033617.1.5",
          "form": "와우 라면에 만두 맛있겠어요!!!!",
          "original_form": "와우 라면에 만두 맛있겠어요!!!!",
          "speaker_id": "1",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100033617.1.6",
          "form": "전 시래기국 먹었어요ㅋㅋ",
          "original_form": "전 시래기국 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033617.1.7",
          "form": "시래기 꽁 맛있었겠다 반찬은 뭐 따로 안 드셨어요?",
          "original_form": "시래기 꽁 맛있었겠다 반찬은 뭐 따로 안 드셨어요?",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033617.1.8",
          "form": "시래시국..",
          "original_form": "시래시국..",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033617.1.9",
          "form": "죄송해요.. 자꾸 오타가나요",
          "original_form": "죄송해요.. 자꾸 오타가나요",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033617.1.10",
          "form": "아니에요ㅋㅋㅋㅋㅋ 반찬은",
          "original_form": "아니에요ㅋㅋㅋㅋㅋ 반찬은",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100033617.1.11",
          "form": "제육 먹었는데 맛있더라구요",
          "original_form": "제육 먹었는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100033617.1.12",
          "form": "제육 완전 맛있었겠다.. 씨래기국이랑 찰떡",
          "original_form": "제육 완전 맛있었겠다.. 씨래기국이랑 찰떡",
          "speaker_id": "2",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100033617.1.13",
          "form": "네.. 정말요..",
          "original_form": "네.. 정말요..",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100033617.1.14",
          "form": "저녁은 뭘 먹을지 고민이에요",
          "original_form": "저녁은 뭘 먹을지 고민이에요",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100033617.1.15",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100033617.1.16",
          "form": "보통 만들어 드시나요 배달시켜 드시나요",
          "original_form": "보통 만들어 드시나요 배달시켜 드시나요",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100033617.1.17",
          "form": "음.... 비도 오고 그러니까 부침개..?",
          "original_form": "음.... 비도 오고 그러니까 부침개..?",
          "speaker_id": "2",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100033617.1.18",
          "form": "만들고싶어도",
          "original_form": "만들고싶어도",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100033617.1.19",
          "form": "요리를 못해서 시켜먹어요ㅋㅋㅋㅋㅋㅠ",
          "original_form": "요리를 못해서 시켜먹어요ㅋㅋㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100033617.1.20",
          "form": "앗 그럼 다른 걸 생각해봐야겠다",
          "original_form": "앗 그럼 다른 걸 생각해봐야겠다",
          "speaker_id": "2",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100033617.1.21",
          "form": "보통 만들어 먹으세요??",
          "original_form": "보통 만들어 먹으세요??",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100033617.1.22",
