      "utterance": [
        {
          "id": "MDRW2100019091.1.1",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019091.1.2",
          "form": "투명 케이스에 샤넬 로고 스티커 붙였는데",
          "original_form": "투명 케이스에 샤넬 로고 스티커 붙였는데",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019091.1.3",
          "form": "잘어울리더라구 ㅋㅋㅋㅋㅋ",
          "original_form": "잘어울리더라구 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019091.1.4",
          "form": "아닠",
          "original_form": "아닠",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019091.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019091.1.6",
          "form": "나 그거 야숨컨셉으로 꾸민거 봤는데",
          "original_form": "나 그거 야숨컨셉으로 꾸민거 봤는데",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019091.1.7",
          "form": "진짜 예쁘긴 하더라",
          "original_form": "진짜 예쁘긴 하더라",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019091.1.8",
          "form": "케이스에 로딩화면 넣고 움짤 게임배경으로 띄욱ᆢ",
          "original_form": "케이스에 로딩화면 넣고 움짤 게임배경으로 띄욱ᆢ",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.9",
          "form": "와 야숨컨셉도 있어?",
          "original_form": "와 야숨컨셉도 있어?",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.10",
          "form": "그건 좀 사고싶다",
          "original_form": "그건 좀 사고싶다",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.11",
          "form": "걍 개인이 그렇게 꾸민것같긴 해",
          "original_form": "걍 개인이 그렇게 꾸민것같긴 해",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.12",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.13",
          "form": "근데 예쁘긴 하더라",
          "original_form": "근데 예쁘긴 하더라",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.14",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.15",
          "form": "대기화면에 동영상 넣을 수 있게 만든게",
          "original_form": "대기화면에 동영상 넣을 수 있게 만든게",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.16",
          "form": "갤럭시의 혁신이냐",
          "original_form": "갤럭시의 혁신이냐",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.17",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.18",
          "form": "ㄹㅇ무궁무진하게 꾸미기 가능해졌어",
          "original_form": "ㄹㅇ무궁무진하게 꾸미기 가능해졌어",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.19",
          "form": "구매 욕구 부추기는듯 ㅋㅋㅋ",
          "original_form": "구매 욕구 부추기는듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019091.1.20",
          "form": "솔직히 농담곰은 좀 노잼인데",
          "original_form": "솔직히 농담곰은 좀 노잼인데",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019091.1.21",
          "form": "각잡고 꾸민건 ㄹㅇ예쁘더라고",
          "original_form": "각잡고 꾸민건 ㄹㅇ예쁘더라고",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019091.1.22",
          "form": "여튼 아빠랑 한번 싸우고서",
          "original_form": "여튼 아빠랑 한번 싸우고서",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019091.1.23",
          "form": "나도 내 이어폰을 장만해야겠단 생각이",
          "original_form": "나도 내 이어폰을 장만해야겠단 생각이",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019091.1.24",
          "form": "좀 격하게 들기시작했어",
          "original_form": "좀 격하게 들기시작했어",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019091.1.25",
          "form": "ㅇㅇ 버즈라이브 요즘 저렴하게 매물 많이나와",
          "original_form": "ㅇㅇ 버즈라이브 요즘 저렴하게 매물 많이나와",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019091.1.26",
          "form": "당근마켓 사용해?",
          "original_form": "당근마켓 사용해?",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019091.1.27",
          "form": "있긴 한데",
          "original_form": "있긴 한데",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019091.1.28",
          "form": "솔직히 잘쓸 자신은 없어",
          "original_form": "솔직히 잘쓸 자신은 없어",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019091.1.29",
          "form": "그냥 이렇게 필요한 거 있을때",
          "original_form": "그냥 이렇게 필요한 거 있을때",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019091.1.30",
          "form": "가끔 쓰면 돼",
          "original_form": "가끔 쓰면 돼",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019091.1.31",
          "form": "중고거래 플랫폼이니까",
          "original_form": "중고거래 플랫폼이니까",
          "speaker_id": "1",
          "time": "20210827 15:18"
        }
      ]
