      "utterance": [
        {
          "id": "MDRW2100008549.1.1",
          "form": "건강을 위한 노력을 하시나요",
          "original_form": "건강을 위한 노력을 하시나요",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008549.1.2",
          "form": "운동하고있어요",
          "original_form": "운동하고있어요",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008549.1.3",
          "form": "필라테스해요",
          "original_form": "필라테스해요",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008549.1.4",
          "form": "운동하시는거 있으세요?",
          "original_form": "운동하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008549.1.5",
          "form": "필라테스 어렵지않나요",
          "original_form": "필라테스 어렵지않나요",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008549.1.6",
          "form": "어렵지않아요",
          "original_form": "어렵지않아요",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008549.1.7",
          "form": "허리아파서 시작했는데 이젠 안아파요",
          "original_form": "허리아파서 시작했는데 이젠 안아파요",
          "speaker_id": "2",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008549.1.8",
          "form": "저도 허리가 아픈데 필라테스가 도움 되나요",
          "original_form": "저도 허리가 아픈데 필라테스가 도움 되나요",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008549.1.9",
          "form": "필라가 코어운동이라",
          "original_form": "필라가 코어운동이라",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008549.1.10",
          "form": "코어잡히면 허리아픈거에 도움되요",
          "original_form": "코어잡히면 허리아픈거에 도움되요",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008549.1.11",
          "form": "아 ? 코어운동 알아보고 싶어지네요",
          "original_form": "아 ? 코어운동 알아보고 싶어지네요",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008549.1.12",
          "form": "저는 출산하고 허리 다쳐서 도수 치료받다가",
          "original_form": "저는 출산하고 허리 다쳐서 도수 치료받다가",
          "speaker_id": "2",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008549.1.13",
          "form": "이비용으로 필라시작해봐야겠다싶어서 했는데",
          "original_form": "이비용으로 필라시작해봐야겠다싶어서 했는데",
          "speaker_id": "2",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008549.1.14",
          "form": "도움많이 되더라구요",
          "original_form": "도움많이 되더라구요",
          "speaker_id": "2",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008549.1.15",
          "form": "도수치료 한번받는데 비용이 2~30만원 우습더라구요",
          "original_form": "도수치료 한번받는데 비용이 2~30만원 우습더라구요",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008549.1.16",
          "form": "도수치료 넘 아프고 힘들어요",
          "original_form": "도수치료 넘 아프고 힘들어요",
          "speaker_id": "1",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008549.1.17",
          "form": "운동이 남는거더라구요",
          "original_form": "운동이 남는거더라구요",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008549.1.18",
          "form": "그렀긴 하죠 나중에 안 아프려면 운동 해야죠",
          "original_form": "그렀긴 하죠 나중에 안 아프려면 운동 해야죠",
          "speaker_id": "1",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008549.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008549.1.20",
          "form": "체력이 되야 멀해도 하겠더라구요",
          "original_form": "체력이 되야 멀해도 하겠더라구요",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008549.1.21",
          "form": "유산소 운동 걷기 달리기 하죠",
          "original_form": "유산소 운동 걷기 달리기 하죠",
