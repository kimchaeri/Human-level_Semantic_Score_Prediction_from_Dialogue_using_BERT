      "utterance": [
        {
          "id": "MDRW2100036310.1.1",
          "form": "저녁 뭐드셨어요",
          "original_form": "저녁 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036310.1.2",
          "form": "전 집밥이랑 가지구이요 ㅋㅋ",
          "original_form": "전 집밥이랑 가지구이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100036310.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100036310.1.4",
          "form": "오 가지구이",
          "original_form": "오 가지구이",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036310.1.5",
          "form": "레시피 있나요",
          "original_form": "레시피 있나요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036310.1.6",
          "form": "저는 소고기국 먹었어용",
          "original_form": "저는 소고기국 먹었어용",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036310.1.7",
          "form": "아니오 ㅋㅋ 그냥 밀가루 무쳐서 후라이팬에 굽굽ㅋㅋ",
          "original_form": "아니오 ㅋㅋ 그냥 밀가루 무쳐서 후라이팬에 굽굽ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036310.1.8",
          "form": "소고기 무국인가요?",
          "original_form": "소고기 무국인가요?",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036310.1.9",
          "form": "네맞아용",
          "original_form": "네맞아용",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036310.1.10",
          "form": "아 가지에 밀가루",
          "original_form": "아 가지에 밀가루",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036310.1.11",
          "form": "부침가루 튀김가루도 가능요",
          "original_form": "부침가루 튀김가루도 가능요",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036310.1.12",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036310.1.13",
          "form": "내일 도전해야겠어요",
          "original_form": "내일 도전해야겠어요",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036310.1.14",
          "form": "음 그냥 가지구이가 전 더 맛있어요",
          "original_form": "음 그냥 가지구이가 전 더 맛있어요",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036310.1.15",
          "form": "팬달궈서 걍 굽다가 소굼만 뿌리셔도 돼요",
          "original_form": "팬달궈서 걍 굽다가 소굼만 뿌리셔도 돼요",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036310.1.16",
          "form": "오정말요 가지는 뭔가 해놓으면 맛난데 손이 잘 안가요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오정말요 가지는 뭔가 해놓으면 맛난데 손이 잘 안가요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036310.1.17",
          "form": "내일 바로 해야겠어요",
          "original_form": "내일 바로 해야겠어요",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036310.1.18",
          "form": "가지가 3개 마침 잇어요",
          "original_form": "가지가 3개 마침 잇어요",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036310.1.19",
          "form": "집에 토마토소스가 있으시면 구워서 토마토소스 치즈랑 드셔도 좋고",
          "original_form": "집에 토마토소스가 있으시면 구워서 토마토소스 치즈랑 드셔도 좋고",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036310.1.20",
          "form": "가지구워서 소금만 뿌리는건 좀 심플한 레시피지만 편해서좋고",
          "original_form": "가지구워서 소금만 뿌리는건 좀 심플한 레시피지만 편해서좋고",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036310.1.21",
          "form": "취향껏하세요 ㅋㅋ",
          "original_form": "취향껏하세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036310.1.22",
          "form": "오호 가지 레시피가 다양하네요",
          "original_form": "오호 가지 레시피가 다양하네요",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036310.1.23",
          "form": "오늘 바로 도전",
          "original_form": "오늘 바로 도전",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100036310.1.24",
          "form": "제가 오늘 좀 바빠서 ㅋㅋ 제가 좀 늦게왔네요",
          "original_form": "제가 오늘 좀 바빠서 ㅋㅋ 제가 좀 늦게왔네요",
          "speaker_id": "1",
          "time": "20210831 20:45"
        },
        {
          "id": "MDRW2100036310.1.25",
          "form": "가지 -토마토는 항상 잘 어울리지만",
          "original_form": "가지 -토마토는 항상 잘 어울리지만",
          "speaker_id": "1",
          "time": "20210831 20:45"
        },
        {
          "id": "MDRW2100036310.1.26",
          "form": "가지육즙은 가지구이자체로도 충분해요",
          "original_form": "가지육즙은 가지구이자체로도 충분해요",
          "speaker_id": "1",
          "time": "20210831 20:45"
        },
        {
          "id": "MDRW2100036310.1.27",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210831 21:14"
        }
      ]
    }
  ]
