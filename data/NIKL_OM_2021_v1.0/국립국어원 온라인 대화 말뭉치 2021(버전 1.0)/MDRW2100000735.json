      "utterance": [
        {
          "id": "MDRW2100000735.1.1",
          "form": "운동 어때요?",
          "original_form": "운동 어때요?",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000735.1.2",
          "form": "운동 좋아하지만 못해요 어떤운동좋아하세요?",
          "original_form": "운동 좋아하지만 못해요 어떤운동좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000735.1.3",
          "form": "전 코로나 때문레 홈트해여",
          "original_form": "전 코로나 때문레 홈트해여",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100000735.1.4",
          "form": "홈트좋죠 유투브 보고 하시나요?",
          "original_form": "홈트좋죠 유투브 보고 하시나요?",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000735.1.5",
          "form": "네 땅크보고 해요",
          "original_form": "네 땅크보고 해요",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000735.1.6",
          "form": "아 저도 땅크부부꺼 몇번봤어요",
          "original_form": "아 저도 땅크부부꺼 몇번봤어요",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000735.1.7",
          "form": "칼소폭?",
          "original_form": "칼소폭?",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000735.1.8",
          "form": "근데 못따라가서 항상 중도포기..",
          "original_form": "근데 못따라가서 항상 중도포기..",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000735.1.9",
          "form": "ㅋㅋ 저도",
          "original_form": "ㅋㅋ 저도",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000735.1.10",
          "form": "그래서 플랭크랑 팔굽혀펴기만 해요",
          "original_form": "그래서 플랭크랑 팔굽혀펴기만 해요",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000735.1.11",
          "form": "우와 두개다 엄청힘들던데 몇개씩하세요?",
          "original_form": "우와 두개다 엄청힘들던데 몇개씩하세요?",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000735.1.12",
          "form": "3세트씩이요 간단히",
          "original_form": "3세트씩이요 간단히",
          "speaker_id": "1",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100000735.1.13",
          "form": "오 대단하시네요",
          "original_form": "오 대단하시네요",
          "speaker_id": "2",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000735.1.14",
          "form": "코로나전에는 즐겨하시거나 좋아하던운동있었나요?",
          "original_form": "코로나전에는 즐겨하시거나 좋아하던운동있었나요?",
          "speaker_id": "2",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000735.1.15",
          "form": "전 수영좋아해요",
          "original_form": "전 수영좋아해요",
          "speaker_id": "2",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000735.1.16",
          "form": "헬스 시작했다가 코로나 터져서 그만요 ㅎㅎ",
          "original_form": "헬스 시작했다가 코로나 터져서 그만요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000735.1.17",
          "form": "아ㅜㅜ맞아요 저도 코로나터지고 수영그만뒀거든요 잠잠해지면 다시할려고했는데",
          "original_form": "아ㅜㅜ맞아요 저도 코로나터지고 수영그만뒀거든요 잠잠해지면 다시할려고했는데",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000735.1.18",
          "form": "안잠잠해지네요ㅜ",
          "original_form": "안잠잠해지네요ㅜ",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000735.1.19",
          "form": "더ㅜ심해지는 거ㅜ같아요",
          "original_form": "더ㅜ심해지는 거ㅜ같아요",
          "speaker_id": "1",
          "time": "20210819 19:09"
        }
      ]
