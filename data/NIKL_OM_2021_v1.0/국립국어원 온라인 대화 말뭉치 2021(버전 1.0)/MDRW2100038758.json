      "utterance": [
        {
          "id": "MDRW2100038758.1.1",
          "form": "최근 잘산템 일으세여",
          "original_form": "최근 잘산템 일으세여",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038758.1.2",
          "form": "아이돌 앨범이요",
          "original_form": "아이돌 앨범이요",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038758.1.3",
          "form": "지르고 나서 후회한적 없습니다ㅋㅋ",
          "original_form": "지르고 나서 후회한적 없습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038758.1.4",
          "form": "오오 저는 아직 안질렀는데",
          "original_form": "오오 저는 아직 안질렀는데",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038758.1.5",
          "form": "누구거사셨나요",
          "original_form": "누구거사셨나요",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038758.1.6",
          "form": "투모로우바이투게더 샀어요",
          "original_form": "투모로우바이투게더 샀어요",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100038758.1.7",
          "form": "아 투바투",
          "original_form": "아 투바투",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100038758.1.8",
          "form": "머리에뿔이자랐다?!",
          "original_form": "머리에뿔이자랐다?!",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100038758.1.9",
          "form": "맞아요 데뷔곡ㅋㅋ",
          "original_form": "맞아요 데뷔곡ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038758.1.10",
          "form": "아 데뷔곡이군요",
          "original_form": "아 데뷔곡이군요",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100038758.1.11",
          "form": "그때부터 앨범은 다 모았어요",
          "original_form": "그때부터 앨범은 다 모았어요",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100038758.1.12",
          "form": "우와대단라시다",
          "original_form": "우와대단라시다",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100038758.1.13",
          "form": "잔 아수트로 앨점 아직 안삿어요ㅠ",
          "original_form": "잔 아수트로 앨점 아직 안삿어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038758.1.14",
          "form": "아로하셨군요",
          "original_form": "아로하셨군요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100038758.1.15",
          "form": "앜ㅋㅋㅋㅋ아로하 아시네여 역시",
          "original_form": "앜ㅋㅋㅋㅋ아로하 아시네여 역시",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100038758.1.16",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100038758.1.17",
          "form": "투바투는 팬클럽 이름 뭐에요",
          "original_form": "투바투는 팬클럽 이름 뭐에요",
          "speaker_id": "1",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100038758.1.18",
          "form": "모아입니다",
          "original_form": "모아입니다",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100038758.1.19",
          "form": "앨범말고는 최근 잘 산템 없으신가용???",
          "original_form": "앨범말고는 최근 잘 산템 없으신가용???",
