      "utterance": [
        {
          "id": "MDRW2100039451.1.1",
          "form": "저 지금도 쇼핑하고 싶어요",
          "original_form": "저 지금도 쇼핑하고 싶어요",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100039451.1.2",
          "form": "쇼핑하고 싶어요",
          "original_form": "쇼핑하고 싶어요",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039451.1.3",
          "form": "어떤거 사구싶길래?",
          "original_form": "어떤거 사구싶길래?",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039451.1.4",
          "form": "배달의 민족",
          "original_form": "배달의 민족",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039451.1.5",
          "form": "나도 배민쇼핑땡긴다",
          "original_form": "나도 배민쇼핑땡긴다",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039451.1.6",
          "form": "최근에 번개장터에서 팔아넘긴 폼롤러요 저도 첨에 중고로 삿엇는데",
          "original_form": "최근에 번개장터에서 팔아넘긴 폼롤러요 저도 첨에 중고로 삿엇는데",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039451.1.7",
          "form": "연어구이도 궁금한데 제가 연어구이용 사서 구울수있을지가",
          "original_form": "연어구이도 궁금한데 제가 연어구이용 사서 구울수있을지가",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039451.1.8",
          "form": "만원에 삿더라고 알고보니",
          "original_form": "만원에 삿더라고 알고보니",
          "speaker_id": "1",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039451.1.9",
          "form": "근데 구천원에 팔앗어요",
          "original_form": "근데 구천원에 팔앗어요",
          "speaker_id": "1",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039451.1.10",
          "form": "중고니까 모 그럴수있죠",
          "original_form": "중고니까 모 그럴수있죠",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039451.1.11",
          "form": "그만하면 잘 팔았네",
          "original_form": "그만하면 잘 팔았네",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039451.1.12",
          "form": "짱인 거 같아요",
          "original_form": "짱인 거 같아요",
          "speaker_id": "1",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039451.1.13",
          "form": "그쵸 잘 팔았어요",
          "original_form": "그쵸 잘 팔았어요",
          "speaker_id": "1",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039451.1.14",
          "form": "최근에 산 핑크폼롤러 너무 좋아",
          "original_form": "최근에 산 핑크폼롤러 너무 좋아",
          "speaker_id": "1",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039451.1.15",
          "form": "아 그리고 샤오미 체중계 샀는데요",
          "original_form": "아 그리고 샤오미 체중계 샀는데요",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039451.1.16",
          "form": "화이트인줄 알았는데 유리녹색끼어있어서 화낫어요",
          "original_form": "화이트인줄 알았는데 유리녹색끼어있어서 화낫어요",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039451.1.17",
          "form": "상품색상에 예민한 바니미",
          "original_form": "상품색상에 예민한 바니미",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039451.1.18",
          "form": "난 예민해 이런 거에 몹시",
          "original_form": "난 예민해 이런 거에 몹시",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039451.1.19",
          "form": "전 얼레벌레 살아서인지 색상구분 님만큼은 힘들어",
          "original_form": "전 얼레벌레 살아서인지 색상구분 님만큼은 힘들어",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100039451.1.20",
          "form": "색깔이 확실하지 않으면 물건을 삿을때 깔맞춤이 안되니까 화난다구요ㅠ",
          "original_form": "색깔이 확실하지 않으면 물건을 삿을때 깔맞춤이 안되니까 화난다구요ㅠ",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100039451.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 쇼핑 목적이 깔맞춤이야ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 쇼핑 목적이 깔맞춤이야ㅠ",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039451.1.22",
          "form": "당연하죠 인테리어의 기본은 깔맞춤인걸!",
          "original_form": "당연하죠 인테리어의 기본은 깔맞춤인걸!",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039451.1.23",
          "form": "저는 단호박죽 샀지롱",
          "original_form": "저는 단호박죽 샀지롱",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039451.1.24",
          "form": "당신의 쇼핑은 단호박죽이었군요",
          "original_form": "당신의 쇼핑은 단호박죽이었군요",
          "speaker_id": "1",
          "time": "20210903 18:02"
        }
      ]
    }
  ]
