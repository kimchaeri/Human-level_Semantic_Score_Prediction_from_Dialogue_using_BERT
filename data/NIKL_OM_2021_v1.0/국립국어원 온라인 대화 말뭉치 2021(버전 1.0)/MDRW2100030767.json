      "utterance": [
        {
          "id": "MDRW2100030767.1.1",
          "form": "가방도 사야 해요",
          "original_form": "가방도 사야 해요",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.2",
          "form": "S 펜 시리즈 중에 뭐 나비 펜 모양으로 나온 거 보셨나요",
          "original_form": "S 펜 시리즈 중에 뭐 나비 펜 모양으로 나온 거 보셨나요",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.3",
          "form": "지금 에코백 하나 크로스백 하나 있어요",
          "original_form": "지금 에코백 하나 크로스백 하나 있어요",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.4",
          "form": "모나미",
          "original_form": "모나미",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.5",
          "form": "모나미 콜라보인데 볼펜처럼 생겨서 정말 편해 보이더라고요",
          "original_form": "모나미 콜라보인데 볼펜처럼 생겨서 정말 편해 보이더라고요",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.6",
          "form": "저는 잘 모르긴 해요",
          "original_form": "저는 잘 모르긴 해요",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.7",
          "form": "전 이민 가방 아직 안 써 봤습니다",
          "original_form": "전 이민 가방 아직 안 써 봤습니다",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.8",
          "form": "엄두가 나질 않네요",
          "original_form": "엄두가 나질 않네요",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.9",
          "form": "쓰는 것만 써서요",
          "original_form": "쓰는 것만 써서요",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.10",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.11",
          "form": "너무 큰가요",
          "original_form": "너무 큰가요",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.12",
          "form": "S 펜 펜촉 또 갈 수 있는 거 아시나요",
          "original_form": "S 펜 펜촉 또 갈 수 있는 거 아시나요",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.13",
          "form": "그래도 몇 천 원 내고 샀는데",
          "original_form": "그래도 몇 천 원 내고 샀는데",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.14",
          "form": "인터넷에서 싸게 구매할 수 있습니다",
          "original_form": "인터넷에서 싸게 구매할 수 있습니다",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100030767.1.15",
          "form": "네 펜촉을 더 주더라고요",
          "original_form": "네 펜촉을 더 주더라고요",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100030767.1.16",
          "form": "샀을 때 들어 있어요",
          "original_form": "샀을 때 들어 있어요",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100030767.1.17",
          "form": "가방 써 보고 싶었는데 귀찮아서 안 해도 물건들을 다시 넣기 귀찮아서요",
          "original_form": "가방 써 보고 싶었는데 귀찮아서 안 해도 물건들을 다시 넣기 귀찮아서요",
          "speaker_id": "2",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100030767.1.18",
          "form": "그냥 플라스틱이 더 군요",
          "original_form": "그냥 플라스틱이 더 군요",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100030767.1.19",
          "form": "원래쓰던 가방에 있던 물건들이 그 가방으로 옮기려고 하니 그게 귀찮아서요",
          "original_form": "원래쓰던 가방에 있던 물건들이 그 가방으로 옮기려고 하니 그게 귀찮아서요",
          "speaker_id": "2",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100030767.1.20",
          "form": "역시 귀차니즘",
          "original_form": "역시 귀차니즘",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100030767.1.21",
          "form": "저는 백팩이 사고 싶어요",
          "original_form": "저는 백팩이 사고 싶어요",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100030767.1.22",
          "form": "그게 두 손이 자유로워서",
          "original_form": "그게 두 손이 자유로워서",
          "speaker_id": "1",
          "time": "20210904 12:24"
        }
      ]
