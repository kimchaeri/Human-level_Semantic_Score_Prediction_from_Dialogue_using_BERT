        {
          "id": "MDRW2100004767.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004767.1.2",
          "form": "요즘 당근마켓 이용하고 있어요",
          "original_form": "요즘 당근마켓 이용하고 있어요",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004767.1.3",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004767.1.4",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004767.1.5",
          "form": "당근에서 주로 뮤 거래하솄어요?",
          "original_form": "당근에서 주로 뮤 거래하솄어요?",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004767.1.6",
          "form": "공기청정기 최근요",
          "original_form": "공기청정기 최근요",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004767.1.7",
          "form": "오 저는 책 팔았어요",
          "original_form": "오 저는 책 팔았어요",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004767.1.8",
          "form": "수요 있나여?",
          "original_form": "수요 있나여?",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004767.1.9",
          "form": "게시글 4개 올렸는데 한세트 팔았어요ㅠㅠ",
          "original_form": "게시글 4개 올렸는데 한세트 팔았어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004767.1.10",
          "form": "책 많이 안 봐서",
          "original_form": "책 많이 안 봐서",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004767.1.11",
          "form": "와이책 팔았어요",
          "original_form": "와이책 팔았어요",
          "speaker_id": "2",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004767.1.12",
          "form": "내용이?",
          "original_form": "내용이?",
          "speaker_id": "1",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004767.1.13",
          "form": "그 학습만화책이요",
          "original_form": "그 학습만화책이요",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004767.1.14",
          "form": "아..",
          "original_form": "아..",
