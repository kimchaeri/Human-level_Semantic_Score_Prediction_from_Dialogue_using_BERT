      "utterance": [
        {
          "id": "MDRW2100003001.1.1",
          "form": "카페 잘가세요?",
          "original_form": "카페 잘가세요?",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100003001.1.2",
          "form": "시간 날 때 가는데 자주는 안가요",
          "original_form": "시간 날 때 가는데 자주는 안가요",
          "speaker_id": "2",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100003001.1.3",
          "form": "카페 좋아하세요?",
          "original_form": "카페 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100003001.1.4",
          "form": "저도 찾아가는건 아니지만",
          "original_form": "저도 찾아가는건 아니지만",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003001.1.5",
          "form": "주변에서 가자하면 잘가요 ㅋㅋ",
          "original_form": "주변에서 가자하면 잘가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003001.1.6",
          "form": "카페가면 주로 뭐 마시나요",
          "original_form": "카페가면 주로 뭐 마시나요",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003001.1.7",
          "form": "저는 커피는 못마셔서 에이드종류 마셔요",
          "original_form": "저는 커피는 못마셔서 에이드종류 마셔요",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003001.1.8",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210820 17:47"
        },
        {
          "id": "MDRW2100003001.1.9",
          "form": "마시긴하는데 커피를 안좋아해서",
          "original_form": "마시긴하는데 커피를 안좋아해서",
          "speaker_id": "1",
          "time": "20210820 17:47"
        },
        {
          "id": "MDRW2100003001.1.10",
          "form": "음료 위주로 마셔요",
          "original_form": "음료 위주로 마셔요",
          "speaker_id": "1",
          "time": "20210820 17:47"
        },
        {
          "id": "MDRW2100003001.1.11",
          "form": "어떤거 마시나요?",
          "original_form": "어떤거 마시나요?",
          "speaker_id": "2",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003001.1.12",
          "form": "에이드? 쥬스? 스무디?",
          "original_form": "에이드? 쥬스? 스무디?",
          "speaker_id": "2",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003001.1.13",
          "form": "보통은 과일주스 좋아해서",
          "original_form": "보통은 과일주스 좋아해서",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003001.1.14",
          "form": "잘 마셔요",
          "original_form": "잘 마셔요",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003001.1.15",
          "form": "과일주스 너무맛있죠",
          "original_form": "과일주스 너무맛있죠",
          "speaker_id": "2",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003001.1.16",
          "form": "카페디저트 음.. 케익류도 즐기시는 편인가요?",
          "original_form": "카페디저트 음.. 케익류도 즐기시는 편인가요?",
          "speaker_id": "2",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003001.1.17",
          "form": "음 아뇽 전 디저트는 잘 ..",
          "original_form": "음 아뇽 전 디저트는 잘 ..",
          "speaker_id": "1",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003001.1.18",
          "form": "name2님은 ?",
          "original_form": "&name2&님은 ?",
          "speaker_id": "1",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003001.1.19",
          "form": "저는 케익류도 좋아해서ㅎㅎ",
          "original_form": "저는 케익류도 좋아해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003001.1.20",
          "form": "같이 즐기는 편이에요",
          "original_form": "같이 즐기는 편이에요",
          "speaker_id": "2",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003001.1.21",
          "form": "그럼 카페가면 음료만 드시는 편이신가봐요",
          "original_form": "그럼 카페가면 음료만 드시는 편이신가봐요",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003001.1.22",
          "form": "네네 아님 씹어먹는 쿠키정도",
          "original_form": "네네 아님 씹어먹는 쿠키정도",
          "speaker_id": "1",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003001.1.23",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003001.1.24",
          "form": "쿠키도 좋죠",
          "original_form": "쿠키도 좋죠",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003001.1.25",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003001.1.26",
          "form": "요즘 카페가 정말 많은거같아요",
          "original_form": "요즘 카페가 정말 많은거같아요",
          "speaker_id": "1",
