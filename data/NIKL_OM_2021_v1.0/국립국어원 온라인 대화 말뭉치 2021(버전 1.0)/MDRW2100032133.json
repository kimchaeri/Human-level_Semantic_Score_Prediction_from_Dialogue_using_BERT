      "utterance": [
        {
          "id": "MDRW2100032133.1.1",
          "form": "아직 모르겠네",
          "original_form": "아직 모르겠네",
          "speaker_id": "2",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100032133.1.2",
          "form": "시골일 다끝나고 벌초 오겠지",
          "original_form": "시골일 다끝나고 벌초 오겠지",
          "speaker_id": "2",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100032133.1.3",
          "form": "늦겠네?",
          "original_form": "늦겠네?",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032133.1.4",
          "form": "아침에 나간것도 몰랐어",
          "original_form": "아침에 나간것도 몰랐어",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032133.1.5",
          "form": "헉 가족에게  관심 좀 가지시지요",
          "original_form": "헉 가족에게  관심 좀 가지시지요",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032133.1.6",
          "form": "ㅠㅠ 쏘리용",
          "original_form": "ㅠㅠ 쏘리용",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032133.1.7",
          "form": "담주가 추석이라 그런가?",
          "original_form": "담주가 추석이라 그런가?",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032133.1.8",
          "form": "벌초능?",
          "original_form": "벌초능?",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032133.1.9",
          "form": "그럼 추석전에 해야지",
          "original_form": "그럼 추석전에 해야지",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032133.1.10",
          "form": "담주는 친척 결혼식이 있어서 이번주에 한대",
          "original_form": "담주는 친척 결혼식이 있어서 이번주에 한대",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100032133.1.11",
          "form": "아 누구 결혼해? 몰랏네",
          "original_form": "아 누구 결혼해? 몰랏네",
          "speaker_id": "1",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032133.1.12",
          "form": "나두 가?",
          "original_form": "나두 가?",
          "speaker_id": "1",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032133.1.13",
          "form": "아니 안가 못가",
          "original_form": "아니 안가 못가",
          "speaker_id": "2",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032133.1.14",
          "form": "아빠도 못갈것",
          "original_form": "아빠도 못갈것",
          "speaker_id": "2",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032133.1.15",
          "form": "백신 맞고 쉬어야지",
          "original_form": "백신 맞고 쉬어야지",
          "speaker_id": "2",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032133.1.16",
          "form": "ㅇㅎㅇㅎ 맞다 나참 나는 일요일에 학교 갈수도 있어",
          "original_form": "ㅇㅎㅇㅎ 맞다 나참 나는 일요일에 학교 갈수도 있어",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100032133.1.17",
          "form": "잠깐?",
          "original_form": "잠깐?",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100032133.1.18",
          "form": "무슨 프로그램 있어가지고",
          "original_form": "무슨 프로그램 있어가지고",
          "speaker_id": "1",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100032133.1.19",
          "form": "아빠는 집에 도착했어",
          "original_form": "아빠는 집에 도착했어",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032133.1.20",
          "form": "헐ㅋㅋㅋ델구가라구할걸ㅠ",
          "original_form": "헐ㅋㅋㅋ델구가라구할걸ㅠ",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032133.1.21",
          "form": "으이그 진작 얘기하지",
          "original_form": "으이그 진작 얘기하지",
          "speaker_id": "2",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100032133.1.22",
          "form": "운동 하러 나간다",
          "original_form": "운동 하러 나간다",
          "speaker_id": "2",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100032133.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ버스타고 지금 집옴",
          "original_form": "ㅋㅋㅋㅋㅋㅋ버스타고 지금 집옴",
          "speaker_id": "1",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032133.1.24",
          "form": "집에 오면 집 정리 부탁한다",
          "original_form": "집에 오면 집 정리 부탁한다",
          "speaker_id": "2",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032133.1.25",
          "form": "내일 아침에 일어나면 깨워줭",
          "original_form": "내일 아침에 일어나면 깨워줭",
          "speaker_id": "1",
          "time": "20210905 22:03"
        }
      ]
    }
