      "utterance": [
        {
          "id": "MDRW2100019826.1.1",
          "form": "월드컵이랑 동계올림픽 아시안게임 다하더라고요 ㅋㅋㅋㅋ",
          "original_form": "월드컵이랑 동계올림픽 아시안게임 다하더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019826.1.2",
          "form": "와 내년에 세개나 하는거에요?",
          "original_form": "와 내년에 세개나 하는거에요?",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019826.1.3",
          "form": "네 ㅋㅋㅋ엄청나죠",
          "original_form": "네 ㅋㅋㅋ엄청나죠",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019826.1.4",
          "form": "선수들도 힘들겠네요ㅠ",
          "original_form": "선수들도 힘들겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019826.1.5",
          "form": "그것도 그래요 ㅜㅜ",
          "original_form": "그것도 그래요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019826.1.6",
          "form": "근데 보는 입장에선 너무 설레고 기대돼요 ㅋㅋㅋ",
          "original_form": "근데 보는 입장에선 너무 설레고 기대돼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.7",
          "form": "볼거는 엄청 많겠어요",
          "original_form": "볼거는 엄청 많겠어요",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.8",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.9",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.10",
          "form": "원래 피려좋아햇는데",
          "original_form": "원래 피려좋아햇는데",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.11",
          "form": "피겨",
          "original_form": "피겨",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.12",
          "form": "김연아 선수가 안나오니..ㅠㅠ아쉬워요",
          "original_form": "김연아 선수가 안나오니..ㅠㅠ아쉬워요",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.13",
          "form": "이제 김연아 선수 안나와서ㅠ",
          "original_form": "이제 김연아 선수 안나와서ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.14",
          "form": "맞아요 꽤됐죠 ㅜㅜ",
          "original_form": "맞아요 꽤됐죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019826.1.15",
          "form": "또 새로운 선수가 빛내주겠죠?ㅠ",
          "original_form": "또 새로운 선수가 빛내주겠죠?ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019826.1.16",
          "form": "얼른 그랬으면 좋겠어요",
          "original_form": "얼른 그랬으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019826.1.17",
          "form": "잘하는 선수가 있어야 볼맛나더라구요",
          "original_form": "잘하는 선수가 있어야 볼맛나더라구요",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019826.1.18",
          "form": "마자요 그래도 잘해야 재밌어요\r\n저는 쇼트트랙도 재밌더라구요",
          "original_form": "마자요 그래도 잘해야 재밌어요\r\n저는 쇼트트랙도 재밌더라구요",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019826.1.19",
          "form": "아 맞아요 이상화선수 생각나네요 ㅋㅋㅋ",
          "original_form": "아 맞아요 이상화선수 생각나네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019826.1.20",
          "form": "진짜  멋있는거같아요ㅠ",
          "original_form": "진짜  멋있는거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019826.1.21",
          "form": "배구 인기 많더라구요!",
          "original_form": "배구 인기 많더라구요!",
          "speaker_id": "1",
          "time": "20210827 22:18"
        }
      ]
