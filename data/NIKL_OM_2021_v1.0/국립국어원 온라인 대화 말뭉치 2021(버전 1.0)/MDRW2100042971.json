      "utterance": [
        {
          "id": "MDRW2100042971.1.1",
          "form": "안녕안녕 운동 하는거 있오??",
          "original_form": "안녕안녕 운동 하는거 있오??",
          "speaker_id": "1",
          "time": "20210907 12:17"
        },
        {
          "id": "MDRW2100042971.1.2",
          "form": "홈트행 너는 ?",
          "original_form": "홈트행 너는 ?",
          "speaker_id": "2",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100042971.1.3",
          "form": "우와 유튜브 보고 해? 난 매일 산책이랑 댄스레슨 받앙",
          "original_form": "우와 유튜브 보고 해? 난 매일 산책이랑 댄스레슨 받앙",
          "speaker_id": "1",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100042971.1.4",
          "form": "웅 유투브",
          "original_form": "웅 유투브",
          "speaker_id": "2",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042971.1.5",
          "form": "댄스레슨 ?",
          "original_form": "댄스레슨 ?",
          "speaker_id": "2",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042971.1.6",
          "form": "대박 나도 받아보고싶다",
          "original_form": "대박 나도 받아보고싶다",
          "speaker_id": "2",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042971.1.7",
          "form": "어어 완전 재밌어",
          "original_form": "어어 완전 재밌어",
          "speaker_id": "1",
          "time": "20210907 12:46"
        },
        {
          "id": "MDRW2100042971.1.8",
          "form": "일주일에 몇번이나 가?",
          "original_form": "일주일에 몇번이나 가?",
          "speaker_id": "2",
          "time": "20210907 12:46"
        },
        {
          "id": "MDRW2100042971.1.9",
          "form": "일주일에 한번 가",
          "original_form": "일주일에 한번 가",
          "speaker_id": "1",
          "time": "20210907 12:51"
        },
        {
          "id": "MDRW2100042971.1.10",
          "form": "지금 백신 일정 때문에 미뤄졌어",
          "original_form": "지금 백신 일정 때문에 미뤄졌어",
          "speaker_id": "1",
          "time": "20210907 12:51"
        },
        {
          "id": "MDRW2100042971.1.11",
          "form": "비용은 ?",
          "original_form": "비용은 ?",
          "speaker_id": "2",
          "time": "20210907 12:52"
        },
        {
          "id": "MDRW2100042971.1.12",
          "form": "비용은 1시간에 1인당 3만원 이야",
          "original_form": "비용은 1시간에 1인당 3만원 이야",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042971.1.13",
          "form": "2명이서 하고등",
          "original_form": "2명이서 하고등",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042971.1.14",
          "form": "쌤은 1시간당 6만원받으시능고지",
          "original_form": "쌤은 1시간당 6만원받으시능고지",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042971.1.15",
          "form": "오호 그럼 요즘노래안무들",
          "original_form": "오호 그럼 요즘노래안무들",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042971.1.16",
          "form": "배우는거야?",
          "original_form": "배우는거야?",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042971.1.17",
          "form": "요즘은 아스트로 좋아해서",
          "original_form": "요즘은 아스트로 좋아해서",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100042971.1.18",
          "form": "애프터 미드나잇 배웠어",
          "original_form": "애프터 미드나잇 배웠어",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100042971.1.19",
          "form": "올 춤원래잘췄어 ?",
          "original_form": "올 춤원래잘췄어 ?",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100042971.1.20",
          "form": "춤 관심 많았어",
          "original_form": "춤 관심 많았어",
          "speaker_id": "1",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100042971.1.21",
          "form": "어릴때도 합창단 했어서 율동 마니 했어 ㅋㅋㅋ",
          "original_form": "어릴때도 합창단 했어서 율동 마니 했어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100042971.1.22",
          "form": "신기하다 ㅋㅋ",
          "original_form": "신기하다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:58"
        }
