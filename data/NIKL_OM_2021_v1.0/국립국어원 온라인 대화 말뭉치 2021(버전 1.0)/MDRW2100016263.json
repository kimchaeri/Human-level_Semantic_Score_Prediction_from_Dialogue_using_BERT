      "utterance": [
        {
          "id": "MDRW2100016263.1.1",
          "form": "메타버스 잘 아시나요",
          "original_form": "메타버스 잘 아시나요",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016263.1.2",
          "form": "저 잘몰라요..  로블록스얘기해도되나요",
          "original_form": "저 잘몰라요..  로블록스얘기해도되나요",
          "speaker_id": "2",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016263.1.3",
          "form": "에스파",
          "original_form": "에스파",
          "speaker_id": "2",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100016263.1.4",
          "form": "로블록스가 메타버스의 선두주자니까 되지 않을까요",
          "original_form": "로블록스가 메타버스의 선두주자니까 되지 않을까요",
          "speaker_id": "1",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016263.1.5",
          "form": "물렸던 거 탈출 축하드립니다",
          "original_form": "물렸던 거 탈출 축하드립니다",
          "speaker_id": "1",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016263.1.6",
          "form": "감사합니다  혹시 로블록스 나중에 다시 사실건가요",
          "original_form": "감사합니다  혹시 로블록스 나중에 다시 사실건가요",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016263.1.7",
          "form": "일단 당분간은 안 사는데 다시 살거같아요",
          "original_form": "일단 당분간은 안 사는데 다시 살거같아요",
          "speaker_id": "1",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016263.1.8",
          "form": "메타버스의세계는 무궁무진하죠",
          "original_form": "메타버스의세계는 무궁무진하죠",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016263.1.9",
          "form": "에스파는 나야아 둘이 될 수 없어어어",
          "original_form": "에스파는 나야아 둘이 될 수 없어어어",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100016263.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016263.1.11",
          "form": "학교에서 메타버스로 입학식하던걸요",
          "original_form": "학교에서 메타버스로 입학식하던걸요",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016263.1.12",
          "form": "그럼 주식 사야겠다는 생각이 들어요",
          "original_form": "그럼 주식 사야겠다는 생각이 들어요",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016263.1.13",
          "form": "우와 메타버스입학식이라니..",
          "original_form": "우와 메타버스입학식이라니..",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016263.1.14",
          "form": "라떼는..",
          "original_form": "라떼는..",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016263.1.15",
          "form": "라떼는 운동장에 세워놓고 픽픽 쓰러져갔지",
          "original_form": "라떼는 운동장에 세워놓고 픽픽 쓰러져갔지",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016263.1.16",
          "form": "그러게 요즘 좋은 세상이야..",
          "original_form": "그러게 요즘 좋은 세상이야..",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016263.1.17",
          "form": "메타버스로 입학식도 하지만",
          "original_form": "메타버스로 입학식도 하지만",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016263.1.18",
          "form": "청와대에서 어린이날 행사한다고",
          "original_form": "청와대에서 어린이날 행사한다고",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016263.1.19",
          "form": "마인크래프트 서버열더라구요",
          "original_form": "마인크래프트 서버열더라구요",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016263.1.20",
          "form": "마인크래프트 해보셨어요?",
          "original_form": "마인크래프트 해보셨어요?",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016263.1.21",
          "form": "안해봤어요",
          "original_form": "안해봤어요",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016263.1.22",
          "form": "그거재밌나요?",
          "original_form": "그거재밌나요?",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016263.1.23",
          "form": "그냥 초등학생용 게임같긴한데",
          "original_form": "그냥 초등학생용 게임같긴한데",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016263.1.24",
          "form": "레고 좋아했다면 좋아할거같은 게임이에요",
          "original_form": "레고 좋아했다면 좋아할거같은 게임이에요",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016263.1.25",
          "form": "레고처럼 집지을 수 있는데 온라인 메타버스 버전",
          "original_form": "레고처럼 집지을 수 있는데 온라인 메타버스 버전",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016263.1.26",
          "form": "악 레고 환장해요",
          "original_form": "악 레고 환장해요",
          "speaker_id": "2",
          "time": "20210826 13:07"
        }
      ]
    }
