      "utterance": [
        {
          "id": "MDRW2100004055.1.1",
          "form": "최근 구매한 게 있나요?",
          "original_form": "최근 구매한 게 있나요?",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004055.1.2",
          "form": "티셔츠 구매했어요",
          "original_form": "티셔츠 구매했어요",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004055.1.3",
          "form": "반팔티를 샀었는데 배송이 오래걸려서 여름이 다 가버렸네요",
          "original_form": "반팔티를 샀었는데 배송이 오래걸려서 여름이 다 가버렸네요",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004055.1.4",
          "form": "구매하신거 있으세요?",
          "original_form": "구매하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004055.1.5",
          "form": "아직 낮은 더워서 괜찮지 않아요? 하긴 금방 가을이네요",
          "original_form": "아직 낮은 더워서 괜찮지 않아요? 하긴 금방 가을이네요",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004055.1.6",
          "form": "저는 식염수랑 불소 치약 샀어요",
          "original_form": "저는 식염수랑 불소 치약 샀어요",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004055.1.7",
          "form": "불소치약 어때요",
          "original_form": "불소치약 어때요",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004055.1.8",
          "form": "제가 요즘 충치가 자주 생기거든요",
          "original_form": "제가 요즘 충치가 자주 생기거든요",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004055.1.9",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004055.1.10",
          "form": "그래서 샀어요 어린이 불소 치약 ㅋㅋㅋ",
          "original_form": "그래서 샀어요 어린이 불소 치약 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004055.1.11",
          "form": "불소치약도 어린이용이 있어요?",
          "original_form": "불소치약도 어린이용이 있어요?",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004055.1.12",
          "form": "처음 알았어요",
          "original_form": "처음 알았어요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004055.1.13",
          "form": "불소 향은 어때요?",
          "original_form": "불소 향은 어때요?",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004055.1.14",
          "form": "네 그게 함량이 많대요",
          "original_form": "네 그게 함량이 많대요",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004055.1.15",
          "form": "효과 본 후기도 봤구요",
          "original_form": "효과 본 후기도 봤구요",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004055.1.16",
          "form": "오 저도 한 번 써봐야겠어요",
          "original_form": "오 저도 한 번 써봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004055.1.17",
          "form": "사과향이랑 소다향 샀어요",
          "original_form": "사과향이랑 소다향 샀어요",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004055.1.18",
          "form": "먼저 쓴 향이 어떤거에요?",
          "original_form": "먼저 쓴 향이 어떤거에요?",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004055.1.19",
          "form": "사과향요",
          "original_form": "사과향요",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004055.1.20",
          "form": "아주 미미하지만요",
          "original_form": "아주 미미하지만요",
          "speaker_id": "2",
