        },
        {
          "id": "MDRW2100022062.1.3",
          "form": "어떤 사람",
          "original_form": "어떤 사람",
          "speaker_id": "1",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100022062.1.4",
          "form": "정확히는 사람들",
          "original_form": "정확히는 사람들",
          "speaker_id": "2",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100022062.1.5",
          "form": "뭐 하고 있었는데",
          "original_form": "뭐 하고 있었는데",
          "speaker_id": "1",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100022062.1.6",
          "form": "다들 핸드폰을 보면서 땅바닥을 걷던데",
          "original_form": "다들 핸드폰을 보면서 땅바닥을 걷던데",
          "speaker_id": "2",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100022062.1.7",
          "form": "그거 아마 피카츄 게임할거야",
          "original_form": "그거 아마 피카츄 게임할거야",
          "speaker_id": "1",
          "time": "20210829 01:48"
        },
        {
          "id": "MDRW2100022062.1.8",
          "form": "무슨 소리야",
          "original_form": "무슨 소리야",
          "speaker_id": "2",
          "time": "20210829 01:49"
        },
        {
          "id": "MDRW2100022062.1.9",
          "form": "너는 메타 버스도 모르니",
          "original_form": "너는 메타 버스도 모르니",
          "speaker_id": "1",
          "time": "20210829 01:49"
        },
        {
          "id": "MDRW2100022062.1.10",
          "form": "그게 뭔데",
          "original_form": "그게 뭔데",
          "speaker_id": "2",
          "time": "20210829 01:49"
        },
        {
          "id": "MDRW2100022062.1.11",
          "form": "가상 세계인데 거기서 게임을 할 수 있어",
          "original_form": "가상 세계인데 거기서 게임을 할 수 있어",
          "speaker_id": "1",
          "time": "20210829 01:49"
        },
        {
          "id": "MDRW2100022062.1.12",
          "form": "그럼 그 사람들이 게임을 하고 있던 거구나",
          "original_form": "그럼 그 사람들이 게임을 하고 있던 거구나",
