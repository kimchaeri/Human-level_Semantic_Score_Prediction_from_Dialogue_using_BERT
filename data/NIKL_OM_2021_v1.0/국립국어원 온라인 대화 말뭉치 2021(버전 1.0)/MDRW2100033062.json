      "utterance": [
        {
          "id": "MDRW2100033062.1.1",
          "form": "음 담주 거제도 벌베디어 예약해놧서용",
          "original_form": "음 담주 거제도 벌베디어 예약해놧서용",
          "speaker_id": "2",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100033062.1.2",
          "form": "거제도 한번 다녀오려구요",
          "original_form": "거제도 한번 다녀오려구요",
          "speaker_id": "2",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100033062.1.3",
          "form": "과학기술인공제회에서 예매해서 한 2만원 이득봣습니다 ㅋㅋㅋ",
          "original_form": "과학기술인공제회에서 예매해서 한 2만원 이득봣습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100033062.1.4",
          "form": "우와......... 부럽..거제도... 벌베디어?",
          "original_form": "우와......... 부럽..거제도... 벌베디어?",
          "speaker_id": "1",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100033062.1.5",
          "form": "리조트인가요",
          "original_form": "리조트인가요",
          "speaker_id": "1",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100033062.1.6",
          "form": "네 한화리조트입니당",
          "original_form": "네 한화리조트입니당",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100033062.1.7",
          "form": "name1 박사님은 뭔계획있나용",
          "original_form": "&name1& 박사님은 뭔계획있나용",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100033062.1.8",
          "form": "아니오..부럼네요.....",
          "original_form": "아니오..부럼네요.....",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100033062.1.9",
          "form": "거제도 ㅜㅡㅜ",
          "original_form": "거제도 ㅜㅡㅜ",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100033062.1.10",
          "form": "그런덴 얼마에용..",
          "original_form": "그런덴 얼마에용..",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100033062.1.11",
          "form": "그렇게 안비싸요",
          "original_form": "그렇게 안비싸요",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100033062.1.12",
          "form": "20만원이면 하루 잠",
          "original_form": "20만원이면 하루 잠",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100033062.1.13",
          "form": "옹.. 가족여행요?",
          "original_form": "옹.. 가족여행요?",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100033062.1.14",
          "form": "네 와이프가 가고싶대서",
          "original_form": "네 와이프가 가고싶대서",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033062.1.15",
          "form": "보내고 저는 자유!",
          "original_form": "보내고 저는 자유!",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033062.1.16",
          "form": "전 늦게 출발!",
          "original_form": "전 늦게 출발!",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033062.1.17",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033062.1.18",
          "form": "ㅋㅋㅋ왜늦게출발이요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ왜늦게출발이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100033062.1.19",
          "form": "오랜만에 한잔 하려구요 ㅋㅋ",
          "original_form": "오랜만에 한잔 하려구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033062.1.20",
          "form": "ㅋㅋ 허락해주나요",
          "original_form": "ㅋㅋ 허락해주나요",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033062.1.21",
          "form": "여행 가시죠 먹짱박사님",
          "original_form": "여행 가시죠 먹짱박사님",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033062.1.22",
          "form": "네 ㅋㅋ해줍니다",
          "original_form": "네 ㅋㅋ해줍니다",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033062.1.23",
          "form": "ㅋㅋㅋ 가고파요..",
          "original_form": "ㅋㅋㅋ 가고파요..",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100033062.1.24",
          "form": "가족 여수가기로햇엇는데",
          "original_form": "가족 여수가기로햇엇는데",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033062.1.25",
          "form": "오 여수 굿",
          "original_form": "오 여수 굿",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033062.1.26",
          "form": "가지말쟤요...코로나땜에....흥",
          "original_form": "가지말쟤요...코로나땜에....흥",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033062.1.27",
          "form": "바다보고싶은디 ㅋㅋㅋ",
          "original_form": "바다보고싶은디 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:26"
        }
      ]
    }
