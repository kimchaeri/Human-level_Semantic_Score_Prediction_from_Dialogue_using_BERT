      "utterance": [
        {
          "id": "MDRW2100003699.1.1",
          "form": "자신있는 요리있을까여",
          "original_form": "자신있는 요리있을까여",
          "speaker_id": "1",
          "time": "20210821 10:55"
        },
        {
          "id": "MDRW2100003699.1.2",
          "form": "전 요리가 다 실험입니다",
          "original_form": "전 요리가 다 실험입니다",
          "speaker_id": "2",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003699.1.3",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003699.1.4",
          "form": "ㅋㅋㅋㅋ저도 다 실험적으로ㅎㅎ잘하지못하고 좋아는해요",
          "original_form": "ㅋㅋㅋㅋ저도 다 실험적으로ㅎㅎ잘하지못하고 좋아는해요",
          "speaker_id": "1",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003699.1.5",
          "form": "그나마 감자탕 잘해요",
          "original_form": "그나마 감자탕 잘해요",
          "speaker_id": "2",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003699.1.6",
          "form": "다때려넣는 전골같은건 맛이괜찮더라구여",
          "original_form": "다때려넣는 전골같은건 맛이괜찮더라구여",
          "speaker_id": "1",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003699.1.7",
          "form": "아들이 오늘도 실험하냐고 이상한거 하지 마래요",
          "original_form": "아들이 오늘도 실험하냐고 이상한거 하지 마래요",
          "speaker_id": "2",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003699.1.8",
          "form": "감자탕 손 많이가지 않나요",
          "original_form": "감자탕 손 많이가지 않나요",
          "speaker_id": "1",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003699.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 10:57"
        },
        {
          "id": "MDRW2100003699.1.10",
          "form": "제가 그래요 다때려 넣어요",
          "original_form": "제가 그래요 다때려 넣어요",
          "speaker_id": "2",
          "time": "20210821 10:57"
        },
        {
          "id": "MDRW2100003699.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도 맨날 실험하지말라고 타박을받아여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도 맨날 실험하지말라고 타박을받아여",
          "speaker_id": "1",
          "time": "20210821 10:57"
        },
        {
          "id": "MDRW2100003699.1.12",
          "form": "그래도 다때려넣으면 중간은가더라구여 ㅋㅋㅋㅋ",
          "original_form": "그래도 다때려넣으면 중간은가더라구여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 10:58"
        },
        {
          "id": "MDRW2100003699.1.13",
          "form": "참나 레시피 보는데 그러네요",
          "original_form": "참나 레시피 보는데 그러네요",
          "speaker_id": "2",
          "time": "20210821 10:58"
        },
        {
          "id": "MDRW2100003699.1.14",
          "form": "ㅋㅋㅋㅋㅋ마자요 레시피보긴봐여 저도",
          "original_form": "ㅋㅋㅋㅋㅋ마자요 레시피보긴봐여 저도",
          "speaker_id": "1",
          "time": "20210821 10:59"
        },
        {
          "id": "MDRW2100003699.1.15",
          "form": "오늘같은날은 국물있는 요리가 좀땡기네요",
          "original_form": "오늘같은날은 국물있는 요리가 좀땡기네요",
          "speaker_id": "1",
          "time": "20210821 10:59"
        },
        {
          "id": "MDRW2100003699.1.16",
          "form": "캬 그렇죠 술도 한잔",
          "original_form": "캬 그렇죠 술도 한잔",
          "speaker_id": "2",
          "time": "20210821 11:01"
        },
        {
          "id": "MDRW2100003699.1.17",
          "form": "근데 보면 저 레시피 안지켜요 ㅋㅋ",
          "original_form": "근데 보면 저 레시피 안지켜요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:01"
        },
        {
          "id": "MDRW2100003699.1.18",
          "form": "저도 보긴볼뿐 ㅋㅋㅋㅋㅋㅋㅋㅋ지킨다고는안했어요",
          "original_form": "저도 보긴볼뿐 ㅋㅋㅋㅋㅋㅋㅋㅋ지킨다고는안했어요",
          "speaker_id": "1",
          "time": "20210821 11:02"
        },
        {
          "id": "MDRW2100003699.1.19",
          "form": "이게 재료가 다있는게아니니까여 ^^",
          "original_form": "이게 재료가 다있는게아니니까여 ^^",
          "speaker_id": "1",
          "time": "20210821 11:03"
        },
        {
          "id": "MDRW2100003699.1.20",
          "form": "맞아요 이것저것 대체해요 ㅋㅋ",
          "original_form": "맞아요 이것저것 대체해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:03"
        },
        {
          "id": "MDRW2100003699.1.21",
          "form": "아저랑 완전 비슷해요",
          "original_form": "아저랑 완전 비슷해요",
          "speaker_id": "2",
          "time": "20210821 11:03"
        },
        {
          "id": "MDRW2100003699.1.22",
          "form": "최선을 다해요 ㅎㅎ",
          "original_form": "최선을 다해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:03"
        },
        {
          "id": "MDRW2100003699.1.23",
          "form": "다들 몰라줘요",
          "original_form": "다들 몰라줘요",
          "speaker_id": "2",
          "time": "20210821 11:04"
        },
        {
          "id": "MDRW2100003699.1.24",
          "form": "그 훠궈도 기냥 육수 사서 재료 퐁당하면 되서 좋아요",
          "original_form": "그 훠궈도 기냥 육수 사서 재료 퐁당하면 되서 좋아요",
          "speaker_id": "2",
          "time": "20210821 11:04"
        },
        {
          "id": "MDRW2100003699.1.25",
          "form": "ㅋㅋㅋㅋ맞아요 그래서 꽤 자주해먹어요",
          "original_form": "ㅋㅋㅋㅋ맞아요 그래서 꽤 자주해먹어요",
          "speaker_id": "1",
          "time": "20210821 11:05"
        },
        {
          "id": "MDRW2100003699.1.26",
          "form": "육수좀내고 야채넣고고기넣으면 끝이라",
          "original_form": "육수좀내고 야채넣고고기넣으면 끝이라",
