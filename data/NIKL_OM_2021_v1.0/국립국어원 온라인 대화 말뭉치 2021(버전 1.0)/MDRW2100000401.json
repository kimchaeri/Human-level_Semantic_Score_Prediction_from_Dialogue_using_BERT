      "utterance": [
        {
          "id": "MDRW2100000401.1.1",
          "form": "안녕 ㅎㅎ점심먹었어?",
          "original_form": "안녕 ㅎㅎ점심먹었어?",
          "speaker_id": "1",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000401.1.2",
          "form": "응 햄버거 먹었어 ㅋㅋ너는?",
          "original_form": "응 햄버거 먹었어 ㅋㅋ너는?",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000401.1.3",
          "form": "오 맛있었겠다. 햄버거는 버거킹인데 ㅎ",
          "original_form": "오 맛있었겠다. 햄버거는 버거킹인데 ㅎ",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000401.1.4",
          "form": "나는 소고기찜 먹었어 ㅎㅎ",
          "original_form": "나는 소고기찜 먹었어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000401.1.5",
          "form": "아쉽지만 집근처에 맘스터치 밖에 없어서 싸이버거 먹었어 ㅠ",
          "original_form": "아쉽지만 집근처에 맘스터치 밖에 없어서 싸이버거 먹었어 ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000401.1.6",
          "form": "더 고퀄리티 음식 먹었구나 맛있었겠다!",
          "original_form": "더 고퀄리티 음식 먹었구나 맛있었겠다!",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000401.1.7",
          "form": "맘스터치도 맛있지~!",
          "original_form": "맘스터치도 맛있지~!",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000401.1.8",
          "form": "나는 뭔가 상큼한게 먹고싶은데 딱히 생각나는게 없네",
          "original_form": "나는 뭔가 상큼한게 먹고싶은데 딱히 생각나는게 없네",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000401.1.9",
          "form": "딸기요거트 어때?",
          "original_form": "딸기요거트 어때?",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000401.1.10",
          "form": "난 상큼한거 땡길때마다 딸기요거트 먹거든 ㅎㅎ",
          "original_form": "난 상큼한거 땡길때마다 딸기요거트 먹거든 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000401.1.11",
          "form": "딸기요거트 좋다!",
          "original_form": "딸기요거트 좋다!",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000401.1.12",
          "form": "이따 퇴근하고 사먹어야겠다 ㅎㅎ",
          "original_form": "이따 퇴근하고 사먹어야겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000401.1.13",
          "form": "아 일중이구나 저녁은 뭐 먹으려구?",
          "original_form": "아 일중이구나 저녁은 뭐 먹으려구?",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000401.1.14",
          "form": "어 일하고있어 ㅎㅎ 저녁은 이따 운동가가지구 안먹으려구",
          "original_form": "어 일하고있어 ㅎㅎ 저녁은 이따 운동가가지구 안먹으려구",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000401.1.15",
          "form": "그럼 하루에 한끼나 두끼정도만 먹는거야?",
          "original_form": "그럼 하루에 한끼나 두끼정도만 먹는거야?",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000401.1.16",
          "form": "아침은 거의 먹구 점심은 항상먹구 저녁은,,약속있을때만 먹는데 아무것도안먹진않아 ㅋㅋㅋ 과일이라도 먹는편이야",
          "original_form": "아침은 거의 먹구 점심은 항상먹구 저녁은,,약속있을때만 먹는데 아무것도안먹진않아 ㅋㅋㅋ 과일이라도 먹는편이야",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000401.1.17",
          "form": "그렇구나 ㅋㅋ난 오히려 저녁에 과식하게되던데 ㅠ 대단하다",
          "original_form": "그렇구나 ㅋㅋ난 오히려 저녁에 과식하게되던데 ㅠ 대단하다",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000401.1.18",
          "form": "그래서 내가 저녁약속을 더 잡는거같기도해,,,ㅋㅋㅋㅋ운동안가는날이면 거의약속있는거같아",
          "original_form": "그래서 내가 저녁약속을 더 잡는거같기도해,,,ㅋㅋㅋㅋ운동안가는날이면 거의약속있는거같아",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000401.1.19",
          "form": "무슨의미가있을까 ㅋㅋㅋ",
          "original_form": "무슨의미가있을까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000401.1.20",
          "form": "ㅋㅋㅋ인기가 많구나 약속이 많고",
          "original_form": "ㅋㅋㅋ인기가 많구나 약속이 많고",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000401.1.21",
          "form": "난 거의 일 집 일 집",
          "original_form": "난 거의 일 집 일 집",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000401.1.22",
          "form": "퇴근하고 친구만나서 시원한 맥주한잔하는게 그렇게좋더라구 ㅎㅎ",
          "original_form": "퇴근하고 친구만나서 시원한 맥주한잔하는게 그렇게좋더라구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000401.1.23",
          "form": "난 집에서 맥주마셔 ㅋㅋㅋㄱ 간이 안 좋아져서 무알콜로 한 박스 사다놨어 ㅋㅋ",
          "original_form": "난 집에서 맥주마셔 ㅋㅋㅋㄱ 간이 안 좋아져서 무알콜로 한 박스 사다놨어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000401.1.24",
          "form": "나 얼마전에 무알콜 처음으로 먹어봤는데 맛으로는 거의 똑같더라 완전신기했어",
          "original_form": "나 얼마전에 무알콜 처음으로 먹어봤는데 맛으로는 거의 똑같더라 완전신기했어",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000401.1.25",
          "form": "어떤 맥주 마셔봤어? 난 카스 추천 받아서 마셔봤어 ㅋㅋ",
          "original_form": "어떤 맥주 마셔봤어? 난 카스 추천 받아서 마셔봤어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000401.1.26",
          "form": "하이네켄도 있더라고 카스도 괜찮았어!",
          "original_form": "하이네켄도 있더라고 카스도 괜찮았어!",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000401.1.27",
          "form": "카스는 약간 물엿맛 나던데 너도 느꼈니?",
          "original_form": "카스는 약간 물엿맛 나던데 너도 느꼈니?",
          "speaker_id": "2",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000401.1.28",
          "form": "나중엔 하이네켄 사 먹어봐야겠다!",
          "original_form": "나중엔 하이네켄 사 먹어봐야겠다!",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000401.1.29",
          "form": "물엿맛? 아니 못느꼈는데 ㅋㅋㅋ난 치킨이랑 먹어서 그럴수도있을거같아",
          "original_form": "물엿맛? 아니 못느꼈는데 ㅋㅋㅋ난 치킨이랑 먹어서 그럴수도있을거같아",
          "speaker_id": "1",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000401.1.30",
          "form": "치킨은 주로 뭐 먹어?",
          "original_form": "치킨은 주로 뭐 먹어?",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000401.1.31",
          "form": "ㄱㅊ바사삭 진짜좋아해 최애치킨이야 굽네치킨,,",
          "original_form": "ㄱㅊ바사삭 진짜좋아해 최애치킨이야 굽네치킨,,",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000401.1.32",
          "form": "아직 안 먹어봤는데!",
          "original_form": "아직 안 먹어봤는데!",
          "speaker_id": "2",
          "time": "20210819 16:41"
        }
      ]
    }
  ]
