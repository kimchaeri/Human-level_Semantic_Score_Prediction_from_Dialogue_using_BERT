      "utterance": [
        {
          "id": "MDRW2100021936.1.1",
          "form": "해산물도 좋아하시나요",
          "original_form": "해산물도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100021936.1.2",
          "form": "좋아는 하는데요 자주 사먹진 못해요",
          "original_form": "좋아는 하는데요 자주 사먹진 못해요",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021936.1.3",
          "form": "좋은걸 구매하기가 까다로워요",
          "original_form": "좋은걸 구매하기가 까다로워요",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021936.1.4",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021936.1.5",
          "form": "안 비리고 신선한 거",
          "original_form": "안 비리고 신선한 거",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021936.1.6",
          "form": "그리고 좋은걸 고르는 안목? 문턱이 고기보다 높아옷",
          "original_form": "그리고 좋은걸 고르는 안목? 문턱이 고기보다 높아옷",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021936.1.7",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021936.1.8",
          "form": "name2은 생선중에서 젤 선호하는게 뭔가요?",
          "original_form": "&name2&은 생선중에서 젤 선호하는게 뭔가요?",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100021936.1.9",
          "form": "음 요즘은 전어가 좋네요",
          "original_form": "음 요즘은 전어가 좋네요",
          "speaker_id": "2",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100021936.1.10",
          "form": "전어요 저도 좋아하는에 가시가 많아서",
          "original_form": "전어요 저도 좋아하는에 가시가 많아서",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100021936.1.11",
          "form": "구이를 젓가락으로 먹으면 다 부숴가며 먹어요",
          "original_form": "구이를 젓가락으로 먹으면 다 부숴가며 먹어요",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100021936.1.12",
          "form": "전어 회? 구이? 어떤 파세요?",
          "original_form": "전어 회? 구이? 어떤 파세요?",
          "speaker_id": "1",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100021936.1.13",
          "form": "전어 뼈는 골라낸다 vs씹어먹는다  두구두구 장칼님의 선택은?",
          "original_form": "전어 뼈는 골라낸다 vs씹어먹는다  두구두구 장칼님의 선택은?",
          "speaker_id": "1",
          "time": "20210829 13:40"
        },
        {
          "id": "MDRW2100021936.1.14",
          "form": "저도 전어 넘 맛있는데 가시땜에 항상 양껏은 안 먹는 편이에요",
          "original_form": "저도 전어 넘 맛있는데 가시땜에 항상 양껏은 안 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100021936.1.15",
          "form": "첨에는 발라내려하다가 나중엔 포기하는 파입이다ㅜㅜㅋㅋㅋㅋㅋ",
          "original_form": "첨에는 발라내려하다가 나중엔 포기하는 파입이다ㅜㅜㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100021936.1.16",
          "form": "구이랑 회 둘다 너무 좋은데 구이가 쫌 더 좋아용",
          "original_form": "구이랑 회 둘다 너무 좋은데 구이가 쫌 더 좋아용",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100021936.1.17",
          "form": "역시 그렇죠? 전어는 뼈째 먹어야 깔끔하게 먹고",
          "original_form": "역시 그렇죠? 전어는 뼈째 먹어야 깔끔하게 먹고",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100021936.1.18",
          "form": "발라내려면 가시사이에 살이남아요",
          "original_form": "발라내려면 가시사이에 살이남아요",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100021936.1.19",
          "form": "꼬소하고 참 좋은데..",
          "original_form": "꼬소하고 참 좋은데..",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100021936.1.20",
          "form": "맞아요 하나하나 발라내기도 힘들구요",
          "original_form": "맞아요 하나하나 발라내기도 힘들구요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100021936.1.21",
          "form": "고등어나 꽁치는 바르겠는데",
          "original_form": "고등어나 꽁치는 바르겠는데",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100021936.1.22",
          "form": "전어는 아직 제가 부족하여..",
          "original_form": "전어는 아직 제가 부족하여..",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100021936.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100021936.1.24",
          "form": "발라서 먹을 수 있는 사람이 있을까요",
          "original_form": "발라서 먹을 수 있는 사람이 있을까요",
          "speaker_id": "2",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100021936.1.25",
          "form": "고수라면 발라먹을 수 있을지도 몰라요",
          "original_form": "고수라면 발라먹을 수 있을지도 몰라요",
          "speaker_id": "1",
          "time": "20210830 15:18"
        },
        {
          "id": "MDRW2100021936.1.26",
          "form": "얘기하다보니 전어가 먹고 싶네요",
          "original_form": "얘기하다보니 전어가 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210830 15:18"
        },
        {
          "id": "MDRW2100021936.1.27",
          "form": "왜 아직 대화가 부족하지,, 저 지금 라떼를 먹고 있어요",
          "original_form": "왜 아직 대화가 부족하지,, 저 지금 라떼를 먹고 있어요",
          "speaker_id": "1",
          "time": "20210830 15:40"
        },
        {
          "id": "MDRW2100021936.1.28",
          "form": "name2의 오늘자 간식을 나중에 알려주세요 ㅋㅋ",
          "original_form": "&name2&의 오늘자 간식을 나중에 알려주세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:40"
        },
        {
          "id": "MDRW2100021936.1.29",
          "form": "저 오늘 회사에서 피자 먹었어요!",
          "original_form": "저 오늘 회사에서 피자 먹었어요!",
