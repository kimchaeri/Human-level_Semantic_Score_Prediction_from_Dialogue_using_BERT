      "utterance": [
        {
          "id": "MDRW2100006768.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006768.1.2",
          "form": "최근에 극장에서 보신 영화 있으신가요?",
          "original_form": "최근에 극장에서 보신 영화 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006768.1.3",
          "form": "올해는 극장에 간적이 없네요",
          "original_form": "올해는 극장에 간적이 없네요",
          "speaker_id": "2",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006768.1.4",
          "form": "보신 영화 있나요",
          "original_form": "보신 영화 있나요",
          "speaker_id": "2",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006768.1.5",
          "form": "최근에 팜스프링스라는 영화 봤어요",
          "original_form": "최근에 팜스프링스라는 영화 봤어요",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006768.1.6",
          "form": "혹시 들어보신적 있으신가요??",
          "original_form": "혹시 들어보신적 있으신가요??",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006768.1.7",
          "form": "아니요 잘모르겠네요",
          "original_form": "아니요 잘모르겠네요",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006768.1.8",
          "form": "어떤 영화 인가요",
          "original_form": "어떤 영화 인가요",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006768.1.9",
          "form": "사실 유명한 영화는 아니에요",
          "original_form": "사실 유명한 영화는 아니에요",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006768.1.10",
          "form": "관객수도 만 명이 안 된걸로 알고 있는데..",
          "original_form": "관객수도 만 명이 안 된걸로 알고 있는데..",
          "speaker_id": "1",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006768.1.11",
          "form": "타임루프 로코 영화에요!",
          "original_form": "타임루프 로코 영화에요!",
          "speaker_id": "1",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006768.1.12",
          "form": "아 그렀군요",
          "original_form": "아 그렀군요",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006768.1.13",
          "form": "그럼 혹시 뮤지컬 보시는건 좋아하세요??",
          "original_form": "그럼 혹시 뮤지컬 보시는건 좋아하세요??",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006768.1.14",
          "form": "싱크홀 영화가 궁금하긴해요",
          "original_form": "싱크홀 영화가 궁금하긴해요",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006768.1.15",
          "form": "뮤지컬 좋아요",
          "original_form": "뮤지컬 좋아요",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006768.1.16",
          "form": "우와 무슨 뮤지컬 제일 좋아하세요??",
          "original_form": "우와 무슨 뮤지컬 제일 좋아하세요??",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006768.1.17",
          "form": "광화문연가 재밌게 봤어요",
          "original_form": "광화문연가 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100006768.1.18",
          "form": "앗 광화문연가는 본 적 없네요...",
          "original_form": "앗 광화문연가는 본 적 없네요...",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006768.1.19",
          "form": "잔잔한 추억여행 같은 뮤지컬이죠",
          "original_form": "잔잔한 추억여행 같은 뮤지컬이죠",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100006768.1.20",
          "form": "광화문연가 어쩌다가 보시게 된거에요?",
          "original_form": "광화문연가 어쩌다가 보시게 된거에요?",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100006768.1.21",
          "form": "딸이 보고 싶다고 해서 보게 되었어요",
          "original_form": "딸이 보고 싶다고 해서 보게 되었어요",
          "speaker_id": "2",
          "time": "20210908 10:34"
        }
      ]
