      "utterance": [
        {
          "id": "MDRW2100032841.1.1",
          "form": "아쉽게도 토마토달걀볶음은 제 점심메뉴가 아니었습니다 짜잔~",
          "original_form": "아쉽게도 토마토달걀볶음은 제 점심메뉴가 아니었습니다 짜잔~",
          "speaker_id": "1",
          "time": "20210906 01:04"
        },
        {
          "id": "MDRW2100032841.1.2",
          "form": "뭘 먹었을까요",
          "original_form": "뭘 먹었을까요",
          "speaker_id": "1",
          "time": "20210906 01:05"
        },
        {
          "id": "MDRW2100032841.1.3",
          "form": "뭘 먹었지\r\n떡볶이 메었니",
          "original_form": "뭘 먹었지\r\n떡볶이 메었니",
          "speaker_id": "2",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100032841.1.4",
          "form": "먹었니",
          "original_form": "먹었니",
          "speaker_id": "2",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100032841.1.5",
          "form": "아니 떡볶이 아니지롱",
          "original_form": "아니 떡볶이 아니지롱",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100032841.1.6",
          "form": "어제 점심 맞추기였나",
          "original_form": "어제 점심 맞추기였나",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100032841.1.7",
          "form": "응 무슨 볶이인지",
          "original_form": "응 무슨 볶이인지",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100032841.1.8",
          "form": "예상이 안가네",
          "original_form": "예상이 안가네",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100032841.1.9",
          "form": "더 물어봐",
          "original_form": "더 물어봐",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100032841.1.10",
          "form": "많이 물어볼수록 무슨 음식인지 맞추기 쉬울거야",
          "original_form": "많이 물어볼수록 무슨 음식인지 맞추기 쉬울거야",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100032841.1.11",
          "form": "내 머리의 한계다",
          "original_form": "내 머리의 한계다",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100032841.1.12",
          "form": "매운양념이니",
          "original_form": "매운양념이니",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100032841.1.13",
          "form": "응 나름..? 근데 너도 안 맵게 먹을 수 있어",
          "original_form": "응 나름..? 근데 너도 안 맵게 먹을 수 있어",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100032841.1.14",
          "form": "양파볶음이니",
          "original_form": "양파볶음이니",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100032841.1.15",
          "form": "아니 양파는 아니야",
          "original_form": "아니 양파는 아니야",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100032841.1.16",
          "form": "흐으으으으으음 야채볶음이니",
          "original_form": "흐으으으으으음 야채볶음이니",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100032841.1.17",
          "form": "야채를 볶는 요리긴 해",
          "original_form": "야채를 볶는 요리긴 해",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100032841.1.18",
          "form": "소시지 야채볶음이니",
          "original_form": "소시지 야채볶음이니",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100032841.1.19",
          "form": "아니! 햄종류가 들어갈 때도 있지만 소시지야볶은 아니야",
          "original_form": "아니! 햄종류가 들어갈 때도 있지만 소시지야볶은 아니야",
          "speaker_id": "1",
          "time": "20210906 21:28"
        }
      ]
