      "utterance": [
        {
          "id": "MDRW2100017485.1.1",
          "form": "그러면 라면의 형제",
          "original_form": "그러면 라면의 형제",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017485.1.2",
          "form": "스파게티는 어때",
          "original_form": "스파게티는 어때",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017485.1.3",
          "form": "아 스파게티쿤",
          "original_form": "아 스파게티쿤",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017485.1.4",
          "form": "토마토스파게티에 치즈가루 올려서 먹는거야",
          "original_form": "토마토스파게티에 치즈가루 올려서 먹는거야",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017485.1.5",
          "form": "우리집에 스파게티가 로제밖에없어",
          "original_form": "우리집에 스파게티가 로제밖에없어",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017485.1.6",
          "form": "헐 에그인헬은 어떄",
          "original_form": "헐 에그인헬은 어떄",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.7",
          "form": "로제",
          "original_form": "로제",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.8",
          "form": "도 좋지",
          "original_form": "도 좋지",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.9",
          "form": "식빵 한 5장 구워먹어야지",
          "original_form": "식빵 한 5장 구워먹어야지",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.10",
          "form": "완전 의식의 흐름인데?",
          "original_form": "완전 의식의 흐름인데?",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.11",
          "form": "우리집에 소세지는 없어도 베이컨은 있는데",
          "original_form": "우리집에 소세지는 없어도 베이컨은 있는데",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.12",
          "form": "스팸 하나 뜯을까",
          "original_form": "스팸 하나 뜯을까",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.13",
          "form": "근데 지나번에 에그인헬 했다가 실패했어",
          "original_form": "근데 지나번에 에그인헬 했다가 실패했어",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.14",
          "form": "어떻게 실패하는거지?",
          "original_form": "어떻게 실패하는거지?",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.15",
          "form": "...?",
          "original_form": "...?",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017485.1.16",
          "form": "내가 한건 라디오 회관의 그 맛이 안나",
          "original_form": "내가 한건 라디오 회관의 그 맛이 안나",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.17",
          "form": "기대한것 : 라디오회관",
          "original_form": "기대한것 : 라디오회관",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.18",
          "form": "맛이 실패구나",
          "original_form": "맛이 실패구나",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.19",
          "form": "내가 만든것 :그냥 토마토와 계란과 치즈",
          "original_form": "내가 만든것 :그냥 토마토와 계란과 치즈",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.21",
          "form": "뭔가 모자라..",
          "original_form": "뭔가 모자라..",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.22",
          "form": "뭔가 재료 하나가",
          "original_form": "뭔가 재료 하나가",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.23",
          "form": "미묘하게",
          "original_form": "미묘하게",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.24",
          "form": "치킨스톡을 좀 넣어볼까",
          "original_form": "치킨스톡을 좀 넣어볼까",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.25",
          "form": "빠졌을거야",
          "original_form": "빠졌을거야",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017485.1.26",
          "form": "인류에서 음식에 뭔가 문제가 있을때는 대체로 msg를 넣으면 해결된다고",
          "original_form": "인류에서 음식에 뭔가 문제가 있을때는 대체로 msg를 넣으면 해결된다고",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017485.1.27",
          "form": "맞지",
          "original_form": "맞지",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017485.1.28",
          "form": "또는 msg가 가득한",
          "original_form": "또는 msg가 가득한",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017485.1.29",
          "form": "마법가루 라면스프도 있어",
          "original_form": "마법가루 라면스프도 있어",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017485.1.30",
          "form": "다시 라면으로?",
          "original_form": "다시 라면으로?",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017485.1.31",
          "form": "돌고돌아 라면인가",
          "original_form": "돌고돌아 라면인가",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017485.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017485.1.33",
          "form": "일단 에그인헬을 생각중이야 베이컨을 냉동실에서 꺼내야겠어",
          "original_form": "일단 에그인헬을 생각중이야 베이컨을 냉동실에서 꺼내야겠어",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017485.1.34",
          "form": "후식으로 크로플해먹어야지 ^^",
          "original_form": "후식으로 크로플해먹어야지 ^^",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017485.1.35",
          "form": "개맛있다",
          "original_form": "개맛있다",
          "speaker_id": "1",
