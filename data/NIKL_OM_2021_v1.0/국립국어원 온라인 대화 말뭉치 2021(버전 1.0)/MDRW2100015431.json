      "utterance": [
        {
          "id": "MDRW2100015431.1.1",
          "form": "연애와 결혼 언제하실거에요?",
          "original_form": "연애와 결혼 언제하실거에요?",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015431.1.2",
          "form": "포기했어요..name1은 언제계획을..",
          "original_form": "포기했어요..&name1&은 언제계획을..",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015431.1.3",
          "form": "저도 잘모르겠어요",
          "original_form": "저도 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015431.1.4",
          "form": "제 손에 칼자루가 없습니다",
          "original_form": "제 손에 칼자루가 없습니다",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015431.1.5",
          "form": "강하게나가봅시다 개척해봐요",
          "original_form": "강하게나가봅시다 개척해봐요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015431.1.6",
          "form": "행복을쟁취해야해요",
          "original_form": "행복을쟁취해야해요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015431.1.7",
          "form": "맞아요 행복을 쟁취해봅시다",
          "original_form": "맞아요 행복을 쟁취해봅시다",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015431.1.8",
          "form": "이제 1년 4개월 남았네요",
          "original_form": "이제 1년 4개월 남았네요",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015431.1.9",
          "form": "생각보다 시간 빠르네요",
          "original_form": "생각보다 시간 빠르네요",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015431.1.10",
          "form": "그러게 시간금방가요..  벌써8월이다갔져..",
          "original_form": "그러게 시간금방가요..  벌써8월이다갔져..",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015431.1.11",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015431.1.12",
          "form": "빨리가는게 좋기도 하고 싫기듀 해요",
          "original_form": "빨리가는게 좋기도 하고 싫기듀 해요",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015431.1.13",
          "form": "장단점이있지여..",
          "original_form": "장단점이있지여..",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015431.1.14",
          "form": "흐엉",
          "original_form": "흐엉",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015431.1.15",
          "form": "name1도좋은시기에",
          "original_form": "&name1&도좋은시기에",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015431.1.16",
          "form": "데이트도못하고",
          "original_form": "데이트도못하고",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015431.1.17",
          "form": "구렇습니다",
          "original_form": "구렇습니다",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015431.1.18",
          "form": "아쉽습니다",
          "original_form": "아쉽습니다",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015431.1.19",
          "form": "하지만 친구들과 보내는 시간도 좋아요",
          "original_form": "하지만 친구들과 보내는 시간도 좋아요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015431.1.20",
          "form": "사실 친규들과는 약솓이 어려워서 잘 못보죠ㅠ",
          "original_form": "사실 친규들과는 약솓이 어려워서 잘 못보죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015431.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015431.1.22",
          "form": "남친은 뷰르며뉴나오니까 그건 좋네요",
          "original_form": "남친은 뷰르며뉴나오니까 그건 좋네요",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015431.1.23",
          "form": "악ㄱㅈㄱㅈㅋㅈㄱ",
          "original_form": "악ㄱㅈㄱㅈㅋㅈㄱ",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015431.1.24",
          "form": "부르면나온대",
          "original_form": "부르면나온대",
