      "utterance": [
        {
          "id": "MDRW2100024049.1.1",
          "form": "싫어하는 음식 있으세요?",
          "original_form": "싫어하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100024049.1.2",
          "form": "어머ㅜ 저 매운거 진짜 싫어해요",
          "original_form": "어머ㅜ 저 매운거 진짜 싫어해요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024049.1.3",
          "form": "특히 불닭소스 같이 인위적인거요",
          "original_form": "특히 불닭소스 같이 인위적인거요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024049.1.4",
          "form": "헐 진짜요 ㅜㅜ?",
          "original_form": "헐 진짜요 ㅜㅜ?",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024049.1.5",
          "form": "떡볶이도 안드시나요??",
          "original_form": "떡볶이도 안드시나요??",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024049.1.6",
          "form": "떡볶이는 탄수화물 덩어리라서 안먹어요...",
          "original_form": "떡볶이는 탄수화물 덩어리라서 안먹어요...",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024049.1.7",
          "form": "대신 맛있게 매운 음식들은 괜찮아요~",
          "original_form": "대신 맛있게 매운 음식들은 괜찮아요~",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024049.1.8",
          "form": "아아 ㅎㅎ 인위적인것만 싫어하시는군요?",
          "original_form": "아아 ㅎㅎ 인위적인것만 싫어하시는군요?",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024049.1.9",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024049.1.10",
          "form": "막 디진다돈까스 이런것도 싫구요",
          "original_form": "막 디진다돈까스 이런것도 싫구요",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024049.1.11",
          "form": "캡사이신 소스 들어간거 다싫어요",
          "original_form": "캡사이신 소스 들어간거 다싫어요",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100024049.1.12",
          "form": "님은 어떤거 싫어해요?",
          "original_form": "님은 어떤거 싫어해요?",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024049.1.13",
          "form": "저는 버섯 싫어해요 ㅎㅎㅅ",
          "original_form": "저는 버섯 싫어해요 ㅎㅎㅅ",
          "speaker_id": "2",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024049.1.14",
          "form": "헐랭 버섯은 왜 싫어해요?",
          "original_form": "헐랭 버섯은 왜 싫어해요?",
          "speaker_id": "1",
          "time": "20210830 23:59"
        },
        {
          "id": "MDRW2100024049.1.15",
          "form": "그 버섯향을 안좋아해요!",
          "original_form": "그 버섯향을 안좋아해요!",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024049.1.16",
          "form": "가지도 같은 계열이라 ㅋㅋ 안좋아한답니다",
          "original_form": "가지도 같은 계열이라 ㅋㅋ 안좋아한답니다",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024049.1.17",
          "form": "아 ㅠㅠ 맞아요 그런거 싫어하는 사람 있더라구요",
          "original_form": "아 ㅠㅠ 맞아요 그런거 싫어하는 사람 있더라구요",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024049.1.18",
          "form": "특히 물컹거리는 식감이 그렇게 싫대요",
          "original_form": "특히 물컹거리는 식감이 그렇게 싫대요",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024049.1.19",
          "form": "아 맞아욬ㄱㄲㄱ",
          "original_form": "아 맞아욬ㄱㄲㄱ",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024049.1.20",
          "form": "파프리카도 향때문에 싫어하네요ㅋㅋㅜㅜ",
          "original_form": "파프리카도 향때문에 싫어하네요ㅋㅋㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100024049.1.21",
          "form": "앗 ㅋㅋㅋ 파프리카 싫어하면 피망은 어때요?",
          "original_form": "앗 ㅋㅋㅋ 파프리카 싫어하면 피망은 어때요?",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100024049.1.22",
          "form": "피망도 싫어해욬ㅋㅋ 같은 친구친구라서 ㅋㅋ",
          "original_form": "피망도 싫어해욬ㅋㅋ 같은 친구친구라서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024049.1.23",
          "form": "같은 친구친구 계열 안좋아해욬ㅋㅋ",
          "original_form": "같은 친구친구 계열 안좋아해욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:05"
        }
      ]
