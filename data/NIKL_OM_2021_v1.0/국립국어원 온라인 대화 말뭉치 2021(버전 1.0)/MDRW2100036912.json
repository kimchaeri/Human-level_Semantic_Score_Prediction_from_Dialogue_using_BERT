      "utterance": [
        {
          "id": "MDRW2100036912.1.1",
          "form": "네 너무좋아해요",
          "original_form": "네 너무좋아해요",
          "speaker_id": "2",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100036912.1.2",
          "form": "커피좋아하세요?",
          "original_form": "커피좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100036912.1.3",
          "form": "저 좋아하다 탈났어요.. 너무 마셔서요",
          "original_form": "저 좋아하다 탈났어요.. 너무 마셔서요",
          "speaker_id": "1",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100036912.1.4",
          "form": "아하ㅜ 너무 많이마셨나보네요",
          "original_form": "아하ㅜ 너무 많이마셨나보네요",
          "speaker_id": "2",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100036912.1.5",
          "form": "하루에 1리터씩 마셨어요.. 빈속에..ㅋ",
          "original_form": "하루에 1리터씩 마셨어요.. 빈속에..ㅋ",
          "speaker_id": "1",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100036912.1.6",
          "form": "헉 진짜 많이드셨네요ㅜㅜ",
          "original_form": "헉 진짜 많이드셨네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100036912.1.7",
          "form": "평소에 그렇게많이드시나요?",
          "original_form": "평소에 그렇게많이드시나요?",
          "speaker_id": "2",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100036912.1.8",
          "form": "한 일년 아침식사 대신 커피 마셨어요",
          "original_form": "한 일년 아침식사 대신 커피 마셨어요",
          "speaker_id": "1",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100036912.1.9",
          "form": "아아 저희신랑이 그러고있어요",
          "original_form": "아아 저희신랑이 그러고있어요",
          "speaker_id": "2",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036912.1.10",
          "form": "ㅠㅜ빈속에 커피ㅜ",
          "original_form": "ㅠㅜ빈속에 커피ㅜ",
          "speaker_id": "2",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036912.1.11",
          "form": "오 완전 속버리는데",
          "original_form": "오 완전 속버리는데",
          "speaker_id": "1",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100036912.1.12",
          "form": "저 그래서 치료받고 커피 끊다시피 했어요. .지금은 다시 먹지만 빈속에는 안먹기 위해 노력하죠",
          "original_form": "저 그래서 치료받고 커피 끊다시피 했어요. .지금은 다시 먹지만 빈속에는 안먹기 위해 노력하죠",
          "speaker_id": "1",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100036912.1.13",
          "form": "아 그러시군요.ㅠ저도 신랑이 걱정되더라구요",
          "original_form": "아 그러시군요.ㅠ저도 신랑이 걱정되더라구요",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036912.1.14",
          "form": "근데 습관인지라ㅜ",
          "original_form": "근데 습관인지라ㅜ",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036912.1.15",
          "form": "잘안되나봐요",
          "original_form": "잘안되나봐요",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036912.1.16",
          "form": "맞아요 습관이에요",
          "original_form": "맞아요 습관이에요",
          "speaker_id": "1",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100036912.1.17",
          "form": "그래서 쉽지 않겠지만 위 건강 생각해서는 자제해하 할텐데요",
          "original_form": "그래서 쉽지 않겠지만 위 건강 생각해서는 자제해하 할텐데요",
          "speaker_id": "1",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100036912.1.18",
          "form": "맞아요   조심시켜야겠어요",
          "original_form": "맞아요   조심시켜야겠어요",
          "speaker_id": "2",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100036912.1.19",
          "form": "이제 저녁해야 될 시간이네요.ㅜㅋㅋㅋㅋㄱ",
          "original_form": "이제 저녁해야 될 시간이네요.ㅜㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100036912.1.20",
          "form": "저녁 만드시나 봐요\n저도 해야 해요 ㅎㅎ",
          "original_form": "저녁 만드시나 봐요\n저도 해야 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100036912.1.21",
          "form": "네ㅜㄱㅋㅋㅋ맛있게 만들어봐요ㅋㅋ",
          "original_form": "네ㅜㄱㅋㅋㅋ맛있게 만들어봐요ㅋㅋ",
          "speaker_id": "2",
