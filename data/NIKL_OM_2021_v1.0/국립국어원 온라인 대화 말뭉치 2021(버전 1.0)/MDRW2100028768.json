        },
        {
          "id": "MDRW2100028768.1.12",
          "form": "엥?????????",
          "original_form": "엥?????????",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028768.1.13",
          "form": "어이없넼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어이없넼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028768.1.14",
          "form": "거기있던글들중에헤어지자때올라왔던비하인드",
          "original_form": "거기있던글들중에헤어지자때올라왔던비하인드",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028768.1.15",
          "form": "글에애정넘쳐보여서좋아했는데",
          "original_form": "글에애정넘쳐보여서좋아했는데",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028768.1.16",
          "form": "알비접 제정신?",
          "original_form": "알비접 제정신?",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028768.1.17",
          "form": "싹밀렸어ㅋㅋㅋㅋㅋㅋㅋㅋ",
