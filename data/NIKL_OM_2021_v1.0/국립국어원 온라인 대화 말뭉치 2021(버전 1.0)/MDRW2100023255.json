      "utterance": [
        {
          "id": "MDRW2100023255.1.1",
          "form": "오늘 진짜 시원해요",
          "original_form": "오늘 진짜 시원해요",
          "speaker_id": "1",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023255.1.2",
          "form": "이제 춥더라구요",
          "original_form": "이제 춥더라구요",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100023255.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100023255.1.4",
          "form": "병원갔는데 에어컨 틀어놔서 진짜ㅜㅜ",
          "original_form": "병원갔는데 에어컨 틀어놔서 진짜ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100023255.1.5",
          "form": "이날씨에 에어컨이요?",
          "original_form": "이날씨에 에어컨이요?",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100023255.1.6",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100023255.1.7",
          "form": "전기 아깝네요",
          "original_form": "전기 아깝네요",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100023255.1.8",
          "form": "이제 슬슬 반팔 집어넣어야할까봐요",
          "original_form": "이제 슬슬 반팔 집어넣어야할까봐요",
          "speaker_id": "2",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100023255.1.9",
          "form": "코트와 워커의 계절이 오나요",
          "original_form": "코트와 워커의 계절이 오나요",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100023255.1.10",
          "form": "크 생각만해도 너무 좋네요",
          "original_form": "크 생각만해도 너무 좋네요",
          "speaker_id": "2",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100023255.1.11",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023255.1.12",
          "form": "청바지가 커져서 겸사겸사 옷구경하러 가야겠어요",
          "original_form": "청바지가 커져서 겸사겸사 옷구경하러 가야겠어요",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023255.1.13",
          "form": "살이 빠지셨나요?",
          "original_form": "살이 빠지셨나요?",
          "speaker_id": "1",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023255.1.14",
          "form": "네 여름에 너무 더워서 입맛이 없더라구요",
          "original_form": "네 여름에 너무 더워서 입맛이 없더라구요",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023255.1.15",
          "form": "저런....",
          "original_form": "저런....",
          "speaker_id": "1",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023255.1.16",
          "form": "가을옷 사실예정이세요?",
          "original_form": "가을옷 사실예정이세요?",
          "speaker_id": "2",
          "time": "20210830 13:15"
        },
        {
          "id": "MDRW2100023255.1.17",
          "form": "저두 고민중이에요",
          "original_form": "저두 고민중이에요",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100023255.1.18",
          "form": "가디건 예쁜 거 하나쯤은...",
          "original_form": "가디건 예쁜 거 하나쯤은...",
          "speaker_id": "2",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100023255.1.19",
          "form": "좋죠ㅎㅎ",
          "original_form": "좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100023255.1.20",
          "form": "날이 춥네요",
          "original_form": "날이 춥네요",
          "speaker_id": "1",
          "time": "20210902 13:54"
        }
      ]
