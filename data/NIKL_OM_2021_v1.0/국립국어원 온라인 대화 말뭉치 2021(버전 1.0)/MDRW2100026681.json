          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026681.1.5",
          "form": "맞다 너",
          "original_form": "맞다 너",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026681.1.6",
          "form": "올해 근로장려금",
          "original_form": "올해 근로장려금",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026681.1.7",
          "form": "신청했어??",
          "original_form": "신청했어??",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026681.1.8",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026681.1.9",
          "form": "근로소득 받는사람들중에",
          "original_form": "근로소득 받는사람들중에",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026681.1.10",
          "form": "헐",
          "original_form": "헐",
