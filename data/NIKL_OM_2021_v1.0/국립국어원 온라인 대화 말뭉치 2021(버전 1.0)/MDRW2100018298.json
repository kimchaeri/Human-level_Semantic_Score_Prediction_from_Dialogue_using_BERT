      "utterance": [
        {
          "id": "MDRW2100018298.1.1",
          "form": "요즘 옷입기 애매해여",
          "original_form": "요즘 옷입기 애매해여",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018298.1.2",
          "form": "진짜 애매하죠 전 오늘 너무 더워서",
          "original_form": "진짜 애매하죠 전 오늘 너무 더워서",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018298.1.3",
          "form": "에어컨 안킬수가 없네요 이제 켤일 없을거라 생각했는데",
          "original_form": "에어컨 안킬수가 없네요 이제 켤일 없을거라 생각했는데",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018298.1.4",
          "form": "하루종일 틀어도 될 정도예요",
          "original_form": "하루종일 틀어도 될 정도예요",
          "speaker_id": "2",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018298.1.5",
          "form": "전  추워서 창문도 닫고 방문도 닫았어요",
          "original_form": "전  추워서 창문도 닫고 방문도 닫았어요",
          "speaker_id": "1",
          "time": "20210826 23:45"
        },
        {
          "id": "MDRW2100018298.1.6",
          "form": "진짜여?? 우리집 왜이렇게 덥지 습해요",
          "original_form": "진짜여?? 우리집 왜이렇게 덥지 습해요",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018298.1.7",
          "form": "같은 경인권인데 무슨일이죠?",
          "original_form": "같은 경인권인데 무슨일이죠?",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018298.1.8",
          "form": "ㅋㅋㅋㅋ 그러니까요",
          "original_form": "ㅋㅋㅋㅋ 그러니까요",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018298.1.9",
          "form": "이불안덮고 자면 아침에 목소리가 가던데ㅋㅋ",
          "original_form": "이불안덮고 자면 아침에 목소리가 가던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018298.1.10",
          "form": "넘나 추워서",
          "original_form": "넘나 추워서",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018298.1.11",
          "form": "여기는 바깥은 그래도 괜찮은데 건물에만 들어오면 더워요",
          "original_form": "여기는 바깥은 그래도 괜찮은데 건물에만 들어오면 더워요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018298.1.12",
          "form": "환절기 맞네요 감기 조심 하세요",
          "original_form": "환절기 맞네요 감기 조심 하세요",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018298.1.13",
          "form": "공감이 안되여ㅋㅋ",
          "original_form": "공감이 안되여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018298.1.14",
          "form": "그러니까요 수도권에서도 이렇게 다르네요 ㅋㅋㅋㅋ",
          "original_form": "그러니까요 수도권에서도 이렇게 다르네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018298.1.15",
          "form": "가디건 입으면 더운뎅",
          "original_form": "가디건 입으면 더운뎅",
          "speaker_id": "2",
