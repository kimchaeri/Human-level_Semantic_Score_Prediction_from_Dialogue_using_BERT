      "utterance": [
        {
          "id": "MDRW2100012107.1.1",
          "form": "오늘 작은 아들 티셔츠 왔어",
          "original_form": "오늘 작은 아들 티셔츠 왔어",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012107.1.2",
          "form": "잘 맞아?",
          "original_form": "잘 맞아?",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012107.1.3",
          "form": "아니 ㅠㅠ 2년 후에나 입겠던데",
          "original_form": "아니 ㅠㅠ 2년 후에나 입겠던데",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012107.1.4",
          "form": "헉 그렇게 커? ㅋㅋㅋ",
          "original_form": "헉 그렇게 커? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012107.1.5",
          "form": "큰아들 입히고 다시 사야될라나 봐~ 팔이 너무 길어",
          "original_form": "큰아들 입히고 다시 사야될라나 봐~ 팔이 너무 길어",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012107.1.6",
          "form": "팔 접혀서 입혀~ 팔에 맞추면 또 몸이 안맞을거야 통통한 편이라",
          "original_form": "팔 접혀서 입혀~ 팔에 맞추면 또 몸이 안맞을거야 통통한 편이라",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012107.1.7",
          "form": "길어도 너무 긴데? 내 팔에도 맞아 ㅋㅋ",
          "original_form": "길어도 너무 긴데? 내 팔에도 맞아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100012107.1.8",
          "form": "흠.. 교환 안되나?",
          "original_form": "흠.. 교환 안되나?",
          "speaker_id": "1",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100012107.1.9",
          "form": "교환하면 왕복 배송비 내야해서 ㅠ 그냥 큰아들 입히지 뭐",
          "original_form": "교환하면 왕복 배송비 내야해서 ㅠ 그냥 큰아들 입히지 뭐",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100012107.1.10",
          "form": "타미 제품이 팔이 유난히 길게 나오는  것 같아 ㅎ",
          "original_form": "타미 제품이 팔이 유난히 길게 나오는  것 같아 ㅎ",
          "speaker_id": "1",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100012107.1.11",
          "form": "좀 그런듯.. 외국인 체형에 맞춰서 그런가",
          "original_form": "좀 그런듯.. 외국인 체형에 맞춰서 그런가",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100012107.1.12",
          "form": "그렇겠지 ㅎㅎ 우리나라 제품이 아니니",
          "original_form": "그렇겠지 ㅎㅎ 우리나라 제품이 아니니",
          "speaker_id": "1",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100012107.1.13",
          "form": "면은 품질이 엄청 좋은데 사이즈 맞추기가 쉽지 않네",
          "original_form": "면은 품질이 엄청 좋은데 사이즈 맞추기가 쉽지 않네",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012107.1.14",
          "form": "헤드셋을 하나 사야할까봐",
          "original_form": "헤드셋을 하나 사야할까봐",
