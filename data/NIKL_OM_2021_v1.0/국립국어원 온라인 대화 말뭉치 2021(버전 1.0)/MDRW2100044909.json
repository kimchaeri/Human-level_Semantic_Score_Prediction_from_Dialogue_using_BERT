      "utterance": [
        {
          "id": "MDRW2100044909.1.1",
          "form": "포장도 되니까 포장도 노려보세요! 포장은 빨라요",
          "original_form": "포장도 되니까 포장도 노려보세요! 포장은 빨라요",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044909.1.2",
          "form": "넵 팁감사해요ㅋㅋㅋㅋ",
          "original_form": "넵 팁감사해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044909.1.3",
          "form": "과일도 좋아하시나요?",
          "original_form": "과일도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044909.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044909.1.5",
          "form": "과일 좋아하시나요?",
          "original_form": "과일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044909.1.6",
          "form": "네네 저완전 좋아해요 ㅋㅋㅋ",
          "original_form": "네네 저완전 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044909.1.7",
          "form": "그중 최도는 수박이요!!",
          "original_form": "그중 최도는 수박이요!!",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044909.1.8",
          "form": "최고는!!",
          "original_form": "최고는!!",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044909.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044909.1.10",
          "form": "저도 수박 좋아해요",
          "original_form": "저도 수박 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044909.1.11",
          "form": "화채로먹으면 행복합니다",
          "original_form": "화채로먹으면 행복합니다",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044909.1.12",
          "form": "ㅋㅋㅋ맞아요ㅠㅠ",
          "original_form": "ㅋㅋㅋ맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044909.1.13",
          "form": "전 미리 다 썰어서 통에 정리해두고",
          "original_form": "전 미리 다 썰어서 통에 정리해두고",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044909.1.14",
          "form": "막 꺼내먹는것 좋아해요 ㅋㅋㅋ",
          "original_form": "막 꺼내먹는것 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044909.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044909.1.16",
          "form": "그래야 먹을때 편하니까요ㅋㅋㅋㅋ",
          "original_form": "그래야 먹을때 편하니까요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044909.1.17",
          "form": "딸기케잌 좋아하시나요?",
          "original_form": "딸기케잌 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044909.1.18",
          "form": "헐 네 완전 좋아하죠ㅠㅠㅠㅠ",
          "original_form": "헐 네 완전 좋아하죠ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044909.1.19",
          "form": "명동에 있는 르빵 추천드려요",
          "original_form": "명동에 있는 르빵 추천드려요",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044909.1.20",
          "form": "이미 유명한곳이지만요",
          "original_form": "이미 유명한곳이지만요",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044909.1.21",
          "form": "오오 저장할래요 아직 안가본 곳이에요!!!!",
          "original_form": "오오 저장할래요 아직 안가본 곳이에요!!!!",
