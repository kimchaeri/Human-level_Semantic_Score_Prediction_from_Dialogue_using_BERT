      "utterance": [
        {
          "id": "MDRW2100012294.1.1",
          "form": "넷플릭스 보세요?",
          "original_form": "넷플릭스 보세요?",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012294.1.2",
          "form": "한 달 정도 본 거 같아요!",
          "original_form": "한 달 정도 본 거 같아요!",
          "speaker_id": "2",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012294.1.3",
          "form": "자주 보시나요?",
          "original_form": "자주 보시나요?",
          "speaker_id": "2",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012294.1.4",
          "form": "오 얼마 안되셨구나~전 자주 봐요ㅎㅎ",
          "original_form": "오 얼마 안되셨구나~전 자주 봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012294.1.5",
          "form": "최근에 본 컨텐츠가 뭐에요?",
          "original_form": "최근에 본 컨텐츠가 뭐에요?",
          "speaker_id": "1",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012294.1.6",
          "form": "스위트홈 봤어요!",
          "original_form": "스위트홈 봤어요!",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100012294.1.7",
          "form": "아~그거 안 무서워요?",
          "original_form": "아~그거 안 무서워요?",
          "speaker_id": "1",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012294.1.8",
          "form": "무서울것 같아서 안 봤는데..",
          "original_form": "무서울것 같아서 안 봤는데..",
          "speaker_id": "1",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012294.1.9",
          "form": "좀 고어하고 무섭긴 해요!",
          "original_form": "좀 고어하고 무섭긴 해요!",
          "speaker_id": "2",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012294.1.10",
          "form": "안 좋아하시면 못 볼 거 같아요ㅠㅜ",
          "original_form": "안 좋아하시면 못 볼 거 같아요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012294.1.11",
          "form": "ㅠㅠ그쵸...그럴 것 같아서 못 보겠어요.",
          "original_form": "ㅠㅠ그쵸...그럴 것 같아서 못 보겠어요.",
          "speaker_id": "1",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012294.1.12",
          "form": "신랑이 킹덤 재밌다고 보라했는데 도저히 못 보겠더라고요 ㅠ",
          "original_form": "신랑이 킹덤 재밌다고 보라했는데 도저히 못 보겠더라고요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012294.1.13",
          "form": "저는 영화나 영상은 무서운 거 잘 보는 편이라 재밌게 봤어요",
          "original_form": "저는 영화나 영상은 무서운 거 잘 보는 편이라 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012294.1.14",
          "form": "킹덤이 좀비물이죠?",
          "original_form": "킹덤이 좀비물이죠?",
          "speaker_id": "2",
          "time": "20210824 21:02"
        },
        {
          "id": "MDRW2100012294.1.15",
          "form": "무서운 거 좀비물 이런거 진짜 못 봐요",
          "original_form": "무서운 거 좀비물 이런거 진짜 못 봐요",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012294.1.16",
          "form": "네네~",
          "original_form": "네네~",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012294.1.17",
          "form": "그럼 어떤 장르 좋아하세요?",
          "original_form": "그럼 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012294.1.18",
          "form": "로맨틱코미디 제일 좋아해요ㅎㅎ\n 그냥 생각없이 볼 수 있는 거",
          "original_form": "로맨틱코미디 제일 좋아해요ㅎㅎ\n 그냥 생각없이 볼 수 있는 거",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012294.1.19",
          "form": "로코 좋죠!",
          "original_form": "로코 좋죠!",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012294.1.20",
          "form": "재밌게 본 작품 있으세요?",
          "original_form": "재밌게 본 작품 있으세요?",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012294.1.21",
          "form": "음..최근에 본 로코는 뭔지 기억은 잘 안 나구...\n요즘엔 슬의생 봐요ㅋㅋㅋ 그건 로코는 아닌 것 같죠??ㅋㅋ",
          "original_form": "음..최근에 본 로코는 뭔지 기억은 잘 안 나구...\n요즘엔 슬의생 봐요ㅋㅋㅋ 그건 로코는 아닌 것 같죠??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012294.1.22",
          "form": "슬의생 많이 보시더라고요!",
          "original_form": "슬의생 많이 보시더라고요!",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012294.1.23",
          "form": "의학 드라마인가요? 로맨스 쪽에 가까운가요?",
          "original_form": "의학 드라마인가요? 로맨스 쪽에 가까운가요?",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100012294.1.24",
          "form": "의학 드라마...인듯 그냥 사는 얘기인듯 해요.\n로맨스는 쪼오금?",
          "original_form": "의학 드라마...인듯 그냥 사는 얘기인듯 해요.\n로맨스는 쪼오금?",
          "speaker_id": "1",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012294.1.25",
          "form": "편하게 볼 수 있겠네요!",
          "original_form": "편하게 볼 수 있겠네요!",
          "speaker_id": "2",
          "time": "20210824 21:09"
        }
      ]
    }
