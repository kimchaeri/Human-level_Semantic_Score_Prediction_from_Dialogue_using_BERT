      "utterance": [
        {
          "id": "MDRW2100024286.1.1",
          "form": "제가김밥을싸고 설거지까지다했거든요?",
          "original_form": "제가김밥을싸고 설거지까지다했거든요?",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024286.1.2",
          "form": "친정오빠 있어요 하나",
          "original_form": "친정오빠 있어요 하나",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024286.1.3",
          "form": "친오빠도 결혼하셨구나ㅎㅎ",
          "original_form": "친오빠도 결혼하셨구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024286.1.4",
          "form": "그랬는데 제가쉬면서 전화기달랬더니",
          "original_form": "그랬는데 제가쉬면서 전화기달랬더니",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024286.1.5",
          "form": "너는 손하나까딱안하냐면서",
          "original_form": "너는 손하나까딱안하냐면서",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024286.1.6",
          "form": "부모님 좋아하시겠어여ㅎㅎ자식들 다 결혼해서ㅎㅎ",
          "original_form": "부모님 좋아하시겠어여ㅎㅎ자식들 다 결혼해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024286.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024286.1.8",
          "form": "헐ㅡㅡ너무하네요",
          "original_form": "헐ㅡㅡ너무하네요",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100024286.1.9",
          "form": "집안일 다 했는데",
          "original_form": "집안일 다 했는데",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.10",
          "form": "엄마가",
          "original_form": "엄마가",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.11",
          "form": "너는 와서 요리라도하지",
          "original_form": "너는 와서 요리라도하지",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.12",
          "form": "며느리는 와서 애나보지 머하냐고",
          "original_form": "며느리는 와서 애나보지 머하냐고",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.13",
          "form": "시는 시더라구요ㅋㅋㅋㅋ",
          "original_form": "시는 시더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.14",
          "form": "엄마가 편들어주셨네요",
          "original_form": "엄마가 편들어주셨네요",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.15",
          "form": "애보는게 어디쉽나요",
          "original_form": "애보는게 어디쉽나요",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.16",
          "form": "새언니는 좀 섭섭했을듯",
          "original_form": "새언니는 좀 섭섭했을듯",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.17",
          "form": "오빠없을때ㅋㅋㅋ",
          "original_form": "오빠없을때ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024286.1.18",
          "form": "우리엄마도 나중에 시짜 될듯요",
          "original_form": "우리엄마도 나중에 시짜 될듯요",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024286.1.19",
          "form": "이번에 귀찮다고 저희만오라고",
          "original_form": "이번에 귀찮다고 저희만오라고",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024286.1.20",
          "form": "불쌍한 예비며느리...",
          "original_form": "불쌍한 예비며느리...",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024286.1.21",
          "form": "어쩔수없어요",
          "original_form": "어쩔수없어요",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024286.1.22",
          "form": "우리는 예비시어머니",
          "original_form": "우리는 예비시어머니",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024286.1.23",
          "form": "나이먹으면 입닫고 지갑열라는데",
          "original_form": "나이먹으면 입닫고 지갑열라는데",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024286.1.24",
          "form": "지갑에 돈이없어요ㅠㅠ",
          "original_form": "지갑에 돈이없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024286.1.25",
          "form": "맞아요ㅋㅋ입닫고 지갑열고ㅋㅋ",
          "original_form": "맞아요ㅋㅋ입닫고 지갑열고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024286.1.26",
          "form": "입이라도닫아야죠",
          "original_form": "입이라도닫아야죠",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024286.1.27",
          "form": "전 제 아이가 결혼안해도 상관없어요ㅋㅋ",
          "original_form": "전 제 아이가 결혼안해도 상관없어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024286.1.28",
          "form": "할수있는건해야지",
          "original_form": "할수있는건해야지",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024286.1.29",
          "form": "저희딸은",
          "original_form": "저희딸은",
          "speaker_id": "2",
          "time": "20210831 00:13"
        }
      ]
    }
  ]
