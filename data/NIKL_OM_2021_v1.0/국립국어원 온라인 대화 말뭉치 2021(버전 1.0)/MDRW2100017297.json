      "utterance": [
        {
          "id": "MDRW2100017297.1.1",
          "form": "그래도 학생때는 다들 좋아하면서 했자늠 ㅋㅋ",
          "original_form": "그래도 학생때는 다들 좋아하면서 했자늠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017297.1.2",
          "form": "빈혈도 있고 몸약하니까 멀리함",
          "original_form": "빈혈도 있고 몸약하니까 멀리함",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017297.1.3",
          "form": "그건 그냥 다들 이미 피곤하고",
          "original_form": "그건 그냥 다들 이미 피곤하고",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017297.1.4",
          "form": "혈관 잘 안잡히고 그래서 그러는거일든",
          "original_form": "혈관 잘 안잡히고 그래서 그러는거일든",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017297.1.5",
          "form": "일듯",
          "original_form": "일듯",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017297.1.6",
          "form": "컨디션필수임",
          "original_form": "컨디션필수임",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017297.1.7",
          "form": "타고난건 어쩔수없음",
          "original_form": "타고난건 어쩔수없음",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017297.1.8",
          "form": "나는 일부러 살찌면감",
          "original_form": "나는 일부러 살찌면감",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017297.1.9",
          "form": "난 일부러 잘자거나 비싼거 사먹고감",
          "original_form": "난 일부러 잘자거나 비싼거 사먹고감",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017297.1.10",
          "form": "약간 미신같지만 ㅋㅋ",
          "original_form": "약간 미신같지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017297.1.11",
          "form": "밥 다먹고",
          "original_form": "밥 다먹고",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017297.1.12",
          "form": "살찌면 안튕김",
          "original_form": "살찌면 안튕김",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017297.1.13",
          "form": "에이그건 아님",
          "original_form": "에이그건 아님",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017297.1.14",
          "form": "난 고혈압으로 제법튕김",
          "original_form": "난 고혈압으로 제법튕김",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017297.1.15",
          "form": "난 그래야 피잘나는뎅",
          "original_form": "난 그래야 피잘나는뎅",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017297.1.16",
          "form": "그리고 나 최근 살 많이쪘는데",
          "original_form": "그리고 나 최근 살 많이쪘는데",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017297.1.17",
          "form": "난 저혈압인데 살찌면 피 뿜뿜함",
          "original_form": "난 저혈압인데 살찌면 피 뿜뿜함",
          "speaker_id": "1",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100017297.1.18",
          "form": "철분은 부족함 ㅋㅋ",
          "original_form": "철분은 부족함 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
