      "utterance": [
        {
          "id": "MDRW2100025538.1.1",
          "form": "곰표 맥주 드셔보셨나요",
          "original_form": "곰표 맥주 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025538.1.2",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025538.1.3",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025538.1.4",
          "form": "밀맥주에요",
          "original_form": "밀맥주에요",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025538.1.5",
          "form": "말표 흑맥주 드셔보셨나요",
          "original_form": "말표 흑맥주 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025538.1.6",
          "form": "보기만요",
          "original_form": "보기만요",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025538.1.7",
          "form": "맛없어요",
          "original_form": "맛없어요",
          "speaker_id": "1",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025538.1.8",
          "form": "드시지마세요",
          "original_form": "드시지마세요",
          "speaker_id": "1",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025538.1.9",
          "form": "최근에 곰표 콜라보 치킨너겟 나왔어요",
          "original_form": "최근에 곰표 콜라보 치킨너겟 나왔어요",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025538.1.10",
          "form": "보신적 있으세요?",
          "original_form": "보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025538.1.11",
          "form": "네 편의점에성",
          "original_form": "네 편의점에성",
          "speaker_id": "2",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025538.1.12",
          "form": "오 전 먹어본적 없어서 궁금해요",
          "original_form": "오 전 먹어본적 없어서 궁금해요",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025538.1.13",
          "form": "곰돌이 모양 너겟이라서 엄청 귀여워 보이더라구요",
          "original_form": "곰돌이 모양 너겟이라서 엄청 귀여워 보이더라구요",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025538.1.14",
          "form": "펭수 케이크는 드셔보셨나요?",
          "original_form": "펭수 케이크는 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025538.1.15",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100025538.1.16",
          "form": "맛있다고해요",
          "original_form": "맛있다고해요",
          "speaker_id": "1",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100025538.1.17",
          "form": "그런데 자를때 힘들대요",
          "original_form": "그런데 자를때 힘들대요",
          "speaker_id": "1",
          "time": "20210901 14:39"
        },
        {
          "id": "MDRW2100025538.1.18",
          "form": "잘 부서지나요?",
          "original_form": "잘 부서지나요?",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025538.1.19",
          "form": "펭귄 모양이다보니 자르면 얼굴부위가 없어져서요 ㅋㅋㅋ",
          "original_form": "펭귄 모양이다보니 자르면 얼굴부위가 없어져서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025538.1.20",
          "form": "잔인한느낌이 든대요",
          "original_form": "잔인한느낌이 든대요",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100025538.1.21",
          "form": "헉 동심파괸 ㅋㅋ",
          "original_form": "헉 동심파괸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100025538.1.22",
          "form": "펭수 콜라보 케이크가 예쁘고 맛있어서 인기 많더라구요",
          "original_form": "펭수 콜라보 케이크가 예쁘고 맛있어서 인기 많더라구요",
          "speaker_id": "1",
          "time": "20210901 18:49"
        }
