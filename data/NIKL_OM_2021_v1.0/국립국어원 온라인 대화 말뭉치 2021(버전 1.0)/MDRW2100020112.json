      "utterance": [
        {
          "id": "MDRW2100020112.1.1",
          "form": "저는 가을이좋더라구요!!!!",
          "original_form": "저는 가을이좋더라구요!!!!",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020112.1.2",
          "form": "동생은요??!",
          "original_form": "동생은요??!",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020112.1.3",
          "form": "저도 가을 좋아해요! 딱 지금계절!!",
          "original_form": "저도 가을 좋아해요! 딱 지금계절!!",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020112.1.4",
          "form": "전 진짜",
          "original_form": "전 진짜",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020112.1.5",
          "form": "유일무이하게",
          "original_form": "유일무이하게",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020112.1.6",
          "form": "name1말고는,,,ㅋㅋㅋㅋ",
          "original_form": "&name1&말고는,,,ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020112.1.7",
          "form": "가을이 선선하기도하고 ㅠㅠ",
          "original_form": "가을이 선선하기도하고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020112.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 통했네요~~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 통했네요~~",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020112.1.9",
          "form": "가을 선선하고 좋아요",
          "original_form": "가을 선선하고 좋아요",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020112.1.10",
          "form": "아 역쉬역쉬~ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 역쉬역쉬~ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020112.1.11",
          "form": "하늘두맑고!",
          "original_form": "하늘두맑고!",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020112.1.12",
          "form": "바람도 적당하곰",
          "original_form": "바람도 적당하곰",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020112.1.13",
          "form": "마쟈욤 ㅋㅋㅋㅋ 그담은 봄이좋아요! 너무 당연한건가요?ㅋㅋ",
          "original_form": "마쟈욤 ㅋㅋㅋㅋ 그담은 봄이좋아요! 너무 당연한건가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020112.1.14",
          "form": "어 저두!ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 저두!ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020112.1.15",
          "form": "그단은 겨울!",
          "original_form": "그단은 겨울!",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020112.1.16",
          "form": "아 그렇군요! 저는 그담이 여름이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그렇군요! 저는 그담이 여름이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020112.1.17",
          "form": "여기서갈리네여",
          "original_form": "여기서갈리네여",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020112.1.18",
          "form": "ㅋㅋㅋㅋ눈을좋아랴서",
          "original_form": "ㅋㅋㅋㅋ눈을좋아랴서",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020112.1.19",
          "form": "좋아해서",
          "original_form": "좋아해서",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020112.1.20",
          "form": "아 저도 눈좋아해요!",
          "original_form": "아 저도 눈좋아해요!",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020112.1.21",
          "form": "부산은 워낙 눈이 귀해서 더 좋아햐요 ㅠㅠ",
          "original_form": "부산은 워낙 눈이 귀해서 더 좋아햐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020112.1.22",
          "form": "하 대구도 마찬가지ㅠㅠㅠㅠ",
          "original_form": "하 대구도 마찬가지ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020112.1.23",
          "form": "철원에서 군생활하는동안",
          "original_form": "철원에서 군생활하는동안",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020112.1.24",
          "form": "눈 실컷봤는데 ㅠㅠ",
          "original_form": "눈 실컷봤는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020112.1.25",
          "form": "엇 철원이면 경기도인가요??",
          "original_form": "엇 철원이면 경기도인가요??",
          "speaker_id": "1",
