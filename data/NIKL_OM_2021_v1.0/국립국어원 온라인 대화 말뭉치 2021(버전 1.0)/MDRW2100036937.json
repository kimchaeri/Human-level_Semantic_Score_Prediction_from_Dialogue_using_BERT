      "utterance": [
        {
          "id": "MDRW2100036937.1.1",
          "form": "안녕하셔요",
          "original_form": "안녕하셔요",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100036937.1.2",
          "form": "오늘은 뭐 구입하신거 있나요?",
          "original_form": "오늘은 뭐 구입하신거 있나요?",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036937.1.3",
          "form": "오늘 아이스크림 사러가용ㅎㅎ",
          "original_form": "오늘 아이스크림 사러가용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036937.1.4",
          "form": "와 어디로요?",
          "original_form": "와 어디로요?",
          "speaker_id": "1",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036937.1.5",
          "form": "베스킨러빈스욥ㅎㅎㅎ",
          "original_form": "베스킨러빈스욥ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036937.1.6",
          "form": "민초파세요?",
          "original_form": "민초파세요?",
          "speaker_id": "1",
          "time": "20210831 15:39"
        },
        {
          "id": "MDRW2100036937.1.7",
          "form": "초코칩, 아몬드 등 딱딱한 무언가가 들어가면 다 싫어용",
          "original_form": "초코칩, 아몬드 등 딱딱한 무언가가 들어가면 다 싫어용",
          "speaker_id": "2",
          "time": "20210831 15:39"
        },
        {
          "id": "MDRW2100036937.1.8",
          "form": "민초에 초코 무조건 들어가는건 아니에요",
          "original_form": "민초에 초코 무조건 들어가는건 아니에요",
          "speaker_id": "1",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036937.1.9",
          "form": "저 민초 안할래요. 민초는 1년에 1번 먹어야해요",
          "original_form": "저 민초 안할래요. 민초는 1년에 1번 먹어야해요",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036937.1.10",
          "form": "신기한 규칙이냉6",
          "original_form": "신기한 규칙이냉6",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036937.1.11",
          "form": "ㅠㅠㅠ저번달에 먹고 지쳤어요",
          "original_form": "ㅠㅠㅠ저번달에 먹고 지쳤어요",
          "speaker_id": "2",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036937.1.12",
          "form": "전 안 지쳐요",
          "original_form": "전 안 지쳐요",
