      "utterance": [
        {
          "id": "MDRW2100042012.1.1",
          "form": "건강을 위해서 무엇을 하나요",
          "original_form": "건강을 위해서 무엇을 하나요",
          "speaker_id": "1",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100042012.1.2",
          "form": "저는 물을 자주 먹어요",
          "original_form": "저는 물을 자주 먹어요",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100042012.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100042012.1.4",
          "form": "전 한시간씩 빨리걷기를 해요",
          "original_form": "전 한시간씩 빨리걷기를 해요",
          "speaker_id": "1",
          "time": "20210906 18:51"
        },
        {
          "id": "MDRW2100042012.1.5",
          "form": "아침에 하시나요 아님 저녁에 하기나요?",
          "original_form": "아침에 하시나요 아님 저녁에 하기나요?",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042012.1.6",
          "form": "저녁먹고 해요",
          "original_form": "저녁먹고 해요",
          "speaker_id": "1",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100042012.1.7",
          "form": "보통 며칠에 한번 하시나요?",
          "original_form": "보통 며칠에 한번 하시나요?",
          "speaker_id": "2",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042012.1.8",
          "form": "일주일에 5일정도 합니다",
          "original_form": "일주일에 5일정도 합니다",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042012.1.9",
          "form": "그럼 평일은 매일하시는건가요?",
          "original_form": "그럼 평일은 매일하시는건가요?",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042012.1.10",
          "form": "네 수 목요일 하루 쉽니다",
          "original_form": "네 수 목요일 하루 쉽니다",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042012.1.11",
          "form": "물은 하루 얼마나 드시나요",
          "original_form": "물은 하루 얼마나 드시나요",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042012.1.12",
          "form": "매일 2리터는 먹으려고해요",
          "original_form": "매일 2리터는 먹으려고해요",
          "speaker_id": "2",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100042012.1.13",
          "form": "하루 2리터 먹기 힘들지 않아요",
          "original_form": "하루 2리터 먹기 힘들지 않아요",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042012.1.14",
          "form": "아직까지는 괜찮은 것 같아요",
          "original_form": "아직까지는 괜찮은 것 같아요",
          "speaker_id": "2",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042012.1.15",
          "form": "name1님이야말로 매일 운동하시는거 안힘드신가요?",
          "original_form": "&name1&님이야말로 매일 운동하시는거 안힘드신가요?",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042012.1.16",
          "form": "전 물먹는것이 힘들어요",
          "original_form": "전 물먹는것이 힘들어요",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042012.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
