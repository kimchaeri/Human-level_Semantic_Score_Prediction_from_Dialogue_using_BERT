      "utterance": [
        {
          "id": "MDRW2100021686.1.1",
          "form": "저녁에 뭐드셨나요?",
          "original_form": "저녁에 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021686.1.2",
          "form": "저 롯데리아 햄버거 먹었어요 ㅎㅎ",
          "original_form": "저 롯데리아 햄버거 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021686.1.3",
          "form": "무슨버거 먹었게요~?",
          "original_form": "무슨버거 먹었게요~?",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021686.1.4",
          "form": "모짜렐라 베이컨??",
          "original_form": "모짜렐라 베이컨??",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021686.1.5",
          "form": "틀렸어용!!",
          "original_form": "틀렸어용!!",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021686.1.6",
          "form": "한우버거??",
          "original_form": "한우버거??",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021686.1.7",
          "form": "허걱 정답이에요 ㅋㅋㅋ",
          "original_form": "허걱 정답이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021686.1.8",
          "form": "저 한우불고기버거 세트 먹었어요~!!",
          "original_form": "저 한우불고기버거 세트 먹었어요~!!",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021686.1.9",
          "form": "이번에 뭐가 25% 정도 더 커졌다고 리뉴얼 됐대용",
          "original_form": "이번에 뭐가 25% 정도 더 커졌다고 리뉴얼 됐대용",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021686.1.10",
          "form": "ㅋㅋㅋㅋ제가 롯데리아 자주 가서 포스터 가끔 봤는데 고기패티? 증량일걸요??ㅎ 맛있으셨나요?",
          "original_form": "ㅋㅋㅋㅋ제가 롯데리아 자주 가서 포스터 가끔 봤는데 고기패티? 증량일걸요??ㅎ 맛있으셨나요?",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021686.1.11",
          "form": "저는 안먹어봐서ㅎ",
          "original_form": "저는 안먹어봐서ㅎ",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021686.1.12",
          "form": "롯데리아 메뉴 중에서 그게 젤 맛있는 것 같아요 ㅎㅎ",
          "original_form": "롯데리아 메뉴 중에서 그게 젤 맛있는 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021686.1.13",
          "form": "그나저나 야채라이스버거 재출시 해줬으면 좋겠네요 ㅠㅠ",
          "original_form": "그나저나 야채라이스버거 재출시 해줬으면 좋겠네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021686.1.14",
          "form": "야채라이스 한번도 안먹어봤는데 맛있나요??",
          "original_form": "야채라이스 한번도 안먹어봤는데 맛있나요??",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021686.1.15",
          "form": "네 엄청 맛있는데 왜 없어졌을까요...",
          "original_form": "네 엄청 맛있는데 왜 없어졌을까요...",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021686.1.16",
          "form": "그게 빵 대신 밥이 있는데 볶음밥이거든요",
          "original_form": "그게 빵 대신 밥이 있는데 볶음밥이거든요",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021686.1.17",
          "form": "그게 지금 봉구스밥의 시초라고 보면 될 것 같아요",
          "original_form": "그게 지금 봉구스밥의 시초라고 보면 될 것 같아요",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021686.1.18",
          "form": "그럼 롯데리아 재출시 투표 하셨겠네요??",
          "original_form": "그럼 롯데리아 재출시 투표 하셨겠네요??",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021686.1.19",
          "form": "어 알고 계시네요!",
          "original_form": "어 알고 계시네요!",
          "speaker_id": "1",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021686.1.20",
          "form": "그거 지금이 두번째 하는거죠? 그거 투표했어요 야채라이스버거에~",
          "original_form": "그거 지금이 두번째 하는거죠? 그거 투표했어요 야채라이스버거에~",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021686.1.21",
          "form": "님은 어디에 투표 했어요?",
          "original_form": "님은 어디에 투표 했어요?",
