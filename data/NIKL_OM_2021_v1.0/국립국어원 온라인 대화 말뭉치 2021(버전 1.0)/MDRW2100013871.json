      "utterance": [
        {
          "id": "MDRW2100013871.1.1",
          "form": "여행지 기억남는곳있으시나요?",
          "original_form": "여행지 기억남는곳있으시나요?",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013871.1.2",
          "form": "저는 파리에 8일정도 있었는데 좋았어요",
          "original_form": "저는 파리에 8일정도 있었는데 좋았어요",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013871.1.3",
          "form": "걷기만해도 ㅎㅎ",
          "original_form": "걷기만해도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013871.1.4",
          "form": "좋았겠네요",
          "original_form": "좋았겠네요",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013871.1.5",
          "form": "기념품은 사셨나요?",
          "original_form": "기념품은 사셨나요?",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013871.1.6",
          "form": "저는 가방을 샀어요",
          "original_form": "저는 가방을 샀어요",
          "speaker_id": "1",
          "time": "20210826 09:04"
        },
        {
          "id": "MDRW2100013871.1.7",
          "form": "역시 파리네요",
          "original_form": "역시 파리네요",
          "speaker_id": "2",
          "time": "20210826 09:13"
        },
        {
          "id": "MDRW2100013871.1.8",
          "form": "네 여행 기념품이라기엔 너무 목적있는 쇼핑이었어요ㅎㅎ",
          "original_form": "네 여행 기념품이라기엔 너무 목적있는 쇼핑이었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100013871.1.9",
          "form": "할인 많이 됐나요?",
          "original_form": "할인 많이 됐나요?",
          "speaker_id": "2",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100013871.1.10",
          "form": "할인이 목적은 아니었고 제품이 다양했어요 세관신고하면 약간 저렴했어요",
          "original_form": "할인이 목적은 아니었고 제품이 다양했어요 세관신고하면 약간 저렴했어요",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100013871.1.11",
          "form": "또 언재 파리 가보려나요",
          "original_form": "또 언재 파리 가보려나요",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100013871.1.12",
          "form": "그러게요 좋은 경험이셨겠어요",
          "original_form": "그러게요 좋은 경험이셨겠어요",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100013871.1.13",
          "form": "계획 세우고 가는 스탈이시나요?",
          "original_form": "계획 세우고 가는 스탈이시나요?",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100013871.1.14",
          "form": "네 ㄱ근데 계획을 좀 널널하게 잡아요",
          "original_form": "네 ㄱ근데 계획을 좀 널널하게 잡아요",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100013871.1.15",
          "form": "좋네요 계획안세우고 가면 할일이 없더라고요",
          "original_form": "좋네요 계획안세우고 가면 할일이 없더라고요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100013871.1.16",
          "form": "네 당일 투어 같은거 미리 예약하고 그정도는 준비해요",
          "original_form": "네 당일 투어 같은거 미리 예약하고 그정도는 준비해요",
          "speaker_id": "1",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100013871.1.17",
          "form": "맛집찾아가시나요?",
          "original_form": "맛집찾아가시나요?",
          "speaker_id": "2",
          "time": "20210826 11:33"
        },
        {
          "id": "MDRW2100013871.1.18",
          "form": "여행때는 가보려고 하는편이에요",
          "original_form": "여행때는 가보려고 하는편이에요",
          "speaker_id": "1",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100013871.1.19",
          "form": "맞아요 ㅎㅎ 항상 조심해야돼요 추석땐 여행 계획 있으신가요",
          "original_form": "맞아요 ㅎㅎ 항상 조심해야돼요 추석땐 여행 계획 있으신가요",
          "speaker_id": "1",
          "time": "20210903 16:06"
        }
