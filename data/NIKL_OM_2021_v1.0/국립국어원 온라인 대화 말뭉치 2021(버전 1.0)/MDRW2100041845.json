      "utterance": [
        {
          "id": "MDRW2100041845.1.1",
          "form": "난 작은건줄 알았는데~",
          "original_form": "난 작은건줄 알았는데~",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041845.1.2",
          "form": "사이즈도 딱좋고~!!",
          "original_form": "사이즈도 딱좋고~!!",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041845.1.3",
          "form": "응 보기에도 커보이더라 ㅋㅋ",
          "original_form": "응 보기에도 커보이더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041845.1.4",
          "form": "좋구만ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋구만ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041845.1.5",
          "form": "응~완젼 만족 ㅋㅋㅋㅋㅋ",
          "original_form": "응~완젼 만족 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:36"
        },
        {
          "id": "MDRW2100041845.1.6",
          "form": "이러니.오프라인으로는 못삼 ㅋㅋㅋㅋ",
          "original_form": "이러니.오프라인으로는 못삼 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041845.1.7",
          "form": "맞어 맞어",
          "original_form": "맞어 맞어",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100041845.1.8",
          "form": "보고에서 가성비 좋은게 많더라",
          "original_form": "보고에서 가성비 좋은게 많더라",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041845.1.9",
          "form": "괜히 삼성카드 해지한 느낌",
          "original_form": "괜히 삼성카드 해지한 느낌",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041845.1.10",
          "form": "나도삼 카 하나 만들어야 되겠다ㅋㅋ",
          "original_form": "나도삼 카 하나 만들어야 되겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041845.1.11",
          "form": "과자도 행사 많이 하고",
          "original_form": "과자도 행사 많이 하고",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041845.1.12",
          "form": "그러니깐 ㅋㅋ 카드만들때는 뭐다?",
          "original_form": "그러니깐 ㅋㅋ 카드만들때는 뭐다?",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041845.1.13",
          "form": "이벤트 통해서 만들어야된다. 히히",
          "original_form": "이벤트 통해서 만들어야된다. 히히",
          "speaker_id": "1",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100041845.1.14",
          "form": "삼카 나중에 만들어야되겟따. 나는 해지 한지 얼마 안되서 ㅋ",
          "original_form": "삼카 나중에 만들어야되겟따. 나는 해지 한지 얼마 안되서 ㅋ",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041845.1.15",
          "form": "네이버에 삼 카 행사 든데",
          "original_form": "네이버에 삼 카 행사 든데",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041845.1.16",
          "form": "13만 포인트",
          "original_form": "13만 포인트",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041845.1.17",
          "form": "하나 파야겠다ㅋㅋㅋㅋ",
          "original_form": "하나 파야겠다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041845.1.18",
          "form": "응응 나 그거했다가 ㅋㅋㅋ 해지했었잖아 ㅋ",
          "original_form": "응응 나 그거했다가 ㅋㅋㅋ 해지했었잖아 ㅋ",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100041845.1.19",
          "form": "빠르다",
          "original_form": "빠르다",
          "speaker_id": "2",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041845.1.20",
          "form": "근데 그거 좋긴했다. ㅋㅋ 네이버포인트로도 조금씩 들어오구",
          "original_form": "근데 그거 좋긴했다. ㅋㅋ 네이버포인트로도 조금씩 들어오구",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041845.1.21",
          "form": "나 국민카드 해지해야 하는데",
          "original_form": "나 국민카드 해지해야 하는데",
          "speaker_id": "2",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041845.1.22",
          "form": "전에 13만 원 포인트",
          "original_form": "전에 13만 원 포인트",
          "speaker_id": "2",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041845.1.23",
          "form": "해지할거 있음 얼른 해지해 ㅋㅋ",
          "original_form": "해지할거 있음 얼른 해지해 ㅋㅋ",
