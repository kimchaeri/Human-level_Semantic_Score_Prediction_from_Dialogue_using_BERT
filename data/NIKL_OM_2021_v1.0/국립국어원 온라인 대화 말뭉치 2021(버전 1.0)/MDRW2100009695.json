      "utterance": [
        {
          "id": "MDRW2100009695.1.1",
          "form": "요즘 날씨가 선선하죠?",
          "original_form": "요즘 날씨가 선선하죠?",
          "speaker_id": "1",
          "time": "20210823 04:41"
        },
        {
          "id": "MDRW2100009695.1.2",
          "form": "이제 가을 느낌이에요 진짜",
          "original_form": "이제 가을 느낌이에요 진짜",
          "speaker_id": "2",
          "time": "20210823 04:42"
        },
        {
          "id": "MDRW2100009695.1.3",
          "form": "22도 정도라 이제 긴팔옷들 꺼냈어요",
          "original_form": "22도 정도라 이제 긴팔옷들 꺼냈어요",
          "speaker_id": "2",
          "time": "20210823 04:42"
        },
        {
          "id": "MDRW2100009695.1.4",
          "form": "오늘이 처서네요",
          "original_form": "오늘이 처서네요",
          "speaker_id": "1",
          "time": "20210823 04:42"
        },
        {
          "id": "MDRW2100009695.1.5",
          "form": "처서 매직인가",
          "original_form": "처서 매직인가",
          "speaker_id": "1",
          "time": "20210823 04:42"
        },
        {
          "id": "MDRW2100009695.1.6",
          "form": "조상님들 사이언스,,",
          "original_form": "조상님들 사이언스,,",
          "speaker_id": "2",
          "time": "20210823 04:43"
        },
        {
          "id": "MDRW2100009695.1.7",
          "form": "저는 열이 많은 편이라 아직 반팔이에요",
          "original_form": "저는 열이 많은 편이라 아직 반팔이에요",
          "speaker_id": "1",
          "time": "20210823 04:43"
        },
        {
          "id": "MDRW2100009695.1.8",
          "form": "하늘도 푸르고 어쩐지 식욕이 돋더라니 가을이네요",
          "original_form": "하늘도 푸르고 어쩐지 식욕이 돋더라니 가을이네요",
          "speaker_id": "2",
          "time": "20210823 04:43"
        },
        {
          "id": "MDRW2100009695.1.9",
          "form": "금새 지나갈것같네요",
          "original_form": "금새 지나갈것같네요",
          "speaker_id": "1",
          "time": "20210823 04:43"
        },
        {
          "id": "MDRW2100009695.1.10",
          "form": "겨울 금방 오잖아요",
          "original_form": "겨울 금방 오잖아요",
          "speaker_id": "1",
          "time": "20210823 04:43"
        },
        {
          "id": "MDRW2100009695.1.11",
          "form": "코트 한두번 입다가 금방 롱패딩 꺼내고 막ㅜ",
          "original_form": "코트 한두번 입다가 금방 롱패딩 꺼내고 막ㅜ",
          "speaker_id": "2",
          "time": "20210823 04:44"
        },
        {
          "id": "MDRW2100009695.1.12",
          "form": "제가 사는 곳은 롱패딩 입을 정도는 아닌데",
          "original_form": "제가 사는 곳은 롱패딩 입을 정도는 아닌데",
          "speaker_id": "1",
          "time": "20210823 04:44"
        },
        {
          "id": "MDRW2100009695.1.13",
          "form": "코트 입고 다니면 쳐다보긴 해요",
          "original_form": "코트 입고 다니면 쳐다보긴 해요",
          "speaker_id": "1",
          "time": "20210823 04:44"
        },
        {
          "id": "MDRW2100009695.1.14",
          "form": "올해도 무지 추울것같은데 여름보단 낫을것같아요",
          "original_form": "올해도 무지 추울것같은데 여름보단 낫을것같아요",
          "speaker_id": "1",
          "time": "20210823 04:45"
        },
        {
          "id": "MDRW2100009695.1.15",
          "form": "여름은 더 더워지고 겨울은 더 추워지는 것 같아요",
          "original_form": "여름은 더 더워지고 겨울은 더 추워지는 것 같아요",
          "speaker_id": "2",
          "time": "20210823 04:45"
        },
        {
          "id": "MDRW2100009695.1.16",
          "form": "지구온난화가 어쩌고..",
          "original_form": "지구온난화가 어쩌고..",
          "speaker_id": "2",
          "time": "20210823 04:46"
        },
        {
          "id": "MDRW2100009695.1.17",
          "form": "동남아처럼 습해지고 스콜 현상도 잦아지고 있어요",
          "original_form": "동남아처럼 습해지고 스콜 현상도 잦아지고 있어요",
          "speaker_id": "1",
          "time": "20210823 04:46"
        },
        {
          "id": "MDRW2100009695.1.18",
          "form": "맞앙ㅎ 요새 하늘 맑다가 갑자기 비 쏟아지는 일이 많아서 되게 동남아같은 기분이었어요",
          "original_form": "맞앙ㅎ 요새 하늘 맑다가 갑자기 비 쏟아지는 일이 많아서 되게 동남아같은 기분이었어요",
          "speaker_id": "2",
          "time": "20210823 04:47"
        },
        {
          "id": "MDRW2100009695.1.19",
          "form": "우산 들고 다니지 않는다면 낭패 보기 쉬워요",
          "original_form": "우산 들고 다니지 않는다면 낭패 보기 쉬워요",
          "speaker_id": "1",
          "time": "20210823 04:47"
        },
        {
          "id": "MDRW2100009695.1.20",
          "form": "오타봐 참나 암튼 무슨 보고서 봤는데 이제 사과 재배하던 곳들에서 키위 키울거래서 진짜 망했구나 싶었어요",
          "original_form": "오타봐 참나 암튼 무슨 보고서 봤는데 이제 사과 재배하던 곳들에서 키위 키울거래서 진짜 망했구나 싶었어요",
          "speaker_id": "2",
          "time": "20210823 04:47"
        }
      ]
