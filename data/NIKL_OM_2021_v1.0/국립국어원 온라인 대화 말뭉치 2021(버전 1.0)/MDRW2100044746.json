      "utterance": [
        {
          "id": "MDRW2100044746.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044746.1.2",
          "form": "네. 내.",
          "original_form": "네. 내.",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044746.1.3",
          "form": "저는 와인말고 페일에일 좋아합니다",
          "original_form": "저는 와인말고 페일에일 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044746.1.4",
          "form": "페일에일은 맥주인가요?",
          "original_form": "페일에일은 맥주인가요?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044746.1.5",
          "form": "네. 에일이니깜요",
          "original_form": "네. 에일이니깜요",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044746.1.6",
          "form": "전 그런거 잘 몰라서...ㅎ 페일에일은 씁쓸할거같은 맛이네요",
          "original_form": "전 그런거 잘 몰라서...ㅎ 페일에일은 씁쓸할거같은 맛이네요",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044746.1.7",
          "form": "아닌데 색다른맛이에요",
          "original_form": "아닌데 색다른맛이에요",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044746.1.8",
          "form": "과일맛이 나나요??",
          "original_form": "과일맛이 나나요??",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044746.1.9",
          "form": "제가 과일맛을 좋아해서..ㅎ",
          "original_form": "제가 과일맛을 좋아해서..ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044746.1.10",
          "form": "네 나죠. 곰표맥주랑은 차웜이 다릅니다",
          "original_form": "네 나죠. 곰표맥주랑은 차웜이 다릅니다",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044746.1.11",
          "form": "페일에일은 어떤 종류가 있나요?",
          "original_form": "페일에일은 어떤 종류가 있나요?",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044746.1.12",
          "form": "제주 펠롱 에일이요",
          "original_form": "제주 펠롱 에일이요",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044746.1.13",
          "form": "드셔보셨나요??",
          "original_form": "드셔보셨나요??",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044746.1.14",
          "form": "네.. 먹어봤어요  저 못믿으시나보다",
          "original_form": "네.. 먹어봤어요  저 못믿으시나보다",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044746.1.15",
          "form": "골든에일도 좋아해요",
          "original_form": "골든에일도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044746.1.16",
          "form": "골든에일 저도 좋아해요ㅎㅎ 시원하면서도 깊은 맛이 느껴지더라고요",
          "original_form": "골든에일 저도 좋아해요ㅎㅎ 시원하면서도 깊은 맛이 느껴지더라고요",
          "speaker_id": "1",
          "time": "20210907 23:51"
        }
