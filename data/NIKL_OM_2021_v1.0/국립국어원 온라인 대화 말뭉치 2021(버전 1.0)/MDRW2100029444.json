        },
        {
          "id": "MDRW2100029444.1.2",
          "form": "name2이님의\r\n연애썰을 풀어봅시닼ㅋ",
          "original_form": "&name2&이님의\r\n연애썰을 풀어봅시닼ㅋ",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029444.1.3",
          "form": "저요?ㅋㅋ 다 아시는거 아니었나요?",
          "original_form": "저요?ㅋㅋ 다 아시는거 아니었나요?",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029444.1.4",
          "form": "여친있고\r\n결혼생각있는것만\r\n말씅하셨잖아염ㅋㅋ",
          "original_form": "여친있고\r\n결혼생각있는것만\r\n말씅하셨잖아염ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029444.1.5",
          "form": "아 그랬나요?",
          "original_form": "아 그랬나요?",
          "speaker_id": "2",
          "time": "20210903 14:43"
        },
        {
          "id": "MDRW2100029444.1.6",
          "form": "저와 제 여친은 어떻게 만났을까요?",
          "original_form": "저와 제 여친은 어떻게 만났을까요?",
          "speaker_id": "2",
          "time": "20210903 14:43"
        },
        {
          "id": "MDRW2100029444.1.7",
          "form": "궁금해요 ㅋㅋ\r\n어떻게 만나셨어요?",
          "original_form": "궁금해요 ㅋㅋ\r\n어떻게 만나셨어요?",
          "speaker_id": "1",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100029444.1.8",
          "form": "자만추세요?",
          "original_form": "자만추세요?",
          "speaker_id": "1",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100029444.1.9",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029444.1.10",
          "form": "소개팅입니다",
          "original_form": "소개팅입니다",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029444.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029444.1.12",
          "form": "누가 소개해 줬나염",
          "original_form": "누가 소개해 줬나염",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100029444.1.13",
          "form": "제 회사동료요",
          "original_form": "제 회사동료요",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100029444.1.14",
          "form": "여친쪽 주선자는 저도 모르는사람",
          "original_form": "여친쪽 주선자는 저도 모르는사람",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100029444.1.15",
          "form": "3번 만나고",
          "original_form": "3번 만나고",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100029444.1.16",
          "form": "여친이 먼저 사귀자고",
          "original_form": "여친이 먼저 사귀자고",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
