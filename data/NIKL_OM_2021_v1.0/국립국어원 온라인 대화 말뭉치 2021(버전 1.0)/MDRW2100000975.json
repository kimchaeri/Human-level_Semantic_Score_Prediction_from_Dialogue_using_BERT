      "utterance": [
        {
          "id": "MDRW2100000975.1.1",
          "form": "음료수 좋아하세요?",
          "original_form": "음료수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 21:00"
        },
        {
          "id": "MDRW2100000975.1.2",
          "form": "네 좋아하는 편인데 자제하고 있어요!",
          "original_form": "네 좋아하는 편인데 자제하고 있어요!",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000975.1.3",
          "form": "저도 하루에 한잔씩은 꼭 직장에서 마시는거같아여",
          "original_form": "저도 하루에 한잔씩은 꼭 직장에서 마시는거같아여",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000975.1.4",
          "form": "요즘 배달음식도 많이 시켜먹구요",
          "original_form": "요즘 배달음식도 많이 시켜먹구요",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000975.1.5",
          "form": "맞아요 배달음식에 콜라 딸려와서 먹게 되는 거 같아요",
          "original_form": "맞아요 배달음식에 콜라 딸려와서 먹게 되는 거 같아요",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000975.1.6",
          "form": "요즘 뭐많이시켜드새여?",
          "original_form": "요즘 뭐많이시켜드새여?",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000975.1.7",
          "form": "요즘 치킨이나 돈가스, 초밥 많이 시켜먹는 거 같아요.",
          "original_form": "요즘 치킨이나 돈가스, 초밥 많이 시켜먹는 거 같아요.",
          "speaker_id": "2",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100000975.1.8",
          "form": "치킨 저도요 !!",
          "original_form": "치킨 저도요 !!",
          "speaker_id": "1",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100000975.1.9",
          "form": "돈가스모밀",
          "original_form": "돈가스모밀",
          "speaker_id": "1",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100000975.1.10",
          "form": "초밥엔우동 최고 ㅠㅠ",
          "original_form": "초밥엔우동 최고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100000975.1.11",
          "form": "모밀도 최고죠! 면이랑 고기 조합 못 참겠어요",
          "original_form": "모밀도 최고죠! 면이랑 고기 조합 못 참겠어요",
          "speaker_id": "2",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100000975.1.12",
          "form": "치킨 어떤 브랜드 좋아하세요?",
          "original_form": "치킨 어떤 브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100000975.1.13",
          "form": "전 요즘에 처갓집이요!",
          "original_form": "전 요즘에 처갓집이요!",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100000975.1.14",
          "form": "족발도 좋아하세요?",
          "original_form": "족발도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100000975.1.15",
          "form": "족발보쌈도 자주 시켜먹어요ㅎㅎ",
          "original_form": "족발보쌈도 자주 시켜먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100000975.1.16",
          "form": "저도 처갓집 슈프림양념 좋아해요",
          "original_form": "저도 처갓집 슈프림양념 좋아해요",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100000975.1.17",
          "form": "어 저두요 !!",
          "original_form": "어 저두요 !!",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100000975.1.18",
          "form": "완전 최애치킨 ㅠ",
          "original_form": "완전 최애치킨 ㅠ",
