        {
          "id": "MDRW2100031129.1.1",
          "form": "어디 여행가고픈가요",
          "original_form": "어디 여행가고픈가요",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100031129.1.2",
          "form": "한동안 해외는 힘들 것 같고.. 국내여행 안 가본 곳으로 가고 싶네요",
          "original_form": "한동안 해외는 힘들 것 같고.. 국내여행 안 가본 곳으로 가고 싶네요",
          "speaker_id": "1",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100031129.1.3",
          "form": "군산도 재밌을 것 같아요",
          "original_form": "군산도 재밌을 것 같아요",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100031129.1.4",
          "form": "군산 좋아요",
          "original_form": "군산 좋아요",
          "speaker_id": "2",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100031129.1.5",
          "form": "가봤어요 ㅎㅎ",
          "original_form": "가봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100031129.1.6",
          "form": "당일치기로 가기도 좋을 것 같구요ㅎㅎ",
          "original_form": "당일치기로 가기도 좋을 것 같구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100031129.1.7",
          "form": "군산이 은근 볼게 많습니당",
          "original_form": "군산이 은근 볼게 많습니당",
          "speaker_id": "2",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100031129.1.8",
          "form": "어디 또 가보고 싶으신 곳 있으신가요?",
          "original_form": "어디 또 가보고 싶으신 곳 있으신가요?",
