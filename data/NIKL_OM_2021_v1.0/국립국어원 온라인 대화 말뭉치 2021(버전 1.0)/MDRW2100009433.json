      "utterance": [
        {
          "id": "MDRW2100009433.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210823 08:35"
        },
        {
          "id": "MDRW2100009433.1.2",
          "form": "아뇽",
          "original_form": "아뇽",
          "speaker_id": "2",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100009433.1.3",
          "form": "보는건 좋아하는데 무서워해서요ㅠㅠ",
          "original_form": "보는건 좋아하는데 무서워해서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100009433.1.4",
          "form": "저두용 ㅋㅋㅋㅋㅋ통하셨네요",
          "original_form": "저두용 ㅋㅋㅋㅋㅋ통하셨네요",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100009433.1.5",
          "form": "오오 신기해여 ㅋㅋㅋ",
          "original_form": "오오 신기해여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100009433.1.6",
          "form": "물릴까봐 무서워요 전 ㅋㅋ",
          "original_form": "물릴까봐 무서워요 전 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100009433.1.7",
          "form": "악 저두요 ㅋㅋㅋ 그래서 멀리 보기만 해요",
          "original_form": "악 저두요 ㅋㅋㅋ 그래서 멀리 보기만 해요",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100009433.1.8",
          "form": "어릴때도 동물안키워보셨어여?",
          "original_form": "어릴때도 동물안키워보셨어여?",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100009433.1.9",
          "form": "물고기 빼고는 한번도 안키워봣어요 ㅋㅋㅋㅋ",
          "original_form": "물고기 빼고는 한번도 안키워봣어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100009433.1.10",
          "form": "저는 물고기랑 토끼 햄스터 키워봣오용 ㅎㅎ",
          "original_form": "저는 물고기랑 토끼 햄스터 키워봣오용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:50"
        },
        {
          "id": "MDRW2100009433.1.11",
          "form": "오오 토끼 저도 키워보고 싶었는데 관리가 귀찮기도 해서 포기햇어요",
          "original_form": "오오 토끼 저도 키워보고 싶었는데 관리가 귀찮기도 해서 포기햇어요",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100009433.1.12",
          "form": "맞아여 ㅋㅋ귀여운데 냄새도 많이나요..",
          "original_form": "맞아여 ㅋㅋ귀여운데 냄새도 많이나요..",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009433.1.13",
          "form": "아아ㅠㅠ 냄새나면 힘들죠",
          "original_form": "아아ㅠㅠ 냄새나면 힘들죠",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009433.1.14",
          "form": "아 거북이도 키워봤오용",
          "original_form": "아 거북이도 키워봤오용",
          "speaker_id": "1",
