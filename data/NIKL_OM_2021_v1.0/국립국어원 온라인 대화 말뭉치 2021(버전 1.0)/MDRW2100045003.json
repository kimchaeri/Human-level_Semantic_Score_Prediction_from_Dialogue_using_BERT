      "utterance": [
        {
          "id": "MDRW2100045003.1.1",
          "form": "왓챠본다했었지?",
          "original_form": "왓챠본다했었지?",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045003.1.2",
          "form": "요즘 드라마 엄청본다",
          "original_form": "요즘 드라마 엄청본다",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045003.1.3",
          "form": "지금 드라마도 왓챠에서 봐?",
          "original_form": "지금 드라마도 왓챠에서 봐?",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045003.1.4",
          "form": "지금은 티빙!!",
          "original_form": "지금은 티빙!!",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100045003.1.5",
          "form": "ㅋㅋ많이 볼수밖에ㅋㅋ\n엄청 풍부하네",
          "original_form": "ㅋㅋ많이 볼수밖에ㅋㅋ\n엄청 풍부하네",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045003.1.6",
          "form": "네이버블로그 에서도 봤다가",
          "original_form": "네이버블로그 에서도 봤다가",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045003.1.7",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045003.1.8",
          "form": "아이치이에서도 봤다가",
          "original_form": "아이치이에서도 봤다가",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045003.1.9",
          "form": "위티비에서도 봤다가",
          "original_form": "위티비에서도 봤다가",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045003.1.10",
          "form": "블로그로 보는거 귀하제?",
          "original_form": "블로그로 보는거 귀하제?",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045003.1.11",
          "form": "아이치이? 위티비? 모르는거다ㅎ",
          "original_form": "아이치이? 위티비? 모르는거다ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045003.1.12",
          "form": "ㅇㅇ잘없어 요즘 저작권때메",
          "original_form": "ㅇㅇ잘없어 요즘 저작권때메",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045003.1.13",
          "form": "중국어플 지들꺼 한국자막제공해",
          "original_form": "중국어플 지들꺼 한국자막제공해",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.14",
          "form": "그체 법을 지키는건 좋은데",
          "original_form": "그체 법을 지키는건 좋은데",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.15",
          "form": "안까는게좋지만 보고싶어서 걍 보고있어",
          "original_form": "안까는게좋지만 보고싶어서 걍 보고있어",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.16",
          "form": "오잉 좋은건가ㅋㅋ",
          "original_form": "오잉 좋은건가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.17",
          "form": "가입안하고",
          "original_form": "가입안하고",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.18",
          "form": "응 중국어플인가보네ㅎ",
          "original_form": "응 중국어플인가보네ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.19",
          "form": "깔기는깜 ㅇㅇ",
          "original_form": "깔기는깜 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.20",
          "form": "첨들었어",
          "original_form": "첨들었어",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.21",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.22",
          "form": "보고바로지워",
          "original_form": "보고바로지워",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045003.1.23",
          "form": "좋군 가입도안하고 볼수있고",
          "original_form": "좋군 가입도안하고 볼수있고",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045003.1.24",
          "form": "중국 오티티여",
          "original_form": "중국 오티티여",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045003.1.25",
          "form": "오티오어?",
          "original_form": "오티오어?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045003.1.26",
          "form": "검색해봐야겠다ㅎ",
          "original_form": "검색해봐야겠다ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:34"
        }
      ]
