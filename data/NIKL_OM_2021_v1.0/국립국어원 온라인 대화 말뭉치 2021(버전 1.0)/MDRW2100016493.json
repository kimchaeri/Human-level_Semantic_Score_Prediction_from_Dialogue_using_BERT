      "utterance": [
        {
          "id": "MDRW2100016493.1.1",
          "form": "왜죠",
          "original_form": "왜죠",
          "speaker_id": "2",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016493.1.2",
          "form": "멀어져서요",
          "original_form": "멀어져서요",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016493.1.3",
          "form": "전 분당벗어나면",
          "original_form": "전 분당벗어나면",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016493.1.4",
          "form": "숨쉬기가 힘들어요",
          "original_form": "숨쉬기가 힘들어요",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016493.1.5",
          "form": "매일매일이 호흡곤란",
          "original_form": "매일매일이 호흡곤란",
          "speaker_id": "1",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016493.1.6",
          "form": "분당부심..",
          "original_form": "분당부심..",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.7",
          "form": "ㅋㅋ ㅋ ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ ㅋ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.8",
          "form": "지금 잠실쪽 무시하시는건가요?",
          "original_form": "지금 잠실쪽 무시하시는건가요?",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.9",
          "form": "우물안 분당",
          "original_form": "우물안 분당",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.10",
          "form": "여기 공기나쁘다고?",
          "original_form": "여기 공기나쁘다고?",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.11",
          "form": "아녀 잠실 무시한적없는데요?",
          "original_form": "아녀 잠실 무시한적없는데요?",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.12",
          "form": "확대해석 오지네요",
          "original_form": "확대해석 오지네요",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.13",
          "form": "네 확대 좀 해봤습니다",
          "original_form": "네 확대 좀 해봤습니다",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.14",
          "form": "그 말싸움 이기는법 책1장이",
          "original_form": "그 말싸움 이기는법 책1장이",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100016493.1.15",
          "form": "확대해석하라",
          "original_form": "확대해석하라",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016493.1.16",
          "form": "이거였음ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이거였음ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016493.1.17",
          "form": "소크라테스",
          "original_form": "소크라테스",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016493.1.18",
          "form": "말하는거죠?",
          "original_form": "말하는거죠?",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016493.1.19",
          "form": "맞을거임",
          "original_form": "맞을거임",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016493.1.20",
          "form": "아닌건같은데",
          "original_form": "아닌건같은데",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016493.1.21",
          "form": "맞다고 할게요",
          "original_form": "맞다고 할게요",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100016493.1.22",
          "form": "여튼",
          "original_form": "여튼",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016493.1.23",
          "form": "뭐가 제일 힘들죠?",
          "original_form": "뭐가 제일 힘들죠?",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016493.1.24",
          "form": "저희회사에",
          "original_form": "저희회사에",
          "speaker_id": "1",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016493.1.25",
          "form": "말많으신분이 계시거든요",
          "original_form": "말많으신분이 계시거든요",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016493.1.26",
          "form": "귀에서 피나올것같아요",
          "original_form": "귀에서 피나올것같아요",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016493.1.27",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016493.1.28",
          "form": "그래도 말을 많이 해서",
          "original_form": "그래도 말을 많이 해서",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016493.1.29",
          "form": "분위기를 띄우시는거죠",
          "original_form": "분위기를 띄우시는거죠",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016493.1.30",
          "form": "희생하시는 분이네요",
          "original_form": "희생하시는 분이네요",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016493.1.31",
          "form": "애써 변명하시네요",
          "original_form": "애써 변명하시네요",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016493.1.32",
          "form": "ㅋㅋㅋㅋㅋ변명ㄴㄴ",
          "original_form": "ㅋㅋㅋㅋㅋ변명ㄴㄴ",
          "speaker_id": "2",
          "time": "20210826 13:24"
        }
      ]
