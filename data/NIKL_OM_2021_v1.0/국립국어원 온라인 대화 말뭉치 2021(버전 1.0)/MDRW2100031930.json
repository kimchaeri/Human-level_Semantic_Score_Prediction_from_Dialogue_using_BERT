      "utterance": [
        {
          "id": "MDRW2100031930.1.1",
          "form": "네페 받으시면 뭐 사시려구요?ㅎ",
          "original_form": "네페 받으시면 뭐 사시려구요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100031930.1.2",
          "form": "전 떡히뭐살게없어서ㅎㅎ 아이 장난감이나 사볼까하구요 ㅡㅎ",
          "original_form": "전 떡히뭐살게없어서ㅎㅎ 아이 장난감이나 사볼까하구요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031930.1.3",
          "form": "음 전 일단 모으는편이라 ㅋ",
          "original_form": "음 전 일단 모으는편이라 ㅋ",
          "speaker_id": "1",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031930.1.4",
          "form": "요즘엔 워터비즈에 꽂혀가지고 ㅎㅎ",
          "original_form": "요즘엔 워터비즈에 꽂혀가지고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031930.1.5",
          "form": "그거 사줄까 생각이긴.해요",
          "original_form": "그거 사줄까 생각이긴.해요",
          "speaker_id": "1",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031930.1.6",
          "form": "아 애기 장난감 잘 사주세요?",
          "original_form": "아 애기 장난감 잘 사주세요?",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031930.1.7",
          "form": "애기장난감이 짐에 절반이에요ㅎㅎ",
          "original_form": "애기장난감이 짐에 절반이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100031930.1.8",
          "form": "저도 사지만 아이엄마도 사주고 이러다보닠간ㅎㅎ",
          "original_form": "저도 사지만 아이엄마도 사주고 이러다보닠간ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100031930.1.9",
          "form": "전 특별한날만 사주는편이라",
          "original_form": "전 특별한날만 사주는편이라",
          "speaker_id": "1",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100031930.1.10",
          "form": "물놀이 장난감도 엄청많아서ㅠㅠ",
          "original_form": "물놀이 장난감도 엄청많아서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100031930.1.11",
          "form": "아아 그래야하는데ㅎ 요즘 자꾸 뭐사달라고하네요ㅎㅎ",
          "original_form": "아아 그래야하는데ㅎ 요즘 자꾸 뭐사달라고하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100031930.1.12",
          "form": "칭찬도장 찍은면 보상으로  단품 쪼고만거 ㅋ",
          "original_form": "칭찬도장 찍은면 보상으로  단품 쪼고만거 ㅋ",
          "speaker_id": "1",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100031930.1.13",
          "form": "오오ㅎ",
          "original_form": "오오ㅎ",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031930.1.14",
          "form": "저희는 아직안하는데ㅎ 그거 꼭해야한다면서요?",
          "original_form": "저희는 아직안하는데ㅎ 그거 꼭해야한다면서요?",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031930.1.15",
          "form": "꼭 할필요는 없어여 ㅎㅎ",
          "original_form": "꼭 할필요는 없어여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100031930.1.16",
          "form": "사달라는데 늘어나니",
          "original_form": "사달라는데 늘어나니",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031930.1.17",
          "form": "다 가질순 없지만  목표를 달성했을때 원하는건 가질수 있다는것더 알려주고",
          "original_form": "다 가질순 없지만  목표를 달성했을때 원하는건 가질수 있다는것더 알려주고",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100031930.1.18",
          "form": "습관 고치려고 하는거죠 ㅎㅎ",
          "original_form": "습관 고치려고 하는거죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031930.1.19",
          "form": "그럼 교육적으로도 좋겟네욯",
          "original_form": "그럼 교육적으로도 좋겟네욯",
          "speaker_id": "2",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031930.1.20",
          "form": "칭찬도장이  순기능도 많지만 가끔은 역기능도 잇어요 ㅎㅎ",
          "original_form": "칭찬도장이  순기능도 많지만 가끔은 역기능도 잇어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100031930.1.21",
          "form": "그렇네요ㅎㅎ 역기능 순기능 오랜만에 들업핬어요ㅎ",
          "original_form": "그렇네요ㅎㅎ 역기능 순기능 오랜만에 들업핬어요ㅎ",
          "speaker_id": "2",
          "time": "20210905 09:08"
        },
        {
          "id": "MDRW2100031930.1.22",
          "form": "도장 받을때만  꼼수 부리기도 해요 ㅎㅎ 아이들도",
          "original_form": "도장 받을때만  꼼수 부리기도 해요 ㅎㅎ 아이들도",
          "speaker_id": "1",
          "time": "20210905 09:08"
        },
        {
          "id": "MDRW2100031930.1.23",
          "form": "교과서에 자주나오는것ㅎㅎ",
          "original_form": "교과서에 자주나오는것ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:08"
        },
        {
          "id": "MDRW2100031930.1.24",
          "form": "글죠ㅎ 아이들은 당욘히ㅎㅎ",
          "original_form": "글죠ㅎ 아이들은 당욘히ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:08"
        },
        {
