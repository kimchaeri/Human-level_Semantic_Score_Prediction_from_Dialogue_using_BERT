      "utterance": [
        {
          "id": "MDRW2100044997.1.1",
          "form": "흑흑 연님이 건강히 잘이씨",
          "original_form": "흑흑 연님이 건강히 잘이씨",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044997.1.2",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044997.1.3",
          "form": "후 오늘도 삼새이는 이쁘네",
          "original_form": "후 오늘도 삼새이는 이쁘네",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044997.1.4",
          "form": "삼색이 마를린 최애",
          "original_form": "삼색이 마를린 최애",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044997.1.5",
          "form": "저어는 야통이랑 무..",
          "original_form": "저어는 야통이랑 무..",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044997.1.6",
          "form": "그리고 뚱땅이는",
          "original_form": "그리고 뚱땅이는",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044997.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.8",
          "form": "야통이랑",
          "original_form": "야통이랑",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.9",
          "form": "좀 킹받는 존재...",
          "original_form": "좀 킹받는 존재...",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.10",
          "form": "무",
          "original_form": "무",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.11",
          "form": "같이있는거보는데",
          "original_form": "같이있는거보는데",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그림체 왤케달라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그림체 왤케달라",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.14",
          "form": "따지고 보면",
          "original_form": "따지고 보면",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.15",
          "form": "이모랑 조카인데",
          "original_form": "이모랑 조카인데",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.16",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.17",
          "form": "완전 다르게생김ㅋㅋㅋㅋ",
          "original_form": "완전 다르게생김ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.18",
          "form": "넘귀여워",
          "original_form": "넘귀여워",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044997.1.19",
          "form": "내가 많이는 안 바라고",
          "original_form": "내가 많이는 안 바라고",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.20",
          "form": "한번만 쓰다듬어보고싶은데",
          "original_form": "한번만 쓰다듬어보고싶은데",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.21",
          "form": "안되겟지...",
          "original_form": "안되겟지...",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.22",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.23",
          "form": "정말...",
          "original_form": "정말...",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.24",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.25",
          "form": "랜선으로만 볼 수 있겟지..",
          "original_form": "랜선으로만 볼 수 있겟지..",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.26",
          "form": "충무아트센터에 사는 애기둘이있어",
          "original_form": "충무아트센터에 사는 애기둘이있어",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.27",
          "form": "근데 걔들이",
          "original_form": "근데 걔들이",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044997.1.28",
          "form": "진짜 애교많아서",
          "original_form": "진짜 애교많아서",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044997.1.29",
          "form": "만나자마자 비벼댐",
          "original_form": "만나자마자 비벼댐",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044997.1.30",
          "form": "오늘도 봤는데",
          "original_form": "오늘도 봤는데",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044997.1.31",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ 터줏대감들 귀여워",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ 터줏대감들 귀여워",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044997.1.32",
          "form": "애기들 비맞으면서",
          "original_form": "애기들 비맞으면서",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044997.1.33",
          "form": "경비아저씨하고 인사중",
          "original_form": "경비아저씨하고 인사중",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044997.1.34",
          "form": "흑흑 저는 요새 매탈남 열심히보는데",
          "original_form": "흑흑 저는 요새 매탈남 열심히보는데",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044997.1.35",
          "form": "쪼꼬미 이제 적응잘헀더라",
          "original_form": "쪼꼬미 이제 적응잘헀더라",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044997.1.36",
          "form": "무니밥도 뺏어먹던데...",
          "original_form": "무니밥도 뺏어먹던데...",
          "speaker_id": "2",
          "time": "20210908 00:33"
        }
      ]
    }
