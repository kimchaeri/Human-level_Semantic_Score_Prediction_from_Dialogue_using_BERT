      "utterance": [
        {
          "id": "MDRW2100042520.1.1",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042520.1.2",
          "form": "유명한거는 다 서울에만 있어서",
          "original_form": "유명한거는 다 서울에만 있어서",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042520.1.3",
          "form": "넌 맛집 줄서는거 이해하는 편이야?",
          "original_form": "넌 맛집 줄서는거 이해하는 편이야?",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042520.1.4",
          "form": "먖음.....",
          "original_form": "먖음.....",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.6",
          "form": "그걸로 싸우는 커플들도 많다더라",
          "original_form": "그걸로 싸우는 커플들도 많다더라",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.7",
          "form": "줄서는거 이해는 되는데",
          "original_form": "줄서는거 이해는 되는데",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.9",
          "form": "나는 막 몇시간웨이팅 이정도는 안할거같음",
          "original_form": "나는 막 몇시간웨이팅 이정도는 안할거같음",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.10",
          "form": "한 2~30분정도는 해볼듯",
          "original_form": "한 2~30분정도는 해볼듯",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.11",
          "form": "나도 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.12",
          "form": "난 최대 1시간?",
          "original_form": "난 최대 1시간?",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.13",
          "form": "폰하면 되니깐",
          "original_form": "폰하면 되니깐",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042520.1.14",
          "form": "맞아 최대 1시간정도면",
          "original_form": "맞아 최대 1시간정도면",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042520.1.15",
          "form": "ㅇㅇ걍 일행이랑 수다떨거나",
          "original_form": "ㅇㅇ걍 일행이랑 수다떨거나",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042520.1.16",
          "form": "폰하면서 기다릴수있을거같음",
          "original_form": "폰하면서 기다릴수있을거같음",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042520.1.17",
          "form": "ㅇㅈㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅈㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042520.1.18",
          "form": "그럼 전시회도 보고",
          "original_form": "그럼 전시회도 보고",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042520.1.19",
          "form": "오오오오 좋다",
          "original_form": "오오오오 좋다",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042520.1.20",
          "form": "칵테일 마셔야지",
          "original_form": "칵테일 마셔야지",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042520.1.21",
          "form": "오ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042520.1.22",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042520.1.23",
          "form": "거기 가보자",
          "original_form": "거기 가보자",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042520.1.24",
          "form": "여성전용 바",
          "original_form": "여성전용 바",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042520.1.25",
          "form": "오 그런것도 있어??",
          "original_form": "오 그런것도 있어??",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042520.1.26",
          "form": "나 이런데 가보고싶었는데 서울에만있어서ㅠㅠㅠ",
          "original_form": "나 이런데 가보고싶었는데 서울에만있어서ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042520.1.27",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042520.1.28",
          "form": "좋다 편하게 마시고  할 수 있을듯",
          "original_form": "좋다 편하게 마시고  할 수 있을듯",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042520.1.29",
          "form": "너무좋다",
          "original_form": "너무좋다",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042520.1.30",
          "form": "근데 진짜 코로나땜에",
          "original_form": "근데 진짜 코로나땜에",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042520.1.31",
          "form": "명동이나 홍대 이런곳도 많이 죽은것 같더라 ㅠ",
          "original_form": "명동이나 홍대 이런곳도 많이 죽은것 같더라 ㅠ",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042520.1.32",
          "form": "맞아 홍대거리 진짜 조용해졌던데",
          "original_form": "맞아 홍대거리 진짜 조용해졌던데",
          "speaker_id": "1",
          "time": "20210906 22:39"
        }
      ]
    }
