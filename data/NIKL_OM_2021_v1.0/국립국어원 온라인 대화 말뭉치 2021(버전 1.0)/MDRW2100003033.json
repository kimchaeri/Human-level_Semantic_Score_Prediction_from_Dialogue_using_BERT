      "utterance": [
        {
          "id": "MDRW2100003033.1.1",
          "form": "로제떡볶이 칼로리가 그렇게 높다고ㅠ",
          "original_form": "로제떡볶이 칼로리가 그렇게 높다고ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100003033.1.2",
          "form": "그렇더라구요 ㅠ 맛있는건 다 높은가봐요",
          "original_form": "그렇더라구요 ㅠ 맛있는건 다 높은가봐요",
          "speaker_id": "2",
          "time": "20210820 17:53"
        },
        {
          "id": "MDRW2100003033.1.3",
          "form": "맛잇게 먹고 운동하면 되죠 뭐",
          "original_form": "맛잇게 먹고 운동하면 되죠 뭐",
          "speaker_id": "1",
          "time": "20210820 18:04"
        },
        {
          "id": "MDRW2100003033.1.4",
          "form": "운동이 귀찮아서 큰일이에요",
          "original_form": "운동이 귀찮아서 큰일이에요",
          "speaker_id": "2",
          "time": "20210820 18:04"
        },
        {
          "id": "MDRW2100003033.1.5",
          "form": "돈주고 끊지 않으면 쉽지 않더라고요",
          "original_form": "돈주고 끊지 않으면 쉽지 않더라고요",
          "speaker_id": "1",
          "time": "20210820 18:05"
        },
        {
          "id": "MDRW2100003033.1.6",
          "form": "맞아요 돈이 들어가야 의지가 생겨요 ㅋㅋ",
          "original_form": "맞아요 돈이 들어가야 의지가 생겨요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100003033.1.7",
          "form": "요가는 재밌긴 하던데",
          "original_form": "요가는 재밌긴 하던데",
          "speaker_id": "1",
          "time": "20210820 18:08"
        },
        {
          "id": "MDRW2100003033.1.8",
          "form": "해보셨나봐요!",
          "original_form": "해보셨나봐요!",
          "speaker_id": "2",
          "time": "20210820 18:09"
        },
        {
          "id": "MDRW2100003033.1.9",
          "form": "네 그냥 문화센터요ㅋㅋ",
          "original_form": "네 그냥 문화센터요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:10"
        },
        {
          "id": "MDRW2100003033.1.10",
          "form": "재밌을 것 같아요 ㅋㅋㅋ 저도 해봐야겠네요 나중에",
          "original_form": "재밌을 것 같아요 ㅋㅋㅋ 저도 해봐야겠네요 나중에",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003033.1.11",
          "form": "네 근데 선생님이 중요하긴 해요ㅎㅎ",
          "original_form": "네 근데 선생님이 중요하긴 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003033.1.12",
          "form": "역시 선생님을 잘 만나야 하나봐요 ㅎㅎ 뭐든지",
          "original_form": "역시 선생님을 잘 만나야 하나봐요 ㅎㅎ 뭐든지",
          "speaker_id": "2",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003033.1.13",
          "form": "네 근데 궁합이 중요ㅋㅋ",
          "original_form": "네 근데 궁합이 중요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:13"
        },
        {
