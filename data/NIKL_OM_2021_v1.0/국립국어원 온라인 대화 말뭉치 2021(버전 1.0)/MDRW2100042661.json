      "utterance": [
        {
          "id": "MDRW2100042661.1.1",
          "form": "못봄ㅋㅋㅋ 마지막꺼",
          "original_form": "못봄ㅋㅋㅋ 마지막꺼",
          "speaker_id": "2",
          "time": "20210907 00:38"
        },
        {
          "id": "MDRW2100042661.1.2",
          "form": "아..나는 손은...못넣겠..ㅋㅋㅋ",
          "original_form": "아..나는 손은...못넣겠..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:39"
        },
        {
          "id": "MDRW2100042661.1.3",
          "form": "음 여튼 ㅠㅠ 아 내맘에 쏙 뜨는게 없다.",
          "original_form": "음 여튼 ㅠㅠ 아 내맘에 쏙 뜨는게 없다.",
          "speaker_id": "2",
          "time": "20210907 00:39"
        },
        {
          "id": "MDRW2100042661.1.4",
          "form": "변기에서 넣고 반 나눠서 기역자 되자나",
          "original_form": "변기에서 넣고 반 나눠서 기역자 되자나",
          "speaker_id": "1",
          "time": "20210907 00:39"
        },
        {
          "id": "MDRW2100042661.1.5",
          "form": "물나오는 곳에 막대기 꽉 힘으로 잡고 막 문대면",
          "original_form": "물나오는 곳에 막대기 꽉 힘으로 잡고 막 문대면",
          "speaker_id": "1",
          "time": "20210907 00:40"
        },
        {
          "id": "MDRW2100042661.1.6",
          "form": "쉬야들 묻어나고고",
          "original_form": "쉬야들 묻어나고고",
          "speaker_id": "1",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.7",
          "form": "그칸다 ㅋㅋ",
          "original_form": "그칸다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.8",
          "form": "아 그래 ㅋ",
          "original_form": "아 그래 ㅋ",
          "speaker_id": "2",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.9",
          "form": "평소엔 저래 닦고",
          "original_form": "평소엔 저래 닦고",
          "speaker_id": "1",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.10",
          "form": "근데 그 구멍을 . 왠지 솔로 비벼야",
          "original_form": "근데 그 구멍을 . 왠지 솔로 비벼야",
          "speaker_id": "2",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.11",
          "form": "한번씩 손 넣어서",
          "original_form": "한번씩 손 넣어서",
          "speaker_id": "1",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.12",
          "form": "될것같애서 그거 썻지",
          "original_form": "될것같애서 그거 썻지",
          "speaker_id": "2",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.13",
          "form": "수세미로 청소하지",
          "original_form": "수세미로 청소하지",
          "speaker_id": "1",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.14",
          "form": "나도 쓰리엠꺼쓸때 옆으로 세워서 이리저리 비비서 ㅋㅋㅋㅋ",
          "original_form": "나도 쓰리엠꺼쓸때 옆으로 세워서 이리저리 비비서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:41"
        },
        {
          "id": "MDRW2100042661.1.15",
          "form": "매일은 못한다 ㅋㅋㅋ",
          "original_form": "매일은 못한다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:42"
        },
        {
          "id": "MDRW2100042661.1.16",
          "form": "하면 시커먼거 나오고 하긴해 ㅋㅋㅋㅋ",
          "original_form": "하면 시커먼거 나오고 하긴해 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:42"
        },
        {
          "id": "MDRW2100042661.1.17",
          "form": "아 건조기가... 40분이나 남았노",
          "original_form": "아 건조기가... 40분이나 남았노",
          "speaker_id": "2",
          "time": "20210907 00:42"
        },
        {
          "id": "MDRW2100042661.1.18",
          "form": "그걸 어찌 매일해 ㅋㅋㅋㅋㅋ 못해못해",
          "original_form": "그걸 어찌 매일해 ㅋㅋㅋㅋㅋ 못해못해",
          "speaker_id": "2",
          "time": "20210907 00:43"
        },
        {
          "id": "MDRW2100042661.1.19",
          "form": "난 건조기 돌리고 자뿐다 ㅋㅋㅋ",
          "original_form": "난 건조기 돌리고 자뿐다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:43"
        },
        {
          "id": "MDRW2100042661.1.20",
          "form": "아니 뒤에 한판 더 있어 돌릴께..",
          "original_form": "아니 뒤에 한판 더 있어 돌릴께..",
          "speaker_id": "2",
          "time": "20210907 00:44"
        }
