      "utterance": [
        {
          "id": "MDRW2100006621.1.1",
          "form": "파리에서 진짜 피렌체 버금가는 사건 있었자나ㅋㅋ",
          "original_form": "파리에서 진짜 피렌체 버금가는 사건 있었자나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100006621.1.2",
          "form": "피런체 쏘가 장님된줄알고 놀랐다는 얘기ㅋㅋㅋ",
          "original_form": "피런체 쏘가 장님된줄알고 놀랐다는 얘기ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100006621.1.3",
          "form": "응 진짜 웃긴데 이것도 비슷함",
          "original_form": "응 진짜 웃긴데 이것도 비슷함",
          "speaker_id": "2",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100006621.1.4",
          "form": "비를 진짜 너무 많이 맞아가지고 감기 걸릴거 같은거야",
          "original_form": "비를 진짜 너무 많이 맞아가지고 감기 걸릴거 같은거야",
          "speaker_id": "2",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100006621.1.5",
          "form": "그치 우산도 없었다며",
          "original_form": "그치 우산도 없었다며",
          "speaker_id": "1",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100006621.1.6",
          "form": "그때 또 겨울이고 그랬어",
          "original_form": "그때 또 겨울이고 그랬어",
          "speaker_id": "2",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100006621.1.7",
          "form": "겨울이면 진짜 너무 추웠겠다",
          "original_form": "겨울이면 진짜 너무 추웠겠다",
          "speaker_id": "1",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100006621.1.8",
          "form": "파리에서도 민박했는데 무슨 저택같은데를 찾았더라고",
          "original_form": "파리에서도 민박했는데 무슨 저택같은데를 찾았더라고",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006621.1.9",
          "form": "씻고 이제 침대에 누웠는데",
          "original_form": "씻고 이제 침대에 누웠는데",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006621.1.10",
          "form": "둘 다 엄청 피곤하고 으슬거리고",
          "original_form": "둘 다 엄청 피곤하고 으슬거리고",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006621.1.11",
          "form": "마침 전기장판이 있길래 강하게 틀고 잠들었어",
          "original_form": "마침 전기장판이 있길래 강하게 틀고 잠들었어",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006621.1.12",
          "form": "한국처럼 난방 잘 안돼서 유럽은 그게 좀 별로야",
          "original_form": "한국처럼 난방 잘 안돼서 유럽은 그게 좀 별로야",
          "speaker_id": "1",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006621.1.13",
          "form": "그니까 전기장판없었으면... ㅠ",
          "original_form": "그니까 전기장판없었으면... ㅠ",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006621.1.14",
          "form": "아무튼ㅋㅋ 틀고자는데 너무 추운거야 둘다",
          "original_form": "아무튼ㅋㅋ 틀고자는데 너무 추운거야 둘다",
          "speaker_id": "2",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100006621.1.15",
          "form": "그래서 우리가 죙일 비 맞아서 추운가보다하고 잠들었어",
          "original_form": "그래서 우리가 죙일 비 맞아서 추운가보다하고 잠들었어",
          "speaker_id": "2",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100006621.1.16",
          "form": "다음날 둘 다 익은거아냐?",
          "original_form": "다음날 둘 다 익은거아냐?",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100006621.1.17",
          "form": "익으면 다행이게 완전 냉골인거야 이불속인데!!!",
          "original_form": "익으면 다행이게 완전 냉골인거야 이불속인데!!!",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006621.1.18",
          "form": "전기장판이 고장났구만?",
          "original_form": "전기장판이 고장났구만?",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006621.1.19",
          "form": "아니야 불도 다 들어오고 고장은 아니였어",
          "original_form": "아니야 불도 다 들어오고 고장은 아니였어",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006621.1.20",
          "form": "주인언니한테 막 얘기했더니 언니가 보고서는...",
          "original_form": "주인언니한테 막 얘기했더니 언니가 보고서는...",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100006621.1.21",
          "form": "전기장판이랑 코드 연결하는게 빠져있었다는거지ㅋㅋㅋ",
          "original_form": "전기장판이랑 코드 연결하는게 빠져있었다는거지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100006621.1.22",
          "form": "ㅋㅋㅋ 아니 그것도 확인을 안해봤어?",
          "original_form": "ㅋㅋㅋ 아니 그것도 확인을 안해봤어?",
          "speaker_id": "1",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100006621.1.23",
          "form": "둘 다 이미 녹초고 그래서 그것까진 몰랐지",
          "original_form": "둘 다 이미 녹초고 그래서 그것까진 몰랐지",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100006621.1.24",
          "form": "또 둘이 웃겨가지고ㅋㅋㅋ",
          "original_form": "또 둘이 웃겨가지고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100006621.1.25",
          "form": "이게뭐라고ㅋㅋ",
          "original_form": "이게뭐라고ㅋㅋ",
