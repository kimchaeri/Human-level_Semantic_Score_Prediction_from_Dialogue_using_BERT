      "utterance": [
        {
          "id": "MDRW2100025092.1.1",
          "form": "흔한카레맛이라",
          "original_form": "흔한카레맛이라",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025092.1.2",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025092.1.3",
          "form": "저 카레 정말 잘 만들어요",
          "original_form": "저 카레 정말 잘 만들어요",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025092.1.4",
          "form": "채소보다 고기를 많이 넣는 것이 비법이죠",
          "original_form": "채소보다 고기를 많이 넣는 것이 비법이죠",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025092.1.5",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025092.1.6",
          "form": "ㅓ희어머니가",
          "original_form": "ㅓ희어머니가",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025092.1.7",
          "form": "카레를하실때마다",
          "original_form": "카레를하실때마다",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025092.1.8",
          "form": "창의력을 너무발휘하셔서",
          "original_form": "창의력을 너무발휘하셔서",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025092.1.9",
          "form": "님이요?",
          "original_form": "님이요?",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025092.1.10",
          "form": "저는 카레를만들때 양파감자 고기외엔 넣지않는 어른이되었죠",
          "original_form": "저는 카레를만들때 양파감자 고기외엔 넣지않는 어른이되었죠",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025092.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ와",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ와",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025092.1.12",
          "form": "저도 딱 그 세개만 넣어요",
          "original_form": "저도 딱 그 세개만 넣어요",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025092.1.13",
          "form": "당근 빠져!!!!!!!!",
          "original_form": "당근 빠져!!!!!!!!",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025092.1.14",
          "form": "제 카레에는 양파 감자 고기만",
          "original_form": "제 카레에는 양파 감자 고기만",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025092.1.15",
          "form": "들어갈수있어요",
          "original_form": "들어갈수있어요",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025092.1.16",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋㅋㅋㅋ당근이뭐죠...?",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋㅋㅋㅋ당근이뭐죠...?",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025092.1.17",
          "form": "딘짜",
          "original_form": "딘짜",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025092.1.18",
          "form": "그 셋이면",
          "original_form": "그 셋이면",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025092.1.19",
          "form": "카레 넣기 전에 볶기만 해도",
          "original_form": "카레 넣기 전에 볶기만 해도",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.20",
          "form": "이미 완성형 아닌가여",
          "original_form": "이미 완성형 아닌가여",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.21",
          "form": "저희 어머님은 카레에",
          "original_form": "저희 어머님은 카레에",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.22",
          "form": "사과도넣으시고",
          "original_form": "사과도넣으시고",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.23",
          "form": "파프리카도 넣으시고",
          "original_form": "파프리카도 넣으시고",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.24",
          "form": "브로콜리도 넣으시고....",
          "original_form": "브로콜리도 넣으시고....",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.25",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.26",
          "form": "방울토마토도 넣으시고...",
          "original_form": "방울토마토도 넣으시고...",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.27",
          "form": "근데 그그",
          "original_form": "근데 그그",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.28",
          "form": "카레레",
          "original_form": "카레레",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.29",
          "form": "카레에",
          "original_form": "카레에",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.30",
          "form": "누텔라 넣으면 맛있대요...",
          "original_form": "누텔라 넣으면 맛있대요...",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025092.1.31",
          "form": "오.........?",
          "original_form": "오.........?",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025092.1.32",
          "form": "진짜예여..",
          "original_form": "진짜예여..",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025092.1.33",
          "form": "너무 예상외의 재료인데요....?",
          "original_form": "너무 예상외의 재료인데요....?",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025092.1.34",
          "form": "저도 귀를 의심했답니다",
          "original_form": "저도 귀를 의심했답니다",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025092.1.35",
          "form": "근데 먹어본 자가 맛있다더라구여",
          "original_form": "근데 먹어본 자가 맛있다더라구여",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025092.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ 담에 작은솥에 한번",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ 담에 작은솥에 한번",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025092.1.37",
          "form": "넣어봐야겠어요",
          "original_form": "넣어봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 23:05"
        }
      ]
