        },
        {
          "id": "MDRW2100041381.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041381.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100041381.1.4",
          "form": "네~ name1님은 맛있는 저녁 식사 하셨나요?",
          "original_form": "네~ &name1&님은 맛있는 저녁 식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100041381.1.5",
          "form": "네 저도 먹었어요",
          "original_form": "네 저도 먹었어요",
          "speaker_id": "1",
          "time": "20210905 23:47"
        },
        {
          "id": "MDRW2100041381.1.6",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 23:47"
        },
        {
          "id": "MDRW2100041381.1.7",
          "form": "전 오늘 꽃게를 몇마리 받아서 그거 쪄서 먹었네요..ㅎㅎ",
          "original_form": "전 오늘 꽃게를 몇마리 받아서 그거 쪄서 먹었네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100041381.1.8",
          "form": "요즘 꽃게는 어떤가요?",
          "original_form": "요즘 꽃게는 어떤가요?",
          "speaker_id": "1",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100041381.1.9",
          "form": "몸통에 살이 가득 차있어서 먹는 맛이 있는게 좋더라구요",
          "original_form": "몸통에 살이 가득 차있어서 먹는 맛이 있는게 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100041381.1.10",
          "form": "진짜 너무 맛있었어요",
          "original_form": "진짜 너무 맛있었어요",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100041381.1.11",
          "form": "맛있을 것 같네요ㅠㅠ",
          "original_form": "맛있을 것 같네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100041381.1.12",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100041381.1.13",
          "form": "너무 많아서 탈이죠 ㅎㅎ",
          "original_form": "너무 많아서 탈이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100041381.1.14",
          "form": "하나만 꼽자면 칼국수 인것 같아요",
          "original_form": "하나만 꼽자면 칼국수 인것 같아요",
          "speaker_id": "2",
          "time": "20210906 00:12"
        },
        {
          "id": "MDRW2100041381.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 00:12"
        },
        {
          "id": "MDRW2100041381.1.16",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "1",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100041381.1.17",
          "form": "허엌 떡볶이 너무 좋아요...",
          "original_form": "허엌 떡볶이 너무 좋아요...",
          "speaker_id": "2",
          "time": "20210906 00:20"
        },
        {
          "id": "MDRW2100041381.1.18",
