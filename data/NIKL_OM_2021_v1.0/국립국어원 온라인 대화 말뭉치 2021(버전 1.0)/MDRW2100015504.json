        {
          "id": "MDRW2100015504.1.1",
          "form": "유튭보시나요",
          "original_form": "유튭보시나요",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015504.1.2",
          "form": "네 자기전에 봐요",
          "original_form": "네 자기전에 봐요",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015504.1.3",
          "form": "유튭 보시죠?",
          "original_form": "유튭 보시죠?",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015504.1.4",
          "form": "네봅니다ㅎㅎ",
          "original_form": "네봅니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015504.1.5",
          "form": "어떤거 주로 보세요?",
          "original_form": "어떤거 주로 보세요?",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015504.1.6",
          "form": "여행 유튭이요",
          "original_form": "여행 유튭이요",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100015504.1.7",
          "form": "아무래도 요즘 여행을 못가니까",
          "original_form": "아무래도 요즘 여행을 못가니까",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100015504.1.8",
          "form": "대리만속이 되겠네요",
          "original_form": "대리만속이 되겠네요",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100015504.1.9",
          "form": "만족",
          "original_form": "만족",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100015504.1.10",
          "form": "네네 대리만족ㅎㅎ",
          "original_form": "네네 대리만족ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100015504.1.11",
          "form": "넷플릭스 구독하시나요?",
          "original_form": "넷플릭스 구독하시나요?",
          "speaker_id": "2",
          "time": "20210826 11:52"
        },
        {
          "id": "MDRW2100015504.1.12",
          "form": "네 보는중입니다",
          "original_form": "네 보는중입니다",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100015504.1.13",
          "form": "넷플에서 뭐보시나요?추천해주세요",
          "original_form": "넷플에서 뭐보시나요?추천해주세요",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100015504.1.14",
          "form": "킹덤이요!",
          "original_form": "킹덤이요!",
          "speaker_id": "1",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100015504.1.15",
          "form": "이번에나온 킹덤 영화...? 전지현씨나오는거 평이좋던데",
          "original_form": "이번에나온 킹덤 영화...? 전지현씨나오는거 평이좋던데",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100015504.1.16",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
