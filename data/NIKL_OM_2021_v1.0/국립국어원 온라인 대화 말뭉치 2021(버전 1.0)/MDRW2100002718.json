      "utterance": [
        {
          "id": "MDRW2100002718.1.1",
          "form": "직장 다니세요?",
          "original_form": "직장 다니세요?",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002718.1.2",
          "form": "아니요 집에있어요",
          "original_form": "아니요 집에있어요",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002718.1.3",
          "form": "아~구직 중이신가요?",
          "original_form": "아~구직 중이신가요?",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002718.1.4",
          "form": "음..네 직장다니세요~?",
          "original_form": "음..네 직장다니세요~?",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002718.1.5",
          "form": "네~다니고 있어요~",
          "original_form": "네~다니고 있어요~",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002718.1.6",
          "form": "아^^ 무슨일하세요~?",
          "original_form": "아^^ 무슨일하세요~?",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002718.1.7",
          "form": "저는 사무직이에요~",
          "original_form": "저는 사무직이에요~",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002718.1.8",
          "form": "구하는 일은 어떤 일이세요?",
          "original_form": "구하는 일은 어떤 일이세요?",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002718.1.9",
          "form": "아..사무직이요 ㅎㅎ",
          "original_form": "아..사무직이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002718.1.10",
          "form": "아~~해피님도 사무직 구하시군요 ㅎ",
          "original_form": "아~~해피님도 사무직 구하시군요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002718.1.11",
          "form": "^^name1님 회사생활은 어떠세요~?",
          "original_form": "^^&name1&님 회사생활은 어떠세요~?",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002718.1.12",
          "form": "스트레스 연속이에요ㅠ",
          "original_form": "스트레스 연속이에요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002718.1.13",
          "form": "아 ㅜㅜ 스트레스 그치용",
          "original_form": "아 ㅜㅜ 스트레스 그치용",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002718.1.14",
          "form": "네~편하게 다닐수 있는곳이 있으면 좋겠어요",
          "original_form": "네~편하게 다닐수 있는곳이 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002718.1.15",
          "form": "맞아요~ 일때문에 스트레스 받으시는건가요 사람때문에도 스트레스 받으시는건가요",
          "original_form": "맞아요~ 일때문에 스트레스 받으시는건가요 사람때문에도 스트레스 받으시는건가요",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002718.1.16",
          "form": "사람때문에요ㅠㅠ",
          "original_form": "사람때문에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002718.1.17",
          "form": "아 ㅜㅜ 정말 힘드시지요",
          "original_form": "아 ㅜㅜ 정말 힘드시지요",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002718.1.18",
          "form": "네~오늘은 스트레스 근원자가 조퇴를 해서 맘놓고 폰하네요 ㅎ",
          "original_form": "네~오늘은 스트레스 근원자가 조퇴를 해서 맘놓고 폰하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002718.1.19",
          "form": "아~^^ 다행이네요 ㅎ 몇시에 퇴근하세요?",
          "original_form": "아~^^ 다행이네요 ㅎ 몇시에 퇴근하세요?",
