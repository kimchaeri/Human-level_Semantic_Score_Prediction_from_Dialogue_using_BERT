        {
          "id": "MDRW2100005847.1.1",
          "form": "이젠 장맛비가 끝났나봐요",
          "original_form": "이젠 장맛비가 끝났나봐요",
          "speaker_id": "1",
          "time": "20210908 06:28"
        },
        {
          "id": "MDRW2100005847.1.2",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100005847.1.3",
          "form": "이젠 비가 그쳤어요",
          "original_form": "이젠 비가 그쳤어요",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100005847.1.4",
          "form": "당분간 해가 난대",
          "original_form": "당분간 해가 난대",
          "speaker_id": "2",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100005847.1.5",
          "form": "이제는 비 좀 그만왔으면 좋겠어요",
          "original_form": "이제는 비 좀 그만왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100005847.1.6",
          "form": "장마는 끝났으니까",
          "original_form": "장마는 끝났으니까",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100005847.1.7",
          "form": "사람들이 건조기가 그렇게 좋다고 하네요",
          "original_form": "사람들이 건조기가 그렇게 좋다고 하네요",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100005847.1.8",
          "form": "장마도 끝났으니 나중에 생각해봅시다",
          "original_form": "장마도 끝났으니 나중에 생각해봅시다",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100005847.1.9",
          "form": "뭐 급한건 아니죠",
          "original_form": "뭐 급한건 아니죠",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100005847.1.10",
          "form": "내년쯤 생각해 봅시다",
          "original_form": "내년쯤 생각해 봅시다",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100005847.1.11",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100005847.1.12",
          "form": "갈수록 비가 오니 필요할 수도",
          "original_form": "갈수록 비가 오니 필요할 수도",
