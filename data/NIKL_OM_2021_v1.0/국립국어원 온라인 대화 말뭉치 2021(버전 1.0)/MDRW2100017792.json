      "utterance": [
        {
          "id": "MDRW2100017792.1.1",
          "form": "너희는",
          "original_form": "너희는",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017792.1.2",
          "form": "재택 가능이야?",
          "original_form": "재택 가능이야?",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017792.1.3",
          "form": "어...우리는 우리가 택할 수는 없고",
          "original_form": "어...우리는 우리가 택할 수는 없고",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017792.1.4",
          "form": "지난번에 에어컨 고장났을때 재택했다고 들었어",
          "original_form": "지난번에 에어컨 고장났을때 재택했다고 들었어",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017792.1.5",
          "form": "코로나 초기에도 2주정도? 했었대",
          "original_form": "코로나 초기에도 2주정도? 했었대",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017792.1.6",
          "form": "너희는 재택있음?",
          "original_form": "너희는 재택있음?",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017792.1.7",
          "form": "우린 안된다고 못박더니",
          "original_form": "우린 안된다고 못박더니",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017792.1.8",
          "form": "누가 코로나검사한다고 출근못하니까",
          "original_form": "누가 코로나검사한다고 출근못하니까",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017792.1.9",
          "form": "원격으로 일시키더라",
          "original_form": "원격으로 일시키더라",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017792.1.10",
          "form": "ㅋㅋㅋㅋ미쳤지",
          "original_form": "ㅋㅋㅋㅋ미쳤지",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017792.1.11",
          "form": "ㅎㄷㄷㄷㄷㄷ",
          "original_form": "ㅎㄷㄷㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017792.1.12",
          "form": "우리 앞쪽에 엄청 큰 건물",
          "original_form": "우리 앞쪽에 엄청 큰 건물",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017792.1.13",
          "form": "확진자 나와도 딱 이틀인가 3일 재택 시키더라..",
          "original_form": "확진자 나와도 딱 이틀인가 3일 재택 시키더라..",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017792.1.14",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017792.1.15",
          "form": "지독하다",
          "original_form": "지독하다",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017792.1.16",
          "form": "거기는 업무상 진짜 재택해도 아무 상관없는데 ㅋㅋㅋ",
          "original_form": "거기는 업무상 진짜 재택해도 아무 상관없는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017792.1.17",
          "form": "내 전회사임",
          "original_form": "내 전회사임",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017792.1.18",
          "form": "허억ㅋㅋㅋㅋ",
          "original_form": "허억ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017792.1.19",
          "form": "근데 우리 건물은 진흥원이 관리하는곳이라",
          "original_form": "근데 우리 건물은 진흥원이 관리하는곳이라",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017792.1.20",
          "form": "회사 의지랑은 상관없이",
          "original_form": "회사 의지랑은 상관없이",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017792.1.21",
          "form": "아 좀 애로사항이 있네",
          "original_form": "아 좀 애로사항이 있네",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017792.1.22",
          "form": "확진자 나오면 전원 강제 재택임 ㄷ",
          "original_form": "확진자 나오면 전원 강제 재택임 ㄷ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017792.1.23",
          "form": "역시 국가 기관이",
          "original_form": "역시 국가 기관이",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017792.1.24",
          "form": "너희도 보안땜에",
          "original_form": "너희도 보안땜에",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017792.1.25",
          "form": "재택 꺼려하는건가",
          "original_form": "재택 꺼려하는건가",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017792.1.26",
          "form": "그런건 아닌데 우리는 규모상",
          "original_form": "그런건 아닌데 우리는 규모상",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017792.1.27",
          "form": "작업 중간중간 대화나눌 일도 많고",
          "original_form": "작업 중간중간 대화나눌 일도 많고",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017792.1.28",
          "form": "오옹..",
          "original_form": "오옹..",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017792.1.29",
          "form": "컴퓨터 보면서 작업하니까 불편하지 재택은",
          "original_form": "컴퓨터 보면서 작업하니까 불편하지 재택은",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017792.1.30",
          "form": "근데 나도 재택은 좀 그런게 ㅋㅋㅋ 내 놋북으로 프로그램 안돌아가질거 같음",
          "original_form": "근데 나도 재택은 좀 그런게 ㅋㅋㅋ 내 놋북으로 프로그램 안돌아가질거 같음",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017792.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ노트북님..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ노트북님..",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017792.1.32",
          "form": "이제그만보내드리라구",
          "original_form": "이제그만보내드리라구",
          "speaker_id": "1",
          "time": "20210826 21:19"
        }
      ]
