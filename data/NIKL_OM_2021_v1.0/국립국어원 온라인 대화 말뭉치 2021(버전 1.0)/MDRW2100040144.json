          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100040144.1.7",
          "form": "백신주사는",
          "original_form": "백신주사는",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100040144.1.8",
          "form": "어떰?",
          "original_form": "어떰?",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100040144.1.9",
          "form": "맞고나서말고맞을때",
          "original_form": "맞고나서말고맞을때",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
