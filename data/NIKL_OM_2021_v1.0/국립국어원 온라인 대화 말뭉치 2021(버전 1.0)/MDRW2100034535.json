      "utterance": [
        {
          "id": "MDRW2100034535.1.1",
          "form": "신전이 좀 덜 걸쭉하지않나요?",
          "original_form": "신전이 좀 덜 걸쭉하지않나요?",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100034535.1.2",
          "form": "아 더 달군요",
          "original_form": "아 더 달군요",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100034535.1.3",
          "form": "네 걸쭉하기도해요 ㅋㅋㅋ",
          "original_form": "네 걸쭉하기도해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100034535.1.4",
          "form": "그래서 전 더 좋더라구요",
          "original_form": "그래서 전 더 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.5",
          "form": "아그래요?",
          "original_form": "아그래요?",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.6",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.7",
          "form": "신전이 지점 차이가 난다그래서",
          "original_form": "신전이 지점 차이가 난다그래서",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.8",
          "form": "아 ㅠㅠ",
          "original_form": "아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.9",
          "form": "그 말 많더라구요",
          "original_form": "그 말 많더라구요",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.10",
          "form": "겁나더라구요",
          "original_form": "겁나더라구요",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.11",
          "form": "저도 그랬는데",
          "original_form": "저도 그랬는데",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.12",
          "form": "근데 또 맛있다는분은 엄청 맛있다그러더라구요",
          "original_form": "근데 또 맛있다는분은 엄청 맛있다그러더라구요",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.13",
          "form": "리뷰보고 걸쭉해보여서 ㅋㅋㅋ",
          "original_form": "리뷰보고 걸쭉해보여서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.14",
          "form": "맞아요 리뷰한번보고 도전해보세요!",
          "original_form": "맞아요 리뷰한번보고 도전해보세요!",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100034535.1.15",
          "form": "아하 그러네요 리뷰보고 시켜봐야겠어요",
          "original_form": "아하 그러네요 리뷰보고 시켜봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100034535.1.16",
          "form": "바보였군요ㅋㅋ",
          "original_form": "바보였군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100034535.1.17",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100034535.1.18",
          "form": "전 점바점일때",
          "original_form": "전 점바점일때",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100034535.1.19",
          "form": "무조건 리뷰부터봐요",
          "original_form": "무조건 리뷰부터봐요",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100034535.1.20",
          "form": "그러게요 리뷰보면 됐었네요ㅋㅋㅋ",
          "original_form": "그러게요 리뷰보면 됐었네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100034535.1.21",
          "form": "배달앱의 순기능인거같아요 반반이지만 ㅋㅋㅋ",
          "original_form": "배달앱의 순기능인거같아요 반반이지만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100034535.1.22",
          "form": "아예 찾아볼생각도 안했어요ㅋㅋ",
          "original_form": "아예 찾아볼생각도 안했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:43"
        }
      ]
