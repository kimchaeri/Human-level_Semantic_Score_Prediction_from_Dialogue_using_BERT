      "utterance": [
        {
          "id": "MDRW2100017865.1.1",
          "form": "그래서 난 데리가 부업으로",
          "original_form": "그래서 난 데리가 부업으로",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017865.1.2",
          "form": "연재를 해줫으면 좋겠어",
          "original_form": "연재를 해줫으면 좋겠어",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017865.1.3",
          "form": "(?",
          "original_form": "(?",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017865.1.4",
          "form": "이런 곳이 바로 블루오션이라구",
          "original_form": "이런 곳이 바로 블루오션이라구",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017865.1.5",
          "form": "아닠 이게 머야",
          "original_form": "아닠 이게 머야",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017865.1.6",
          "form": "블루오션을 공략해줘",
          "original_form": "블루오션을 공략해줘",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017865.1.7",
          "form": "블루오션..인가 카카페는 레드오션인데?",
          "original_form": "블루오션..인가 카카페는 레드오션인데?",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017865.1.8",
          "form": "아냐 지금 조아라는 블루오션이야",
          "original_form": "아냐 지금 조아라는 블루오션이야",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017865.1.9",
          "form": "그리고 시장이 전보다 커지고 있어서",
          "original_form": "그리고 시장이 전보다 커지고 있어서",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017865.1.10",
          "form": "사람을 더 많이 뽑는다구",
          "original_form": "사람을 더 많이 뽑는다구",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017865.1.11",
          "form": "ㅋㅋㅋㅋ 그정도야..?",
          "original_form": "ㅋㅋㅋㅋ 그정도야..?",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017865.1.12",
          "form": "예전같았으면 이북/카카페 둘밖에 선택지가 없눈이",
          "original_form": "예전같았으면 이북/카카페 둘밖에 선택지가 없눈이",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017865.1.13",
          "form": "없는데",
          "original_form": "없는데",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017865.1.14",
          "form": "지금은 카카페랑 리디랑 네이버랑 크게 선점하고있고",
          "original_form": "지금은 카카페랑 리디랑 네이버랑 크게 선점하고있고",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017865.1.15",
          "form": "먼가 급 솔깃했는데 그러려면 일단 넘치는 체력과 상상력을 패치해야만 하는데",
          "original_form": "먼가 급 솔깃했는데 그러려면 일단 넘치는 체력과 상상력을 패치해야만 하는데",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017865.1.16",
          "form": "하긴..",
          "original_form": "하긴..",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017865.1.17",
          "form": "다른 중소 플랫폼도 은근 잇어서",
          "original_form": "다른 중소 플랫폼도 은근 잇어서",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017865.1.18",
          "form": "부업..이 될 정도면 감지덕지겠지..",
          "original_form": "부업..이 될 정도면 감지덕지겠지..",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.19",
          "form": "자리가 많아",
          "original_form": "자리가 많아",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.20",
          "form": "요즘은 스마트스토어 구매대행도",
          "original_form": "요즘은 스마트스토어 구매대행도",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.21",
          "form": "주말에 함 다시 노트북을..켜봐야.......",
          "original_form": "주말에 함 다시 노트북을..켜봐야.......",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.22",
          "form": "주에 2~30씩 벌더라구",
          "original_form": "주에 2~30씩 벌더라구",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.24",
          "form": "복잡하게 쓰지말고",
          "original_form": "복잡하게 쓰지말고",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.25",
          "form": "너무 고민하지말고",
          "original_form": "너무 고민하지말고",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.26",
          "form": "일단 그냥 올려 ㅋㅋㅋㅋ",
          "original_form": "일단 그냥 올려 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.27",
          "form": "고민은 나중에 편집자랑 같이 해 ㅋㅋㅋ",
          "original_form": "고민은 나중에 편집자랑 같이 해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017865.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ 구랭 지금 쓰던 것들은 일단 밀어두고...ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 구랭 지금 쓰던 것들은 일단 밀어두고...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        }
      ]
