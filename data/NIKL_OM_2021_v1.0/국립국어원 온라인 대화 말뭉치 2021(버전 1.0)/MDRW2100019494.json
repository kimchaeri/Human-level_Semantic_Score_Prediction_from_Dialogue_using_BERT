      "utterance": [
        {
          "id": "MDRW2100019494.1.1",
          "form": "해피쿠킹은 하는 요리 종류가 다양해서",
          "original_form": "해피쿠킹은 하는 요리 종류가 다양해서",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019494.1.2",
          "form": "그게 좀 장점일수도잇는거같아요 엥간한 메뉴 다 해서 ㅋㅋ",
          "original_form": "그게 좀 장점일수도잇는거같아요 엥간한 메뉴 다 해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019494.1.3",
          "form": "구독 완료 ㅋㅋ",
          "original_form": "구독 완료 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019494.1.4",
          "form": "주꾸미 볶음 너무 좋아요",
          "original_form": "주꾸미 볶음 너무 좋아요",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019494.1.5",
          "form": "구독ㅋㅋㅋㅋㅋ",
          "original_form": "구독ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019494.1.6",
          "form": "아미요라고 레시피의 이유를 과학적으로 설명해주는 사람이 있는데",
          "original_form": "아미요라고 레시피의 이유를 과학적으로 설명해주는 사람이 있는데",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019494.1.7",
          "form": "구독은 안 하지만 그 중에",
          "original_form": "구독은 안 하지만 그 중에",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019494.1.8",
          "form": "주꾸미 야 ㅑ 들 야 ㅏ 들 하다고 ㅍ쳤는데",
          "original_form": "주꾸미 야 ㅑ 들 야 ㅏ 들 하다고 ㅍ쳤는데",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019494.1.9",
          "form": "안갔네요",
          "original_form": "안갔네요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019494.1.10",
          "form": "아미요는 처음듣네요",
          "original_form": "아미요는 처음듣네요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019494.1.11",
          "form": "스탭밀로 만든 토마토소스 파스타 영상이 추천에 떠서 봤더니",
          "original_form": "스탭밀로 만든 토마토소스 파스타 영상이 추천에 떠서 봤더니",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019494.1.12",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019494.1.13",
          "form": "요리 과학적으로 접근하는거 좋아요",
          "original_form": "요리 과학적으로 접근하는거 좋아요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019494.1.14",
          "form": "헐 진짜네요",
          "original_form": "헐 진짜네요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019494.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 황당",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 황당",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019494.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019494.1.17",
          "form": "그래서 아미요 그 레시피를 봤더니 제가 해 먹던 거랑 몇 가지만 다르고 거의 비슷하더라고요",
          "original_form": "그래서 아미요 그 레시피를 봤더니 제가 해 먹던 거랑 몇 가지만 다르고 거의 비슷하더라고요",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019494.1.18",
          "form": "오 정통 요리법을 쓰셨군요",
          "original_form": "오 정통 요리법을 쓰셨군요",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019494.1.19",
          "form": "저는 토마토 퓨레에 샐러리 줄기를 넣고 푹 고아서 육수를 내는데",
          "original_form": "저는 토마토 퓨레에 샐러리 줄기를 넣고 푹 고아서 육수를 내는데",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019494.1.20",
          "form": "그 사람은 생토마토 캔토마토 홀토마토를 섞더라고요",
          "original_form": "그 사람은 생토마토 캔토마토 홀토마토를 섞더라고요",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019494.1.21",
          "form": "맛의 레이어를 만든다는 개념!",
          "original_form": "맛의 레이어를 만든다는 개념!",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019494.1.22",
          "form": "와 샐러리 줄기로 육수까지 내세요?",
          "original_form": "와 샐러리 줄기로 육수까지 내세요?",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019494.1.23",
          "form": "그거 우연히 듣고 해봤는데",
          "original_form": "그거 우연히 듣고 해봤는데",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019494.1.24",
          "form": "맛있더라고요?!",
          "original_form": "맛있더라고요?!",
          "speaker_id": "2",
          "time": "20210827 19:52"
        }
      ]
    }
