      "utterance": [
        {
          "id": "MDRW2100010012.1.1",
          "form": "음악프로 많이 봐?",
          "original_form": "음악프로 많이 봐?",
          "speaker_id": "2",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100010012.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100010012.1.3",
          "form": "예전엔 봤는데 요샌 안보네",
          "original_form": "예전엔 봤는데 요샌 안보네",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100010012.1.4",
          "form": "근데 유튜브에서 봐",
          "original_form": "근데 유튜브에서 봐",
          "speaker_id": "1",
          "time": "20210824 12:47"
        },
        {
          "id": "MDRW2100010012.1.5",
          "form": "지나간거 찾아봐",
          "original_form": "지나간거 찾아봐",
          "speaker_id": "1",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100010012.1.6",
          "form": "ㅋㅋㅋ난 놀토 왜이렇게 재밋니 요새ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ난 놀토 왜이렇게 재밋니 요새ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100010012.1.7",
          "form": "예전꺼 혜리나올때?",
          "original_form": "예전꺼 혜리나올때?",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100010012.1.8",
          "form": "아니도 햬리 나올때 맨날 봤는데",
          "original_form": "아니도 햬리 나올때 맨날 봤는데",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100010012.1.9",
          "form": "근데 해리 진짜 완전 분위기 메이커",
          "original_form": "근데 해리 진짜 완전 분위기 메이커",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100010012.1.10",
          "form": "잘추고 잘 정리해서 맞추고",
          "original_form": "잘추고 잘 정리해서 맞추고",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100010012.1.11",
          "form": "그지 잘먹구 잘추고 귀여웤ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그지 잘먹구 잘추고 귀여웤ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100010012.1.12",
          "form": "구박도 당하구 구박도 하구ㅋㅋㅋㅋ",
          "original_form": "구박도 당하구 구박도 하구ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100010012.1.13",
          "form": "완전 사랑스러움",
          "original_form": "완전 사랑스러움",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100010012.1.14",
          "form": "응 요새 태연은 재미없어",
          "original_form": "응 요새 태연은 재미없어",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100010012.1.15",
          "form": "비긴어게인 때도 난 수현은 너무 좋았는데, 태연은 뭐랄까 항상 아주 조금씩 아쉬워ㅋㅋ",
          "original_form": "비긴어게인 때도 난 수현은 너무 좋았는데, 태연은 뭐랄까 항상 아주 조금씩 아쉬워ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100010012.1.16",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100010012.1.17",
          "form": "수현은 목소리 천재",
          "original_form": "수현은 목소리 천재",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100010012.1.18",
          "form": "태연은좀 애매해",
          "original_form": "태연은좀 애매해",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100010012.1.19",
          "form": "혜리는 드라마 때문에 하차한건가?",
          "original_form": "혜리는 드라마 때문에 하차한건가?",
          "speaker_id": "1",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100010012.1.20",
          "form": "너무 아쉽더라",
          "original_form": "너무 아쉽더라",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100010012.1.21",
          "form": "다시하면 잼나고 다시 또 본방 사수할듯",
          "original_form": "다시하면 잼나고 다시 또 본방 사수할듯",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100010012.1.22",
          "form": "하차 아쉬운 사람 많지 뭐",
          "original_form": "하차 아쉬운 사람 많지 뭐",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100010012.1.23",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100010012.1.24",
          "form": "런닝맨에 광수도 그렇구 나혼산에 1얼도 그렇구",
          "original_form": "런닝맨에 광수도 그렇구 나혼산에 1얼도 그렇구",
          "speaker_id": "2",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100010012.1.25",
          "form": "전임 만한 후임이 없어",
          "original_form": "전임 만한 후임이 없어",
          "speaker_id": "1",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100010012.1.26",
          "form": "그러니까 아 아쉽다",
          "original_form": "그러니까 아 아쉽다",
          "speaker_id": "1",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100010012.1.27",
          "form": "근데 도대체 그걸 듣고 문제를 어찌 맞추는거야",
          "original_form": "근데 도대체 그걸 듣고 문제를 어찌 맞추는거야",
          "speaker_id": "1",
          "time": "20210825 13:05"
        },
        {
          "id": "MDRW2100010012.1.28",
          "form": "난 하나도 안들려 ㅋㅋ",
          "original_form": "난 하나도 안들려 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:05"
        },
        {
          "id": "MDRW2100010012.1.29",
          "form": "넌 잘맞춰?",
          "original_form": "넌 잘맞춰?",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100010012.1.30",
          "form": "미안ㅋㅋㅋ이번주 왜이렇게 바쁜지",
          "original_form": "미안ㅋㅋㅋ이번주 왜이렇게 바쁜지",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100010012.1.31",
          "form": "아닠ㅋㅋㅋ나도 모르겠든데ㅋㅋㅋ저건 아니지~ 이랬던게 맞고 막!",
          "original_form": "아닠ㅋㅋㅋ나도 모르겠든데ㅋㅋㅋ저건 아니지~ 이랬던게 맞고 막!",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100010012.1.32",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210827 06:49"
        },
        {
          "id": "MDRW2100010012.1.33",
          "form": "참 진짜 하나도 모르겠더라 바빴구나",
          "original_form": "참 진짜 하나도 모르겠더라 바빴구나",
          "speaker_id": "1",
          "time": "20210827 06:50"
        }
      ]
