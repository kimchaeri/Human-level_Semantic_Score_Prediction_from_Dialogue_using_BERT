      "utterance": [
        {
          "id": "MDRW2100046729.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20211104 13:01"
        },
        {
          "id": "MDRW2100046729.1.2",
          "form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "original_form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "speaker_id": "2",
          "time": "20211104 13:04"
        },
        {
          "id": "MDRW2100046729.1.3",
          "form": "맛점이 뭐니?",
          "original_form": "맛점이 뭐니?",
          "speaker_id": "1",
          "time": "20211104 13:05"
        },
        {
          "id": "MDRW2100046729.1.4",
          "form": "좀 노티나는데",
          "original_form": "좀 노티나는데",
          "speaker_id": "1",
          "time": "20211104 13:05"
        },
        {
          "id": "MDRW2100046729.1.5",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20211104 13:20"
        },
        {
          "id": "MDRW2100046729.1.6",
          "form": "네넵!",
          "original_form": "네넵!",
          "speaker_id": "1",
          "time": "20211104 13:20"
        },
        {
          "id": "MDRW2100046729.1.7",
          "form": "점심에 뭐 드셨어요~?",
          "original_form": "점심에 뭐 드셨어요~?",
          "speaker_id": "2",
          "time": "20211104 13:24"
        },
        {
          "id": "MDRW2100046729.1.8",
          "form": "쏘야 아세요?",
          "original_form": "쏘야 아세요?",
          "speaker_id": "1",
          "time": "20211104 13:24"
        },
        {
          "id": "MDRW2100046729.1.9",
          "form": "쏘세지 야채볶음",
          "original_form": "쏘세지 야채볶음",
          "speaker_id": "1",
          "time": "20211104 13:24"
        },
        {
          "id": "MDRW2100046729.1.10",
          "form": "닭개장",
          "original_form": "닭개장",
          "speaker_id": "1",
          "time": "20211104 13:24"
        },
        {
          "id": "MDRW2100046729.1.11",
          "form": "감자조림",
          "original_form": "감자조림",
          "speaker_id": "1",
          "time": "20211104 13:25"
        },
        {
          "id": "MDRW2100046729.1.12",
          "form": "깍두기 이렇게 나왔어요",
          "original_form": "깍두기 이렇게 나왔어요",
          "speaker_id": "1",
          "time": "20211104 13:25"
        },
        {
          "id": "MDRW2100046729.1.13",
          "form": "아! 급식 드시는군요!!",
          "original_form": "아! 급식 드시는군요!!",
          "speaker_id": "2",
          "time": "20211104 13:28"
        },
        {
          "id": "MDRW2100046729.1.14",
          "form": "네! 맞아욯ㅎ",
          "original_form": "네! 맞아욯ㅎ",
          "speaker_id": "1",
          "time": "20211104 13:28"
        },
        {
          "id": "MDRW2100046729.1.15",
          "form": "부러워요 저도 급식 먹고싶어요 ㅠㅠ",
          "original_form": "부러워요 저도 급식 먹고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 13:28"
        },
        {
          "id": "MDRW2100046729.1.16",
          "form": "매일 점심 메뉴 고민하는게 일이에요",
          "original_form": "매일 점심 메뉴 고민하는게 일이에요",
          "speaker_id": "2",
          "time": "20211104 13:28"
        },
        {
          "id": "MDRW2100046729.1.17",
          "form": "ㅎㅎ 전에 있던 곳에서는 ",
          "original_form": "ㅎㅎ 전에 있던 곳에서는 ",
          "speaker_id": "1",
          "time": "20211104 13:29"
        },
        {
          "id": "MDRW2100046729.1.18",
          "form": "구내식당이 맛없어서",
          "original_form": "구내식당이 맛없어서",
          "speaker_id": "1",
          "time": "20211104 13:29"
        },
        {
          "id": "MDRW2100046729.1.19",
          "form": "매일 나가 먹었는데",
          "original_form": "매일 나가 먹었는데",
          "speaker_id": "1",
          "time": "20211104 13:29"
        },
        {
          "id": "MDRW2100046729.1.20",
          "form": "그떄는 고민이 많았어요",
          "original_form": "그떄는 고민이 많았어요",
          "speaker_id": "1",
          "time": "20211104 13:29"
        },
        {
          "id": "MDRW2100046729.1.21",
          "form": "구내식당이 맛있는게 진짜 복이에요!!",
          "original_form": "구내식당이 맛있는게 진짜 복이에요!!",
          "speaker_id": "2",
          "time": "20211104 13:30"
        },
        {
          "id": "MDRW2100046729.1.22",
          "form": "급식표 보고 기대하게 되잖아요 ㅋㅋㅋ",
          "original_form": "급식표 보고 기대하게 되잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:31"
        },
        {
          "id": "MDRW2100046729.1.23",
          "form": "ㅎㅎ 그렇죠",
          "original_form": "ㅎㅎ 그렇죠",
          "speaker_id": "1",
          "time": "20211104 13:31"
        },
        {
          "id": "MDRW2100046729.1.24",
          "form": "저는 급식 먹을때 카레 나오는 날을 가장 좋아했어요",
          "original_form": "저는 급식 먹을때 카레 나오는 날을 가장 좋아했어요",
          "speaker_id": "2",
          "time": "20211104 13:32"
        },
        {
          "id": "MDRW2100046729.1.25",
          "form": "저도 카레 좋아하는데..ㅎㅎ 근데 많은 사람들이 급식에 카레 나오면 싫어하더라구요",
