      "utterance": [
        {
          "id": "MDRW2100003479.1.1",
          "form": "밤늦게 술 마시지 마라",
          "original_form": "밤늦게 술 마시지 마라",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003479.1.2",
          "form": "술주정뱅이 된다",
          "original_form": "술주정뱅이 된다",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.3",
          "form": "야식먹자",
          "original_form": "야식먹자",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.4",
          "form": "술안먹음",
          "original_form": "술안먹음",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.5",
          "form": "이미 먹었잖아",
          "original_form": "이미 먹었잖아",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.6",
          "form": "적게먹는편이다",
          "original_form": "적게먹는편이다",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.7",
          "form": "무슨소리",
          "original_form": "무슨소리",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.8",
          "form": "1병 먹는데",
          "original_form": "1병 먹는데",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.9",
          "form": "많이 먹으면서",
          "original_form": "많이 먹으면서",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.10",
          "form": "한병도 많다",
          "original_form": "한병도 많다",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.11",
          "form": "1병은 간에 기별도 안감",
          "original_form": "1병은 간에 기별도 안감",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.12",
          "form": "그러다 알콜중독자 된다",
          "original_form": "그러다 알콜중독자 된다",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.13",
          "form": "난 5병먹어도 안취함",
          "original_form": "난 5병먹어도 안취함",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.14",
          "form": "갠찬타",
          "original_form": "갠찬타",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.15",
          "form": "그럼 안됌",
          "original_form": "그럼 안됌",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.16",
          "form": "안취하는게 더 안좋대",
          "original_form": "안취하는게 더 안좋대",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.17",
          "form": "술 끊어라 돈도 아끼게",
          "original_form": "술 끊어라 돈도 아끼게",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003479.1.18",
          "form": "1병은 간에 기별도 안간다 난 5병먹어도 괜찬타고",
          "original_form": "1병은 간에 기별도 안간다 난 5병먹어도 괜찬타고",
          "speaker_id": "1",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003479.1.19",
          "form": "괜찮은게 괜찮은게 아니라고했음",
          "original_form": "괜찮은게 괜찮은게 아니라고했음",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003479.1.20",
          "form": "차라리 취하는게 낫다했다 몸에는",
          "original_form": "차라리 취하는게 낫다했다 몸에는",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003479.1.21",
          "form": "내일은 뭐 먹지",
          "original_form": "내일은 뭐 먹지",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003479.1.22",
          "form": "니가 요리좀 해봐라",
          "original_form": "니가 요리좀 해봐라",
          "speaker_id": "2",
          "time": "20210820 23:38"
        }
      ]
