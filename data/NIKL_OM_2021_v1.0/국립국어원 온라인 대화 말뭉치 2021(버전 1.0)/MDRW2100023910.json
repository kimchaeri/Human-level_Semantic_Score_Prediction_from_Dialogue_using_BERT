      "utterance": [
        {
          "id": "MDRW2100023910.1.1",
          "form": "혹시 김치",
          "original_form": "혹시 김치",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023910.1.2",
          "form": "직접 담궈드세요?",
          "original_form": "직접 담궈드세요?",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023910.1.3",
          "form": "아뇨 거의 사먹는거 같아요~",
          "original_form": "아뇨 거의 사먹는거 같아요~",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023910.1.4",
          "form": "어디꺼 드세요??",
          "original_form": "어디꺼 드세요??",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023910.1.5",
          "form": "저는 홈쇼핑 김치는 다 물리더라구요ㅠ",
          "original_form": "저는 홈쇼핑 김치는 다 물리더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023910.1.6",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023910.1.7",
          "form": "엄마한테 여쭤보니 담궈드신다네요..!",
          "original_form": "엄마한테 여쭤보니 담궈드신다네요..!",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023910.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023910.1.9",
          "form": "김치 사먹을땐 종가집 많이 먹어요",
          "original_form": "김치 사먹을땐 종가집 많이 먹어요",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023910.1.10",
          "form": "아하...?ㅋㅋㅋㅋㅋ",
          "original_form": "아하...?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023910.1.11",
          "form": "어머니 솜씨가 시판제품으로 내놔도 손색없을정도로",
          "original_form": "어머니 솜씨가 시판제품으로 내놔도 손색없을정도로",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023910.1.12",
          "form": "맛있어서 헷갈리셨나봐요!",
          "original_form": "맛있어서 헷갈리셨나봐요!",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023910.1.13",
          "form": "김치를 잘 안먹어서요. ,ㅠㅠ",
          "original_form": "김치를 잘 안먹어서요. ,ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023910.1.14",
          "form": "별로 김치에 관심이 없달까;;;",
          "original_form": "별로 김치에 관심이 없달까;;;",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023910.1.15",
          "form": "무던한 입맛이셔서 좋으시겠어용",
          "original_form": "무던한 입맛이셔서 좋으시겠어용",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023910.1.16",
          "form": "넘 까다로우면 음식 해주는 사람도",
          "original_form": "넘 까다로우면 음식 해주는 사람도",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023910.1.17",
          "form": "싫어하더라구요ㅋㅋㅋ",
          "original_form": "싫어하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023910.1.18",
          "form": "맞아요 잘먹는거 좋아하더라구요ㅎㅎ",
          "original_form": "맞아요 잘먹는거 좋아하더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023910.1.19",
          "form": "혹시 젤 좋았던 맛집 있으세요?",
          "original_form": "혹시 젤 좋았던 맛집 있으세요?",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023910.1.20",
          "form": "저는~~~ 맛집은 아닌데",
          "original_form": "저는~~~ 맛집은 아닌데",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023910.1.21",
          "form": "제가 원래 카페인때문에 커피를 안마시는데",
          "original_form": "제가 원래 카페인때문에 커피를 안마시는데",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023910.1.22",
          "form": "게다가 코코넛도 되게 싫어하거든요",
          "original_form": "게다가 코코넛도 되게 싫어하거든요",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100023910.1.23",
          "form": "근데 연남동에 콩카페라는곳을 갔는데",
          "original_form": "근데 연남동에 콩카페라는곳을 갔는데",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023910.1.24",
          "form": "진짜 너무 맛있었어요...",
          "original_form": "진짜 너무 맛있었어요...",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023910.1.25",
          "form": "오 메모해놔야겠어요",
          "original_form": "오 메모해놔야겠어요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023910.1.26",
          "form": "얼마나 맛있으셨길래..궁금하네요",
          "original_form": "얼마나 맛있으셨길래..궁금하네요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023910.1.27",
          "form": "코코넛 특유의 향도 안나면서 커피가 고소하고 달콤해요",
          "original_form": "코코넛 특유의 향도 안나면서 커피가 고소하고 달콤해요",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023910.1.28",
          "form": "커피 쓴맛도 없구요ㅋㅋ",
          "original_form": "커피 쓴맛도 없구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100023910.1.29",
          "form": "그쪽 가게되면 꼭 먹어봐야겠어요~~~",
          "original_form": "그쪽 가게되면 꼭 먹어봐야겠어요~~~",
          "speaker_id": "2",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023910.1.30",
          "form": "이거 아마 연남동이 본점이고",
          "original_form": "이거 아마 연남동이 본점이고",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100023910.1.31",
          "form": "인기 많아서 체인냈을거에용",
          "original_form": "인기 많아서 체인냈을거에용",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100023910.1.32",
          "form": "다른곳에도 있으니 꼭 드셔보세용!ㅎㅎ",
          "original_form": "다른곳에도 있으니 꼭 드셔보세용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:12"
        }
      ]
    }
