      "utterance": [
        {
          "id": "MDRW2100039273.1.1",
          "form": "안녕하세요~ 예능 좋아하세요?",
          "original_form": "안녕하세요~ 예능 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039273.1.2",
          "form": "좋아해요 놀토는 꼭 챙겨봐요",
          "original_form": "좋아해요 놀토는 꼭 챙겨봐요",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039273.1.3",
          "form": "챙겨 보시는 거 있나요?",
          "original_form": "챙겨 보시는 거 있나요?",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039273.1.4",
          "form": "저는 지금 해방타운 보고 있어요ㅎㅎㅎ",
          "original_form": "저는 지금 해방타운 보고 있어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039273.1.5",
          "form": "유퀴즈나 놀뭐도 자주 봅니당",
          "original_form": "유퀴즈나 놀뭐도 자주 봅니당",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039273.1.6",
          "form": "앗 집이시구나 부러워요ㅠㅠ",
          "original_form": "앗 집이시구나 부러워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039273.1.7",
          "form": "네ㅋㅋㅋㅋ소파에 누워서 보고있어요",
          "original_form": "네ㅋㅋㅋㅋ소파에 누워서 보고있어요",
          "speaker_id": "1",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100039273.1.8",
          "form": "넘 부럽다ㅜㅜ 빨리 퇴근하고 싶어요ㅠㅠ",
          "original_form": "넘 부럽다ㅜㅜ 빨리 퇴근하고 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039273.1.9",
          "form": "해방타운은 근데 처음 들어요",
          "original_form": "해방타운은 근데 처음 들어요",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039273.1.10",
          "form": "무슨 예능인가요?",
          "original_form": "무슨 예능인가요?",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039273.1.11",
          "form": "오늘 금요일이니까 그래도 화이팅..!",
          "original_form": "오늘 금요일이니까 그래도 화이팅..!",
          "speaker_id": "1",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039273.1.12",
          "form": "화이팅 ㅋㅋㅋㅋㅋ",
          "original_form": "화이팅 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039273.1.13",
          "form": "해방타운 기혼이신 분들이 오피스텔에서 독립하는 프로에요!",
          "original_form": "해방타운 기혼이신 분들이 오피스텔에서 독립하는 프로에요!",
          "speaker_id": "1",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039273.1.14",
          "form": "약간 나혼산 기혼버전 같아요ㅋㅋㅋ",
          "original_form": "약간 나혼산 기혼버전 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039273.1.15",
          "form": "일박이일 동안 오피스텔서 묵고 가시더라구요",
          "original_form": "일박이일 동안 오피스텔서 묵고 가시더라구요",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039273.1.16",
          "form": "오 그래서 해방ㅋㅋㅋㅋㅋㅋㅋ이군요",
          "original_form": "오 그래서 해방ㅋㅋㅋㅋㅋㅋㅋ이군요",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039273.1.17",
          "form": "각각 다른 오피스텔 가는 건가요?",
          "original_form": "각각 다른 오피스텔 가는 건가요?",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039273.1.18",
          "form": "넵ㅋㅋㅋ전 미혼인데도 재밌더라구요",
          "original_form": "넵ㅋㅋㅋ전 미혼인데도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039273.1.19",
          "form": "부부 예능 좋아하시나요?",
          "original_form": "부부 예능 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039273.1.20",
          "form": "한사람당 한 호실에 살아요!",
          "original_form": "한사람당 한 호실에 살아요!",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039273.1.21",
          "form": "부부예능 예전에 함소원 진화부부 나오는 프로그램 가끔 봤었어요 ㅎㅎ",
          "original_form": "부부예능 예전에 함소원 진화부부 나오는 프로그램 가끔 봤었어요 ㅎㅎ",
          "speaker_id": "1",
