      "utterance": [
        {
          "id": "MDRW2100042975.1.1",
          "form": "지금 회사시겠네요!",
          "original_form": "지금 회사시겠네요!",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042975.1.2",
          "form": "점심시간이신가?!",
          "original_form": "점심시간이신가?!",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042975.1.3",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 12:24"
        },
        {
          "id": "MDRW2100042975.1.4",
          "form": "어디세요?",
          "original_form": "어디세요?",
          "speaker_id": "2",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042975.1.5",
          "form": "회사에욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 퇴근하고싶다....",
          "original_form": "회사에욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 퇴근하고싶다....",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042975.1.6",
          "form": "후...너무요ㅋㅋㅋㅋㅋ",
          "original_form": "후...너무요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042975.1.7",
          "form": "몇시퇴근이세요",
          "original_form": "몇시퇴근이세요",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042975.1.8",
          "form": "9to6.........",
          "original_form": "9to6.........",
          "speaker_id": "1",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042975.1.9",
          "form": "왜 아직 화요일이죠? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 일도없는데 퇴근시켜주세요 사장님",
          "original_form": "왜 아직 화요일이죠? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 일도없는데 퇴근시켜주세요 사장님",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042975.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100042975.1.11",
          "form": "어우공감가네요",
          "original_form": "어우공감가네요",
          "speaker_id": "2",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100042975.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어디 외근나갈일없나 싶어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어디 외근나갈일없나 싶어요",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042975.1.13",
          "form": "시간때우기 최고...",
          "original_form": "시간때우기 최고...",
          "speaker_id": "1",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042975.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ왤케웃기죵",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ왤케웃기죵",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042975.1.15",
          "form": "주40시간인가요",
          "original_form": "주40시간인가요",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042975.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네네 맞아욬ㅋㅋㅋ 코로나라고 단축근무하는것도없네요..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네네 맞아욬ㅋㅋㅋ 코로나라고 단축근무하는것도없네요..",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100042975.1.17",
          "form": "ㅋㅋㅋㅋㅋ그르게요",
          "original_form": "ㅋㅋㅋㅋㅋ그르게요",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100042975.1.18",
          "form": "직장분위기는좋나요",
          "original_form": "직장분위기는좋나요",
          "speaker_id": "2",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100042975.1.19",
          "form": "솔직히 급여가 좀 적은데 그거빼곤 좋아요 ㅋㅋㅋ",
          "original_form": "솔직히 급여가 좀 적은데 그거빼곤 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100042975.1.20",
          "form": "사람들도 좋고..",
          "original_form": "사람들도 좋고..",
          "speaker_id": "1",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100042975.1.21",
          "form": "첫직장인데 ㅋㅋ잘온거같아요",
          "original_form": "첫직장인데 ㅋㅋ잘온거같아요",
          "speaker_id": "1",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100042975.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100042975.1.23",
          "form": "막내세요?",
          "original_form": "막내세요?",
