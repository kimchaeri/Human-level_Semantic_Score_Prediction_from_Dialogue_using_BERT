      "utterance": [
        {
          "id": "MDRW2100004007.1.1",
          "form": "오늘 식사는 하셨나요",
          "original_form": "오늘 식사는 하셨나요",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.3",
          "form": "저는 방금 라면 끓여먹었습니다",
          "original_form": "저는 방금 라면 끓여먹었습니다",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.4",
          "form": "저는 삼겹살에 비빔면 먹었어요",
          "original_form": "저는 삼겹살에 비빔면 먹었어요",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.5",
          "form": "남겼지만...",
          "original_form": "남겼지만...",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.6",
          "form": "무슨라면 드셨어요",
          "original_form": "무슨라면 드셨어요",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.7",
          "form": "남겼어도 식사하셨다니 훌륭합니다",
          "original_form": "남겼어도 식사하셨다니 훌륭합니다",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.8",
          "form": "박수 짝짝",
          "original_form": "박수 짝짝",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.9",
          "form": "진라면에 고춧가루 넣어먹었어요",
          "original_form": "진라면에 고춧가루 넣어먹었어요",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004007.1.10",
          "form": "저는 이상하게 비빔면이 잘 손이 안가요",
          "original_form": "저는 이상하게 비빔면이 잘 손이 안가요",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004007.1.11",
          "form": "새콤해서 그런가",
          "original_form": "새콤해서 그런가",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004007.1.12",
          "form": "아 신맛 별로 안좋아하신댔죠",
          "original_form": "아 신맛 별로 안좋아하신댔죠",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004007.1.13",
          "form": "비빔장엔 주로 식초가 들어가니까",
          "original_form": "비빔장엔 주로 식초가 들어가니까",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004007.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004007.1.15",
          "form": "전 신거 좋아해요",
          "original_form": "전 신거 좋아해요",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004007.1.16",
          "form": "사실 단거 쓴거도 다 잘 먹지만 후후",
          "original_form": "사실 단거 쓴거도 다 잘 먹지만 후후",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004007.1.17",
          "form": "무슨 라면을 제일 좋아하시나요",
          "original_form": "무슨 라면을 제일 좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004007.1.18",
          "form": "저 라면 요새 많이 안먹긴 하는데",
          "original_form": "저 라면 요새 많이 안먹긴 하는데",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004007.1.19",
          "form": "너구리 매운맛 오래 먹었어요",
          "original_form": "너구리 매운맛 오래 먹었어요",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004007.1.20",
          "form": "아 그거 괜찮더라구요",
          "original_form": "아 그거 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004007.1.21",
          "form": "근데 어떤 날은 엄청 매운 것 같고",
          "original_form": "근데 어떤 날은 엄청 매운 것 같고",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004007.1.22",
          "form": "어떤날은 하나도 안매운 것 같은",
          "original_form": "어떤날은 하나도 안매운 것 같은",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004007.1.23",
          "form": "이상한...",
          "original_form": "이상한...",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004007.1.24",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004007.1.25",
          "form": "어느날은 엄청 느끼한것도 같고",
          "original_form": "어느날은 엄청 느끼한것도 같고",
          "speaker_id": "2",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004007.1.26",
          "form": "그래도 그 오동통한 면이 좋아요",
          "original_form": "그래도 그 오동통한 면이 좋아요",
          "speaker_id": "2",
          "time": "20210821 14:49"
        },
        {
          "id": "MDRW2100004007.1.27",
          "form": "그래도 너구리가 다른 라면보다 덜 물리는 것 같아요",
          "original_form": "그래도 너구리가 다른 라면보다 덜 물리는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004007.1.28",
          "form": "면이 통통해서 그런가",
          "original_form": "면이 통통해서 그런가",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004007.1.29",
          "form": "오 너무 가는 면은 별로에요?",
          "original_form": "오 너무 가는 면은 별로에요?",
          "speaker_id": "2",
          "time": "20210821 14:52"
        },
        {
          "id": "MDRW2100004007.1.30",
          "form": "막 별로는 아닌데",
          "original_form": "막 별로는 아닌데",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004007.1.31",
          "form": "금방 물리는 것 같아요",
          "original_form": "금방 물리는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004007.1.32",
          "form": "라면이 다 그렇지만...",
          "original_form": "라면이 다 그렇지만...",
          "speaker_id": "1",
          "time": "20210821 14:53"
        }
      ]
