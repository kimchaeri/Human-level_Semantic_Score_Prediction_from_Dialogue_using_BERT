      "utterance": [
        {
          "id": "MDRW2100008434.1.1",
          "form": "하시는 커뮤 있으신가요???",
          "original_form": "하시는 커뮤 있으신가요???",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100008434.1.2",
          "form": "아니요 딱히 없어요",
          "original_form": "아니요 딱히 없어요",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100008434.1.3",
          "form": "하시는 커뮤있으세요?",
          "original_form": "하시는 커뮤있으세요?",
          "speaker_id": "1",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100008434.1.4",
          "form": "전 주로 sns 위주로 한답니다",
          "original_form": "전 주로 sns 위주로 한답니다",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100008434.1.5",
          "form": "sns에다 뭘 올려야 될지 모르겠어서",
          "original_form": "sns에다 뭘 올려야 될지 모르겠어서",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100008434.1.6",
          "form": "저는 계정만있어요ㅋㅋㅋㄱ",
          "original_form": "저는 계정만있어요ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100008434.1.7",
          "form": "ㅋㅋ 저도 보기만 해요",
          "original_form": "ㅋㅋ 저도 보기만 해요",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100008434.1.8",
          "form": "블로그 해보려다가 포기 했어요",
          "original_form": "블로그 해보려다가 포기 했어요",
          "speaker_id": "2",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100008434.1.9",
          "form": "저는 게임 좋아하다보니까 인터넷으로 만난 분들하고 정모는 해봤어요",
          "original_form": "저는 게임 좋아하다보니까 인터넷으로 만난 분들하고 정모는 해봤어요",
          "speaker_id": "1",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100008434.1.10",
          "form": "오 정말요?",
          "original_form": "오 정말요?",
          "speaker_id": "2",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100008434.1.11",
          "form": "정모해서 같이 게임해요?",
          "original_form": "정모해서 같이 게임해요?",
          "speaker_id": "2",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100008434.1.12",
          "form": "게임같이 하던분들하고 만나서 밥먹고 이야기하고 술마시고 했어요",
          "original_form": "게임같이 하던분들하고 만나서 밥먹고 이야기하고 술마시고 했어요",
          "speaker_id": "1",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100008434.1.13",
          "form": "우와 신기해요",
          "original_form": "우와 신기해요",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100008434.1.14",
          "form": "전 그렇게 정모를 해본 적이 없어서요",
          "original_form": "전 그렇게 정모를 해본 적이 없어서요",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100008434.1.15",
          "form": "몇명안되었지만 그분들이랑",
          "original_form": "몇명안되었지만 그분들이랑",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100008434.1.16",
          "form": "아직도 잘지내요",
          "original_form": "아직도 잘지내요",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100008434.1.17",
          "form": "한 3년쯤 되었어요",
          "original_form": "한 3년쯤 되었어요",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100008434.1.18",
          "form": "게임 덕분에",
          "original_form": "게임 덕분에",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100008434.1.19",
          "form": "좋은 인연이 생겼네요!!",
          "original_form": "좋은 인연이 생겼네요!!",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100008434.1.20",
          "form": "네! 그래서 인식이 좋아요",
          "original_form": "네! 그래서 인식이 좋아요",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100008434.1.21",
          "form": "정모를 하면",
          "original_form": "정모를 하면",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100008434.1.22",
          "form": "다양한 인연을 만들 수 있어서",
