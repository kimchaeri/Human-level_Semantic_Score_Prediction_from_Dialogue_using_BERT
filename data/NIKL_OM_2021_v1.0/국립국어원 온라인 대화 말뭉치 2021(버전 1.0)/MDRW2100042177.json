        {
          "id": "MDRW2100042177.1.1",
          "form": "식사는하셨어요?",
          "original_form": "식사는하셨어요?",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042177.1.2",
          "form": "저녁은 그냥 과자로 때웟어염",
          "original_form": "저녁은 그냥 과자로 때웟어염",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042177.1.3",
          "form": "무슨과자드셨어용",
          "original_form": "무슨과자드셨어용",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042177.1.4",
          "form": "뽀또랑 포테토칩이용",
          "original_form": "뽀또랑 포테토칩이용",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042177.1.5",
          "form": "좋아하는 음식이썽요?",
          "original_form": "좋아하는 음식이썽요?",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042177.1.6",
          "form": "저도과자좋아해요",
          "original_form": "저도과자좋아해요",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042177.1.7",
          "form": "버터링이랑감자칩이요",
          "original_form": "버터링이랑감자칩이요",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042177.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042177.1.9",
          "form": "감자칩은 만인의과자죠 ㅠㅠ",
          "original_form": "감자칩은 만인의과자죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042177.1.10",
          "form": "당기네요..",
          "original_form": "당기네요..",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042177.1.11",
          "form": "야식 잘드세요?",
          "original_form": "야식 잘드세요?",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042177.1.12",
          "form": "ㅎㅎ매일밤고비죠",
          "original_form": "ㅎㅎ매일밤고비죠",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042177.1.13",
          "form": "속이안좋아서참으려고는해요",
          "original_form": "속이안좋아서참으려고는해요",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042177.1.14",
          "form": "저두요 ㅠ 살안찌는체질이면 엄청먹응텐데 ..",
          "original_form": "저두요 ㅠ 살안찌는체질이면 엄청먹응텐데 ..",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042177.1.15",
          "form": "그르니까요ㅜ",
          "original_form": "그르니까요ㅜ",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042177.1.16",
          "form": "저는단것도좋아해요",
          "original_form": "저는단것도좋아해요",
          "speaker_id": "1",
          "time": "20210906 21:56"
