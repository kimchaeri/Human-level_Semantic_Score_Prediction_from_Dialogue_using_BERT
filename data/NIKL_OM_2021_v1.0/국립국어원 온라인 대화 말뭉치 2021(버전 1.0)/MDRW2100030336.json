      "utterance": [
        {
          "id": "MDRW2100030336.1.1",
          "form": "진짜...ㅠㅠ",
          "original_form": "진짜...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030336.1.2",
          "form": "그리고 저 미우보면서 감탄했던게",
          "original_form": "그리고 저 미우보면서 감탄했던게",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030336.1.3",
          "form": "많기는 하지만 그중에 하나가",
          "original_form": "많기는 하지만 그중에 하나가",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030336.1.4",
          "form": "마지막에 감전 나오는 타이밍이 너무",
          "original_form": "마지막에 감전 나오는 타이밍이 너무",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030336.1.5",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030336.1.6",
          "form": "헐헐!!! 맞아요!!!!! 전 근데 그 이부키 스승 에피만큼은",
          "original_form": "헐헐!!! 맞아요!!!!! 전 근데 그 이부키 스승 에피만큼은",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030336.1.7",
          "form": "레몬이 좀 더...",
          "original_form": "레몬이 좀 더...",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030336.1.8",
          "form": "생각났어요..",
          "original_form": "생각났어요..",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030336.1.9",
          "form": "맞아요 그 레몬 편집한 영상",
          "original_form": "맞아요 그 레몬 편집한 영상",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030336.1.10",
          "form": "아마 찾으면 나올거에요",
          "original_form": "아마 찾으면 나올거에요",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030336.1.11",
          "form": "그거 진짜 좋아요...",
          "original_form": "그거 진짜 좋아요...",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030336.1.12",
          "form": "헐 유튜브에 치면 나오나요?!?@?!???!?",
          "original_form": "헐 유튜브에 치면 나오나요?!?@?!???!?",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030336.1.13",
          "form": "지금 쳐보고 오겠슴다!",
          "original_form": "지금 쳐보고 오겠슴다!",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030336.1.14",
          "form": "유튜브는 아니고 트위터에서 찾았던거같은데 키워드가 기억이 잘 안나네요ㅠㅠㅠ",
          "original_form": "유튜브는 아니고 트위터에서 찾았던거같은데 키워드가 기억이 잘 안나네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030336.1.15",
          "form": "이거 링크 보내지나",
          "original_form": "이거 링크 보내지나",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030336.1.16",
          "form": "",
          "original_form": "{share:info}",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030336.1.17",
          "form": "흐악 복사가 안 돼요",
          "original_form": "흐악 복사가 안 돼요",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030336.1.18",
          "form": "쳐서 보고 오겠습니닷!",
          "original_form": "쳐서 보고 오겠습니닷!",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030336.1.19",
          "form": "ㅎ.ㅎ 아쌈",
          "original_form": "ㅎ.ㅎ 아쌈",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030336.1.20",
          "form": "아싸!",
          "original_form": "아싸!",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030336.1.21",
          "form": "ㅠㅠㅠㅠㅠ아니 복사가 안된다니 8화 엔딩 레몬",
          "original_form": "ㅠㅠㅠㅠㅠ아니 복사가 안된다니 8화 엔딩 레몬",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030336.1.22",
          "form": "이라고 검색하시면",
          "original_form": "이라고 검색하시면",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030336.1.23",
          "form": "어떤분이 엉디 까고 날라오는 영상이랑 같이 찾는트윗올린더있는데",
          "original_form": "어떤분이 엉디 까고 날라오는 영상이랑 같이 찾는트윗올린더있는데",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030336.1.24",
          "form": "그거타래에 있어요!!!",
          "original_form": "그거타래에 있어요!!!",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030336.1.25",
          "form": "아니!!!!! 짱웃기네 저 방금 그 엉디 까는게 대체 뭐지",
          "original_form": "아니!!!!! 짱웃기네 저 방금 그 엉디 까는게 대체 뭐지",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030336.1.26",
          "form": "하면서 슉슉 내리고 있다가",
          "original_form": "하면서 슉슉 내리고 있다가",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030336.1.27",
          "form": "별안간 엉덩이를 봤네요...",
          "original_form": "별안간 엉덩이를 봤네요...",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030336.1.28",
          "form": "보고 오겠습다!",
          "original_form": "보고 오겠습다!",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030336.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030336.1.30",
          "form": "별안간 날라오는 엉디...다녀오세용!!",
          "original_form": "별안간 날라오는 엉디...다녀오세용!!",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030336.1.31",
          "form": "ㅜ.ㅜ 또 주책맞게 눈물",
          "original_form": "ㅜ.ㅜ 또 주책맞게 눈물",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030336.1.32",
