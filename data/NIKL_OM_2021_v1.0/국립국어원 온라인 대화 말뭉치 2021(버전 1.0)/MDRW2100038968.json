      "utterance": [
        {
          "id": "MDRW2100038968.1.1",
          "form": "촉법범죄인가",
          "original_form": "촉법범죄인가",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100038968.1.2",
          "form": "글쎄 범죄조사가 조작되었다네",
          "original_form": "글쎄 범죄조사가 조작되었다네",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038968.1.3",
          "form": "헐 기가막힌다",
          "original_form": "헐 기가막힌다",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038968.1.4",
          "form": "그러게 1심 유죄",
          "original_form": "그러게 1심 유죄",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038968.1.5",
          "form": "열심히 하셨네",
          "original_form": "열심히 하셨네",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038968.1.6",
          "form": "응 그래서 2심에서 무죄",
          "original_form": "응 그래서 2심에서 무죄",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038968.1.7",
          "form": "수임료는 적겠다",
          "original_form": "수임료는 적겠다",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038968.1.8",
          "form": "재심사건이 해결되면 가치가 올라간데 ㅋㅋ",
          "original_form": "재심사건이 해결되면 가치가 올라간데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038968.1.9",
          "form": "솔직한 애기하신다 ㅋㄱ",
          "original_form": "솔직한 애기하신다 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038968.1.10",
          "form": "출연료&강연료도 수입이",
          "original_form": "출연료&강연료도 수입이",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038968.1.11",
          "form": "코르나때문에 강연이 어럽단다",
          "original_form": "코르나때문에 강연이 어럽단다",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038968.1.12",
          "form": "진짜 솔직하게 하시네 ㅋㅋ",
          "original_form": "진짜 솔직하게 하시네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038968.1.13",
          "form": "응 재밌겠다",
          "original_form": "응 재밌겠다",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038968.1.14",
          "form": "그러게 계속 시청해야겟다",
          "original_form": "그러게 계속 시청해야겟다",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038968.1.15",
