          "speaker_id": "1",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.12",
          "form": "그거대화맞지",
          "original_form": "그거대화맞지",
          "speaker_id": "1",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.13",
          "form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.14",
          "form": "웃긴게남자쪽에서그걸",
          "original_form": "웃긴게남자쪽에서그걸",
          "speaker_id": "1",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.15",
          "form": "올린게",
          "original_form": "올린게",
          "speaker_id": "1",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.16",
          "form": "그거 남자 측에서",
          "original_form": "그거 남자 측에서",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.17",
          "form": "그니가",
          "original_form": "그니가",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.18",
          "form": "ㅋㅋㅋㅋㅋ완전양심없어",
          "original_form": "ㅋㅋㅋㅋㅋ완전양심없어",
          "speaker_id": "1",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.19",
          "form": "진심 정말 웃김",
          "original_form": "진심 정말 웃김",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.20",
          "form": "그것도있는거알아?",
          "original_form": "그것도있는거알아?",
          "speaker_id": "1",
          "time": "20210830 14:22"
