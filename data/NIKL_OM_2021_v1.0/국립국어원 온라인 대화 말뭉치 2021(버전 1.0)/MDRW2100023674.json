      "utterance": [
        {
          "id": "MDRW2100023674.1.1",
          "form": "그니까 왜 더 퇴화하냐고",
          "original_form": "그니까 왜 더 퇴화하냐고",
          "speaker_id": "2",
          "time": "20210830 14:19"
        },
        {
          "id": "MDRW2100023674.1.2",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "1",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100023674.1.3",
          "form": "근데웃긴건반반결혼은",
          "original_form": "근데웃긴건반반결혼은",
          "speaker_id": "1",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100023674.1.4",
          "form": "더당연하게생각하는거",
          "original_form": "더당연하게생각하는거",
          "speaker_id": "1",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100023674.1.5",
          "form": "옛날보다",
          "original_form": "옛날보다",
          "speaker_id": "1",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100023674.1.6",
          "form": "아 나 그거 봄",
          "original_form": "아 나 그거 봄",
          "speaker_id": "2",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100023674.1.7",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "1",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100023674.1.8",
          "form": "여자가 진짜 반반 결혼 하자고 세세하게 따지니까 남자가 효도 하는 건 당연한거 아니냐고 넌 왜 그러냐고 했던고",
          "original_form": "여자가 진짜 반반 결혼 하자고 세세하게 따지니까 남자가 효도 하는 건 당연한거 아니냐고 넌 왜 그러냐고 했던고",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.9",
          "form": "거",
          "original_form": "거",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023674.1.10",
