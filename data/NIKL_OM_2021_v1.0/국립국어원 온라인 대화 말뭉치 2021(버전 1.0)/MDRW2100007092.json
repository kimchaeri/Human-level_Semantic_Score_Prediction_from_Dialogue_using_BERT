      "utterance": [
        {
          "id": "MDRW2100007092.1.1",
          "form": "오 하는 게임 있어?",
          "original_form": "오 하는 게임 있어?",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007092.1.2",
          "form": "응 나 포켓몬 좋아해",
          "original_form": "응 나 포켓몬 좋아해",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007092.1.3",
          "form": "혹시 포켓몬 유나이티드...? 아니면 포켓몬고?",
          "original_form": "혹시 포켓몬 유나이티드...? 아니면 포켓몬고?",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007092.1.4",
          "form": "포켓몬고!",
          "original_form": "포켓몬고!",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007092.1.5",
          "form": "포켓몬 고 아직도 재밌나보네!",
          "original_form": "포켓몬 고 아직도 재밌나보네!",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007092.1.6",
          "form": "포켓몬고가 걸을 때 쏠쏠하지",
          "original_form": "포켓몬고가 걸을 때 쏠쏠하지",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007092.1.7",
          "form": "맞아 재밌어! 요즘 코로나 때문에 잘 안하게 되지만ㅋㅋ",
          "original_form": "맞아 재밌어! 요즘 코로나 때문에 잘 안하게 되지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007092.1.8",
          "form": "근데 내 집에서 체육관 잡혀서ㅋㅋㅋㅋ",
          "original_form": "근데 내 집에서 체육관 잡혀서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007092.1.9",
          "form": "와 대박 잭팟이네ㅋㅋㅋ",
          "original_form": "와 대박 잭팟이네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007092.1.10",
          "form": "ㅇㅇ체육관 관장된 느낌ㅋㅋㅋ",
          "original_form": "ㅇㅇ체육관 관장된 느낌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007092.1.11",
          "form": "완전 부럽닼ㅋㅋㅋㅋ 나 첨에 출시되고 몇번 하다가 접었는데 집 근처에 체육관 많이 생겼을려나...",
          "original_form": "완전 부럽닼ㅋㅋㅋㅋ 나 첨에 출시되고 몇번 하다가 접었는데 집 근처에 체육관 많이 생겼을려나...",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007092.1.12",
          "form": "요즘 많이 생겼더라고! 요즘은제보로도 체육관 만들수 있어 누가 내집근처에다 요청했나봐ㅋㅋ",
          "original_form": "요즘 많이 생겼더라고! 요즘은제보로도 체육관 만들수 있어 누가 내집근처에다 요청했나봐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007092.1.13",
          "form": "헐 대박 신기해ㅋㅋㅋ 나도 깔아서 해봐야겠다",
          "original_form": "헐 대박 신기해ㅋㅋㅋ 나도 깔아서 해봐야겠다",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007092.1.14",
          "form": "추천해! 혹시 몰라 잭팟이 있을지",
          "original_form": "추천해! 혹시 몰라 잭팟이 있을지",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007092.1.15",
          "form": "그래그래 나도 관장 함 되어보고 싶네",
          "original_form": "그래그래 나도 관장 함 되어보고 싶네",
          "speaker_id": "2",
          "time": "20210822 00:40"
        }
      ]
