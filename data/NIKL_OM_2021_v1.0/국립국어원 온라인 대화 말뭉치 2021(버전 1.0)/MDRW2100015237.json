      "utterance": [
        {
          "id": "MDRW2100015237.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015237.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015237.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015237.1.4",
          "form": "그저 그래요",
          "original_form": "그저 그래요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015237.1.5",
          "form": "어떤거 좋아해요?",
          "original_form": "어떤거 좋아해요?",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015237.1.6",
          "form": "크아라고 아세요?",
          "original_form": "크아라고 아세요?",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015237.1.7",
          "form": "저 완전 고수예요",
          "original_form": "저 완전 고수예요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015237.1.8",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015237.1.9",
          "form": "아네",
          "original_form": "아네",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015237.1.10",
          "form": "요즘에 안한지 꽤 됐지만요",
          "original_form": "요즘에 안한지 꽤 됐지만요",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015237.1.11",
          "form": "초면에 ㅋ하나는 기분 나쁘네요",
          "original_form": "초면에 ㅋ하나는 기분 나쁘네요",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.12",
          "form": "그럼 짭고수네요",
          "original_form": "그럼 짭고수네요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.13",
          "form": "조심해주시겠어요?",
          "original_form": "조심해주시겠어요?",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.14",
          "form": "저희 대화 많이 했는데",
          "original_form": "저희 대화 많이 했는데",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.15",
          "form": "장난하세요?",
          "original_form": "장난하세요?",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.16",
          "form": "구면이잖아요~",
          "original_form": "구면이잖아요~",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.17",
          "form": "초면인데요.",
          "original_form": "초면인데요.",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.18",
          "form": "왜 그러세요 저희 게임 얘기중인데",
          "original_form": "왜 그러세요 저희 게임 얘기중인데",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.19",
          "form": "왜 친한척 하시죠 ?",
          "original_form": "왜 친한척 하시죠 ?",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.20",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.21",
          "form": "아네",
          "original_form": "아네",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.22",
          "form": "테일즈런너 아세요?",
          "original_form": "테일즈런너 아세요?",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.23",
          "form": "오프라인에서 하는 게임도 좋아해요?",
          "original_form": "오프라인에서 하는 게임도 좋아해요?",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015237.1.24",
          "form": "알죠",
          "original_form": "알죠",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015237.1.25",
          "form": "네 너무 좋아해요",
          "original_form": "네 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015237.1.26",
          "form": "허이허성~",
          "original_form": "허이허성~",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015237.1.27",
          "form": "오프에서 하는게 더 좋아요",
          "original_form": "오프에서 하는게 더 좋아요",
          "speaker_id": "2",
