      "utterance": [
        {
          "id": "MDRW2100002852.1.1",
          "form": "대학 졸업하고 어떤 일 하게 될거 같아?",
          "original_form": "대학 졸업하고 어떤 일 하게 될거 같아?",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002852.1.2",
          "form": "아마 전공쪽으로 가게 되지 않을까",
          "original_form": "아마 전공쪽으로 가게 되지 않을까",
          "speaker_id": "2",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002852.1.3",
          "form": "전공이 뭐였지?",
          "original_form": "전공이 뭐였지?",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002852.1.4",
          "form": "컴퓨터인데",
          "original_form": "컴퓨터인데",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002852.1.5",
          "form": "너무 어려워",
          "original_form": "너무 어려워",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002852.1.6",
          "form": "너는 무슨 일 할거야?",
          "original_form": "너는 무슨 일 할거야?",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002852.1.7",
          "form": "컴퓨터...듣기만해도 어려워보여",
          "original_form": "컴퓨터...듣기만해도 어려워보여",
          "speaker_id": "1",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002852.1.8",
          "form": "나는 프리랜서 할거 같아",
          "original_form": "나는 프리랜서 할거 같아",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002852.1.9",
          "form": "번역가나 작가 과외선생님이 현재 목표야",
          "original_form": "번역가나 작가 과외선생님이 현재 목표야",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002852.1.10",
          "form": "오 프리랜서 멋있다",
          "original_form": "오 프리랜서 멋있다",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002852.1.11",
          "form": "열심히 안하면 굶어",
          "original_form": "열심히 안하면 굶어",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002852.1.12",
          "form": "열심히 해야하는 직종",
          "original_form": "열심히 해야하는 직종",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002852.1.13",
          "form": "하는만큼 벌어가니까",
          "original_form": "하는만큼 벌어가니까",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002852.1.14",
          "form": "아무래도 그렇지",
          "original_form": "아무래도 그렇지",
          "speaker_id": "2",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002852.1.15",
          "form": "너는 취직하려면 뭘 준비해야해?",
          "original_form": "너는 취직하려면 뭘 준비해야해?",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002852.1.16",
          "form": "자격증?",
          "original_form": "자격증?",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002852.1.17",
          "form": "어떤 자격증이 있으면 좋아?",
          "original_form": "어떤 자격증이 있으면 좋아?",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002852.1.18",
          "form": "일단 컴활 따려고 했는데",
          "original_form": "일단 컴활 따려고 했는데",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002852.1.19",
          "form": "계속 미루네",
          "original_form": "계속 미루네",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002852.1.20",
          "form": "나 그거 이번달 초에 접수했는데",
          "original_form": "나 그거 이번달 초에 접수했는데",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002852.1.21",
          "form": "시험장에 1분 늦어서",
          "original_form": "시험장에 1분 늦어서",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002852.1.22",
          "form": "날렸어",
          "original_form": "날렸어",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002852.1.23",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002852.1.24",
          "form": "다음에 빨리 가자",
          "original_form": "다음에 빨리 가자",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002852.1.25",
          "form": "응응 ㅜㅜ",
          "original_form": "응응 ㅜㅜ",
