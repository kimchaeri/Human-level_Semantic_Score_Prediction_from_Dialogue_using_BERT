      "utterance": [
        {
          "id": "MDRW2100004731.1.1",
          "form": "몰디브 내놔",
          "original_form": "몰디브 내놔",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100004731.1.2",
          "form": "몰디브 신혼여행지에 친구들이랑 단체예약하기",
          "original_form": "몰디브 신혼여행지에 친구들이랑 단체예약하기",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100004731.1.3",
          "form": "목표야",
          "original_form": "목표야",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100004731.1.4",
          "form": "맛있겠다... 새우랑 랍스터먹고싶다",
          "original_form": "맛있겠다... 새우랑 랍스터먹고싶다",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100004731.1.5",
          "form": "여행 좋아요 여행가고싶어요",
          "original_form": "여행 좋아요 여행가고싶어요",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100004731.1.6",
          "form": "어디로 여행가고싶나요?",
          "original_form": "어디로 여행가고싶나요?",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100004731.1.7",
          "form": "전엔 막 이집트 마추픽추 이런",
          "original_form": "전엔 막 이집트 마추픽추 이런",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100004731.1.8",
          "form": "유적지가 1순위였는데",
          "original_form": "유적지가 1순위였는데",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100004731.1.9",
          "form": "이젠 걍 휴양지 가고싶어요..",
          "original_form": "이젠 걍 휴양지 가고싶어요..",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100004731.1.10",
          "form": "너덜해진 몸과 정신",
          "original_form": "너덜해진 몸과 정신",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100004731.1.11",
          "form": "휴양지 호캉스 짱이죠",
          "original_form": "휴양지 호캉스 짱이죠",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100004731.1.12",
          "form": "돈많이벌어서 호캉스 가고싶어요..",
          "original_form": "돈많이벌어서 호캉스 가고싶어요..",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100004731.1.13",
          "form": "바람 선선하니 부는데 썬베드 누버서 생과일쥬스 홀짝대고싶어요",
          "original_form": "바람 선선하니 부는데 썬베드 누버서 생과일쥬스 홀짝대고싶어요",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100004731.1.14",
          "form": "와 호사도 그런 호사가.. 흑흑..",
          "original_form": "와 호사도 그런 호사가.. 흑흑..",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100004731.1.15",
          "form": "그렇게 누워있으면 고영갱얼햄쥐 배위로 올라와서 누버자고..",
          "original_form": "그렇게 누워있으면 고영갱얼햄쥐 배위로 올라와서 누버자고..",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100004731.1.16",
          "form": "상상만해도 행벅한 여행이네요",
          "original_form": "상상만해도 행벅한 여행이네요",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100004731.1.17",
          "form": "둥실둥실.. 나른나른",
          "original_form": "둥실둥실.. 나른나른",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100004731.1.18",
          "form": "코로나가 빨리 종식됐음 좋겠어요",
          "original_form": "코로나가 빨리 종식됐음 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100004731.1.19",
          "form": "와 맞네 돈시간 있어도 못가네",
          "original_form": "와 맞네 돈시간 있어도 못가네",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100004731.1.20",
          "form": "이게 아포칼립스지",
          "original_form": "이게 아포칼립스지",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100004731.1.21",
          "form": "아포칼립쓰~~~~",
          "original_form": "아포칼립쓰~~~~",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004731.1.22",
          "form": "그래도 돈과 시간은 있으면좋겠어요",
          "original_form": "그래도 돈과 시간은 있으면좋겠어요",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004731.1.23",
          "form": "아당연하죠 아포칼립스에 필요한건 손전등? 수통? 건전지? 빠루?",
          "original_form": "아당연하죠 아포칼립스에 필요한건 손전등? 수통? 건전지? 빠루?",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100004731.1.24",
          "form": "돈이다",
          "original_form": "돈이다",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100004731.1.25",
          "form": "하.. 호텔+접사 붙이면 다 필터링되네",
          "original_form": "하.. 호텔+접사 붙이면 다 필터링되네",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100004731.1.26",
          "form": "수십번 고쳐말하는데 한개도 안보내져",
          "original_form": "수십번 고쳐말하는데 한개도 안보내져",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100004731.1.27",
          "form": "대환장쑈쑈쑈~",
          "original_form": "대환장쑈쑈쑈~",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100004731.1.28",
          "form": "당장 리조트로 옮겨줘~~",
          "original_form": "당장 리조트로 옮겨줘~~",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100004731.1.29",
          "form": "돈이짱이죠 여행도 가려면 돈이 있어야ㅜㅠ",
          "original_form": "돈이짱이죠 여행도 가려면 돈이 있어야ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004731.1.30",
          "form": "리조트도 가고싶다...",
          "original_form": "리조트도 가고싶다...",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004731.1.31",
          "form": "누워있으면 밥나오는 곳으로 보내줘",
          "original_form": "누워있으면 밥나오는 곳으로 보내줘",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004731.1.32",
          "form": "그게 힐링이지",
          "original_form": "그게 힐링이지",
          "speaker_id": "1",
          "time": "20210821 21:13"
        }
      ]
    }
