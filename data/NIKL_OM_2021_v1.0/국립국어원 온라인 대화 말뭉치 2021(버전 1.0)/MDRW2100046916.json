      "utterance": [
        {
          "id": "MDRW2100046916.1.1",
          "form": "파이어족 준비해볼까",
          "original_form": "파이어족 준비해볼까",
          "speaker_id": "1",
          "time": "20211008 00:57"
        },
        {
          "id": "MDRW2100046916.1.2",
          "form": "40대까지만 일하면 좋겠다",
          "original_form": "40대까지만 일하면 좋겠다",
          "speaker_id": "2",
          "time": "20211008 00:58"
        },
        {
          "id": "MDRW2100046916.1.3",
          "form": "지금부터 열심히 모으면 우리도 되지 않을까",
          "original_form": "지금부터 열심히 모으면 우리도 되지 않을까",
          "speaker_id": "1",
          "time": "20211008 00:58"
        },
        {
          "id": "MDRW2100046916.1.4",
          "form": "그럼 한달에 얼마나 저축해야하지",
          "original_form": "그럼 한달에 얼마나 저축해야하지",
          "speaker_id": "2",
          "time": "20211008 00:58"
        },
        {
          "id": "MDRW2100046916.1.5",
          "form": "최대한 절약하고 10년만 고생해보자",
          "original_form": "최대한 절약하고 10년만 고생해보자",
          "speaker_id": "1",
          "time": "20211008 00:58"
        },
        {
          "id": "MDRW2100046916.1.6",
          "form": "집에서 취미로 일하고 싶네",
          "original_form": "집에서 취미로 일하고 싶네",
          "speaker_id": "2",
          "time": "20211008 00:58"
        },
        {
          "id": "MDRW2100046916.1.7",
          "form": "절약하고 부수입 만들고 해서",
          "original_form": "절약하고 부수입 만들고 해서",
          "speaker_id": "1",
          "time": "20211008 00:58"
        },
        {
          "id": "MDRW2100046916.1.8",
          "form": "10년만 일하고 평생 좀 여유로우면 할만하지",
          "original_form": "10년만 일하고 평생 좀 여유로우면 할만하지",
          "speaker_id": "2",
          "time": "20211008 00:59"
        },
        {
          "id": "MDRW2100046916.1.9",
          "form": "파이어족 이미 된 사람들보면 대단함",
          "original_form": "파이어족 이미 된 사람들보면 대단함",
          "speaker_id": "1",
          "time": "20211008 00:59"
        },
        {
          "id": "MDRW2100046916.1.10",
          "form": "저축 90프로 넘게 하던데",
          "original_form": "저축 90프로 넘게 하던데",
          "speaker_id": "2",
          "time": "20211008 00:59"
        },
        {
          "id": "MDRW2100046916.1.11",
          "form": "500은 넘게 저축해야할켄데",
          "original_form": "500은 넘게 저축해야할켄데",
          "speaker_id": "1",
          "time": "20211008 00:59"
        },
        {
          "id": "MDRW2100046916.1.12",
          "form": "그래야 2년에 1억",
          "original_form": "그래야 2년에 1억",
          "speaker_id": "2",
          "time": "20211008 00:59"
        },
        {
          "id": "MDRW2100046916.1.13",
          "form": "10년에 5억도 힘들겠자",
          "original_form": "10년에 5억도 힘들겠자",
          "speaker_id": "1",
          "time": "20211008 01:00"
        },
        {
          "id": "MDRW2100046916.1.14",
          "form": "부동산을 해야하지 않을까",
          "original_form": "부동산을 해야하지 않을까",
