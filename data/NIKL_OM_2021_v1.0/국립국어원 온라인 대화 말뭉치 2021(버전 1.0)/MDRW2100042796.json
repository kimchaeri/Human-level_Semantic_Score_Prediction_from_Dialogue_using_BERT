      "utterance": [
        {
          "id": "MDRW2100042796.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042796.1.2",
          "form": "제주 사시나 봐요",
          "original_form": "제주 사시나 봐요",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042796.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042796.1.4",
          "form": "근데 미혼이예요",
          "original_form": "근데 미혼이예요",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042796.1.5",
          "form": "우와 원래 집은 어디세요??",
          "original_form": "우와 원래 집은 어디세요??",
          "speaker_id": "1",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042796.1.6",
          "form": "창원이예여",
          "original_form": "창원이예여",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042796.1.7",
          "form": "추석전에 부모님 뵙고 이제 오후비행기로 제주가요",
          "original_form": "추석전에 부모님 뵙고 이제 오후비행기로 제주가요",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042796.1.8",
          "form": "혼자 살기 괜찮으세요??",
          "original_form": "혼자 살기 괜찮으세요??",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042796.1.9",
          "form": "제주살이 얼마동안 하시는 거예요??",
          "original_form": "제주살이 얼마동안 하시는 거예요??",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042796.1.10",
          "form": "혼자 살이 심심해요",
          "original_form": "혼자 살이 심심해요",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042796.1.11",
          "form": "내년3월이요",
          "original_form": "내년3월이요",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042796.1.12",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042796.1.13",
          "form": "어떻게 결심하셨어요??",
          "original_form": "어떻게 결심하셨어요??",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042796.1.14",
          "form": "상반기에 매달 한번씩 오다가요",
          "original_form": "상반기에 매달 한번씩 오다가요",
          "speaker_id": "2",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100042796.1.15",
          "form": "제주살이 하는 게 쉽지 않으실 텐데",
          "original_form": "제주살이 하는 게 쉽지 않으실 텐데",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042796.1.16",
          "form": "살고 계시는 게 대단하네요",
          "original_form": "살고 계시는 게 대단하네요",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042796.1.17",
          "form": "저도 아이들이랑 같이",
          "original_form": "저도 아이들이랑 같이",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042796.1.18",
          "form": "제주살이 한 달 정도 하고 싶다는 생각을 했었는데",
          "original_form": "제주살이 한 달 정도 하고 싶다는 생각을 했었는데",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042796.1.19",
          "form": "쉽지 않더라구요",
          "original_form": "쉽지 않더라구요",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042796.1.20",
          "form": "오 아이들이랑 오시면 넘 좋죠",
          "original_form": "오 아이들이랑 오시면 넘 좋죠",
          "speaker_id": "2",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042796.1.21",
          "form": "자연이 가득하자나요",
          "original_form": "자연이 가득하자나요",
          "speaker_id": "2",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042796.1.22",
          "form": "네 정말 좋을 거 같더라구요",
          "original_form": "네 정말 좋을 거 같더라구요",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042796.1.23",
          "form": "아는 언니가 제주로 이사를 갔거든요",
          "original_form": "아는 언니가 제주로 이사를 갔거든요",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042796.1.24",
          "form": "친정이 먼저 제주로 이사를 가 고",
          "original_form": "친정이 먼저 제주로 이사를 가 고",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042796.1.25",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042796.1.26",
          "form": "언니도 따라서 갔는데",
          "original_form": "언니도 따라서 갔는데",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042796.1.27",
          "form": "가족끼리 좋을 것 같아요",
          "original_form": "가족끼리 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042796.1.28",
          "form": "아이들도 여유 있어 하고 언니도 마음에 여유가 생긴 거 같다고",
          "original_form": "아이들도 여유 있어 하고 언니도 마음에 여유가 생긴 거 같다고",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042796.1.29",
          "form": "전 혼자라",
          "original_form": "전 혼자라",
          "speaker_id": "2",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042796.1.30",
          "form": "굉장히 만족하면서 살더라구요",
          "original_form": "굉장히 만족하면서 살더라구요",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042796.1.31",
          "form": "근데 여기 물가가삐싸요",
          "original_form": "근데 여기 물가가삐싸요",
          "speaker_id": "2",
          "time": "20210907 09:32"
        }
      ]
    }
