      "utterance": [
        {
          "id": "MDRW2100000463.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000463.1.2",
          "form": "지금은 물고기여",
          "original_form": "지금은 물고기여",
          "speaker_id": "2",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000463.1.3",
          "form": "동물 하나 하나 물어보시면 비극사만 애기해줄수있어요",
          "original_form": "동물 하나 하나 물어보시면 비극사만 애기해줄수있어요",
          "speaker_id": "2",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000463.1.4",
          "form": "동물에  너무 죄를 많이 지어서 더 안키우네요 ㅠ",
          "original_form": "동물에  너무 죄를 많이 지어서 더 안키우네요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000463.1.5",
          "form": "네 무슨 사연이 있으신지..?",
          "original_form": "네 무슨 사연이 있으신지..?",
          "speaker_id": "1",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000463.1.6",
          "form": "ㅣ달가족 해외여행간사이 새 4마리 사망",
          "original_form": "ㅣ달가족 해외여행간사이 새 4마리 사망",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100000463.1.7",
          "form": "햄스터 탈출 죽음",
          "original_form": "햄스터 탈출 죽음",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100000463.1.8",
          "form": "죄다 죽어서",
          "original_form": "죄다 죽어서",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100000463.1.9",
          "form": "그리고 아파트 오층이라 ㅠㅠ",
          "original_form": "그리고 아파트 오층이라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100000463.1.10",
          "form": "네 사연이 많네요ㅠ",
          "original_form": "네 사연이 많네요ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:04"
        },
        {
          "id": "MDRW2100000463.1.11",
          "form": "물고기는 무슨 간식 억나요?",
          "original_form": "물고기는 무슨 간식 억나요?",
          "speaker_id": "1",
          "time": "20210819 23:04"
        },
        {
          "id": "MDRW2100000463.1.12",
          "form": "물고기는 아버지가키우는대",
          "original_form": "물고기는 아버지가키우는대",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100000463.1.13",
          "form": "유일하게 안죽어요",
          "original_form": "유일하게 안죽어요",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100000463.1.14",
          "form": "엔젤피쉬 같은 열대어",
          "original_form": "엔젤피쉬 같은 열대어",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100000463.1.15",
          "form": "본인은 뭣 키우시나요",
          "original_form": "본인은 뭣 키우시나요",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100000463.1.16",
          "form": "물도 갈아주고 손이 많이 가겠어요\r\n저는 개키워요",
          "original_form": "물도 갈아주고 손이 많이 가겠어요\r\n저는 개키워요",
          "speaker_id": "1",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100000463.1.17",
          "form": "물고기도 이름 불러주시나요?",
          "original_form": "물고기도 이름 불러주시나요?",
          "speaker_id": "1",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100000463.1.18",
          "form": "스무고개로 종류 맞춰볼께요",
          "original_form": "스무고개로 종류 맞춰볼께요",
          "speaker_id": "2",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100000463.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100000463.1.20",
          "form": "털이  복실복실한 편이 맞나요?",
          "original_form": "털이  복실복실한 편이 맞나요?",
          "speaker_id": "2",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100000463.1.21",
          "form": "네 복실해요",
          "original_form": "네 복실해요",
          "speaker_id": "1",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100000463.1.22",
          "form": "털이 흰색인가요?",
          "original_form": "털이 흰색인가요?",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100000463.1.23",
          "form": "맞아요 하얀털",
          "original_form": "맞아요 하얀털",
          "speaker_id": "1",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100000463.1.24",
          "form": "푸들인가요?",
          "original_form": "푸들인가요?",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100000463.1.25",
          "form": "푸들은 아니에요",
          "original_form": "푸들은 아니에요",
          "speaker_id": "1",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100000463.1.26",
          "form": "시츄",
          "original_form": "시츄",
          "speaker_id": "2",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100000463.1.27",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100000463.1.28",
          "form": "시츄 아니고 그냥 믹스견 키워요 간식 좋아하고 까칠한 개님과 살아요ㅎ",
          "original_form": "시츄 아니고 그냥 믹스견 키워요 간식 좋아하고 까칠한 개님과 살아요ㅎ",
          "speaker_id": "1",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100000463.1.29",
          "form": "강쥐에 대해 궁금한 점 있으세요?",
          "original_form": "강쥐에 대해 궁금한 점 있으세요?",
          "speaker_id": "1",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100000463.1.30",
          "form": "강쥐  키우게 된사연요",
          "original_form": "강쥐  키우게 된사연요",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100000463.1.31",
          "form": "5년전에 부모님이 아는분한테 분양 받아서 지금까지 함께하고 있어요",
          "original_form": "5년전에 부모님이 아는분한테 분양 받아서 지금까지 함께하고 있어요",
          "speaker_id": "1",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100000463.1.32",
          "form": "아항 ㅎ",
          "original_form": "아항 ㅎ",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100000463.1.33",
          "form": "조금 있다 주제바꾸죵 ㅎ",
          "original_form": "조금 있다 주제바꾸죵 ㅎ",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100000463.1.34",
          "form": "물고기는 하루에 밥 몇번 먹나요?",
          "original_form": "물고기는 하루에 밥 몇번 먹나요?",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100000463.1.35",
          "form": "1번이요 저녁에요",
          "original_form": "1번이요 저녁에요",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100000463.1.36",
          "form": "강아지는 손이 많이 가죠?",
          "original_form": "강아지는 손이 많이 가죠?",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100000463.1.37",
          "form": "간식 식사 ㅎ",
          "original_form": "간식 식사 ㅎ",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100000463.1.38",
          "form": "강쥐는 아침 저녁 2번 먹어요",
          "original_form": "강쥐는 아침 저녁 2번 먹어요",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100000463.1.39",
          "form": "정말 궁금한데  강아지 지출 ㅣ달 어느 정도 드나요",
          "original_form": "정말 궁금한데  강아지 지출 ㅣ달 어느 정도 드나요",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100000463.1.40",
          "form": "미용 예방접종  포함 미포함",
          "original_form": "미용 예방접종  포함 미포함",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100000463.1.41",
          "form": "병원 한번 가면 5만원은 생각하고 가고요 \r\n간식사료 한달 7만원 정도 써요",
          "original_form": "병원 한번 가면 5만원은 생각하고 가고요 \r\n간식사료 한달 7만원 정도 써요",
          "speaker_id": "1",
