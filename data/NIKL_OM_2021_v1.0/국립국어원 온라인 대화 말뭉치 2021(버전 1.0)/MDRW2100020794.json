      "utterance": [
        {
          "id": "MDRW2100020794.1.1",
          "form": "스포츠는",
          "original_form": "스포츠는",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020794.1.2",
          "form": "별로즐기는게없지?",
          "original_form": "별로즐기는게없지?",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020794.1.3",
          "form": "엉 딱히",
          "original_form": "엉 딱히",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020794.1.4",
          "form": "이번에 여배만 좀 봤어 ㅋㅋㅋ",
          "original_form": "이번에 여배만 좀 봤어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020794.1.5",
          "form": "양궁은 시간이 안맞아서 못보고",
          "original_form": "양궁은 시간이 안맞아서 못보고",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020794.1.6",
          "form": "ㅋㅋㅋㅋㅋ평소에도",
          "original_form": "ㅋㅋㅋㅋㅋ평소에도",
          "speaker_id": "1",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020794.1.7",
          "form": "즐기지않으시는..",
          "original_form": "즐기지않으시는..",
          "speaker_id": "1",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020794.1.8",
          "form": "ㅋㅋㅋㅋㅋ 난...2002 월드컵때도 혼자 시큰둥해 있었음",
          "original_form": "ㅋㅋㅋㅋㅋ 난...2002 월드컵때도 혼자 시큰둥해 있었음",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020794.1.9",
          "form": "헐 그건좀",
          "original_form": "헐 그건좀",
          "speaker_id": "1",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100020794.1.10",
          "form": "심하지않니",
          "original_form": "심하지않니",
          "speaker_id": "1",
          "time": "20210828 13:19"
        },
        {
          "id": "MDRW2100020794.1.11",
          "form": "어릴때는 진짜 관심 0이었어",
          "original_form": "어릴때는 진짜 관심 0이었어",
          "speaker_id": "2",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100020794.1.12",
          "form": "고등학생때나 돼서 연느님 보고 첨으로 올림픽 봄",
          "original_form": "고등학생때나 돼서 연느님 보고 첨으로 올림픽 봄",
          "speaker_id": "2",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100020794.1.13",
          "form": "와 연아킴ㄱ",
          "original_form": "와 연아킴ㄱ",
          "speaker_id": "1",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100020794.1.14",
          "form": "큰일하셨네",
          "original_form": "큰일하셨네",
          "speaker_id": "1",
          "time": "20210828 13:20"
        },
        {
          "id": "MDRW2100020794.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그 뒤로도 딱히 챙겨보진 않지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그 뒤로도 딱히 챙겨보진 않지만",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020794.1.16",
          "form": "큰 시즌에만 기회 되면 보긴 함",
          "original_form": "큰 시즌에만 기회 되면 보긴 함",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020794.1.17",
          "form": "요즘도 야구 매일 봄?",
          "original_form": "요즘도 야구 매일 봄?",
          "speaker_id": "2",
          "time": "20210831 09:06"
        },
        {
          "id": "MDRW2100020794.1.18",
          "form": "ㅇㅇ 매일 봄",
          "original_form": "ㅇㅇ 매일 봄",
          "speaker_id": "1",
          "time": "20210831 09:20"
        },
        {
          "id": "MDRW2100020794.1.19",
          "form": "어제는 간만에 안해서",
          "original_form": "어제는 간만에 안해서",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100020794.1.20",
          "form": "요즘엔 어때 ㅋㅋㅋ 계속 맥주각?",
          "original_form": "요즘엔 어때 ㅋㅋㅋ 계속 맥주각?",
          "speaker_id": "2",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100020794.1.21",
          "form": "비와서 막 8연전 하루2경기",
          "original_form": "비와서 막 8연전 하루2경기",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100020794.1.22",
          "form": "있긴한데",
          "original_form": "있긴한데",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100020794.1.23",
          "form": "올림픽이후로좀",
          "original_form": "올림픽이후로좀",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100020794.1.24",
          "form": "잘해",
          "original_form": "잘해",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100020794.1.25",
          "form": "어제쉬었으니 이제 잘하겠지",
          "original_form": "어제쉬었으니 이제 잘하겠지",
          "speaker_id": "1",
          "time": "20210831 09:21"
        },
        {
          "id": "MDRW2100020794.1.26",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100020794.1.27",
          "form": "그래도 좀 삶의 즐거움이 되겠네 ㅋㅋㅋ",
          "original_form": "그래도 좀 삶의 즐거움이 되겠네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100020794.1.28",
          "form": "그맛에보는데",
          "original_form": "그맛에보는데",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100020794.1.29",
          "form": "욕할때가더많지",
          "original_form": "욕할때가더많지",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
