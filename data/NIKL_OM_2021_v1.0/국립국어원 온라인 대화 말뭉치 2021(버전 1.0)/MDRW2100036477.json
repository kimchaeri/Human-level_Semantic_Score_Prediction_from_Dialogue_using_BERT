      "utterance": [
        {
          "id": "MDRW2100036477.1.1",
          "form": "가장 기억에 남는 여행지가 있나요?",
          "original_form": "가장 기억에 남는 여행지가 있나요?",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036477.1.2",
          "form": "저는 춘천이요",
          "original_form": "저는 춘천이요",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036477.1.3",
          "form": "name1님은 있으신가요?",
          "original_form": "&name1&님은 있으신가요?",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100036477.1.4",
          "form": "왜 기억에 남는지 궁금해요!",
          "original_form": "왜 기억에 남는지 궁금해요!",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100036477.1.5",
          "form": "거기서 레일바이크를 탔는데 너무 재미있고 닭갈비도 맛있어서요",
          "original_form": "거기서 레일바이크를 탔는데 너무 재미있고 닭갈비도 맛있어서요",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100036477.1.6",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100036477.1.7",
          "form": "저는 군산이여",
          "original_form": "저는 군산이여",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100036477.1.8",
          "form": "동네가 바다도 있고 아기자기해서 걸어다녀도 웬만한거 다 볼수있더라구요",
          "original_form": "동네가 바다도 있고 아기자기해서 걸어다녀도 웬만한거 다 볼수있더라구요",
          "speaker_id": "1",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100036477.1.9",
          "form": "그렇군요 그럼 군산에 먹을 거 추천받을 수 있을까요?",
          "original_form": "그렇군요 그럼 군산에 먹을 거 추천받을 수 있을까요?",
          "speaker_id": "2",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100036477.1.10",
          "form": "이성당이요 ㅎㅎ",
          "original_form": "이성당이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036477.1.11",
          "form": "체인이라 여기저기 볼수있지만 ㅎ",
          "original_form": "체인이라 여기저기 볼수있지만 ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036477.1.12",
          "form": "어떤 음식을 파는 곳인가요??",
          "original_form": "어떤 음식을 파는 곳인가요??",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036477.1.13",
          "form": "이성당 모르세오???",
          "original_form": "이성당 모르세오???",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036477.1.14",
          "form": "빵집이요!",
          "original_form": "빵집이요!",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036477.1.15",
          "form": "지금은 체인이라 여러곳에 있는데 거기가 본점이에요 ㅎㅎ",
          "original_form": "지금은 체인이라 여러곳에 있는데 거기가 본점이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036477.1.16",
          "form": "그렇군요 처음 들어봐요 어떤 빵이 제일 맛있나요?",
          "original_form": "그렇군요 처음 들어봐요 어떤 빵이 제일 맛있나요?",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036477.1.17",
          "form": "아~ 거기 다 맛있어요! 아마 메인은 단팥빵일거에요",
          "original_form": "아~ 거기 다 맛있어요! 아마 메인은 단팥빵일거에요",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036477.1.18",
          "form": "저는 근처에 체인이 있어서 자주 가거든요 저는 거기 슈크림빵 좋아해요!!",
          "original_form": "저는 근처에 체인이 있어서 자주 가거든요 저는 거기 슈크림빵 좋아해요!!",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036477.1.19",
          "form": "찾아보시면 아마 근처에 있을수도 있어요",
          "original_form": "찾아보시면 아마 근처에 있을수도 있어요",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036477.1.20",
          "form": "거기빵은 전체적으로 단맛이 강한가요??",
          "original_form": "거기빵은 전체적으로 단맛이 강한가요??",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036477.1.21",
          "form": "종류에따라 단빵은 달고 아닌건 아니에요 ㅎㅎ",
          "original_form": "종류에따라 단빵은 달고 아닌건 아니에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036477.1.22",
          "form": "막 고로케가 달고 그러진않아요!",
          "original_form": "막 고로케가 달고 그러진않아요!",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036477.1.23",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210830 23:54"
        }
      ]
    }
