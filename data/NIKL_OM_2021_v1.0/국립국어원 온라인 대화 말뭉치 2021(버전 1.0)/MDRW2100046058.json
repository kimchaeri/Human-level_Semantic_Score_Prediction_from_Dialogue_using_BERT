        {
          "id": "MDRW2100046058.1.1",
          "form": "내가 보는 유튜브는 요리하는 유튜브야",
          "original_form": "내가 보는 유튜브는 요리하는 유튜브야",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.2",
          "form": "영상으로 요리를 배우면 좋지",
          "original_form": "영상으로 요리를 배우면 좋지",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.3",
          "form": "그 분은 정말 친절해",
          "original_form": "그 분은 정말 친절해",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.4",
          "form": "레시피를 세심하게 알려주니",
          "original_form": "레시피를 세심하게 알려주니",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.5",
          "form": "맞아 엄청 꼼꼼하게 알려줘",
          "original_form": "맞아 엄청 꼼꼼하게 알려줘",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.6",
          "form": "요리를 따라 하겠다",
          "original_form": "요리를 따라 하겠다",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.7",
          "form": "벌써 한열 부한 정도",
          "original_form": "벌써 한열 부한 정도",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.8",
          "form": "많이 따라했네",
          "original_form": "많이 따라했네",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.9",
          "form": "워낙 쉬워서 따라 하기가 좋아",
          "original_form": "워낙 쉬워서 따라 하기가 좋아",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.10",
          "form": "맛은 어때서",
          "original_form": "맛은 어때서",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.11",
          "form": "엄청 맛있던데",
          "original_form": "엄청 맛있던데",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.12",
          "form": "나도 링크 보내줘",
          "original_form": "나도 링크 보내줘",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046058.1.13",
          "form": "해보고 호기 알려줘",
          "original_form": "해보고 호기 알려줘",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046058.1.14",
          "form": "꼭 그럴게",
