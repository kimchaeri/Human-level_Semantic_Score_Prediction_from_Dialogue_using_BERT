      "utterance": [
        {
          "id": "MDRW2100045230.1.1",
          "form": "스카이림 해보셨어요??",
          "original_form": "스카이림 해보셨어요??",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045230.1.2",
          "form": "네 개꿀잼",
          "original_form": "네 개꿀잼",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045230.1.3",
          "form": "완젼 좋아요",
          "original_form": "완젼 좋아요",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045230.1.4",
          "form": "주종족 뭐세요?",
          "original_form": "주종족 뭐세요?",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045230.1.5",
          "form": "전 던머",
          "original_form": "전 던머",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045230.1.6",
          "form": "완전 핫하죠",
          "original_form": "완전 핫하죠",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045230.1.7",
          "form": "저 종족은 모르겠네요",
          "original_form": "저 종족은 모르겠네요",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045230.1.8",
          "form": "?? 뭐가 핫한데요",
          "original_form": "?? 뭐가 핫한데요",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045230.1.9",
          "form": "얼굴...",
          "original_form": "얼굴...",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045230.1.10",
          "form": "그들의 망한 인성..",
          "original_form": "그들의 망한 인성..",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045230.1.11",
          "form": "사실 불마법을쓰니까요",
          "original_form": "사실 불마법을쓰니까요",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045230.1.12",
          "form": "그쵸... 절 사지로 내몰더라구오...",
          "original_form": "그쵸... 절 사지로 내몰더라구오...",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045230.1.13",
          "form": "적어도 말이라도 해주지",
          "original_form": "적어도 말이라도 해주지",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045230.1.14",
          "form": "스카이림 아는거 맞아요? 허 참..",
          "original_form": "스카이림 아는거 맞아요? 허 참..",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045230.1.15",
          "form": "불마법을 쓰는데 핫하죠 쿨하겠나요",
          "original_form": "불마법을 쓰는데 핫하죠 쿨하겠나요",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045230.1.16",
          "form": "저는 불이랑 얼음 마법 다 써서",
          "original_form": "저는 불이랑 얼음 마법 다 써서",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045230.1.17",
          "form": "핫하고 쿨한 마법사입니다",
          "original_form": "핫하고 쿨한 마법사입니다",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045230.1.18",
          "form": "던머들은 화산지대에서 사는 불마법사라서",
          "original_form": "던머들은 화산지대에서 사는 불마법사라서",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045230.1.19",
          "form": "화염저항도 있어요",
          "original_form": "화염저항도 있어요",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045230.1.20",
          "form": "노드는 추운데 살아서 냉기저항",
          "original_form": "노드는 추운데 살아서 냉기저항",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045230.1.21",
          "form": "종족은 아직 잘 모르겠네요",
          "original_form": "종족은 아직 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045230.1.22",
          "form": "그냥 하는 중인데",
          "original_form": "그냥 하는 중인데",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045230.1.23",
          "form": "오늘 집 구매했어요 !",
          "original_form": "오늘 집 구매했어요 !",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045230.1.24",
          "form": "어디에요?",
          "original_form": "어디에요?",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045230.1.25",
          "form": "화이트런에요!",
          "original_form": "화이트런에요!",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045230.1.26",
          "form": "짐 정리하는데",
          "original_form": "짐 정리하는데",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045230.1.27",
          "form": "제가 은근 쟁여둔게 있어서",
          "original_form": "제가 은근 쟁여둔게 있어서",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045230.1.28",
          "form": "한참 걸렸어요",
          "original_form": "한참 걸렸어요",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045230.1.29",
          "form": "전 다락에 한번에 넣어둬요",
          "original_form": "전 다락에 한번에 넣어둬요",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045230.1.30",
          "form": "어짜피 안찾아가서",
          "original_form": "어짜피 안찾아가서",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045230.1.31",
          "form": "저도 그래서 좀 많이 넣어뒀어요",
          "original_form": "저도 그래서 좀 많이 넣어뒀어요",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045230.1.32",
          "form": "ㅋㅋㅋ 팔긴 아깝고",
          "original_form": "ㅋㅋㅋ 팔긴 아깝고",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045230.1.33",
          "form": "들고다니긴 귀찮아요",
          "original_form": "들고다니긴 귀찮아요",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045230.1.34",
          "form": "아니 제가 얻을 때는 150이었던걸",
          "original_form": "아니 제가 얻을 때는 150이었던걸",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045230.1.35",
          "form": "애들이 50골에 사줘사",
          "original_form": "애들이 50골에 사줘사",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045230.1.36",
          "form": "너무 억울해요",
          "original_form": "너무 억울해요",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045230.1.37",
          "form": "제가 호갱같고",
          "original_form": "제가 호갱같고",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045230.1.38",
          "form": "흥정높여요",
          "original_form": "흥정높여요",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045230.1.39",
          "form": "화술이랑",
          "original_form": "화술이랑",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045230.1.40",
          "form": "퍽찍고 이성이랑 거래해요",
          "original_form": "퍽찍고 이성이랑 거래해요",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045230.1.41",
          "form": "아직.. 11렙 인걸요...",
          "original_form": "아직.. 11렙 인걸요...",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045230.1.42",
          "form": "렙업 빨리하는 방법 있나요?",
          "original_form": "렙업 빨리하는 방법 있나요?",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045230.1.43",
          "form": "뭐든 하고 퀘스트 많이하기?",
          "original_form": "뭐든 하고 퀘스트 많이하기?",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045230.1.44",
          "form": "은신 도둑질",
          "original_form": "은신 도둑질",
          "speaker_id": "1",
          "time": "20210908 01:42"
        }
      ]
    }
  ]
