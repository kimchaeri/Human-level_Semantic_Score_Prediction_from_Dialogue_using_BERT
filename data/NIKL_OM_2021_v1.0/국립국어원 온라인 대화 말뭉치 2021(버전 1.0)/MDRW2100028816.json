        },
        {
          "id": "MDRW2100028816.1.2",
          "form": "집안일 가족이랑 분배해서 잘하고 있니?",
          "original_form": "집안일 가족이랑 분배해서 잘하고 있니?",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028816.1.3",
          "form": "갑자기아프다",
          "original_form": "갑자기아프다",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028816.1.4",
          "form": "아닌거같음...ㅋㅋㅋㅋ",
          "original_form": "아닌거같음...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028816.1.5",
          "form": "좀해야하는데...",
          "original_form": "좀해야하는데...",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028816.1.6",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028816.1.7",
          "form": "난 집에 혼자있는 시간이 내가 젤 많아서인지",
          "original_form": "난 집에 혼자있는 시간이 내가 젤 많아서인지",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028816.1.8",
          "form": "설거지 거의 내 담당",
          "original_form": "설거지 거의 내 담당",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028816.1.9",
          "form": "사실 집안일 다같이 하면 좋은데... 하는 사람이 하게 된다는",
          "original_form": "사실 집안일 다같이 하면 좋은데... 하는 사람이 하게 된다는",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028816.1.10",
          "form": "글쿠만.. 약간 다들 하기싫어서 슬쩍슬쩍 미루니까",
          "original_form": "글쿠만.. 약간 다들 하기싫어서 슬쩍슬쩍 미루니까",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100028816.1.11",
          "form": "젤 못견딘 사람이 가서 하게되는건가 싶기두 혀",
          "original_form": "젤 못견딘 사람이 가서 하게되는건가 싶기두 혀",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100028816.1.12",
          "form": "맞앜ㅋㅋㄲ 쌓여있는거 못참는 사람이",
          "original_form": "맞앜ㅋㅋㄲ 쌓여있는거 못참는 사람이",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100028816.1.13",
          "form": "집안일하더라..그게 나야",
          "original_form": "집안일하더라..그게 나야",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100028816.1.14",
          "form": "짜증내면서 하다가 설거지산 없애면",
          "original_form": "짜증내면서 하다가 설거지산 없애면",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100028816.1.15",
          "form": "뿌듯해",
          "original_form": "뿌듯해",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100028816.1.16",
          "form": "ㅋㅋㅋㅋㅋ멋지고 슬프다...",
          "original_form": "ㅋㅋㅋㅋㅋ멋지고 슬프다...",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100028816.1.17",
          "form": "나도 자주해야하는데말이여",
          "original_form": "나도 자주해야하는데말이여",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100028816.1.18",
          "form": "혼자 살면 또 내 일이되겟지만...",
          "original_form": "혼자 살면 또 내 일이되겟지만...",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100028816.1.19",
          "form": "하다보면 설거지 스킬늡니다(??)",
          "original_form": "하다보면 설거지 스킬늡니다(??)",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
