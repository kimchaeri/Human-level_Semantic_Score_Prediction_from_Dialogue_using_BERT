      "utterance": [
        {
          "id": "MDRW2100015041.1.1",
          "form": "물론이죠",
          "original_form": "물론이죠",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.2",
          "form": "명랑 최애 뭐세요",
          "original_form": "명랑 최애 뭐세요",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.3",
          "form": "당연히 감자핫도그",
          "original_form": "당연히 감자핫도그",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.4",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.5",
          "form": "전 로제떡볶이요(?)",
          "original_form": "전 로제떡볶이요(?)",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.6",
          "form": "전 통모짜",
          "original_form": "전 통모짜",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.7",
          "form": "생각도 못한 메뉴 ㄴㅇㄱ",
          "original_form": "생각도 못한 메뉴 ㄴㅇㄱ",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.8",
          "form": "근데 명랑 로제 드셔보셖어요?",
          "original_form": "근데 명랑 로제 드셔보셖어요?",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.9",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.10",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.11",
          "form": "여친 있으시면 꼭 같이 드시기",
          "original_form": "여친 있으시면 꼭 같이 드시기",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015041.1.12",
          "form": "천국의맛.",
          "original_form": "천국의맛.",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.13",
          "form": "없으면 먹지도말라,",
          "original_form": "없으면 먹지도말라,",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.14",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.15",
          "form": "누나랑 먹기는 좀 그르니까요",
          "original_form": "누나랑 먹기는 좀 그르니까요",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.16",
          "form": "없으면 먹으면 안된다?",
          "original_form": "없으면 먹으면 안된다?",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.17",
          "form": "그럼 어머니랑 드시는 걸로",
          "original_form": "그럼 어머니랑 드시는 걸로",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.18",
          "form": "죄삼다",
          "original_form": "죄삼다",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.19",
          "form": "혼자는 먹지도 못하게 하다니",
          "original_form": "혼자는 먹지도 못하게 하다니",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.20",
          "form": "너무해",
          "original_form": "너무해",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.21",
          "form": "그게 2인분으로 팔아요",
          "original_form": "그게 2인분으로 팔아요",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015041.1.22",
          "form": "핫도그 3개에 2인분 떡볶이만 파는데",
          "original_form": "핫도그 3개에 2인분 떡볶이만 파는데",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015041.1.23",
          "form": "핫도그집 주제에 로제 맛있게합니다.",
          "original_form": "핫도그집 주제에 로제 맛있게합니다.",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015041.1.24",
          "form": "신전 로제보다 맛있어요",
          "original_form": "신전 로제보다 맛있어요",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015041.1.25",
          "form": "와 배떡보다는 어때요",
          "original_form": "와 배떡보다는 어때요",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015041.1.26",
          "form": "배떡은 좀 느끼하지 않으셨나요?!",
          "original_form": "배떡은 좀 느끼하지 않으셨나요?!",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015041.1.27",
          "form": "그런가 난 괜찮던데",
          "original_form": "그런가 난 괜찮던데",
          "speaker_id": "1",
          "time": "20210825 21:38"
        }
      ]
