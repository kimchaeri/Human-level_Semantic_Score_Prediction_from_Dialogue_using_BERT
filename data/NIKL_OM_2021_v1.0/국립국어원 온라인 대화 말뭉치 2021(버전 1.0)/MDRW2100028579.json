      "utterance": [
        {
          "id": "MDRW2100028579.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028579.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028579.1.3",
          "form": "지금 뭐하고 계세요? 저는 씻고 누웠어요ㅎㅎ",
          "original_form": "지금 뭐하고 계세요? 저는 씻고 누웠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028579.1.4",
          "form": "저 양치해야 되는데 계속 누워있네요 ㅠㅠ",
          "original_form": "저 양치해야 되는데 계속 누워있네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028579.1.5",
          "form": "사실 저도 머리카락 때문에 청소기 돌려야하는데...",
          "original_form": "사실 저도 머리카락 때문에 청소기 돌려야하는데...",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028579.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028579.1.7",
          "form": "너무 귀찮아요 ㅠㅠ",
          "original_form": "너무 귀찮아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028579.1.8",
          "form": "로봇청소기 하나 들일까봐요 정말...",
          "original_form": "로봇청소기 하나 들일까봐요 정말...",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028579.1.9",
          "form": "그르니까요...",
          "original_form": "그르니까요...",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028579.1.10",
          "form": "저는 힘내서 양치하고 왔습니당 ㅎㅎ",
          "original_form": "저는 힘내서 양치하고 왔습니당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028579.1.11",
          "form": "저는 내일 입을 옷 꺼내놓고 옷장 정리 좀 했어요 ㅎㅎ",
          "original_form": "저는 내일 입을 옷 꺼내놓고 옷장 정리 좀 했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028579.1.12",
          "form": "오 ㅎㅎ 부지런하신데요",
          "original_form": "오 ㅎㅎ 부지런하신데요",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028579.1.13",
          "form": "저는 집안일 중에서 설거지하는 게 제일 좋더라구요",
          "original_form": "저는 집안일 중에서 설거지하는 게 제일 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028579.1.14",
          "form": "직접 요리도 해서 드시나봐요!",
          "original_form": "직접 요리도 해서 드시나봐요!",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028579.1.15",
          "form": "아뇨 요리는 어머니가 하시고 저는 설거지만 ㅎㅎ...",
          "original_form": "아뇨 요리는 어머니가 하시고 저는 설거지만 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028579.1.16",
          "form": "요리 잘하세요??",
          "original_form": "요리 잘하세요??",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028579.1.17",
          "form": "아뇨 저도... 최대가 김치볶음밥이에요 ㅋㅋㅋ",
          "original_form": "아뇨 저도... 최대가 김치볶음밥이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028579.1.18",
          "form": "김볶이면 잘하시는 거 아닌가요 ㅎㅎ",
          "original_form": "김볶이면 잘하시는 거 아닌가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028579.1.19",
          "form": "전 라면만 끓여요...",
