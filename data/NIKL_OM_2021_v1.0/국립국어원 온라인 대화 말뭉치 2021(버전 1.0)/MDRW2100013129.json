      "utterance": [
        {
          "id": "MDRW2100013129.1.1",
          "form": "넷플 보시나요",
          "original_form": "넷플 보시나요",
          "speaker_id": "1",
          "time": "20210825 12:49"
        },
        {
          "id": "MDRW2100013129.1.2",
          "form": "넷플 예전에 보다가 지금은 안봐요ㅎ",
          "original_form": "넷플 예전에 보다가 지금은 안봐요ㅎ",
          "speaker_id": "2",
          "time": "20210825 12:53"
        },
        {
          "id": "MDRW2100013129.1.3",
          "form": "넷플 보세요?",
          "original_form": "넷플 보세요?",
          "speaker_id": "2",
          "time": "20210825 12:53"
        },
        {
          "id": "MDRW2100013129.1.4",
          "form": "네 봐요ㅎㅎ",
          "original_form": "네 봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100013129.1.5",
          "form": "주로 뭐보시나요",
          "original_form": "주로 뭐보시나요",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100013129.1.6",
          "form": "킹덤이요!",
          "original_form": "킹덤이요!",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100013129.1.7",
          "form": "킹덤 시리즈별로 다 보셨어요?",
          "original_form": "킹덤 시리즈별로 다 보셨어요?",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100013129.1.8",
          "form": "네 다봤습니다ㅎㅎ",
          "original_form": "네 다봤습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100013129.1.9",
          "form": "시리즈 다 재밌어요? 저는 한 번도 안봐가지구...",
          "original_form": "시리즈 다 재밌어요? 저는 한 번도 안봐가지구...",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100013129.1.10",
          "form": "네네 재밌어요!",
          "original_form": "네네 재밌어요!",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100013129.1.11",
          "form": "좀비물을 평소에 무서워서 안보는데  많이 무섭진않나요ㄷㄷ",
          "original_form": "좀비물을 평소에 무서워서 안보는데  많이 무섭진않나요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100013129.1.12",
          "form": "조금 잔인하긴해요",
          "original_form": "조금 잔인하긴해요",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100013129.1.13",
          "form": "ㅜㅠ 저는 밤에는 못보겠네요ㅜ",
          "original_form": "ㅜㅠ 저는 밤에는 못보겠네요ㅜ",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100013129.1.14",
          "form": "밤에 불끄고봐야 재밌는데ㅠ",
          "original_form": "밤에 불끄고봐야 재밌는데ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:51"
        }
      ]
