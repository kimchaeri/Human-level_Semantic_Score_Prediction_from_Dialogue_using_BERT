      "utterance": [
        {
          "id": "MDRW2100007357.1.1",
          "form": "공연 다니는 거 좋아하시나요",
          "original_form": "공연 다니는 거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007357.1.2",
          "form": "아이돌 본진으로 할때 콘서트 잘 다녔어요",
          "original_form": "아이돌 본진으로 할때 콘서트 잘 다녔어요",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007357.1.3",
          "form": "오페라? 도 봤고 뮤지컬도 봤고",
          "original_form": "오페라? 도 봤고 뮤지컬도 봤고",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007357.1.4",
          "form": "공연 너무 좋아해요",
          "original_form": "공연 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007357.1.5",
          "form": "오 저도 공연 다니는 거 좋아해요",
          "original_form": "오 저도 공연 다니는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007357.1.6",
          "form": "연극 뮤지컬 콘서트 다요",
          "original_form": "연극 뮤지컬 콘서트 다요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007357.1.7",
          "form": "근데 뮤지컬은 비싸서 잘 못갔어요",
          "original_form": "근데 뮤지컬은 비싸서 잘 못갔어요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007357.1.8",
          "form": "저도 한두번밖에 못 가봤어용",
          "original_form": "저도 한두번밖에 못 가봤어용",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007357.1.9",
          "form": "소극장 연극도 재밌게 봤어요!",
          "original_form": "소극장 연극도 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007357.1.10",
          "form": "맞아요 소극장 연극은 진짜 생생해서 재밌죠",
          "original_form": "맞아요 소극장 연극은 진짜 생생해서 재밌죠",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007357.1.11",
          "form": "가수 콘서트 누구누구 가보셨어요?",
          "original_form": "가수 콘서트 누구누구 가보셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007357.1.12",
          "form": "전 성시경 샤이니 방탄 가봤어요",
          "original_form": "전 성시경 샤이니 방탄 가봤어요",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007357.1.13",
          "form": "저는 김필이랑 뉴이스트 가봤어요",
          "original_form": "저는 김필이랑 뉴이스트 가봤어요",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007357.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007357.1.15",
          "form": "그 가기힘들다는 방탄콘서트를 가셨어요?!",
          "original_form": "그 가기힘들다는 방탄콘서트를 가셨어요?!",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007357.1.16",
          "form": "티켓팅 천재시다",
          "original_form": "티켓팅 천재시다",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007357.1.17",
          "form": "신기하게 누가 부탁해서 티켓팅했는데",
          "original_form": "신기하게 누가 부탁해서 티켓팅했는데",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007357.1.18",
          "form": "새로산 노트북이아그런지 바로 되더라구요",
          "original_form": "새로산 노트북이아그런지 바로 되더라구요",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007357.1.19",
          "form": "아이돌 콘서트는 다 티켓팅 힘들죠",
          "original_form": "아이돌 콘서트는 다 티켓팅 힘들죠",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007357.1.20",
          "form": "티켓팅 천재신데요..",
          "original_form": "티켓팅 천재신데요..",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007357.1.21",
          "form": "나중에 드림콘서트나 시상식 무대도 가보고싶어요",
          "original_form": "나중에 드림콘서트나 시상식 무대도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007357.1.22",
          "form": "아이돌 두루 좋아해서",
          "original_form": "아이돌 두루 좋아해서",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007357.1.23",
          "form": "오 두루두루 좋아하시는구나",
          "original_form": "오 두루두루 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007357.1.24",
          "form": "저는 하나 꽂히면 옆에 잘 못봐서..",
          "original_form": "저는 하나 꽂히면 옆에 잘 못봐서..",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007357.1.25",
          "form": "이제 빠져나오니까 관계도? 같은게 보여요",
          "original_form": "이제 빠져나오니까 관계도? 같은게 보여요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007357.1.26",
          "form": "예전에는 그냥 우리애 옆에 누가 있구나였는데",
          "original_form": "예전에는 그냥 우리애 옆에 누가 있구나였는데",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007357.1.27",
          "form": "이제 그 누구가 인식되는...",
          "original_form": "이제 그 누구가 인식되는...",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007357.1.28",
          "form": "아 맞아요ㅋㅋㅋ 저도 원래 옛날엔 한 가수만 좋아하고",
          "original_form": "아 맞아요ㅋㅋㅋ 저도 원래 옛날엔 한 가수만 좋아하고",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007357.1.29",
          "form": "나머지는 다 경쟁자였는데",
          "original_form": "나머지는 다 경쟁자였는데",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007357.1.30",
          "form": "프로듀스101 같은 거 보면 다들 열심히 살길래 다 응원하기로 했어요",
          "original_form": "프로듀스101 같은 거 보면 다들 열심히 살길래 다 응원하기로 했어요",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007357.1.31",
          "form": "아 맞아요ㅠㅋㅋㅋㅋㅋ 다들 완전 열심히 해요",
          "original_form": "아 맞아요ㅠㅋㅋㅋㅋㅋ 다들 완전 열심히 해요",
