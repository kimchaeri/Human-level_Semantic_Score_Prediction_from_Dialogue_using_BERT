      "utterance": [
        {
          "id": "MDRW2100016163.1.1",
          "form": "뮤지컬 좋아하세요?",
          "original_form": "뮤지컬 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100016163.1.2",
          "form": "네 요새 관심 갖고있어요 ㅠㅠ",
          "original_form": "네 요새 관심 갖고있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:27"
        },
        {
          "id": "MDRW2100016163.1.3",
          "form": "오 뭐보셨나요",
          "original_form": "오 뭐보셨나요",
          "speaker_id": "1",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100016163.1.4",
          "form": "최근에 관심 생긴거라",
          "original_form": "최근에 관심 생긴거라",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016163.1.5",
          "form": "본 거는 없어요",
          "original_form": "본 거는 없어요",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016163.1.6",
          "form": "저도 아직 한번도못봐서",
          "original_form": "저도 아직 한번도못봐서",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016163.1.7",
          "form": "ㅋㅋㅋㅋ 뭐ㅜ보고 싶으세요?",
          "original_form": "ㅋㅋㅋㅋ 뭐ㅜ보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016163.1.8",
          "form": "그날들이요!",
          "original_form": "그날들이요!",
          "speaker_id": "1",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016163.1.9",
          "form": "그 군대에서 하는 거 맞나요?",
          "original_form": "그 군대에서 하는 거 맞나요?",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016163.1.10",
          "form": "잘 몰라서!",
          "original_form": "잘 몰라서!",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016163.1.11",
          "form": "김광석 노래라네요",
          "original_form": "김광석 노래라네요",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016163.1.12",
          "form": "아ㅋㅋㅋㅋㅋㅋ 뮤지컬인줄 알았어요!",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ 뮤지컬인줄 알았어요!",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016163.1.13",
          "form": "아 김광석 노래 바탕으로 뮤지컬이요ㅎㅎ",
          "original_form": "아 김광석 노래 바탕으로 뮤지컬이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016163.1.14",
          "form": "아 그쵸ㅋㅋㅋㅋ",
          "original_form": "아 그쵸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016163.1.15",
          "form": "제가 아는 거 같네여",
          "original_form": "제가 아는 거 같네여",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016163.1.16",
