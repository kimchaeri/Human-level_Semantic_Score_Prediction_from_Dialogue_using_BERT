      "utterance": [
        {
          "id": "MDRW2100020299.1.1",
          "form": "최근 구입하신거있으세요?",
          "original_form": "최근 구입하신거있으세요?",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020299.1.2",
          "form": "로션샀어요!",
          "original_form": "로션샀어요!",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020299.1.3",
          "form": "일리윤 아토로션",
          "original_form": "일리윤 아토로션",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020299.1.4",
          "form": "오 그거 좋나요??",
          "original_form": "오 그거 좋나요??",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020299.1.5",
          "form": "원래 세타필을 썼는데",
          "original_form": "원래 세타필을 썼는데",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020299.1.6",
          "form": "한번갈아타봤는데, 그냥,, 무난한거같아요,,",
          "original_form": "한번갈아타봤는데, 그냥,, 무난한거같아요,,",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020299.1.7",
          "form": "name1은 최근에 뭐 구매하셨나요 ?",
          "original_form": "&name1&은 최근에 뭐 구매하셨나요 ?",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020299.1.8",
          "form": "음 저는 트렌치코트 샀어요",
          "original_form": "음 저는 트렌치코트 샀어요",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020299.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020299.1.10",
          "form": "가을이니까~",
          "original_form": "가을이니까~",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020299.1.11",
          "form": "무슨색사셨어요 ?",
          "original_form": "무슨색사셨어요 ?",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020299.1.12",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020299.1.13",
          "form": "저는 정석인 택배박스색을 샀답니다",
          "original_form": "저는 정석인 택배박스색을 샀답니다",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020299.1.14",
          "form": "아 당연한선택",
          "original_form": "아 당연한선택",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020299.1.15",
          "form": "ㅋㅋㅋㅋㅋ 정석인게 갖고싶었어요",
          "original_form": "ㅋㅋㅋㅋㅋ 정석인게 갖고싶었어요",
          "speaker_id": "1",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020299.1.16",
          "form": "중고거래인 당근은 하시나요 ??",
          "original_form": "중고거래인 당근은 하시나요 ??",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020299.1.17",
          "form": "네 가끔해요!",
          "original_form": "네 가끔해요!",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020299.1.18",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "1",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020299.1.19",
          "form": "네 ! 판매만!",
          "original_form": "네 ! 판매만!",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020299.1.20",
          "form": "뭐파셨어요??",
          "original_form": "뭐파셨어요??",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020299.1.21",
          "form": "치마 팔았어요 !",
          "original_form": "치마 팔았어요 !",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020299.1.22",
          "form": "전에 사놨던 ,,",
          "original_form": "전에 사놨던 ,,",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020299.1.23",
          "form": "한번도 안입은거라서 아까워서 팔았어요 ㅋㅋㅋ",
          "original_form": "한번도 안입은거라서 아까워서 팔았어요 ㅋㅋㅋ",
