        },
        {
          "id": "MDRW2100008593.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008593.1.3",
          "form": "아 유부남이시군요",
          "original_form": "아 유부남이시군요",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008593.1.4",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008593.1.5",
          "form": "애가 둘이에요",
          "original_form": "애가 둘이에요",
          "speaker_id": "2",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008593.1.6",
          "form": "아하 네..",
          "original_form": "아하 네..",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008593.1.7",
          "form": "전 연애준비중이에요",
          "original_form": "전 연애준비중이에요",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008593.1.8",
          "form": "네 그러세요",
          "original_form": "네 그러세요",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008593.1.9",
          "form": "소개팅 해보세요",
          "original_form": "소개팅 해보세요",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008593.1.10",
          "form": "자만추입니다",
          "original_form": "자만추입니다",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008593.1.11",
          "form": "저도요 자연스러운게 좋아요",
          "original_form": "저도요 자연스러운게 좋아요",
          "speaker_id": "2",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008593.1.12",
          "form": "자만추는 아무나 하는게 아니에요",
          "original_form": "자만추는 아무나 하는게 아니에요",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008593.1.13",
          "form": "놀이동산 데이트 하고 싶어요",
          "original_form": "놀이동산 데이트 하고 싶어요",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008593.1.14",
          "form": "유부남이시라면서요",
          "original_form": "유부남이시라면서요",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008593.1.15",
          "form": "이상형은 한지민입니다",
          "original_form": "이상형은 한지민입니다",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008593.1.16",
          "form": "아 네. 저는 딱히...",
          "original_form": "아 네. 저는 딱히...",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008593.1.17",
          "form": "그냥 다 좋아요",
          "original_form": "그냥 다 좋아요",
          "speaker_id": "1",
          "time": "20210822 17:38"
        },
        {
