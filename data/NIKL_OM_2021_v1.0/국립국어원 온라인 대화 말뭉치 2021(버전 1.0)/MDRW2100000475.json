      "utterance": [
        {
          "id": "MDRW2100000475.1.1",
          "form": "여행 안갔어요",
          "original_form": "여행 안갔어요",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000475.1.2",
          "form": "ㅠㅠ 가고싶은곳없나요",
          "original_form": "ㅠㅠ 가고싶은곳없나요",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000475.1.3",
          "form": "혹시 ㅣ억이 있다면요?",
          "original_form": "혹시 ㅣ억이 있다면요?",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000475.1.4",
          "form": "저는 숲 좋아해요",
          "original_form": "저는 숲 좋아해요",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000475.1.5",
          "form": "여행 많이 다니셨어요?",
          "original_form": "여행 많이 다니셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000475.1.6",
          "form": "코스프레하고 금속탐지기 하니 산과 공원 등 많이다니죠 ㅎ",
          "original_form": "코스프레하고 금속탐지기 하니 산과 공원 등 많이다니죠 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000475.1.7",
          "form": "제일 최근 다녀온곳이 광명동굴과 수원  광교산이네요",
          "original_form": "제일 최근 다녀온곳이 광명동굴과 수원  광교산이네요",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000475.1.8",
          "form": "동굴도 가셨어요? 동굴 시원하고 좋나요?",
          "original_form": "동굴도 가셨어요? 동굴 시원하고 좋나요?",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000475.1.9",
          "form": "네 ㅎ 집이 서울대입구 역   근처라 가까워서 좋았네요",
          "original_form": "네 ㅎ 집이 서울대입구 역   근처라 가까워서 좋았네요",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000475.1.10",
          "form": "제가  장애증을 보여줬더니 입장료가 무료라 더 좋았고여 ㅎ",
          "original_form": "제가  장애증을 보여줬더니 입장료가 무료라 더 좋았고여 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000475.1.11",
          "form": "그렇군요 저는 작년에 여수에 가족들하고 다녀온게 생각나네요",
          "original_form": "그렇군요 저는 작년에 여수에 가족들하고 다녀온게 생각나네요",
          "speaker_id": "1",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000475.1.12",
          "form": "여행지에서 먹었던  기억에 남는 음식 있으신가요?",
          "original_form": "여행지에서 먹었던  기억에 남는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000475.1.13",
          "form": "흠 시골이 부산이라  아나구 회 이네요 ㅎ",
          "original_form": "흠 시골이 부산이라  아나구 회 이네요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000475.1.14",
          "form": "현재 3턴 ㅎ",
          "original_form": "현재 3턴 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000475.1.15",
          "form": "회 맛있죠 저는 여수가서 게장 맛있게 먹었어요",
          "original_form": "회 맛있죠 저는 여수가서 게장 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000475.1.16",
          "form": "해외여행 계회 있으신지?",
          "original_form": "해외여행 계회 있으신지?",
          "speaker_id": "1",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000475.1.17",
          "form": "ㅋ 넵 ㅎ",
          "original_form": "ㅋ 넵 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000475.1.18",
          "form": "코로나요 ㅠ",
          "original_form": "코로나요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000475.1.19",
          "form": "미국 가고십죠",
          "original_form": "미국 가고십죠",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000475.1.20",
          "form": "중학교 고등학교 미국나와서요",
          "original_form": "중학교 고등학교 미국나와서요",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000475.1.21",
          "form": "오 해외파!\r\n미국 여행지 추천해주세요",
          "original_form": "오 해외파!\r\n미국 여행지 추천해주세요",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000475.1.22",
          "form": "전 뉴욕주 에 살았어요",
          "original_form": "전 뉴욕주 에 살았어요",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000475.1.23",
          "form": "오 뉴욕",
          "original_form": "오 뉴욕",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000475.1.24",
          "form": "뉴욕에 대해 로망이나 아시는게 있나요?",
          "original_form": "뉴욕에 대해 로망이나 아시는게 있나요?",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000475.1.25",
          "form": "지리적 정보로 제가 아는 것이",
          "original_form": "지리적 정보로 제가 아는 것이",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000475.1.26",
          "form": "뉴욕주가 산이 많아 50개 주 중 유일하게 수돗물등 마실수 잇네요",
          "original_form": "뉴욕주가 산이 많아 50개 주 중 유일하게 수돗물등 마실수 잇네요",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000475.1.27",
          "form": "뉴욕가서 빌딩 구경하고 싶어요~",
          "original_form": "뉴욕가서 빌딩 구경하고 싶어요~",
          "speaker_id": "1",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000475.1.28",
          "form": "그래서 스키장이 다 몰려있네요 ㅎ",
          "original_form": "그래서 스키장이 다 몰려있네요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000475.1.29",
          "form": "충격적이지만 강남 여의도랑 합친 느낌이예요 ㅎ",
          "original_form": "충격적이지만 강남 여의도랑 합친 느낌이예요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000475.1.30",
          "form": "건물색만 조금 다르지",
          "original_form": "건물색만 조금 다르지",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000475.1.31",
          "form": "30분  있다 다시뵈요 ㅎ",
          "original_form": "30분  있다 다시뵈요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000475.1.32",
          "form": "그렇군요 뉴욕 미술관도 많던데 가보셨어요?",
          "original_form": "그렇군요 뉴욕 미술관도 많던데 가보셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000475.1.33",
          "form": "네셔널 뮤지엄 나봤네요",
          "original_form": "네셔널 뮤지엄 나봤네요",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000475.1.34",
          "form": "박물관이살아있다에나오는",
          "original_form": "박물관이살아있다에나오는",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000475.1.35",
          "form": "네 가고싶은 여행지 찾아서 다시 올게요!",
          "original_form": "네 가고싶은 여행지 찾아서 다시 올게요!",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000475.1.36",
          "form": "넵 ㅎ 잠시있다뵈요",
          "original_form": "넵 ㅎ 잠시있다뵈요",
          "speaker_id": "2",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000475.1.37",
          "form": "찾아보니까 뉴욕에 관광지 정말 많아요 항공모함 박물관도 있네요",
          "original_form": "찾아보니까 뉴욕에 관광지 정말 많아요 항공모함 박물관도 있네요",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000475.1.38",
          "form": "네 ㅎ ㅎ",
          "original_form": "네 ㅎ ㅎ",
