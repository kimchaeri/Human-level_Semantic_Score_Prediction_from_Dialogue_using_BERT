      "utterance": [
        {
          "id": "MDRW2100008173.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008173.1.2",
          "form": "해요",
          "original_form": "해요",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008173.1.3",
          "form": "남편이 많이 해줘요 애기가 어려서",
          "original_form": "남편이 많이 해줘요 애기가 어려서",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100008173.1.4",
          "form": "저도 청소담당이요",
          "original_form": "저도 청소담당이요",
          "speaker_id": "1",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008173.1.5",
          "form": "자주하시나요?",
          "original_form": "자주하시나요?",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008173.1.6",
          "form": "일주일에 한번",
          "original_form": "일주일에 한번",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008173.1.7",
          "form": "저희도요 ㅎ",
          "original_form": "저희도요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 19:40"
        },
        {
          "id": "MDRW2100008173.1.8",
          "form": "매일하는사람들은 부지런한거같아요",
          "original_form": "매일하는사람들은 부지런한거같아요",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008173.1.9",
          "form": "곰팡이걱정이",
          "original_form": "곰팡이걱정이",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008173.1.10",
          "form": "화장실이요?",
          "original_form": "화장실이요?",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008173.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008173.1.12",
          "form": "습해서",
          "original_form": "습해서",
          "speaker_id": "1",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008173.1.13",
          "form": "환풍기 자주틀어야겠네요",
          "original_form": "환풍기 자주틀어야겠네요",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008173.1.14",
          "form": "그래도 습도는 안되요",
          "original_form": "그래도 습도는 안되요",
          "speaker_id": "1",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008173.1.15",
          "form": "제습기도 트셔야겠네요",
          "original_form": "제습기도 트셔야겠네요",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100008173.1.16",
          "form": "저희는  괜찮더라고요",
          "original_form": "저희는  괜찮더라고요",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100008173.1.17",
          "form": "제습기 틀면 온도 올라가서요",
          "original_form": "제습기 틀면 온도 올라가서요",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100008173.1.18",
          "form": "흠 이래저래 여름은 ㅠ",
          "original_form": "흠 이래저래 여름은 ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
