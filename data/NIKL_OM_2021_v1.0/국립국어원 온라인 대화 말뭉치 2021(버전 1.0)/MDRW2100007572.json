      "utterance": [
        {
          "id": "MDRW2100007572.1.1",
          "form": "요즘 가족끼리 머해?",
          "original_form": "요즘 가족끼리 머해?",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007572.1.2",
          "form": "집에서 같이 할만한 게 없다",
          "original_form": "집에서 같이 할만한 게 없다",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007572.1.3",
          "form": "가족끼리 티비보면서 이야기하기??",
          "original_form": "가족끼리 티비보면서 이야기하기??",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007572.1.4",
          "form": "거의 이야기만 하는듯ㅠㅠ",
          "original_form": "거의 이야기만 하는듯ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007572.1.5",
          "form": "ㅠㅠㅠ그치",
          "original_form": "ㅠㅠㅠ그치",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007572.1.6",
          "form": "코로나라 어쩔수없나봐ㅠ",
          "original_form": "코로나라 어쩔수없나봐ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007572.1.7",
          "form": "티비 아니면 영화야",
          "original_form": "티비 아니면 영화야",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007572.1.8",
          "form": "보드게임 같은걸 하기도 애매하고ㅠㅠ",
          "original_form": "보드게임 같은걸 하기도 애매하고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007572.1.9",
          "form": "카드게임도 꽤 재밌는데",
          "original_form": "카드게임도 꽤 재밌는데",
          "speaker_id": "1",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007572.1.10",
          "form": "얘도 너무 많이 함",
          "original_form": "얘도 너무 많이 함",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.11",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.12",
          "form": "보드게임 재밌어!!",
          "original_form": "보드게임 재밌어!!",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.13",
          "form": "가족이랑 다같이살아??",
          "original_form": "가족이랑 다같이살아??",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.14",
          "form": "웅 아빠만 따로 살아",
          "original_form": "웅 아빠만 따로 살아",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.15",
          "form": "아항 기러기시네ㅠㅠ",
          "original_form": "아항 기러기시네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.16",
          "form": "나는 혼자 따로나와살아서",
          "original_form": "나는 혼자 따로나와살아서",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.17",
          "form": "자주 같이있진 않거덩..",
          "original_form": "자주 같이있진 않거덩..",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.18",
          "form": "헉 자취야?",
          "original_form": "헉 자취야?",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.19",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.20",
          "form": "아궁 외롭겟다",
          "original_form": "아궁 외롭겟다",
          "speaker_id": "1",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007572.1.21",
          "form": "자취하면 좋은데 안좋고 그렇지",
          "original_form": "자취하면 좋은데 안좋고 그렇지",
          "speaker_id": "2",
          "time": "20210822 03:26"
        }
      ]
