      "utterance": [
        {
          "id": "MDRW2100041991.1.1",
          "form": "ㅋㅋ 떠리몰 아시나요?? ㅋㅋ",
          "original_form": "ㅋㅋ 떠리몰 아시나요?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041991.1.2",
          "form": "떠리몰 들어본것같아요",
          "original_form": "떠리몰 들어본것같아요",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041991.1.3",
          "form": "제가 사보지는않았지만",
          "original_form": "제가 사보지는않았지만",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041991.1.4",
          "form": "추천",
          "original_form": "추천",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041991.1.5",
          "form": "어떻든가요?",
          "original_form": "어떻든가요?",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041991.1.6",
          "form": "잘골라서 사시면 득템 많이",
          "original_form": "잘골라서 사시면 득템 많이",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041991.1.7",
          "form": "물론 맛없어서 떨이하는 경우도 있지만 대부분 베스트 상품은 맛있어요 ㅋㅋ 대량이고요",
          "original_form": "물론 맛없어서 떨이하는 경우도 있지만 대부분 베스트 상품은 맛있어요 ㅋㅋ 대량이고요",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041991.1.8",
          "form": "인기많을것같아요",
          "original_form": "인기많을것같아요",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041991.1.9",
          "form": "떠리몰",
          "original_form": "떠리몰",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041991.1.10",
          "form": "저도언젠가는",
          "original_form": "저도언젠가는",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041991.1.11",
          "form": "이용해봐야죠",
          "original_form": "이용해봐야죠",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100041991.1.12",
          "form": "ㅋㅋ 주로 외식하세요? 요리하세요?",
          "original_form": "ㅋㅋ 주로 외식하세요? 요리하세요?",
          "speaker_id": "2",
          "time": "20210906 18:22"
        },
        {
          "id": "MDRW2100041991.1.13",
          "form": "저는 외식을좋아하는데",
          "original_form": "저는 외식을좋아하는데",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100041991.1.14",
          "form": "집에서 요리를해줘요",
          "original_form": "집에서 요리를해줘요",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.15",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.16",
          "form": "돈아낀다고...",
          "original_form": "돈아낀다고...",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.17",
          "form": "저는 외식하면 위장이ㅜㅠ",
          "original_form": "저는 외식하면 위장이ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.18",
          "form": "외식하고싶은데",
          "original_form": "외식하고싶은데",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.19",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.20",
          "form": "배달하는사람주변에많던데",
          "original_form": "배달하는사람주변에많던데",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.21",
          "form": "배달라이더인기많자나요",
          "original_form": "배달라이더인기많자나요",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.22",
          "form": "자전거타고하는사람도봤어요",
          "original_form": "자전거타고하는사람도봤어요",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.23",
          "form": "아ㅋㅋ저 알바해봤어요ㅋ ㅋ",
          "original_form": "아ㅋㅋ저 알바해봤어요ㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.24",
          "form": "뚜벅이",
          "original_form": "뚜벅이",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100041991.1.25",
          "form": "살빼려고",
          "original_form": "살빼려고",
          "speaker_id": "2",
          "time": "20210906 18:26"
        }
      ]
