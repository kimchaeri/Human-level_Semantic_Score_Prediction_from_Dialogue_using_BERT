        {
          "id": "MDRW2100043618.1.1",
          "form": "이진이랑 조안이 나오는데 본것같기ㄷ",
          "original_form": "이진이랑 조안이 나오는데 본것같기ㄷ",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043618.1.2",
          "form": "조안이 악역이었을 거예요 . 기억나요",
          "original_form": "조안이 악역이었을 거예요 . 기억나요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043618.1.3",
          "form": "위장이혼",
          "original_form": "위장이혼",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043618.1.4",
          "form": "ㅋㅋ 누구랑요?",
          "original_form": "ㅋㅋ 누구랑요?",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043618.1.5",
          "form": "엉 조안이 악역이다",
          "original_form": "엉 조안이 악역이다",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043618.1.6",
          "form": "하여간 3각 관계 ..  기억이 조금 나네요",
          "original_form": "하여간 3각 관계 ..  기억이 조금 나네요",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043618.1.7",
          "form": "이진이랑 박윤재가 부부였는데 위장이혼한다며 불륜녀와",
          "original_form": "이진이랑 박윤재가 부부였는데 위장이혼한다며 불륜녀와",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043618.1.8",
          "form": "ㅋㅋ 내용이 딱 일일이다",
          "original_form": "ㅋㅋ 내용이 딱 일일이다",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043618.1.9",
          "form": "사기로 이혼한거지",
          "original_form": "사기로 이혼한거지",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043618.1.10",
          "form": "사기로 이혼?  내용이 좀 그렇네요",
          "original_form": "사기로 이혼?  내용이 좀 그렇네요",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043618.1.11",
          "form": "불굴의차여사 주연",
          "original_form": "불굴의차여사 주연",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043618.1.12",
          "form": "계속 주연으로 가네요",
          "original_form": "계속 주연으로 가네요",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043618.1.13",
          "form": "이제 시작인거지",
          "original_form": "이제 시작인거지",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043618.1.14",
          "form": "근데 공백기가 좀 잇네",
          "original_form": "근데 공백기가 좀 잇네",
