      "utterance": [
        {
          "id": "MDRW2100022672.1.1",
          "form": "최근에 쇼핑하신거 있나요 ?",
          "original_form": "최근에 쇼핑하신거 있나요 ?",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022672.1.2",
          "form": "요즘은 주로 팔고있어서.. ㅋㅋㅋ",
          "original_form": "요즘은 주로 팔고있어서.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022672.1.3",
          "form": "쓸데엄는거중엔 쓰레빠 삿어요.. ㅋㅋㅋ",
          "original_form": "쓸데엄는거중엔 쓰레빠 삿어요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022672.1.4",
          "form": "오 어디꺼 사셨어요 ㅋㅋ ??",
          "original_form": "오 어디꺼 사셨어요 ㅋㅋ ??",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022672.1.5",
          "form": "이름 엄는거요.. 포인트 주는데.. 살꺼가 엄서서.. \n가격 맞는거오.. ㅋㅋㅋ",
          "original_form": "이름 엄는거요.. 포인트 주는데.. 살꺼가 엄서서.. \n가격 맞는거오.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022672.1.6",
          "form": "아아 ㅋㅋ 포인트는 써야죠 ㅋㅋㅋ",
          "original_form": "아아 ㅋㅋ 포인트는 써야죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022672.1.7",
          "form": "당근마켓도 자주하셔요?",
          "original_form": "당근마켓도 자주하셔요?",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022672.1.8",
          "form": "베이지 넘 좋아해서 그냥 색만 보고.. ㅋㅋ",
          "original_form": "베이지 넘 좋아해서 그냥 색만 보고.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022672.1.9",
          "form": "요즘 많이했어여.. ㅋㅋㅋ",
          "original_form": "요즘 많이했어여.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022672.1.10",
          "form": "살림살이 다 내다 팜.. ㅋㅋㅋ",
          "original_form": "살림살이 다 내다 팜.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022672.1.11",
          "form": "당근 해요?",
          "original_form": "당근 해요?",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022672.1.12",
          "form": "넼ㅋㅋ 진짜 안쓰는데 눈에 띄는건 거의 다 팔아요 ㅋㅋ",
          "original_form": "넼ㅋㅋ 진짜 안쓰는데 눈에 띄는건 거의 다 팔아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022672.1.13",
          "form": "옷도 팔구 ...",
          "original_form": "옷도 팔구 ...",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022672.1.14",
          "form": "옷 잘 팔리건가요?",
          "original_form": "옷 잘 팔리건가요?",
          "speaker_id": "2",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022672.1.15",
          "form": "음 아니요 ㅋㅋㅋ 전 거의 첨 올린 가격보다 낮춰서 파는거 같아요 ㅋㅋㄱ",
          "original_form": "음 아니요 ㅋㅋㅋ 전 거의 첨 올린 가격보다 낮춰서 파는거 같아요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022672.1.16",
          "form": "다들 안사시던데요 ,,",
          "original_form": "다들 안사시던데요 ,,",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022672.1.17",
          "form": "그쵸.. 옷은 브랜드 유행품아니면.. 조회도 잘 안되고",
          "original_form": "그쵸.. 옷은 브랜드 유행품아니면.. 조회도 잘 안되고",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022672.1.18",
          "form": "맞아요 ㅎㅎ 그래서 전 그냥 버리긴 아까운 옷만 팔아요",
          "original_form": "맞아요 ㅎㅎ 그래서 전 그냥 버리긴 아까운 옷만 팔아요",
          "speaker_id": "1",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100022672.1.19",
          "form": "막 한번도 안입은 ㅋㅋ 그런옷이요",
          "original_form": "막 한번도 안입은 ㅋㅋ 그런옷이요",
          "speaker_id": "1",
          "time": "20210829 17:06"
        },
        {
          "id": "MDRW2100022672.1.20",
          "form": "한번도 안입은 옷이 있어요?",
          "original_form": "한번도 안입은 옷이 있어요?",
          "speaker_id": "2",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100022672.1.21",
          "form": "충동구매? ㅋㅋ",
          "original_form": "충동구매? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100022672.1.22",
          "form": "그런 옷 있잖아요 ㅋㅋ 아 안입을거같긴한데 ...사놓으면 입겠지하고 사는 ㅋㅋ",
          "original_form": "그런 옷 있잖아요 ㅋㅋ 아 안입을거같긴한데 ...사놓으면 입겠지하고 사는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100022672.1.23",
          "form": "충동구매죠 ㅋㅋ",
          "original_form": "충동구매죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100022672.1.24",
          "form": "말은 제대로 하셔야죠.. \n아 살빠지면 잊어야지 하고 못입는.. ㅋㅋㅋㅋ",
          "original_form": "말은 제대로 하셔야죠.. \n아 살빠지면 잊어야지 하고 못입는.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022672.1.25",
          "form": "아 ㅋㅋㅋㅋㅋ 완전 뼈때리시네요 ...",
          "original_form": "아 ㅋㅋㅋㅋㅋ 완전 뼈때리시네요 ...",
