      "utterance": [
        {
          "id": "MDRW2100016483.1.1",
          "form": "점심은 뭘드셨나요?",
          "original_form": "점심은 뭘드셨나요?",
          "speaker_id": "1",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016483.1.2",
          "form": "오늘 점심은 와규볶음밥",
          "original_form": "오늘 점심은 와규볶음밥{emoji:☺}",
          "speaker_id": "2",
          "time": "20210826 18:41"
        },
        {
          "id": "MDRW2100016483.1.3",
          "form": "와 배달시켜드셨나요?",
          "original_form": "와 배달시켜드셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100016483.1.4",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100016483.1.5",
          "form": "무슨가게에서요?",
          "original_form": "무슨가게에서요?",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100016483.1.6",
          "form": "원래 샐러드도 주는 곳인데 배달시켰더니 샐러드는 안주더라고요 ",
          "original_form": "원래 샐러드도 주는 곳인데 배달시켰더니 샐러드는 안주더라고요 ",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100016483.1.7",
          "form": "중국집이요!",
          "original_form": "중국집이요!",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.8",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.9",
          "form": "여기 음식들 다 맛있답니다",
          "original_form": "여기 음식들 다 맛있답니다",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.10",
          "form": "와 중국집에서 일본식 메뉴를 파는군요",
          "original_form": "와 중국집에서 일본식 메뉴를 파는군요",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 베이컨 볶음밥도 맛나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 베이컨 볶음밥도 맛나요",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.12",
          "form": "여기 탕슉과 칠리새우도 아주 맛있어요",
          "original_form": "여기 탕슉과 칠리새우도 아주 맛있어요",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.13",
          "form": "와 그게무슨 혼종이에요",
          "original_form": "와 그게무슨 혼종이에요",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.14",
          "form": "중식일식한식양식",
          "original_form": "중식일식한식양식",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.15",
          "form": "다있네요",
          "original_form": "다있네요",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.16",
          "form": "근데 정말 맛있어요",
          "original_form": "근데 정말 맛있어요",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100016483.1.17",
          "form": "먹어보고싶네요",
          "original_form": "먹어보고싶네요",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.18",
          "form": "자스민 차와 후식으론 고구마 맛탕도 준답니다",
          "original_form": "자스민 차와 후식으론 고구마 맛탕도 준답니다",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.19",
          "form": "국물도주나요?",
          "original_form": "국물도주나요?",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.20",
          "form": "와 혜자네요",
          "original_form": "와 혜자네요",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.21",
          "form": "배달이라서 이건 못받았지만",
          "original_form": "배달이라서 이건 못받았지만",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.22",
          "form": "근데 가격이 비싸요",
          "original_form": "근데 가격이 비싸요",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.23",
          "form": "하지만 비싼값을 하는 곳이에요",
          "original_form": "하지만 비싼값을 하는 곳이에요",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.24",
          "form": "얼만데요",
          "original_form": "얼만데요",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.25",
          "form": "볶음밥이 7천원이에요",
          "original_form": "볶음밥이 7천원이에요",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100016483.1.26",
          "form": "?? 안비싼데요",
          "original_form": "?? 안비싼데요",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.27",
          "form": "그렇군요...9천원이었나?",
          "original_form": "그렇군요...9천원이었나?",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.28",
          "form": "기억이 가물",
          "original_form": "기억이 가물",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.29",
          "form": "오늘먹은걸",
          "original_form": "오늘먹은걸",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.30",
          "form": "기억못하시면어떡해요",
          "original_form": "기억못하시면어떡해요",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.31",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.32",
          "form": "확인해보니",
          "original_form": "확인해보니",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.33",
          "form": "9천원이 맞군요",
          "original_form": "9천원이 맞군요",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.34",
          "form": "비싸죠?",
          "original_form": "비싸죠?",
          "speaker_id": "2",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100016483.1.35",
          "form": "비싸군요",
          "original_form": "비싸군요",
          "speaker_id": "1",
          "time": "20210826 18:46"
        }
      ]
