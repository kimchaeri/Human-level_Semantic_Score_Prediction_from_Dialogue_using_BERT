        },
        {
          "id": "MDRW2100037449.1.2",
          "form": "아니 900개",
          "original_form": "아니 900개",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100037449.1.3",
          "form": "ㄹㅇ 3m당 하나씩",
          "original_form": "ㄹㅇ 3m당 하나씩",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100037449.1.4",
          "form": "묻어놨는데",
          "original_form": "묻어놨는데",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100037449.1.5",
          "form": "우리만 몰랐나봐",
          "original_form": "우리만 몰랐나봐",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100037449.1.6",
          "form": "충격적인점은",
          "original_form": "충격적인점은",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100037449.1.7",
          "form": "아직 100프로 찍은 지역이 하나도 없단거야...",
          "original_form": "아직 100프로 찍은 지역이 하나도 없단거야...",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100037449.1.8",
          "form": "난 이나즈마에서 신의눈 찾고있는데",
          "original_form": "난 이나즈마에서 신의눈 찾고있는데",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100037449.1.9",
          "form": "아니 분명 거기 있다는데",
          "original_form": "아니 분명 거기 있다는데",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100037449.1.10",
          "form": "심지어 그 바닥 밑에있는 동굴까지 봤는데",
          "original_form": "심지어 그 바닥 밑에있는 동굴까지 봤는데",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.11",
          "form": "안보여서",
          "original_form": "안보여서",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.12",
          "form": "약간 편두통왔어",
          "original_form": "약간 편두통왔어",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.13",
          "form": "어딘데?",
          "original_form": "어딘데?",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.14",
          "form": "나 나루카미 정가운데에 잇는거",
          "original_form": "나 나루카미 정가운데에 잇는거",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.15",
          "form": "뱀뼈광갱",
          "original_form": "뱀뼈광갱",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.16",
          "form": "아직도 못찾음",
          "original_form": "아직도 못찾음",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.17",
          "form": "그건 나도",
          "original_form": "그건 나도",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100037449.1.19",
          "form": "몰라 막",
          "original_form": "몰라 막",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100037449.1.20",
          "form": "걸어만 다니면",
          "original_form": "걸어만 다니면",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100037449.1.21",
          "form": "삑 하고",
          "original_form": "삑 하고",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100037449.1.22",
          "form": "어디 신의눈이 있다는데",
          "original_form": "어디 신의눈이 있다는데",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100037449.1.23",
          "form": "보이는게 없어",
          "original_form": "보이는게 없어",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100037449.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100037449.1.25",
          "form": "뱀뼈광갱에서 뱀의 머리쪽인가?",
          "original_form": "뱀뼈광갱에서 뱀의 머리쪽인가?",
          "speaker_id": "1",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100037449.1.26",
          "form": "그 결계깨기 퀴즈한곳 있잖아",
          "original_form": "그 결계깨기 퀴즈한곳 있잖아",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100037449.1.27",
          "form": "그쪽",
          "original_form": "그쪽",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100037449.1.28",
          "form": "그 꼬맹이 집 옆 감실쪽",
          "original_form": "그 꼬맹이 집 옆 감실쪽",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100037449.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100037449.1.30",
          "form": "그 벚꽃수구 뿌려진",
          "original_form": "그 벚꽃수구 뿌려진",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100037449.1.31",
          "form": "밥 막 먹고",
          "original_form": "밥 막 먹고",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100037449.1.32",
          "form": "너무 헤메서",
          "original_form": "너무 헤메서",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100037449.1.33",
          "form": "토할것같아;;",
          "original_form": "토할것같아;;",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100037449.1.34",
          "form": "게임 끄고 좀 쉬어야겠어...",
          "original_form": "게임 끄고 좀 쉬어야겠어...",
          "speaker_id": "2",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100037449.1.35",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100037449.1.36",
          "form": "이게 맵 위아래 계속 돌려가면서 보니까",
          "original_form": "이게 맵 위아래 계속 돌려가면서 보니까",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100037449.1.37",
          "form": "디지털멀미나...",
          "original_form": "디지털멀미나...",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100037449.1.38",
          "form": "쓸디멀미있어서",
          "original_form": "쓸디멀미있어서",
          "speaker_id": "2",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100037449.1.39",
          "form": "더해...",
          "original_form": "더해...",
          "speaker_id": "2",
          "time": "20210901 00:15"
        }
      ]
