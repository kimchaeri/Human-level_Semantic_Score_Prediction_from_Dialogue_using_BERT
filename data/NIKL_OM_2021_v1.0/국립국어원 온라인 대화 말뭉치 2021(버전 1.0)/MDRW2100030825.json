      "utterance": [
        {
          "id": "MDRW2100030825.1.1",
          "form": "예전에 했던거중에 그게 좋았어",
          "original_form": "예전에 했던거중에 그게 좋았어",
          "speaker_id": "2",
          "time": "20210904 13:00"
        },
        {
          "id": "MDRW2100030825.1.2",
          "form": "뭔데요?",
          "original_form": "뭔데요?",
          "speaker_id": "1",
          "time": "20210904 13:00"
        },
        {
          "id": "MDRW2100030825.1.3",
          "form": "와인박물관",
          "original_form": "와인박물관",
          "speaker_id": "2",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030825.1.4",
          "form": "가서 보기도 하고",
          "original_form": "가서 보기도 하고",
          "speaker_id": "2",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030825.1.5",
          "form": "아 그거ㅜ체험하는게 좋았어요",
          "original_form": "아 그거ㅜ체험하는게 좋았어요",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030825.1.6",
          "form": "마시기도 했는데 너 맛있다고 하고",
          "original_form": "마시기도 했는데 너 맛있다고 하고",
          "speaker_id": "2",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030825.1.7",
          "form": "근데 이젠 술 안마셔서 ㅎㅎ 원래 안마시는데 그때 딱 한모금만",
          "original_form": "근데 이젠 술 안마셔서 ㅎㅎ 원래 안마시는데 그때 딱 한모금만",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030825.1.8",
          "form": "근데 달아서 맛있었어요",
          "original_form": "근데 달아서 맛있었어요",
          "speaker_id": "1",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030825.1.9",
          "form": "전시회에 체험관 있으면",
          "original_form": "전시회에 체험관 있으면",
          "speaker_id": "2",
          "time": "20210904 13:01"
        },
        {
          "id": "MDRW2100030825.1.10",
          "form": "애들이랑 다니기 좋더라",
          "original_form": "애들이랑 다니기 좋더라",
          "speaker_id": "2",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030825.1.11",
          "form": "맞아요 저두 그때 초등학생 아니었나?",
          "original_form": "맞아요 저두 그때 초등학생 아니었나?",
          "speaker_id": "1",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030825.1.12",
          "form": "그래서 에이 무슨 와인~~ 했다가",
          "original_form": "그래서 에이 무슨 와인~~ 했다가",
          "speaker_id": "1",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030825.1.13",
          "form": "맛있다고 ㅋㅋㅋ 근데 시원하기도 하고 좋았네요",
          "original_form": "맛있다고 ㅋㅋㅋ 근데 시원하기도 하고 좋았네요",
          "speaker_id": "1",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030825.1.14",
          "form": "넌 실내만 좋아해",
          "original_form": "넌 실내만 좋아해",
          "speaker_id": "2",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030825.1.15",
          "form": "햇빛도 봐야지",
          "original_form": "햇빛도 봐야지",
          "speaker_id": "2",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030825.1.16",
          "form": "ㅎㅎ 아무튼... 애들이랑 갈때 뭐 시키면 애들 집중 잘하니까요",
          "original_form": "ㅎㅎ 아무튼... 애들이랑 갈때 뭐 시키면 애들 집중 잘하니까요",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030825.1.17",
          "form": "그것두 있구...",
          "original_form": "그것두 있구...",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030825.1.18",
          "form": "종이만드는거",
          "original_form": "종이만드는거",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030825.1.19",
          "form": "한지박물관?",
          "original_form": "한지박물관?",
          "speaker_id": "2",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030825.1.20",
          "form": "너 거기서 종이 만들어왔지",
          "original_form": "너 거기서 종이 만들어왔지",
          "speaker_id": "2",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030825.1.21",
          "form": "네 그 닭나무인가 하는걸로다가",
          "original_form": "네 그 닭나무인가 하는걸로다가",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030825.1.22",
          "form": "닥나무겠지",
          "original_form": "닥나무겠지",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030825.1.23",
          "form": "그런가 하여튼 그거 녹여가지구",
          "original_form": "그런가 하여튼 그거 녹여가지구",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030825.1.24",
          "form": "닥나무 물에 풀어가지구",
          "original_form": "닥나무 물에 풀어가지구",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030825.1.25",
          "form": "죽처럼 만들어서",
          "original_form": "죽처럼 만들어서",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030825.1.26",
          "form": "채로 걸러서 하는거요",
          "original_form": "채로 걸러서 하는거요",
          "speaker_id": "1",
          "time": "20210904 13:04"
        }
      ]
    }
  ]
