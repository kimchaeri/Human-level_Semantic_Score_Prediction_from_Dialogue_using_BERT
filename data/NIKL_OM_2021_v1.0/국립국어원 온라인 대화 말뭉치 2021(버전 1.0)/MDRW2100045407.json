        {
          "id": "MDRW2100045407.1.1",
          "form": "빵야에서 사봐야지",
          "original_form": "빵야에서 사봐야지",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045407.1.2",
          "form": "그냥그래...",
          "original_form": "그냥그래...",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045407.1.3",
          "form": "난 너무생크림같은건싫던데",
          "original_form": "난 너무생크림같은건싫던데",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045407.1.4",
          "form": "아.....ㅋㅋㅋㅋㅋ앗 의외네",
          "original_form": "아.....ㅋㅋㅋㅋㅋ앗 의외네",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045407.1.5",
          "form": "난 옵스 슈크림이 땡기네",
          "original_form": "난 옵스 슈크림이 땡기네",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045407.1.6",
          "form": "빵야는 나비파이가맛있다",
          "original_form": "빵야는 나비파이가맛있다",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045407.1.7",
          "form": "진심 그거만 한 다섯개사고싶은데 눈치보여서못삼",
          "original_form": "진심 그거만 한 다섯개사고싶은데 눈치보여서못삼",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045407.1.8",
          "form": "ㅋㅋㅋ왜눈치보여?",
          "original_form": "ㅋㅋㅋ왜눈치보여?",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045407.1.9",
          "form": "나비파이?바삭해?",
          "original_form": "나비파이?바삭해?",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045407.1.10",
          "form": "학부모집임ㅋㅋㅋ",
          "original_form": "학부모집임ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045407.1.11",
          "form": "바삭달달",
          "original_form": "바삭달달",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045407.1.12",
          "form": "그집 패스츄리맛집",
          "original_form": "그집 패스츄리맛집",
