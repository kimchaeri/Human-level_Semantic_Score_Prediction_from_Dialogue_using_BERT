      "utterance": [
        {
          "id": "MDRW2100045944.1.1",
          "form": "아뇨...ㅠㅜ",
          "original_form": "아뇨...ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.2",
          "form": "전 집이 언덕이라 반강제로ㅠㅠㅠ",
          "original_form": "전 집이 언덕이라 반강제로ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.3",
          "form": "큰일이에요",
          "original_form": "큰일이에요",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.4",
          "form": "그래도 그렇게라도 운동하면 좋은거같아요 ㅠ",
          "original_form": "그래도 그렇게라도 운동하면 좋은거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.5",
          "form": "요새는진짜걷지도않아서",
          "original_form": "요새는진짜걷지도않아서",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.6",
          "form": "침대에 누워서운동해요...ㅋㅋ",
          "original_form": "침대에 누워서운동해요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.7",
          "form": "요즘 코로나때문에",
          "original_form": "요즘 코로나때문에",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.8",
          "form": "운동인진모르겠지만",
          "original_form": "운동인진모르겠지만",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.9",
          "form": "더 그런 것 같아요ㅜㅜ",
          "original_form": "더 그런 것 같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045944.1.10",
          "form": "네 ㅠ",
          "original_form": "네 ㅠ",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045944.1.11",
          "form": "나가기도 무섭구ㅜㅜ",
          "original_form": "나가기도 무섭구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045944.1.12",
          "form": "맨날찾능게 누워서하는운동",
          "original_form": "맨날찾능게 누워서하는운동",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045944.1.13",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045944.1.14",
          "form": "다 누워서 해결",
          "original_form": "다 누워서 해결",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045944.1.15",
          "form": "근데 누워있는게",
          "original_form": "근데 누워있는게",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045944.1.16",
          "form": "척추에는제일좋대요",
          "original_form": "척추에는제일좋대요",
          "speaker_id": "2",
          "time": "20210908 08:57"
