        },
        {
          "id": "MDRW2100029040.1.2",
          "form": "안녕!!!!!",
          "original_form": "안녕!!!!!",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029040.1.3",
          "form": "쇼핑 좋아해?",
          "original_form": "쇼핑 좋아해?",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029040.1.4",
          "form": "나 좋아하지~ 너는?",
          "original_form": "나 좋아하지~ 너는?",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029040.1.5",
          "form": "나도ㅋㅋㅋ",
          "original_form": "나도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029040.1.6",
          "form": "항상 습관적으로",
          "original_form": "항상 습관적으로",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029040.1.7",
          "form": "물건을 사는것같아",
          "original_form": "물건을 사는것같아",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029040.1.8",
          "form": "나도!! 특히 요즘은 집에 누워서 인터넷 쇼핑 진짜 많이해...",
          "original_form": "나도!! 특히 요즘은 집에 누워서 인터넷 쇼핑 진짜 많이해...",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029040.1.9",
          "form": "너는 온라인 오프라인 어느쪽이 좋아??",
          "original_form": "너는 온라인 오프라인 어느쪽이 좋아??",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029040.1.10",
          "form": "나는 온라인?",
          "original_form": "나는 온라인?",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029040.1.11",
          "form": "오프라인은 비싸기도하고",
          "original_form": "오프라인은 비싸기도하고",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029040.1.12",
          "form": "돌아다니기힘들어서ㅋㅋㅋ",
          "original_form": "돌아다니기힘들어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029040.1.13",
          "form": "와...나랑 똑같다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와...나랑 똑같다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029040.1.14",
          "form": "제일 최근에 산게 뭔지 물어봐도돼?",
          "original_form": "제일 최근에 산게 뭔지 물어봐도돼?",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029040.1.15",
          "form": "음 뭐였지? 워낙 많이 사서",
          "original_form": "음 뭐였지? 워낙 많이 사서",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029040.1.16",
          "form": "식초였나ㅋㅋ로켓배송!",
          "original_form": "식초였나ㅋㅋ로켓배송!",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029040.1.17",
          "form": "ㅋㅋㅋㅋ요리해먹나보다!!!!",
          "original_form": "ㅋㅋㅋㅋ요리해먹나보다!!!!",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029040.1.18",
          "form": "나는...샴푸샀어",
          "original_form": "나는...샴푸샀어",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029040.1.19",
          "form": "오늘의 tmi",
          "original_form": "오늘의 tmi",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029040.1.20",
          "form": "당근마켓 이런거 좋아해??",
          "original_form": "당근마켓 이런거 좋아해??",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029040.1.21",
          "form": "오 ㅋㅋㅋ 샴푸~",
          "original_form": "오 ㅋㅋㅋ 샴푸~",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029040.1.22",
          "form": "응 자주 쓰는듯?",
          "original_form": "응 자주 쓰는듯?",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029040.1.23",
          "form": "싸고 좋아ㅎㅎㅎ",
          "original_form": "싸고 좋아ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029040.1.24",
          "form": "나...사실 내향성 인간이라",
          "original_form": "나...사실 내향성 인간이라",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029040.1.25",
          "form": "맨날 눈팅만해ㅋㅋㅋㅋㅋ",
          "original_form": "맨날 눈팅만해ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029040.1.26",
          "form": "요즘은 전자기기에 꽂혀서 갤럭시 워치 찾아보는중...",
          "original_form": "요즘은 전자기기에 꽂혀서 갤럭시 워치 찾아보는중...",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029040.1.27",
          "form": "한번하면 중독된다ㅎㅎㅎ",
          "original_form": "한번하면 중독된다ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029040.1.28",
          "form": "한번 해봐 은근 쉬워",
          "original_form": "한번 해봐 은근 쉬워",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029040.1.29",
          "form": "거의 쿨거래야",
          "original_form": "거의 쿨거래야",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029040.1.30",
          "form": "주로 뭐사?? 옷같은거??",
          "original_form": "주로 뭐사?? 옷같은거??",
