      "utterance": [
        {
          "id": "MDRW2100045254.1.1",
          "form": "3분 20초부터",
          "original_form": "3분 20초부터",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045254.1.2",
          "form": "그 제가 좋아하는 류님 싸이코같은 연기",
          "original_form": "그 제가 좋아하는 류님 싸이코같은 연기",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045254.1.3",
          "form": "저런 목소리내는거 좋아합니다",
          "original_form": "저런 목소리내는거 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045254.1.4",
          "form": "그래서 다른 지킬은 인정할수없지",
          "original_form": "그래서 다른 지킬은 인정할수없지",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045254.1.5",
          "form": "연쇄살인마로 변하는데 저정도 목소리는 뒤집어주면서 변해야지",
          "original_form": "연쇄살인마로 변하는데 저정도 목소리는 뒤집어주면서 변해야지",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045254.1.6",
          "form": "어~~~~~~~~~~어!!!!!!!!!!!!!!ㅇ\r\n이러면서 변하다니",
          "original_form": "어~~~~~~~~~~어!!!!!!!!!!!!!!ㅇ\r\n이러면서 변하다니",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045254.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045254.1.8",
          "form": "아예 다른 인격으로 넘어가는거니까",
          "original_form": "아예 다른 인격으로 넘어가는거니까",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045254.1.9",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045254.1.10",
          "form": "확 뒤집어줘야 살긴 하지..",
          "original_form": "확 뒤집어줘야 살긴 하지..",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045254.1.11",
          "form": "마지막부분\r\n들어보시면",
          "original_form": "마지막부분\r\n들어보시면",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045254.1.12",
          "form": "약간 류님이 저런목소리잘해",
          "original_form": "약간 류님이 저런목소리잘해",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045254.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.14",
          "form": "오 빅스레오도 토드했나보네",
          "original_form": "오 빅스레오도 토드했나보네",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.15",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.16",
          "form": "걔가???",
          "original_form": "걔가???",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.17",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.18",
          "form": "상상이 안되네",
          "original_form": "상상이 안되네",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.19",
          "form": "토드 어떤 성격인가요",
          "original_form": "토드 어떤 성격인가요",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.20",
          "form": "하긴 배역 맡는거랑 배우 성격이랑",
          "original_form": "하긴 배역 맡는거랑 배우 성격이랑",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.21",
          "form": "일치할 필욘 없지만",
          "original_form": "일치할 필욘 없지만",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.22",
          "form": "ㅇㅇㅇ상관은없어",
          "original_form": "ㅇㅇㅇ상관은없어",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045254.1.23",
          "form": "근데 약간 위태위태하고",
          "original_form": "근데 약간 위태위태하고",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.24",
          "form": "치명치명한 역이라서",
          "original_form": "치명치명한 역이라서",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.25",
          "form": "정택운 겁나 미성에..",
          "original_form": "정택운 겁나 미성에..",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.26",
          "form": "좀 그런식으로",
          "original_form": "좀 그런식으로",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.27",
          "form": "아 치명적인 느낌...",
          "original_form": "아 치명적인 느낌...",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.28",
          "form": "노골적이고약간",
          "original_form": "노골적이고약간",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.29",
          "form": "그 계열이었나보네",
          "original_form": "그 계열이었나보네",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.30",
          "form": "돌 아이같은 느낌이이어야해서",
          "original_form": "돌 아이같은 느낌이이어야해서",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.31",
          "form": "싸이코같은..",
          "original_form": "싸이코같은..",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.33",
          "form": "근데우리나라 토드가 약간 젊고 샤프하고",
          "original_form": "근데우리나라 토드가 약간 젊고 샤프하고",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.34",
          "form": "악마적인 느낌...?",
          "original_form": "악마적인 느낌...?",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.35",
          "form": "다들댄디한느낌이라고하더라",
          "original_form": "다들댄디한느낌이라고하더라",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045254.1.36",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 01:32"
        }
      ]
    }
