      "utterance": [
        {
          "id": "MDRW2100020536.1.1",
          "form": "틈새라면 먹어보셨나요",
          "original_form": "틈새라면 먹어보셨나요",
          "speaker_id": "2",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020536.1.2",
          "form": "근데 진짜 눈물빠지게 맵더라고요...하..ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "근데 진짜 눈물빠지게 맵더라고요...하..ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020536.1.3",
          "form": "네 먹어봤습니다",
          "original_form": "네 먹어봤습니다",
          "speaker_id": "1",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020536.1.4",
          "form": "저는 틈새라면이 그랬답니다",
          "original_form": "저는 틈새라면이 그랬답니다",
          "speaker_id": "2",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020536.1.5",
          "form": "불닭정도의 충격은 아니었는데",
          "original_form": "불닭정도의 충격은 아니었는데",
          "speaker_id": "2",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.6",
          "form": "먹다가 이거 왜 맵지...?",
          "original_form": "먹다가 이거 왜 맵지...?",
          "speaker_id": "2",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.7",
          "form": "왜 맵지...? 보통 라면이 이렇게까지 맵나..??",
          "original_form": "왜 맵지...? 보통 라면이 이렇게까지 맵나..??",
          "speaker_id": "2",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.8",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ 이렇게 매운걸 못먹는데 자꾸 도전하는 나 정상인가요",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ 이렇게 매운걸 못먹는데 자꾸 도전하는 나 정상인가요",
          "speaker_id": "1",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.9",
          "form": "싶었어요",
          "original_form": "싶었어요",
          "speaker_id": "2",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.10",
          "form": "저 딱 그랬어요",
          "original_form": "저 딱 그랬어요",
          "speaker_id": "1",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.11",
          "form": "ㅋㅋㅋㅋㅋ  이거 뭐야 왜 점점 매워지는데",
          "original_form": "ㅋㅋㅋㅋㅋ  이거 뭐야 왜 점점 매워지는데",
          "speaker_id": "1",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "2",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.13",
          "form": "예전엔 신라면도 매워했는데",
          "original_form": "예전엔 신라면도 매워했는데",
          "speaker_id": "2",
          "time": "20210828 09:23"
        },
        {
          "id": "MDRW2100020536.1.14",
          "form": "요즘은 기업들덕에 좀많이 성장한거 같아요",
          "original_form": "요즘은 기업들덕에 좀많이 성장한거 같아요",
          "speaker_id": "2",
          "time": "20210828 09:24"
        },
        {
          "id": "MDRW2100020536.1.15",
          "form": "내위장...",
          "original_form": "내위장...",
          "speaker_id": "2",
          "time": "20210828 09:24"
        },
        {
          "id": "MDRW2100020536.1.16",
          "form": "신라면은 내성이 생겨서 이제 좀 괜찮고",
          "original_form": "신라면은 내성이 생겨서 이제 좀 괜찮고",
          "speaker_id": "1",
          "time": "20210828 09:24"
        },
        {
          "id": "MDRW2100020536.1.17",
          "form": "그니까요 위장박살",
          "original_form": "그니까요 위장박살",
          "speaker_id": "1",
          "time": "20210828 09:24"
        },
        {
          "id": "MDRW2100020536.1.18",
          "form": "ㅋㅋㅋㅋ 불닭은 어느시리즈를 가장 좋아하세요",
          "original_form": "ㅋㅋㅋㅋ 불닭은 어느시리즈를 가장 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 09:24"
        },
        {
          "id": "MDRW2100020536.1.19",
          "form": "저는 뭐든 오리지널!",
          "original_form": "저는 뭐든 오리지널!",
          "speaker_id": "2",
          "time": "20210828 09:24"
        },
        {
          "id": "MDRW2100020536.1.20",
          "form": "매운맛을 즐기시는구나",
          "original_form": "매운맛을 즐기시는구나",
          "speaker_id": "1",
          "time": "20210828 09:24"
        },
        {
          "id": "MDRW2100020536.1.21",
          "form": "마라맛도 맛있더라구요",
          "original_form": "마라맛도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.22",
          "form": "근데 돌고돌아도 결국 오리지널로 돌아오더라구요",
          "original_form": "근데 돌고돌아도 결국 오리지널로 돌아오더라구요",
          "speaker_id": "1",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.23",
          "form": "저 못먹는거 치고는 좋아해요",
          "original_form": "저 못먹는거 치고는 좋아해요",
          "speaker_id": "2",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.24",
          "form": "name1님은 그럼 분홍색?",
          "original_form": "&name1&님은 그럼 분홍색?",
          "speaker_id": "2",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.25",
          "form": "마라맛도 있어요? 전 카레맛까지 먹어봐서",
          "original_form": "마라맛도 있어요? 전 카레맛까지 먹어봐서",
          "speaker_id": "1",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.26",
          "form": "분홍색도 맛있어요 그거 덜맵더라구요",
          "original_form": "분홍색도 맛있어요 그거 덜맵더라구요",
          "speaker_id": "1",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.27",
          "form": "카레맛이었나..",
          "original_form": "카레맛이었나..",
          "speaker_id": "2",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.28",
          "form": "대만가서 먹었던거같아요",
          "original_form": "대만가서 먹었던거같아요",
          "speaker_id": "2",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.29",
          "form": "네..혹시 보인다면 근데 드시지마세요",
          "original_form": "네..혹시 보인다면 근데 드시지마세요",
          "speaker_id": "1",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.30",
          "form": "오호 분홍색 사실 아직 안먹어봤어요",
          "original_form": "오호 분홍색 사실 아직 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 09:25"
        },
        {
          "id": "MDRW2100020536.1.31",
          "form": "헉.. 아직 안드셔보셨다니",
          "original_form": "헉.. 아직 안드셔보셨다니",
          "speaker_id": "1",
          "time": "20210828 09:26"
        }
      ]
