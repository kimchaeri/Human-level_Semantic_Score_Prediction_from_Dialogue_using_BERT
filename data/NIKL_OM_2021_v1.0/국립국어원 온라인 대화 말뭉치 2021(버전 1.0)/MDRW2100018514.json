      "utterance": [
        {
          "id": "MDRW2100018514.1.1",
          "form": "안녕하세용 최근에 본 영화 있으세요?",
          "original_form": "안녕하세용 최근에 본 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018514.1.2",
          "form": "안녕하세요~ 저는 최근에 말할 수 없는 비밀을 봤어요",
          "original_form": "안녕하세요~ 저는 최근에 말할 수 없는 비밀을 봤어요",
          "speaker_id": "2",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018514.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018514.1.4",
          "form": "대만영화 맞죠?",
          "original_form": "대만영화 맞죠?",
          "speaker_id": "1",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018514.1.5",
          "form": "주걸륜 나오는거!",
          "original_form": "주걸륜 나오는거!",
          "speaker_id": "1",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018514.1.6",
          "form": "저는 킬러의보디가드2 봤어요!ㅋㅋ",
          "original_form": "저는 킬러의보디가드2 봤어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018514.1.7",
          "form": "네! 보셨나요?",
          "original_form": "네! 보셨나요?",
          "speaker_id": "2",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018514.1.8",
          "form": "유명해서 언젠간 봐야지봐야지했는데",
          "original_form": "유명해서 언젠간 봐야지봐야지했는데",
          "speaker_id": "1",
          "time": "20210827 07:27"
        },
        {
          "id": "MDRW2100018514.1.9",
          "form": "아직 못봤어요ㅠㅠㅋㅋ",
          "original_form": "아직 못봤어요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:27"
        },
        {
          "id": "MDRW2100018514.1.10",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210827 07:27"
        },
        {
          "id": "MDRW2100018514.1.11",
          "form": "학생때 학교에서 틀어줘서 봤었는데 피아노 장면이 눈길을 사로잡아서 다시 보게되더라구요",
          "original_form": "학생때 학교에서 틀어줘서 봤었는데 피아노 장면이 눈길을 사로잡아서 다시 보게되더라구요",
          "speaker_id": "2",
          "time": "20210827 07:28"
        },
        {
          "id": "MDRW2100018514.1.12",
          "form": "맞아요 그 장면은 너무 유명해서",
          "original_form": "맞아요 그 장면은 너무 유명해서",
          "speaker_id": "1",
          "time": "20210827 07:28"
        },
        {
          "id": "MDRW2100018514.1.13",
          "form": "저도 본것같아요ㅋㅋ",
          "original_form": "저도 본것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:28"
        },
        {
          "id": "MDRW2100018514.1.14",
          "form": "재밌게 봤어요!ㅋㅋ 심심하실때 가볍게 보기 좋아요ㅋㅋ",
          "original_form": "재밌게 봤어요!ㅋㅋ 심심하실때 가볍게 보기 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:28"
        },
        {
          "id": "MDRW2100018514.1.15",
          "form": "영화관을 가고싶은데 코로나때문에 못가겠네요ㅠ",
          "original_form": "영화관을 가고싶은데 코로나때문에 못가겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 07:28"
        },
        {
          "id": "MDRW2100018514.1.16",
          "form": "추천감사합니당!ㅋㅋ 꼭 볼게요",
          "original_form": "추천감사합니당!ㅋㅋ 꼭 볼게요",
          "speaker_id": "1",
          "time": "20210827 07:28"
        },
        {
          "id": "MDRW2100018514.1.17",
          "form": "맞아요ㅠ 최근에 크루엘라 보고싶더라구요",
          "original_form": "맞아요ㅠ 최근에 크루엘라 보고싶더라구요",
          "speaker_id": "2",
          "time": "20210827 07:29"
        },
        {
          "id": "MDRW2100018514.1.18",
          "form": "엇 저 크루엘라 봤어요!",
          "original_form": "엇 저 크루엘라 봤어요!",
          "speaker_id": "1",
          "time": "20210827 07:29"
        },
        {
          "id": "MDRW2100018514.1.19",
          "form": "디즈니에서 이런 영화도 만들 수 있구나 감탄했어요",
          "original_form": "디즈니에서 이런 영화도 만들 수 있구나 감탄했어요",
          "speaker_id": "1",
          "time": "20210827 07:29"
        },
        {
          "id": "MDRW2100018514.1.20",
          "form": "오! 어떤가요??",
          "original_form": "오! 어떤가요??",
          "speaker_id": "2",
          "time": "20210827 07:29"
        },
        {
          "id": "MDRW2100018514.1.21",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210827 07:29"
        },
        {
          "id": "MDRW2100018514.1.22",
          "form": "일단 디테일이 엄청나요",
          "original_form": "일단 디테일이 엄청나요",
          "speaker_id": "1",
          "time": "20210827 07:29"
        },
        {
          "id": "MDRW2100018514.1.23",
          "form": "더 보고싶어지네요ㅋㅋ",
          "original_form": "더 보고싶어지네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:29"
        },
        {
          "id": "MDRW2100018514.1.24",
          "form": "주인공 감정선은 살짝이해가 안가지만",
          "original_form": "주인공 감정선은 살짝이해가 안가지만",
          "speaker_id": "1",
          "time": "20210827 07:29"
        },
        {
          "id": "MDRW2100018514.1.25",
          "form": "원래 그런캐릭터라ㅋㅋㅋ",
          "original_form": "원래 그런캐릭터라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:29"
        }
      ]
    }
