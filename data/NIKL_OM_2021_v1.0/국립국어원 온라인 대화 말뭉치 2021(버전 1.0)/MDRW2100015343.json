      "utterance": [
        {
          "id": "MDRW2100015343.1.1",
          "form": "그러네",
          "original_form": "그러네",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015343.1.2",
          "form": "가장 최근에 뭐삼?",
          "original_form": "가장 최근에 뭐삼?",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015343.1.3",
          "form": "가장 최근에 산거.....에스트라 샘플 삼",
          "original_form": "가장 최근에 산거.....에스트라 샘플 삼",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015343.1.4",
          "form": "도착은 했는데 언제쯤 개봉할지 미지수",
          "original_form": "도착은 했는데 언제쯤 개봉할지 미지수",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015343.1.5",
          "form": "ㅋㅋㅋㅋㅋ나도 에스트라 저번주에 샀는데 귀찮아서 안뜯다가 이거 시작하기 전에 택배박스 싹 정리",
          "original_form": "ㅋㅋㅋㅋㅋ나도 에스트라 저번주에 샀는데 귀찮아서 안뜯다가 이거 시작하기 전에 택배박스 싹 정리",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015343.1.6",
          "form": "아토피에 그래도 그나마 효과있다고 생각되는게 에스트라인듯",
          "original_form": "아토피에 그래도 그나마 효과있다고 생각되는게 에스트라인듯",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015343.1.7",
          "form": "딴건 그냥 피부에 바르면 무난하고 ㄱㅊ아도 아토피에는 오히려 자극적이거나 하나도 보습감 없고",
          "original_form": "딴건 그냥 피부에 바르면 무난하고 ㄱㅊ아도 아토피에는 오히려 자극적이거나 하나도 보습감 없고",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015343.1.8",
          "form": "에스트라가 피부과에서 파는거랑 그냥 쇼핑몰에서 파는거랑 성분 다르다는 말이 있던데",
          "original_form": "에스트라가 피부과에서 파는거랑 그냥 쇼핑몰에서 파는거랑 성분 다르다는 말이 있던데",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015343.1.9",
          "form": "그냥 사도 괜찮음?",
          "original_form": "그냥 사도 괜찮음?",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015343.1.10",
          "form": "일단 사봄",
          "original_form": "일단 사봄",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015343.1.11",
          "form": "지금 쓰는거 반정도 남아가지고.",
          "original_form": "지금 쓰는거 반정도 남아가지고.",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015343.1.12",
          "form": "가격은 비슷하던데 안되면 처방해주는 피부과 찾ㅇ봐야할듯",
          "original_form": "가격은 비슷하던데 안되면 처방해주는 피부과 찾ㅇ봐야할듯",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015343.1.13",
          "form": "요즘 기초제품은 뭐쓰세요?",
          "original_form": "요즘 기초제품은 뭐쓰세요?",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015343.1.14",
          "form": "요즘은 스킨푸드에서 산거",
          "original_form": "요즘은 스킨푸드에서 산거",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015343.1.15",
          "form": "네고왕때 샀던 수분크림 이제 뜯어서 쓰고 있어여",
          "original_form": "네고왕때 샀던 수분크림 이제 뜯어서 쓰고 있어여",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015343.1.16",
          "form": "시기가 비슷한가보다 ㅋㅋㅋ 저두요",
          "original_form": "시기가 비슷한가보다 ㅋㅋㅋ 저두요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015343.1.17",
          "form": "노란거 이제 쓰는중",
          "original_form": "노란거 이제 쓰는중",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015343.1.18",
          "form": "나쁘지 않음",
          "original_form": "나쁘지 않음",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015343.1.19",
          "form": "그리고 그때 산 언청 비싼거.. 캐비어 머시기도 같이 쓰는중인데 잘 모르곗음",
          "original_form": "그리고 그때 산 언청 비싼거.. 캐비어 머시기도 같이 쓰는중인데 잘 모르곗음",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015343.1.20",
          "form": "일단 용기가 유리라서 넘 무겁",
          "original_form": "일단 용기가 유리라서 넘 무겁",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015343.1.21",
          "form": "아 그 착한수분크림어쩌구",
          "original_form": "아 그 착한수분크림어쩌구",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015343.1.22",
          "form": "너도 쓰고있니",
          "original_form": "너도 쓰고있니",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015343.1.23",
          "form": "야스 그거 쓰는중",
          "original_form": "야스 그거 쓰는중",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015343.1.24",
          "form": "화장품 사놓은거 다 쓰고 새로 사야지",
          "original_form": "화장품 사놓은거 다 쓰고 새로 사야지",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015343.1.25",
          "form": "맞아.. 나지금 앰플 사뉸게 넘 많다",
          "original_form": "맞아.. 나지금 앰플 사뉸게 넘 많다",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015343.1.26",
          "form": "겁나 부지런히 써야해",
          "original_form": "겁나 부지런히 써야해",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015343.1.27",
          "form": "면세로 뭐 살거야?",
          "original_form": "면세로 뭐 살거야?",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015343.1.28",
          "form": "부기 나갔다올때?",
          "original_form": "부기 나갔다올때?",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015343.1.29",
          "form": "생각 안해봤는데,,,,",
          "original_form": "생각 안해봤는데,,,,",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015343.1.30",
          "form": "짐 안될만한거 있으면 좀 구경해봐야겠다..ㅋㅋㅋㅋㅋ",
          "original_form": "짐 안될만한거 있으면 좀 구경해봐야겠다..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015343.1.31",
          "form": "넌",
          "original_form": "넌",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015343.1.32",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015343.1.33",
          "form": "아마 틴트 살듯",
          "original_form": "아마 틴트 살듯",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015343.1.34",
          "form": "차차 거의 다 써가지구",
          "original_form": "차차 거의 다 써가지구",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015343.1.35",
          "form": "아하. 난 요즘 러브틴트 쓴다아악",
          "original_form": "아하. 난 요즘 러브틴트 쓴다아악",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015343.1.36",
          "form": "립도 새거 넘 많아서 빨랑 써야하는데 마스크가 문제다",
          "original_form": "립도 새거 넘 많아서 빨랑 써야하는데 마스크가 문제다",
