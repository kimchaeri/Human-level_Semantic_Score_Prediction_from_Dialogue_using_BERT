      "utterance": [
        {
          "id": "MDRW2100036719.1.1",
          "form": "날씨 갑자기 오늘은 춥네요",
          "original_form": "날씨 갑자기 오늘은 춥네요",
          "speaker_id": "1",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036719.1.2",
          "form": "네 너무추워졌어요,  저녁에는요.",
          "original_form": "네 너무추워졌어요,  저녁에는요.",
          "speaker_id": "2",
          "time": "20210831 23:50"
        },
        {
          "id": "MDRW2100036719.1.3",
          "form": "옷을 또 뭔가 꺼내야할려나 가을옷 본격적으로 개시하셨나요",
          "original_form": "옷을 또 뭔가 꺼내야할려나 가을옷 본격적으로 개시하셨나요",
          "speaker_id": "1",
          "time": "20210901 07:35"
        },
        {
          "id": "MDRW2100036719.1.4",
          "form": "아니요 오늘 가을옷좀 꺼내려구요ㅜㅜㅋㅋㅋ",
          "original_form": "아니요 오늘 가을옷좀 꺼내려구요ㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 08:31"
        },
        {
          "id": "MDRW2100036719.1.5",
          "form": "여름옷 정리해야될것같아요",
          "original_form": "여름옷 정리해야될것같아요",
          "speaker_id": "2",
          "time": "20210901 08:31"
        },
        {
          "id": "MDRW2100036719.1.6",
          "form": "옷 정리하셨나요?",
          "original_form": "옷 정리하셨나요?",
          "speaker_id": "2",
          "time": "20210901 08:31"
        },
        {
          "id": "MDRW2100036719.1.7",
          "form": "음 아직요ㅜ",
          "original_form": "음 아직요ㅜ",
          "speaker_id": "1",
          "time": "20210901 09:35"
        },
        {
          "id": "MDRW2100036719.1.8",
          "form": "입을옷이 없어요ㅜㅜ",
          "original_form": "입을옷이 없어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 09:35"
        },
        {
          "id": "MDRW2100036719.1.9",
          "form": "ㅋㅋ저두요. 옷 사야되는데ㅜ가을옷ㅜㅜ",
          "original_form": "ㅋㅋ저두요. 옷 사야되는데ㅜ가을옷ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100036719.1.10",
          "form": "없네요ㅜ",
          "original_form": "없네요ㅜ",
          "speaker_id": "2",
          "time": "20210901 09:38"
        },
        {
          "id": "MDRW2100036719.1.11",
          "form": "저는 갑자기 살이 너무쪄서",
          "original_form": "저는 갑자기 살이 너무쪄서",
          "speaker_id": "1",
          "time": "20210901 09:41"
        },
        {
          "id": "MDRW2100036719.1.12",
          "form": "어휴 ㅜ 더입을옷이없네요",
          "original_form": "어휴 ㅜ 더입을옷이없네요",
          "speaker_id": "1",
          "time": "20210901 09:41"
        },
        {
          "id": "MDRW2100036719.1.13",
          "form": "저도 살이 엄청쪄서요ㅠㅠㄱㄲㅋ",
          "original_form": "저도 살이 엄청쪄서요ㅠㅠㄱㄲㅋ",
          "speaker_id": "2",
          "time": "20210901 09:48"
        },
        {
          "id": "MDRW2100036719.1.14",
          "form": "그러고보니 코로나덕에 미세먼지는 더이상 신경 안쓰네요 ㅋㅋㅋ",
          "original_form": "그러고보니 코로나덕에 미세먼지는 더이상 신경 안쓰네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100036719.1.15",
          "form": "네ㅜ그러니깐요ㅜㅠ미세미세안본지 꽤됐네요",
          "original_form": "네ㅜ그러니깐요ㅜㅠ미세미세안본지 꽤됐네요",
          "speaker_id": "2",
          "time": "20210901 10:02"
        },
        {
          "id": "MDRW2100036719.1.16",
          "form": "어차피 마스크쓰고다니니ㅜ",
          "original_form": "어차피 마스크쓰고다니니ㅜ",
          "speaker_id": "2",
          "time": "20210901 10:02"
        },
        {
          "id": "MDRW2100036719.1.17",
          "form": "예전엔 빨래할때 미세먼지 수치보고하고 그랬었는데 ㅋㅋ",
          "original_form": "예전엔 빨래할때 미세먼지 수치보고하고 그랬었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:03"
        },
        {
          "id": "MDRW2100036719.1.18",
          "form": "맞아요ㅜㅜ지금은 그게 어디있나요ㅜ",
          "original_form": "맞아요ㅜㅜ지금은 그게 어디있나요ㅜ",
          "speaker_id": "2",
          "time": "20210901 10:03"
        },
        {
          "id": "MDRW2100036719.1.19",
          "form": "흑ㅜㅋㅋ",
          "original_form": "흑ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:03"
        },
        {
          "id": "MDRW2100036719.1.20",
          "form": "그때가 더좋았네요",
          "original_form": "그때가 더좋았네요",
          "speaker_id": "2",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100036719.1.21",
          "form": "그때도 세상 진짜 어이없다 싶었는데",
          "original_form": "그때도 세상 진짜 어이없다 싶었는데",
          "speaker_id": "1",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100036719.1.22",
          "form": "그보다 더한 세상이 왔네요 ㅋㅋ",
          "original_form": "그보다 더한 세상이 왔네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100036719.1.23",
          "form": "맞아요   진짜 막 화나고 그랬는데ㅋㅋ",
          "original_form": "맞아요   진짜 막 화나고 그랬는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100036719.1.24",
          "form": "지금은 더 하네요 후",
          "original_form": "지금은 더 하네요 후",
          "speaker_id": "2",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100036719.1.25",
          "form": "안써도 되는 돈을 쓰고 있어요 ㅋㅋ",
          "original_form": "안써도 되는 돈을 쓰고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100036719.1.26",
          "form": "ㅋㅋㅋ맞아요. 마스크값도 장난아니고ㅜㅜ",
          "original_form": "ㅋㅋㅋ맞아요. 마스크값도 장난아니고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100036719.1.27",
          "form": "피부트러블도장난아니고ㅜ",
          "original_form": "피부트러블도장난아니고ㅜ",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100036719.1.28",
          "form": "미세먼지는막아주지만ㅜ",
          "original_form": "미세먼지는막아주지만ㅜ",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100036719.1.29",
          "form": "기상 이변이 어디까지 가려나요ㅜㅜ",
          "original_form": "기상 이변이 어디까지 가려나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 10:21"
        }
      ]
    }
