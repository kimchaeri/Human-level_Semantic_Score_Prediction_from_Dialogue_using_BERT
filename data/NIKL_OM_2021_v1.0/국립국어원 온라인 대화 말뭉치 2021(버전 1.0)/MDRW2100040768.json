      "utterance": [
        {
          "id": "MDRW2100040768.1.1",
          "form": "딱히 밀접접촉자지만음성이다 해서 병원에서 따로부른것도아니니까",
          "original_form": "딱히 밀접접촉자지만음성이다 해서 병원에서 따로부른것도아니니까",
          "speaker_id": "2",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040768.1.2",
          "form": "그냥 번번히일어나는 사례가아닐까",
          "original_form": "그냥 번번히일어나는 사례가아닐까",
          "speaker_id": "2",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040768.1.3",
          "form": "한번 생각을해봅니다",
          "original_form": "한번 생각을해봅니다",
          "speaker_id": "2",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040768.1.4",
          "form": "허걱.... 와...",
          "original_form": "허걱.... 와...",
          "speaker_id": "1",
          "time": "20210905 04:11"
        },
        {
          "id": "MDRW2100040768.1.5",
          "form": "근데 진짜 그 때 당시에",
          "original_form": "근데 진짜 그 때 당시에",
          "speaker_id": "1",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.6",
          "form": "별 생각 다 드셨겠어요 ㅠㅠ",
          "original_form": "별 생각 다 드셨겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.7",
          "form": "뉴욕에서 언니 투병 중이실 때...",
          "original_form": "뉴욕에서 언니 투병 중이실 때...",
          "speaker_id": "1",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.8",
          "form": "ㅠㅠ 저는딱히괜찮았는데",
          "original_form": "ㅠㅠ 저는딱히괜찮았는데",
          "speaker_id": "2",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.9",
          "form": "막 뉴스에선 무서운 소식 들리고...",
          "original_form": "막 뉴스에선 무서운 소식 들리고...",
          "speaker_id": "1",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.11",
          "form": "걍언니건강만좋아졌으면했죠",
          "original_form": "걍언니건강만좋아졌으면했죠",
          "speaker_id": "2",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.12",
          "form": "앗 맞아요 맞아요",
          "original_form": "앗 맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.13",
          "form": "ㄹ이럴땨일수록",
          "original_form": "ㄹ이럴땨일수록",
          "speaker_id": "2",
          "time": "20210905 04:12"
        },
        {
          "id": "MDRW2100040768.1.14",
          "form": "건강이최고라고생각되는...",
          "original_form": "건강이최고라고생각되는...",
          "speaker_id": "2",
          "time": "20210905 04:13"
        },
        {
          "id": "MDRW2100040768.1.15",
          "form": "침착하신 분이시군요!!!",
          "original_form": "침착하신 분이시군요!!!",
          "speaker_id": "1",
          "time": "20210905 04:13"
        },
        {
          "id": "MDRW2100040768.1.16",
          "form": "맞아요....ㅠㅠ",
          "original_form": "맞아요....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 04:13"
        },
        {
          "id": "MDRW2100040768.1.17",
          "form": "그나저나 9일이면",
          "original_form": "그나저나 9일이면",
          "speaker_id": "1",
          "time": "20210905 04:13"
        },
        {
          "id": "MDRW2100040768.1.18",
          "form": "다음주",
          "original_form": "다음주",
          "speaker_id": "1",
          "time": "20210905 04:13"
        },
        {
