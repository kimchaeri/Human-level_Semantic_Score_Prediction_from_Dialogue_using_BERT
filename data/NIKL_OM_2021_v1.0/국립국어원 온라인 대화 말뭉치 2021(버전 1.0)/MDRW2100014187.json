      "utterance": [
        {
          "id": "MDRW2100014187.1.1",
          "form": "운동은 하고계세요?",
          "original_form": "운동은 하고계세요?",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014187.1.2",
          "form": "네 매일 하려고 노력은 하는데\n지키기 힘드네요ㅠ",
          "original_form": "네 매일 하려고 노력은 하는데\n지키기 힘드네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014187.1.3",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014187.1.4",
          "form": "운동 좋아하는 편입니다",
          "original_form": "운동 좋아하는 편입니다",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014187.1.5",
          "form": "헬스 좀 했어요",
          "original_form": "헬스 좀 했어요",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014187.1.6",
          "form": "저도 pt 6개월정도 했었는데 \n비싸도 확실히 배우면서 운동하니까 좋더라구요",
          "original_form": "저도 pt 6개월정도 했었는데 \n비싸도 확실히 배우면서 운동하니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014187.1.7",
          "form": "그렇죠. 자세도 도움 되고 알아서 최적의 운동을 골라주잖아요",
          "original_form": "그렇죠. 자세도 도움 되고 알아서 최적의 운동을 골라주잖아요",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014187.1.8",
          "form": "중량운동 해보셨나요?",
          "original_form": "중량운동 해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014187.1.9",
          "form": "중량운동이 데드리프트 이런거 말씀하시는거 맞나요?",
          "original_form": "중량운동이 데드리프트 이런거 말씀하시는거 맞나요?",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014187.1.10",
          "form": "네 무거운 중량운동",
          "original_form": "네 무거운 중량운동",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014187.1.11",
          "form": "저는 유산소보다 중량운동하는게 더 좋더라구요",
          "original_form": "저는 유산소보다 중량운동하는게 더 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014187.1.12",
          "form": "근육 생기면 너무 신기하더라구요",
          "original_form": "근육 생기면 너무 신기하더라구요",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014187.1.13",
          "form": "그리고 다음날 근육통 안 오면 제대로 안한거같고 그렇더라구요ㅋㅋ",
          "original_form": "그리고 다음날 근육통 안 오면 제대로 안한거같고 그렇더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014187.1.14",
          "form": "맞아요ㅋㅋㅋ 계단내려갈때 멀쩡하게 걸어가면 뭔가 잘못한것같고",
          "original_form": "맞아요ㅋㅋㅋ 계단내려갈때 멀쩡하게 걸어가면 뭔가 잘못한것같고",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014187.1.15",
          "form": "마자요ㅋㅋㅋ계단 내려갈때 후들후들 거려야 뿌듯하죠ㅋㅋ",
          "original_form": "마자요ㅋㅋㅋ계단 내려갈때 후들후들 거려야 뿌듯하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014187.1.16",
          "form": "웃을때 배 땡기고ㅋㅋ",
          "original_form": "웃을때 배 땡기고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014187.1.17",
          "form": "그러다가 헬스 중독되는거 아닐까요ㅋㅋ",
          "original_form": "그러다가 헬스 중독되는거 아닐까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014187.1.18",
          "form": "저는 그게 기분 좋아서 헬스 자주했거든요",
          "original_form": "저는 그게 기분 좋아서 헬스 자주했거든요",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014187.1.19",
          "form": "저두요 하고나면 개운하기도 하고 해서 하면  좋은데\n하러가기까지가 힘든거같아요ㅋㅋ",
          "original_form": "저두요 하고나면 개운하기도 하고 해서 하면  좋은데\n하러가기까지가 힘든거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014187.1.20",
          "form": "샤워 딱 하면 진짜 최곤데ㅋㅋ",
          "original_form": "샤워 딱 하면 진짜 최곤데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014187.1.21",
          "form": "무조건 집 근처로 해야 그나마 가죠",
          "original_form": "무조건 집 근처로 해야 그나마 가죠",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014187.1.22",
          "form": "마자요ㅋㅋㅋ\n아니면 차라리 회사 근처로해서 일 끝나고 바로 가는게 나을거같아요",
          "original_form": "마자요ㅋㅋㅋ\n아니면 차라리 회사 근처로해서 일 끝나고 바로 가는게 나을거같아요",
          "speaker_id": "2",
          "time": "20210825 18:46"
        }
      ]
    }
