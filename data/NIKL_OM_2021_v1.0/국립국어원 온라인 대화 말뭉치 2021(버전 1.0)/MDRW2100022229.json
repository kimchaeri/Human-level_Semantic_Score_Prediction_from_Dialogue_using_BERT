      "utterance": [
        {
          "id": "MDRW2100022229.1.1",
          "form": "목살이요! 집에 남은게 그것뿐이네요 ㅋㅋ",
          "original_form": "목살이요! 집에 남은게 그것뿐이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100022229.1.2",
          "form": "목살... 맛있겠어요",
          "original_form": "목살... 맛있겠어요",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022229.1.3",
          "form": "집에 처들어가고 싶네요ㅎㅎ",
          "original_form": "집에 처들어가고 싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022229.1.4",
          "form": "저기요 무섭거든요ㅋㅋㅋ 님은 뭐 드실건가요?",
          "original_form": "저기요 무섭거든요ㅋㅋㅋ 님은 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022229.1.5",
          "form": "똑똑똑 집앞이에요 문 열어 주세요ㅎㅎ",
          "original_form": "똑똑똑 집앞이에요 문 열어 주세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022229.1.6",
          "form": "전 어죽 먹으려고요!",
          "original_form": "전 어죽 먹으려고요!",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100022229.1.7",
          "form": "와ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022229.1.8",
          "form": "어죽이요? 우와",
          "original_form": "어죽이요? 우와",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022229.1.9",
          "form": "뭐지 이 분 잘챙겨드신다",
          "original_form": "뭐지 이 분 잘챙겨드신다",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022229.1.10",
          "form": "직접 만들어 )드시나요?",
          "original_form": "직접 만들어 )드시나요?",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022229.1.11",
          "form": "직접 만들어 드시나요?",
          "original_form": "직접 만들어 드시나요?",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022229.1.12",
          "form": "목살 드시는 님도 만만찮아요ㅎ",
          "original_form": "목살 드시는 님도 만만찮아요ㅎ",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022229.1.13",
          "form": "당연히...배달입니다ㅜ",
          "original_form": "당연히...배달입니다ㅜ",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022229.1.14",
          "form": "요리는 제게 허락된 영역이 아녜요",
          "original_form": "요리는 제게 허락된 영역이 아녜요",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022229.1.15",
          "form": "어죽 드시나요?",
          "original_form": "어죽 드시나요?",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022229.1.16",
          "form": "ㅋㅋㅋㅋㅋ웃기는 분이네",
          "original_form": "ㅋㅋㅋㅋㅋ웃기는 분이네",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022229.1.17",
          "form": "아니 배달로 어죽을?",
          "original_form": "아니 배달로 어죽을?",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022229.1.18",
          "form": "저 사실 어죽 잘 몰라요..",
          "original_form": "저 사실 어죽 잘 몰라요..",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022229.1.19",
          "form": "정글의 법칙에서 옛날에 먹는거 보고 아..저거구나..이러고 말았네요",
          "original_form": "정글의 법칙에서 옛날에 먹는거 보고 아..저거구나..이러고 말았네요",
          "speaker_id": "2",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022229.1.20",
          "form": "아 전 맹꽁이서당이란 책서 가마솥에 끓여먹는 거 보고 혹해서요",
          "original_form": "아 전 맹꽁이서당이란 책서 가마솥에 끓여먹는 거 보고 혹해서요",
          "speaker_id": "1",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022229.1.21",
          "form": "그후 어죽을 엄청 찾아다녔죠ㅎ",
          "original_form": "그후 어죽을 엄청 찾아다녔죠ㅎ",
          "speaker_id": "1",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022229.1.22",
          "form": "포장도 안해주던 곳이 코로나 이후엔 배달도 되네요",
          "original_form": "포장도 안해주던 곳이 코로나 이후엔 배달도 되네요",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022229.1.23",
          "form": "민물매운탕은 드시나요?",
          "original_form": "민물매운탕은 드시나요?",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022229.1.24",
          "form": "우와 대박이다",
          "original_form": "우와 대박이다",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100022229.1.25",
          "form": "네 매운탕은 먹어요!!!",
          "original_form": "네 매운탕은 먹어요!!!",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100022229.1.26",
          "form": "오..어죽 저도 나중에 먹어봐야겠어요",
          "original_form": "오..어죽 저도 나중에 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022229.1.27",
          "form": "민물매운탕 먹으시면 잘 맞을 거예요ㅎㅎ",
          "original_form": "민물매운탕 먹으시면 잘 맞을 거예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022229.1.28",
          "form": "중독성 있는 맛이에요. 추천!",
          "original_form": "중독성 있는 맛이에요. 추천!",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100022229.1.29",
          "form": "그래요? 먹어봐야겠어요!!",
          "original_form": "그래요? 먹어봐야겠어요!!",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022229.1.30",
          "form": "저 목살 먹었다고 했잖아요",
          "original_form": "저 목살 먹었다고 했잖아요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022229.1.31",
          "form": "완전 맛없었어요.. 육즙이 다 빠져서..하",
          "original_form": "완전 맛없었어요.. 육즙이 다 빠져서..하",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022229.1.32",
          "form": "질겅질겅했답니다",
          "original_form": "질겅질겅했답니다",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022229.1.33",
          "form": "ㅜㅇㅜ 아니;; 어디서 산 목살이길래ㅜ",
          "original_form": "ㅜㅇㅜ 아니;; 어디서 산 목살이길래ㅜ",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022229.1.34",
          "form": "듣는 제가 다 가슴이 아프네요",
          "original_form": "듣는 제가 다 가슴이 아프네요",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022229.1.35",
          "form": "ㅋㅋㅋㅋㅋ그냥 마트에서 샀는데 잘못골랐나봐요..쩝",
          "original_form": "ㅋㅋㅋㅋㅋ그냥 마트에서 샀는데 잘못골랐나봐요..쩝",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022229.1.36",
          "form": "어죽은 잘 드셨나요?",
          "original_form": "어죽은 잘 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022229.1.37",
          "form": "저도 저녁은 뜨끈한 국물 먹어야겠어요",
          "original_form": "저도 저녁은 뜨끈한 국물 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100022229.1.38",
          "form": "ㅎㅎ 좋은 생각이에요",
          "original_form": "ㅎㅎ 좋은 생각이에요",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022229.1.39",
          "form": "어죽 뜨끈 얼큰한게 보양하는 기분이었어요",
          "original_form": "어죽 뜨끈 얼큰한게 보양하는 기분이었어요",
          "speaker_id": "1",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022229.1.40",
          "form": "저녁메뉴로 추천!",
          "original_form": "저녁메뉴로 추천!",
          "speaker_id": "1",
          "time": "20210829 14:40"
        }
      ]
    }
