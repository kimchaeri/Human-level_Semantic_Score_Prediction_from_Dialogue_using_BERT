      "utterance": [
        {
          "id": "MDRW2100043462.1.1",
          "form": "네 약 먹고 출근하고",
          "original_form": "네 약 먹고 출근하고",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043462.1.2",
          "form": "대신 좀 일찍 퇴근했어요",
          "original_form": "대신 좀 일찍 퇴근했어요",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043462.1.3",
          "form": "아이고ㅠㅠ 고생하셨네요 2차때가 더 아프다던대",
          "original_form": "아이고ㅠㅠ 고생하셨네요 2차때가 더 아프다던대",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043462.1.4",
          "form": "다음날은 좀 아팠는데 지금은 괜찮아요",
          "original_form": "다음날은 좀 아팠는데 지금은 괜찮아요",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043462.1.5",
          "form": "그땐 휴가줬으면 좋겠네요ㅠㅠ",
          "original_form": "그땐 휴가줬으면 좋겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043462.1.6",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043462.1.7",
          "form": "팀사람들이랑은 친하신가요?",
          "original_form": "팀사람들이랑은 친하신가요?",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043462.1.8",
          "form": "네 팀 분위기는 좋아요ㅎㅎ",
          "original_form": "네 팀 분위기는 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043462.1.9",
          "form": "ㅋㅋ저희두요 ㅋㅋ 다행히 나이대가 비슷해서료",
          "original_form": "ㅋㅋ저희두요 ㅋㅋ 다행히 나이대가 비슷해서료",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043462.1.10",
          "form": "아 그런 것도 중요하죠",
          "original_form": "아 그런 것도 중요하죠",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043462.1.11",
          "form": "업무보다 관계가 진짜 중요한 것 같아요",
          "original_form": "업무보다 관계가 진짜 중요한 것 같아요",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043462.1.12",
          "form": "맞아요 진짜 ㅋㅋㅋ 결국엔 다 사람끼리 하는일이니까ㅠ 그게 가장 중요한것 같아요ㅋㅋ",
          "original_form": "맞아요 진짜 ㅋㅋㅋ 결국엔 다 사람끼리 하는일이니까ㅠ 그게 가장 중요한것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043462.1.13",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043462.1.14",
          "form": "일은 해치우면 되지만",
          "original_form": "일은 해치우면 되지만",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043462.1.15",
          "form": "관계가 힘들면...",
          "original_form": "관계가 힘들면...",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043462.1.16",
          "form": "맞아요 진짜 ㅋㅋㅋ관계는 진짜.. 걍 둘중하나가 그만 두는 수밖에 없는것같아요",
          "original_form": "맞아요 진짜 ㅋㅋㅋ관계는 진짜.. 걍 둘중하나가 그만 두는 수밖에 없는것같아요",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043462.1.17",
          "form": "네네 관계가 틀어지면 일하기 힘들죠",
          "original_form": "네네 관계가 틀어지면 일하기 힘들죠",
          "speaker_id": "2",
          "time": "20210907 19:16"
        }
