      "utterance": [
        {
          "id": "MDRW2100029648.1.1",
          "form": "이번 휴일이 긴 추석에는 여행계획이 어떻게되시나요???",
          "original_form": "이번 휴일이 긴 추석에는 여행계획이 어떻게되시나요???",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029648.1.2",
          "form": "아직은 계획이 없습니다",
          "original_form": "아직은 계획이 없습니다",
          "speaker_id": "1",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029648.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029648.1.4",
          "form": "저는 집에서 뒹굴거리다가 친정하루 시댁하루 갈예정입니다",
          "original_form": "저는 집에서 뒹굴거리다가 친정하루 시댁하루 갈예정입니다",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029648.1.5",
          "form": "오 서울여행을 하시는군욤",
          "original_form": "오 서울여행을 하시는군욤",
          "speaker_id": "1",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029648.1.6",
          "form": "시골도 가십니까??",
          "original_form": "시골도 가십니까??",
          "speaker_id": "1",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029648.1.7",
          "form": "시골이 따로  없습니다",
          "original_form": "시골이 따로  없습니다",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029648.1.8",
          "form": "다들 서울경기에 있어서",
          "original_form": "다들 서울경기에 있어서",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029648.1.9",
          "form": "시골이 없어요ㅠㅠ",
          "original_form": "시골이 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029648.1.10",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029648.1.11",
          "form": "대전은 안가시나요 이제?",
          "original_form": "대전은 안가시나요 이제?",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029648.1.12",
          "form": "네 대전은 일하러 가는거예요",
          "original_form": "네 대전은 일하러 가는거예요",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029648.1.13",
          "form": "앗....여행이 아니라니....!!!!",
          "original_form": "앗....여행이 아니라니....!!!!",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.14",
          "form": "천안도 할머니 돌아가시고 갈일이 없어요",
          "original_form": "천안도 할머니 돌아가시고 갈일이 없어요",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.15",
          "form": "기념품 없나오",
          "original_form": "기념품 없나오",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.16",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.17",
          "form": "근데 추석여행은",
          "original_form": "근데 추석여행은",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.18",
          "form": "어디든 사람이 몰릴 것 같아요",
          "original_form": "어디든 사람이 몰릴 것 같아요",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.19",
          "form": "대전에는 빵집",
          "original_form": "대전에는 빵집",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.20",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.21",
          "form": "성심당인가요??",
          "original_form": "성심당인가요??",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.22",
          "form": "유명한빵집이 있어요",
          "original_form": "유명한빵집이 있어요",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.24",
          "form": "성심당",
          "original_form": "성심당",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029648.1.25",
          "form": "아 성심당은 못참죠ㅠㅋㅋㅋㅋ",
          "original_form": "아 성심당은 못참죠ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029648.1.26",
          "form": "대전 여행가면 칼국수 먹으라던데요",
          "original_form": "대전 여행가면 칼국수 먹으라던데요",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029648.1.27",
          "form": "맛있나요??",
          "original_form": "맛있나요??",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029648.1.28",
          "form": "저는 잘모르겠어요",
          "original_form": "저는 잘모르겠어요",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029648.1.29",
          "form": "여전에 맛녀석들에서 꿩냉면이랑 비빔국수 맛집있다고 들었어요",
          "original_form": "여전에 맛녀석들에서 꿩냉면이랑 비빔국수 맛집있다고 들었어요",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029648.1.30",
          "form": "비빔칼국수인가???",
          "original_form": "비빔칼국수인가???",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029648.1.31",
          "form": "우와 꿩냉면 진짜 끌리네요",
          "original_form": "우와 꿩냉면 진짜 끌리네요",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029648.1.32",
          "form": "대전 여행 꼭 가야겠네요",
          "original_form": "대전 여행 꼭 가야겠네요",
          "speaker_id": "1",
          "time": "20210903 11:33"
        }
      ]
