      "utterance": [
        {
          "id": "MDRW2100042239.1.1",
          "form": "여행은좋아하세요?",
          "original_form": "여행은좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042239.1.2",
          "form": "여행도 좋하하긴하는데 누가 같이 가자 안하면 귀차니즘땜에 안가더라구요 ㅋㄱㅋ",
          "original_form": "여행도 좋하하긴하는데 누가 같이 가자 안하면 귀차니즘땜에 안가더라구요 ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042239.1.3",
          "form": "매번 여기 가보고 싶다는 하는데 말이죠",
          "original_form": "매번 여기 가보고 싶다는 하는데 말이죠",
          "speaker_id": "2",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042239.1.4",
          "form": "ㅎㅎ가보고싶으신데가어디에요?",
          "original_form": "ㅎㅎ가보고싶으신데가어디에요?",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042239.1.5",
          "form": "사실 맛집 찾아가는 거랑 비슷한 것 같아요",
          "original_form": "사실 맛집 찾아가는 거랑 비슷한 것 같아요",
          "speaker_id": "2",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042239.1.6",
          "form": "ㅎㅎㅎ아하",
          "original_form": "ㅎㅎㅎ아하",
          "speaker_id": "1",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042239.1.7",
          "form": "그럼가봤던곳중에좋으셨던데있나요?",
          "original_form": "그럼가봤던곳중에좋으셨던데있나요?",
          "speaker_id": "1",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042239.1.8",
          "form": "제주도요!!",
          "original_form": "제주도요!!",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042239.1.9",
          "form": "사실 수학여행으론 제주도 갔다온 게 그냥 그랬는데",
          "original_form": "사실 수학여행으론 제주도 갔다온 게 그냥 그랬는데",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042239.1.10",
          "form": "커서 친구들이랑 자유여행?으로 갔다오니깐",
          "original_form": "커서 친구들이랑 자유여행?으로 갔다오니깐",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042239.1.11",
          "form": "제주도가 진짜 좋더라구요",
          "original_form": "제주도가 진짜 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042239.1.12",
          "form": "제주도 좋죠~",
          "original_form": "제주도 좋죠~",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042239.1.13",
          "form": "바다너무이뻐요",
          "original_form": "바다너무이뻐요",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042239.1.14",
          "form": "협재해수욕장",
          "original_form": "협재해수욕장",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042239.1.15",
          "form": "맞아요!! 진짜 바다색이 에메랄드라서 너무 좋았어요",
          "original_form": "맞아요!! 진짜 바다색이 에메랄드라서 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042239.1.16",
          "form": "한라산 도로도 좋더라구요",
          "original_form": "한라산 도로도 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042239.1.17",
          "form": "밥도맛있죠ㅎㅎ",
          "original_form": "밥도맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042239.1.18",
          "form": "맞아요 해물라면이랑 너무 맛있었어요",
          "original_form": "맞아요 해물라면이랑 너무 맛있었어요",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042239.1.19",
          "form": "크",
          "original_form": "크",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042239.1.20",
          "form": "한라산소주드셔보셨나요",
          "original_form": "한라산소주드셔보셨나요",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042239.1.21",
