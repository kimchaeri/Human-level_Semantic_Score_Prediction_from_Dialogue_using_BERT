      "utterance": [
        {
          "id": "MDRW2100015048.1.1",
          "form": "요즘 하는 운동이있나요?",
          "original_form": "요즘 하는 운동이있나요?",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015048.1.2",
          "form": "걷기운동만해요  하는것있나요?",
          "original_form": "걷기운동만해요  하는것있나요?",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015048.1.3",
          "form": "자러간거아니었나용",
          "original_form": "자러간거아니었나용",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100015048.1.4",
          "form": "씻고왓어여",
          "original_form": "씻고왓어여",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015048.1.5",
          "form": "저도 걷기 산책하기 댄스하기",
          "original_form": "저도 걷기 산책하기 댄스하기",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015048.1.6",
          "form": "요새댄스하구싶당!!!",
          "original_form": "요새댄스하구싶당!!!",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015048.1.7",
          "form": "같이하즈아!",
          "original_form": "같이하즈아!",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015048.1.8",
          "form": "다음노래는 24시간이 모자라 문빈 버전이야",
          "original_form": "다음노래는 24시간이 모자라 문빈 버전이야",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015048.1.9",
          "form": "유투브로 보고와",
          "original_form": "유투브로 보고와",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015048.1.10",
          "form": "다이어트 최고",
          "original_form": "다이어트 최고",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015048.1.11",
          "form": "악ㅋㅋㅋㅈㄱㄲㅋㄲ 문빈버전결국하나",
          "original_form": "악ㅋㅋㅋㅈㄱㄲㅋㄲ 문빈버전결국하나",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015048.1.12",
          "form": "아직 허락 못받았다",
          "original_form": "아직 허락 못받았다",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015048.1.13",
          "form": "이제 미드나잇 촬영 남음",
          "original_form": "이제 미드나잇 촬영 남음",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015048.1.14",
          "form": "아 허락은 못받았지만 이미 결정내렸넹",
          "original_form": "아 허락은 못받았지만 이미 결정내렸넹",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015048.1.15",
          "form": "엌ㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015048.1.16",
          "form": "이제 이거 하고 예스올노 할거야수지",
          "original_form": "이제 이거 하고 예스올노 할거야수지",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015048.1.17",
          "form": "ㅋㅋㅋㅋㅋ과감한추진력",
          "original_form": "ㅋㅋㅋㅋㅋ과감한추진력",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015048.1.18",
          "form": "수지?",
          "original_form": "수지?",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015048.1.19",
          "form": "친구랑계속같이?",
          "original_form": "친구랑계속같이?",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015048.1.20",
          "form": "그렇습니다 수지 예스올노 친구가 같이 해줄지 모르겠어요",
          "original_form": "그렇습니다 수지 예스올노 친구가 같이 해줄지 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015048.1.21",
          "form": "우와..",
          "original_form": "우와..",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015048.1.22",
          "form": "너무멀다 안멀면같이가구싶다흑",
          "original_form": "너무멀다 안멀면같이가구싶다흑",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015048.1.23",
          "form": "그치ㅠㅠ",
          "original_form": "그치ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015048.1.24",
          "form": "내가 애프터미드나잇은 알려줄수있어",
          "original_form": "내가 애프터미드나잇은 알려줄수있어",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015048.1.25",
          "form": "집빌때배우러가야하나..ㄱㄲㅋㅋㅋ",
          "original_form": "집빌때배우러가야하나..ㄱㄲㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015048.1.26",
          "form": "운동할거많네",
          "original_form": "운동할거많네",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015048.1.27",
          "form": "저에게 오시면 건강을 선물해드링게요",
          "original_form": "저에게 오시면 건강을 선물해드링게요",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015048.1.28",
          "form": "스피드민턴과 댄스와",
          "original_form": "스피드민턴과 댄스와",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015048.1.29",
          "form": "맛난 식사도 준비해드립니다",
          "original_form": "맛난 식사도 준비해드립니다",
