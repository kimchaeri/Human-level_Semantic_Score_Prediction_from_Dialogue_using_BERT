      "utterance": [
        {
          "id": "MDRW2100020230.1.1",
          "form": "오늘 저녁은 야채곱창",
          "original_form": "오늘 저녁은 야채곱창",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100020230.1.2",
          "form": "오늘 아침은 라면이다",
          "original_form": "오늘 아침은 라면이다",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100020230.1.3",
          "form": "와 진짜 알바 대타 너무 정신없네",
          "original_form": "와 진짜 알바 대타 너무 정신없네",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100020230.1.4",
          "form": "오늘 알바 대타 다녀온거예여?",
          "original_form": "오늘 알바 대타 다녀온거예여?",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100020230.1.5",
          "form": "네 어제 밤까지",
          "original_form": "네 어제 밤까지",
          "speaker_id": "1",
          "time": "20210830 09:46"
        },
        {
          "id": "MDRW2100020230.1.6",
          "form": "어우 기절할것같아요",
          "original_form": "어우 기절할것같아요",
          "speaker_id": "1",
          "time": "20210830 09:46"
        },
        {
          "id": "MDRW2100020230.1.7",
          "form": "와우 사람 많은곳인가보네요",
          "original_form": "와우 사람 많은곳인가보네요",
          "speaker_id": "2",
          "time": "20210830 18:35"
        },
        {
          "id": "MDRW2100020230.1.8",
          "form": "맛집인가요?",
          "original_form": "맛집인가요?",
          "speaker_id": "2",
          "time": "20210830 18:36"
        },
        {
          "id": "MDRW2100020230.1.9",
          "form": "일은 없어도 서있는게 힘드러요ㅜㅜ",
          "original_form": "일은 없어도 서있는게 힘드러요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100020230.1.10",
          "form": "손님 열명도 없어서 너무 힘드렀어요ㅋㅋㅋㅋㅋ",
          "original_form": "손님 열명도 없어서 너무 힘드렀어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100020230.1.11",
          "form": "ㅋㅋㅋㅋ마자요 서있는것도 일이긴 하죠",
          "original_form": "ㅋㅋㅋㅋ마자요 서있는것도 일이긴 하죠",
          "speaker_id": "2",
          "time": "20210831 19:02"
        },
        {
          "id": "MDRW2100020230.1.12",
          "form": "저녁은 뭐먹었어요?",
          "original_form": "저녁은 뭐먹었어요?",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100020230.1.13",
          "form": "저는 김치부침개요~",
          "original_form": "저는 김치부침개요~",
          "speaker_id": "2",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100020230.1.14",
          "form": "혹시 오징어 들어가있었나요",
          "original_form": "혹시 오징어 들어가있었나요",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100020230.1.15",
          "form": "제발",
          "original_form": "제발",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100020230.1.16",
          "form": "아쉽게도 ㅠ",
          "original_form": "아쉽게도 ㅠ",
          "speaker_id": "2",
          "time": "20210901 08:54"
        },
        {
          "id": "MDRW2100020230.1.17",
          "form": "오징어도 없는데 무슨 김치부침개입니까",
          "original_form": "오징어도 없는데 무슨 김치부침개입니까",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100020230.1.18",
          "form": "아니 이름이 김치부침개인데 오징어 안들어갔다고 무시하지마시죠",
          "original_form": "아니 이름이 김치부침개인데 오징어 안들어갔다고 무시하지마시죠",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100020230.1.19",
          "form": "김치찌개는 돼지고기 안들어가면 무시하시자나요",
          "original_form": "김치찌개는 돼지고기 안들어가면 무시하시자나요",
          "speaker_id": "1",
          "time": "20210903 20:35"
        }
      ]
