      "utterance": [
        {
          "id": "MDRW2100004722.1.1",
          "form": "엄마 생일이라 불고기ㅋㅋ",
          "original_form": "엄마 생일이라 불고기ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004722.1.2",
          "form": "오 어머님 생신이시구나 축하축하",
          "original_form": "오 어머님 생신이시구나 축하축하",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004722.1.3",
          "form": "오늘 아니고 다음주 화욜인디 미리 하는거ㅋㅋ",
          "original_form": "오늘 아니고 다음주 화욜인디 미리 하는거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004722.1.4",
          "form": "베라 아이스크림에 초 꼽는다",
          "original_form": "베라 아이스크림에 초 꼽는다",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004722.1.5",
          "form": "그렇구만 맛있는거 많이 먹겠네",
          "original_form": "그렇구만 맛있는거 많이 먹겠네",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004722.1.6",
          "form": "ㅋㅋㅋㅋ 괜찮네 그거",
          "original_form": "ㅋㅋㅋㅋ 괜찮네 그거",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004722.1.7",
          "form": "근데 어제 엄마가 아이스크림 우리 몰래 퍼먹음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 어제 엄마가 아이스크림 우리 몰래 퍼먹음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004722.1.8",
          "form": "몰래 ㅋㅋㅋㅋㅋ 어머님도 좋아하시는구나",
          "original_form": "몰래 ㅋㅋㅋㅋㅋ 어머님도 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004722.1.9",
          "form": "응 초를 꼽았지만 볼품 없네ㅋㅋㅋㅋ",
          "original_form": "응 초를 꼽았지만 볼품 없네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004722.1.10",
          "form": "그치만 아이스크림 케익보단 그게 훨 합리적이야 ㅋㅋ",
          "original_form": "그치만 아이스크림 케익보단 그게 훨 합리적이야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004722.1.11",
          "form": "응 울아빠가 아이스크림통 크기보고 놀람ㅋㅋ",
          "original_form": "응 울아빠가 아이스크림통 크기보고 놀람ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004722.1.12",
          "form": "나도보면 놀랄듯 ㅋㅋㅋㅋㅋㅋ 업소용 아니냐며 ㅋㅋㅋ",
          "original_form": "나도보면 놀랄듯 ㅋㅋㅋㅋㅋㅋ 업소용 아니냐며 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004722.1.13",
          "form": "불고기랑 밥 많이 먹었더니 더부룩하다 탄산수 땡기넴ㅋㅋ",
          "original_form": "불고기랑 밥 많이 먹었더니 더부룩하다 탄산수 땡기넴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004722.1.14",
          "form": "나도 밥을 먹어야겠다 놀뭐 끝남딘 먹어야지 ㅋㅋ",
          "original_form": "나도 밥을 먹어야겠다 놀뭐 끝남딘 먹어야지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004722.1.15",
          "form": "메뉴가 뭐닝",
          "original_form": "메뉴가 뭐닝",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004722.1.16",
          "form": "돈까스 먹을랬는데 엄마가 호박잎 찌셔서 그거 먹어야겠다 ㅋㅋ",
          "original_form": "돈까스 먹을랬는데 엄마가 호박잎 찌셔서 그거 먹어야겠다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004722.1.17",
          "form": "된찌에 호박잎 싸무면 맛있지ㅋㅋㅋ",
          "original_form": "된찌에 호박잎 싸무면 맛있지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004722.1.18",
          "form": "된찌까지 만드셨기에 그걸로 ㅋㅋㅋ",
          "original_form": "된찌까지 만드셨기에 그걸로 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004722.1.19",
          "form": "점심 먹었니",
          "original_form": "점심 먹었니",
          "speaker_id": "1",
