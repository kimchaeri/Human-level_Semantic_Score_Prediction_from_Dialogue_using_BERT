      "utterance": [
        {
          "id": "MDRW2100010903.1.1",
          "form": "태풍이 온다고 해서 난리네요",
          "original_form": "태풍이 온다고 해서 난리네요",
          "speaker_id": "1",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010903.1.2",
          "form": "아까부터 비오더라고요",
          "original_form": "아까부터 비오더라고요",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010903.1.3",
          "form": "경보도 울리고",
          "original_form": "경보도 울리고",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010903.1.4",
          "form": "그러게요 뉴스속보로 계속나오네요",
          "original_form": "그러게요 뉴스속보로 계속나오네요",
          "speaker_id": "1",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100010903.1.5",
          "form": "그래서인지 쌀쌀해지고",
          "original_form": "그래서인지 쌀쌀해지고",
          "speaker_id": "2",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100010903.1.6",
          "form": "조용히 지나가야할텐데요",
          "original_form": "조용히 지나가야할텐데요",
          "speaker_id": "1",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100010903.1.7",
          "form": "좀 약해지긴 한거래요",
          "original_form": "좀 약해지긴 한거래요",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100010903.1.8",
          "form": "남부지방은 비 피해가 클수도...",
          "original_form": "남부지방은 비 피해가 클수도...",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100010903.1.9",
          "form": "제주도 지나면서 약해진거라고 하긴하던데",
          "original_form": "제주도 지나면서 약해진거라고 하긴하던데",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010903.1.10",
          "form": "비가 많이 올까 걱정이예요",
          "original_form": "비가 많이 올까 걱정이예요",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010903.1.11",
          "form": "제주도 지났군요",
          "original_form": "제주도 지났군요",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010903.1.12",
          "form": "태풍영향권이면 저는 회사로 출동해야할듯해서",
          "original_form": "태풍영향권이면 저는 회사로 출동해야할듯해서",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010903.1.13",
          "form": "긴장중입니다 ㅎ",
          "original_form": "긴장중입니다 ㅎ",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010903.1.14",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010903.1.15",
          "form": "업무에 바로 영향을 주는거면....",
          "original_form": "업무에 바로 영향을 주는거면....",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010903.1.16",
          "form": "그래서 속보만 계속 여기저기 돌려보고 있네요 ㅎ",
          "original_form": "그래서 속보만 계속 여기저기 돌려보고 있네요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010903.1.17",
          "form": "정말로 별일 없기를 기도합니다",
          "original_form": "정말로 별일 없기를 기도합니다",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010903.1.18",
          "form": "태풍 이름보니까... 그나라말로 주위를 어슬렁거리는... 이라는 뜻이래요",
          "original_form": "태풍 이름보니까... 그나라말로 주위를 어슬렁거리는... 이라는 뜻이래요",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010903.1.19",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010903.1.20",
          "form": "이름은 생각도 못했어요",
          "original_form": "이름은 생각도 못했어요",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010903.1.21",
          "form": "딱봐도 약해보이는 이름이에요",
          "original_form": "딱봐도 약해보이는 이름이에요",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010903.1.22",
          "form": "누가지었는지!!!",
          "original_form": "누가지었는지!!!",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010903.1.23",
          "form": "미국애들이래요",
          "original_form": "미국애들이래요",
          "speaker_id": "2",
