      "utterance": [
        {
          "id": "MDRW2100035137.1.1",
          "form": "좋아하는 카페 체인은요?",
          "original_form": "좋아하는 카페 체인은요?",
          "speaker_id": "2",
          "time": "20210829 08:56"
        },
        {
          "id": "MDRW2100035137.1.2",
          "form": "저 스타벅스 좋아해용!ㅎㅎ",
          "original_form": "저 스타벅스 좋아해용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 08:56"
        },
        {
          "id": "MDRW2100035137.1.3",
          "form": "당근케이크 넘 맛있어서ㅠㅠㅋㅋ",
          "original_form": "당근케이크 넘 맛있어서ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:56"
        },
        {
          "id": "MDRW2100035137.1.4",
          "form": "당근케이크 맛있나요 안먹어봤어요",
          "original_form": "당근케이크 맛있나요 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210829 08:58"
        },
        {
          "id": "MDRW2100035137.1.5",
          "form": "네네 사실 당근보다는 시나몬 향이 더 많이 나긴하지만ㅋㅋㅋ",
          "original_form": "네네 사실 당근보다는 시나몬 향이 더 많이 나긴하지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:58"
        },
        {
          "id": "MDRW2100035137.1.6",
          "form": "맛있어요!",
          "original_form": "맛있어요!",
          "speaker_id": "1",
          "time": "20210829 08:58"
        },
        {
          "id": "MDRW2100035137.1.7",
          "form": "저는 아이가 생크림 카스테라?그거 좋아해서 종종 먹었어요",
          "original_form": "저는 아이가 생크림 카스테라?그거 좋아해서 종종 먹었어요",
          "speaker_id": "2",
          "time": "20210829 08:59"
        },
        {
          "id": "MDRW2100035137.1.8",
          "form": "와아 카스테라 넘 맛있져ㅠㅠ",
          "original_form": "와아 카스테라 넘 맛있져ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 08:59"
        },
        {
          "id": "MDRW2100035137.1.9",
          "form": "주로 카페가면 무슨 음료 드세요?",
          "original_form": "주로 카페가면 무슨 음료 드세요?",
          "speaker_id": "1",
          "time": "20210829 09:00"
        },
        {
          "id": "MDRW2100035137.1.10",
          "form": "저는 두유라떼 종종 먹어요!",
          "original_form": "저는 두유라떼 종종 먹어요!",
          "speaker_id": "2",
          "time": "20210829 09:00"
        },
        {
          "id": "MDRW2100035137.1.11",
          "form": "저도 고소한거 땡낄때 우유대신",
          "original_form": "저도 고소한거 땡낄때 우유대신",
          "speaker_id": "1",
          "time": "20210829 09:00"
        },
        {
          "id": "MDRW2100035137.1.12",
          "form": "두유로",
          "original_form": "두유로",
          "speaker_id": "1",
          "time": "20210829 09:01"
        },
        {
          "id": "MDRW2100035137.1.13",
          "form": "변경해서 먹어용ㅋㅋ",
          "original_form": "변경해서 먹어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:01"
        },
        {
          "id": "MDRW2100035137.1.14",
          "form": "스타벅스는 그런점이 좋더라구요 ^^",
          "original_form": "스타벅스는 그런점이 좋더라구요 ^^",
          "speaker_id": "2",
          "time": "20210829 09:01"
        },
        {
          "id": "MDRW2100035137.1.15",
          "form": "스타벅스 종이빨대가 음료 맛을 해치는것같아요ㅠㅠ",
          "original_form": "스타벅스 종이빨대가 음료 맛을 해치는것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 09:02"
        },
        {
          "id": "MDRW2100035137.1.16",
          "form": "아 그런 평들이 있더라구요 프라프치노 먹을때 안습 ㅠㅠ",
          "original_form": "아 그런 평들이 있더라구요 프라프치노 먹을때 안습 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 09:03"
        },
        {
          "id": "MDRW2100035137.1.17",
          "form": "네네ㅜㅜ 음료는 넘 맛있는데",
