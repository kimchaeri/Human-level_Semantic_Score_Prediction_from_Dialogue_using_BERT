      "utterance": [
        {
          "id": "MDRW2100041689.1.1",
          "form": "고기 좋아하죠!!",
          "original_form": "고기 좋아하죠!!",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041689.1.2",
          "form": "좋아하시죠?",
          "original_form": "좋아하시죠?",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041689.1.3",
          "form": "사실 안좋아하는 사람이 있나요ㅋㅋㅋ",
          "original_form": "사실 안좋아하는 사람이 있나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041689.1.4",
          "form": "채식 하실수도 있긴 한뎈ㅋㅋㅋ",
          "original_form": "채식 하실수도 있긴 한뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041689.1.5",
          "form": "보통은 ㅋㅋㅋㅋ",
          "original_form": "보통은 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041689.1.6",
          "form": "ㅋㅋㅋ 전 보쌈 족발 좋아해요",
          "original_form": "ㅋㅋㅋ 전 보쌈 족발 좋아해요",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041689.1.7",
          "form": "보쌈 족발 저도 좋아해요",
          "original_form": "보쌈 족발 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041689.1.8",
          "form": "보쌈은 김장철에 먹는게 제일인데",
          "original_form": "보쌈은 김장철에 먹는게 제일인데",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041689.1.9",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041689.1.10",
          "form": "맞아요 김장하는날 꼭 보쌈햇어요",
          "original_form": "맞아요 김장하는날 꼭 보쌈햇어요",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100041689.1.11",
          "form": "배추가 맛있으면 더 맛있더라고요",
          "original_form": "배추가 맛있으면 더 맛있더라고요",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041689.1.12",
          "form": "근데 전에 놀면 뭐하니에서 김장하면서 라면을 먹는거에요",
          "original_form": "근데 전에 놀면 뭐하니에서 김장하면서 라면을 먹는거에요",
          "speaker_id": "1",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041689.1.13",
          "form": "그후론 김장날은 라면으로 바꿨어요ㅋㅋ",
          "original_form": "그후론 김장날은 라면으로 바꿨어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041689.1.14",
          "form": "ㅋㅋㅋㅋ 놀면뭐하니에서 김장하면서 라면을 먹었어요??",
          "original_form": "ㅋㅋㅋㅋ 놀면뭐하니에서 김장하면서 라면을 먹었어요??",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041689.1.15",
          "form": "네 어찌나 맛잇게먹던지",
          "original_form": "네 어찌나 맛잇게먹던지",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041689.1.16",
          "form": "김장하는게 나왔다는것도 신기하네옄ㄴㅋㅋ",
          "original_form": "김장하는게 나왔다는것도 신기하네옄ㄴㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041689.1.17",
          "form": "저는 노래 자주하길래 안보거든요",
          "original_form": "저는 노래 자주하길래 안보거든요",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041689.1.18",
          "form": "제가 생각했던 느낌이 아니네욬ㅋㅋㅋ",
          "original_form": "제가 생각했던 느낌이 아니네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041689.1.19",
          "form": "먹은게 한 2년된거같아요",
          "original_form": "먹은게 한 2년된거같아요",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041689.1.20",
          "form": "그렇구나!! 그래서 올해는 라면이신가요 보쌈이신가요",
          "original_form": "그렇구나!! 그래서 올해는 라면이신가요 보쌈이신가요",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041689.1.21",
          "form": "사실 손 덜가는건 라면이긴 한데 ㅎㅎㅎ",
          "original_form": "사실 손 덜가는건 라면이긴 한데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041689.1.22",
          "form": "고르기 어렵네요 둘다 맛있어요",
          "original_form": "고르기 어렵네요 둘다 맛있어요",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041689.1.23",
          "form": "사실보쌈은 막국수죠ㅋㅋ",
          "original_form": "사실보쌈은 막국수죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041689.1.24",
          "form": "저는 그래도 보쌈 하겠습니닼ㅋㅋ",
          "original_form": "저는 그래도 보쌈 하겠습니닼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041689.1.25",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041689.1.26",
          "form": "전 근데 족발에도 막국수",
          "original_form": "전 근데 족발에도 막국수",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041689.1.27",
          "form": "막국수 언제 먹어도 맛있어요!! ㅎㅎㅎㅎ",
          "original_form": "막국수 언제 먹어도 맛있어요!! ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041689.1.28",
          "form": "보족과 막국수는 세트죠",
          "original_form": "보족과 막국수는 세트죠",
          "speaker_id": "1",
          "time": "20210906 14:38"
        }
      ]
