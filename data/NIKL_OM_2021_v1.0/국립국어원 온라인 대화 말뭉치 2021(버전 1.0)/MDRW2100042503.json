      "utterance": [
        {
          "id": "MDRW2100042503.1.1",
          "form": "고시식당 무한리필에 식빵 라면 무제한",
          "original_form": "고시식당 무한리필에 식빵 라면 무제한",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042503.1.2",
          "form": "와 배고플 때 가면",
          "original_form": "와 배고플 때 가면",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042503.1.3",
          "form": "진짜 천국이겠네요",
          "original_form": "진짜 천국이겠네요",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042503.1.4",
          "form": "후기검색하고 와보세요ㅋㅋ",
          "original_form": "후기검색하고 와보세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042503.1.5",
          "form": "저도 오년전쯤 가보고 ㅋㅋ",
          "original_form": "저도 오년전쯤 가보고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042503.1.6",
          "form": "전에 티비에 나오는 건 봤는데",
          "original_form": "전에 티비에 나오는 건 봤는데",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042503.1.7",
          "form": "박리다매",
          "original_form": "박리다매",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042503.1.8",
          "form": "name2님 얘기 들으니 더 좋아보이네요",
          "original_form": "&name2&님 얘기 들으니 더 좋아보이네요",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042503.1.9",
          "form": "저도 노량진 컵밥은 거의 10년전에 먹어봤어요ㅋㅋ",
          "original_form": "저도 노량진 컵밥은 거의 10년전에 먹어봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042503.1.10",
          "form": "회사에서 식사 구내식당 이용하시나요??",
          "original_form": "회사에서 식사 구내식당 이용하시나요??",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042503.1.11",
          "form": "그럼 비교되어 별로일수 있어요ㅋㅋ",
          "original_form": "그럼 비교되어 별로일수 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042503.1.12",
          "form": "네 구내식당ㅋㅋ",
          "original_form": "네 구내식당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042503.1.13",
          "form": "아 그럴까요?",
          "original_form": "아 그럴까요?",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042503.1.14",
          "form": "그거에 저렴이버젼",
          "original_form": "그거에 저렴이버젼",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042503.1.15",
          "form": "하긴 고시식당은 싼값에 가성비를 느끼는 곳이니",
          "original_form": "하긴 고시식당은 싼값에 가성비를 느끼는 곳이니",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042503.1.16",
          "form": "네네 이미 비싸진 입맛은\r\n다시 저렴해질수 없어요ㅋㅋ",
          "original_form": "네네 이미 비싸진 입맛은\r\n다시 저렴해질수 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042503.1.17",
          "form": "아앗 그렇긴하죠",
          "original_form": "아앗 그렇긴하죠",
          "speaker_id": "1",
