      "utterance": [
        {
          "id": "MDRW2100007748.1.1",
          "form": "결혼이라는 주제에 대해 어떻게 생각하시나요?",
          "original_form": "결혼이라는 주제에 대해 어떻게 생각하시나요?",
          "speaker_id": "1",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100007748.1.2",
          "form": "아 참고로 저는 결혼 안했습니다",
          "original_form": "아 참고로 저는 결혼 안했습니다",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007748.1.3",
          "form": "저도 미혼이에요!",
          "original_form": "저도 미혼이에요!",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100007748.1.4",
          "form": "음 예전엔 언젠간 막연히 하겠다 싶었는데 요샌 그냥 할수도 안할수도.. 이런 느낌이에요",
          "original_form": "음 예전엔 언젠간 막연히 하겠다 싶었는데 요샌 그냥 할수도 안할수도.. 이런 느낌이에요",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100007748.1.5",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007748.1.6",
          "form": "굳이 해야 할 필요를 못느낀다랄까",
          "original_form": "굳이 해야 할 필요를 못느낀다랄까",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007748.1.7",
          "form": "예전이랑은 확실히 인식이 달라졌어요",
          "original_form": "예전이랑은 확실히 인식이 달라졌어요",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007748.1.8",
          "form": "맞아요 굳이 필요할까 싶기도 하고",
          "original_form": "맞아요 굳이 필요할까 싶기도 하고",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100007748.1.9",
          "form": "결혼관이 변하나봐요",
          "original_form": "결혼관이 변하나봐요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007748.1.10",
          "form": "그래서 그런가 그게 좀 있어요",
          "original_form": "그래서 그런가 그게 좀 있어요",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007748.1.11",
          "form": "뭔가 주위에 하나 둘 결혼하는 거 보면 진심 안부러운데 묘하다랄까",
          "original_form": "뭔가 주위에 하나 둘 결혼하는 거 보면 진심 안부러운데 묘하다랄까",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007748.1.12",
          "form": "난 아직도 애같은데 친구가 결혼을 한다고?",
          "original_form": "난 아직도 애같은데 친구가 결혼을 한다고?",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007748.1.13",
          "form": "어 맞아요ㅋㅋㅋ부럽거나 그런건 아닌데",
          "original_form": "어 맞아요ㅋㅋㅋ부럽거나 그런건 아닌데",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100007748.1.14",
          "form": "신기하다 생각도 들고",
          "original_form": "신기하다 생각도 들고",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100007748.1.15",
          "form": "그래서 와 니가 결혼을 한다고?",
          "original_form": "그래서 와 니가 결혼을 한다고?",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100007748.1.16",
          "form": "했는데 이제 그 친구가 애도낳아",
          "original_form": "했는데 이제 그 친구가 애도낳아",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100007748.1.17",
          "form": "그러니까요ㅋㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100007748.1.18",
          "form": "와우 뭔가 묘해요",
          "original_form": "와우 뭔가 묘해요",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007748.1.19",
          "form": "뭔가 더 어른같은 느낌은 있엉ㅎ",
          "original_form": "뭔가 더 어른같은 느낌은 있엉ㅎ",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007748.1.20",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007748.1.21",
          "form": "그렇다고 제가 결혼이나 육아를 하고 샆은건 아니지만",
          "original_form": "그렇다고 제가 결혼이나 육아를 하고 샆은건 아니지만",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007748.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007748.1.23",
          "form": "제가 하기는 싫은데",
          "original_form": "제가 하기는 싫은데",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007748.1.24",
          "form": "남이 하는거 보면 와우 소리가 절로 나오는",
          "original_form": "남이 하는거 보면 와우 소리가 절로 나오는",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007748.1.25",
          "form": "맞아요ㅋㅋㅋ기분이 묘하긴 해요",
          "original_form": "맞아요ㅋㅋㅋ기분이 묘하긴 해요",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100007748.1.26",
          "form": "진짜 나빼고 다 사라지는 기분",
          "original_form": "진짜 나빼고 다 사라지는 기분",
          "speaker_id": "1",
          "time": "20210822 15:39"
        }
      ]
    }
