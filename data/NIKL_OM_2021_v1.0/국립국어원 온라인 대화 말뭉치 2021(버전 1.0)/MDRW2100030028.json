        {
          "id": "MDRW2100030028.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210903 19:00"
        },
        {
          "id": "MDRW2100030028.1.2",
          "form": "넹 코로나때문에 못 가는 게 아쉽네요",
          "original_form": "넹 코로나때문에 못 가는 게 아쉽네요",
          "speaker_id": "2",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100030028.1.3",
          "form": "최근에 여행 간적 있으세요?",
          "original_form": "최근에 여행 간적 있으세요?",
          "speaker_id": "2",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100030028.1.4",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100030028.1.5",
          "form": "오 저는 영종도요",
          "original_form": "오 저는 영종도요",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100030028.1.6",
          "form": "남해 좋나요",
          "original_form": "남해 좋나요",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100030028.1.7",
          "form": "바다좋아여",
          "original_form": "바다좋아여",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100030028.1.8",
          "form": "와 그러네요 여름엔 바다를 봐줘야죠",
          "original_form": "와 그러네요 여름엔 바다를 봐줘야죠",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100030028.1.9",
          "form": "해외여행은 어디어디 가보셨나요?",
          "original_form": "해외여행은 어디어디 가보셨나요?",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100030028.1.10",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100030028.1.11",
          "form": "우와 호주 좋겠네요.....",
          "original_form": "우와 호주 좋겠네요.....",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100030028.1.12",
          "form": "캥거루 보셨나요",
          "original_form": "캥거루 보셨나요",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100030028.1.13",
          "form": "코알라도요",
          "original_form": "코알라도요",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100030028.1.14",
          "form": "와 짱이네요...",
          "original_form": "와 짱이네요...",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100030028.1.15",
          "form": "생각보다 크더라구요",
          "original_form": "생각보다 크더라구요",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100030028.1.16",
          "form": "아 진짜요? 반전이네요",
          "original_form": "아 진짜요? 반전이네요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
