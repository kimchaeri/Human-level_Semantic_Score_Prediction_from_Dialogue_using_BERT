      "utterance": [
        {
          "id": "MDRW2100032555.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 13:48"
        },
        {
          "id": "MDRW2100032555.1.2",
          "form": "ㅎㅎ넵 드셨어요?",
          "original_form": "ㅎㅎ넵 드셨어요?",
          "speaker_id": "1",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100032555.1.3",
          "form": "네 지금은 저녁먹으려고 준비중이에요 ㅋㅋ",
          "original_form": "네 지금은 저녁먹으려고 준비중이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100032555.1.4",
          "form": "점심을 대충 떼웠더니 빨리 배고파졌네요",
          "original_form": "점심을 대충 떼웠더니 빨리 배고파졌네요",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100032555.1.5",
          "form": "ㅎㅎ메뉴가무엇인가요",
          "original_form": "ㅎㅎ메뉴가무엇인가요",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100032555.1.6",
          "form": "까르보나라에요!! 저번에 밀키트 사온 게 있어서 만들고 그걸로 만들었머요",
          "original_form": "까르보나라에요!! 저번에 밀키트 사온 게 있어서 만들고 그걸로 만들었머요",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100032555.1.7",
          "form": "오왕",
          "original_form": "오왕",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100032555.1.8",
          "form": "밀키트 요새 맛있는거 많더라고요",
          "original_form": "밀키트 요새 맛있는거 많더라고요",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100032555.1.9",
          "form": "맞아요 집에서 맛있는 걸 해먹을 수 있게 돼서 좋더라구요",
          "original_form": "맞아요 집에서 맛있는 걸 해먹을 수 있게 돼서 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100032555.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100032555.1.11",
          "form": "인스턴트랑은확실히다르더라고요",
          "original_form": "인스턴트랑은확실히다르더라고요",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100032555.1.12",
          "form": "맞아요!! 그래서 아주 만족스러워요",
          "original_form": "맞아요!! 그래서 아주 만족스러워요",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100032555.1.13",
          "form": "어떤 거 먹어보셨나요??",
          "original_form": "어떤 거 먹어보셨나요??",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100032555.1.14",
          "form": "저는 떡볶이요!",
          "original_form": "저는 떡볶이요!",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100032555.1.15",
          "form": "야채썰려서들어있어서좋았어요!",
          "original_form": "야채썰려서들어있어서좋았어요!",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100032555.1.16",
          "form": "맞아요 손질해야하는 것들 다 손질 되어있어서",
          "original_form": "맞아요 손질해야하는 것들 다 손질 되어있어서",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100032555.1.17",
          "form": "거의 그냥 불만 있으면 돼서 너무 편하죠",
          "original_form": "거의 그냥 불만 있으면 돼서 너무 편하죠",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100032555.1.18",
          "form": "맛도 나름 괜찮구요!",
          "original_form": "맛도 나름 괜찮구요!",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100032555.1.19",
          "form": "ㅎㅎ맞아요 배달도드세요?",
          "original_form": "ㅎㅎ맞아요 배달도드세요?",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100032555.1.20",
          "form": "배달도 많이 먹죠",
          "original_form": "배달도 많이 먹죠",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100032555.1.21",
          "form": "줄여야한다곤 생각하는데",
          "original_form": "줄여야한다곤 생각하는데",
          "speaker_id": "2",
          "time": "20210906 18:05"
        }
