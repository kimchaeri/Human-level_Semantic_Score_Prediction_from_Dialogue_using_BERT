      "utterance": [
        {
          "id": "MDRW2100015880.1.1",
          "form": "그렇군요! 도전해봐야겟어요",
          "original_form": "그렇군요! 도전해봐야겟어요",
          "speaker_id": "2",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015880.1.2",
          "form": "롤케익도 맛있더라구요",
          "original_form": "롤케익도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 02:19"
        },
        {
          "id": "MDRW2100015880.1.3",
          "form": "함 드셔보세요 전 좋아하는빵이라 남아있음 무조건 담아요",
          "original_form": "함 드셔보세요 전 좋아하는빵이라 남아있음 무조건 담아요",
          "speaker_id": "1",
          "time": "20210826 02:19"
        },
        {
          "id": "MDRW2100015880.1.4",
          "form": "샐러드도 괜찮더라구요",
          "original_form": "샐러드도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015880.1.5",
          "form": "제값주고 먹을일 없는데 행사할때 가끔 사먹어요",
          "original_form": "제값주고 먹을일 없는데 행사할때 가끔 사먹어요",
          "speaker_id": "1",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015880.1.6",
          "form": "맞아요 샐러드 맛있어요 ㅠㅠ 근데 양대비 가격이...사악하더라구여",
          "original_form": "맞아요 샐러드 맛있어요 ㅠㅠ 근데 양대비 가격이...사악하더라구여",
          "speaker_id": "2",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015880.1.7",
          "form": "그래서 행사할때ㅋ",
          "original_form": "그래서 행사할때ㅋ",
          "speaker_id": "1",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015880.1.8",
          "form": "이거땜에 슈클 가입했어요",
          "original_form": "이거땜에 슈클 가입했어요",
          "speaker_id": "1",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015880.1.9",
          "form": "오 그렇군요 저도 슈클하려다가",
          "original_form": "오 그렇군요 저도 슈클하려다가",
          "speaker_id": "2",
          "time": "20210826 02:24"
        },
        {
          "id": "MDRW2100015880.1.10",
          "form": "너무 배달시켜서 살찔가봐...ㅎ",
          "original_form": "너무 배달시켜서 살찔가봐...ㅎ",
          "speaker_id": "2",
          "time": "20210826 02:24"
        },
        {
          "id": "MDRW2100015880.1.11",
          "form": "배달은 배달비나와서 포장해요",
          "original_form": "배달은 배달비나와서 포장해요",
          "speaker_id": "1",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015880.1.12",
          "form": "우유사시는분도 많던데",
          "original_form": "우유사시는분도 많던데",
          "speaker_id": "1",
          "time": "20210826 02:27"
        },
        {
          "id": "MDRW2100015880.1.13",
          "form": "맞아요 배달비 좀 아까워서 저두 가까운데는 포장하려구해요",
          "original_form": "맞아요 배달비 좀 아까워서 저두 가까운데는 포장하려구해요",
          "speaker_id": "2",
          "time": "20210826 02:28"
        },
        {
          "id": "MDRW2100015880.1.14",
          "form": "빵얘기하니 먹고싶네요",
          "original_form": "빵얘기하니 먹고싶네요",
          "speaker_id": "1",
          "time": "20210826 02:29"
        },
        {
          "id": "MDRW2100015880.1.15",
          "form": "저두요..크로와상이랑 앙버터먹굽네요",
          "original_form": "저두요..크로와상이랑 앙버터먹굽네요",
          "speaker_id": "2",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015880.1.16",
          "form": "지금요?ㅎㅎ",
          "original_form": "지금요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015880.1.17",
          "form": "아 먹고싶다고ㅎ",
          "original_form": "아 먹고싶다고ㅎ",
          "speaker_id": "1",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015880.1.18",
          "form": "전 굽고있다는줄ㅋㅋ",
          "original_form": "전 굽고있다는줄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015880.1.19",
          "form": "앙버터 어디맛난데 있던데",
          "original_form": "앙버터 어디맛난데 있던데",
          "speaker_id": "1",
          "time": "20210826 02:31"
        },
        {
          "id": "MDRW2100015880.1.20",
          "form": "이름을 모르겠네요 체인이였는데",
          "original_form": "이름을 모르겠네요 체인이였는데",
          "speaker_id": "1",
          "time": "20210826 02:31"
        },
        {
          "id": "MDRW2100015880.1.21",
          "form": "아진짜요 궁금하네요 ㅜㅜ",
          "original_form": "아진짜요 궁금하네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 02:33"
        },
        {
          "id": "MDRW2100015880.1.22",
          "form": "갑자기 배고프네요..",
          "original_form": "갑자기 배고프네요..",
          "speaker_id": "2",
          "time": "20210826 02:33"
        },
        {
          "id": "MDRW2100015880.1.23",
          "form": "여수가서 먹어서 더맛있게  먹은걸지도^^",
          "original_form": "여수가서 먹어서 더맛있게  먹은걸지도^^",
          "speaker_id": "1",
          "time": "20210826 02:35"
        },
        {
          "id": "MDRW2100015880.1.24",
          "form": "크 여수 그럴수도있겠네요 ㅋㅋㅋㅋ분위기때문에",
          "original_form": "크 여수 그럴수도있겠네요 ㅋㅋㅋㅋ분위기때문에",
          "speaker_id": "2",
          "time": "20210826 02:37"
        },
        {
          "id": "MDRW2100015880.1.25",
          "form": "마자요ㅎㅎ",
          "original_form": "마자요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 02:37"
        },
        {
          "id": "MDRW2100015880.1.26",
          "form": "여수 맛집 아세요?",
          "original_form": "여수 맛집 아세요?",
          "speaker_id": "2",
          "time": "20210826 02:39"
        },
        {
          "id": "MDRW2100015880.1.27",
          "form": "간장게장먹고",
          "original_form": "간장게장먹고",
          "speaker_id": "1",
          "time": "20210826 02:39"
        },
        {
          "id": "MDRW2100015880.1.28",
          "form": "순천광양 여수 돌아댕겨서 어딘지몰겠어요",
          "original_form": "순천광양 여수 돌아댕겨서 어딘지몰겠어요",
          "speaker_id": "1",
          "time": "20210826 02:40"
        },
        {
          "id": "MDRW2100015880.1.29",
          "form": "재첩무침 먹고싶네요ㅎ",
          "original_form": "재첩무침 먹고싶네요ㅎ",
          "speaker_id": "1",
          "time": "20210826 02:40"
        },
        {
          "id": "MDRW2100015880.1.30",
          "form": "비빔밥",
          "original_form": "비빔밥",
          "speaker_id": "1",
          "time": "20210826 02:40"
        },
        {
          "id": "MDRW2100015880.1.31",
          "form": "아 비빔밥...계란후라이얹어서 ㅜㅜㅜ",
          "original_form": "아 비빔밥...계란후라이얹어서 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 02:40"
        },
        {
          "id": "MDRW2100015880.1.32",
          "form": "도다리쑥국ㅎ",
          "original_form": "도다리쑥국ㅎ",
          "speaker_id": "1",
          "time": "20210826 02:41"
        },
        {
          "id": "MDRW2100015880.1.33",
          "form": "배고프네요...",
          "original_form": "배고프네요...",
          "speaker_id": "2",
          "time": "20210826 02:41"
        },
        {
          "id": "MDRW2100015880.1.34",
          "form": "도다리 회도 먹고ㅎ",
          "original_form": "도다리 회도 먹고ㅎ",
          "speaker_id": "1",
          "time": "20210826 02:41"
        },
        {
          "id": "MDRW2100015880.1.35",
          "form": "여행가고싶네요",
          "original_form": "여행가고싶네요",
