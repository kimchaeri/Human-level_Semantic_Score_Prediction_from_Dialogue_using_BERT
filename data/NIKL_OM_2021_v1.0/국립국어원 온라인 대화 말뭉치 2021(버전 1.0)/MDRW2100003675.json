      "utterance": [
        {
          "id": "MDRW2100003675.1.1",
          "form": "비가 많이 와요",
          "original_form": "비가 많이 와요",
          "speaker_id": "1",
          "time": "20210821 10:36"
        },
        {
          "id": "MDRW2100003675.1.2",
          "form": "네 막 쏟아지네요",
          "original_form": "네 막 쏟아지네요",
          "speaker_id": "2",
          "time": "20210821 10:37"
        },
        {
          "id": "MDRW2100003675.1.3",
          "form": "네 가을장마라고 하더라고요",
          "original_form": "네 가을장마라고 하더라고요",
          "speaker_id": "1",
          "time": "20210821 10:37"
        },
        {
          "id": "MDRW2100003675.1.4",
          "form": "네 전 이비에 운전하고 치과가느라 비를 차로 느끼고 있어요",
          "original_form": "네 전 이비에 운전하고 치과가느라 비를 차로 느끼고 있어요",
          "speaker_id": "2",
          "time": "20210821 10:38"
        },
        {
          "id": "MDRW2100003675.1.5",
          "form": "아~~차 안에서는 낭만 있을것 같아요 ㅎ",
          "original_form": "아~~차 안에서는 낭만 있을것 같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 10:39"
        },
        {
          "id": "MDRW2100003675.1.6",
          "form": "아니요 ㅋㅋ넘 웨험해서 김장돼요",
          "original_form": "아니요 ㅋㅋ넘 웨험해서 김장돼요",
          "speaker_id": "2",
          "time": "20210821 10:39"
        },
        {
          "id": "MDRW2100003675.1.7",
          "form": "치과가 멀어요 2시간 더걸려요",
          "original_form": "치과가 멀어요 2시간 더걸려요",
          "speaker_id": "2",
          "time": "20210821 10:39"
        },
        {
          "id": "MDRW2100003675.1.8",
          "form": "아~빗길운전이 위험하죠ㅠ ㅠ",
          "original_form": "아~빗길운전이 위험하죠ㅠ ㅠ",
          "speaker_id": "1",
          "time": "20210821 10:39"
        },
        {
          "id": "MDRW2100003675.1.9",
          "form": "네 운전할땐 날씨가 너무 중요한거 같아요",
          "original_form": "네 운전할땐 날씨가 너무 중요한거 같아요",
          "speaker_id": "2",
          "time": "20210821 10:40"
        },
        {
          "id": "MDRW2100003675.1.10",
          "form": "눈 비 너무웨험해요",
          "original_form": "눈 비 너무웨험해요",
          "speaker_id": "2",
          "time": "20210821 10:40"
        },
        {
          "id": "MDRW2100003675.1.11",
          "form": "위험",
          "original_form": "위험",
          "speaker_id": "2",
          "time": "20210821 10:40"
        },
        {
          "id": "MDRW2100003675.1.12",
          "form": "예전에 눈 많이 올때 차에 갇힌적도 있어요",
          "original_form": "예전에 눈 많이 올때 차에 갇힌적도 있어요",
          "speaker_id": "2",
          "time": "20210821 10:40"
        },
        {
          "id": "MDRW2100003675.1.13",
          "form": "헙 눈 비 많이 올땐 집에 있어야ㅠ",
          "original_form": "헙 눈 비 많이 올땐 집에 있어야ㅠ",
          "speaker_id": "1",
          "time": "20210821 10:41"
        },
        {
          "id": "MDRW2100003675.1.14",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210821 10:41"
        },
        {
          "id": "MDRW2100003675.1.15",
          "form": "요샌 그렇게는 안오는것 같아요",
          "original_form": "요샌 그렇게는 안오는것 같아요",
          "speaker_id": "2",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003675.1.16",
          "form": "극한의 동포를 느꼈었어요",
          "original_form": "극한의 동포를 느꼈었어요",
          "speaker_id": "2",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003675.1.17",
          "form": "공포",
          "original_form": "공포",
          "speaker_id": "2",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003675.1.18",
          "form": "옛날인가요?",
          "original_form": "옛날인가요?",
          "speaker_id": "1",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003675.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003675.1.20",
          "form": "18년전요 ㅋㅋ",
          "original_form": "18년전요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003675.1.21",
          "form": "연료가 막 떨어지고 그래서요",
          "original_form": "연료가 막 떨어지고 그래서요",
          "speaker_id": "2",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003675.1.22",
