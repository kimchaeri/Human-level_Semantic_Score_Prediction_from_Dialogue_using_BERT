        },
        {
          "id": "MDRW2100012196.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100012196.1.3",
          "form": "어떤 책 좋아하시나요",
          "original_form": "어떤 책 좋아하시나요",
          "speaker_id": "2",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100012196.1.4",
          "form": "저는 에세이 좋아해요",
          "original_form": "저는 에세이 좋아해요",
          "speaker_id": "1",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012196.1.5",
          "form": "name2은 어떤책 좋아하세요",
          "original_form": "&name2&은 어떤책 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100012196.1.6",
          "form": "저는  스릴러  관련된 책 좋아해요",
          "original_form": "저는  스릴러  관련된 책 좋아해요",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012196.1.7",
          "form": "혹시 지금 어떤 책 읽으시나요",
          "original_form": "혹시 지금 어떤 책 읽으시나요",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012196.1.8",
          "form": "저는 오늘 서른의품격 책 빌려왔어요",
          "original_form": "저는 오늘 서른의품격 책 빌려왔어요",
          "speaker_id": "1",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012196.1.9",
          "form": "어떤 내용일지 기대되요",
          "original_form": "어떤 내용일지 기대되요",
          "speaker_id": "1",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012196.1.10",
          "form": "좋은 책 읽으시네요",
          "original_form": "좋은 책 읽으시네요",
          "speaker_id": "2",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012196.1.11",
          "form": "원래는 양희은 그러라그래",
          "original_form": "원래는 양희은 그러라그래",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012196.1.12",
          "form": "빌려오고싶었는데 대출중이라",
          "original_form": "빌려오고싶었는데 대출중이라",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012196.1.13",
          "form": "책빌리기도 쉽지 않군요",
          "original_form": "책빌리기도 쉽지 않군요",
          "speaker_id": "2",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100012196.1.14",
          "form": "저는 예전 천사와악마 같은 책 좋아해요",
          "original_form": "저는 예전 천사와악마 같은 책 좋아해요",
          "speaker_id": "2",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100012196.1.15",
          "form": "ㅎㅎ천사와악마 책이름은 기억나는데 내용이 기억이나지않네요...",
          "original_form": "ㅎㅎ천사와악마 책이름은 기억나는데 내용이 기억이나지않네요...",
          "speaker_id": "1",
          "time": "20210824 20:22"
        },
        {
          "id": "MDRW2100012196.1.16",
          "form": "그런가요 검색해보면 아실듯해요",
          "original_form": "그런가요 검색해보면 아실듯해요",
          "speaker_id": "2",
          "time": "20210824 20:22"
        },
        {
          "id": "MDRW2100012196.1.17",
          "form": "ㅎㅎ검색해봐야겠어요. 궁금하네요",
          "original_form": "ㅎㅎ검색해봐야겠어요. 궁금하네요",
          "speaker_id": "1",
          "time": "20210824 20:23"
        },
        {
          "id": "MDRW2100012196.1.18",
          "form": "검색해 보시면 금방 알아요",
          "original_form": "검색해 보시면 금방 알아요",
          "speaker_id": "2",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012196.1.19",
          "form": "ㅎㅎ네. 최근에는 보신책 있으신가요",
          "original_form": "ㅎㅎ네. 최근에는 보신책 있으신가요",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012196.1.20",
          "form": "네 라이트노벨 책 본 것 같네요",
          "original_form": "네 라이트노벨 책 본 것 같네요",
