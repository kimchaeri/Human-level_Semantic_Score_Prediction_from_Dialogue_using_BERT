      "utterance": [
        {
          "id": "MDRW2100015730.1.1",
          "form": "그 톰브라운이라고 유명한 상표 있어요",
          "original_form": "그 톰브라운이라고 유명한 상표 있어요",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.2",
          "form": "명품 브랜드랑 삼성 플립폰이랑 콜라보했어요~",
          "original_form": "명품 브랜드랑 삼성 플립폰이랑 콜라보했어요~",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.3",
          "form": "진짜 너무 뽐뿌오고 예쁘더라고요",
          "original_form": "진짜 너무 뽐뿌오고 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.4",
          "form": "휴대폰 + 톰브라운 디자인",
          "original_form": "휴대폰 + 톰브라운 디자인",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.5",
          "form": "진짜 다들 예쁘다 난리에요 그거 ㅜㅜ",
          "original_form": "진짜 다들 예쁘다 난리에요 그거 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.6",
          "form": "삼성라이온즈도 있고",
          "original_form": "삼성라이온즈도 있고",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.7",
          "form": "되게 브랜드 다양한거같아요",
          "original_form": "되게 브랜드 다양한거같아요",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.8",
          "form": "야구요...?ㅋㅋㅋ",
          "original_form": "야구요...?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.10",
          "form": "카페 도 다 있더라구요",
          "original_form": "카페 도 다 있더라구요",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.11",
          "form": "그거 ㄴ아마 콜라보 아니라 야구단 이름일 뿐일걸요",
          "original_form": "그거 ㄴ아마 콜라보 아니라 야구단 이름일 뿐일걸요",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015730.1.12",
          "form": "아 야구단 그파랑색 로고요 ㅋㅋ",
          "original_form": "아 야구단 그파랑색 로고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015730.1.13",
          "form": "유명하진 않은데 까스활명수랑 모나미랑 콜라보도 했어요",
          "original_form": "유명하진 않은데 까스활명수랑 모나미랑 콜라보도 했어요",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015730.1.14",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015730.1.15",
          "form": "그건진짜안땡기는 ㅋㅋㅋ",
          "original_form": "그건진짜안땡기는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015730.1.16",
          "form": "네네 근데 모나미는 삼성에스펜이랑 한게 젤 잘한듯",
          "original_form": "네네 근데 모나미는 삼성에스펜이랑 한게 젤 잘한듯",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015730.1.17",
          "form": "먹으면 큰일날거같은데요",
          "original_form": "먹으면 큰일날거같은데요",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015730.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015730.1.19",
          "form": "ㅎㅎ혹시 콜라보 했으면 좋겠다 하는 브랜드 있어요?",
          "original_form": "ㅎㅎ혹시 콜라보 했으면 좋겠다 하는 브랜드 있어요?",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015730.1.20",
          "form": "뭔가 시선을 딱 끄는 효과도 있는거같아요",
          "original_form": "뭔가 시선을 딱 끄는 효과도 있는거같아요",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015730.1.21",
          "form": "특이해서 ㅋㅋㅋ",
          "original_form": "특이해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015730.1.22",
          "form": "그쵸그쵸 그런걸로 마케팅 하는건가봐요",
          "original_form": "그쵸그쵸 그런걸로 마케팅 하는건가봐요",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015730.1.23",
          "form": "생각안해봤는데 name2님은요?",
          "original_form": "생각안해봤는데 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015730.1.24",
          "form": "저도 딱히 생각해본 건 없는데",
          "original_form": "저도 딱히 생각해본 건 없는데",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015730.1.25",
          "form": "나이키를 워낙 좋아해서 나이키하고 슈피켄코리아 콜라보해서",
          "original_form": "나이키를 워낙 좋아해서 나이키하고 슈피켄코리아 콜라보해서",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015730.1.26",
          "form": "폰케이스 하나 깔끔하게 만들어줬으면....ㅋㅋㅋ",
          "original_form": "폰케이스 하나 깔끔하게 만들어줬으면....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015730.1.27",
          "form": "오 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015730.1.28",
          "form": "아이디어 괜찮은데요?",
          "original_form": "아이디어 괜찮은데요?",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015730.1.29",
          "form": "흰 케이스에 나이키로고 하나만... 심플하게 ㅋㅋㅋ",
          "original_form": "흰 케이스에 나이키로고 하나만... 심플하게 ㅋㅋㅋ",
