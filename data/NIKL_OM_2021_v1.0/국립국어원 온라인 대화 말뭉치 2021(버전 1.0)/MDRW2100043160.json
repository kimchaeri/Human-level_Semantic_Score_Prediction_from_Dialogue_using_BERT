      "utterance": [
        {
          "id": "MDRW2100043160.1.1",
          "form": "쿠죠사라랑 라이덴 같이쓰면",
          "original_form": "쿠죠사라랑 라이덴 같이쓰면",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043160.1.2",
          "form": "글케 좋다는데",
          "original_form": "글케 좋다는데",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043160.1.3",
          "form": "둘다 전기 아냐??",
          "original_form": "둘다 전기 아냐??",
          "speaker_id": "1",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043160.1.4",
          "form": "ㅇㅇ근데 좋다네",
          "original_form": "ㅇㅇ근데 좋다네",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043160.1.5",
          "form": "사라이덴 조합이래",
          "original_form": "사라이덴 조합이래",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043160.1.6",
          "form": "쿠죠사라가",
          "original_form": "쿠죠사라가",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043160.1.7",
          "form": "피슬이랑 비슷하다고 들엇는데",
          "original_form": "피슬이랑 비슷하다고 들엇는데",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043160.1.8",
          "form": "근데 좀 모자라대",
          "original_form": "근데 좀 모자라대",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043160.1.9",
          "form": "1티어라고 하기엔",
          "original_form": "1티어라고 하기엔",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043160.1.10",
          "form": "그걸 라이덴이 채워준다는ㄷ니ㅣ",
          "original_form": "그걸 라이덴이 채워준다는ㄷ니ㅣ",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043160.1.11",
          "form": "그리그 컨셉 조합 보면",
          "original_form": "그리그 컨셉 조합 보면",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043160.1.12",
          "form": "아예 같은 원소 둘 넣어서",
          "original_form": "아예 같은 원소 둘 넣어서",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043160.1.13",
          "form": "그 속성 강화로 쓰기도 하더라",
          "original_form": "그 속성 강화로 쓰기도 하더라",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043160.1.14",
          "form": "주로 불 둘 넣어서",
          "original_form": "주로 불 둘 넣어서",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043160.1.15",
          "form": "화염데미지 증가? 그거 쓰던데?",
          "original_form": "화염데미지 증가? 그거 쓰던데?",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043160.1.16",
          "form": "ㅇㅎ 신기하네",
          "original_form": "ㅇㅎ 신기하네",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100043160.1.17",
          "form": "다이루크 베넷조합 같은건가",
          "original_form": "다이루크 베넷조합 같은건가",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100043160.1.18",
          "form": "그런가바",
          "original_form": "그런가바",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100043160.1.19",
          "form": "베넷 키워야하는데",
          "original_form": "베넷 키워야하는데",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043160.1.20",
          "form": "지금 있는 파티원도 못키우고 있어서",
          "original_form": "지금 있는 파티원도 못키우고 있어서",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043160.1.21",
          "form": "계속 미루는중...",
          "original_form": "계속 미루는중...",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043160.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠㅠㅠ 그건 나도.....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠㅠㅠ 그건 나도.....",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043160.1.23",
          "form": "나 80렙 돌파 상태나",
          "original_form": "나 80렙 돌파 상태나",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043160.1.24",
          "form": "90렙이나",
          "original_form": "90렙이나",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043160.1.25",
          "form": "큰 차이 안나길래",
          "original_form": "큰 차이 안나길래",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043160.1.26",
          "form": "ㅇㄹ단 다 돌파만 시키고",
          "original_form": "ㅇㄹ단 다 돌파만 시키고",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043160.1.27",
          "form": "나머지 키우려구",
          "original_form": "나머지 키우려구",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043160.1.28",
          "form": "90렙 경험치책 드는거에 비해",
          "original_form": "90렙 경험치책 드는거에 비해",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043160.1.29",
          "form": "효율이 구려",
          "original_form": "효율이 구려",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043160.1.30",
          "form": "ㅇㅇ 90렙은",
          "original_form": "ㅇㅇ 90렙은",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043160.1.31",
          "form": "특성 80렙에서 풀로 올리고",
          "original_form": "특성 80렙에서 풀로 올리고",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043160.1.32",
          "form": "그 이상 올리고싶을때 하는게 맞는듯....",
          "original_form": "그 이상 올리고싶을때 하는게 맞는듯....",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043160.1.33",
          "form": "ㅇㅇ.........",
          "original_form": "ㅇㅇ.........",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043160.1.34",
          "form": "특성도 다 못올렷는데 렙업은 의미 없는거같아",
          "original_form": "특성도 다 못올렷는데 렙업은 의미 없는거같아",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043160.1.35",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043160.1.36",
          "form": "ㅎㅏ 특성",
          "original_form": "ㅎㅏ 특성",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043160.1.37",
          "form": "비경 진짜 삽노잼.....",
          "original_form": "비경 진짜 삽노잼.....",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043160.1.38",
          "form": "ㅇㅇㅇㅇ 특성의 가장 큰 고난은",
          "original_form": "ㅇㅇㅇㅇ 특성의 가장 큰 고난은",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043160.1.39",
          "form": "노잼인 비경이야....",
          "original_form": "노잼인 비경이야....",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043160.1.40",
          "form": "한 번 돌 때마다 급격하게 피로해짐..",
          "original_form": "한 번 돌 때마다 급격하게 피로해짐..",
