        },
        {
          "id": "MDRW2100000726.1.7",
          "form": "게임은 유튭으로만 봐요",
          "original_form": "게임은 유튭으로만 봐요",
          "speaker_id": "2",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000726.1.8",
          "form": "어려운 주제네요",
