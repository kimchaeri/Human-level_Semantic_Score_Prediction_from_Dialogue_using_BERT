      "utterance": [
        {
          "id": "MDRW2100003828.1.1",
          "form": "그래서 이혼 소송중에 살 날리고",
          "original_form": "그래서 이혼 소송중에 살 날리고",
          "speaker_id": "2",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003828.1.2",
          "form": "난리도 아녔던데",
          "original_form": "난리도 아녔던데",
          "speaker_id": "2",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003828.1.3",
          "form": "그게 소장에 다 써있더라구요",
          "original_form": "그게 소장에 다 써있더라구요",
          "speaker_id": "2",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003828.1.4",
          "form": "읽으면서도 이게 무슨 막장드라마야 이랬어요",
          "original_form": "읽으면서도 이게 무슨 막장드라마야 이랬어요",
          "speaker_id": "2",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003828.1.5",
          "form": "부인엄마는 무당이요?",
          "original_form": "부인엄마는 무당이요?",
          "speaker_id": "1",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003828.1.6",
          "form": "정확히는 장모 동생이요",
          "original_form": "정확히는 장모 동생이요",
          "speaker_id": "2",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003828.1.7",
          "form": "정말 사기결혼이네요!!",
          "original_form": "정말 사기결혼이네요!!",
          "speaker_id": "1",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003828.1.8",
          "form": "대박이죠ㅋㅋㅋㅋ",
          "original_form": "대박이죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:37"
        },
        {
          "id": "MDRW2100003828.1.9",
          "form": "아 동생이!!",
          "original_form": "아 동생이!!",
          "speaker_id": "1",
          "time": "20210821 12:37"
        },
        {
          "id": "MDRW2100003828.1.10",
          "form": "저 무슨 드라마본줄 알았다니까요",
          "original_form": "저 무슨 드라마본줄 알았다니까요",
          "speaker_id": "2",
          "time": "20210821 12:37"
        },
        {
          "id": "MDRW2100003828.1.11",
          "form": "그래서 결혼생각 완전히 0됐어요",
          "original_form": "그래서 결혼생각 완전히 0됐어요",
          "speaker_id": "2",
          "time": "20210821 12:37"
        },
        {
          "id": "MDRW2100003828.1.12",
          "form": "네~~진짜 너무 심하네요",
          "original_form": "네~~진짜 너무 심하네요",
          "speaker_id": "1",
          "time": "20210821 12:37"
        },
        {
          "id": "MDRW2100003828.1.13",
          "form": "내가 겪었으면 난 제정신으로 못살았겠다싶어서",
          "original_form": "내가 겪었으면 난 제정신으로 못살았겠다싶어서",
          "speaker_id": "2",
          "time": "20210821 12:37"
        },
        {
          "id": "MDRW2100003828.1.14",
          "form": "저는 약과네요",
          "original_form": "저는 약과네요",
