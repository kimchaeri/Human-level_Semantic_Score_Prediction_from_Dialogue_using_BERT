      "utterance": [
        {
          "id": "MDRW2100033556.1.1",
          "form": "취업중이신가요?",
          "original_form": "취업중이신가요?",
          "speaker_id": "1",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100033556.1.2",
          "form": "아뇨 직장인이요  name1님은?",
          "original_form": "아뇨 직장인이요  &name1&님은?",
          "speaker_id": "2",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100033556.1.3",
          "form": "백수입니다ㅎㅎ",
          "original_form": "백수입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100033556.1.4",
          "form": "취업준비중이신가요?",
          "original_form": "취업준비중이신가요?",
          "speaker_id": "2",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100033556.1.5",
          "form": "회사다니다가 번아웃와서 그냥 쉬고 있어요~",
          "original_form": "회사다니다가 번아웃와서 그냥 쉬고 있어요~",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100033556.1.6",
          "form": "아 스트레스가 많으셨나봐요",
          "original_form": "아 스트레스가 많으셨나봐요",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100033556.1.7",
          "form": "야근하는게 너무 힘들더라고요",
          "original_form": "야근하는게 너무 힘들더라고요",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100033556.1.8",
          "form": "어떤일 하시나요?",
          "original_form": "어떤일 하시나요?",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100033556.1.9",
          "form": "아 야근 힘들죠 전 원자력관련일해요",
          "original_form": "아 야근 힘들죠 전 원자력관련일해요",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100033556.1.10",
          "form": "잘모르지만 말로만들어서는 엄청난 일을 하시는거같아요",
          "original_form": "잘모르지만 말로만들어서는 엄청난 일을 하시는거같아요",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100033556.1.11",
          "form": "그렇지도않아요ㅎ",
          "original_form": "그렇지도않아요ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100033556.1.12",
          "form": "걍 작은회사 다녀서",
          "original_form": "걍 작은회사 다녀서",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100033556.1.13",
          "form": "업무 스트레스 있으신가요?",
          "original_form": "업무 스트레스 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033556.1.14",
          "form": "예전엔 엄청 스트레스받았는데",
          "original_form": "예전엔 엄청 스트레스받았는데",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033556.1.15",
          "form": "상사랑 넘싸워서 팀바꿨어요ㅎ",
          "original_form": "상사랑 넘싸워서 팀바꿨어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033556.1.16",
          "form": "요즘은 널널하고 좋네요 내할일만 하면돼서",
          "original_form": "요즘은 널널하고 좋네요 내할일만 하면돼서",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100033556.1.17",
          "form": "어딜가나 사람이 문제네요",
          "original_form": "어딜가나 사람이 문제네요",
          "speaker_id": "1",
          "time": "20210824 14:18"
        }
      ]
