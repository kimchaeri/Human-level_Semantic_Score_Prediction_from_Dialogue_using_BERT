      "utterance": [
        {
          "id": "MDRW2100022780.1.1",
          "form": "와 그럼 옷을 거의 다",
          "original_form": "와 그럼 옷을 거의 다",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022780.1.2",
          "form": "바꾸셔야할텐데",
          "original_form": "바꾸셔야할텐데",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022780.1.3",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022780.1.4",
          "form": "저 그리고 화장안한지 좀 됐는데",
          "original_form": "저 그리고 화장안한지 좀 됐는데",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022780.1.5",
          "form": "원래 화장하던 때에는 무조건 레드오렌지립만 발랐거든요?",
          "original_form": "원래 화장하던 때에는 무조건 레드오렌지립만 발랐거든요?",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022780.1.6",
          "form": "립도 핑크립 바르래요",
          "original_form": "립도 핑크립 바르래요",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022780.1.7",
          "form": "전 대체 어떻게 살아온건지",
          "original_form": "전 대체 어떻게 살아온건지",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022780.1.8",
          "form": "완전 정반대로 진단받으신거군여ㅋㅋㅋㅋㅋ",
          "original_form": "완전 정반대로 진단받으신거군여ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022780.1.9",
          "form": "네네 ㅋㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022780.1.10",
          "form": "퍼스널컬러 받아보신적있으세요?",
          "original_form": "퍼스널컬러 받아보신적있으세요?",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022780.1.11",
          "form": "아님 추측하는 톤",
          "original_form": "아님 추측하는 톤",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022780.1.12",
          "form": "어떡혀.. 뚝심있게 밀고나가야되시나",
          "original_form": "어떡혀.. 뚝심있게 밀고나가야되시나",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022780.1.13",
          "form": "아 저는",
          "original_form": "아 저는",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022780.1.14",
          "form": "받아본 적은 없는데 저도 name1님이랑 비슷할거라고 추측 중이예요!",
          "original_form": "받아본 적은 없는데 저도 &name1&님이랑 비슷할거라고 추측 중이예요!",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022780.1.15",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022780.1.16",
          "form": "전 립조차 안 받아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 립조차 안 받아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022780.1.17",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022780.1.18",
          "form": "톤이 약간 걸친 것 같은데",
          "original_form": "톤이 약간 걸친 것 같은데",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022780.1.19",
          "form": "발라도 연하게 하라고",
          "original_form": "발라도 연하게 하라고",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022780.1.20",
          "form": "꾸안꾸...",
          "original_form": "꾸안꾸...",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022780.1.21",
          "form": "맞아유 그래서 전 옷 색깔에 투자를 해볼까",
          "original_form": "맞아유 그래서 전 옷 색깔에 투자를 해볼까",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022780.1.22",
          "form": "생각중이예요",
          "original_form": "생각중이예요",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022780.1.23",
          "form": "정확한 진단은 받은 적 없는데",
          "original_form": "정확한 진단은 받은 적 없는데",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022780.1.24",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022780.1.25",
          "form": "야매로 집에서 수건 여러 장 가지고",
          "original_form": "야매로 집에서 수건 여러 장 가지고",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022780.1.26",
          "form": "얼굴에 대어보고ㅋㅋㅋ",
          "original_form": "얼굴에 대어보고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022780.1.27",
          "form": "근데 솔직히 저는 진단 받고도 제 톤에 의심중이라 굳이 비싼돈 주고 받을 필요없는거같아요",
          "original_form": "근데 솔직히 저는 진단 받고도 제 톤에 의심중이라 굳이 비싼돈 주고 받을 필요없는거같아요",
          "speaker_id": "1",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022780.1.28",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022780.1.29",
          "form": "하얀 옷 잘어울리세요?",
          "original_form": "하얀 옷 잘어울리세요?",
          "speaker_id": "1",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022780.1.30",
          "form": "뭔가 좀 덜 정호ㅓㄱ한가ㅛ",
          "original_form": "뭔가 좀 덜 정호ㅓㄱ한가ㅛ",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022780.1.31",
          "form": "네! 명도 올라갈 수록",
          "original_form": "네! 명도 올라갈 수록",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022780.1.32",
          "form": "다크서클이 사라집니다",
          "original_form": "다크서클이 사라집니다",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022780.1.33",
          "form": "저는 흰옷이 진짜 안어울리거든요",
          "original_form": "저는 흰옷이 진짜 안어울리거든요",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022780.1.34",
          "form": "크림색은 ㄱㅊ은데",
          "original_form": "크림색은 ㄱㅊ은데",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022780.1.35",
          "form": "쌩화이트옷 입으면 얼굴이 완전 흙빛돼요",
          "original_form": "쌩화이트옷 입으면 얼굴이 완전 흙빛돼요",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022780.1.36",
          "form": "근데 자꾸.. 이게 맞는 옷이라고",
          "original_form": "근데 자꾸.. 이게 맞는 옷이라고",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022780.1.37",
          "form": "쌩화이트 빼고 혹시 다른게 잘 맞으시면",
          "original_form": "쌩화이트 빼고 혹시 다른게 잘 맞으시면",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022780.1.38",
          "form": "맞..지않을까요?!",
          "original_form": "맞..지않을까요?!",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022780.1.39",
          "form": "ㅠㅅ ㅠ",
          "original_form": "ㅠㅅ ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022780.1.40",
          "form": "전 회색옷이 어울리는것같아요",
          "original_form": "전 회색옷이 어울리는것같아요",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022780.1.41",
          "form": "고집인가..",
          "original_form": "고집인가..",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022780.1.42",
          "form": "회색 옷 입으면 안색이 좋아지시나요!",
          "original_form": "회색 옷 입으면 안색이 좋아지시나요!",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022780.1.43",
          "form": "네네 그런거같아요",
          "original_form": "네네 그런거같아요",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022780.1.44",
          "form": "어어어어어 추측인디 혹시 여름뮤트랑 라이트 걸치신 게 아닐까요?",
          "original_form": "어어어어어 추측인디 혹시 여름뮤트랑 라이트 걸치신 게 아닐까요?",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022780.1.45",
          "form": "신기하다 전 회색 입으면 얼굴 주황색 돼요",
          "original_form": "신기하다 전 회색 입으면 얼굴 주황색 돼요",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022780.1.46",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022780.1.47",
          "form": "ㅋㅋㅌㅋ안색에 황사맞아요",
          "original_form": "ㅋㅋㅌㅋ안색에 황사맞아요",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022780.1.48",
          "form": "뮤트도 한번 알아봐야겠어요",
          "original_form": "뮤트도 한번 알아봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022780.1.49",
          "form": "안색에 황샄ㄱㅋㄱㅋㄱㅋㄱㅋ",
          "original_form": "안색에 황샄ㄱㅋㄱㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022780.1.50",
          "form": "아 ㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋ",
          "original_form": "아 ㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022780.1.51",
          "form": "똥빛된다고만 생각했는데 넘 참신한 표현이에요",
          "original_form": "똥빛된다고만 생각했는데 넘 참신한 표현이에요",
