      "utterance": [
        {
          "id": "MDRW2100043163.1.1",
          "form": "자주 보는 채널 있나요",
          "original_form": "자주 보는 채널 있나요",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043163.1.2",
          "form": "티비엔이요",
          "original_form": "티비엔이요",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043163.1.3",
          "form": "재밌는거 많이해서 주로 보고있어요",
          "original_form": "재밌는거 많이해서 주로 보고있어요",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043163.1.4",
          "form": "티비엔 갯마을 차차차 보세요?",
          "original_form": "티비엔 갯마을 차차차 보세요?",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043163.1.5",
          "form": "처음부터는 못봤어요ㅋㅋ",
          "original_form": "처음부터는 못봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043163.1.6",
          "form": "재밌다고 하더라구요",
          "original_form": "재밌다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043163.1.7",
          "form": "저도 띄엄띠ㅣ엄봐요",
          "original_form": "저도 띄엄띠ㅣ엄봐요",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043163.1.8",
          "form": "동백꽃과 분위기 비슷해요",
          "original_form": "동백꽃과 분위기 비슷해요",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043163.1.9",
          "form": "몰랑몰랑",
          "original_form": "몰랑몰랑",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043163.1.10",
          "form": "저 동백꽃 재밌게 봤었거든요",
          "original_form": "저 동백꽃 재밌게 봤었거든요",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043163.1.11",
          "form": "챙겨봐야겠네요ㅋㅋ",
          "original_form": "챙겨봐야겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043163.1.12",
          "form": "비스새요",
          "original_form": "비스새요",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043163.1.13",
          "form": "ㅂ슷",
          "original_form": "ㅂ슷",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043163.1.14",
          "form": "또 보는거 있으세요?",
          "original_form": "또 보는거 있으세요?",
          "speaker_id": "2",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043163.1.15",
          "form": "음 평소에 여신강림 보고 또 봐요 차은우 나오는",
          "original_form": "음 평소에 여신강림 보고 또 봐요 차은우 나오는",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043163.1.16",
          "form": "아 그거 소재 재밌더라구요",
          "original_form": "아 그거 소재 재밌더라구요",
          "speaker_id": "2",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043163.1.17",
          "form": "그쵸 유치한데 차은우 얼굴 보려고 봐요 문가영이랑",
          "original_form": "그쵸 유치한데 차은우 얼굴 보려고 봐요 문가영이랑",
          "speaker_id": "1",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100043163.1.18",
          "form": "다들 예쁘고 잘생겼죠ㅋㅋㅋ",
          "original_form": "다들 예쁘고 잘생겼죠ㅋㅋㅋ",
