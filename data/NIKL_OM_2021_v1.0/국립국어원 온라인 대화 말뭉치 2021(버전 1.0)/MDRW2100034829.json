        {
          "id": "MDRW2100034829.1.1",
          "form": "저녁뭐드셨나요",
          "original_form": "저녁뭐드셨나요",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034829.1.2",
          "form": "수육 먹을 예정이에요",
          "original_form": "수육 먹을 예정이에요",
          "speaker_id": "1",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034829.1.3",
          "form": "식사하셧나요",
          "original_form": "식사하셧나요",
          "speaker_id": "1",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034829.1.4",
          "form": "오 전아직요 수육좋네요",
          "original_form": "오 전아직요 수육좋네요",
          "speaker_id": "2",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034829.1.5",
          "form": "언능 드세요",
          "original_form": "언능 드세요",
          "speaker_id": "1",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034829.1.6",
          "form": "칼국수끓이고있어요",
          "original_form": "칼국수끓이고있어요",
          "speaker_id": "2",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100034829.1.7",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034829.1.8",
          "form": "저는 탄수화물중독자",
          "original_form": "저는 탄수화물중독자",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034829.1.9",
          "form": "이제라도 먹어야죠ㅜㅜ",
          "original_form": "이제라도 먹어야죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034829.1.10",
          "form": "저둥",
          "original_form": "저둥",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034829.1.11",
          "form": "면이 최애에여",
          "original_form": "면이 최애에여",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
