      "utterance": [
        {
          "id": "MDRW2100026459.1.1",
          "form": "너무 좋아해요",
          "original_form": "너무 좋아해요",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026459.1.2",
          "form": "한동안 크레이프 케익 좋아했어요",
          "original_form": "한동안 크레이프 케익 좋아했어요",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026459.1.3",
          "form": "케익 !_! ㅋㅋㅋㅋ 제가 혼자사는데",
          "original_form": "케익 !_! ㅋㅋㅋㅋ 제가 혼자사는데",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026459.1.4",
          "form": "주말에 조각이 다팔렸길래 걍 홀케익을 사왓더니",
          "original_form": "주말에 조각이 다팔렸길래 걍 홀케익을 사왓더니",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026459.1.5",
          "form": "계속 그거만 먹고있어요...ㅋㅋㅋㅋ",
          "original_form": "계속 그거만 먹고있어요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026459.1.6",
          "form": "크레이프진짜마싯죠 ㅎㅎ",
          "original_form": "크레이프진짜마싯죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026459.1.7",
          "form": "무슨케익 사셨어요?",
          "original_form": "무슨케익 사셨어요?",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026459.1.8",
          "form": "딸케먹고싶었는데 없어서 그냥 순생크림케익샀거든요",
          "original_form": "딸케먹고싶었는데 없어서 그냥 순생크림케익샀거든요",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026459.1.9",
          "form": "근데 크림이 예술이라 넘맛 ㅎㅎ",
          "original_form": "근데 크림이 예술이라 넘맛 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026459.1.10",
          "form": "크윽",
          "original_form": "크윽",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026459.1.11",
          "form": "어디케익 사셨어요?",
          "original_form": "어디케익 사셨어요?",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026459.1.12",
          "form": "성심당이용",
          "original_form": "성심당이용",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026459.1.13",
          "form": "저는 요즘 투썸 케익이 너무 맛아요",
          "original_form": "저는 요즘 투썸 케익이 너무 맛아요",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026459.1.14",
          "form": "오오 튀김소보로 파는곳이요?",
          "original_form": "오오 튀김소보로 파는곳이요?",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026459.1.15",
          "form": "오오 투썸 투썸은 초크생 !!",
          "original_form": "오오 투썸 투썸은 초크생 !!",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026459.1.16",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026459.1.17",
          "form": "거기 케익도 맛있군요",
          "original_form": "거기 케익도 맛있군요",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026459.1.18",
          "form": "오오 저랑 입맛이 비슷하신가봐요",
          "original_form": "오오 저랑 입맛이 비슷하신가봐요",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026459.1.19",
          "form": "네...빵 케익 샌드위치 다맛잇어요",
          "original_form": "네...빵 케익 샌드위치 다맛잇어요",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026459.1.20",
          "form": "진짜 부추빵 튀김소보로만 먹어본거 같아요ㅋㅋ",
          "original_form": "진짜 부추빵 튀김소보로만 먹어본거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026459.1.21",
          "form": "투썸이 카페중엔 케익이 젤 맛나용",
          "original_form": "투썸이 카페중엔 케익이 젤 맛나용",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026459.1.22",
          "form": "오오 저도..부추빵 엄청 조아해요",
          "original_form": "오오 저도..부추빵 엄청 조아해요",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026459.1.23",
          "form": "동의 합니다ㅋ",
