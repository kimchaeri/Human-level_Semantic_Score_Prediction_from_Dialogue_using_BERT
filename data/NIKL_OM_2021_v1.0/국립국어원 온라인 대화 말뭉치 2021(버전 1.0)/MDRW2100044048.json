        {
          "id": "MDRW2100044048.1.1",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ 아니 어제 맞으셨다고 하지 않았나요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ 아니 어제 맞으셨다고 하지 않았나요",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044048.1.2",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044048.1.3",
          "form": "하루만에 이래도 되는건지...",
          "original_form": "하루만에 이래도 되는건지...",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044048.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 갈치말고도 맛난거 많이 드셨나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 갈치말고도 맛난거 많이 드셨나요",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044048.1.5",
          "form": "ㅋㅋㅋ야식으로 핫도그도 시켜먹고",
          "original_form": "ㅋㅋㅋ야식으로 핫도그도 시켜먹고",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044048.1.6",
          "form": "저녁으로 라볶이랑 김밥이랑...",
          "original_form": "저녁으로 라볶이랑 김밥이랑...",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044048.1.7",
          "form": "많이...먹긴했어요...",
          "original_form": "많이...먹긴했어요...",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044048.1.8",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044048.1.9",
          "form": "백신맞고 아픈것보단",
          "original_form": "백신맞고 아픈것보단",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044048.1.10",
          "form": "잘먹는게 훨씬 낫죠",
          "original_form": "잘먹는게 훨씬 낫죠",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044048.1.11",
          "form": "그쵸..?",
          "original_form": "그쵸..?",
