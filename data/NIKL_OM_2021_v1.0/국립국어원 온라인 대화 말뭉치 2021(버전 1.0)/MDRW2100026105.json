        {
          "id": "MDRW2100026105.1.1",
          "form": "곰표맥주 먹어봤어?",
          "original_form": "곰표맥주 먹어봤어?",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026105.1.2",
          "form": "모나미 음료수도 있더라",
          "original_form": "모나미 음료수도 있더라",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026105.1.3",
          "form": "아직",
          "original_form": "아직",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026105.1.4",
          "form": "품절돼서 구하기힘들어",
          "original_form": "품절돼서 구하기힘들어",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026105.1.5",
          "form": "ㅇㅇ 구경도 못함",
          "original_form": "ㅇㅇ 구경도 못함",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026105.1.6",
          "form": "진로 두꺼비 굿즈는 사고싶더라",
          "original_form": "진로 두꺼비 굿즈는 사고싶더라",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026105.1.7",
          "form": "피규어 엄청 귀엽지",
          "original_form": "피규어 엄청 귀엽지",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026105.1.8",
          "form": "인기많아",
          "original_form": "인기많아",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026105.1.9",
          "form": "카카오랑 콜라보하면 다 귀엽지",
          "original_form": "카카오랑 콜라보하면 다 귀엽지",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026105.1.10",
          "form": "응 품절에 절판에 난리야",
          "original_form": "응 품절에 절판에 난리야",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026105.1.11",
          "form": "다sns에 보여주려고 그러는걸까",
          "original_form": "다sns에 보여주려고 그러는걸까",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026105.1.12",
          "form": "과시욕도 있지만",
          "original_form": "과시욕도 있지만",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026105.1.13",
          "form": "남들하는거 다 하고싶으니까",
          "original_form": "남들하는거 다 하고싶으니까",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026105.1.14",
          "form": "뒤쳐지면 안돼지",
          "original_form": "뒤쳐지면 안돼지",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026105.1.15",
          "form": "딱풀도 콜라보있더라",
          "original_form": "딱풀도 콜라보있더라",
          "speaker_id": "2",
          "time": "20210901 19:47"
