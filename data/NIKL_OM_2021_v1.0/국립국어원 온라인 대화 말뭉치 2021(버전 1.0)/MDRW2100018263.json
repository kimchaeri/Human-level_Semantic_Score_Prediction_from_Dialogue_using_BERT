      "utterance": [
        {
          "id": "MDRW2100018263.1.1",
          "form": "저는 태국여",
          "original_form": "저는 태국여",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018263.1.2",
          "form": "너무 좋아여",
          "original_form": "너무 좋아여",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018263.1.3",
          "form": "아 태국 좋나보네여",
          "original_form": "아 태국 좋나보네여",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018263.1.4",
          "form": "거기 가면 과일도 맛있을거 같은데요",
          "original_form": "거기 가면 과일도 맛있을거 같은데요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018263.1.5",
          "form": "진짜 코로나 터지기 전에 동남아 한번 가보려고 했거든요?",
          "original_form": "진짜 코로나 터지기 전에 동남아 한번 가보려고 했거든요?",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018263.1.6",
          "form": "흑흑 못갔어",
          "original_form": "흑흑 못갔어",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018263.1.7",
          "form": "저는 태국 재밌었어여",
          "original_form": "저는 태국 재밌었어여",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018263.1.8",
          "form": "클럽좋아해요?",
          "original_form": "클럽좋아해요?",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018263.1.9",
          "form": "그럼 태국 안좋아하면 베트남 다낭",
          "original_form": "그럼 태국 안좋아하면 베트남 다낭",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018263.1.10",
          "form": "태국이 클럽이 유명한가보네영 오",
          "original_form": "태국이 클럽이 유명한가보네영 오",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018263.1.11",
          "form": "쿨럽은 아니고 그냥bar에여",
          "original_form": "쿨럽은 아니고 그냥bar에여",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018263.1.12",
          "form": "어디든 가서 쌀국수나 먹고 오려고 했어요 ㅋㅋㅋ",
          "original_form": "어디든 가서 쌀국수나 먹고 오려고 했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018263.1.13",
          "form": "분위기가 흥겨운ㅋ",
          "original_form": "분위기가 흥겨운ㅋ",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018263.1.14",
          "form": "쌀국수는 베트남이 존맛탱이네요",
          "original_form": "쌀국수는 베트남이 존맛탱이네요",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018263.1.15",
          "form": "어디든 가고싶어요",
          "original_form": "어디든 가고싶어요",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018263.1.16",
          "form": "그렇구나 중국이랑 일본밖에 안가봐서 해외 좀 다녀봤어야 됐는데 아쉬움이 남아요",
          "original_form": "그렇구나 중국이랑 일본밖에 안가봐서 해외 좀 다녀봤어야 됐는데 아쉬움이 남아요",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018263.1.17",
          "form": "유럽한번을 못가고 ㅠㅠ 이제 코로나 풀리고 가면 인종차별 더할거 같지 않아요?",
          "original_form": "유럽한번을 못가고 ㅠㅠ 이제 코로나 풀리고 가면 인종차별 더할거 같지 않아요?",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018263.1.18",
          "form": "이번생은 망한거 같아요",
          "original_form": "이번생은 망한거 같아요",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018263.1.19",
          "form": "한 오년은 더 못갈거 같아 ㅠㅠ",
          "original_form": "한 오년은 더 못갈거 같아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018263.1.20",
          "form": "유럽 한번 가고싶었는데",
          "original_form": "유럽 한번 가고싶었는데",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018263.1.21",
          "form": "중국 어디 다녀오셨어요?",
          "original_form": "중국 어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018263.1.22",
          "form": "저는 중국 좋아해여",
          "original_form": "저는 중국 좋아해여",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018263.1.23",
          "form": "중드좀 봤었어서",
          "original_form": "중드좀 봤었어서",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018263.1.24",
          "form": "북경 다녀왔어요",
          "original_form": "북경 다녀왔어요",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018263.1.25",
          "form": "가족여행이라 ㅋㅋㅋ만리장성은 보고왔어요",
          "original_form": "가족여행이라 ㅋㅋㅋ만리장성은 보고왔어요",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018263.1.26",
          "form": "오 북경좋져",
          "original_form": "오 북경좋져",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018263.1.27",
          "form": "근데 중국 ㅠㅠㅠㅠㅠ 너희때문에",
          "original_form": "근데 중국 ㅠㅠㅠㅠㅠ 너희때문에",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018263.1.28",
          "form": "이화원 갔다 자금성 갔다 만리장성 보셨나요?ㅎ",
          "original_form": "이화원 갔다 자금성 갔다 만리장성 보셨나요?ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018263.1.29",
          "form": "넹 ㅋㅋㅋㅋㅋㅋㅋ 딱 그루트",
          "original_form": "넹 ㅋㅋㅋㅋㅋㅋㅋ 딱 그루트",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018263.1.30",
          "form": "저는 상해 디즈니랜드 가보고 싶어요",
          "original_form": "저는 상해 디즈니랜드 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018263.1.31",
          "form": "저도 그런 곳을 가고 싶었는데",
          "original_form": "저도 그런 곳을 가고 싶었는데",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018263.1.32",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018263.1.33",
          "form": "오픈할때 갔어야 했는데 다들 중국 싫다고",
          "original_form": "오픈할때 갔어야 했는데 다들 중국 싫다고",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018263.1.34",
          "form": "안가주더라구여",
          "original_form": "안가주더라구여",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018263.1.35",
          "form": "맞아요 그런곳은 오픈할때 가야",
          "original_form": "맞아요 그런곳은 오픈할때 가야",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018263.1.36",
          "form": "거긴 지금도 박터질거 같은데여",
          "original_form": "거긴 지금도 박터질거 같은데여",
          "speaker_id": "1",
          "time": "20210826 23:34"
        }
      ]
    }
  ]
