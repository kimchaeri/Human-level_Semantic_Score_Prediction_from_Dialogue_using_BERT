      "utterance": [
        {
          "id": "MDRW2100006623.1.1",
          "form": "사고싶은가봐요??",
          "original_form": "사고싶은가봐요??",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100006623.1.2",
          "form": "당연하죵",
          "original_form": "당연하죵",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100006623.1.3",
          "form": "이번에 진짜 흥잖아염",
          "original_form": "이번에 진짜 흥잖아염",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100006623.1.4",
          "form": "나도 진심으로 빠지긴했는데",
          "original_form": "나도 진심으로 빠지긴했는데",
          "speaker_id": "2",
          "time": "20210908 03:36"
        },
        {
          "id": "MDRW2100006623.1.5",
          "form": "내 지갑이 허락하지 않아요 플립 ㅠ",
          "original_form": "내 지갑이 허락하지 않아요 플립 ㅠ",
          "speaker_id": "2",
          "time": "20210908 03:36"
        },
        {
          "id": "MDRW2100006623.1.6",
          "form": "오웅 안타까워요 ㅠㅠ",
          "original_form": "오웅 안타까워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100006623.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ하나도 진심이 아닌것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ하나도 진심이 아닌것 같아요",
          "speaker_id": "2",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100006623.1.8",
          "form": "플립 쓰는 사람 많이 봤어요 저는",
          "original_form": "플립 쓰는 사람 많이 봤어요 저는",
          "speaker_id": "1",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100006623.1.9",
          "form": "예쁘면 하나 질러보지 왜",
          "original_form": "예쁘면 하나 질러보지 왜",
          "speaker_id": "2",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100006623.1.10",
          "form": "길거리에서요!",
          "original_form": "길거리에서요!",
          "speaker_id": "1",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100006623.1.11",
          "form": "나도 보고싶어요",
          "original_form": "나도 보고싶어요",
          "speaker_id": "2",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100006623.1.12",
          "form": "밖에 안다니시나봐염?",
          "original_form": "밖에 안다니시나봐염?",
          "speaker_id": "1",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100006623.1.13",
          "form": "많던데요",
          "original_form": "많던데요",
          "speaker_id": "1",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100006623.1.14",
          "form": "학교만 다녀요ㅠ 폴드든 플립이든",
          "original_form": "학교만 다녀요ㅠ 폴드든 플립이든",
          "speaker_id": "2",
          "time": "20210908 03:39"
        },
        {
          "id": "MDRW2100006623.1.15",
          "form": "어서 내 애기들이 되었으면하네요",
          "original_form": "어서 내 애기들이 되었으면하네요",
          "speaker_id": "2",
          "time": "20210908 03:39"
        },
        {
          "id": "MDRW2100006623.1.16",
          "form": "지갑이 싫다고 하네요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "지갑이 싫다고 하네요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:39"
        },
        {
          "id": "MDRW2100006623.1.17",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210908 03:40"
        },
        {
          "id": "MDRW2100006623.1.18",
          "form": "같은 이유군요",
          "original_form": "같은 이유군요",
          "speaker_id": "2",
          "time": "20210908 03:40"
        },
        {
          "id": "MDRW2100006623.1.19",
          "form": "ㅋㅋㅋㅋㅋ넵 그런걸로 하죵",
          "original_form": "ㅋㅋㅋㅋㅋ넵 그런걸로 하죵",
          "speaker_id": "1",
          "time": "20210908 03:40"
        },
        {
          "id": "MDRW2100006623.1.20",
          "form": "근데 사람들 진짜 많이 사더라",
          "original_form": "근데 사람들 진짜 많이 사더라",
          "speaker_id": "2",
          "time": "20210908 03:41"
        },
        {
          "id": "MDRW2100006623.1.21",
          "form": "형도 살거라하던데 부러웠어",
          "original_form": "형도 살거라하던데 부러웠어",
          "speaker_id": "2",
          "time": "20210908 03:41"
        },
        {
          "id": "MDRW2100006623.1.22",
          "form": "그치 많다니까",
          "original_form": "그치 많다니까",
