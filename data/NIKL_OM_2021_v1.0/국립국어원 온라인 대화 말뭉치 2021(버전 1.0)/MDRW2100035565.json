      "utterance": [
        {
          "id": "MDRW2100035565.1.1",
          "form": "예전에 내이름은 김삼순이요!",
          "original_form": "예전에 내이름은 김삼순이요!",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035565.1.2",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100035565.1.3",
          "form": "오...ㅋ 저는 그사세요. 현빈 송혜교 주연",
          "original_form": "오...ㅋ 저는 그사세요. 현빈 송혜교 주연",
          "speaker_id": "1",
          "time": "20210830 05:38"
        },
        {
          "id": "MDRW2100035565.1.4",
          "form": "그것도 명작이죠",
          "original_form": "그것도 명작이죠",
          "speaker_id": "2",
          "time": "20210830 07:43"
        },
        {
          "id": "MDRW2100035565.1.5",
          "form": "전 그거 두번째에요!",
          "original_form": "전 그거 두번째에요!",
          "speaker_id": "2",
          "time": "20210830 07:44"
        },
        {
          "id": "MDRW2100035565.1.6",
          "form": "우와 아시는군요^^",
          "original_form": "우와 아시는군요^^",
          "speaker_id": "1",
          "time": "20210830 08:19"
        },
        {
          "id": "MDRW2100035565.1.7",
          "form": "넹! 그 때 그 둘에게 앓았더랬죠",
          "original_form": "넹! 그 때 그 둘에게 앓았더랬죠",
          "speaker_id": "2",
          "time": "20210830 08:22"
        },
        {
          "id": "MDRW2100035565.1.8",
          "form": "저두요..애정씬 정말 사랑스러웠죠",
          "original_form": "저두요..애정씬 정말 사랑스러웠죠",
          "speaker_id": "1",
          "time": "20210830 08:26"
        },
        {
          "id": "MDRW2100035565.1.9",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210830 08:35"
        },
        {
          "id": "MDRW2100035565.1.10",
          "form": "사랑에 빠질 수 밖에 없었어요",
          "original_form": "사랑에 빠질 수 밖에 없었어요",
          "speaker_id": "2",
          "time": "20210830 08:35"
        },
        {
          "id": "MDRW2100035565.1.11",
          "form": "대사도 정말 좋았어요",
          "original_form": "대사도 정말 좋았어요",
          "speaker_id": "1",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100035565.1.12",
          "form": "아 좋았죠 진짜",
          "original_form": "아 좋았죠 진짜",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100035565.1.13",
          "form": "와닿는것도 많았고",
          "original_form": "와닿는것도 많았고",
          "speaker_id": "2",
          "time": "20210830 08:45"
        },
        {
          "id": "MDRW2100035565.1.14",
          "form": "노희경 작가 글이 참 좋죠",
          "original_form": "노희경 작가 글이 참 좋죠",
          "speaker_id": "1",
          "time": "20210830 08:45"
        },
        {
          "id": "MDRW2100035565.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100035565.1.16",
          "form": "또 하나 해줌 좋겠어요",
          "original_form": "또 하나 해줌 좋겠어요",
          "speaker_id": "2",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100035565.1.17",
          "form": "요즘엔 막장이 많아서ㅠ",
          "original_form": "요즘엔 막장이 많아서ㅠ",
          "speaker_id": "2",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100035565.1.18",
          "form": "맞아요 아 디어마이프렌드 아세요? 그것도 노희경작가 아닌가요",
          "original_form": "맞아요 아 디어마이프렌드 아세요? 그것도 노희경작가 아닌가요",
          "speaker_id": "1",
          "time": "20210830 08:58"
        },
        {
          "id": "MDRW2100035565.1.19",
          "form": "고현정 나온거죠??",
          "original_form": "고현정 나온거죠??",
          "speaker_id": "2",
          "time": "20210830 09:00"
        },
        {
          "id": "MDRW2100035565.1.20",
          "form": "네 맞아요 중년?배우들하구요",
          "original_form": "네 맞아요 중년?배우들하구요",
