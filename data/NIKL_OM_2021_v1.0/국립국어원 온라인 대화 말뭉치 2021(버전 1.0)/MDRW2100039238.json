      "utterance": [
        {
          "id": "MDRW2100039238.1.1",
          "form": "저녁드셨나여 name1님",
          "original_form": "저녁드셨나여 &name1&님",
          "speaker_id": "2",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039238.1.2",
          "form": "먹고 있어요. 요리 금수저 name2님은 드셨어요?",
          "original_form": "먹고 있어요. 요리 금수저 &name2&님은 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039238.1.3",
          "form": "아니요 흑흑 저는 이따 야식처럼 묵을랍니더",
          "original_form": "아니요 흑흑 저는 이따 야식처럼 묵을랍니더",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039238.1.4",
          "form": "뭐 드시나요?",
          "original_form": "뭐 드시나요?",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039238.1.5",
          "form": "ㅋㅋ 왜 안드셨나요",
          "original_form": "ㅋㅋ 왜 안드셨나요",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039238.1.6",
          "form": "전 고등어구이에 콩나물국 먹고 있어요!",
          "original_form": "전 고등어구이에 콩나물국 먹고 있어요!",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039238.1.7",
          "form": "집밥 최고",
          "original_form": "집밥 최고",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039238.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039238.1.9",
          "form": "직접 하신 건가여?",
          "original_form": "직접 하신 건가여?",
          "speaker_id": "2",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039238.1.10",
          "form": "창조주님 솜씨입니다",
          "original_form": "창조주님 솜씨입니다",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039238.1.11",
          "form": "제가 하면 최고가 아니쥬",
          "original_form": "제가 하면 최고가 아니쥬",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039238.1.12",
          "form": "그럼 오늘 계속",
          "original_form": "그럼 오늘 계속",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039238.1.13",
          "form": "생선을 드시는 거네유",
          "original_form": "생선을 드시는 거네유",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039238.1.14",
          "form": "ㅎㅎ 야식으로는 쥐포를 먹을까 합니다",
          "original_form": "ㅎㅎ 야식으로는 쥐포를 먹을까 합니다",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039238.1.15",
          "form": "님은 야식 뭐 드실건가요",
          "original_form": "님은 야식 뭐 드실건가요",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039238.1.16",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039238.1.17",
          "form": "돼지고기를 에프에 구워먹을 것이어요!",
          "original_form": "돼지고기를 에프에 구워먹을 것이어요!",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039238.1.18",
          "form": "헉 야식 스케일이 어마무시하군요ㄷㄷ",
          "original_form": "헉 야식 스케일이 어마무시하군요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039238.1.19",
          "form": "당연하져",
          "original_form": "당연하져",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039238.1.20",
          "form": "저녁 겸해서 먹는 건데!!",
          "original_form": "저녁 겸해서 먹는 건데!!",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039238.1.21",
          "form": "무슨 레시피가 있나요?",
          "original_form": "무슨 레시피가 있나요?",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039238.1.22",
          "form": "아님 그냥 삼겹살 굽듯 굽기만?",
          "original_form": "아님 그냥 삼겹살 굽듯 굽기만?",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039238.1.23",
          "form": "수육용 고기에다가",
          "original_form": "수육용 고기에다가",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039238.1.24",
          "form": "소금이랑 후추간만 해서 구울 거여요",
          "original_form": "소금이랑 후추간만 해서 구울 거여요",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039238.1.25",
          "form": "겉바속촉이랍니다",
          "original_form": "겉바속촉이랍니다",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039238.1.26",
          "form": "겉바속촉ㅜㅜ 알흠답군요",
          "original_form": "겉바속촉ㅜㅜ 알흠답군요",
          "speaker_id": "1",
          "time": "20210903 20:20"
        }
      ]
