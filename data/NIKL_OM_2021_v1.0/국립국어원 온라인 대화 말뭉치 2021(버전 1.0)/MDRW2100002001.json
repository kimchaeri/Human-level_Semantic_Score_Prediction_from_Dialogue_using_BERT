      "utterance": [
        {
          "id": "MDRW2100002001.1.1",
          "form": "티비 봐요?",
          "original_form": "티비 봐요?",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100002001.1.2",
          "form": "티비보나요?",
          "original_form": "티비보나요?",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002001.1.3",
          "form": "넹 오늘 펜트보려고욤",
          "original_form": "넹 오늘 펜트보려고욤",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002001.1.4",
          "form": "재밌어요?",
          "original_form": "재밌어요?",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002001.1.5",
          "form": "좀 막장이긴한데",
          "original_form": "좀 막장이긴한데",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002001.1.6",
          "form": "거의다끝나가염",
          "original_form": "거의다끝나가염",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002001.1.7",
          "form": "재미더있고 ㅋㅋ",
          "original_form": "재미더있고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002001.1.8",
          "form": "ㄱ시즌3이죠?",
          "original_form": "ㄱ시즌3이죠?",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002001.1.9",
          "form": "네네 3화정도 남았나",
          "original_form": "네네 3화정도 남았나",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002001.1.10",
          "form": "아직도 ㅋㅋ",
          "original_form": "아직도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002001.1.11",
          "form": "2화 더늘어났더라구요 ㅋㅋ",
          "original_form": "2화 더늘어났더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002001.1.12",
          "form": "길어져서",
          "original_form": "길어져서",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002001.1.13",
          "form": "저는 가끔씩 봐요",
          "original_form": "저는 가끔씩 봐요",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002001.1.14",
          "form": "가끔봐도 내용이 이해가시나여 ㅋㅋ",
          "original_form": "가끔봐도 내용이 이해가시나여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002001.1.15",
          "form": "잘 ㅋㅋ",
          "original_form": "잘 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002001.1.16",
          "form": "안가죠",
          "original_form": "안가죠",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002001.1.17",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002001.1.18",
          "form": "이것만 챙겨보고",
          "original_form": "이것만 챙겨보고",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002001.1.19",
          "form": "다른건 그냥 나오면 봐요 ㅋㅋ",
          "original_form": "다른건 그냥 나오면 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002001.1.20",
          "form": "자주 하더라구요",
          "original_form": "자주 하더라구요",
