          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100021000.1.13",
          "form": "전 원래 50분정도인데",
          "original_form": "전 원래 50분정도인데",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100021000.1.14",
          "form": "오 오래하시네요",
          "original_form": "오 오래하시네요",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100021000.1.15",
          "form": "요샌 땀이너무나니까 기운이 빠져서",
          "original_form": "요샌 땀이너무나니까 기운이 빠져서",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100021000.1.16",
