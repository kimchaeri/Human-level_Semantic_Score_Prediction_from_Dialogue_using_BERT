      "utterance": [
        {
          "id": "MDRW2100012719.1.1",
          "form": "Dog 키우시나요?",
          "original_form": "Dog 키우시나요?",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012719.1.2",
          "form": "아뇨! 저는 지금은 키우는게 없네요",
          "original_form": "아뇨! 저는 지금은 키우는게 없네요",
          "speaker_id": "1",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012719.1.3",
          "form": "그럼 전에 키워본 적 있어요?",
          "original_form": "그럼 전에 키워본 적 있어요?",
          "speaker_id": "2",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012719.1.4",
          "form": "네 씨몽키 키워봤어요",
          "original_form": "네 씨몽키 키워봤어요",
          "speaker_id": "1",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100012719.1.5",
          "form": "와 씨몽키 오랜만이다 ㅋㅋㅋ",
          "original_form": "와 씨몽키 오랜만이다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012719.1.6",
          "form": "걔네 진짜 생명체인가요?",
          "original_form": "걔네 진짜 생명체인가요?",
          "speaker_id": "2",
          "time": "20210825 00:16"
        },
        {
          "id": "MDRW2100012719.1.7",
          "form": "당연하죠...ㅠㅠ 살아있어욬ㅋㅋㅋ",
          "original_form": "당연하죠...ㅠㅠ 살아있어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012719.1.8",
          "form": "전 매번 실패했던 것 같아요..",
          "original_form": "전 매번 실패했던 것 같아요..",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012719.1.9",
          "form": "비결이 뭐에요?",
          "original_form": "비결이 뭐에요?",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012719.1.10",
          "form": "저는 진짜 교과서적 말밖에.. 적힌대로 해주면 잘자라욬ㅋㅋ",
          "original_form": "저는 진짜 교과서적 말밖에.. 적힌대로 해주면 잘자라욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012719.1.11",
          "form": "왜 나는 그마저도 실패했을까요 ㅠㅜ",
          "original_form": "왜 나는 그마저도 실패했을까요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012719.1.12",
          "form": "저런.. 뭐가 문제였을까요..",
          "original_form": "저런.. 뭐가 문제였을까요..",
          "speaker_id": "1",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100012719.1.13",
          "form": "혹시 키워보고 싶은 반려동물 있어요?",
          "original_form": "혹시 키워보고 싶은 반려동물 있어요?",
          "speaker_id": "2",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100012719.1.14",
          "form": "저는 고양이요! 근데 털알러지..",
          "original_form": "저는 고양이요! 근데 털알러지..",
          "speaker_id": "1",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100012719.1.15",
          "form": "앗.. ㅋㅋㅋㅋ 털알러지 심해요?",
          "original_form": "앗.. ㅋㅋㅋㅋ 털알러지 심해요?",
          "speaker_id": "2",
          "time": "20210825 00:20"
        },
        {
          "id": "MDRW2100012719.1.16",
          "form": "네 콧물나욬ㅋㅋ",
          "original_form": "네 콧물나욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:20"
        },
        {
          "id": "MDRW2100012719.1.17",
          "form": "헐.. 그럼 안되겠다.",
          "original_form": "헐.. 그럼 안되겠다.",
          "speaker_id": "2",
          "time": "20210825 00:21"
        },
        {
          "id": "MDRW2100012719.1.18",
          "form": "털빠짐 덜한 고양이도 있던거 같은데",
          "original_form": "털빠짐 덜한 고양이도 있던거 같은데",
          "speaker_id": "2",
          "time": "20210825 00:21"
        },
        {
          "id": "MDRW2100012719.1.19",
          "form": "닿기만 해도 털알러지 나요?ㅠㅠ",
          "original_form": "닿기만 해도 털알러지 나요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:21"
        },
        {
          "id": "MDRW2100012719.1.20",
          "form": "맞아욬ㅋㅋ",
          "original_form": "맞아욬ㅋㅋ",
