      "utterance": [
        {
          "id": "MDRW2100028718.1.1",
          "form": "부모님 잘 계시지?",
          "original_form": "부모님 잘 계시지?",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028718.1.2",
          "form": "옆에 침대에 누워계셔",
          "original_form": "옆에 침대에 누워계셔",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028718.1.3",
          "form": "독립하니까 좋니",
          "original_form": "독립하니까 좋니",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028718.1.4",
          "form": "지금 스스로 빨래 중이야",
          "original_form": "지금 스스로 빨래 중이야",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028718.1.5",
          "form": "힘들어도 스스로 해야 해",
          "original_form": "힘들어도 스스로 해야 해",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028718.1.6",
          "form": "지금 시간에 빨래 돌려도 돼?",
          "original_form": "지금 시간에 빨래 돌려도 돼?",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028718.1.7",
          "form": "난 집안일을 아예 할줄 몰라",
          "original_form": "난 집안일을 아예 할줄 몰라",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028718.1.8",
          "form": "외동딸은 다르다",
          "original_form": "외동딸은 다르다",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028718.1.9",
          "form": "뚝딱거려서 안시켜",
          "original_form": "뚝딱거려서 안시켜",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028718.1.10",
          "form": "우리는 다 1인 가구라 빨래 늦은 시간에 돌려도 다들 이해하더라",
          "original_form": "우리는 다 1인 가구라 빨래 늦은 시간에 돌려도 다들 이해하더라",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028718.1.11",
          "form": "밥은?",
          "original_form": "밥은?",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028718.1.12",
          "form": "밥은 할줄알지 밥솥에 하는건",
          "original_form": "밥은 할줄알지 밥솥에 하는건",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028718.1.13",
          "form": "집안일중에 뭐가 제일 하기싫냐",
          "original_form": "집안일중에 뭐가 제일 하기싫냐",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028718.1.14",
          "form": "반찬은 만들 줄 아니?",
          "original_form": "반찬은 만들 줄 아니?",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028718.1.15",
          "form": "나는 설거지가 제일 싫어",
          "original_form": "나는 설거지가 제일 싫어",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028718.1.16",
          "form": "계란후라이 할 줄 알아",
          "original_form": "계란후라이 할 줄 알아",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028718.1.17",
          "form": "설거지가 제일 자신있어",
          "original_form": "설거지가 제일 자신있어",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028718.1.18",
          "form": "너가 요리해주면 설거지 해줄게",
          "original_form": "너가 요리해주면 설거지 해줄게",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028718.1.19",
          "form": "나랑 같이 살자",
          "original_form": "나랑 같이 살자",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028718.1.20",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028718.1.21",
          "form": "육아는 니가 하렴",
          "original_form": "육아는 니가 하렴",
          "speaker_id": "1",
          "time": "20210902 21:34"
        }
      ]
    }
