      "utterance": [
        {
          "id": "MDRW2100001084.1.1",
          "form": "해외여행 가봤어?",
          "original_form": "해외여행 가봤어?",
          "speaker_id": "1",
          "time": "20210723 14:00"
        },
        {
          "id": "MDRW2100001084.1.2",
          "form": "아니 나 비행기 자체를 아예 못타봤어ㅋㅋ",
          "original_form": "아니 나 비행기 자체를 아예 못타봤어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 14:01"
        },
        {
          "id": "MDRW2100001084.1.3",
          "form": "아진짜?",
          "original_form": "아진짜?",
          "speaker_id": "1",
          "time": "20210723 14:13"
        },
        {
          "id": "MDRW2100001084.1.4",
          "form": "나도 한번 가봤어",
          "original_form": "나도 한번 가봤어",
          "speaker_id": "1",
          "time": "20210723 14:13"
        },
        {
          "id": "MDRW2100001084.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 14:13"
        },
        {
          "id": "MDRW2100001084.1.6",
          "form": "그럼최근에 간 여행지 어디야",
          "original_form": "그럼최근에 간 여행지 어디야",
          "speaker_id": "1",
          "time": "20210723 14:13"
        },
        {
          "id": "MDRW2100001084.1.7",
          "form": "최근이라기엔 좀 많이 예전인데 작년에 경주 다녀왔어!",
          "original_form": "최근이라기엔 좀 많이 예전인데 작년에 경주 다녀왔어!",
          "speaker_id": "2",
          "time": "20210723 14:14"
        },
        {
          "id": "MDRW2100001084.1.8",
          "form": "오 경주~",
          "original_form": "오 경주~",
          "speaker_id": "1",
          "time": "20210723 14:15"
        },
        {
          "id": "MDRW2100001084.1.9",
          "form": "나도 경주 너무 좋았는데!",
          "original_form": "나도 경주 너무 좋았는데!",
          "speaker_id": "1",
          "time": "20210723 14:15"
        },
        {
          "id": "MDRW2100001084.1.10",
          "form": "오오 경주 어디 가셨어요?",
          "original_form": "오오 경주 어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210723 15:02"
        },
        {
          "id": "MDRW2100001084.1.11",
          "form": "저는 대릉원이랑 동궁과 월지요!",
          "original_form": "저는 대릉원이랑 동궁과 월지요!",
          "speaker_id": "1",
          "time": "20210723 15:06"
        },
        {
          "id": "MDRW2100001084.1.12",
          "form": "황리단길에서 밥도 먹었어요",
          "original_form": "황리단길에서 밥도 먹었어요",
          "speaker_id": "1",
          "time": "20210723 15:06"
        },
        {
          "id": "MDRW2100001084.1.13",
          "form": "우오 이쁜 곳 많이 가셨네요",
          "original_form": "우오 이쁜 곳 많이 가셨네요",
          "speaker_id": "2",
          "time": "20210723 15:25"
        },
        {
          "id": "MDRW2100001084.1.14",
          "form": "ㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋ네",
          "speaker_id": "1",
          "time": "20210723 15:30"
        },
        {
          "id": "MDRW2100001084.1.15",
          "form": "name2님은 어디가셨어요?",
          "original_form": "&name2&님은 어디가셨어요?",
          "speaker_id": "1",
          "time": "20210723 15:30"
        },
        {
          "id": "MDRW2100001084.1.16",
          "form": "방학인데 여행 안가세요?",
          "original_form": "방학인데 여행 안가세요?",
          "speaker_id": "1",
          "time": "20210723 15:30"
        },
        {
          "id": "MDRW2100001084.1.17",
          "form": "방학이라서 서울 여행 계획 했었는데 코로나 때문에 다 무산 됐어요ㅠㅠ",
          "original_form": "방학이라서 서울 여행 계획 했었는데 코로나 때문에 다 무산 됐어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210723 17:05"
        },
        {
          "id": "MDRW2100001084.1.18",
          "form": "ㅠㅠ 코로나 너무 심하죠",
          "original_form": "ㅠㅠ 코로나 너무 심하죠",
          "speaker_id": "1",
          "time": "20210726 10:49"
        },
        {
          "id": "MDRW2100001084.1.19",
          "form": "맞아요... ㅠㅠㅠ",
          "original_form": "맞아요... ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210726 14:09"
        },
        {
          "id": "MDRW2100001084.1.20",
          "form": "코로나 잠잠해지면 서울 오세요!",
          "original_form": "코로나 잠잠해지면 서울 오세요!",
          "speaker_id": "1",
          "time": "20210726 15:38"
        },
        {
          "id": "MDRW2100001084.1.21",
          "form": "어디가시려고 계획하셨어요?",
          "original_form": "어디가시려고 계획하셨어요?",
          "speaker_id": "1",
          "time": "20210726 15:38"
        },
        {
          "id": "MDRW2100001084.1.22",
          "form": "잠실이랑 홍대 가려고 생각했어요",
          "original_form": "잠실이랑 홍대 가려고 생각했어요",
          "speaker_id": "2",
