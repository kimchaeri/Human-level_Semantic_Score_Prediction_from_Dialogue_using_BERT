      "utterance": [
        {
          "id": "MDRW2100044003.1.1",
          "form": "꼭 가고 싶은 여행지 있으세요?",
          "original_form": "꼭 가고 싶은 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044003.1.2",
          "form": "저 스페인 아직 못가봐서 가보고싶엉ㅅ",
          "original_form": "저 스페인 아직 못가봐서 가보고싶엉ㅅ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044003.1.3",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044003.1.4",
          "form": "저도 못가봤어요 가셔서 뭐 하고 싶으세요?",
          "original_form": "저도 못가봤어요 가셔서 뭐 하고 싶으세요?",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044003.1.5",
          "form": "여행지는 가서 걸어다니기만 해도 좋은거같아요 ㅋㅋㅋ",
          "original_form": "여행지는 가서 걸어다니기만 해도 좋은거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044003.1.6",
          "form": "갔다온 사람들 말로는 음식도 맛있고 볼거리도 많다더라구요",
          "original_form": "갔다온 사람들 말로는 음식도 맛있고 볼거리도 많다더라구요",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044003.1.7",
          "form": "나가서 음식 입에 안맞으면 그것고 넘나 힘든데요",
          "original_form": "나가서 음식 입에 안맞으면 그것고 넘나 힘든데요",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044003.1.8",
          "form": "맛있다니 좋겠네요^^",
          "original_form": "맛있다니 좋겠네요^^",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044003.1.9",
          "form": "가고싶은 여행지 있으세요?",
          "original_form": "가고싶은 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044003.1.10",
          "form": "전 아이스란드요",
          "original_form": "전 아이스란드요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044003.1.11",
          "form": "어떤것 때문에 가고싶으신거예요?",
          "original_form": "어떤것 때문에 가고싶으신거예요?",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044003.1.12",
          "form": "오로라 보고 싶어서요 ㅎ",
          "original_form": "오로라 보고 싶어서요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044003.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044003.1.14",
          "form": "저도 오로라 보고싶어요",
          "original_form": "저도 오로라 보고싶어요",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044003.1.15",
          "form": "오로라 보는 패키지도 팔더라구요",
          "original_form": "오로라 보는 패키지도 팔더라구요",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044003.1.16",
          "form": "정말요???",
          "original_form": "정말요???",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044003.1.17",
          "form": "그 패키지.가면 100프로 보나요?",
          "original_form": "그 패키지.가면 100프로 보나요?",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044003.1.18",
          "form": "아뇨 그건 아닌거같아요 ㅋㅋㅋ ㅠ",
          "original_form": "아뇨 그건 아닌거같아요 ㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044003.1.19",
          "form": "젤 확률 높은 날짜 2일인가 3일 찍어서 다니는거같긴 한데",
          "original_form": "젤 확률 높은 날짜 2일인가 3일 찍어서 다니는거같긴 한데",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044003.1.20",
          "form": "자연현상이라 변수는 있는거같아요",
          "original_form": "자연현상이라 변수는 있는거같아요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044003.1.21",
          "form": "전 무조건 보고 오고 싶어서 이주정도 길게 가려고 하거든요",
          "original_form": "전 무조건 보고 오고 싶어서 이주정도 길게 가려고 하거든요",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044003.1.22",
          "form": "오 그정도면 백프로 볼 수 있겠네요",
          "original_form": "오 그정도면 백프로 볼 수 있겠네요",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044003.1.23",
          "form": "23년이 태양 흑점 극대기라 오로라 볼 확률이높아요",
          "original_form": "23년이 태양 흑점 극대기라 오로라 볼 확률이높아요",
