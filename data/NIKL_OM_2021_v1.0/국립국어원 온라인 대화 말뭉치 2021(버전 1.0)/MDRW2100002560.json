      "utterance": [
        {
          "id": "MDRW2100002560.1.1",
          "form": "예전에 싸이월드 사용하셨어요?",
          "original_form": "예전에 싸이월드 사용하셨어요?",
          "speaker_id": "2",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002560.1.2",
          "form": "네ㅋㅋㅋ 사용하셨나요?",
          "original_form": "네ㅋㅋㅋ 사용하셨나요?",
          "speaker_id": "1",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002560.1.3",
          "form": "저도 예전에 했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 예전에 했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002560.1.4",
          "form": "싸이 다시 할 수 있다는 거 보셨어요?!",
          "original_form": "싸이 다시 할 수 있다는 거 보셨어요?!",
          "speaker_id": "2",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002560.1.5",
          "form": "아진짜요??ㅋㅋㅋ",
          "original_form": "아진짜요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002560.1.6",
          "form": "대박이네요 서비스재개인가요",
          "original_form": "대박이네요 서비스재개인가요",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002560.1.7",
          "form": "네네 그렇대요!",
          "original_form": "네네 그렇대요!",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002560.1.8",
          "form": "그러면 다시 사용하실거예요..??!",
          "original_form": "그러면 다시 사용하실거예요..??!",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002560.1.9",
          "form": "아니요ㅎㅎㅎ",
          "original_form": "아니요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002560.1.10",
          "form": "다시사용하시나요?",
          "original_form": "다시사용하시나요?",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002560.1.11",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002560.1.12",
          "form": "저는 이메일이랑 비밀번호를 까먹어서요..",
          "original_form": "저는 이메일이랑 비밀번호를 까먹어서요..",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002560.1.13",
          "form": "이메일이 지금은 없는 홈페이지거라 찾을 수가 없네요ㅠ",
          "original_form": "이메일이 지금은 없는 홈페이지거라 찾을 수가 없네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002560.1.14",
          "form": "또륵ㅠ 안타깝네요ㅈ",
          "original_form": "또륵ㅠ 안타깝네요ㅈ",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002560.1.15",
          "form": "진짜 추억의싸이가",
          "original_form": "진짜 추억의싸이가",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002560.1.16",
          "form": "되었군요",
          "original_form": "되었군요",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002560.1.17",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002560.1.18",
          "form": "저는 개인정보때문에 다시 부활하지않았으먄 좋겠어요",
          "original_form": "저는 개인정보때문에 다시 부활하지않았으먄 좋겠어요",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002560.1.19",
          "form": "아ㅋㅋㅋ 그러네요아무래도",
          "original_form": "아ㅋㅋㅋ 그러네요아무래도",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002560.1.20",
          "form": "흑역사 사진들도많겠어요",
          "original_form": "흑역사 사진들도많겠어요",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002560.1.21",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002560.1.22",
          "form": "그리고 싸이 이웃들 미니홈피에 있는 사진들도 신경쓰이네요",
          "original_form": "그리고 싸이 이웃들 미니홈피에 있는 사진들도 신경쓰이네요",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002560.1.23",
          "form": "그러게요진짜ㅋㅋㅋ",
          "original_form": "그러게요진짜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002560.1.24",
          "form": "그시절모두가 싸이를 했었으니",
          "original_form": "그시절모두가 싸이를 했었으니",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002560.1.25",
          "form": "그대로 남아있겠네요",
          "original_form": "그대로 남아있겠네요",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002560.1.26",
          "form": "진짜 추억이네요",
          "original_form": "진짜 추억이네요",
          "speaker_id": "2",
          "time": "20210820 15:36"
        }
      ]
    }
