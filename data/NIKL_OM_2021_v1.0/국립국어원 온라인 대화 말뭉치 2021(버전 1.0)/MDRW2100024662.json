      "utterance": [
        {
          "id": "MDRW2100024662.1.1",
          "form": "네ㅋㅋ하필이면 비올때",
          "original_form": "네ㅋㅋ하필이면 비올때",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024662.1.2",
          "form": "신나",
          "original_form": "신나",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024662.1.3",
          "form": "ㅋㅋㅋㅋ 비는 내친구",
          "original_form": "ㅋㅋㅋㅋ 비는 내친구",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024662.1.4",
          "form": "ㅋㅋㅋ차 발수코팅도 다 벗겨져서 앞도 안보이고",
          "original_form": "ㅋㅋㅋ차 발수코팅도 다 벗겨져서 앞도 안보이고",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100024662.1.5",
          "form": "조심 운전 해야겠네",
          "original_form": "조심 운전 해야겠네",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100024662.1.6",
          "form": "ㅋㅋㅋ여행의 묘미 아슬아슬운전",
          "original_form": "ㅋㅋㅋ여행의 묘미 아슬아슬운전",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024662.1.7",
          "form": "ㅋㅋㅋ 비가 퍼붓는 여행",
          "original_form": "ㅋㅋㅋ 비가 퍼붓는 여행",
          "speaker_id": "1",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024662.1.8",
          "form": "ㅋㅋ아이 신나",
          "original_form": "ㅋㅋ아이 신나",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024662.1.9",
          "form": "여행은 어떰?",
          "original_form": "여행은 어떰?",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100024662.1.10",
          "form": "다행이도 비가 안옴ㅋㅋ",
          "original_form": "다행이도 비가 안옴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100024662.1.11",
          "form": "리얼 다행 ㅋㅋ",
          "original_form": "리얼 다행 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100024662.1.12",
          "form": "왓으면고생",
          "original_form": "왓으면고생",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100024662.1.13",
          "form": "하지만 한것도 없음ㅋㅋ",
          "original_form": "하지만 한것도 없음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100024662.1.14",
          "form": "그래도 간것이 부럽",
          "original_form": "그래도 간것이 부럽",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100024662.1.15",
          "form": "ㅋㅋ와서 수발만들고",
          "original_form": "ㅋㅋ와서 수발만들고",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100024662.1.16",
          "form": "밥도 서서먹고",
          "original_form": "밥도 서서먹고",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100024662.1.17",
          "form": "아 부녀 수발러",
          "original_form": "아 부녀 수발러",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100024662.1.18",
          "form": "지금 왠일로 주무셔서 자유시간ㅋ",
          "original_form": "지금 왠일로 주무셔서 자유시간ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100024662.1.19",
          "form": "와우 ㅎㅎ",
          "original_form": "와우 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100024662.1.20",
          "form": "ㅋㅋ왠일",
          "original_form": "ㅋㅋ왠일",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100024662.1.21",
          "form": "피곤하셧대",
          "original_form": "피곤하셧대",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100024662.1.22",
          "form": "여행은 일찍 자는거라고",
          "original_form": "여행은 일찍 자는거라고",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100024662.1.23",
          "form": "ㅋㅋㅋ첨임ㅋㅋ맨날 미저리엿는데",
          "original_form": "ㅋㅋㅋ첨임ㅋㅋ맨날 미저리엿는데",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100024662.1.24",
          "form": "어제 아름다운 밤 보낸건가",
          "original_form": "어제 아름다운 밤 보낸건가",
