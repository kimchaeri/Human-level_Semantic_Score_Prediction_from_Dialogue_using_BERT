      "utterance": [
        {
          "id": "MDRW2100003615.1.1",
          "form": "아무래도 아침은 잘 안먹게 되더라구요",
          "original_form": "아무래도 아침은 잘 안먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210821 08:36"
        },
        {
          "id": "MDRW2100003615.1.2",
          "form": "아침 드세요?",
          "original_form": "아침 드세요?",
          "speaker_id": "1",
          "time": "20210821 08:36"
        },
        {
          "id": "MDRW2100003615.1.3",
          "form": "근데 지금 저 배고파요",
          "original_form": "근데 지금 저 배고파요",
          "speaker_id": "2",
          "time": "20210821 08:36"
        },
        {
          "id": "MDRW2100003615.1.4",
          "form": "저두요ㅅㄴ",
          "original_form": "저두요ㅅㄴ",
          "speaker_id": "1",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.5",
          "form": "아침 매일 시리얼먹어요",
          "original_form": "아침 매일 시리얼먹어요",
          "speaker_id": "2",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.6",
          "form": "출근전에",
          "original_form": "출근전에",
          "speaker_id": "2",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.7",
          "form": "시리얼 맛있겠다",
          "original_form": "시리얼 맛있겠다",
          "speaker_id": "1",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.8",
          "form": "어떤거 좋아해요?",
          "original_form": "어떤거 좋아해요?",
          "speaker_id": "1",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.9",
          "form": "전 아몬드후레이크",
          "original_form": "전 아몬드후레이크",
          "speaker_id": "1",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.10",
          "form": "저 그 그래놀라?",
          "original_form": "저 그 그래놀라?",
          "speaker_id": "2",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.11",
          "form": "그거 집에서 사놔서",
          "original_form": "그거 집에서 사놔서",
          "speaker_id": "2",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.12",
          "form": "와우 비싼거",
          "original_form": "와우 비싼거",
          "speaker_id": "1",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.13",
          "form": "그거 맛있잖아요",
          "original_form": "그거 맛있잖아요",
          "speaker_id": "1",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.14",
          "form": "그냥 주는대로 먹어요",
          "original_form": "그냥 주는대로 먹어요",
          "speaker_id": "2",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003615.1.16",
          "form": "씨리얼 눅눅한거좋아하나요",
          "original_form": "씨리얼 눅눅한거좋아하나요",
          "speaker_id": "1",
          "time": "20210821 08:38"
        },
        {
          "id": "MDRW2100003615.1.17",
          "form": "바삭한거?",
          "original_form": "바삭한거?",
          "speaker_id": "1",
          "time": "20210821 08:38"
        },
        {
          "id": "MDRW2100003615.1.18",
          "form": "좀 질린상태....",
          "original_form": "좀 질린상태....",
          "speaker_id": "2",
          "time": "20210821 08:38"
        },
        {
          "id": "MDRW2100003615.1.19",
          "form": "바삭한거요",
          "original_form": "바삭한거요",
          "speaker_id": "2",
          "time": "20210821 08:38"
        },
        {
          "id": "MDRW2100003615.1.20",
          "form": "아주 바삭한거",
          "original_form": "아주 바삭한거",
          "speaker_id": "2",
          "time": "20210821 08:38"
        },
        {
          "id": "MDRW2100003615.1.21",
          "form": "아무래도 바삭이 좋죠",
          "original_form": "아무래도 바삭이 좋죠",
          "speaker_id": "1",
          "time": "20210821 08:38"
        },
