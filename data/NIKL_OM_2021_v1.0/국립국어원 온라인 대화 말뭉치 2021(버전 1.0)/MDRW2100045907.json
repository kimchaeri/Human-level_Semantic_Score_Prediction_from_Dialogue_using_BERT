      "utterance": [
        {
          "id": "MDRW2100045907.1.1",
          "form": "이제 일어나서 먹을라구...",
          "original_form": "이제 일어나서 먹을라구...",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045907.1.2",
          "form": "근디 입맛없음ㅋㅋㅋㅋ",
          "original_form": "근디 입맛없음ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045907.1.3",
          "form": "아침에일어나서",
          "original_form": "아침에일어나서",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045907.1.4",
          "form": "밥맛없지ㅜ",
          "original_form": "밥맛없지ㅜ",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045907.1.5",
          "form": "완전ㅋㅋㅋ걍 아무것더.....",
          "original_form": "완전ㅋㅋㅋ걍 아무것더.....",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045907.1.6",
          "form": "언니는 밥 먹엇남?!",
          "original_form": "언니는 밥 먹엇남?!",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045907.1.7",
          "form": "노노요\r\n...",
          "original_form": "노노요\r\n...",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045907.1.8",
          "form": "너의 톡에깻지 키키",
          "original_form": "너의 톡에깻지 키키",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045907.1.9",
          "form": "아침은 몇시에 먹어? 벌써시간지난거가냐!?",
          "original_form": "아침은 몇시에 먹어? 벌써시간지난거가냐!?",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045907.1.10",
          "form": "ㅋㅋ내가 깨웠구먼ㅋㅋㅋ",
          "original_form": "ㅋㅋ내가 깨웠구먼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045907.1.11",
          "form": "아냐 ㅎ 근데 오빠올때쯤 일어나려햇음 ㅎ",
          "original_form": "아냐 ㅎ 근데 오빠올때쯤 일어나려햇음 ㅎ",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045907.1.12",
          "form": "지금 엄마가 먹자고 하는데...난 좀 쉴래 ㅋㅋㅋㅋㅋ",
          "original_form": "지금 엄마가 먹자고 하는데...난 좀 쉴래 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045907.1.13",
          "form": "큰일이구만 진짜 입맛안돌아오네",
          "original_form": "큰일이구만 진짜 입맛안돌아오네",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045907.1.14",
          "form": "흑흑ㅋㅋ 약먹어야되서 억지로 먹는 수준...",
          "original_form": "흑흑ㅋㅋ 약먹어야되서 억지로 먹는 수준...",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045907.1.15",
          "form": "진짜 괴롭겟구만..",
          "original_form": "진짜 괴롭겟구만..",
          "speaker_id": "1",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045907.1.16",
          "form": "ㅜㅠ 언니두 먹고 가야지",
          "original_form": "ㅜㅠ 언니두 먹고 가야지",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045907.1.17",
          "form": "아냐ㅜㅜㅜㅋㅋㅋ그럼 더힘들꺼같아",
          "original_form": "아냐ㅜㅜㅜㅋㅋㅋ그럼 더힘들꺼같아",
          "speaker_id": "1",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100045907.1.18",
          "form": "빈속으로 가게??ㅠㅠㅠㅠ",
          "original_form": "빈속으로 가게??ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045907.1.19",
          "form": "네ㅜㅜ 그게 나아요ㅠ",
          "original_form": "네ㅜㅜ 그게 나아요ㅠ",
