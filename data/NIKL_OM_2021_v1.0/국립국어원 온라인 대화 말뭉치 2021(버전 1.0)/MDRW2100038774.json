      "utterance": [
        {
          "id": "MDRW2100038774.1.1",
          "form": "맞죠 아직 0대0이네요ㅠㅠ",
          "original_form": "맞죠 아직 0대0이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100038774.1.2",
          "form": "무승부로 끝났나바요?",
          "original_form": "무승부로 끝났나바요?",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100038774.1.3",
          "form": "허무 ㅜㅜㅋ",
          "original_form": "허무 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100038774.1.4",
          "form": "그런거같야요ㅠ",
          "original_form": "그런거같야요ㅠ",
          "speaker_id": "2",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100038774.1.5",
          "form": "유투브에 에센셜 플레이리스트 좋아요",
          "original_form": "유투브에 에센셜 플레이리스트 좋아요",
          "speaker_id": "2",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100038774.1.6",
          "form": "오오 그거저 알아요",
          "original_form": "오오 그거저 알아요",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038774.1.7",
          "form": "오늘의집 가면",
          "original_form": "오늘의집 가면",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038774.1.8",
          "form": "티비에 맨날 틀어져있움 ㅋㅋㅋ",
          "original_form": "티비에 맨날 틀어져있움 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038774.1.9",
          "form": "아정말요",
          "original_form": "아정말요",
          "speaker_id": "2",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038774.1.10",
          "form": "아맞다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아맞다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038774.1.11",
          "form": "예쁜집 노트북 티비에 다 그거자나요",
          "original_form": "예쁜집 노트북 티비에 다 그거자나요",
          "speaker_id": "2",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038774.1.12",
          "form": "저 그거 먼지 몰라서",
          "original_form": "저 그거 먼지 몰라서",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038774.1.13",
          "form": "티비에 자동으로 탑재된건줄 ㅋㅋㅋㅋ",
          "original_form": "티비에 자동으로 탑재된건줄 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100038774.1.14",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100038774.1.15",
          "form": "그래서저도 알게됐어요 ㅋㅋ",
          "original_form": "그래서저도 알게됐어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:48"
        },
        {
          "id": "MDRW2100038774.1.16",
          "form": "들어 보니까 노래 주제별로 찰떡 이더라구요 ㅎㅎ",
          "original_form": "들어 보니까 노래 주제별로 찰떡 이더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100038774.1.17",
          "form": "그쵸 저도 오늘 틀어보려고요",
          "original_form": "그쵸 저도 오늘 틀어보려고요",
          "speaker_id": "2",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100038774.1.18",
          "form": "유투브 프리미엄 하셨나요?",
          "original_form": "유투브 프리미엄 하셨나요?",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100038774.1.19",
          "form": "프리미엄 2개월 무료체험 중이에요",
          "original_form": "프리미엄 2개월 무료체험 중이에요",
          "speaker_id": "2",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100038774.1.20",
          "form": "프리미엄이세요?",
          "original_form": "프리미엄이세요?",
          "speaker_id": "2",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100038774.1.21",
          "form": "오 저도 1개월 체험으로만 해봐써요 ㅜㅜ",
          "original_form": "오 저도 1개월 체험으로만 해봐써요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100038774.1.22",
          "form": "근데 너무 편해요",
          "original_form": "근데 너무 편해요",
          "speaker_id": "1",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100038774.1.23",
          "form": "그렇죠ㅠㅠ",
          "original_form": "그렇죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100038774.1.24",
          "form": "근데 자주 안봐서 돈 아까어요 프리미엄 ㅠㅠ",
          "original_form": "근데 자주 안봐서 돈 아까어요 프리미엄 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100038774.1.25",
          "form": "그쵸 너무 비싸버림",
          "original_form": "그쵸 너무 비싸버림",
