      "utterance": [
        {
          "id": "MDRW2100037343.1.1",
          "form": "음 글쎄 나는 독심술을 못해",
          "original_form": "음 글쎄 나는 독심술을 못해",
          "speaker_id": "2",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037343.1.2",
          "form": "말해봐 사줄수는 없지만 들어는줄께!",
          "original_form": "말해봐 사줄수는 없지만 들어는줄께!",
          "speaker_id": "2",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037343.1.3",
          "form": "흠 ㅠㅠ 힌트를 주자면 네가 갖고있는거야",
          "original_form": "흠 ㅠㅠ 힌트를 주자면 네가 갖고있는거야",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037343.1.4",
          "form": "엘지 빔프로젝트가ㅜ갖고싶어",
          "original_form": "엘지 빔프로젝트가ㅜ갖고싶어",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.5",
          "form": "내가?? 음 내 전공책인가",
          "original_form": "내가?? 음 내 전공책인가",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.6",
          "form": "아아 내 빔프로젝트는 십만원짜리야!",
          "original_form": "아아 내 빔프로젝트는 십만원짜리야!",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.7",
          "form": "아.. 그럼 40만원만 더 보태 줄 수ㅜ있을까?",
          "original_form": "아.. 그럼 40만원만 더 보태 줄 수ㅜ있을까?",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.8",
          "form": "근데 개인적으로 저가형도 충분히 쓸만한 것 같아!!",
          "original_form": "근데 개인적으로 저가형도 충분히 쓸만한 것 같아!!",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.9",
          "form": "빔프로젝트 정말 갖고싶어. 어때 쓰면 좋아?",
          "original_form": "빔프로젝트 정말 갖고싶어. 어때 쓰면 좋아?",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.10",
          "form": "소음은 심하지않은거야?",
          "original_form": "소음은 심하지않은거야?",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.11",
          "form": "응 나는 영화를 좋아하니까 아주 잘 써",
          "original_form": "응 나는 영화를 좋아하니까 아주 잘 써",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.12",
          "form": "넌 어디서 산거야?",
          "original_form": "넌 어디서 산거야?",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.13",
          "form": "나는 별로 안예민해서 영화를 보면 소음을 까먹어",
          "original_form": "나는 별로 안예민해서 영화를 보면 소음을 까먹어",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.14",
          "form": "나는 오늘의 집에서 샀어",
          "original_form": "나는 오늘의 집에서 샀어",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037343.1.15",
          "form": "나는 여러 사이트를 찾아봐야겟어",
          "original_form": "나는 여러 사이트를 찾아봐야겟어",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037343.1.16",
          "form": "니가 직접와서 내꺼 화질을 보고",
          "original_form": "니가 직접와서 내꺼 화질을 보고",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037343.1.17",
          "form": "결정해봐!",
          "original_form": "결정해봐!",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037343.1.18",
          "form": "아 좋아. 드디어 실제로 보는구나",
          "original_form": "아 좋아. 드디어 실제로 보는구나",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037343.1.19",
          "form": "합리적인 쇼핑을 할 수있을꺼야",
          "original_form": "합리적인 쇼핑을 할 수있을꺼야",
          "speaker_id": "2",
          "time": "20210831 22:43"
        }
