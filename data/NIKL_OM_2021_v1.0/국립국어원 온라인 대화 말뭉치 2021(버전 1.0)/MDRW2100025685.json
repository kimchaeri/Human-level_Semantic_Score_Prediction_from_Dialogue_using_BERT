      "utterance": [
        {
          "id": "MDRW2100025685.1.1",
          "form": "안녕하세요 점심 드셨나요?",
          "original_form": "안녕하세요 점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025685.1.2",
          "form": "점심 먹었습니다",
          "original_form": "점심 먹었습니다",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100025685.1.3",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100025685.1.4",
          "form": "저녁 못먹었어요ㅠ",
          "original_form": "저녁 못먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100025685.1.5",
          "form": "일하고있어서요",
          "original_form": "일하고있어서요",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100025685.1.6",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100025685.1.7",
          "form": "일끝나면 뭐드실건가요?",
          "original_form": "일끝나면 뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100025685.1.8",
          "form": "저는 끝나고 빵 먹으려구요!",
          "original_form": "저는 끝나고 빵 먹으려구요!",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100025685.1.9",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025685.1.10",
          "form": "평소 요리를 좋아하나요?",
          "original_form": "평소 요리를 좋아하나요?",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025685.1.11",
          "form": "요즘 배우고 있어요ㅎㅎ",
          "original_form": "요즘 배우고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100025685.1.12",
          "form": "요리 즐겨 하세요?",
          "original_form": "요리 즐겨 하세요?",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100025685.1.13",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100025685.1.14",
          "form": "가끔 하고 있습니다",
          "original_form": "가끔 하고 있습니다",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100025685.1.15",
          "form": "무슨 요리를 배우셨나요?",
          "original_form": "무슨 요리를 배우셨나요?",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100025685.1.16",
          "form": "음 각잡고 배우진 않고 그냥 레시피 쳐서 해먹는 편이에요!",
          "original_form": "음 각잡고 배우진 않고 그냥 레시피 쳐서 해먹는 편이에요!",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100025685.1.17",
          "form": "오늘 낮에는 카레 해먹었어요ㅎㅎ",
          "original_form": "오늘 낮에는 카레 해먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100025685.1.18",
          "form": "그러시군요 카레 맛있었겠어요",
          "original_form": "그러시군요 카레 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100025685.1.19",
          "form": "무슨빵드실거에요?",
          "original_form": "무슨빵드실거에요?",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100025685.1.20",
          "form": "크림치즈호두빵이요!",
          "original_form": "크림치즈호두빵이요!",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100025685.1.21",
          "form": "파리바게트에서 오는길에 사왔어요ㅎ",
          "original_form": "파리바게트에서 오는길에 사왔어요ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100025685.1.22",
          "form": "맛있게 드셨나요? 일이 늦게 끝났나봐요",
          "original_form": "맛있게 드셨나요? 일이 늦게 끝났나봐요",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100025685.1.23",
          "form": "아침은 드셨나요?",
          "original_form": "아침은 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100025685.1.24",
          "form": "네 맞아요ㅜㅜㅜㅜ",
          "original_form": "네 맞아요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100025685.1.25",
          "form": "아침은 먹었어요~",
          "original_form": "아침은 먹었어요~",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100025685.1.26",
          "form": "아침 드셨나요??",
          "original_form": "아침 드셨나요??",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100025685.1.27",
          "form": "네 빵으로 먹었어요",
          "original_form": "네 빵으로 먹었어요",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100025685.1.28",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100025685.1.29",
          "form": "아침에 김치볶음밥 해먹었어요~",
