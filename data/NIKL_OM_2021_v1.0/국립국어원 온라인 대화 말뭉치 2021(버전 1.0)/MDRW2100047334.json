        },
        {
          "id": "MDRW2100047334.1.2",
          "form": "네 안녕하세요\n당근 이용하세요?",
          "original_form": "네 안녕하세요\n당근 이용하세요?",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047334.1.3",
          "form": "자주 이용해요",
          "original_form": "자주 이용해요",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047334.1.4",
          "form": "저도요...특히 아이 책은 중고로 사도 괜찮더라구요",
          "original_form": "저도요...특히 아이 책은 중고로 사도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100047334.1.5",
          "form": "팔아본적있으세요?",
          "original_form": "팔아본적있으세요?",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100047334.1.6",
          "form": "네.. 책 옷 가방..팔아봤어요",
          "original_form": "네.. 책 옷 가방..팔아봤어요",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100047334.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100047334.1.8",
          "form": "최근에 버즈 팔아봤어요.",
          "original_form": "최근에 버즈 팔아봤어요.",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047334.1.9",
          "form": "버즈가 뭐에요??",
          "original_form": "버즈가 뭐에요??",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100047334.1.10",
          "form": "이어폰이요!",
          "original_form": "이어폰이요!",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100047334.1.11",
          "form": "아 ㅎ 당근거래잘되죠..ㅎ",
          "original_form": "아 ㅎ 당근거래잘되죠..ㅎ",
          "speaker_id": "1",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100047334.1.12",
          "form": "잘팔리더라고요",
