      "utterance": [
        {
          "id": "MDRW2100035017.1.1",
          "form": "기름기는 있는편이라ㅠ 다른 샘플도 알아볼까하고 있어요",
          "original_form": "기름기는 있는편이라ㅠ 다른 샘플도 알아볼까하고 있어요",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100035017.1.2",
          "form": "고단백사료인가여 기름기있으면 모질반질반질해질것같은데",
          "original_form": "고단백사료인가여 기름기있으면 모질반질반질해질것같은데",
          "speaker_id": "2",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100035017.1.3",
          "form": "네넵 고단백이예요!",
          "original_form": "네넵 고단백이예요!",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100035017.1.4",
          "form": "모질은 괜찮더라구요",
          "original_form": "모질은 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100035017.1.5",
          "form": "지금 어떤거 먹이고 계세요?",
          "original_form": "지금 어떤거 먹이고 계세요?",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100035017.1.6",
          "form": "지금은 뉴트리언스 서브제로먹이고있는데",
          "original_form": "지금은 뉴트리언스 서브제로먹이고있는데",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100035017.1.7",
          "form": "기호성은 엄청좋거든여 근데 오리젠먹일때에 비해 모질이 좀 떨어지는듯해요",
          "original_form": "기호성은 엄청좋거든여 근데 오리젠먹일때에 비해 모질이 좀 떨어지는듯해요",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100035017.1.8",
          "form": "똥은잘싸구여",
          "original_form": "똥은잘싸구여",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100035017.1.9",
          "form": "그전에 오리젠 드셨나봐요",
          "original_form": "그전에 오리젠 드셨나봐요",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100035017.1.10",
          "form": "주기적으로 사료 바꿔줘야한다는데 은근 어렵네요",
          "original_form": "주기적으로 사료 바꿔줘야한다는데 은근 어렵네요",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100035017.1.11",
          "form": "사료엔 좀 까다롭더라구요ㅜ",
          "original_form": "사료엔 좀 까다롭더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100035017.1.12",
          "form": "네ㅜ 저희애는 사료는 다요트빼곤 안가리는데 몸에서 티가나니까",
          "original_form": "네ㅜ 저희애는 사료는 다요트빼곤 안가리는데 몸에서 티가나니까",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100035017.1.13",
          "form": "어렵네요",
          "original_form": "어렵네요",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100035017.1.14",
          "form": "아ㅠ 그쵸",
          "original_form": "아ㅠ 그쵸",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100035017.1.15",
          "form": "영양제 먹이시는것 있으세요?",
          "original_form": "영양제 먹이시는것 있으세요?",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100035017.1.16",
          "form": "영양제 시도는했는데...",
          "original_form": "영양제 시도는했는데...",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100035017.1.17",
          "form": "얘가 사료만먹는다고했잖아요..ㅜ",
          "original_form": "얘가 사료만먹는다고했잖아요..ㅜ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100035017.1.18",
          "form": "억지로먹이면 토해버려요ㅜ",
          "original_form": "억지로먹이면 토해버려요ㅜ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100035017.1.19",
          "form": "천하의 못된사람이된것같은기분...",
          "original_form": "천하의 못된사람이된것같은기분...",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100035017.1.20",
          "form": "영양제도 다 안되는거군요..",
          "original_form": "영양제도 다 안되는거군요..",
          "speaker_id": "1",
          "time": "20210829 08:04"
        },
        {
          "id": "MDRW2100035017.1.21",
          "form": "name1님은 영양제뭐먹이고 계세요",
          "original_form": "&name1&님은 영양제뭐먹이고 계세요",
          "speaker_id": "2",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100035017.1.22",
          "form": "저는 유산균만 먹이고 있어요!",
          "original_form": "저는 유산균만 먹이고 있어요!",
          "speaker_id": "1",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100035017.1.23",
          "form": "먹일때랑안먹일때 차이가 어때요?",
          "original_form": "먹일때랑안먹일때 차이가 어때요?",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100035017.1.24",
          "form": "변이 좀 이쁜 것 같달까요?!",
          "original_form": "변이 좀 이쁜 것 같달까요?!",
          "speaker_id": "1",
          "time": "20210829 19:03"
        }
      ]
    }
