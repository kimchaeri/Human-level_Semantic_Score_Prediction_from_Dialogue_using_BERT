      "utterance": [
        {
          "id": "MDRW2100044307.1.1",
          "form": "중국면 먹으면 진ㅉㅏ",
          "original_form": "중국면 먹으면 진ㅉㅏ",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044307.1.2",
          "form": "이상해",
          "original_form": "이상해",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044307.1.3",
          "form": "중국면이 이상하다구?",
          "original_form": "중국면이 이상하다구?",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044307.1.4",
          "form": "비교하면 어떤데?",
          "original_form": "비교하면 어떤데?",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044307.1.5",
          "form": "대부분이 쫀득거리고",
          "original_form": "대부분이 쫀득거리고",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044307.1.6",
          "form": "떡같은 식감이야",
          "original_form": "떡같은 식감이야",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044307.1.7",
          "form": "떡..........?",
          "original_form": "떡..........?",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044307.1.8",
          "form": "응 떡",
          "original_form": "응 떡",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044307.1.9",
          "form": "그럼 한국면보다 질겨?",
          "original_form": "그럼 한국면보다 질겨?",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044307.1.10",
          "form": "떡같은게 싫으면 옥수수면추천",
          "original_form": "떡같은게 싫으면 옥수수면추천",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044307.1.11",
          "form": "옥수수면은 한국 면이랑 비슷해?",
          "original_form": "옥수수면은 한국 면이랑 비슷해?",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044307.1.12",
          "form": "응 쫀득거려",
          "original_form": "응 쫀득거려",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044307.1.13",
          "form": "쫀득쫀득해서 신기해",
          "original_form": "쫀득쫀득해서 신기해",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044307.1.14",
          "form": "지이이인짜 궁금하다",
          "original_form": "지이이인짜 궁금하다",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044307.1.15",
          "form": "쫄면 식감이랑 비슷해?",
          "original_form": "쫄면 식감이랑 비슷해?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044307.1.16",
          "form": "마라탕 국물에 쫄면을 말은 느낌이려나",
          "original_form": "마라탕 국물에 쫄면을 말은 느낌이려나",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044307.1.17",
          "form": "잘 모르겠어",
          "original_form": "잘 모르겠어",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044307.1.18",
          "form": "쫄면이 두꺼운만이려나",
          "original_form": "쫄면이 두꺼운만이려나",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044307.1.19",
          "form": "당면이랑 비교하면 어때?",
          "original_form": "당면이랑 비교하면 어때?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044307.1.20",
          "form": "당면보다 쫀득거려",
          "original_form": "당면보다 쫀득거려",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044307.1.21",
          "form": "거의 떡이랑 비슷해",
          "original_form": "거의 떡이랑 비슷해",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044307.1.22",
          "form": "뭐 진짜? 당면보다 옥수수면이 더 질겨?",
          "original_form": "뭐 진짜? 당면보다 옥수수면이 더 질겨?",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044307.1.23",
          "form": "밀떡보다는 땡글한맛",
          "original_form": "밀떡보다는 땡글한맛",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044307.1.24",
          "form": "아니 옥수수는 안 쫀득거리는 면",
          "original_form": "아니 옥수수는 안 쫀득거리는 면",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044307.1.25",
          "form": "그외의 중국면은 쫀득해",
          "original_form": "그외의 중국면은 쫀득해",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044307.1.26",
          "form": "아 중국 면이 당면이랑 옥수수면만 있는 게 아니야?",
          "original_form": "아 중국 면이 당면이랑 옥수수면만 있는 게 아니야?",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044307.1.27",
          "form": "ㅋㅋㅋㅋㅋ와 걔네는 면이 몇 종류야",
          "original_form": "ㅋㅋㅋㅋㅋ와 걔네는 면이 몇 종류야",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044307.1.28",
          "form": "응 그외에도 꽤 많아",
          "original_form": "응 그외에도 꽤 많아",
          "speaker_id": "2",
          "time": "20210907 22:19"
        }
      ]
    }
  ]
