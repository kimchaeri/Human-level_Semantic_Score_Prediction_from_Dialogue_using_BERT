      "utterance": [
        {
          "id": "MDRW2100036826.1.1",
          "form": "점심드셨어용",
          "original_form": "점심드셨어용",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036826.1.2",
          "form": "넹! 김치전먹었어요!",
          "original_form": "넹! 김치전먹었어요!",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036826.1.3",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036826.1.4",
          "form": "우와 김치전맛있었겠네요ㅜㅜ \r\n저는 아직요!  컵라면먹을까하구..ㅎㅎ",
          "original_form": "우와 김치전맛있었겠네요ㅜㅜ{emoji:♡} \r\n저는 아직요!  컵라면먹을까하구..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036826.1.5",
          "form": "앗ㅠ 언넝드셔야하는데!",
          "original_form": "앗ㅠ 언넝드셔야하는데!",
          "speaker_id": "1",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100036826.1.6",
          "form": "어떤컵라면 드셔용?",
          "original_form": "어떤컵라면 드셔용?",
          "speaker_id": "1",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100036826.1.7",
          "form": "진라면이요ㅋㅋㅋ좋아하지않는데 집에 이거뿐이네요ㅋㅋㅋㅋ",
          "original_form": "진라면이요ㅋㅋㅋ좋아하지않는데 집에 이거뿐이네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100036826.1.8",
          "form": "앗ㅋㅋ 선택권이 없었군욥",
          "original_form": "앗ㅋㅋ 선택권이 없었군욥",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100036826.1.9",
          "form": "네..ㅋㅋㅋ있는거먹어야죠..ㅎㅎㅎ 라면좋아하셔용",
          "original_form": "네..ㅋㅋㅋ있는거먹어야죠..ㅎㅎㅎ 라면좋아하셔용",
          "speaker_id": "2",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036826.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210831 16:11"
        },
        {
          "id": "MDRW2100036826.1.11",
          "form": "넹ㅋㅋ 짜파게티에 치즈 올린거나 안성탕면 좋아해용",
          "original_form": "넹ㅋㅋ 짜파게티에 치즈 올린거나 안성탕면 좋아해용",
          "speaker_id": "1",
          "time": "20210831 16:29"
        },
        {
          "id": "MDRW2100036826.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100036826.1.13",
          "form": "오 짜파게티치즈조합! ㅋㅋㅋ\r\n안성탕면도맛있죠\r\n저라면이면 웬만하면 다좋아하는데\r\n진비빔면 콩국수라면 좋아합니다ㅋㅋㅋ",
          "original_form": "오 짜파게티치즈조합! ㅋㅋㅋ\r\n안성탕면도맛있죠\r\n저라면이면 웬만하면 다좋아하는데\r\n진비빔면 콩국수라면 좋아합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100036826.1.14",
          "form": "크으 저도 좋아합니닷!",
          "original_form": "크으 저도 좋아합니닷!",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100036826.1.15",
          "form": "최고잖아여",
          "original_form": "최고잖아여",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100036826.1.16",
          "form": "팔도가최곤줄알았는데",
          "original_form": "팔도가최곤줄알았는데",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100036826.1.17",
          "form": "진비빔짱..",
          "original_form": "진비빔짱..",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036826.1.18",
          "form": "아니.. 다른거였나요..?",
          "original_form": "아니.. 다른거였나요..?",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036826.1.19",
          "form": "저 모르는 새 신상 낸거 실환가요..",
          "original_form": "저 모르는 새 신상 낸거 실환가요..",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100036826.1.20",
          "form": "우잉?  농심진비빔면 안드셔보셨나용!",
          "original_form": "우잉?  농심진비빔면 안드셔보셨나용!",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100036826.1.21",
          "form": "넹ㅠ",
          "original_form": "넹ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100036826.1.22",
          "form": "있던건가요?",
          "original_form": "있던건가요?",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100036826.1.23",
          "form": "라면길이 너무 좁았네요..",
          "original_form": "라면길이 너무 좁았네요..",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100036826.1.24",
          "form": "으어  겁나맛있어용",
          "original_form": "으어  겁나맛있어용",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100036826.1.25",
          "form": "진비빔면강추입니다ㅋㅋㅋ",
          "original_form": "진비빔면강추입니다ㅋㅋㅋ",
