      "utterance": [
        {
          "id": "MDRW2100037574.1.1",
          "form": "할땐 재밌어서 모르는데 내려와보니까 팔다리가 후덜덜",
          "original_form": "할땐 재밌어서 모르는데 내려와보니까 팔다리가 후덜덜",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037574.1.2",
          "form": "팔다리는 확실히 다 쓰구요",
          "original_form": "팔다리는 확실히 다 쓰구요",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037574.1.3",
          "form": "와와",
          "original_form": "와와",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037574.1.4",
          "form": "복근이나 등근육도 좀 쓰는거 같아요",
          "original_form": "복근이나 등근육도 좀 쓰는거 같아요",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037574.1.5",
          "form": "와 거의 다 사용하네요?",
          "original_form": "와 거의 다 사용하네요?",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037574.1.6",
          "form": "악력도 좀 쎄 야 되는거 같던데요 !",
          "original_form": "악력도 좀 쎄 야 되는거 같던데요 !",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037574.1.7",
          "form": "생각해보면 플랭크를 벽에 매달려서 하는거랑 비슷한것도 같아요",
          "original_form": "생각해보면 플랭크를 벽에 매달려서 하는거랑 비슷한것도 같아요",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037574.1.8",
          "form": "악력도 좀 있어야죠 네",
          "original_form": "악력도 좀 있어야죠 네",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100037574.1.9",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037574.1.10",
          "form": "전 플랭크를 벽에서라니..",
          "original_form": "전 플랭크를 벽에서라니..",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037574.1.11",
          "form": "불가능합니다 ㅋㅋ",
          "original_form": "불가능합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037574.1.12",
          "form": "정확히 플랭크는 아닌데 코어 근육 많이 쓰는거 같아요 ㅋㅋ",
          "original_form": "정확히 플랭크는 아닌데 코어 근육 많이 쓰는거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037574.1.13",
          "form": "오 그렇군요 !",
          "original_form": "오 그렇군요 !",
          "speaker_id": "1",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037574.1.14",
          "form": "보통 한번 수업하는데 몇분인가요?",
          "original_form": "보통 한번 수업하는데 몇분인가요?",
          "speaker_id": "1",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037574.1.15",
          "form": "아직은 초보라 역경사? 는 안하는데 그거까지하면 완전 근육맨 될듯",
          "original_form": "아직은 초보라 역경사? 는 안하는데 그거까지하면 완전 근육맨 될듯",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037574.1.16",
          "form": "와우 ㅋㅋㅋㅋ",
          "original_form": "와우 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037574.1.17",
          "form": "저는 토욜 아침마다 한번 가는데요",
          "original_form": "저는 토욜 아침마다 한번 가는데요",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037574.1.18",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037574.1.19",
          "form": "한번 가면 1시간 반걸려요",
          "original_form": "한번 가면 1시간 반걸려요",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037574.1.20",
          "form": "근데 1대 8 수업인",
          "original_form": "근데 1대 8 수업인",
