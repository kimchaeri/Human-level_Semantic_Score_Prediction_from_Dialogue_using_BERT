      "utterance": [
        {
          "id": "MDRW2100009532.1.1",
          "form": "결혼 하셨나요?",
          "original_form": "결혼 하셨나요?",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009532.1.2",
          "form": "아니요 미혼이에요",
          "original_form": "아니요 미혼이에요",
          "speaker_id": "1",
          "time": "20210823 11:53"
        },
        {
          "id": "MDRW2100009532.1.3",
          "form": "결혼하신분들 대단하신것 같아요",
          "original_form": "결혼하신분들 대단하신것 같아요",
          "speaker_id": "1",
          "time": "20210823 11:53"
        },
        {
          "id": "MDRW2100009532.1.4",
          "form": "저도 이럴줄 몰랐으니 결혼했네요. 만나는 분은 있으세요?",
          "original_form": "저도 이럴줄 몰랐으니 결혼했네요. 만나는 분은 있으세요?",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100009532.1.5",
          "form": "아니요ㅜ 음 요즘 코로나라서?",
          "original_form": "아니요ㅜ 음 요즘 코로나라서?",
          "speaker_id": "1",
          "time": "20210823 14:00"
        },
        {
          "id": "MDRW2100009532.1.6",
          "form": "ㅋㅋㅋ  아무래도 요즘은 모르는 사람 만나기 좀 그렇죠 ? ㅋㅋ",
          "original_form": "ㅋㅋㅋ  아무래도 요즘은 모르는 사람 만나기 좀 그렇죠 ? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100009532.1.7",
          "form": "근데 적극적으로 나서야 뭐가 생겨도 생길건데요",
          "original_form": "근데 적극적으로 나서야 뭐가 생겨도 생길건데요",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100009532.1.8",
          "form": "그러게요. 이상형은 어찌 되시나요?",
          "original_form": "그러게요. 이상형은 어찌 되시나요?",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100009532.1.9",
          "form": "음 특별한건 없네요 대화가 잘통하는 사람 ? ㅎ",
          "original_form": "음 특별한건 없네요 대화가 잘통하는 사람 ? ㅎ",
          "speaker_id": "1",
          "time": "20210823 17:27"
        },
        {
          "id": "MDRW2100009532.1.10",
          "form": "그게 제일 어려워요. 연애할땐 대화가 통하거든요? 결혼하면 갑자기 안통합니다 ㅋ",
          "original_form": "그게 제일 어려워요. 연애할땐 대화가 통하거든요? 결혼하면 갑자기 안통합니다 ㅋ",
          "speaker_id": "2",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100009532.1.11",
          "form": "저런 ㅋㅋㅋ",
          "original_form": "저런 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009532.1.12",
          "form": "분명 그사람과 결혼했는데 ㅋㅋ",
          "original_form": "분명 그사람과 결혼했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009532.1.13",
          "form": "경제 관념도 비슷해야할것같아요",
          "original_form": "경제 관념도 비슷해야할것같아요",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009532.1.14",
          "form": "근데 연애할땐 경제 관념을 잘 모르겠더라고요",
          "original_form": "근데 연애할땐 경제 관념을 잘 모르겠더라고요",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100009532.1.15",
