      "utterance": [
        {
          "id": "MDRW2100002803.1.1",
          "form": "지역 커뮤니티에 가입했니??",
          "original_form": "지역 커뮤니티에 가입했니??",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002803.1.2",
          "form": "이번 클라이밍대회 봤니?",
          "original_form": "이번 클라이밍대회 봤니?",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002803.1.3",
          "form": "봤지.",
          "original_form": "봤지.",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002803.1.4",
          "form": "선수들의 근육이 참 멋있어.",
          "original_form": "선수들의 근육이 참 멋있어.",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002803.1.5",
          "form": "너도 클라이밍을 해 본 적 있니?",
          "original_form": "너도 클라이밍을 해 본 적 있니?",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002803.1.6",
          "form": "응 나는 예전에 배워본적있어",
          "original_form": "응 나는 예전에 배워본적있어",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002803.1.7",
          "form": "그래서 선수들이 더 대단해보이더라",
          "original_form": "그래서 선수들이 더 대단해보이더라",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002803.1.8",
          "form": "넌 한번 해보고싶지않아?",
          "original_form": "넌 한번 해보고싶지않아?",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002803.1.9",
          "form": "오 해보고싶어",
          "original_form": "오 해보고싶어",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002803.1.10",
          "form": "어디서 해본거야?",
          "original_form": "어디서 해본거야?",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002803.1.11",
          "form": "나는 동네에 실내클라이밍장이 있어",
          "original_form": "나는 동네에 실내클라이밍장이 있어",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002803.1.12",
          "form": "너네집과도 멀지 않을꺼야",
          "original_form": "너네집과도 멀지 않을꺼야",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002803.1.13",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002803.1.14",
          "form": "나도 다음에 도전해볼게",
          "original_form": "나도 다음에 도전해볼게",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002803.1.15",
          "form": "관람도 많이 해봤겠다",
          "original_form": "관람도 많이 해봤겠다",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002803.1.16",
          "form": "맞아 다른사람이 잘하면 재미있어",
          "original_form": "맞아 다른사람이 잘하면 재미있어",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002803.1.17",
          "form": "특히 관장님이 엄청 잘하셨거든",
          "original_form": "특히 관장님이 엄청 잘하셨거든",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002803.1.18",
          "form": "우와 그래서 관장님이구나",
          "original_form": "우와 그래서 관장님이구나",
          "speaker_id": "2",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002803.1.19",
          "form": "경사가 심할수록 힘든데 관장님은 잘 올라가셨어",
          "original_form": "경사가 심할수록 힘든데 관장님은 잘 올라가셨어",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002803.1.20",
          "form": "포켓몬스터관장님같아",
          "original_form": "포켓몬스터관장님같아",
          "speaker_id": "2",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002803.1.21",
          "form": "대단해!",
          "original_form": "대단해!",
          "speaker_id": "2",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002803.1.22",
          "form": "맞아맞아 아무나 하는게 아닌거같아",
          "original_form": "맞아맞아 아무나 하는게 아닌거같아",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002803.1.23",
          "form": "너는 이제 안하니ㅡ",
          "original_form": "너는 이제 안하니ㅡ",
          "speaker_id": "2",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002803.1.24",
          "form": "응 아무래도 돈이랑 시간이 부족해서",
          "original_form": "응 아무래도 돈이랑 시간이 부족해서",
          "speaker_id": "1",
          "time": "20210820 16:31"
        }
      ]
    }
  ]
