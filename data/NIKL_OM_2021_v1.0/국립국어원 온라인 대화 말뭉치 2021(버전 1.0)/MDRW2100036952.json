        },
        {
          "id": "MDRW2100036952.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036952.1.4",
          "form": "저 모바일게임이요!",
          "original_form": "저 모바일게임이요!",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036952.1.5",
          "form": "어떤게임하세요?",
          "original_form": "어떤게임하세요?",
          "speaker_id": "1",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036952.1.6",
          "form": "꿈의 집이요!",
          "original_form": "꿈의 집이요!",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036952.1.7",
          "form": "혹시 아세요?",
          "original_form": "혹시 아세요?",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036952.1.8",
          "form": "오 그 대머리",
          "original_form": "오 그 대머리",
          "speaker_id": "1",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036952.1.9",
          "form": "그 아저씨많이봤어요 광고에",
          "original_form": "그 아저씨많이봤어요 광고에",
          "speaker_id": "1",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036952.1.10",
          "form": "다들 대머리로 아시네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다들 대머리로 아시네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036952.1.11",
          "form": "ㅋㅋㅋㅋㅋ네 대머리맞져?",
          "original_form": "ㅋㅋㅋㅋㅋ네 대머리맞져?",
          "speaker_id": "1",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036952.1.12",
          "form": "네ㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036952.1.13",
          "form": "그 아저씨가 넘강력해서 ㅋㅋㅋ",
          "original_form": "그 아저씨가 넘강력해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036952.1.14",
          "form": "광고를 많이 뿌리긴했죠",
          "original_form": "광고를 많이 뿌리긴했죠",
          "speaker_id": "2",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036952.1.15",
          "form": "어떤게임이에요?",
          "original_form": "어떤게임이에요?",
          "speaker_id": "1",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036952.1.16",
          "form": "퍼즐게임...?",
          "original_form": "퍼즐게임...?",
          "speaker_id": "2",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036952.1.17",
          "form": "오오..? 전혀예상치못했어요",
