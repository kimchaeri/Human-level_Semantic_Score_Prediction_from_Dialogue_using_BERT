      "utterance": [
        {
          "id": "MDRW2100018675.1.1",
          "form": "자취나 기숙사 경함이 있으신가요?",
          "original_form": "자취나 기숙사 경함이 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100018675.1.2",
          "form": "네 둘 다 있어요 ㅎㅎ",
          "original_form": "네 둘 다 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018675.1.3",
          "form": "기숙사 너무 최악이였어요",
          "original_form": "기숙사 너무 최악이였어요",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018675.1.4",
          "form": "룸메이트와 같이 사용하셨나요?",
          "original_form": "룸메이트와 같이 사용하셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018675.1.5",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018675.1.6",
          "form": "룸메랑 잘 안맞았네요",
          "original_form": "룸메랑 잘 안맞았네요",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018675.1.7",
          "form": "생활패턴 때문이었나요?",
          "original_form": "생활패턴 때문이었나요?",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018675.1.8",
          "form": "맞아요. 저는 12시-1시에 취침하는 편인데",
          "original_form": "맞아요. 저는 12시-1시에 취침하는 편인데",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018675.1.9",
          "form": "그 친구는 밤 9-10시만 되면 불 딱 꺼버려서요..",
          "original_form": "그 친구는 밤 9-10시만 되면 불 딱 꺼버려서요..",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018675.1.10",
          "form": "오....흔치 않은 바른생활인데 하필 룸메가 그러면 ㅠㅠㅠ",
          "original_form": "오....흔치 않은 바른생활인데 하필 룸메가 그러면 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018675.1.11",
          "form": "그거까진 괜찮은데 말 없이 불을 딱 끄니까 당황스럽더라구요",
          "original_form": "그거까진 괜찮은데 말 없이 불을 딱 끄니까 당황스럽더라구요",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018675.1.12",
          "form": "심지어 저 시험 공부 하는데도 불 끄더라니까요?",
          "original_form": "심지어 저 시험 공부 하는데도 불 끄더라니까요?",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018675.1.13",
          "form": "아니....? 장난아닌 분이네요, 아니ㅋㅋㅋ 어이가 없으셨겠는데요????",
          "original_form": "아니....? 장난아닌 분이네요, 아니ㅋㅋㅋ 어이가 없으셨겠는데요????",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018675.1.14",
          "form": "그니까요 진짜 개황당..",
          "original_form": "그니까요 진짜 개황당..",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018675.1.15",
          "form": "아침에는 또 화장실 불 켜놓고 가서 안에 볼일 보는 줄 알았는데",
          "original_form": "아침에는 또 화장실 불 켜놓고 가서 안에 볼일 보는 줄 알았는데",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018675.1.16",
          "form": "아무도 없는 거 있죠... ㅠㅠㅠ",
          "original_form": "아무도 없는 거 있죠... ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018675.1.17",
          "form": "진짜 룸메로써는 최악이네요...그럴거면 자취를 하지ㅠ",
          "original_form": "진짜 룸메로써는 최악이네요...그럴거면 자취를 하지ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018675.1.18",
          "form": "결국 제가 여름방학 때 원룸 구해서 나갔어요..",
          "original_form": "결국 제가 여름방학 때 원룸 구해서 나갔어요..",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018675.1.19",
          "form": "저희 부모님은 참고 살으라고만 하시고 ㅠㅠ",
          "original_form": "저희 부모님은 참고 살으라고만 하시고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018675.1.20",
          "form": "서운하고 많이 힘드셨겠어요....한 학기동안 얼마나 짜증나셨을까요ㅠㅠ",
          "original_form": "서운하고 많이 힘드셨겠어요....한 학기동안 얼마나 짜증나셨을까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018675.1.21",
          "form": "그니까요 ㅎㅎ 그래도 가족이 보증금이랑 월세 내줘서 좋았어요",
          "original_form": "그니까요 ㅎㅎ 그래도 가족이 보증금이랑 월세 내줘서 좋았어요",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018675.1.22",
          "form": "그땐 몰랐는데 새삼 감사하네요",
          "original_form": "그땐 몰랐는데 새삼 감사하네요",
          "speaker_id": "2",
          "time": "20210827 13:38"
        }
      ]
    }
