      "utterance": [
        {
          "id": "MDRW2100043860.1.1",
          "form": "버터에 마늘이라니 너무 끝내주는 풍미일 것 같아요",
          "original_form": "버터에 마늘이라니 너무 끝내주는 풍미일 것 같아요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043860.1.2",
          "form": "침나온다..",
          "original_form": "침나온다..",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043860.1.3",
          "form": "진짜 짱입니다 조리법도 간단하니까",
          "original_form": "진짜 짱입니다 조리법도 간단하니까",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043860.1.4",
          "form": "시간되시면 꼭 드셔보세요~!",
          "original_form": "시간되시면 꼭 드셔보세요~!",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043860.1.5",
          "form": "꼭꼭 해먹을거예요 마늘 진짜 좋아하거든요!!!",
          "original_form": "꼭꼭 해먹을거예요 마늘 진짜 좋아하거든요!!!",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043860.1.6",
          "form": "지금 먹고싶은 간식이 있는데 맞춰보실래요ㅋㅋㅋㅋㅋ?",
          "original_form": "지금 먹고싶은 간식이 있는데 맞춰보실래요ㅋㅋㅋㅋㅋ?",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043860.1.7",
          "form": "문제폭탄",
          "original_form": "문제폭탄",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043860.1.8",
          "form": "ㅋㅋㅋㅋㅋ아 그냥 한번 바로 맞춰볼께요 초코파이 인가요",
          "original_form": "ㅋㅋㅋㅋㅋ아 그냥 한번 바로 맞춰볼께요 초코파이 인가요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043860.1.9",
          "form": "아닙니다~!!! 초코파이도 맛있죠 쫀득쫀득",
          "original_form": "아닙니다~!!! 초코파이도 맛있죠 쫀득쫀득",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043860.1.10",
          "form": "맞아요 쫀득쫀득",
          "original_form": "맞아요 쫀득쫀득",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043860.1.11",
          "form": "그러면... 계란빵? ㅋㅋㅋ",
          "original_form": "그러면... 계란빵? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043860.1.12",
          "form": "계란빵도 아닙니다~! ㅠㅠㅜㅜㅜㅜㅠ저희지역은 계란빵 포장마차를 볼 수 없습니다",
          "original_form": "계란빵도 아닙니다~! ㅠㅠㅜㅜㅜㅜㅠ저희지역은 계란빵 포장마차를 볼 수 없습니다",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043860.1.13",
          "form": "멸종했어요",
          "original_form": "멸종했어요",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043860.1.14",
          "form": "악 근데 저도 계란빵은 지하철 타고 가야 먹을수있더라고요",
          "original_form": "악 근데 저도 계란빵은 지하철 타고 가야 먹을수있더라고요",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043860.1.15",
          "form": "ㅠㅠ 맛있는데...",
          "original_form": "ㅠㅠ 맛있는데...",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043860.1.16",
          "form": "저두요ㅋㅋㅋㅋ 계란빵보다 교통비가 더 들겠어요",
          "original_form": "저두요ㅋㅋㅋㅋ 계란빵보다 교통비가 더 들겠어요",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043860.1.17",
          "form": "계란빵은 아닙니다~!",
          "original_form": "계란빵은 아닙니다~!",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043860.1.18",
          "form": "혹시 아이스크림인가요?",
          "original_form": "혹시 아이스크림인가요?",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043860.1.19",
          "form": "아니요~! 아이스크림은 아니예요",
          "original_form": "아니요~! 아이스크림은 아니예요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043860.1.20",
          "form": "그러면 음... 뜨거운 음식인가요",
          "original_form": "그러면 음... 뜨거운 음식인가요",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043860.1.21",
          "form": "아니요! 뜨겁지도 차갑지도 않은 간식입니다",
          "original_form": "아니요! 뜨겁지도 차갑지도 않은 간식입니다",
