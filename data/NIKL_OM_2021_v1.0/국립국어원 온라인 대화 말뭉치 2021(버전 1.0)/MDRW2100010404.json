      "utterance": [
        {
          "id": "MDRW2100010404.1.1",
          "form": "최근에 구매하신 물건 있으신가요?",
          "original_form": "최근에 구매하신 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 17:30"
        },
        {
          "id": "MDRW2100010404.1.2",
          "form": "런닝화 보려고 하고 있어요 구매하신거 있으세요?",
          "original_form": "런닝화 보려고 하고 있어요 구매하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 17:31"
        },
        {
          "id": "MDRW2100010404.1.3",
          "form": "저는 후드집업을 샀어요! 겨울대비 벌써하고 있답니다",
          "original_form": "저는 후드집업을 샀어요! 겨울대비 벌써하고 있답니다",
          "speaker_id": "2",
          "time": "20210823 17:31"
        },
        {
          "id": "MDRW2100010404.1.4",
          "form": "트레이닝 바지도 사고싶네요...",
          "original_form": "트레이닝 바지도 사고싶네요...",
          "speaker_id": "2",
          "time": "20210823 17:32"
        },
        {
          "id": "MDRW2100010404.1.5",
          "form": "저도요ㅎㅎ 운동관련으로 사고있어요ㅋㅋ",
          "original_form": "저도요ㅎㅎ 운동관련으로 사고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:32"
        },
        {
          "id": "MDRW2100010404.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 퓨즈서울에서 나온바지 넘 탐나더라고요 레깅스는 못입겠는데 펄럭거리지 않는 옷을 찾고 있었거든요ㅋㅋㅋㅋㅋㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 퓨즈서울에서 나온바지 넘 탐나더라고요 레깅스는 못입겠는데 펄럭거리지 않는 옷을 찾고 있었거든요ㅋㅋㅋㅋㅋㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010404.1.7",
          "form": "오 저도 그런데 찾아봐야겠네요ㅎㅎ 저도 그런옷 갖구싶어서ㅎㅎ",
          "original_form": "오 저도 그런데 찾아봐야겠네요ㅎㅎ 저도 그런옷 갖구싶어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 17:55"
        },
        {
          "id": "MDRW2100010404.1.8",
          "form": "바지 진짜 생각보다 잘 나왔더라고요ㅠㅠ 히든주머니? 하튼 뒤에 있는 포켓도 좋고...사고싶어요",
          "original_form": "바지 진짜 생각보다 잘 나왔더라고요ㅠㅠ 히든주머니? 하튼 뒤에 있는 포켓도 좋고...사고싶어요",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010404.1.9",
          "form": "레깅스는 너무 부담스럽고 딱이네요ㅎㅎ",
          "original_form": "레깅스는 너무 부담스럽고 딱이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010404.1.10",
          "form": "그쵸! 런닝화는 어떤 걸 생각하고 계세요?",
          "original_form": "그쵸! 런닝화는 어떤 걸 생각하고 계세요?",
          "speaker_id": "2",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100010404.1.11",
          "form": "저는 그냥 아무운동화 신고 뛰었었는데 발이 아프더라고요ㅠㅠ",
          "original_form": "저는 그냥 아무운동화 신고 뛰었었는데 발이 아프더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010404.1.12",
          "form": "나이키랑 뉴발중에 고민인데 아무래도 에어있는걸 골라야될거같아요",
          "original_form": "나이키랑 뉴발중에 고민인데 아무래도 에어있는걸 골라야될거같아요",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010404.1.13",
          "form": "맞아요! 그래야 무릎에도 무리가 덜 가고 좋잖아요!",
          "original_form": "맞아요! 그래야 무릎에도 무리가 덜 가고 좋잖아요!",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100010404.1.14",
          "form": "운동하는 분들 보면 나이키 많이 신더라고요",
          "original_form": "운동하는 분들 보면 나이키 많이 신더라고요",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100010404.1.15",
          "form": "돌고돌아서 결국 나이키인거같아요ㅋㅋ",
          "original_form": "돌고돌아서 결국 나이키인거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:56"
        }
      ]
