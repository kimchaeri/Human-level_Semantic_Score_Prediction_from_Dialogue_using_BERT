      "utterance": [
        {
          "id": "MDRW2100036521.1.1",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036521.1.2",
          "form": "요즘 어떤 옷차림을 주로 입으시여?",
          "original_form": "요즘 어떤 옷차림을 주로 입으시여?",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036521.1.3",
          "form": "밖에나갈땐 반팔 긴바지요",
          "original_form": "밖에나갈땐 반팔 긴바지요",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100036521.1.4",
          "form": "name1님은여",
          "original_form": "&name1&님은여",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036521.1.5",
          "form": "저도 반팔 긴바지 요",
          "original_form": "저도 반팔 긴바지 요",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036521.1.6",
          "form": "요즘 날씨가 딱좋은것같아요. 더 추운건 싫어서",
          "original_form": "요즘 날씨가 딱좋은것같아요. 더 추운건 싫어서",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100036521.1.7",
          "form": "저도용ㅎㅎ 선선한거 좋네요",
          "original_form": "저도용ㅎㅎ 선선한거 좋네요",
          "speaker_id": "1",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100036521.1.8",
          "form": "목욕도 맨날하능것도 스트레스더라고요",
          "original_form": "목욕도 맨날하능것도 스트레스더라고요",
          "speaker_id": "2",
          "time": "20210831 00:55"
        },
        {
          "id": "MDRW2100036521.1.9",
          "form": "그쵸 목욕 맨날하면 개운하긴 하지만 귀찮아요",
          "original_form": "그쵸 목욕 맨날하면 개운하긴 하지만 귀찮아요",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100036521.1.10",
          "form": "이젠추워서.",
          "original_form": "이젠추워서.",
          "speaker_id": "2",
          "time": "20210831 01:02"
        },
        {
          "id": "MDRW2100036521.1.11",
          "form": "네 춥죠 찬물샤워 못하네여",
          "original_form": "네 춥죠 찬물샤워 못하네여",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036521.1.12",
          "form": "이제 뜨거운샤워만 가능해요",
          "original_form": "이제 뜨거운샤워만 가능해요",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036521.1.13",
          "form": "네이제 각질도 올라올 시기네여",
          "original_form": "네이제 각질도 올라올 시기네여",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036521.1.14",
          "form": "네.. 그럼 핸드크림 열심히 바르세여",
          "original_form": "네.. 그럼 핸드크림 열심히 바르세여",
          "speaker_id": "2",
