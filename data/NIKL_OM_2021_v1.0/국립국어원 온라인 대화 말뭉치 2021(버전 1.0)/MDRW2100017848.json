      "utterance": [
        {
          "id": "MDRW2100017848.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017848.1.2",
          "form": "날씨가 많이 선선해졌네요",
          "original_form": "날씨가 많이 선선해졌네요",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017848.1.3",
          "form": "안녕하세요 맞아요  이제 비도 덜오고요!",
          "original_form": "안녕하세요 맞아요  이제 비도 덜오고요!",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017848.1.4",
          "form": "네 근데 저희 지역은 내일 비온다더라고요",
          "original_form": "네 근데 저희 지역은 내일 비온다더라고요",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017848.1.5",
          "form": "아 찾아보니 저희 지역도네요 ㅜㅜ",
          "original_form": "아 찾아보니 저희 지역도네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017848.1.6",
          "form": "장마지나면 정말 가을 시작되겠엉ㆍ르",
          "original_form": "장마지나면 정말 가을 시작되겠엉ㆍ르",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017848.1.7",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017848.1.8",
          "form": "ㅠㅠ그러게요",
          "original_form": "ㅠㅠ그러게요",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017848.1.9",
          "form": "가을장마 온다던데",
          "original_form": "가을장마 온다던데",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017848.1.10",
          "form": "가장 좋아하는 계절은 언제이신가요?",
          "original_form": "가장 좋아하는 계절은 언제이신가요?",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017848.1.11",
          "form": "저는 봄을 좋아해요",
          "original_form": "저는 봄을 좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017848.1.12",
          "form": "점점 따뜻해지는 날씨가 좋아요",
          "original_form": "점점 따뜻해지는 날씨가 좋아요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017848.1.13",
          "form": "저는 가을을 좋아해서 곧 올 가을이 기대돼요~",
          "original_form": "저는 가을을 좋아해서 곧 올 가을이 기대돼요~",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017848.1.14",
          "form": "아 그럼 선선해지는 날씨가 더 반가우시겠네요",
          "original_form": "아 그럼 선선해지는 날씨가 더 반가우시겠네요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017848.1.15",
          "form": "네 덥지도 춥지도 않고 딱 좋은것같아요",
          "original_form": "네 덥지도 춥지도 않고 딱 좋은것같아요",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017848.1.16",
          "form": "네 요즘은 선선해서 다니기 좋죠~",
          "original_form": "네 요즘은 선선해서 다니기 좋죠~",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017848.1.17",
          "form": "비만 좀 덜 오면 좋을텐데",
          "original_form": "비만 좀 덜 오면 좋을텐데",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017848.1.18",
          "form": "그러게요 저는 비오는거 별로 안좋아해서요ㅜㅜ",
          "original_form": "그러게요 저는 비오는거 별로 안좋아해서요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017848.1.19",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017848.1.20",
          "form": "비 오면 컨디션도 축 처지고",
          "original_form": "비 오면 컨디션도 축 처지고",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017848.1.21",
          "form": "맞아요 날씨에 따라 되게 기분이 다운되는것 같앙노",
          "original_form": "맞아요 날씨에 따라 되게 기분이 다운되는것 같앙노",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017848.1.22",
          "form": "같아요",
          "original_form": "같아요",
          "speaker_id": "2",
          "time": "20210826 21:42"
        }
      ]
