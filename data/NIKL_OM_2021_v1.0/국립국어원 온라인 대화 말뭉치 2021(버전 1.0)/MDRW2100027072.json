      "utterance": [
        {
          "id": "MDRW2100027072.1.1",
          "form": "운동경기 좋아하세요?",
          "original_form": "운동경기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100027072.1.2",
          "form": "예전에는",
          "original_form": "예전에는",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100027072.1.3",
          "form": "축구 잘 봤어요",
          "original_form": "축구 잘 봤어요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100027072.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100027072.1.5",
          "form": "저는 요새 골때리는그녀들 보고있어요 축구예능 ㅋㅋ",
          "original_form": "저는 요새 골때리는그녀들 보고있어요 축구예능 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100027072.1.6",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100027072.1.7",
          "form": "그 김민경 나오는 거 맞나요?",
          "original_form": "그 김민경 나오는 거 맞나요?",
          "speaker_id": "2",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100027072.1.8",
          "form": "아 네 ㅋㅋㅋㅋ 김ㅁ민경 나왓엇어요 전 재밋게보고잇어요!",
          "original_form": "아 네 ㅋㅋㅋㅋ 김ㅁ민경 나왓엇어요 전 재밋게보고잇어요!",
          "speaker_id": "1",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100027072.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ전 나혼산에서 김민경이 자기 경기 보는 거 보는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 나혼산에서 김민경이 자기 경기 보는 거 보는데",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100027072.1.10",
          "form": "되게 재밌어보이더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "되게 재밌어보이더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100027072.1.11",
          "form": "앜ㅋㅋㅋㅋ 김민경 나혼산도 나오는구나",
          "original_form": "앜ㅋㅋㅋㅋ 김민경 나혼산도 나오는구나",
          "speaker_id": "1",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100027072.1.12",
          "form": "경기 되게 진지하게해요",
          "original_form": "경기 되게 진지하게해요",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100027072.1.13",
          "form": "네ㅋㅋㅋㅋ 완전 진짜 경기처럼 하던데요 !!",
          "original_form": "네ㅋㅋㅋㅋ 완전 진짜 경기처럼 하던데요 !!",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100027072.1.14",
          "form": "진심으로 하는 거 처럼 보였어요ㅋㅋㅋㅋ",
          "original_form": "진심으로 하는 거 처럼 보였어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100027072.1.15",
          "form": "부상도 많이 당하고 울고 진짜 난리에요",
          "original_form": "부상도 많이 당하고 울고 진짜 난리에요",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100027072.1.16",
          "form": "헐 ㅠ 완전 진심이네여 ㅠㅠㅠㅠ",
          "original_form": "헐 ㅠ 완전 진심이네여 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100027072.1.17",
          "form": "맞아욬ㅋㅋㅋㅋ 이번주 화도 장난아니더라구요",
          "original_form": "맞아욬ㅋㅋㅋㅋ 이번주 화도 장난아니더라구요",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100027072.1.18",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100027072.1.19",
