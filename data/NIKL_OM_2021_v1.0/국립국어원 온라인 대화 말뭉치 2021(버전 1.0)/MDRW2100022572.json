      "utterance": [
        {
          "id": "MDRW2100022572.1.1",
          "form": "메타버스 아시나요?",
          "original_form": "메타버스 아시나요?",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022572.1.2",
          "form": "그건 뭔가요?",
          "original_form": "그건 뭔가요?",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022572.1.3",
          "form": "가상세계 서비스래요",
          "original_form": "가상세계 서비스래요",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022572.1.4",
          "form": "아 요새는 가상서비스가 많아졋죠",
          "original_form": "아 요새는 가상서비스가 많아졋죠",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022572.1.5",
          "form": "네 게임 마인크래프트 아시나요,",
          "original_form": "네 게임 마인크래프트 아시나요,",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022572.1.6",
          "form": "넵 들어봣어요",
          "original_form": "넵 들어봣어요",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022572.1.7",
          "form": "저도 들어만봤는데 이게 가상현실 게임이러더라구요",
          "original_form": "저도 들어만봤는데 이게 가상현실 게임이러더라구요",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022572.1.8",
          "form": "아 그래요? 검색해봐야겠네요",
          "original_form": "아 그래요? 검색해봐야겠네요",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022572.1.9",
          "form": "네네 그리고 네이버 제페토 서비스는 아시나요?",
          "original_form": "네네 그리고 네이버 제페토 서비스는 아시나요?",
          "speaker_id": "1",
          "time": "20210829 18:16"
        },
        {
          "id": "MDRW2100022572.1.10",
          "form": "아뇨ㅎㅎ첨듣는게많네요",
          "original_form": "아뇨ㅎㅎ첨듣는게많네요",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022572.1.11",
          "form": "그것도 가상현실인데 이용자가 많아진다고 해요",
          "original_form": "그것도 가상현실인데 이용자가 많아진다고 해요",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022572.1.12",
          "form": "배울게많네요",
          "original_form": "배울게많네요",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022572.1.13",
          "form": "그렇죠 점점 뒤쳐지는 기분이예요",
          "original_form": "그렇죠 점점 뒤쳐지는 기분이예요",
          "speaker_id": "1",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100022572.1.14",
          "form": "네 가상쪽은 하나도 모르니까요ㅎ",
          "original_form": "네 가상쪽은 하나도 모르니까요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:19"
        }
