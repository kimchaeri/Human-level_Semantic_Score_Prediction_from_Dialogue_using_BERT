        },
        {
          "id": "MDRW2100047208.1.5",
          "form": "저는 하는 거 좋아해여!",
          "original_form": "저는 하는 거 좋아해여!",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047208.1.6",
          "form": "최근에 구매하신 물건 없으신가용?",
          "original_form": "최근에 구매하신 물건 없으신가용?",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047208.1.7",
          "form": "운동화요ㅋㅋㅋ",
          "original_form": "운동화요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100047208.1.8",
          "form": "님은요?",
