      "utterance": [
        {
          "id": "MDRW2100032608.1.1",
          "form": "종교 있어?",
          "original_form": "종교 있어?",
          "speaker_id": "1",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100032608.1.2",
          "form": "아니 난 무교",
          "original_form": "아니 난 무교",
          "speaker_id": "2",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100032608.1.3",
          "form": "글쿤 한번도 종교 있었던적 없어?",
          "original_form": "글쿤 한번도 종교 있었던적 없어?",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032608.1.4",
          "form": "어렸을 때 성당 다녔었지",
          "original_form": "어렸을 때 성당 다녔었지",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100032608.1.5",
          "form": "가족들이 다 천주교니까",
          "original_form": "가족들이 다 천주교니까",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032608.1.6",
          "form": "아 언제부터 무교야 그럼?",
          "original_form": "아 언제부터 무교야 그럼?",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032608.1.7",
          "form": "이게 성당은 세례를 받아야 종교를 가진것으로 인정해줘서",
          "original_form": "이게 성당은 세례를 받아야 종교를 가진것으로 인정해줘서",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032608.1.8",
          "form": "무교는 계속 무교지",
          "original_form": "무교는 계속 무교지",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032608.1.9",
          "form": "성당 안 나간건 초등학생 이후부턴 자유롭게 하라고해서 안나갔어",
          "original_form": "성당 안 나간건 초등학생 이후부턴 자유롭게 하라고해서 안나갔어",
          "speaker_id": "2",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032608.1.10",
          "form": "왜 안나가서?",
          "original_form": "왜 안나가서?",
          "speaker_id": "1",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032608.1.11",
          "form": "음 난 별로 안 믿어서?",
          "original_form": "음 난 별로 안 믿어서?",
          "speaker_id": "2",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032608.1.12",
          "form": "우리 가족들 중에 나만 무교야",
          "original_form": "우리 가족들 중에 나만 무교야",
          "speaker_id": "2",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032608.1.13",
          "form": "친인척까지 다 데려와도 ㅋㅋㅋㅋㅋ",
          "original_form": "친인척까지 다 데려와도 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032608.1.14",
          "form": "이게바로 자발적 왕따...?",
          "original_form": "이게바로 자발적 왕따...?",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032608.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럴수도?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그럴수도?",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032608.1.16",
          "form": "가족들 다 모이면 밥 먹기 전에 기도했었는데",
          "original_form": "가족들 다 모이면 밥 먹기 전에 기도했었는데",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032608.1.17",
          "form": "나만 멀뚱멀뚱 있었으니까",
          "original_form": "나만 멀뚱멀뚱 있었으니까",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032608.1.18",
          "form": "하나도 안믿ㅇㅓ?",
          "original_form": "하나도 안믿ㅇㅓ?",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100032608.1.19",
          "form": "신이 존재한다는 후기 있자나",
          "original_form": "신이 존재한다는 후기 있자나",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100032608.1.20",
          "form": "전세계적으로",
          "original_form": "전세계적으로",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100032608.1.21",
          "form": "음 무신론자는 아닌데",
          "original_form": "음 무신론자는 아닌데",
          "speaker_id": "2",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100032608.1.22",
          "form": "종교에 나오는 신을 믿지는 않아",
          "original_form": "종교에 나오는 신을 믿지는 않아",
          "speaker_id": "2",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100032608.1.23",
          "form": "그럼 가족들을 보면 어떻게 생각해?",
          "original_form": "그럼 가족들을 보면 어떻게 생각해?",
          "speaker_id": "1",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100032608.1.24",
          "form": "긍정적으로 생각해",
          "original_form": "긍정적으로 생각해",
          "speaker_id": "2",
          "time": "20210905 21:39"
        }
      ]
    }
  ]
