        },
        {
          "id": "MDRW2100029965.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029965.1.3",
          "form": "강아지 키우시나요?",
          "original_form": "강아지 키우시나요?",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029965.1.4",
          "form": "저는 키우지 않는데",
          "original_form": "저는 키우지 않는데",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029965.1.5",
          "form": "제 주변 친규들이 많 키워요",
          "original_form": "제 주변 친규들이 많 키워요",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029965.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029965.1.7",
          "form": "그럼 다른 동물은 안키우시나요?",
          "original_form": "그럼 다른 동물은 안키우시나요?",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029965.1.8",
          "form": "예전에 햄스터랑 금붕어랑...미꾸라지 키운 적 있어요",
          "original_form": "예전에 햄스터랑 금붕어랑...미꾸라지 키운 적 있어요",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029965.1.9",
          "form": "미꾸라지요??",
          "original_form": "미꾸라지요??",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029965.1.10",
          "form": "처음봤어요! 신기하네요",
          "original_form": "처음봤어요! 신기하네요",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029965.1.11",
          "form": "이름이 뭐였어요?",
          "original_form": "이름이 뭐였어요?",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029965.1.12",
          "form": "미꾸라지 이름은 붙이지 않았는데",
          "original_form": "미꾸라지 이름은 붙이지 않았는데",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029965.1.13",
          "form": "크기가 서로 달라서 크기 보고 알아봤어요",
          "original_form": "크기가 서로 달라서 크기 보고 알아봤어요",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029965.1.14",
          "form": "아..! 여러마리 키우셨군요",
          "original_form": "아..! 여러마리 키우셨군요",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029965.1.15",
          "form": "미꾸라지들은 뭐 먹여요?",
          "original_form": "미꾸라지들은 뭐 먹여요?",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029965.1.16",
          "form": "금붕어랑 같은 어항에 키워서 금붕어 사료를 줬는데",
          "original_form": "금붕어랑 같은 어항에 키워서 금붕어 사료를 줬는데",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029965.1.17",
          "form": "잘 먹더라구요...",
          "original_form": "잘 먹더라구요...",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029965.1.18",
          "form": "4년 이상 잘 살았어요... ㅋㅋㅋㅋ",
          "original_form": "4년 이상 잘 살았어요... ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029965.1.19",
          "form": "허억",
          "original_form": "허억",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029965.1.20",
          "form": "장수했네요..",
          "original_form": "장수했네요..",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029965.1.21",
          "form": "물고기가 원래 그렇게 오래 사는 애들이었나요..??",
          "original_form": "물고기가 원래 그렇게 오래 사는 애들이었나요..??",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029965.1.22",
          "form": "금붕어랑 안싸우고 지내는 것도 신기하네요 둘 다 성격이 순한가봐요",
          "original_form": "금붕어랑 안싸우고 지내는 것도 신기하네요 둘 다 성격이 순한가봐요",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029965.1.23",
          "form": "그쵸 ㅋㅋㅋㅋ 지금 생각해보면 진짜 놀라운 일이었어요...",
          "original_form": "그쵸 ㅋㅋㅋㅋ 지금 생각해보면 진짜 놀라운 일이었어요...",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029965.1.24",
          "form": "미꾸라지는 어렸을 때 문화센터에서 가져왔던 거라...",
          "original_form": "미꾸라지는 어렸을 때 문화센터에서 가져왔던 거라...",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029965.1.25",
          "form": "생각보다 오래살고 크기도 커져서 어렸을때 미꾸라지가 용되는게 아닐까 라고 상각도 했었죠",
          "original_form": "생각보다 오래살고 크기도 커져서 어렸을때 미꾸라지가 용되는게 아닐까 라고 상각도 했었죠",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029965.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ너무 귀여운 발상이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ너무 귀여운 발상이에요",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029965.1.27",
          "form": "창의력 넘치시네요",
          "original_form": "창의력 넘치시네요",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029965.1.28",
          "form": "감사합니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "감사합니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:35"
        }
      ]
