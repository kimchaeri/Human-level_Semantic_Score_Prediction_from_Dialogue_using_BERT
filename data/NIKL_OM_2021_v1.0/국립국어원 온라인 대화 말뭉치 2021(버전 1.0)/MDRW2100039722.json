      "utterance": [
        {
          "id": "MDRW2100039722.1.1",
          "form": "어떻게 오늘도 비가 오냐",
          "original_form": "어떻게 오늘도 비가 오냐",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039722.1.2",
          "form": "근데 서울엔 안와서 비포없음ㅠ",
          "original_form": "근데 서울엔 안와서 비포없음ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039722.1.3",
          "form": "아ㅋㅋ 비포가 뭔가 했네",
          "original_form": "아ㅋㅋ 비포가 뭔가 했네",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039722.1.4",
          "form": "난 요즘 이북 안사서 자동결제를 안해 놨더니",
          "original_form": "난 요즘 이북 안사서 자동결제를 안해 놨더니",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039722.1.5",
          "form": "근데 요근래에 비 많이 와서 쏠쏠했겠다",
          "original_form": "근데 요근래에 비 많이 와서 쏠쏠했겠다",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039722.1.6",
          "form": "ㅇㅇ8월말에 엄청 받았지ㅋㅋ",
          "original_form": "ㅇㅇ8월말에 엄청 받았지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039722.1.7",
          "form": "언제쯤 우산 없이 외출할수 있을지 모르겠다",
          "original_form": "언제쯤 우산 없이 외출할수 있을지 모르겠다",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039722.1.8",
          "form": "그러게 여름은 다 간것 같은데 장마는 계속되네ㅠ",
          "original_form": "그러게 여름은 다 간것 같은데 장마는 계속되네ㅠ",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039722.1.9",
          "form": "수요일에는 개강이라 학교갔는데 비 엄청 와서 서럽더라ㅋㅋ",
          "original_form": "수요일에는 개강이라 학교갔는데 비 엄청 와서 서럽더라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039722.1.10",
          "form": "여름엔 비가 안 오고 뒤늦게 장마임",
          "original_form": "여름엔 비가 안 오고 뒤늦게 장마임",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039722.1.11",
          "form": "학교도 먼데 고생했다ㅠㅠ",
          "original_form": "학교도 먼데 고생했다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039722.1.12",
          "form": "어젯밤은 추워서 잠을 설쳤어",
          "original_form": "어젯밤은 추워서 잠을 설쳤어",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039722.1.13",
          "form": "맞아 저녁엔 쌀쌀하더라",
          "original_form": "맞아 저녁엔 쌀쌀하더라",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039722.1.14",
          "form": "아 쌀국수 먹고 싶다",
          "original_form": "아 쌀국수 먹고 싶다",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039722.1.15",
          "form": "순식간에 겨울 되겠어 전기장판도 틀고싶어짐",
          "original_form": "순식간에 겨울 되겠어 전기장판도 틀고싶어짐",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100039722.1.16",
          "form": "뜨끈한 국물이 그립네",
          "original_form": "뜨끈한 국물이 그립네",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100039722.1.17",
          "form": "진짜 장마끝나면 겨울 금방  올것 같아",
          "original_form": "진짜 장마끝나면 겨울 금방  올것 같아",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100039722.1.18",
          "form": "요즘 계절이 봄, 가을은 스쳐지나가는 것 같아",
          "original_form": "요즘 계절이 봄, 가을은 스쳐지나가는 것 같아",
          "speaker_id": "2",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100039722.1.19",
          "form": "늘 그랬지만 점점더 짧아지는 느낌...",
          "original_form": "늘 그랬지만 점점더 짧아지는 느낌...",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100039722.1.20",
          "form": "가을옷 꺼내고 돌아서면 겨울옷 꺼낼때야ㅋㅋ",
          "original_form": "가을옷 꺼내고 돌아서면 겨울옷 꺼낼때야ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100039722.1.21",
          "form": "간절기 옷이 젤 예쁜데ㅠ 진짜 잠깐 밖에 못 입어서 아쉬워",
          "original_form": "간절기 옷이 젤 예쁜데ㅠ 진짜 잠깐 밖에 못 입어서 아쉬워",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100039722.1.22",
          "form": "오늘 햇빛 쨍하다!",
          "original_form": "오늘 햇빛 쨍하다!",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100039722.1.23",
          "form": "그리고 완전히 가을이네",
          "original_form": "그리고 완전히 가을이네",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100039722.1.24",
          "form": "그러게 놀러나가고 싶어지는 날씨다",
          "original_form": "그러게 놀러나가고 싶어지는 날씨다",
          "speaker_id": "2",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100039722.1.25",
          "form": "밤에는 두꺼운 이불 꺼냈는데 낮에는 긴팔입고 나오니 덥네ㅋㅋ",
          "original_form": "밤에는 두꺼운 이불 꺼냈는데 낮에는 긴팔입고 나오니 덥네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100039722.1.26",
          "form": "흠  지금은 다시 흐려져서 내일이나 오늘 저녁에 비올듯",
          "original_form": "흠  지금은 다시 흐려져서 내일이나 오늘 저녁에 비올듯",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100039722.1.27",
          "form": "너무 짧은 햇빛이었다ㅠ",
          "original_form": "너무 짧은 햇빛이었다ㅠ",
          "speaker_id": "1",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100039722.1.28",
          "form": "맞아  장마가 넘 기니까 다니기도 힘들고 광합성하고 싶다",
          "original_form": "맞아  장마가 넘 기니까 다니기도 힘들고 광합성하고 싶다",
          "speaker_id": "2",
          "time": "20210904 19:15"
        }
      ]
    }
