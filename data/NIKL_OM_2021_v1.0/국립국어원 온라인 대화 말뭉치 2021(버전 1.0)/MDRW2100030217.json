      "utterance": [
        {
          "id": "MDRW2100030217.1.1",
          "form": "난 좋다구생각해 ㅎㅎ",
          "original_form": "난 좋다구생각해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030217.1.2",
          "form": "뱀 귀엽잖어",
          "original_form": "뱀 귀엽잖어",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030217.1.3",
          "form": "뱀이나 사자 호랑이",
          "original_form": "뱀이나 사자 호랑이",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030217.1.4",
          "form": "깜찍하구",
          "original_form": "깜찍하구",
          "speaker_id": "2",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030217.1.5",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100030217.1.6",
          "form": "사자랑 뱀은 너무 다르지 안아?",
          "original_form": "사자랑 뱀은 너무 다르지 안아?",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.7",
          "form": "무섭잖아",
          "original_form": "무섭잖아",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.8",
          "form": "뱀은 집에서 기를 수 있잖아",
          "original_form": "뱀은 집에서 기를 수 있잖아",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.9",
          "form": "사자는 평원에서 살아야 돼,,",
          "original_form": "사자는 평원에서 살아야 돼,,",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.10",
          "form": "비슷한데",
          "original_form": "비슷한데",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.11",
          "form": "그럴수있지",
          "original_form": "그럴수있지",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.12",
          "form": "흠... 그럴 수 잇지..",
          "original_form": "흠... 그럴 수 잇지..",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.13",
          "form": "근데 뱀은",
          "original_form": "근데 뱀은",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.14",
          "form": "난 그래도 하얀뱀은",
          "original_form": "난 그래도 하얀뱀은",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.15",
          "form": "크기가 엄청 다양하잖어 알어?",
          "original_form": "크기가 엄청 다양하잖어 알어?",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.16",
          "form": "좀 귀여운거같긴하더라",
          "original_form": "좀 귀여운거같긴하더라",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.17",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.18",
          "form": "몰랐어",
          "original_form": "몰랐어",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.19",
          "form": "ㅋㅋ색깔이 중요한거냐구 ㅋㅋ",
          "original_form": "ㅋㅋ색깔이 중요한거냐구 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.20",
          "form": "웅 ㅋㅋ",
          "original_form": "웅 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.21",
          "form": "구렁이같은 뱀도 잇잖어 ㅋㅋ",
          "original_form": "구렁이같은 뱀도 잇잖어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030217.1.22",
          "form": "이름을 name3로 짓겠어",
          "original_form": "이름을 &name3&로 짓겠어",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030217.1.23",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030217.1.24",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030217.1.25",
          "form": "무섭지 그건",
          "original_form": "무섭지 그건",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030217.1.26",
          "form": "그냥 하얗구 푹신푹신해보이잖아",
