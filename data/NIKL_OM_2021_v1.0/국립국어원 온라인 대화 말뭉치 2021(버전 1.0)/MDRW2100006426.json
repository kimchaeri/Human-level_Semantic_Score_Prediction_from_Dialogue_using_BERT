      "utterance": [
        {
          "id": "MDRW2100006426.1.1",
          "form": "친구보고 근처 오라고해서",
          "original_form": "친구보고 근처 오라고해서",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006426.1.2",
          "form": "만나면 안되나요?",
          "original_form": "만나면 안되나요?",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006426.1.3",
          "form": "친구들도 아이들이 있으니까요 :)",
          "original_form": "친구들도 아이들이 있으니까요 :)",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006426.1.4",
          "form": "남편 친구는 오더라구요ㅋㅋ",
          "original_form": "남편 친구는 오더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006426.1.5",
          "form": "쉽지 않군여",
          "original_form": "쉽지 않군여",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006426.1.6",
          "form": "남편 친구들도 남편 성격 아니까",
          "original_form": "남편 친구들도 남편 성격 아니까",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006426.1.7",
          "form": "전화해서 걱정해줘요ㅋㅋ",
          "original_form": "전화해서 걱정해줘요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006426.1.8",
          "form": "애들 키우느라 힘든데\r\n어디 나가지도 못하고\r\n답답해서 어떻게 하냐고ㅋㅋ",
          "original_form": "애들 키우느라 힘든데\r\n어디 나가지도 못하고\r\n답답해서 어떻게 하냐고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006426.1.9",
          "form": "그래서 엄마들이",
          "original_form": "그래서 엄마들이",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006426.1.10",
          "form": "집에 놀러가고 싶어도\r\n저희 남편이 코시국인데\r\n어딜 오냐고 욕해서 못간다고ㅋ",
          "original_form": "집에 놀러가고 싶어도\r\n저희 남편이 코시국인데\r\n어딜 오냐고 욕해서 못간다고ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006426.1.11",
          "form": "애들 학교보내고 싶어하는군여",
          "original_form": "애들 학교보내고 싶어하는군여",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006426.1.12",
          "form": "ㅋㅋ 제가 육휴 끝나고 복직했다가",
          "original_form": "ㅋㅋ 제가 육휴 끝나고 복직했다가",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006426.1.13",
          "form": "퇴사할때 이제 아이 얼집 보내고",
          "original_form": "퇴사할때 이제 아이 얼집 보내고",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006426.1.14",
          "form": "나도 사람답게 살자 했거든요ㅋ",
          "original_form": "나도 사람답게 살자 했거든요ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006426.1.15",
          "form": "근데 퇴사하고 다음달에\r\n둘째가 생겼어요......ㅠㅠ",
          "original_form": "근데 퇴사하고 다음달에\r\n둘째가 생겼어요......ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100006426.1.16",
          "form": "참 인생이 내 맘 같지가 않죠 ㅋㅋㅋ",
          "original_form": "참 인생이 내 맘 같지가 않죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100006426.1.17",
          "form": "어린이집 몇살부터 가나요?",
          "original_form": "어린이집 몇살부터 가나요?",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100006426.1.18",
          "form": "어린이집은 0세부터도\r\n갈수야 있어요:)",
          "original_form": "어린이집은 0세부터도\r\n갈수야 있어요:)",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006426.1.19",
          "form": "가정어린이집 보내면요",
          "original_form": "가정어린이집 보내면요",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006426.1.20",
          "form": "근데 너무 어리기도하고...",
          "original_form": "근데 너무 어리기도하고...",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006426.1.21",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006426.1.22",
          "form": "코시국이라고 5살도\r\n집에 있으니 가정보육하라는데",
          "original_form": "코시국이라고 5살도\r\n집에 있으니 가정보육하라는데",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006426.1.23",
          "form": "둘째 보낼 생각은 더더욱 못하죠ㅋ",
          "original_form": "둘째 보낼 생각은 더더욱 못하죠ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006426.1.24",
          "form": "코시국인데 어딜가냐 코시국에 뭘하냐 하지만",
          "original_form": "코시국인데 어딜가냐 코시국에 뭘하냐 하지만",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006426.1.25",
          "form": "코로나 안끝나요",
          "original_form": "코로나 안끝나요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006426.1.26",
          "form": "평생 그래야되는데 그 논리면...",
          "original_form": "평생 그래야되는데 그 논리면...",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
