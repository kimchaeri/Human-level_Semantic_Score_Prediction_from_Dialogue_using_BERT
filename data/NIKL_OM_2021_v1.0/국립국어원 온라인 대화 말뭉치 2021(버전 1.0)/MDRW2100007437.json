      "utterance": [
        {
          "id": "MDRW2100007437.1.1",
          "form": "여름 휴가 다녀오셨어요?",
          "original_form": "여름 휴가 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007437.1.2",
          "form": "저 강릉 다녀왔어요",
          "original_form": "저 강릉 다녀왔어요",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007437.1.3",
          "form": "오 이번에요?",
          "original_form": "오 이번에요?",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007437.1.4",
          "form": "네 7월 초에 좀 이르게 다녀왔어요",
          "original_form": "네 7월 초에 좀 이르게 다녀왔어요",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007437.1.5",
          "form": "그래도 다행이네요",
          "original_form": "그래도 다행이네요",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007437.1.6",
          "form": "나중에는 강릉도 4단계로 격상됐잖아요",
          "original_form": "나중에는 강릉도 4단계로 격상됐잖아요",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007437.1.7",
          "form": "부럽습니다 ㅜㅜ",
          "original_form": "부럽습니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007437.1.8",
          "form": "맞아요 딱 그전에 다녀왔어요",
          "original_form": "맞아요 딱 그전에 다녀왔어요",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007437.1.9",
          "form": "여행 자주 다니세요?",
          "original_form": "여행 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007437.1.10",
          "form": "코로나 전에는 자주 다녔는데",
          "original_form": "코로나 전에는 자주 다녔는데",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007437.1.11",
          "form": "코시국 이후에는 잘 못다녔어요 ㅜㅜ",
          "original_form": "코시국 이후에는 잘 못다녔어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007437.1.12",
          "form": "강릉 넘 좋죠",
          "original_form": "강릉 넘 좋죠",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007437.1.13",
          "form": "강릉...",
          "original_form": "강릉...",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007437.1.14",
          "form": "재밌는 게",
          "original_form": "재밌는 게",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007437.1.15",
          "form": "7월에 여행으로 다녀와놓구 지난주에 아예 이사하게 됐어요...^^",
          "original_form": "7월에 여행으로 다녀와놓구 지난주에 아예 이사하게 됐어요...^^",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007437.1.16",
          "form": "직장때문에....",
          "original_form": "직장때문에....",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007437.1.17",
          "form": "대박 ㅋㅋㅋ",
          "original_form": "대박 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007437.1.18",
          "form": "생활은 모르지만",
          "original_form": "생활은 모르지만",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007437.1.19",
          "form": "일단은 좋을 것 같아요",
          "original_form": "일단은 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007437.1.20",
          "form": "참ㅋㅋㅋ 이럴 줄 알았으면 지난달에 굳이 안 왔어도 되는데",
          "original_form": "참ㅋㅋㅋ 이럴 줄 알았으면 지난달에 굳이 안 왔어도 되는데",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007437.1.21",
          "form": "그래도 여행지로 찾았을 때의 그 자유로움이 있잖아요 ㅋㅋ",
          "original_form": "그래도 여행지로 찾았을 때의 그 자유로움이 있잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007437.1.22",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007437.1.23",
          "form": "추천해주실 여행지 있으신가요",
          "original_form": "추천해주실 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007437.1.24",
          "form": "저도 맨날 강릉 속초만 가구..",
          "original_form": "저도 맨날 강릉 속초만 가구..",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007437.1.25",
          "form": "그 외에는 부산 경주 제주 이쯤이라",
          "original_form": "그 외에는 부산 경주 제주 이쯤이라",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007437.1.26",
          "form": "구관이 명관이죠 ㅎㅎ",
          "original_form": "구관이 명관이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007437.1.27",
          "form": "맞아요 유명한 곳이 유명한 이유가 있어요",
          "original_form": "맞아요 유명한 곳이 유명한 이유가 있어요",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007437.1.28",
          "form": "네 가도가도 좋고 ㅋㅋ",
          "original_form": "네 가도가도 좋고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007437.1.29",
          "form": "강릉중앙시장은 좀 정리가 잘돼있는데",
          "original_form": "강릉중앙시장은 좀 정리가 잘돼있는데",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007437.1.30",
          "form": "속초중앙시장은 영 별로더라구요",
          "original_form": "속초중앙시장은 영 별로더라구요",
          "speaker_id": "1",
          "time": "20210822 02:20"
        }
      ]
    }
