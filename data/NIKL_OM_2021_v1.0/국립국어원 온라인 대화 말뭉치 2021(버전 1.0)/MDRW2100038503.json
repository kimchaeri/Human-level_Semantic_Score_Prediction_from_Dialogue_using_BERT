        },
        {
          "id": "MDRW2100038503.1.2",
          "form": "안녕하세요 백신 맞으셨나요",
          "original_form": "안녕하세요 백신 맞으셨나요",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038503.1.3",
          "form": "네 백신 1차 맞았어요,",
          "original_form": "네 백신 1차 맞았어요,",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038503.1.4",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038503.1.5",
          "form": "저도 1차까지는 맞았어요",
          "original_form": "저도 1차까지는 맞았어요",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038503.1.6",
          "form": "저두요 ㅋㅋ 뭐 맞으셨어요?",
          "original_form": "저두요 ㅋㅋ 뭐 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038503.1.7",
          "form": "화이자를 맞았아요 첫날엔 좀 아팠지만 지금은 쌩쌩하답니다",
          "original_form": "화이자를 맞았아요 첫날엔 좀 아팠지만 지금은 쌩쌩하답니다",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038503.1.8",
          "form": "아 그러시구나 ㅋㅋ 저도 화이자1차요",
          "original_form": "아 그러시구나 ㅋㅋ 저도 화이자1차요",
          "speaker_id": "1",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100038503.1.9",
          "form": "2차는 언제세요?",
          "original_form": "2차는 언제세요?",
          "speaker_id": "1",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100038503.1.10",
          "form": "10월초중순이예요",
          "original_form": "10월초중순이예요",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100038503.1.11",
          "form": "아 제가 먼저 맞았네요 ㅋㅋ 저는 9월 말이요 ㅋㅋ",
          "original_form": "아 제가 먼저 맞았네요 ㅋㅋ 저는 9월 말이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100038503.1.12",
          "form": "2차 긴장이 되네요 ㅋ",
          "original_form": "2차 긴장이 되네요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100038503.1.13",
          "form": "딱 백신 한번에 끝났으면 좋겠는데 이번 코로나가 워낙 독해서 그건 안되겠죠",
          "original_form": "딱 백신 한번에 끝났으면 좋겠는데 이번 코로나가 워낙 독해서 그건 안되겠죠",
          "speaker_id": "2",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100038503.1.14",
          "form": "맞아요 ㅠㅠㅠ 백신 한번에 맞고 끝냈으면 좋겠네요 ㅋㅋ",
          "original_form": "맞아요 ㅠㅠㅠ 백신 한번에 맞고 끝냈으면 좋겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100038503.1.15",
          "form": "그게 안되니 ㅠㅠ",
          "original_form": "그게 안되니 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100038503.1.16",
          "form": "그냥 조금 아프고 건강해지길 바랄뿐입니다",
          "original_form": "그냥 조금 아프고 건강해지길 바랄뿐입니다",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100038503.1.17",
          "form": "맞아요 ㅠㅠㅠ 코로나 빨리 없어졌으면 좋겠네요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ 코로나 빨리 없어졌으면 좋겠네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
