      "utterance": [
        {
          "id": "MDRW2100037484.1.1",
          "form": "어떤 과자종류를 좋아하세요?",
          "original_form": "어떤 과자종류를 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100037484.1.2",
          "form": "달콤한종류?",
          "original_form": "달콤한종류?",
          "speaker_id": "1",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100037484.1.3",
          "form": "과자는 쏘쏘...",
          "original_form": "과자는 쏘쏘...",
          "speaker_id": "2",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100037484.1.4",
          "form": "굳이 따지면 짭짤한 거요",
          "original_form": "굳이 따지면 짭짤한 거요",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100037484.1.5",
          "form": "아 과자는 취향이 아니시군요...",
          "original_form": "아 과자는 취향이 아니시군요...",
          "speaker_id": "1",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100037484.1.6",
          "form": "새우깡 꼬깔콘같은ㅎㅎ",
          "original_form": "새우깡 꼬깔콘같은ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100037484.1.7",
          "form": "취향은 아니지만 앉은자리서 노래방사이즈 아작내쥬",
          "original_form": "취향은 아니지만 앉은자리서 노래방사이즈 아작내쥬",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100037484.1.8",
          "form": "name1님은 달콤한 취향이죠?",
          "original_form": "&name1&님은 달콤한 취향이죠?",
          "speaker_id": "2",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100037484.1.9",
          "form": "아아 저 요즘 계속 먹는게 꼬깔콘이에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 저 요즘 계속 먹는게 꼬깔콘이에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100037484.1.10",
          "form": "하지만.. 디저트종류는 기본적으로 달콤한거 좋아해요!",
          "original_form": "하지만.. 디저트종류는 기본적으로 달콤한거 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100037484.1.11",
          "form": "뭔가 이상하게 계속 손이 가죠ㅎ 배부른데",
          "original_form": "뭔가 이상하게 계속 손이 가죠ㅎ 배부른데",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100037484.1.12",
          "form": "초코케이크 초코마카롱 초코과자...",
          "original_form": "초코케이크 초코마카롱 초코과자...",
          "speaker_id": "1",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100037484.1.13",
          "form": "앗 두번씩이나 강조를ㅎㅎ",
          "original_form": "앗 두번씩이나 강조를ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100037484.1.14",
          "form": "최애 과자는 뭔가요",
          "original_form": "최애 과자는 뭔가요",
          "speaker_id": "2",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100037484.1.15",
          "form": "최애과자는....... 어라? 저 근데 중독적으로 연달아서 사먹은건 짠맛 과자에요",
          "original_form": "최애과자는....... 어라? 저 근데 중독적으로 연달아서 사먹은건 짠맛 과자에요",
          "speaker_id": "1",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100037484.1.16",
          "form": "엄머나? ㅋㅋㅋㅋ",
          "original_form": "엄머나? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100037484.1.17",
          "form": "최근엔 꼬깔콘 몇번씩 사먹었고",
          "original_form": "최근엔 꼬깔콘 몇번씩 사먹었고",
          "speaker_id": "1",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100037484.1.18",
          "form": "이전엔 고래밥이 제 최애였어욬ㅋㅋㅋㅋ",
          "original_form": "이전엔 고래밥이 제 최애였어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100037484.1.19",
          "form": "네모난 곽에 든 것도 좋아하고 그거 감질맛나면 천원짜리 고래밥 봉지과자 있거든요!",
          "original_form": "네모난 곽에 든 것도 좋아하고 그거 감질맛나면 천원짜리 고래밥 봉지과자 있거든요!",
          "speaker_id": "1",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100037484.1.20",
          "form": "그거 계속 먹었네요... 뭐뎌.... 막상 말하려니 이런 아이러니가 ㅋㅋㅋㅋ",
          "original_form": "그거 계속 먹었네요... 뭐뎌.... 막상 말하려니 이런 아이러니가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100037484.1.21",
          "form": "아니ㅋㅋ 다 짭짤한 거잖아요!",
          "original_form": "아니ㅋㅋ 다 짭짤한 거잖아요!",
          "speaker_id": "2",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100037484.1.22",
          "form": "ㅋㅋㅋㅋㅋ저도 생각해보니 웃겨욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저도 생각해보니 웃겨욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100037484.1.23",
          "form": "사실 짭짤한 과자를 좋아하는 저는...",
          "original_form": "사실 짭짤한 과자를 좋아하는 저는...",
          "speaker_id": "2",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100037484.1.24",
          "form": "맛동산을 좋아합니다ㅎ",
          "original_form": "맛동산을 좋아합니다ㅎ",
          "speaker_id": "2",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100037484.1.25",
          "form": "기본적으로는 달콤한걸 좋아하는데 최근에 중독적으로 연달아서 사먹은건 짭짤과자..",
          "original_form": "기본적으로는 달콤한걸 좋아하는데 최근에 중독적으로 연달아서 사먹은건 짭짤과자..",
          "speaker_id": "1",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100037484.1.26",
          "form": "맛동산.... 아...아빠?",
          "original_form": "맛동산.... 아...아빠?",
          "speaker_id": "1",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100037484.1.27",
          "form": "엄.... 마?",
          "original_form": "엄.... 마?",
          "speaker_id": "1",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100037484.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100037484.1.29",
          "form": "흘흘 조상이라 불러주세요",
          "original_form": "흘흘 조상이라 불러주세요",
          "speaker_id": "2",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100037484.1.30",
          "form": "탕비실에 둔거 먹고 반해서 한동안 계속 사먹었네요ㅎ",
          "original_form": "탕비실에 둔거 먹고 반해서 한동안 계속 사먹었네요ㅎ",
          "speaker_id": "2",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100037484.1.31",
          "form": "근데 그런거 중독되면 또 계속 먹게돼요 ㅋㅋㅋㅋ",
          "original_form": "근데 그런거 중독되면 또 계속 먹게돼요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100037484.1.32",
          "form": "특히나 남돈남산일때 ㅋㅋㅋ",
          "original_form": "특히나 남돈남산일때 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100037484.1.33",
          "form": "내돈으론 안사지만 있으면 중독적으로 먹게되는",
          "original_form": "내돈으론 안사지만 있으면 중독적으로 먹게되는",
          "speaker_id": "1",
          "time": "20210901 01:08"
        }
      ]
    }
