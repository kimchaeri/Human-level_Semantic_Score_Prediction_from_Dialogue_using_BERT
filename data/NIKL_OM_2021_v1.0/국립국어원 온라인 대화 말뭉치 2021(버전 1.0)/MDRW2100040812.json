      "utterance": [
        {
          "id": "MDRW2100040812.1.1",
          "form": "ㅋㅋㅋ옥수수 뺏길까봐 진짜 조금 심었는데",
          "original_form": "ㅋㅋㅋ옥수수 뺏길까봐 진짜 조금 심었는데",
          "speaker_id": "2",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100040812.1.2",
          "form": "그래서 그거보고 아빠가 고구마랑 옥수수를 더 심자!! 이렇게됨ㅋㅋㅋ",
          "original_form": "그래서 그거보고 아빠가 고구마랑 옥수수를 더 심자!! 이렇게됨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:35"
        },
        {
          "id": "MDRW2100040812.1.3",
          "form": "옥수수 심은거...",
          "original_form": "옥수수 심은거...",
          "speaker_id": "2",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100040812.1.4",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100040812.1.5",
          "form": "난 진짜 옥수수 고구마 걈자이런거 안먹네",
          "original_form": "난 진짜 옥수수 고구마 걈자이런거 안먹네",
          "speaker_id": "1",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100040812.1.6",
          "form": "고구마 에프에 구워서 먹으면 딘짜 맛나",
          "original_form": "고구마 에프에 구워서 먹으면 딘짜 맛나",
          "speaker_id": "2",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100040812.1.7",
          "form": "먹는건데마리야",
          "original_form": "먹는건데마리야",
          "speaker_id": "1",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100040812.1.8",
          "form": "완전 꿀고구마임",
          "original_form": "완전 꿀고구마임",
          "speaker_id": "2",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100040812.1.9",
          "form": "내말이!!웃겨",
          "original_form": "내말이!!웃겨",
          "speaker_id": "2",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100040812.1.10",
          "form": "진짜 어찌 농사가 다성공인겨!!",
          "original_form": "진짜 어찌 농사가 다성공인겨!!",
          "speaker_id": "1",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100040812.1.11",
          "form": "ㅋㅋ땅이 좋아서 그렇대!",
          "original_form": "ㅋㅋ땅이 좋아서 그렇대!",
          "speaker_id": "2",
          "time": "20210905 08:37"
        },
        {
          "id": "MDRW2100040812.1.12",
          "form": "올해 처음으로 심어서 재배한거니까 엄청 영양이 풍뷰한가봐 ㅋㅋ",
          "original_form": "올해 처음으로 심어서 재배한거니까 엄청 영양이 풍뷰한가봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100040812.1.13",
          "form": "진짜 땅이 기반이니",
          "original_form": "진짜 땅이 기반이니",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100040812.1.14",
          "form": "중요하구나",
          "original_form": "중요하구나",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100040812.1.15",
          "form": "아 그런가!",
          "original_form": "아 그런가!",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100040812.1.16",
          "form": "해갈이가 그런거겟지",
          "original_form": "해갈이가 그런거겟지",
          "speaker_id": "1",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100040812.1.17",
          "form": "글치 ㅋㅋㅋㅋ",
          "original_form": "글치 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100040812.1.18",
          "form": "깨알 한국사 공부",
          "original_form": "깨알 한국사 공부",
          "speaker_id": "2",
          "time": "20210905 08:38"
        },
        {
          "id": "MDRW2100040812.1.19",
          "form": "ㅋㅋㅋㅋ주말을 더 빡새게 보내신다ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ주말을 더 빡새게 보내신다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100040812.1.20",
          "form": "ㅋㅋㅋ난 진짜 부모 못해...",
          "original_form": "ㅋㅋㅋ난 진짜 부모 못해...",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040812.1.21",
