      "utterance": [
        {
          "id": "MDRW2100023780.1.1",
          "form": "예를 들면",
          "original_form": "예를 들면",
          "speaker_id": "2",
          "time": "20210830 17:22"
        },
        {
          "id": "MDRW2100023780.1.2",
          "form": "어떤 색?",
          "original_form": "어떤 색?",
          "speaker_id": "2",
          "time": "20210830 17:22"
        },
        {
          "id": "MDRW2100023780.1.3",
          "form": "잘 모르겠어 근데 구현하기 어려운 색이 있다고 하더라고",
          "original_form": "잘 모르겠어 근데 구현하기 어려운 색이 있다고 하더라고",
          "speaker_id": "1",
          "time": "20210830 17:23"
        },
        {
          "id": "MDRW2100023780.1.4",
          "form": "그건 어쩔 수 없지",
          "original_form": "그건 어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210830 17:23"
        },
        {
          "id": "MDRW2100023780.1.5",
          "form": "외국은 염색해 주는 미용실이랑 커트 해 주는 미용실이랑 아예 따로 있대",
          "original_form": "외국은 염색해 주는 미용실이랑 커트 해 주는 미용실이랑 아예 따로 있대",
          "speaker_id": "1",
          "time": "20210830 17:23"
        },
        {
          "id": "MDRW2100023780.1.6",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210830 17:23"
        },
        {
          "id": "MDRW2100023780.1.7",
          "form": "우리처럼 같이 안하고?",
          "original_form": "우리처럼 같이 안하고?",
          "speaker_id": "2",
          "time": "20210830 17:23"
        },
        {
          "id": "MDRW2100023780.1.8",
          "form": "응 그래서 염색하는데 서는 염색만 하고  미용사 호칭도 다르대",
          "original_form": "응 그래서 염색하는데 서는 염색만 하고  미용사 호칭도 다르대",
          "speaker_id": "1",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100023780.1.9",
          "form": "호칭이 어떤데?",
          "original_form": "호칭이 어떤데?",
          "speaker_id": "2",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100023780.1.10",
          "form": "컬러리스튼 가 그렇게 부른다고 하던데",
          "original_form": "컬러리스튼 가 그렇게 부른다고 하던데",
          "speaker_id": "1",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100023780.1.11",
          "form": "염색해주는 사람을?",
          "original_form": "염색해주는 사람을?",
          "speaker_id": "2",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100023780.1.12",
          "form": "응  미용사라 안하고",
          "original_form": "응  미용사라 안하고",
          "speaker_id": "1",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100023780.1.13",
          "form": "말이 되기는 하네",
          "original_form": "말이 되기는 하네",
          "speaker_id": "2",
          "time": "20210830 17:24"
        },
        {
          "id": "MDRW2100023780.1.14",
          "form": "그치 그럴 듯했어",
          "original_form": "그치 그럴 듯했어",
          "speaker_id": "1",
          "time": "20210830 17:25"
        },
        {
          "id": "MDRW2100023780.1.15",
          "form": "근데 조금 신기하기는 하네",
          "original_form": "근데 조금 신기하기는 하네",
          "speaker_id": "2",
          "time": "20210830 17:25"
        },
        {
          "id": "MDRW2100023780.1.16",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023780.1.17",
          "form": "근데 커트도 하고 염색도 같이하는 게 훨씬 편하지 않나",
          "original_form": "근데 커트도 하고 염색도 같이하는 게 훨씬 편하지 않나",
          "speaker_id": "1",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023780.1.18",
          "form": "복잡하지 않으니까",
          "original_form": "복잡하지 않으니까",
          "speaker_id": "2",
          "time": "20210830 17:26"
        },
        {
          "id": "MDRW2100023780.1.19",
          "form": "그래서 좀 불편할 거 같다는 생각을 했어",
          "original_form": "그래서 좀 불편할 거 같다는 생각을 했어",
          "speaker_id": "1",
          "time": "20210830 17:26"
        }
