      "utterance": [
        {
          "id": "MDRW2100029257.1.1",
          "form": "연락오나",
          "original_form": "연락오나",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.2",
          "form": "소개남",
          "original_form": "소개남",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.3",
          "form": "오늘은 연락 안 했어요",
          "original_form": "오늘은 연락 안 했어요",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.4",
          "form": "아 그러고 보니 그렇네요",
          "original_form": "아 그러고 보니 그렇네요",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.5",
          "form": "어제는했어?",
          "original_form": "어제는했어?",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.6",
          "form": "어제는 했 써요",
          "original_form": "어제는 했 써요",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.7",
          "form": "대화 어떤거해?",
          "original_form": "대화 어떤거해?",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.8",
          "form": "오늘 통화하자면서 그랬는데",
          "original_form": "오늘 통화하자면서 그랬는데",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.9",
          "form": "대화가 거의 안 돼요",
          "original_form": "대화가 거의 안 돼요",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.10",
          "form": "통화안해서 다행",
          "original_form": "통화안해서 다행",
          "speaker_id": "2",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100029257.1.11",
          "form": "밤에 일하고 아침에 일하고",
          "original_form": "밤에 일하고 아침에 일하고",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.12",
          "form": "얼굴도 안봤는데부담되자나",
          "original_form": "얼굴도 안봤는데부담되자나",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.13",
          "form": "대화 타이밍이 안 맞아요",
          "original_form": "대화 타이밍이 안 맞아요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.14",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.15",
          "form": "공항에서 일한다는데",
          "original_form": "공항에서 일한다는데",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.16",
          "form": "너보러 부산올 기세지",
          "original_form": "너보러 부산올 기세지",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.17",
          "form": "아맞다",
          "original_form": "아맞다",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.18",
          "form": "뭔 일하는지는 모르겠어요",
          "original_form": "뭔 일하는지는 모르겠어요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.19",
          "form": "공항에서 일한댔다는늣",
          "original_form": "공항에서 일한댔다는늣",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.20",
          "form": "핑퐁이 잘 안 돼요",
          "original_form": "핑퐁이 잘 안 돼요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.21",
          "form": "연애의 세포가깨어나나",
          "original_form": "연애의 세포가깨어나나",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.22",
          "form": "안맞나보네",
          "original_form": "안맞나보네",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.23",
          "form": "까비",
          "original_form": "까비",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029257.1.24",
          "form": "대화가 되어야지",
          "original_form": "대화가 되어야지",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029257.1.25",
          "form": "내가 전에 소개받은",
          "original_form": "내가 전에 소개받은",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029257.1.26",
          "form": "사람얘기 해줄까",
          "original_form": "사람얘기 해줄까",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029257.1.27",
          "form": "전에 소개받은 사람은 어땠어요",
          "original_form": "전에 소개받은 사람은 어땠어요",
          "speaker_id": "1",
          "time": "20210903 00:47"
        }
