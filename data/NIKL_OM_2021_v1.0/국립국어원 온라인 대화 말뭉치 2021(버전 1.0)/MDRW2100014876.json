      "utterance": [
        {
          "id": "MDRW2100014876.1.1",
          "form": "넷플릭스 보세요?",
          "original_form": "넷플릭스 보세요?",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014876.1.2",
          "form": "넷플릭스는 안보구 왓챠 주로봐요~",
          "original_form": "넷플릭스는 안보구 왓챠 주로봐요~",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014876.1.3",
          "form": "오오 영화보는거 좋아하시는구나?",
          "original_form": "오오 영화보는거 좋아하시는구나?",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014876.1.4",
          "form": "맞아요~ 한국영화보다는 해외영화 좋아해요 ㅎㅎ",
          "original_form": "맞아요~ 한국영화보다는 해외영화 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014876.1.5",
          "form": "오 저는 해외영화는",
          "original_form": "오 저는 해외영화는",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014876.1.6",
          "form": "놀란 감독꺼 좋아해요",
          "original_form": "놀란 감독꺼 좋아해요",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014876.1.7",
          "form": "아하! 저 인셉션 진짜좋아해요 ㅠ",
          "original_form": "아하! 저 인셉션 진짜좋아해요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014876.1.8",
          "form": "최근에 메멘토 재개봉했었던거 본거같아요!",
          "original_form": "최근에 메멘토 재개봉했었던거 본거같아요!",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014876.1.9",
          "form": "헐 저도저도!!",
          "original_form": "헐 저도저도!!",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014876.1.10",
          "form": "메멘토 재개봉은 아니구 옛날거 봤었는데",
          "original_form": "메멘토 재개봉은 아니구 옛날거 봤었는데",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014876.1.11",
          "form": "진짜 영상흐름 진짜 세련됐어요ㅠㅠ",
          "original_form": "진짜 영상흐름 진짜 세련됐어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014876.1.12",
          "form": "놀란감독 정말 놀라워요ㅋㅋㅋ",
          "original_form": "놀란감독 정말 놀라워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014876.1.13",
          "form": "혹시 테넷 보셨어요??",
          "original_form": "혹시 테넷 보셨어요??",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014876.1.14",
          "form": "테넷은 배경 과학지식 없으면 보기 힘들대서",
          "original_form": "테넷은 배경 과학지식 없으면 보기 힘들대서",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014876.1.15",
          "form": "아직 엄두를 못내고 있어요...",
          "original_form": "아직 엄두를 못내고 있어요...",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014876.1.16",
          "form": "ㅋㅋㅋㅋㅋ 저두 못봤어요 ㅋㅋㅋ ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ 저두 못봤어요 ㅋㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100014876.1.17",
          "form": "요새는 헝거게임 또다시 정주행중이에요 ㅋㅋ",
          "original_form": "요새는 헝거게임 또다시 정주행중이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100014876.1.18",
          "form": "호오 헝거게임 엄청 유명하죠",
          "original_form": "호오 헝거게임 엄청 유명하죠",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014876.1.19",
          "form": "시즌 엄청 나왔죠?",
          "original_form": "시즌 엄청 나왔죠?",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100014876.1.20",
          "form": "네! 세개인가 4개인데 몰아보려니 꽤 길드라고요 ㅋㅋ",
          "original_form": "네! 세개인가 4개인데 몰아보려니 꽤 길드라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100014876.1.21",
          "form": "ㅋㅋㅋ그쵸 시즌제로 하는건 진짜 각잡고 몰아봐야 재밌어요",
          "original_form": "ㅋㅋㅋ그쵸 시즌제로 하는건 진짜 각잡고 몰아봐야 재밌어요",
          "speaker_id": "2",
          "time": "20210825 22:04"
        }
      ]
    }
