      "utterance": [
        {
          "id": "MDRW2100005871.1.1",
          "form": "아 뭐 맞으시나요",
          "original_form": "아 뭐 맞으시나요",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005871.1.2",
          "form": "이번주 백신 2차인데 무서워ㅠㅠ",
          "original_form": "이번주 백신 2차인데 무서워ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005871.1.3",
          "form": "화이자 맞아요",
          "original_form": "화이자 맞아요",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100005871.1.4",
          "form": "1.2차 모두 화이자인가요?",
          "original_form": "1.2차 모두 화이자인가요?",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005871.1.5",
          "form": "네네 모두 화이자에요",
          "original_form": "네네 모두 화이자에요",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005871.1.6",
          "form": "이제 화이자 인이 되겠네요",
          "original_form": "이제 화이자 인이 되겠네요",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005871.1.7",
          "form": "칟구는 교차 접종이라",
          "original_form": "칟구는 교차 접종이라",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100005871.1.8",
          "form": "자기 아제르화이자인이라고 슬퍼하더라고요",
          "original_form": "자기 아제르화이자인이라고 슬퍼하더라고요",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100005871.1.9",
          "form": "와이프도 그런데..왜 슬퍼하죠?",
          "original_form": "와이프도 그런데..왜 슬퍼하죠?",
          "speaker_id": "2",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100005871.1.10",
          "form": "교차접종이 케이스가 많이없고",
          "original_form": "교차접종이 케이스가 많이없고",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100005871.1.11",
          "form": "해외에서 권고를 안한대요ㅠㅠ",
          "original_form": "해외에서 권고를 안한대요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100005871.1.12",
          "form": "저도 잘 모르는데 친구가 걱정하더라고요",
          "original_form": "저도 잘 모르는데 친구가 걱정하더라고요",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100005871.1.13",
          "form": "그렇군요 무탈하면 좋겠네요..",
          "original_form": "그렇군요 무탈하면 좋겠네요..",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005871.1.14",
          "form": "1차땐 별증상 없었나요?",
          "original_form": "1차땐 별증상 없었나요?",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005871.1.15",
          "form": "아 두통이 ㅇ있었어요",
          "original_form": "아 두통이 ㅇ있었어요",
          "speaker_id": "1",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005871.1.16",
          "form": "오래 갔나요?",
          "original_form": "오래 갔나요?",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100005871.1.17",
          "form": "음 하루이틀 간거같아여",
          "original_form": "음 하루이틀 간거같아여",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005871.1.18",
          "form": "두통 외엔 없었나요?",
          "original_form": "두통 외엔 없었나요?",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005871.1.19",
          "form": "그거랑 팔 못 올리는 거요 ㅋㅋ",
          "original_form": "그거랑 팔 못 올리는 거요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005871.1.20",
          "form": "아 주변보니 진짜 팔 통증 심하더라구요 멍도 있고",
          "original_form": "아 주변보니 진짜 팔 통증 심하더라구요 멍도 있고",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005871.1.21",
          "form": "와ㅠㅠ그쵸",
          "original_form": "와ㅠㅠ그쵸",
