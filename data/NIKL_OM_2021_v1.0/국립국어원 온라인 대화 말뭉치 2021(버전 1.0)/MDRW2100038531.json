      "utterance": [
        {
          "id": "MDRW2100038531.1.1",
          "form": "저녁 맛있게 먹었어요?",
          "original_form": "저녁 맛있게 먹었어요?",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038531.1.2",
          "form": "어ㅇ 배부르다주",
          "original_form": "어ㅇ 배부르다주",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038531.1.3",
          "form": "애들 자러갔는데 잠이안오는갑네ㅡㅡ",
          "original_form": "애들 자러갔는데 잠이안오는갑네ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038531.1.4",
          "form": "오늘 슬의생도안한다하고",
          "original_form": "오늘 슬의생도안한다하고",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038531.1.5",
          "form": "오늘도 애들은 속제 많았나요",
          "original_form": "오늘도 애들은 속제 많았나요",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100038531.1.6",
          "form": "숙제",
          "original_form": "숙제",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100038531.1.7",
          "form": "오늘은 거의안했다",
          "original_form": "오늘은 거의안했다",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100038531.1.8",
          "form": "내일학원안가서ㅋ",
          "original_form": "내일학원안가서ㅋ",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100038531.1.9",
          "form": "그래요 좀 쉬는 날도 있어야죠",
          "original_form": "그래요 좀 쉬는 날도 있어야죠",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038531.1.10",
          "form": "뭐 하고 있어요",
          "original_form": "뭐 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038531.1.11",
          "form": "굳모닝ㅋ",
          "original_form": "굳모닝ㅋ",
          "speaker_id": "2",
          "time": "20210903 07:47"
        },
        {
          "id": "MDRW2100038531.1.12",
          "form": "날씨가 좋아질생각을안하네",
          "original_form": "날씨가 좋아질생각을안하네",
          "speaker_id": "2",
          "time": "20210903 07:48"
        },
        {
          "id": "MDRW2100038531.1.13",
          "form": "밥먹었나?",
          "original_form": "밥먹었나?",
          "speaker_id": "2",
          "time": "20210903 07:49"
        },
        {
          "id": "MDRW2100038531.1.14",
          "form": "보내고 먹을려구요ㅋㅋ",
          "original_form": "보내고 먹을려구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 07:49"
        },
        {
          "id": "MDRW2100038531.1.15",
          "form": "뭐 먹었어요??",
          "original_form": "뭐 먹었어요??",
          "speaker_id": "1",
          "time": "20210903 07:49"
        },
        {
          "id": "MDRW2100038531.1.16",
          "form": "난 아직",
          "original_form": "난 아직",
          "speaker_id": "2",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100038531.1.17",
          "form": "오늘아침 모닝커피?",
          "original_form": "오늘아침 모닝커피?",
          "speaker_id": "2",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100038531.1.18",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100038531.1.19",
          "form": "밥보다 모닝커피가 더 중요하죠ㅋㅋㅋ",
          "original_form": "밥보다 모닝커피가 더 중요하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 07:58"
        },
        {
          "id": "MDRW2100038531.1.20",
          "form": "아이고 몸이 주욱 죽 쳐집니다~",
          "original_form": "아이고 몸이 주욱 죽 쳐집니다~",
          "speaker_id": "1",
          "time": "20210903 07:59"
        },
        {
          "id": "MDRW2100038531.1.21",
          "form": "출근하기싫은 금욜이구먼",
          "original_form": "출근하기싫은 금욜이구먼",
          "speaker_id": "2",
          "time": "20210903 08:06"
        },
        {
          "id": "MDRW2100038531.1.22",
          "form": "시간이있어도 집치우는건 느무싫으네ㅋ",
          "original_form": "시간이있어도 집치우는건 느무싫으네ㅋ",
          "speaker_id": "2",
          "time": "20210903 08:07"
        },
        {
          "id": "MDRW2100038531.1.23",
          "form": "좋은거 뭐없나?ㅋ",
          "original_form": "좋은거 뭐없나?ㅋ",
          "speaker_id": "2",
          "time": "20210903 08:07"
        },
        {
          "id": "MDRW2100038531.1.24",
          "form": "언니..돈을 많이벌어서",
          "original_form": "언니..돈을 많이벌어서",
          "speaker_id": "1",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100038531.1.25",
          "form": "도우미 쓰는거 밖에는 ㅋㅋㅋㅋㅋ",
          "original_form": "도우미 쓰는거 밖에는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100038531.1.26",
          "form": "침대 커버 미루다 미루다 오늘 갈았네여;;",
          "original_form": "침대 커버 미루다 미루다 오늘 갈았네여;;",
          "speaker_id": "1",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100038531.1.27",
          "form": "도우미 좋다좋다",
          "original_form": "도우미 좋다좋다",
          "speaker_id": "2",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100038531.1.28",
          "form": "그런날 오길",
          "original_form": "그런날 오길",
          "speaker_id": "2",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100038531.1.29",
          "form": "얼마를 더 벌어야 해여??ㅋㅋㅋㅋㅋ",
          "original_form": "얼마를 더 벌어야 해여??ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:38"
        },
        {
          "id": "MDRW2100038531.1.30",
          "form": "상대적이라ㅋㅋ",
          "original_form": "상대적이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100038531.1.31",
          "form": "300만벌어도 쓰고싶네 일주일에한번ㅋㅋㅋ",
