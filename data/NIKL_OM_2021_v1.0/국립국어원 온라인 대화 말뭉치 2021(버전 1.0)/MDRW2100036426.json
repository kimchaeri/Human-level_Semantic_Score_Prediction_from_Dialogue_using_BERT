        },
        {
          "id": "MDRW2100036426.1.3",
          "form": "저는 김치찌개요",
          "original_form": "저는 김치찌개요",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100036426.1.4",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100036426.1.5",
          "form": "저는 돼지불고기요!!",
          "original_form": "저는 돼지불고기요!!",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100036426.1.6",
          "form": "김치찌개에는 뭐 넣으셨어용??",
          "original_form": "김치찌개에는 뭐 넣으셨어용??",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100036426.1.7",
          "form": "돼지고기요!!",
          "original_form": "돼지고기요!!",
          "speaker_id": "1",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100036426.1.8",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100036426.1.9",
          "form": "저는 샤브샤브좋아합니다!",
          "original_form": "저는 샤브샤브좋아합니다!",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036426.1.10",
          "form": "등촌샤브샤브",
          "original_form": "등촌샤브샤브{emoji:♡}",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036426.1.11",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036426.1.12",
          "form": "네 저 거기 볶음밥 엄청 좋아해요",
          "original_form": "네 저 거기 볶음밥 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036426.1.13",
          "form": "거기 볶음밥 좋아하시나요??",
          "original_form": "거기 볶음밥 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036426.1.14",
          "form": "완전 맛있쥬",
          "original_form": "완전 맛있쥬",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100036426.1.15",
          "form": "그 안에 들어간 미나리도 좋아해요ㅠㅠ",
          "original_form": "그 안에 들어간 미나리도 좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036426.1.16",
          "form": "맞아요 정말 맛있죠 보통 가면 뭐 시켜 드시나요??",
          "original_form": "맞아요 정말 맛있죠 보통 가면 뭐 시켜 드시나요??",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036426.1.17",
          "form": "가면 버섯칼국수에 고기먹죠!",
          "original_form": "가면 버섯칼국수에 고기먹죠!",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036426.1.18",
          "form": "메뉴가 또 있나요?",
          "original_form": "메뉴가 또 있나요?",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100036426.1.19",
          "form": "앗 저는 만두 꼭 추가해서 먹거든요",
          "original_form": "앗 저는 만두 꼭 추가해서 먹거든요",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036426.1.20",
          "form": "등촌에 만두도 추가가되나요????",
          "original_form": "등촌에 만두도 추가가되나요????",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100036426.1.21",
          "form": "오 제가 가는곳은 그런 사이드 없어요~~~",
          "original_form": "오 제가 가는곳은 그런 사이드 없어요~~~",
          "speaker_id": "2",
