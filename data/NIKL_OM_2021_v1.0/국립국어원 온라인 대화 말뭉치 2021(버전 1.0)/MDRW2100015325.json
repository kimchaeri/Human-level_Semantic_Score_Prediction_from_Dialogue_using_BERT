      "utterance": [
        {
          "id": "MDRW2100015325.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015325.1.2",
          "form": "아뇨!! 근데 키우고싶어요",
          "original_form": "아뇨!! 근데 키우고싶어요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015325.1.3",
          "form": "키우시나요??",
          "original_form": "키우시나요??",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015325.1.4",
          "form": "반려동물은 부모님께서 키우세요",
          "original_form": "반려동물은 부모님께서 키우세요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.5",
          "form": "그 개는 절 미워하구요..",
          "original_form": "그 개는 절 미워하구요..",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.6",
          "form": "오 어떤거요?",
          "original_form": "오 어떤거요?",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.7",
          "form": "아 ㅋㅋㅋㅋ종이 뭔가요??",
          "original_form": "아 ㅋㅋㅋㅋ종이 뭔가요??",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.8",
          "form": "비글이랑 말티즈 합친 잡종이에요",
          "original_form": "비글이랑 말티즈 합친 잡종이에요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.9",
          "form": "헐 귀여울거같은데",
          "original_form": "헐 귀여울거같은데",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.10",
          "form": "근본도 없는 강아지에게 개무시 당해요 ㅜㅜ",
          "original_form": "근본도 없는 강아지에게 개무시 당해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.11",
          "form": "왠지 작을거같애요 ㅋㅋㅋㅋㅋ",
          "original_form": "왠지 작을거같애요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.12",
          "form": "중형견이에요...",
          "original_form": "중형견이에요...",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.13",
          "form": "아니 왜요 ㅋㅋ",
          "original_form": "아니 왜요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.14",
          "form": "잘안대하시는거아니에요?",
          "original_form": "잘안대하시는거아니에요?",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015325.1.15",
          "form": "오 장난감으로 놀아봐주세요",
          "original_form": "오 장난감으로 놀아봐주세요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.16",
          "form": "저를 하찮게 보더라구요..",
          "original_form": "저를 하찮게 보더라구요..",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.17",
          "form": "쓰다듬어주다가 물려보셨나요?ㅠㅠ",
          "original_form": "쓰다듬어주다가 물려보셨나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.19",
          "form": "간식줘보세요",
          "original_form": "간식줘보세요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.20",
          "form": "놀아주면 흥분해서 손 콱 물어요",
          "original_form": "놀아주면 흥분해서 손 콱 물어요",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.21",
          "form": "강아지들 간식좋아하시던데",
          "original_form": "강아지들 간식좋아하시던데",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.22",
          "form": "간식 주면 그때만 말잘듣고",
          "original_form": "간식 주면 그때만 말잘듣고",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.23",
          "form": "name1님한테만 그러는거에요??",
          "original_form": "&name1&님한테만 그러는거에요??",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015325.1.24",
          "form": "간식 떨어지는 순간 다시 또 으르릉",
          "original_form": "간식 떨어지는 순간 다시 또 으르릉",
