      "utterance": [
        {
          "id": "MDRW2100018815.1.1",
          "form": "오늘 점심은 뭐드실거에요?",
          "original_form": "오늘 점심은 뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018815.1.2",
          "form": "방금일어나서 아직 아무생각이 없어요",
          "original_form": "방금일어나서 아직 아무생각이 없어요",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018815.1.3",
          "form": "2시에 시연있는데 망했내요",
          "original_form": "2시에 시연있는데 망했내요",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018815.1.4",
          "form": "그거끝나고 먹을거같은디",
          "original_form": "그거끝나고 먹을거같은디",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018815.1.5",
          "form": "name1님 뭐드셨나요",
          "original_form": "&name1&님 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018815.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ방금일어나셧다닠ㅋㅋㅋㅋ아웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ방금일어나셧다닠ㅋㅋㅋㅋ아웃겨",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018815.1.7",
          "form": "저는 김치닭볶음탕? 먹었어요",
          "original_form": "저는 김치닭볶음탕? 먹었어요",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018815.1.8",
          "form": "오 버블에서는 닭볶음탕 먹어도 먹었다고 말 못하는데",
          "original_form": "오 버블에서는 닭볶음탕 먹어도 먹었다고 말 못하는데",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018815.1.9",
          "form": "심심이는 다행히 닭볶음탕을 허락해주네요",
          "original_form": "심심이는 다행히 닭볶음탕을 허락해주네요",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018815.1.10",
          "form": "아니 닭복음탕이 왴ㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋ",
          "original_form": "아니 닭복음탕이 왴ㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018815.1.11",
          "form": "버블은 신기한곳이군여~~",
          "original_form": "버블은 신기한곳이군여~~",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018815.1.12",
          "form": "어머니가 해주셨나요",
          "original_form": "어머니가 해주셨나요",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018815.1.13",
          "form": "네 버블은 욕이랑",
          "original_form": "네 버블은 욕이랑",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018815.1.14",
          "form": "하 여기도",
          "original_form": "하 여기도",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018815.1.15",
          "form": "야",
          "original_form": "야",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018815.1.16",
          "form": "한",
          "original_form": "한",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018815.1.17",
          "form": "말",
          "original_form": "말",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018815.1.18",
          "form": "을 못쓰는군요",
          "original_form": "을 못쓰는군요",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018815.1.19",
          "form": "어쨌든 닭볶'음탕'이라서 안된대요 버블은ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어쨌든 닭볶'음탕'이라서 안된대요 버블은ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018815.1.20",
          "form": "엄마가 해주셨어요 ㅋㅋㅋ김치도넣구 ㅋㅋ맛있었습니다",
          "original_form": "엄마가 해주셨어요 ㅋㅋㅋ김치도넣구 ㅋㅋ맛있었습니다",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018815.1.21",
          "form": "점심은 뭐먹을지 고민입니다 ㅋㅋㅋ 요즘 사과떡볶이가 유명하다는데 들어보셧나요?",
          "original_form": "점심은 뭐먹을지 고민입니다 ㅋㅋㅋ 요즘 사과떡볶이가 유명하다는데 들어보셧나요?",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018815.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ음 ㅋㅋㅋㅋㅋ탕ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ음 ㅋㅋㅋㅋㅋ탕ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018815.1.23",
          "form": "아 닭볶음이 ㅋㅋㅋㅋ점심이 아니라 아침...!!!!",
          "original_form": "아 닭볶음이 ㅋㅋㅋㅋ점심이 아니라 아침...!!!!",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018815.1.24",
          "form": "사과떡볶이 첨들어봐요 어디서파나요",
          "original_form": "사과떡볶이 첨들어봐요 어디서파나요",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100018815.1.25",
          "form": "ㅋㅋ네 아침이에용",
          "original_form": "ㅋㅋ네 아침이에용",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018815.1.26",
          "form": "사과떡볶이가 되게유명하대요 그 일산? 파주? 쪽에있는데",
          "original_form": "사과떡볶이가 되게유명하대요 그 일산? 파주? 쪽에있는데",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018815.1.27",
          "form": "밀키트 배송으로도 살수있는데 3달기다려야한대요",
          "original_form": "밀키트 배송으로도 살수있는데 3달기다려야한대요",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018815.1.28",
          "form": "대박이죠 ㅋㅋㅋ소스에 사과가들어가서 사과떡볶이인가봐요",
          "original_form": "대박이죠 ㅋㅋㅋ소스에 사과가들어가서 사과떡볶이인가봐요",
          "speaker_id": "1",
          "time": "20210827 12:46"
        },
        {
          "id": "MDRW2100018815.1.29",
          "form": "와 3달ㅋㅋㅋㅋㅋㅋㅋ너무 맵지않으면 먹어보고싶네요!!",
          "original_form": "와 3달ㅋㅋㅋㅋㅋㅋㅋ너무 맵지않으면 먹어보고싶네요!!",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018815.1.30",
          "form": "그쵸 ㅋㅋ갑자기 떡볶이끌려서 저도지금 떡볶이하고있어요! ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋ갑자기 떡볶이끌려서 저도지금 떡볶이하고있어요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018815.1.31",
          "form": "어머 직접 요리도 하시나봐요",
          "original_form": "어머 직접 요리도 하시나봐요",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018815.1.32",
          "form": "떡볶이 만들때 어떤거 넣으세요??",
          "original_form": "떡볶이 만들때 어떤거 넣으세요??",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018815.1.33",
          "form": "양파?",
          "original_form": "양파?",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018815.1.34",
          "form": "햄?",
          "original_form": "햄?",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018815.1.35",
          "form": "넹 방금 떡 스팸 치즈 양배추 양파 파 킹크랩맛살 토마토 넣어서 해먹었어요 ㅋㅋㅋ",
          "original_form": "넹 방금 떡 스팸 치즈 양배추 양파 파 킹크랩맛살 토마토 넣어서 해먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100018815.1.36",
          "form": "저는 쌀떡이좋은데 오늘은 밀떡으로 해먹었네용ㅋㅋㅋ",
          "original_form": "저는 쌀떡이좋은데 오늘은 밀떡으로 해먹었네용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100018815.1.37",
          "form": "와 토마토?!?!?!",
          "original_form": "와 토마토?!?!?!",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018815.1.38",
          "form": "우리 사과떡볶이말고 토마토떡볶이 만들어서 사업합시다",
          "original_form": "우리 사과떡볶이말고 토마토떡볶이 만들어서 사업합시다",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018815.1.39",
          "form": "ㅋㅋ좋아용ㅋㅋ저는 토마토 익혀먹는거 너무좋아해서 여기에도 넣어봤어요",
          "original_form": "ㅋㅋ좋아용ㅋㅋ저는 토마토 익혀먹는거 너무좋아해서 여기에도 넣어봤어요",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100018815.1.40",
          "form": "토마토 익혀먹으면 진짜맛있거든용",
