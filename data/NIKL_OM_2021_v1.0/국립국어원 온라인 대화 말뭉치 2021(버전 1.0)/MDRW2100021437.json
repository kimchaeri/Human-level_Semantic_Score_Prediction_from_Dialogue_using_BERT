      "utterance": [
        {
          "id": "MDRW2100021437.1.1",
          "form": "오늘 저녁드셨나요?",
          "original_form": "오늘 저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:00"
        },
        {
          "id": "MDRW2100021437.1.2",
          "form": "지금 떡볶이 먹고있습니다 ㅎㅎ",
          "original_form": "지금 떡볶이 먹고있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021437.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021437.1.4",
          "form": "저는지금먹으러가요",
          "original_form": "저는지금먹으러가요",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021437.1.5",
          "form": "버거킹ㅎㅎ",
          "original_form": "버거킹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100021437.1.6",
          "form": "맛있는 걸로 먹고 오세요 ㅋㅋㅋㅋㅋ",
          "original_form": "맛있는 걸로 먹고 오세요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021437.1.7",
          "form": "버거킹 다 맛있어서 뭘 먹어도 맛있겠지만요ㅠ ㅋㅋㅋㅋ",
          "original_form": "버거킹 다 맛있어서 뭘 먹어도 맛있겠지만요ㅠ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021437.1.8",
          "form": "과트로 치즈와퍼 먹고왔어요ㅎㅎㅠ",
          "original_form": "과트로 치즈와퍼 먹고왔어요ㅎㅎㅠ",
          "speaker_id": "1",
          "time": "20210829 14:45"
        },
        {
          "id": "MDRW2100021437.1.9",
          "form": "어제바빠서 못봤네요",
          "original_form": "어제바빠서 못봤네요",
          "speaker_id": "1",
          "time": "20210829 14:45"
        },
        {
          "id": "MDRW2100021437.1.10",
          "form": "무슨떡볶이 먹었나요?",
          "original_form": "무슨떡볶이 먹었나요?",
          "speaker_id": "1",
          "time": "20210829 14:45"
        },
        {
          "id": "MDRW2100021437.1.11",
          "form": "명랑 로제떡볶이 먹었어요!",
          "original_form": "명랑 로제떡볶이 먹었어요!",
          "speaker_id": "2",
          "time": "20210829 14:46"
        },
        {
          "id": "MDRW2100021437.1.12",
          "form": "핫도그 찍어 먹으니까 맛있더라고요 ㅎㅎ",
          "original_form": "핫도그 찍어 먹으니까 맛있더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100021437.1.13",
          "form": "명랑 떡볶이 궁금햿는데! 다른 로제 드셔보셨으면 어디랑 비슷한것같나요??",
          "original_form": "명랑 떡볶이 궁금햿는데! 다른 로제 드셔보셨으면 어디랑 비슷한것같나요??",
          "speaker_id": "1",
          "time": "20210829 14:47"
        },
        {
          "id": "MDRW2100021437.1.14",
          "form": "핫도그가 있냐없냐 그 차이인 것 같아요!",
          "original_form": "핫도그가 있냐없냐 그 차이인 것 같아요!",
          "speaker_id": "2",
          "time": "20210829 14:49"
        },
        {
          "id": "MDRW2100021437.1.15",
          "form": "소스 맛은 비슷하고, 아 치즈가 더 맛있었던 것 같아요. 떡은 밀떡!",
          "original_form": "소스 맛은 비슷하고, 아 치즈가 더 맛있었던 것 같아요. 떡은 밀떡!",
          "speaker_id": "2",
          "time": "20210829 14:49"
        },
        {
          "id": "MDRW2100021437.1.16",
          "form": "아 그렇군요ㅋㅋㅋ 보편적인? 로제 맛인가보ㅓ요!",
          "original_form": "아 그렇군요ㅋㅋㅋ 보편적인? 로제 맛인가보ㅓ요!",
          "speaker_id": "1",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100021437.1.17",
          "form": "밀떡파세요 쌀떡파세요?",
          "original_form": "밀떡파세요 쌀떡파세요?",
          "speaker_id": "1",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100021437.1.18",
          "form": "저는 쌀떡파에요  ㅎㅎ\r\n양념 베이는 걸 좋아해서!",
          "original_form": "저는 쌀떡파에요  ㅎㅎ\r\n양념 베이는 걸 좋아해서!",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100021437.1.19",
          "form": "어느 쪽이세요?",
          "original_form": "어느 쪽이세요?",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100021437.1.20",
          "form": "그렇군요!!",
          "original_form": "그렇군요!!",
          "speaker_id": "1",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100021437.1.21",
          "form": "저는 밀떡이요",
          "original_form": "저는 밀떡이요",
          "speaker_id": "1",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100021437.1.22",
          "form": "쫀득한게 좋아서요ㅎㅎ",
          "original_form": "쫀득한게 좋아서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100021437.1.23",
          "form": "쌀떡은 파는곳이 적지 않나요?",
          "original_form": "쌀떡은 파는곳이 적지 않나요?",
          "speaker_id": "1",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100021437.1.24",
          "form": "명량 로제는 밀떡이었어요!",
          "original_form": "명량 로제는 밀떡이었어요!",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100021437.1.25",
          "form": "저희 지역은 쌀떡을 더 많이 먹어요 ㅎㅎ",
          "original_form": "저희 지역은 쌀떡을 더 많이 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100021437.1.26",
          "form": "아 그렇군요!!! 저희동네는 아딸 아시나요?",
          "original_form": "아 그렇군요!!! 저희동네는 아딸 아시나요?",
          "speaker_id": "1",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100021437.1.27",
          "form": "아딸있었는데 몇년전에 사라졋거든요..",
          "original_form": "아딸있었는데 몇년전에 사라졋거든요..",
          "speaker_id": "1",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100021437.1.28",
          "form": "저는 처음 들어봤어요 ㅎㅎ",
          "original_form": "저는 처음 들어봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100021437.1.29",
          "form": "좋아하시는 곳 같은데 어쩌다 없어졌을까요ㅠ",
          "original_form": "좋아하시는 곳 같은데 어쩌다 없어졌을까요ㅠ",
          "speaker_id": "2",
          "time": "20210831 14:24"
        }
      ]
    }
