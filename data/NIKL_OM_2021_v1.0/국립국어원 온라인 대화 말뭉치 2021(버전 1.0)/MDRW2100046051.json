      "utterance": [
        {
          "id": "MDRW2100046051.1.1",
          "form": "진짜요ㅜㅜ",
          "original_form": "진짜요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046051.1.2",
          "form": "별로있지도않고",
          "original_form": "별로있지도않고",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046051.1.3",
          "form": "그나마 쿠에른 기스전 할 때 좀 건지고",
          "original_form": "그나마 쿠에른 기스전 할 때 좀 건지고",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046051.1.4",
          "form": "근데 예쁜 구두 보면 또 눈은 돌아가고",
          "original_form": "근데 예쁜 구두 보면 또 눈은 돌아가고",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046051.1.5",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046051.1.6",
          "form": "저는신발은이제",
          "original_form": "저는신발은이제",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046051.1.7",
          "form": "하두못신으니까",
          "original_form": "하두못신으니까",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046051.1.8",
          "form": "욕심도사라지더라구요 ㅋㅋ",
          "original_form": "욕심도사라지더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046051.1.9",
          "form": "저도 결국은 거의 운동화이긴 한데ㅠㅠㅠ",
          "original_form": "저도 결국은 거의 운동화이긴 한데ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046051.1.10",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.11",
          "form": "근데 아직도 욕심은 자꾸 나네요...",
          "original_form": "근데 아직도 욕심은 자꾸 나네요...",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.12",
          "form": "코시국 이전의 화장품처럼ㅜㅜ",
          "original_form": "코시국 이전의 화장품처럼ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.13",
          "form": "근데 코로나 끝나면",
          "original_form": "근데 코로나 끝나면",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.14",
          "form": "저는 신발은 직구로사려고 벼르고있어용",
          "original_form": "저는 신발은 직구로사려고 벼르고있어용",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.15",
          "form": "과연 제가 화장품을 다시 모을지가",
          "original_form": "과연 제가 화장품을 다시 모을지가",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.16",
          "form": "궁금해요ㅋㅋㅋㅋㅋ",
          "original_form": "궁금해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.17",
          "form": "과연 물욕은 부활하는가",
          "original_form": "과연 물욕은 부활하는가",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.18",
          "form": "저는안살거같아요 ㅋㅋㅋ화장품은 이제",
          "original_form": "저는안살거같아요 ㅋㅋㅋ화장품은 이제",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.19",
          "form": "졸업 ㅋㅋㅋ",
          "original_form": "졸업 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046051.1.20",
          "form": "저도 한풀 꺾이긴 했는데ㅋㅋㅋ",
          "original_form": "저도 한풀 꺾이긴 했는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046051.1.21",
          "form": "대신 옷 쪽으로 좀 더 쏠린 거 같긴해요",
          "original_form": "대신 옷 쪽으로 좀 더 쏠린 거 같긴해요",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046051.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046051.1.23",
          "form": "저두요ㅜㅋㅋㅋ",
          "original_form": "저두요ㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046051.1.24",
          "form": "코트 왕창 사고싶구",
          "original_form": "코트 왕창 사고싶구",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046051.1.25",
          "form": "화징품사던돈 옷사요",
          "original_form": "화징품사던돈 옷사요",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046051.1.26",
          "form": "코트가 진짜 돈 쓸 맛 나는 거 같아요",
          "original_form": "코트가 진짜 돈 쓸 맛 나는 거 같아요",
