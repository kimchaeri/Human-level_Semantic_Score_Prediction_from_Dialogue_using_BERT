        {
          "id": "MDRW2100001768.1.1",
          "form": "오늘도즐거운하루",
          "original_form": "오늘도즐거운하루",
          "speaker_id": "2",
          "time": "20210820 09:05"
        },
        {
          "id": "MDRW2100001768.1.2",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100001768.1.3",
          "form": "한시에병원가 밥먹어",
          "original_form": "한시에병원가 밥먹어",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100001768.1.4",
          "form": "네 어제남은부침개있잔아요",
          "original_form": "네 어제남은부침개있잔아요",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100001768.1.5",
          "form": "그거랑밥꼭먹어",
          "original_form": "그거랑밥꼭먹어",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100001768.1.6",
          "form": "걱정시켜드리지않게 꼭먹을께요",
          "original_form": "걱정시켜드리지않게 꼭먹을께요",
          "speaker_id": "1",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100001768.1.7",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100001768.1.8",
          "form": "날이덥네요",
          "original_form": "날이덥네요",
          "speaker_id": "1",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100001768.1.9",
          "form": "안태워자줘도되",
          "original_form": "안태워자줘도되",
          "speaker_id": "2",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100001768.1.10",
          "form": "덥잔아요 태워다드릴께요",
          "original_form": "덥잔아요 태워다드릴께요",
          "speaker_id": "1",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100001768.1.11",
          "form": "괜찬아",
          "original_form": "괜찬아",
          "speaker_id": "2",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100001768.1.12",
          "form": "필요하면말만하세요",
          "original_form": "필요하면말만하세요",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100001768.1.13",
          "form": "그럴께아들",
          "original_form": "그럴께아들",
          "speaker_id": "2",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100001768.1.14",
          "form": "네 아프면안되잔아요",
          "original_form": "네 아프면안되잔아요",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100001768.1.15",
          "form": "그래서병원가는거지",
          "original_form": "그래서병원가는거지",
          "speaker_id": "2",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100001768.1.16",
          "form": "그렇기는해요",
          "original_form": "그렇기는해요",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100001768.1.17",
          "form": "집이나잘보고 집안일할꺼있음하고",
          "original_form": "집이나잘보고 집안일할꺼있음하고",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100001768.1.18",
          "form": "물론이죠 걱정마세요",
          "original_form": "물론이죠 걱정마세요",
          "speaker_id": "1",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100001768.1.19",
          "form": "그래믿는다",
          "original_form": "그래믿는다",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100001768.1.20",
          "form": "네 저녁에는 닭먹어요",
          "original_form": "네 저녁에는 닭먹어요",
          "speaker_id": "1",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100001768.1.21",
          "form": "그래 노력의결과디",
          "original_form": "그래 노력의결과디",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100001768.1.22",
          "form": "이것도노력이겠죠",
          "original_form": "이것도노력이겠죠",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100001768.1.23",
          "form": "열심히해보자",
          "original_form": "열심히해보자",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100001768.1.24",
          "form": "네 당연하죠",
          "original_form": "네 당연하죠",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100001768.1.25",
          "form": "아빠는 밥먹었다",
          "original_form": "아빠는 밥먹었다",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100001768.1.26",
          "form": "식사하고 유투브보시는것같네요",
          "original_form": "식사하고 유투브보시는것같네요",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100001768.1.27",
          "form": "즐거움이지",
          "original_form": "즐거움이지",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100001768.1.28",
          "form": "21세기라디오죠",
          "original_form": "21세기라디오죠",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100001768.1.29",
          "form": "나도 즐기지",
          "original_form": "나도 즐기지",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100001768.1.30",
