      "utterance": [
        {
          "id": "MDRW2100032770.1.1",
          "form": "안녕하세요 거기 날씨는 어때요",
          "original_form": "안녕하세요 거기 날씨는 어때요",
          "speaker_id": "1",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032770.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032770.1.3",
          "form": "여기 어제부터 갑자기 쌀쌀해졌어요",
          "original_form": "여기 어제부터 갑자기 쌀쌀해졌어요",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032770.1.4",
          "form": "비도 조금씩 오고 그러네요ㅜ",
          "original_form": "비도 조금씩 오고 그러네요ㅜ",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032770.1.5",
          "form": "제주는 아직 그래도 반팔입을만 해요",
          "original_form": "제주는 아직 그래도 반팔입을만 해요",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032770.1.6",
          "form": "아하 아직 따뜻한가봐요 아무래도!",
          "original_form": "아하 아직 따뜻한가봐요 아무래도!",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032770.1.7",
          "form": "비는 안오죠?",
          "original_form": "비는 안오죠?",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032770.1.8",
          "form": "네 근데 최근엔 많이 왔어요",
          "original_form": "네 근데 최근엔 많이 왔어요",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032770.1.9",
          "form": "축축 처지고 습하고",
          "original_form": "축축 처지고 습하고",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032770.1.10",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032770.1.11",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032770.1.12",
          "form": "아직 여름 장마인가봐요ㅠㅠ",
          "original_form": "아직 여름 장마인가봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032770.1.13",
          "form": "여기는 막상 추워지고 나니까",
          "original_form": "여기는 막상 추워지고 나니까",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032770.1.14",
          "form": "여름이 벌써 그립네요ㅠㅠ 어제는 반팔입고 나가서 떨다 왔어요ㅋㅋ",
          "original_form": "여름이 벌써 그립네요ㅠㅠ 어제는 반팔입고 나가서 떨다 왔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032770.1.15",
          "form": "저도 내내 샌들신고 다녔는데",
          "original_form": "저도 내내 샌들신고 다녔는데",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032770.1.16",
          "form": "양말안빨아도 되서 좋았거든요 ㅋㅋ",
          "original_form": "양말안빨아도 되서 좋았거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032770.1.17",
          "form": "ㅋㅋㅋㅋㅋ마자요",
          "original_form": "ㅋㅋㅋㅋㅋ마자요",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100032770.1.18",
          "form": "양말 안빠는거 진짜 좋았죠ㅋㅋ",
          "original_form": "양말 안빠는거 진짜 좋았죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100032770.1.19",
          "form": "이제 또 껴입을 생각하니까",
          "original_form": "이제 또 껴입을 생각하니까",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100032770.1.20",
          "form": "벌써 빨래할 생각에 막막하네요ㅋㅋ 겨울옷이 워낙 무거우니까",
          "original_form": "벌써 빨래할 생각에 막막하네요ㅋㅋ 겨울옷이 워낙 무거우니까",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100032770.1.21",
          "form": "옷도 비싸자나여",
          "original_form": "옷도 비싸자나여",
          "speaker_id": "1",
          "time": "20210905 23:41"
        },
        {
          "id": "MDRW2100032770.1.22",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:42"
        },
        {
          "id": "MDRW2100032770.1.23",
          "form": "오늘 날씨는 어때요?",
          "original_form": "오늘 날씨는 어때요?",
          "speaker_id": "2",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100032770.1.24",
          "form": "여기는 화창해요 여전히 쌀쌀하긴한데",
          "original_form": "여기는 화창해요 여전히 쌀쌀하긴한데",
          "speaker_id": "2",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100032770.1.25",
          "form": "구름껴있어요",
          "original_form": "구름껴있어요",
          "speaker_id": "1",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032770.1.26",
          "form": "바다수영하러갈까 생각중이예요",
          "original_form": "바다수영하러갈까 생각중이예요",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100032770.1.27",
          "form": "우와 엄청 부럽네요ㅜㅜ",
          "original_form": "우와 엄청 부럽네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100032770.1.28",
          "form": "바다수영이라니ㅠ",
          "original_form": "바다수영이라니ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100032770.1.29",
          "form": "제주도라이프는 역시 다르네요ㅎㅎ",
          "original_form": "제주도라이프는 역시 다르네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100032770.1.30",
          "form": "가면 수영하는 사람들 많죠??",
          "original_form": "가면 수영하는 사람들 많죠??",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100032770.1.31",
          "form": "네 9월중순까지 가능하대요",
          "original_form": "네 9월중순까지 가능하대요",
          "speaker_id": "1",
