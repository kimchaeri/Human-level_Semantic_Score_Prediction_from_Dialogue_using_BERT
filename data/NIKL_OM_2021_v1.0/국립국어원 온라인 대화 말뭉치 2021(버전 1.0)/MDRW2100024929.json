      "utterance": [
        {
          "id": "MDRW2100024929.1.1",
          "form": "게임 뭐가 재밌나요?",
          "original_form": "게임 뭐가 재밌나요?",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024929.1.2",
          "form": "모바일 게임 하나 하고 있어서 잘 모르겠어요",
          "original_form": "모바일 게임 하나 하고 있어서 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024929.1.3",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024929.1.4",
          "form": "사실 저도 막 하는편이아니라서",
          "original_form": "사실 저도 막 하는편이아니라서",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024929.1.5",
          "form": "어쩌다가",
          "original_form": "어쩌다가",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024929.1.6",
          "form": "네 그러시군요",
          "original_form": "네 그러시군요",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024929.1.7",
          "form": "조금씩 하다가 말다가 해서",
          "original_form": "조금씩 하다가 말다가 해서",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024929.1.8",
          "form": "최근에 심즈 했어요 ㅎ",
          "original_form": "최근에 심즈 했어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024929.1.9",
          "form": "올만에 하니까 또 며칠내내하고 그러더라고요",
          "original_form": "올만에 하니까 또 며칠내내하고 그러더라고요",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024929.1.10",
          "form": "심즈는 구축되는게 재밌어서 시간 많이 뺏기긴하죠",
          "original_form": "심즈는 구축되는게 재밌어서 시간 많이 뺏기긴하죠",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100024929.1.11",
          "form": "다른 게임은 어떤거 해보셨어요?",
          "original_form": "다른 게임은 어떤거 해보셨어요?",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024929.1.12",
          "form": "네 ㅎ 별거안했는데 시간 훅 가있더라고요",
          "original_form": "네 ㅎ 별거안했는데 시간 훅 가있더라고요",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100024929.1.13",
          "form": "저 최근에 물감? 채우는거 해봤어요",
          "original_form": "저 최근에 물감? 채우는거 해봤어요",
          "speaker_id": "2",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100024929.1.14",
          "form": "게임 이름이 생각안나네요ㅜ",
          "original_form": "게임 이름이 생각안나네요ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100024929.1.15",
          "form": "컬러링북 같은 건가요??",
          "original_form": "컬러링북 같은 건가요??",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100024929.1.16",
          "form": "물감 퍼즐?",
          "original_form": "물감 퍼즐?",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100024929.1.17",
          "form": "인거같아요!",
          "original_form": "인거같아요!",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100024929.1.18",
          "form": "잘 모르겠지만 킬링타임용으로는 좋을 것 같아요!",
          "original_form": "잘 모르겠지만 킬링타임용으로는 좋을 것 같아요!",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100024929.1.19",
          "form": "네네 좋아요!",
          "original_form": "네네 좋아요!",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100024929.1.20",
          "form": "게임 재밌게 하신거있나여?",
          "original_form": "게임 재밌게 하신거있나여?",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100024929.1.21",
          "form": "디즈니 매직킹덤 이라고 지금 하나 하고있어요!",
          "original_form": "디즈니 매직킹덤 이라고 지금 하나 하고있어요!",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100024929.1.22",
          "form": "디즈니 캐릭터 모으는 그런 게임이에요 ㅎ",
          "original_form": "디즈니 캐릭터 모으는 그런 게임이에요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100024929.1.23",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100024929.1.24",
          "form": "디즈니 완전 좋아하는데",
          "original_form": "디즈니 완전 좋아하는데",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100024929.1.25",
          "form": "재밌겠네요!!!!!!!!!!!",
          "original_form": "재밌겠네요!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210831 22:48"
        }
      ]
    }
  ]
