      "utterance": [
        {
          "id": "MDRW2100043600.1.1",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043600.1.2",
          "form": "네 만둣국 먹었어요 드셨나요?",
          "original_form": "네 만둣국 먹었어요 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043600.1.3",
          "form": "저는 삼겹살 에프에돌리고있어요",
          "original_form": "저는 삼겹살 에프에돌리고있어요",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043600.1.4",
          "form": "대박..",
          "original_form": "대박..",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043600.1.5",
          "form": "맛있겠네요 ㅠㅠ",
          "original_form": "맛있겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043600.1.6",
          "form": "ㅎㅎ백신푸드입니당",
          "original_form": "ㅎㅎ백신푸드입니당",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043600.1.7",
          "form": "백신푸드 ㅋㅋㅋㅋ 먹고 기운내셔야겠어요 ㅋㅋ",
          "original_form": "백신푸드 ㅋㅋㅋㅋ 먹고 기운내셔야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043600.1.8",
          "form": "네네 오늘 백신2차맞고 고기먹을거가고 벼르고있었어요",
          "original_form": "네네 오늘 백신2차맞고 고기먹을거가고 벼르고있었어요",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043600.1.9",
          "form": "name2님은 백신푸드뭘로하시나요",
          "original_form": "&name2&님은 백신푸드뭘로하시나요",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043600.1.10",
          "form": "아 전 아직이라 고민되는데 엄마가 삼겹살 얘기하긴 하더라고요 ㅋㅋㅋㅋ",
          "original_form": "아 전 아직이라 고민되는데 엄마가 삼겹살 얘기하긴 하더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043600.1.11",
          "form": "먹어야된다고 ㅋㅋ",
          "original_form": "먹어야된다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043600.1.12",
          "form": "네네 ㅋㅋㅋㅋㅋ몸에 기력넣어줘여해요",
          "original_form": "네네 ㅋㅋㅋㅋㅋ몸에 기력넣어줘여해요",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043600.1.13",
          "form": "ㅋㅋㅋㅋㅋ 에프 돌리면 기름 쫙 빠지나요? 안 먹어봐서 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ 에프 돌리면 기름 쫙 빠지나요? 안 먹어봐서 ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043600.1.14",
          "form": "막 다빠지지는않구여 구울때보단 확실히 빠지긴해요 돌돌말아서 돌리면",
          "original_form": "막 다빠지지는않구여 구울때보단 확실히 빠지긴해요 돌돌말아서 돌리면",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043600.1.15",
          "form": "기름이 많이안빠져나가서 겉은바삭하고 안은촉촉해요 ㅎㅎ",
          "original_form": "기름이 많이안빠져나가서 겉은바삭하고 안은촉촉해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043600.1.16",
          "form": "아 정말요 완전 완벽해지는거네요 ㅋㅋㅋㅋ",
          "original_form": "아 정말요 완전 완벽해지는거네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043600.1.17",
          "form": "그럼 그대로 먹는게 더 낫나요 쌈보다?",
          "original_form": "그럼 그대로 먹는게 더 낫나요 쌈보다?",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043600.1.18",
          "form": "그건또 아녀요 ㅋㅋㅋㅋㅋ그대로먹는게 더맛있긴해요 아무래도 기름좔좔하다보니",
          "original_form": "그건또 아녀요 ㅋㅋㅋㅋㅋ그대로먹는게 더맛있긴해요 아무래도 기름좔좔하다보니",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043600.1.19",
          "form": "제가 기름진거좋아하거든야",
          "original_form": "제가 기름진거좋아하거든야",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043600.1.20",
          "form": "아하 ㅋㅋㅋ 아무튼 기름진 음식으로 충전하시겠네요 ㅎㅎ",
          "original_form": "아하 ㅋㅋㅋ 아무튼 기름진 음식으로 충전하시겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043600.1.21",
          "form": "네네 기름칠좀해주고 몸보신해여져",
          "original_form": "네네 기름칠좀해주고 몸보신해여져",
          "speaker_id": "1",
          "time": "20210907 19:55"
        }
      ]
    }
