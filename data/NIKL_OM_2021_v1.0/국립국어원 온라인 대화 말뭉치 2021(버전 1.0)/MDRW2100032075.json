      "utterance": [
        {
          "id": "MDRW2100032075.1.1",
          "form": "쉑쉑 버거가 보셨나요",
          "original_form": "쉑쉑 버거가 보셨나요",
          "speaker_id": "2",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032075.1.2",
          "form": "사람들이 엄청 많더라구요",
          "original_form": "사람들이 엄청 많더라구요",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032075.1.3",
          "form": "제가 서면까지 잘 안 나갑니다",
          "original_form": "제가 서면까지 잘 안 나갑니다",
          "speaker_id": "2",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032075.1.4",
          "form": "제주도에 있는지 모르겠네요",
          "original_form": "제주도에 있는지 모르겠네요",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032075.1.5",
          "form": "버거 하니 말인데 혹시 노브랜드버거 드셔 보셨나요",
          "original_form": "버거 하니 말인데 혹시 노브랜드버거 드셔 보셨나요",
          "speaker_id": "2",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032075.1.6",
          "form": "그렇게 인기 많으면 남포동에 있을 거 같기도 하고",
          "original_form": "그렇게 인기 많으면 남포동에 있을 거 같기도 하고",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032075.1.7",
          "form": "노브랜드버거 남포동 애는 생겼거든요",
          "original_form": "노브랜드버거 남포동 애는 생겼거든요",
          "speaker_id": "2",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032075.1.8",
          "form": "얘기만 들어 봤어요",
          "original_form": "얘기만 들어 봤어요",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100032075.1.9",
          "form": "가성비 냄새가 나네요",
          "original_form": "가성비 냄새가 나네요",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.10",
          "form": "제가 서울 쪽 가서 먹었을 때 굉장히 맛있었는데 부산 쪽에서는 그냥 그냥 이었어요",
          "original_form": "제가 서울 쪽 가서 먹었을 때 굉장히 맛있었는데 부산 쪽에서는 그냥 그냥 이었어요",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.11",
          "form": "인기는 엄청 많았어요",
          "original_form": "인기는 엄청 많았어요",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.12",
          "form": "사람들이 바글바글 했거든요",
          "original_form": "사람들이 바글바글 했거든요",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.13",
          "form": "체인점 임현 차이가 없어야 할 텐데",
          "original_form": "체인점 임현 차이가 없어야 할 텐데",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.14",
          "form": "먹어 봐야겠네요",
          "original_form": "먹어 봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.15",
          "form": "맛이 뭐랄까 버거킹에서 그 불맛을 뺀 그런 느낌입니다 저는 그 불 맛이 별로 좋아하지 않아서 노브랜드버거가 더 제 취향입니다",
          "original_form": "맛이 뭐랄까 버거킹에서 그 불맛을 뺀 그런 느낌입니다 저는 그 불 맛이 별로 좋아하지 않아서 노브랜드버거가 더 제 취향입니다",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.16",
          "form": "버거킹 만큼의 퀄리티면 인기 많겠네요",
          "original_form": "버거킹 만큼의 퀄리티면 인기 많겠네요",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.17",
          "form": "저는 맛있게 먹었어요",
          "original_form": "저는 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.18",
          "form": "저는 치킨 법을 좋아해요",
          "original_form": "저는 치킨 법을 좋아해요",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.19",
          "form": "저희 고모는 별로 맛이 없었대요",
          "original_form": "저희 고모는 별로 맛이 없었대요",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100032075.1.20",
          "form": "치킨 버거",
          "original_form": "치킨 버거",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032075.1.21",
          "form": "전 예전부터 치킨까스 먹는 것도 좋아했어요",
          "original_form": "전 예전부터 치킨까스 먹는 것도 좋아했어요",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032075.1.22",
          "form": "치느님은 진리입니다",
          "original_form": "치느님은 진리입니다",
          "speaker_id": "2",
          "time": "20210905 12:08"
        }
      ]
