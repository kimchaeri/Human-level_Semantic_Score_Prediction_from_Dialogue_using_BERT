      "utterance": [
        {
          "id": "MDRW2100032411.1.1",
          "form": "여름도 좋아하시나요",
          "original_form": "여름도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032411.1.2",
          "form": "여름은 제가 더위에 약해서 별로에요",
          "original_form": "여름은 제가 더위에 약해서 별로에요",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032411.1.3",
          "form": "별로 덥지 않았어요",
          "original_form": "별로 덥지 않았어요",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032411.1.4",
          "form": "하지만 비가 오질 않았지요",
          "original_form": "하지만 비가 오질 않았지요",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032411.1.5",
          "form": "가을에 비가 많이 오나 봅니다",
          "original_form": "가을에 비가 많이 오나 봅니다",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032411.1.6",
          "form": "한 해의 강수량이 정해져있는걸지도 모르겠어요",
          "original_form": "한 해의 강수량이 정해져있는걸지도 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032411.1.7",
          "form": "여름이 가서 아쉬워요",
          "original_form": "여름이 가서 아쉬워요",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032411.1.8",
          "form": "여름은 여름만의 활기가 있지요",
          "original_form": "여름은 여름만의 활기가 있지요",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032411.1.9",
          "form": "전 꽃을 좋아해서 여름이 좋아요",
          "original_form": "전 꽃을 좋아해서 여름이 좋아요",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032411.1.10",
          "form": "저는 땀을 많이 흘려서 여름이 싫어요",
          "original_form": "저는 땀을 많이 흘려서 여름이 싫어요",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032411.1.11",
          "form": "여름가면 한 해가 다가는 느낌이에요",
          "original_form": "여름가면 한 해가 다가는 느낌이에요",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032411.1.12",
          "form": "남은 시간이라도 보람차게 보내야지요",
          "original_form": "남은 시간이라도 보람차게 보내야지요",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032411.1.13",
          "form": "음식은 구하는게 없나요",
          "original_form": "음식은 구하는게 없나요",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032411.1.14",
          "form": "여름엔 냉면을 먹어야지요",
          "original_form": "여름엔 냉면을 먹어야지요",
          "speaker_id": "1",
          "time": "20210905 17:08"
        }
