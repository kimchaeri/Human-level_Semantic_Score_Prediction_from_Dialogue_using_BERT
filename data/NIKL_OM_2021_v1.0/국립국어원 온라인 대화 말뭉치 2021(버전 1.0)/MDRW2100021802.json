      "utterance": [
        {
          "id": "MDRW2100021802.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021802.1.2",
          "form": "여행가면 너무 즐겁죠",
          "original_form": "여행가면 너무 즐겁죠",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021802.1.3",
          "form": "임신과 출산",
          "original_form": "임신과 출산",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021802.1.4",
          "form": "그리고 육아하고나니",
          "original_form": "그리고 육아하고나니",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021802.1.5",
          "form": "코로나네요ㅠㅠ",
          "original_form": "코로나네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021802.1.6",
          "form": "코로나 끝이없을거 같은데.. 너무 여행가고싶어요!",
          "original_form": "코로나 끝이없을거 같은데.. 너무 여행가고싶어요!",
          "speaker_id": "1",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100021802.1.7",
          "form": "아이구ㅠ 어디가보셨나요",
          "original_form": "아이구ㅠ 어디가보셨나요",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100021802.1.8",
          "form": "코로나 직전에 마지막으로 갔던곳이",
          "original_form": "코로나 직전에 마지막으로 갔던곳이",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100021802.1.9",
          "form": "경주였네요^^;",
          "original_form": "경주였네요^^;",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100021802.1.10",
          "form": "국내여행지 중 재밌었던곳 있으신가요~ㅎ",
          "original_form": "국내여행지 중 재밌었던곳 있으신가요~ㅎ",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100021802.1.11",
          "form": "역시 제주도요!",
          "original_form": "역시 제주도요!",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100021802.1.12",
          "form": "그쵸 저도 제주도 좋았는데  또가고싶네요",
          "original_form": "그쵸 저도 제주도 좋았는데  또가고싶네요",
          "speaker_id": "1",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100021802.1.13",
          "form": "섭지코지 너무좋았어요",
          "original_form": "섭지코지 너무좋았어요",
          "speaker_id": "2",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100021802.1.14",
          "form": "제주도가면 한번쯤 다들 가는 관광 명소죠 ㅎㅎㅎ",
          "original_form": "제주도가면 한번쯤 다들 가는 관광 명소죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100021802.1.15",
          "form": "또 재밌으셨던곳 있나요 국내여행지중?",
          "original_form": "또 재밌으셨던곳 있나요 국내여행지중?",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100021802.1.16",
          "form": "음 부산이요!",
          "original_form": "음 부산이요!",
          "speaker_id": "2",
          "time": "20210829 13:40"
        },
        {
          "id": "MDRW2100021802.1.17",
          "form": "오~ 저는 부산은 해운대 다녀와보고 뮤지컬보러등등  3번정도 다녀왔네요 ㅋㅋ",
          "original_form": "오~ 저는 부산은 해운대 다녀와보고 뮤지컬보러등등  3번정도 다녀왔네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100021802.1.18",
          "form": "해운대는 국룰이죠ㅎㅎ",
          "original_form": "해운대는 국룰이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100021802.1.19",
          "form": "ㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100021802.1.20",
          "form": "전 코로나 잠잠해지면",
          "original_form": "전 코로나 잠잠해지면",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100021802.1.21",
          "form": "제주도 여행 계획이나 세워봐야겠어요!!",
          "original_form": "제주도 여행 계획이나 세워봐야겠어요!!",
