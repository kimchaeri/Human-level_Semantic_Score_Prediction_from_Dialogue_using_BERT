      "utterance": [
        {
          "id": "MDRW2100017986.1.1",
          "form": "건강에 신경쓰시는 편이신가요?",
          "original_form": "건강에 신경쓰시는 편이신가요?",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017986.1.2",
          "form": "신경쓰려고 노력하는 편이에요!",
          "original_form": "신경쓰려고 노력하는 편이에요!",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017986.1.3",
          "form": "영양제 혹시 챙겨드세요?",
          "original_form": "영양제 혹시 챙겨드세요?",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017986.1.4",
          "form": "요즘에 유산균이랑 msm은 먹고있어요!",
          "original_form": "요즘에 유산균이랑 msm은 먹고있어요!",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017986.1.5",
          "form": "어떤거 챙겨드세요?",
          "original_form": "어떤거 챙겨드세요?",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017986.1.6",
          "form": "전 비타민 BD 칼슘마그네슘이요!",
          "original_form": "전 비타민 BD 칼슘마그네슘이요!",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017986.1.7",
          "form": "msm은 어떤 영양제인가요?",
          "original_form": "msm은 어떤 영양제인가요?",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017986.1.8",
          "form": "관절에 도움이 된다고 알고있어요!",
          "original_form": "관절에 도움이 된다고 알고있어요!",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017986.1.9",
          "form": "아 홍합추출물인가요?",
          "original_form": "아 홍합추출물인가요?",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017986.1.10",
          "form": "앗 그건 모르겠어요ㅠ",
          "original_form": "앗 그건 모르겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017986.1.11",
          "form": "식물성이라고 해서 샀었어요",
          "original_form": "식물성이라고 해서 샀었어요",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017986.1.12",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017986.1.13",
          "form": "요즘 눈을 많이 쓰다보니까",
          "original_form": "요즘 눈을 많이 쓰다보니까",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017986.1.14",
          "form": "루테인을 먹어야하나 고민인데",
          "original_form": "루테인을 먹어야하나 고민인데",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017986.1.15",
          "form": "효과가 있는지 잘 모르겠어요",
          "original_form": "효과가 있는지 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017986.1.16",
          "form": "눈건강 챙기기 너무 어려워요 ㅠ",
          "original_form": "눈건강 챙기기 너무 어려워요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017986.1.17",
          "form": "맞아요ㅠ 유독 눈영양제는 잘 못챙기게되네요",
          "original_form": "맞아요ㅠ 유독 눈영양제는 잘 못챙기게되네요",
          "speaker_id": "1",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100017986.1.18",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017986.1.19",
          "form": "혹시 라식하셨나요?",
          "original_form": "혹시 라식하셨나요?",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017986.1.20",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017986.1.21",
          "form": "와 어떠세요??",
          "original_form": "와 어떠세요??",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017986.1.22",
          "form": "저도 지금 고민중이에요",
          "original_form": "저도 지금 고민중이에요",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017986.1.23",
          "form": "저는 빛번짐이랑 건조가 좀 안맞았는데 친구는 괜찮다더라구요",
          "original_form": "저는 빛번짐이랑 건조가 좀 안맞았는데 친구는 괜찮다더라구요",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017986.1.24",
          "form": "아침 안경 없이 일어나는건 최고에요",
          "original_form": "아침 안경 없이 일어나는건 최고에요",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017986.1.25",
          "form": "저도 그게 제일 부러워요 ㅋㅋㅋㅋ",
          "original_form": "저도 그게 제일 부러워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017986.1.26",
          "form": "그건 진짜 최고에요..!ㅋㅋ",
          "original_form": "그건 진짜 최고에요..!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017986.1.27",
          "form": "시력이 좋으면 이런 고민 안해도 될텐데",
          "original_form": "시력이 좋으면 이런 고민 안해도 될텐데",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017986.1.28",
          "form": "정말 갈수록 건강이 최고라는 어른들 말이 공감가요 ㅋㅋㅋㅋㅋ",
          "original_form": "정말 갈수록 건강이 최고라는 어른들 말이 공감가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017986.1.29",
          "form": "맞아요ㅋㅋ 어릴때 왜 눈이 나쁜걸 내버려둔건지.. 싶어요",
          "original_form": "맞아요ㅋㅋ 어릴때 왜 눈이 나쁜걸 내버려둔건지.. 싶어요",
