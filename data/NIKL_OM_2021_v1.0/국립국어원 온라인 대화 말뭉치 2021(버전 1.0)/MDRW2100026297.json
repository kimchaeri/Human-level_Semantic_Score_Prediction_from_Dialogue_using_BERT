      "utterance": [
        {
          "id": "MDRW2100026297.1.1",
          "form": "오 군대",
          "original_form": "오 군대",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026297.1.2",
          "form": "안가봐서",
          "original_form": "안가봐서",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026297.1.3",
          "form": "다냐오셨나요",
          "original_form": "다냐오셨나요",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026297.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026297.1.5",
          "form": "할아버지가 장교셔서",
          "original_form": "할아버지가 장교셔서",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026297.1.6",
          "form": "저는 좋은 이미지가 있어요 군대에 대해",
          "original_form": "저는 좋은 이미지가 있어요 군대에 대해",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026297.1.7",
          "form": "그럼 별을 다신 분이셨나요?",
          "original_form": "그럼 별을 다신 분이셨나요?",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026297.1.8",
          "form": "아뇨 그냥 북한에 보내는 군인 장교셨대요",
          "original_form": "아뇨 그냥 북한에 보내는 군인 장교셨대요",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026297.1.9",
          "form": "무슨 무서운 조직인데 잘 몰라요",
          "original_form": "무슨 무서운 조직인데 잘 몰라요",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026297.1.10",
          "form": "북한에 사람을 보내셨나요?",
          "original_form": "북한에 사람을 보내셨나요?",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026297.1.11",
          "form": "네에 베트남전도 참전하셨구요",
          "original_form": "네에 베트남전도 참전하셨구요",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026297.1.12",
          "form": "굉장히 규칙적인 생활을 하셨어요",
          "original_form": "굉장히 규칙적인 생활을 하셨어요",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026297.1.13",
          "form": "돌아가실때까지",
          "original_form": "돌아가실때까지",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026297.1.14",
          "form": "와 그정도면 엄청 높으신 분이셨나봐요",
          "original_form": "와 그정도면 엄청 높으신 분이셨나봐요",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026297.1.15",
          "form": "군대가 규칙적이고 딱딱한 분위기긴하죠",
          "original_form": "군대가 규칙적이고 딱딱한 분위기긴하죠",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026297.1.16",
          "form": "네에 전 그런 분위기는 싫은데",
          "original_form": "네에 전 그런 분위기는 싫은데",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026297.1.17",
          "form": "다만 할아버지를 좋아해서 ㅎㅎ",
          "original_form": "다만 할아버지를 좋아해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026297.1.18",
          "form": "군인이시지만 따듯하신 분이셨나봐요",
          "original_form": "군인이시지만 따듯하신 분이셨나봐요",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026297.1.19",
          "form": "네 하지만 군대이야기는 거의 안하셨고 조용하셨어요",
          "original_form": "네 하지만 군대이야기는 거의 안하셨고 조용하셨어요",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026297.1.20",
          "form": "아마 좋은 기억이 많지 않으썼던 듯 ㅠ",
          "original_form": "아마 좋은 기억이 많지 않으썼던 듯 ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026297.1.21",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026297.1.22",
          "form": "군대가 아무래도 폐쇄적인 공간이기도 하니까요",
          "original_form": "군대가 아무래도 폐쇄적인 공간이기도 하니까요",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026297.1.23",
          "form": "남자분이신가요",
          "original_form": "남자분이신가요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026297.1.24",
          "form": "군대 어떤 어떠세요",
          "original_form": "군대 어떤 어떠세요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026297.1.25",
          "form": "아니요 여자예요ㅋㅋㅋㅋㅋ",
          "original_form": "아니요 여자예요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026297.1.26",
          "form": "동반입대가 좋다던데 의지가되서",
          "original_form": "동반입대가 좋다던데 의지가되서",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026297.1.27",
          "form": "아하 그러시군요",
          "original_form": "아하 그러시군요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        }
