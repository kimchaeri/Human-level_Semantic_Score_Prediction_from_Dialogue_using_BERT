      "utterance": [
        {
          "id": "MDRW2100001016.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100001016.1.2",
          "form": "안녕하세요 요즘 구매하고 싶은물건이 어떤것인 가요~?",
          "original_form": "안녕하세요 요즘 구매하고 싶은물건이 어떤것인 가요~?",
          "speaker_id": "2",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001016.1.3",
          "form": "플립핸드폰이 갖고픈데ㅎㅎ 비싸서 못사고 있어용ㅠ",
          "original_form": "플립핸드폰이 갖고픈데ㅎㅎ 비싸서 못사고 있어용ㅠ",
          "speaker_id": "1",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001016.1.4",
          "form": "저도 플립3  보고있는데 가격이 비싸네요",
          "original_form": "저도 플립3  보고있는데 가격이 비싸네요",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001016.1.5",
          "form": "아직 액정도 못믿겠구",
          "original_form": "아직 액정도 못믿겠구",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001016.1.6",
          "form": "아 액정이?접히는부분요?ㅠ",
          "original_form": "아 액정이?접히는부분요?ㅠ",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001016.1.7",
          "form": "네 그부분이 완전히 좋을거란 생각은 아직안들어서요",
          "original_form": "네 그부분이 완전히 좋을거란 생각은 아직안들어서요",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001016.1.8",
          "form": "어떤색상을 생각하시나요~?",
          "original_form": "어떤색상을 생각하시나요~?",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001016.1.9",
          "form": "아 그것도그렇네요ㅠ저도 자주폈다접었다하니까요ㅠ",
          "original_form": "아 그것도그렇네요ㅠ저도 자주폈다접었다하니까요ㅠ",
          "speaker_id": "1",
          "time": "20210820 11:57"
        },
        {
          "id": "MDRW2100001016.1.10",
          "form": "라벤다는튀고 크림?",
          "original_form": "라벤다는튀고 크림?",
          "speaker_id": "1",
          "time": "20210820 11:57"
        },
        {
          "id": "MDRW2100001016.1.11",
          "form": "크림이 질리지 않을거같아요",
          "original_form": "크림이 질리지 않을거같아요",
          "speaker_id": "2",
          "time": "20210820 11:58"
        },
        {
          "id": "MDRW2100001016.1.12",
          "form": "저도 동의해요",
          "original_form": "저도 동의해요",
          "speaker_id": "2",
          "time": "20210820 11:58"
        },
        {
          "id": "MDRW2100001016.1.13",
          "form": "맞아요ㅡ2년이나 써야하니ㅠ 너무튀는거는 금방 질릴것같아요",
          "original_form": "맞아요ㅡ2년이나 써야하니ㅠ 너무튀는거는 금방 질릴것같아요",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100001016.1.14",
          "form": "2년약정으로 사실려나봐요",
          "original_form": "2년약정으로 사실려나봐요",
          "speaker_id": "2",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100001016.1.15",
          "form": "네ㅠ근데 요즘은 약정할인이 비싸긴하더라구요ㅠ",
          "original_form": "네ㅠ근데 요즘은 약정할인이 비싸긴하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100001016.1.16",
          "form": "자급제는 102만이면 사더라구요",
          "original_form": "자급제는 102만이면 사더라구요",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100001016.1.17",
          "form": "나쁘지 않은것 같아요",
          "original_form": "나쁘지 않은것 같아요",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100001016.1.18",
          "form": "그쵸ㅡ 공시나 자급제사는게 더 싸게 치이더라구요",
          "original_form": "그쵸ㅡ 공시나 자급제사는게 더 싸게 치이더라구요",
          "speaker_id": "1",
          "time": "20210820 12:46"
