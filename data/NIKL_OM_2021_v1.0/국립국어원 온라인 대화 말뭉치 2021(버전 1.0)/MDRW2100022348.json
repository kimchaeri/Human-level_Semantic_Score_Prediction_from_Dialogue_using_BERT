      "utterance": [
        {
          "id": "MDRW2100022348.1.1",
          "form": "저 9월 9일날 맞아요!",
          "original_form": "저 9월 9일날 맞아요!",
          "speaker_id": "2",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100022348.1.2",
          "form": "오 저도 9.9예약인데ㅎㅎ",
          "original_form": "오 저도 9.9예약인데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022348.1.3",
          "form": "오 대박이네요",
          "original_form": "오 대박이네요",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022348.1.4",
          "form": "조금 걱정은 안되세요?",
          "original_form": "조금 걱정은 안되세요?",
          "speaker_id": "1",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022348.1.5",
          "form": "걱정은 되긴하는데",
          "original_form": "걱정은 되긴하는데",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022348.1.6",
          "form": "그냥 백신보다 조금 아프다? 라고 생각하고 있어서",
          "original_form": "그냥 백신보다 조금 아프다? 라고 생각하고 있어서",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022348.1.7",
          "form": "큰 걱정은 안하구있네요",
          "original_form": "큰 걱정은 안하구있네요",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022348.1.8",
          "form": "모더나인지 화이자인지 모르겠네요",
          "original_form": "모더나인지 화이자인지 모르겠네요",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022348.1.9",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022348.1.10",
          "form": "듣기론 모더나라고 하는거같긴하는데..",
          "original_form": "듣기론 모더나라고 하는거같긴하는데..",
          "speaker_id": "2",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022348.1.11",
          "form": "저는 조금 걱정되네요 백신ㅠ",
          "original_form": "저는 조금 걱정되네요 백신ㅠ",
          "speaker_id": "1",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100022348.1.12",
          "form": "ㅠㅠ 그렇죠",
          "original_form": "ㅠㅠ 그렇죠",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100022348.1.13",
          "form": "아무래도 주변에 맞아서 몇일간 아팠다고 한 친구도 있고 그러니",
          "original_form": "아무래도 주변에 맞아서 몇일간 아팠다고 한 친구도 있고 그러니",
          "speaker_id": "2",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100022348.1.14",
          "form": "걱정은 좀 되긴하네요",
          "original_form": "걱정은 좀 되긴하네요",
          "speaker_id": "2",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100022348.1.15",
          "form": "2차가 부작용크다고 들었어요",
          "original_form": "2차가 부작용크다고 들었어요",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100022348.1.16",
          "form": "아 맞아요 ㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022348.1.17",
          "form": "죽은사람도 몇 있고ㅠ",
          "original_form": "죽은사람도 몇 있고ㅠ",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022348.1.18",
          "form": "백신자체로 죽는 사람들은 의외로 좀 있어서",
          "original_form": "백신자체로 죽는 사람들은 의외로 좀 있어서",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022348.1.19",
          "form": "그렇게 놀라진않는데 아픈사람들이 많으니 ㅠㅠ",
          "original_form": "그렇게 놀라진않는데 아픈사람들이 많으니 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:51"
        }
      ]
    }
