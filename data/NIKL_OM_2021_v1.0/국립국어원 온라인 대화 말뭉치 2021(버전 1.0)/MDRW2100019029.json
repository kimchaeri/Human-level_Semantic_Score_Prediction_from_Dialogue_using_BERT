      "utterance": [
        {
          "id": "MDRW2100019029.1.1",
          "form": "지역센터 근로",
          "original_form": "지역센터 근로",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019029.1.2",
          "form": "점심부터 가능?",
          "original_form": "점심부터 가능?",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019029.1.3",
          "form": "여기는1시부터",
          "original_form": "여기는1시부터",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019029.1.4",
          "form": "7시까지",
          "original_form": "7시까지",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019029.1.5",
          "form": "시간대가 애매하구만",
          "original_form": "시간대가 애매하구만",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019029.1.6",
          "form": "이번에여기도 교외근로지",
          "original_form": "이번에여기도 교외근로지",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019029.1.7",
          "form": "제외라서",
          "original_form": "제외라서",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019029.1.8",
          "form": "보니까 사복다니는 후배",
          "original_form": "보니까 사복다니는 후배",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019029.1.9",
          "form": "는",
          "original_form": "는",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019029.1.10",
          "form": "교외근로되는지",
          "original_form": "교외근로되는지",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019029.1.11",
          "form": "센터랑학교 협의중이라던데",
          "original_form": "센터랑학교 협의중이라던데",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019029.1.12",
          "form": "그냥 다니면",
          "original_form": "그냥 다니면",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019029.1.13",
          "form": "시급 얼마길래",
          "original_form": "시급 얼마길래",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019029.1.14",
          "form": "확실히 교외근로 인정되면 좋지",
          "original_form": "확실히 교외근로 인정되면 좋지",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019029.1.15",
          "form": "11150",
          "original_form": "11150",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019029.1.16",
          "form": "교외근로랑같음",
          "original_form": "교외근로랑같음",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019029.1.17",
          "form": "시급은",
          "original_form": "시급은",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019029.1.18",
          "form": "그냥 교외근로 짤려서",
          "original_form": "그냥 교외근로 짤려서",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019029.1.19",
          "form": "추가로 근로지구하는거임",
          "original_form": "추가로 근로지구하는거임",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019029.1.20",
          "form": "아 개인적으로 구하는거임?",
          "original_form": "아 개인적으로 구하는거임?",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019029.1.21",
          "form": "ㅇㅇㅇㄴ",
          "original_form": "ㅇㅇㅇㄴ",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019029.1.22",
          "form": "근로할곳없다고",
          "original_form": "근로할곳없다고",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019029.1.23",
          "form": "사람도 부족하고하니까",
          "original_form": "사람도 부족하고하니까",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019029.1.24",
          "form": "센터에서도 교외근로떨어진사람 받고싶다고는 하는데",
          "original_form": "센터에서도 교외근로떨어진사람 받고싶다고는 하는데",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019029.1.25",
          "form": "학교에서는 이번에 교외근로지 다줄여서",
          "original_form": "학교에서는 이번에 교외근로지 다줄여서",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019029.1.26",
          "form": "장학처랑 이야기하는중인걸로암",
          "original_form": "장학처랑 이야기하는중인걸로암",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019029.1.27",
          "form": "일 강도는 어떰",
          "original_form": "일 강도는 어떰",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019029.1.28",
          "form": "애들만 가르치는건가",
          "original_form": "애들만 가르치는건가",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019029.1.29",
          "form": "5시간동안",
          "original_form": "5시간동안",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019029.1.30",
          "form": "일강도 크게 뭐없음",
          "original_form": "일강도 크게 뭐없음",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019029.1.31",
          "form": "간식 2시간마다 주고",
          "original_form": "간식 2시간마다 주고",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019029.1.32",
          "form": "애들 가르치는것도아니고애들",
          "original_form": "애들 가르치는것도아니고애들",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019029.1.33",
          "form": "문제집풀면",
          "original_form": "문제집풀면",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019029.1.34",
          "form": "모르는거 풀이보고 설명해주면됨",
          "original_form": "모르는거 풀이보고 설명해주면됨",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019029.1.35",
          "form": "애들 몇살인데?",
          "original_form": "애들 몇살인데?",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019029.1.36",
          "form": "초1~6",
          "original_form": "초1~6",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019029.1.37",
          "form": "근로생 많은거보니까",
          "original_form": "근로생 많은거보니까",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019029.1.38",
          "form": "애들도 엄청많나보네",
          "original_form": "애들도 엄청많나보네",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019029.1.39",
          "form": "애들 25명정도",
          "original_form": "애들 25명정도",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019029.1.40",
