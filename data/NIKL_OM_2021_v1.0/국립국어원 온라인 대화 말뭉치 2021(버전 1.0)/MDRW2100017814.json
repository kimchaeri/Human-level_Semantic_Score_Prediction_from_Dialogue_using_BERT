      "utterance": [
        {
          "id": "MDRW2100017814.1.1",
          "form": "비 오는데 고생했어",
          "original_form": "비 오는데 고생했어",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100017814.1.2",
          "form": "주말에 날씨가  맑음이래~ 얏호",
          "original_form": "주말에 날씨가  맑음이래~ 얏호",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100017814.1.3",
          "form": "빨래랑 청소해야겠네 밀린 일 좀 해",
          "original_form": "빨래랑 청소해야겠네 밀린 일 좀 해",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100017814.1.4",
          "form": "응 그래야지 모처럼 좋은 날씨인데",
          "original_form": "응 그래야지 모처럼 좋은 날씨인데",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100017814.1.5",
          "form": "나 혼나는 것? 알았어 열심히 할게 비 안오니까",
          "original_form": "나 혼나는 것? 알았어 열심히 할게 비 안오니까",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100017814.1.6",
          "form": "미세먼지 예보도 잘 챙겨보고",
          "original_form": "미세먼지 예보도 잘 챙겨보고",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100017814.1.7",
          "form": "당연하지 일기예보 미세먼지 예보 챙기는 건 필수야",
          "original_form": "당연하지 일기예보 미세먼지 예보 챙기는 건 필수야",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100017814.1.8",
          "form": "날씨 추우니까 이불 꺼내자",
          "original_form": "날씨 추우니까 이불 꺼내자",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100017814.1.9",
          "form": "언제? 오늘 당장?? 하긴 새벽에 온도가 마니 내려갔어",
          "original_form": "언제? 오늘 당장?? 하긴 새벽에 온도가 마니 내려갔어",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100017814.1.10",
          "form": "이제 9월이면 가을이지 옷도 바꿔야지 말이야",
          "original_form": "이제 9월이면 가을이지 옷도 바꿔야지 말이야",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100017814.1.11",
          "form": "난 가디건 입고 다니는데 ㅋㅋ",
          "original_form": "난 가디건 입고 다니는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100017814.1.12",
          "form": "추위 잘 타면서...가을이니까 봄가을용으로 바꿔",
          "original_form": "추위 잘 타면서...가을이니까 봄가을용으로 바꿔",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100017814.1.13",
          "form": "아 걱정해줘서 고마워ㅎㅎ 가을에 맞게 입을게",
          "original_form": "아 걱정해줘서 고마워ㅎㅎ 가을에 맞게 입을게",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100017814.1.14",
          "form": "오케이 와 다행히 내일 비 없네ㅋㅋ 청소하기 딱 좋아",
          "original_form": "오케이 와 다행히 내일 비 없네ㅋㅋ 청소하기 딱 좋아",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100017814.1.15",
          "form": "진짜네 일기예보보니까 난 주말만 봤거든",
          "original_form": "진짜네 일기예보보니까 난 주말만 봤거든",
