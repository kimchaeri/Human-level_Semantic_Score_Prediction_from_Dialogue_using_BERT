      "utterance": [
        {
          "id": "MDRW2100008961.1.1",
          "form": "오늘 뭐드셨어요?",
          "original_form": "오늘 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210822 19:59"
        },
        {
          "id": "MDRW2100008961.1.2",
          "form": "오늘 달걀 하나 치킨너겟 3개 먹었어요",
          "original_form": "오늘 달걀 하나 치킨너겟 3개 먹었어요",
          "speaker_id": "2",
          "time": "20210822 20:01"
        },
        {
          "id": "MDRW2100008961.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 20:02"
        },
        {
          "id": "MDRW2100008961.1.4",
          "form": "설빙에서 팥 빙수, 카페라떼 먹고",
          "original_form": "설빙에서 팥 빙수, 카페라떼 먹고",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100008961.1.5",
          "form": "찹쌀도너츠 참깨도너츠 먹고",
          "original_form": "찹쌀도너츠 참깨도너츠 먹고",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100008961.1.6",
          "form": "예감 과자먹고",
          "original_form": "예감 과자먹고",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100008961.1.7",
          "form": "집에서 자장면해먹고",
          "original_form": "집에서 자장면해먹고",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100008961.1.8",
          "form": "수박도먹었어오",
          "original_form": "수박도먹었어오",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100008961.1.9",
          "form": "오 집에서 맛있는거 많이 드셧네요",
          "original_form": "오 집에서 맛있는거 많이 드셧네요",
          "speaker_id": "2",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100008961.1.10",
          "form": "저도 어제 과일 먹고싶었는데 과일은 자르기가 왜이렇게 귀찮을까요~~",
          "original_form": "저도 어제 과일 먹고싶었는데 과일은 자르기가 왜이렇게 귀찮을까요~~",
          "speaker_id": "2",
          "time": "20210823 10:15"
        },
        {
          "id": "MDRW2100008961.1.11",
          "form": "특히 수박이요!ㅋㅋ",
          "original_form": "특히 수박이요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:15"
        },
        {
          "id": "MDRW2100008961.1.12",
          "form": "맞아요ㅋㅋ 시댁에서 잘라주셔서 먹었어요",
          "original_form": "맞아요ㅋㅋ 시댁에서 잘라주셔서 먹었어요",
          "speaker_id": "1",
          "time": "20210824 08:05"
        },
        {
          "id": "MDRW2100008961.1.13",
          "form": "오 좋은 시댁이시네요!!ㅎㅎㅎㄹ",
          "original_form": "오 좋은 시댁이시네요!!ㅎㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210824 11:49"
        },
        {
          "id": "MDRW2100008961.1.14",
          "form": "요리도 자주 하시는 편이신가요??",
          "original_form": "요리도 자주 하시는 편이신가요??",
          "speaker_id": "2",
          "time": "20210824 11:49"
        },
        {
          "id": "MDRW2100008961.1.15",
          "form": "요리 자주 했었는데\r\n애기낳고 복직하고나서는 못했어요ㅠ",
          "original_form": "요리 자주 했었는데\r\n애기낳고 복직하고나서는 못했어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100008961.1.16",
          "form": "두달이나됬는데",
          "original_form": "두달이나됬는데",
          "speaker_id": "1",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100008961.1.17",
          "form": "다음주부터 다이어트할거라서 요리하려고요..ㅎㅎ",
          "original_form": "다음주부터 다이어트할거라서 요리하려고요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100008961.1.18",
          "form": "애기가 아직 이유식을 먹어서 자유롭네요",
          "original_form": "애기가 아직 이유식을 먹어서 자유롭네요",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100008961.1.19",
          "form": "아 복직이 두달 되었다는 말씀이신가요? 다이어트까지 하시다니 대단하세요!!",
          "original_form": "아 복직이 두달 되었다는 말씀이신가요? 다이어트까지 하시다니 대단하세요!!",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100008961.1.20",
          "form": "이유식듀 만들어서 주시나요?",
          "original_form": "이유식듀 만들어서 주시나요?",
          "speaker_id": "2",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100008961.1.21",
          "form": "아니요ㅋㅋㅋ 사서줘요",
          "original_form": "아니요ㅋㅋㅋ 사서줘요",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100008961.1.22",
          "form": "꽝손입니다!",
          "original_form": "꽝손입니다!",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100008961.1.23",
          "form": "애기가 워낙 안먹는애기라",
          "original_form": "애기가 워낙 안먹는애기라",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100008961.1.24",
          "form": "산골이유식이랑 푸드케어만먹고",
          "original_form": "산골이유식이랑 푸드케어만먹고",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100008961.1.25",
          "form": "본죽베이비도 끌릴때 가끔먹고",
          "original_form": "본죽베이비도 끌릴때 가끔먹고",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100008961.1.26",
          "form": "앨빈즈에서 한우듬뿍 런칭해서 그것도 먹어요",
          "original_form": "앨빈즈에서 한우듬뿍 런칭해서 그것도 먹어요",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100008961.1.27",
          "form": "한우가 15퍼센트 이상이예요",
          "original_form": "한우가 15퍼센트 이상이예요",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100008961.1.28",
          "form": "우와 이유식 종류가 엄청 많네요!!",
          "original_form": "우와 이유식 종류가 엄청 많네요!!",
          "speaker_id": "2",
          "time": "20210825 06:40"
        },
        {
          "id": "MDRW2100008961.1.29",
          "form": "한우듬뿍 뭔가 이름도 한우가 많이 들어있을거 같은 느낌이에요 ㅎㅎ",
          "original_form": "한우듬뿍 뭔가 이름도 한우가 많이 들어있을거 같은 느낌이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 06:41"
        },
        {
          "id": "MDRW2100008961.1.30",
          "form": "저도 나중에 애기를 낳게 된다면 사서 줄거같아요..ㅎㅎㅎ",
          "original_form": "저도 나중에 애기를 낳게 된다면 사서 줄거같아요..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 06:42"
        },
        {
          "id": "MDRW2100008961.1.31",
          "form": "저는 아직 생각없고 애기가 원하면 그때 다시 생각해볼거같아요",
          "original_form": "저는 아직 생각없고 애기가 원하면 그때 다시 생각해볼거같아요",
          "speaker_id": "1",
          "time": "20210825 07:57"
        },
        {
          "id": "MDRW2100008961.1.32",
          "form": "애기가 이유식을 원한다면요?ㅎㅎ",
          "original_form": "애기가 이유식을 원한다면요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:11"
        },
        {
          "id": "MDRW2100008961.1.33",
          "form": "이유식은 보통 몇살까지 먹나요?",
          "original_form": "이유식은 보통 몇살까지 먹나요?",
          "speaker_id": "2",
          "time": "20210825 09:11"
        },
        {
          "id": "MDRW2100008961.1.34",
          "form": "12개월~13개월먹고\r\n유아식으로 먹어요!",
          "original_form": "12개월~13개월먹고\r\n유아식으로 먹어요!",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100008961.1.35",
          "form": "오 저는 아직 아이가 없어서 그런지 그런걸 잘 몰라서 나중에 유아식 이유식 분류도 잘 해줘서 줘야겠네요 ㅎㅎ",
          "original_form": "오 저는 아직 아이가 없어서 그런지 그런걸 잘 몰라서 나중에 유아식 이유식 분류도 잘 해줘서 줘야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100008961.1.36",
          "form": "언제 가지실거예요!!?",
          "original_form": "언제 가지실거예요!!?",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100008961.1.37",
          "form": "아직 미혼이라 잘은 모르겠지만 언젠가는 가지고 싶어요!ㅎㅎ",
          "original_form": "아직 미혼이라 잘은 모르겠지만 언젠가는 가지고 싶어요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100008961.1.38",
          "form": "뭔가 맛있는 음식도 챙겨주고 영양도 잘 챙겨주는 그런 엄마가 되고싶은데.. 잘 할 수 있을지 조금 걱정되기는해요 ㅎㅎ아직 결혼도 안하긴 했지만요 ㅎㅎ",
          "original_form": "뭔가 맛있는 음식도 챙겨주고 영양도 잘 챙겨주는 그런 엄마가 되고싶은데.. 잘 할 수 있을지 조금 걱정되기는해요 ㅎㅎ아직 결혼도 안하긴 했지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100008961.1.39",
          "form": "워킹맘은 늘 미안하죠",
          "original_form": "워킹맘은 늘 미안하죠",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100008961.1.40",
          "form": "3살까지는 같이 있는게 정서상 좋은데",
          "original_form": "3살까지는 같이 있는게 정서상 좋은데",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100008961.1.41",
          "form": "아 그런가요?ㅠㅠ그말을 들으니 슬프네요",
          "original_form": "아 그런가요?ㅠㅠ그말을 들으니 슬프네요",
