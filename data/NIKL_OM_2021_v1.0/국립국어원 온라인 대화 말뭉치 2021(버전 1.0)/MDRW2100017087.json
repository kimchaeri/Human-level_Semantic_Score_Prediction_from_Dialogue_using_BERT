          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017087.1.2",
          "form": "사탄교가 싫어?",
          "original_form": "사탄교가 싫어?",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017087.1.3",
          "form": "아무래도",
          "original_form": "아무래도",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017087.1.4",
          "form": "최근 읽은 책이 뭔가요",
          "original_form": "최근 읽은 책이 뭔가요",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017087.1.5",
          "form": "공격적이네",
          "original_form": "공격적이네",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017087.1.6",
          "form": "문동균의한국사",
          "original_form": "문동균의한국사",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.8",
          "form": "그쪽은요",
          "original_form": "그쪽은요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.9",
          "form": "역겹네",
          "original_form": "역겹네",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.10",
          "form": "저는... 경제책인데",
          "original_form": "저는... 경제책인데",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.11",
          "form": "제가 사람도 책도",
          "original_form": "제가 사람도 책도",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.12",
          "form": "조선돌아가는꼬라지가 꽤 웃겨요",
          "original_form": "조선돌아가는꼬라지가 꽤 웃겨요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.13",
          "form": "이름을기억못해요",
          "original_form": "이름을기억못해요",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.14",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.15",
          "form": "조선은 진짜",
          "original_form": "조선은 진짜",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.16",
          "form": "돌아갔죠",
          "original_form": "돌아갔죠",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.17",
          "form": "내용은기억하죠?",
          "original_form": "내용은기억하죠?",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.18",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.19",
          "form": "안하면 엄마한테",
          "original_form": "안하면 엄마한테",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.20",
          "form": "죽죠",
          "original_form": "죽죠",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017087.1.21",
          "form": "어째서...",
          "original_form": "어째서...",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.22",
          "form": "맨날 경제관련 영상이랑",
          "original_form": "맨날 경제관련 영상이랑",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.23",
          "form": "엄마랑 독서모임햐요,??",
          "original_form": "엄마랑 독서모임햐요,??",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.24",
          "form": "재테크책 사진 와요",
          "original_form": "재테크책 사진 와요",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.25",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.26",
          "form": "안읽으면 죽는 독서모임",
          "original_form": "안읽으면 죽는 독서모임",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도 살아가는데 꽤 필요한 장르네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도 살아가는데 꽤 필요한 장르네요",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.29",
          "form": "아무래도 조선역사보단",
          "original_form": "아무래도 조선역사보단",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017087.1.30",
          "form": "꽤 필요하죠",
          "original_form": "꽤 필요하죠",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.31",
          "form": "아무래도...",
          "original_form": "아무래도...",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.32",
          "form": "빙글빙글",
          "original_form": "빙글빙글",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.33",
          "form": "그렇죠...",
          "original_form": "그렇죠...",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.34",
          "form": "중종이 말에서 떨어져서생긴 부상으로죽었어요",
          "original_form": "중종이 말에서 떨어져서생긴 부상으로죽었어요",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.35",
          "form": "달달하다",
          "original_form": "달달하다",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.36",
          "form": "낙마!",
          "original_form": "낙마!",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.37",
          "form": "이름이 왜 중종이죠",
          "original_form": "이름이 왜 중종이죠",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.38",
          "form": "중증같아요",
          "original_form": "중증같아요",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.39",
          "form": "중상!",
          "original_form": "중상!",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.40",
          "form": "비슷하지않나요",
          "original_form": "비슷하지않나요",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.41",
          "form": "그쵸....",
          "original_form": "그쵸....",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.42",
          "form": "조롱아냐?",
          "original_form": "조롱아냐?",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017087.1.43",
          "form": "사인으로 이름 지어버리네",
          "original_form": "사인으로 이름 지어버리네",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017087.1.44",
          "form": "기만맥스",
          "original_form": "기만맥스",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017087.1.45",
          "form": "그리고어제읽은책은",
          "original_form": "그리고어제읽은책은",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017087.1.46",
          "form": "이선재의 국어문법",
          "original_form": "이선재의 국어문법",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017087.1.47",
          "form": "역겨운말만 해",
          "original_form": "역겨운말만 해",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017087.1.48",
          "form": "어제 외운 사자성어 말해보세요",
          "original_form": "어제 외운 사자성어 말해보세요",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017087.1.49",
          "form": "가도벽립",
          "original_form": "가도벽립",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.50",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.51",
          "form": "그거말고요",
          "original_form": "그거말고요",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.52",
          "form": "매우가난함",
          "original_form": "매우가난함",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.53",
          "form": "킹받네",
          "original_form": "킹받네",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.54",
          "form": "절 저격하지 않는 걸로",
          "original_form": "절 저격하지 않는 걸로",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.55",
          "form": "말해주세요",
          "original_form": "말해주세요",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.56",
          "form": "흠....",
          "original_form": "흠....",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.57",
          "form": "왜 그런거만 외워",
          "original_form": "왜 그런거만 외워",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.58",
          "form": "규탄한다",
          "original_form": "규탄한다",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.59",
          "form": "각고면려",
          "original_form": "각고면려",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017087.1.60",
          "form": "엄청 애씀 부지런히 노력함",
          "original_form": "엄청 애씀 부지런히 노력함",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017087.1.61",
          "form": "진짜싫다",
          "original_form": "진짜싫다",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017087.1.62",
          "form": "간명범의",
          "original_form": "간명범의",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017087.1.63",
          "form": "그게 독서인가요?",
          "original_form": "그게 독서인가요?",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017087.1.64",
          "form": "배신함",
          "original_form": "배신함",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017087.1.65",
          "form": "자학같아요",
          "original_form": "자학같아요",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017087.1.66",
          "form": "자꾸 저격하지 마세요",
          "original_form": "자꾸 저격하지 마세요",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017087.1.67",
          "form": "어렵네요",
          "original_form": "어렵네요",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017087.1.68",
          "form": "조롱특화인간",
          "original_form": "조롱특화인간",
          "speaker_id": "2",
          "time": "20210826 17:26"
        }
      ]
