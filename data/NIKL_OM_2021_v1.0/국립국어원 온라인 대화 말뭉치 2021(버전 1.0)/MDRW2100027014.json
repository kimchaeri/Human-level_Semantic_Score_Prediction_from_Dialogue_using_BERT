        },
        {
          "id": "MDRW2100027014.1.2",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210902 07:53"
        },
        {
          "id": "MDRW2100027014.1.3",
          "form": "네 국내중에는 제주도가고싶네요!",
          "original_form": "네 국내중에는 제주도가고싶네요!",
          "speaker_id": "2",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100027014.1.4",
          "form": "저도 바쪽",
          "original_form": "저도 바쪽",
          "speaker_id": "1",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100027014.1.5",
          "form": "바다쫃",
          "original_form": "바다쫃",
          "speaker_id": "1",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100027014.1.6",
          "form": "저는 부산 살아서 바닷가를 자주가는거 같아요",
          "original_form": "저는 부산 살아서 바닷가를 자주가는거 같아요",
          "speaker_id": "2",
          "time": "20210902 07:58"
        },
        {
          "id": "MDRW2100027014.1.7",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100027014.1.8",
          "form": "해외는 어디가 가고싶으신가요?",
          "original_form": "해외는 어디가 가고싶으신가요?",
          "speaker_id": "2",
          "time": "20210902 08:02"
        },
        {
          "id": "MDRW2100027014.1.9",
          "form": "유럽이요",
          "original_form": "유럽이요",
          "speaker_id": "1",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100027014.1.10",
          "form": "거기 해리포터 테마파크가 가보고싶네요",
          "original_form": "거기 해리포터 테마파크가 가보고싶네요",
          "speaker_id": "2",
          "time": "20210902 08:04"
        },
        {
          "id": "MDRW2100027014.1.11",
          "form": "그런것도 있나요?",
          "original_form": "그런것도 있나요?",
          "speaker_id": "1",
          "time": "20210902 08:05"
        },
        {
          "id": "MDRW2100027014.1.12",
          "form": "그런가바요 해리포터 재밌게봤는데",
          "original_form": "그런가바요 해리포터 재밌게봤는데",
          "speaker_id": "2",
          "time": "20210902 08:06"
        },
        {
          "id": "MDRW2100027014.1.13",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100027014.1.14",
          "form": "저는 일본 가고 싶어요",
          "original_form": "저는 일본 가고 싶어요",
          "speaker_id": "2",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100027014.1.15",
