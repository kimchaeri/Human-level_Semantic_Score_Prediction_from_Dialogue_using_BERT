          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027561.1.7",
          "form": "온라인 어제",
          "original_form": "온라인 어제",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027561.1.8",
          "form": "쿠폰 잘못 뿌려서",
          "original_form": "쿠폰 잘못 뿌려서",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027561.1.9",
          "form": "난리났어요 ㅋㅋ",
          "original_form": "난리났어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027561.1.10",
          "form": "아 저도 봤어요ㅎㅎ",
          "original_form": "아 저도 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027561.1.11",
          "form": "지금은 막혔다던데ㅎ",
          "original_form": "지금은 막혔다던데ㅎ",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027561.1.12",
          "form": "취소는 제대로 되고 있나 몰겠네요ㅋㅋ",
          "original_form": "취소는 제대로 되고 있나 몰겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027561.1.13",
          "form": "어 안되는데요",
          "original_form": "어 안되는데요",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027561.1.14",
          "form": "저 샀는데요",
          "original_form": "저 샀는데요",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027561.1.15",
          "form": "엇 아직 취소 안됐음",
          "original_form": "엇 아직 취소 안됐음",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027561.1.16",
          "form": "대기업이니 걍  마케팅 셈치고 보내줄 수도?",
          "original_form": "대기업이니 걍  마케팅 셈치고 보내줄 수도?",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027561.1.17",
          "form": "언니도 간밤에 뭐 샀댔거든요",
          "original_form": "언니도 간밤에 뭐 샀댔거든요",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027561.1.18",
          "form": "소불고기 가즈아",
          "original_form": "소불고기 가즈아",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027561.1.19",
          "form": "저는 귀찮다고 뒹굴하다 확인하니 끝ㅎ",
          "original_form": "저는 귀찮다고 뒹굴하다 확인하니 끝ㅎ",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027561.1.20",
          "form": "근데 전 쿠폰만 미리 받고 오늘 결제함ㅋㅋ",
          "original_form": "근데 전 쿠폰만 미리 받고 오늘 결제함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027561.1.21",
          "form": "흐음",
          "original_form": "흐음",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027561.1.22",
          "form": "오오 소불고기 좋군요ㅋㅋ",
          "original_form": "오오 소불고기 좋군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027561.1.23",
          "form": "미리받은 쿠폰은 안 바뀌나 봐요!",
          "original_form": "미리받은 쿠폰은 안 바뀌나 봐요!",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027561.1.24",
          "form": "잠만요",
          "original_form": "잠만요",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027561.1.25",
          "form": "아 언니는 올반 우삼겹 샀댔어요ㅎㅎ",
          "original_form": "아 언니는 올반 우삼겹 샀댔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027561.1.26",
          "form": "우삽겹 전골 가즈아~",
          "original_form": "우삽겹 전골 가즈아~",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027561.1.27",
          "form": "저도 일단 쿠폰 받아두는건데 원통",
          "original_form": "저도 일단 쿠폰 받아두는건데 원통",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027561.1.28",
          "form": "올반 우삼겹은",
          "original_form": "올반 우삼겹은",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027561.1.29",
          "form": "어제 밤부터 매진이었습니다",
          "original_form": "어제 밤부터 매진이었습니다",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027561.1.30",
          "form": "언니분 얼리버드시네요",
          "original_form": "언니분 얼리버드시네요",
          "speaker_id": "2",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027561.1.31",
          "form": "ㅋㅋ 핫한 제품인가요ㅎ",
          "original_form": "ㅋㅋ 핫한 제품인가요ㅎ",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027561.1.32",
          "form": "언니가 카톡한게 새벽 1시였네요",
          "original_form": "언니가 카톡한게 새벽 1시였네요",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027561.1.33",
          "form": "언니는 클릭 한번에 우삼겹을",
          "original_form": "언니는 클릭 한번에 우삼겹을",
          "speaker_id": "2",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027561.1.34",
          "form": "name2도 위너잖아욧!",
          "original_form": "&name2&도 위너잖아욧!",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027561.1.35",
          "form": "늦게 일어났는데도 벌레를 잡다니ㅂㄷㅂㄷ",
          "original_form": "늦게 일어났는데도 벌레를 잡다니ㅂㄷㅂㄷ",
