      "utterance": [
        {
          "id": "MDRW2100038229.1.1",
          "form": "ㅎㅎ 그런가여? 근데 진짜 지점마다 달라요",
          "original_form": "ㅎㅎ 그런가여? 근데 진짜 지점마다 달라요",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100038229.1.2",
          "form": "잠 늦게 주무시나봐요",
          "original_form": "잠 늦게 주무시나봐요",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038229.1.3",
          "form": "ㅎㅎ 네",
          "original_form": "ㅎㅎ 네",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038229.1.4",
          "form": "저는 일 그만둬서 괜찮은데 괜찮으신가 싶어서 ㅋㅋㅋ",
          "original_form": "저는 일 그만둬서 괜찮은데 괜찮으신가 싶어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038229.1.5",
          "form": "늦게 자기시작하니 습관이 ㅎㅎ",
          "original_form": "늦게 자기시작하니 습관이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038229.1.6",
          "form": "잠이 적으신가보다",
          "original_form": "잠이 적으신가보다",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038229.1.7",
          "form": "매일 커피는 드셔야겠어요",
          "original_form": "매일 커피는 드셔야겠어요",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038229.1.8",
          "form": "자는시간이 아까워서요 ㅎㅎ",
          "original_form": "자는시간이 아까워서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038229.1.9",
          "form": "오 대단하신 분이다!!",
          "original_form": "오 대단하신 분이다!!",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038229.1.10",
          "form": "오전에 한잔 오후에 한잔씩 먹어요 ㅎㅎ",
          "original_form": "오전에 한잔 오후에 한잔씩 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038229.1.11",
          "form": "저는 자는 시간이 좋아요 ㅋㅋㅋ",
          "original_form": "저는 자는 시간이 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100038229.1.12",
          "form": "생각없을수 있는 시간이라",
          "original_form": "생각없을수 있는 시간이라",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038229.1.13",
          "form": "회사에서 점심은 시켜드세요??",
          "original_form": "회사에서 점심은 시켜드세요??",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038229.1.14",
          "form": "구내식당이요",
          "original_form": "구내식당이요",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038229.1.15",
          "form": "그럼 편하시겠어요",
          "original_form": "그럼 편하시겠어요",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038229.1.16",
          "form": "구내식당에서 주는건 저는 다 좋던데 ㅎㅎ",
          "original_form": "구내식당에서 주는건 저는 다 좋던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038229.1.17",
          "form": "저는 메뉴 고르는게 진짜 피곤했거든요 ㅋㅋ",
          "original_form": "저는 메뉴 고르는게 진짜 피곤했거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038229.1.18",
          "form": "어제 남자직원은 투덜대더라구요",
          "original_form": "어제 남자직원은 투덜대더라구요",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038229.1.19",
          "form": "다이어트식단이라고 풀떼기만 나왔다고요 ㅋ",
          "original_form": "다이어트식단이라고 풀떼기만 나왔다고요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038229.1.20",
          "form": "ㅋㅋㅋㅋㅋ 다이어트 식단이면 풀떼기 나와야 정상이져",
          "original_form": "ㅋㅋㅋㅋㅋ 다이어트 식단이면 풀떼기 나와야 정상이져",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038229.1.21",
          "form": "저두요 예전에구내식당 없는곳에서\r\n매번 메뉴 정하기 힘들었어요",
          "original_form": "저두요 예전에구내식당 없는곳에서\r\n매번 메뉴 정하기 힘들었어요",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038229.1.22",
          "form": "비오는 날에는 대리님 과장님들이 나가기 귀찮다고",
          "original_form": "비오는 날에는 대리님 과장님들이 나가기 귀찮다고",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038229.1.23",
          "form": "중국집 배달로 했는데요",
          "original_form": "중국집 배달로 했는데요",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038229.1.24",
          "form": "장마철에는 진짜 징글징글했어요",
          "original_form": "장마철에는 진짜 징글징글했어요",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038229.1.25",
          "form": "맞아요 구내식당 있으니까 훨씬 편하시겠다",
          "original_form": "맞아요 구내식당 있으니까 훨씬 편하시겠다",
          "speaker_id": "2",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038229.1.26",
          "form": "그땐 막내라 시키는대로 할때라 ㅋ",
          "original_form": "그땐 막내라 시키는대로 할때라 ㅋ",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038229.1.27",
          "form": "내가 올드보이 된거마냥 군만두서비스에 좋아해야하나싶었죠 ㅋ",
          "original_form": "내가 올드보이 된거마냥 군만두서비스에 좋아해야하나싶었죠 ㅋ",
          "speaker_id": "1",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038229.1.28",
          "form": "저는 그만둔 곳이 거의 막내여서 제가 시켰어요ㅠㅠ",
          "original_form": "저는 그만둔 곳이 거의 막내여서 제가 시켰어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038229.1.29",
          "form": "그런건 남자직원 안시키더라고요",
          "original_form": "그런건 남자직원 안시키더라고요",
          "speaker_id": "2",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038229.1.30",
          "form": "글쵸?!!!",
          "original_form": "글쵸?!!!",
          "speaker_id": "1",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038229.1.31",
          "form": "그만두고 나니까 더 짜증ㅋㅋㅋ",
          "original_form": "그만두고 나니까 더 짜증ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038229.1.32",
          "form": "왜 내가 했지 싶어욬ㅋㅋㅋ",
          "original_form": "왜 내가 했지 싶어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038229.1.33",
          "form": "ㅋㅋㅋㅋ 구내식당 있는 지금은 저에겐 평화가",
          "original_form": "ㅋㅋㅋㅋ 구내식당 있는 지금은 저에겐 평화가",
          "speaker_id": "1",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038229.1.34",
