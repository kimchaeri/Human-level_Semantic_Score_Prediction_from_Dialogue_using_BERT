      "utterance": [
        {
          "id": "MDRW2100037401.1.1",
          "form": "기억에 남는 기념품 있나요",
          "original_form": "기억에 남는 기념품 있나요",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100037401.1.2",
          "form": "전 냉장고에 붙어있는 자석들ㅎㅎ",
          "original_form": "전 냉장고에 붙어있는 자석들ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037401.1.3",
          "form": "특별한건 없네요",
          "original_form": "특별한건 없네요",
          "speaker_id": "1",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037401.1.4",
          "form": "오 어디에서 사오신것들인가요",
          "original_form": "오 어디에서 사오신것들인가요",
          "speaker_id": "2",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037401.1.5",
          "form": "여행갈때마다 샀는데",
          "original_form": "여행갈때마다 샀는데",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037401.1.6",
          "form": "처치곤란이네요",
          "original_form": "처치곤란이네요",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037401.1.7",
          "form": "아이들이 좋아할것 같은데용",
          "original_form": "아이들이 좋아할것 같은데용",
          "speaker_id": "2",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037401.1.8",
          "form": "던지기용으로ㅠ",
          "original_form": "던지기용으로ㅠ",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100037401.1.9",
          "form": "앗..ㅠㅠ 저는 주로 먹는게 기억에 남아요?ㅋㅋ",
          "original_form": "앗..ㅠㅠ 저는 주로 먹는게 기억에 남아요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037401.1.10",
          "form": "진짜 먹는행복은 넘사벽",
          "original_form": "진짜 먹는행복은 넘사벽",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037401.1.11",
          "form": "먹으러 여행가요ㅋ",
          "original_form": "먹으러 여행가요ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037401.1.12",
          "form": "ㅋㅋ 기억에 남는 여행지 음식은요?",
          "original_form": "ㅋㅋ 기억에 남는 여행지 음식은요?",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037401.1.13",
          "form": "일본 초밥이여",
          "original_form": "일본 초밥이여",
          "speaker_id": "1",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100037401.1.14",
          "form": "넘 맛있어여ㅠ",
          "original_form": "넘 맛있어여ㅠ",
          "speaker_id": "1",
          "time": "20210831 23:32"
        },
        {
          "id": "MDRW2100037401.1.15",
          "form": "우왓..초밥 좋아하시는군요",
          "original_form": "우왓..초밥 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100037401.1.16",
          "form": "여행 내내 먹었던",
          "original_form": "여행 내내 먹었던",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037401.1.17",
          "form": "경치는 기억이 안나요ㅋ",
          "original_form": "경치는 기억이 안나요ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037401.1.18",
          "form": "저는 경주빵..통영 꿀빵이런것만 기억난다는..",
          "original_form": "저는 경주빵..통영 꿀빵이런것만 기억난다는..",
          "speaker_id": "2",
          "time": "20210831 23:36"
        }
      ]
