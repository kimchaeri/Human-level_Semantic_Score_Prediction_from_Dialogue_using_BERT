      "utterance": [
        {
          "id": "MDRW2100005164.1.1",
          "form": "역시 다시 식음료",
          "original_form": "역시 다시 식음료",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005164.1.2",
          "form": "탄산수 떨어져가는데 또 사야하나 고민",
          "original_form": "탄산수 떨어져가는데 또 사야하나 고민",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005164.1.3",
          "form": "탄산수 좋은데",
          "original_form": "탄산수 좋은데",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005164.1.4",
          "form": "플라스틱 폐기물 너무 많음",
          "original_form": "플라스틱 폐기물 너무 많음",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005164.1.5",
          "form": "그치 플라스틱 병이 넘 나와서",
          "original_form": "그치 플라스틱 병이 넘 나와서",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005164.1.6",
          "form": "근데 엄마가 넘 좋아하시니...",
          "original_form": "근데 엄마가 넘 좋아하시니...",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005164.1.7",
          "form": "응 그리고 뭔가 시원하고 톡 쏘는 게 당길 때가 있음",
          "original_form": "응 그리고 뭔가 시원하고 톡 쏘는 게 당길 때가 있음",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005164.1.8",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005164.1.9",
          "form": "술안마시고 커피 안마시는 대신 탄산수면",
          "original_form": "술안마시고 커피 안마시는 대신 탄산수면",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005164.1.10",
          "form": "괜찮지 않나 자기합리화 하게되네",
          "original_form": "괜찮지 않나 자기합리화 하게되네",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005164.1.11",
          "form": "플라스틱 통 문제만 해결하면",
          "original_form": "플라스틱 통 문제만 해결하면",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005164.1.12",
          "form": "응 캔으로 마시나",
          "original_form": "응 캔으로 마시나",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005164.1.13",
          "form": "지난번 빅토리아는 플라스틱이 할인해서 산건데",
          "original_form": "지난번 빅토리아는 플라스틱이 할인해서 산건데",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005164.1.14",
          "form": "캔도 쓰레기는 마찬가지",
          "original_form": "캔도 쓰레기는 마찬가지",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005164.1.15",
          "form": "캔이 더 재활용률이 높지",
          "original_form": "캔이 더 재활용률이 높지",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005164.1.16",
          "form": "캔얘기하니까 콘치즈 먹고싶네",
          "original_form": "캔얘기하니까 콘치즈 먹고싶네",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005164.1.17",
          "form": "탄산음료나",
          "original_form": "탄산음료나",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005164.1.18",
          "form": "이런것보다야 탄산수가 낫지",
          "original_form": "이런것보다야 탄산수가 낫지",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005164.1.19",
          "form": "콘치즈라니!!",
          "original_form": "콘치즈라니!!",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005164.1.20",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005164.1.21",
          "form": "고소하고 달콤한 그맛",
          "original_form": "고소하고 달콤한 그맛",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005164.1.22",
          "form": "마요네즈랑 피자치즈 넣어서 고소하게",
          "original_form": "마요네즈랑 피자치즈 넣어서 고소하게",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005164.1.23",
          "form": "지글지글",
          "original_form": "지글지글",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005164.1.24",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005164.1.25",
          "form": "야식타임",
          "original_form": "야식타임",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005164.1.26",
          "form": "콘 치즈 다 없네 지금은",
          "original_form": "콘 치즈 다 없네 지금은",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005164.1.27",
          "form": "뿌리는 치즈는 냉동실에 가득한데",
          "original_form": "뿌리는 치즈는 냉동실에 가득한데",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005164.1.28",
          "form": "그러게 담에 장볼때 콘이랑 치즈랑 사자",
          "original_form": "그러게 담에 장볼때 콘이랑 치즈랑 사자",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005164.1.29",
          "form": "그것도 뿌림 맛있음 굵은 고춧가루랑",
          "original_form": "그것도 뿌림 맛있음 굵은 고춧가루랑",
          "speaker_id": "1",
