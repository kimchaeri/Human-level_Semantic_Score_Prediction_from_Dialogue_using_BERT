      "utterance": [
        {
          "id": "MDRW2100010830.1.1",
          "form": "커뮤니티 하시나요?",
          "original_form": "커뮤니티 하시나요?",
          "speaker_id": "1",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010830.1.2",
          "form": "전에는 햇는데 지금은 잘안해요ㅎ",
          "original_form": "전에는 햇는데 지금은 잘안해요ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010830.1.3",
          "form": "오 어떤거 하셨었어요?",
          "original_form": "오 어떤거 하셨었어요?",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010830.1.4",
          "form": "게임이나 운동요",
          "original_form": "게임이나 운동요",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010830.1.5",
          "form": "이름대면 알만한 곳이에요?",
          "original_form": "이름대면 알만한 곳이에요?",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010830.1.6",
          "form": "아뇨ㅎ그렇진않아요",
          "original_form": "아뇨ㅎ그렇진않아요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010830.1.7",
          "form": "오홍..정모같은 것도 해보셨나요?",
          "original_form": "오홍..정모같은 것도 해보셨나요?",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010830.1.8",
          "form": "음 차 정모 한번가봣어요ㅎ궁금해서요",
          "original_form": "음 차 정모 한번가봣어요ㅎ궁금해서요",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010830.1.9",
          "form": "헐 저도 궁금해요!!어떠셨어요?",
          "original_form": "헐 저도 궁금해요!!어떠셨어요?",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010830.1.10",
          "form": "그냥 남의차 구경정도요ㅅㅅㄴ",
          "original_form": "그냥 남의차 구경정도요ㅅㅅㄴ",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010830.1.11",
          "form": "차알못이라서ㅎ",
          "original_form": "차알못이라서ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010830.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 차도 마시고 그랬겠네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 차도 마시고 그랬겠네요",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010830.1.13",
          "form": "분위기는 좋았어요?",
          "original_form": "분위기는 좋았어요?",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010830.1.14",
          "form": "넵ㅎㅎ괜찮앗어요",
          "original_form": "넵ㅎㅎ괜찮앗어요",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010830.1.15",
          "form": "저는 소모임 이런 것도 궁금하긴 하던데",
          "original_form": "저는 소모임 이런 것도 궁금하긴 하던데",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010830.1.16",
          "form": "막상 가면 연애할 생각 하는 사람만 많대서 용기가 안나더라구요 ㅎ..",
          "original_form": "막상 가면 연애할 생각 하는 사람만 많대서 용기가 안나더라구요 ㅎ..",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010830.1.17",
          "form": "그런거아니시면 원데이클래스같은거해보세요ㅎ",
          "original_form": "그런거아니시면 원데이클래스같은거해보세요ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010830.1.18",
          "form": "그럴까봐요!",
          "original_form": "그럴까봐요!",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010830.1.19",
          "form": "코로나만 잠잠해지면 ㅠ..",
          "original_form": "코로나만 잠잠해지면 ㅠ..",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010830.1.20",
          "form": "관심잇는분야 검색해서 수업받는데 좋아요ㅎ",
          "original_form": "관심잇는분야 검색해서 수업받는데 좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010830.1.21",
          "form": "그쳐ㅜㅜ",
          "original_form": "그쳐ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010830.1.22",
          "form": "지금은 돌아다니는 것도 민폐처럼 느껴질 때가 있어요 ㅠㅠ",
          "original_form": "지금은 돌아다니는 것도 민폐처럼 느껴질 때가 있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010830.1.23",
          "form": "맞아요ㅎㅎ근데 밖에 사람 꽉꽉차있고",
          "original_form": "맞아요ㅎㅎ근데 밖에 사람 꽉꽉차있고",
