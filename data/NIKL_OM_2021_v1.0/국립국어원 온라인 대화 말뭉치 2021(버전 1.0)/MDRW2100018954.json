      "utterance": [
        {
          "id": "MDRW2100018954.1.1",
          "form": "혹시 드라마 이런건 안보세요? ㅋㅋ",
          "original_form": "혹시 드라마 이런건 안보세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018954.1.2",
          "form": "저 드라마 좋아해요ㅋㅋㅋ",
          "original_form": "저 드라마 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018954.1.3",
          "form": "최근엔 슬의랑 악마판사 봤어요",
          "original_form": "최근엔 슬의랑 악마판사 봤어요",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018954.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018954.1.5",
          "form": "저도 드라마 진짜 좋아해요! ㅋㅋ 슬의 다 끝나면 보려고 킵해두고있어요 몰아보는걸 좋아하거든요 ㅋㅋ악마판사 재밌나요?",
          "original_form": "저도 드라마 진짜 좋아해요! ㅋㅋ 슬의 다 끝나면 보려고 킵해두고있어요 몰아보는걸 좋아하거든요 ㅋㅋ악마판사 재밌나요?",
          "speaker_id": "1",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100018954.1.6",
          "form": "넵 악마판사 재밌어요!",
          "original_form": "넵 악마판사 재밌어요!",
          "speaker_id": "2",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100018954.1.7",
          "form": "name1님은 어떤 드라마 보세요?",
          "original_form": "&name1&님은 어떤 드라마 보세요?",
          "speaker_id": "2",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100018954.1.8",
          "form": "저요즘 왓쳐랑 오월의 청춘이요!",
          "original_form": "저요즘 왓쳐랑 오월의 청춘이요!",
          "speaker_id": "1",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100018954.1.9",
          "form": "오월의 청춘이 그 광주민주항쟁이야기죠?",
          "original_form": "오월의 청춘이 그 광주민주항쟁이야기죠?",
          "speaker_id": "2",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100018954.1.10",
          "form": "저도 부분부분 봤는데 엄청 슬프더라구요 ㅠㅠ",
          "original_form": "저도 부분부분 봤는데 엄청 슬프더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100018954.1.11",
          "form": "네 그건 재밌는데 왓쳐가 진짜 노잼이에요...",
          "original_form": "네 그건 재밌는데 왓쳐가 진짜 노잼이에요...",
          "speaker_id": "1",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100018954.1.12",
          "form": "앗 저런...",
          "original_form": "앗 저런...",
          "speaker_id": "2",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100018954.1.13",
          "form": "괴물들 나오는 내용인가요?",
          "original_form": "괴물들 나오는 내용인가요?",
          "speaker_id": "2",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100018954.1.14",
          "form": "얼핏 소개영상 봤었는데",
          "original_form": "얼핏 소개영상 봤었는데",
          "speaker_id": "2",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100018954.1.15",
          "form": "아나요 서강준 김현주 한석규 나오는 경찰물? 이에요... 서강준 얼굴만 유잼..",
          "original_form": "아나요 서강준 김현주 한석규 나오는 경찰물? 이에요... 서강준 얼굴만 유잼..",
          "speaker_id": "1",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100018954.1.16",
          "form": "앗 한국드라마였군요",
          "original_form": "앗 한국드라마였군요",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100018954.1.17",
          "form": "그 배우들로 재미가 없다니 세상에..",
          "original_form": "그 배우들로 재미가 없다니 세상에..",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100018954.1.18",
          "form": "그냥 제 취향에 안맞는걸수도있어욫ㅋㅋ화면이 어두우면 집중이안되요 ㅋ",
          "original_form": "그냥 제 취향에 안맞는걸수도있어욫ㅋㅋ화면이 어두우면 집중이안되요 ㅋ",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100018954.1.19",
          "form": "저랑 비슷하신 분이 계셨네요ㅋㅋㅋ",
          "original_form": "저랑 비슷하신 분이 계셨네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100018954.1.20",
          "form": "저 그래서 해리포터 뒷편들 집중을 못했어요ㅋㅋㅋㅋㅋㅋㅠ 어두워서",
          "original_form": "저 그래서 해리포터 뒷편들 집중을 못했어요ㅋㅋㅋㅋㅋㅋㅠ 어두워서",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100018954.1.21",
          "form": "헐 저도요 ㅋㅋㅋㅋ 보다 자꾸 잠들어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 저도요 ㅋㅋㅋㅋ 보다 자꾸 잠들어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100018954.1.22",
          "form": "화면 밝아도 재밌는데 왜 자꾸 어둡게 만드는검ㅣ",
          "original_form": "화면 밝아도 재밌는데 왜 자꾸 어둡게 만드는검ㅣ",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100018954.1.23",
          "form": "건지 ㅠㅠ",
          "original_form": "건지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100018954.1.24",
          "form": "그니까요 밝기 높이면 눈만아프고 ㅋㅋㅋ",
          "original_form": "그니까요 밝기 높이면 눈만아프고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100018954.1.25",
          "form": "어두운화면은 그대로이고 그런경우도있더라구요 ㅋㅋ",
          "original_form": "어두운화면은 그대로이고 그런경우도있더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100018954.1.26",
          "form": "맞아요 아무리 밝기 높혀도 안밝아짐ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 아무리 밝기 높혀도 안밝아짐ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:18"
        }
      ]
