        },
        {
          "id": "MDRW2100011015.1.2",
          "form": "아이패드요",
          "original_form": "아이패드요",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100011015.1.3",
          "form": "아이패드 없으세요?",
          "original_form": "아이패드 없으세요?",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100011015.1.4",
          "form": "근데 노트북을 최근에 사서 괜찮아요",
          "original_form": "근데 노트북을 최근에 사서 괜찮아요",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100011015.1.5",
          "form": "우와 노트북 어떤거요?",
          "original_form": "우와 노트북 어떤거요?",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100011015.1.6",
          "form": "그램노트북이요",
          "original_form": "그램노트북이요",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100011015.1.7",
          "form": "우와 진짜 가볍나요?",
          "original_form": "우와 진짜 가볍나요?",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100011015.1.8",
          "form": "네 가벼워서 들고다니기좋아요",
          "original_form": "네 가벼워서 들고다니기좋아요",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
