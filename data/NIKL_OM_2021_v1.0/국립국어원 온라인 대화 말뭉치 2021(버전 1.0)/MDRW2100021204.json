        {
          "id": "MDRW2100021204.1.1",
          "form": "네 은근 덥네요 아직",
          "original_form": "네 은근 덥네요 아직",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021204.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021204.1.3",
          "form": "아직 가을이 오려면 멀엇나봐요",
          "original_form": "아직 가을이 오려면 멀엇나봐요",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021204.1.4",
          "form": "그쵸 곧 올듯 하더니",
          "original_form": "그쵸 곧 올듯 하더니",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021204.1.5",
          "form": "가을 옷은 장만하셧나요",
          "original_form": "가을 옷은 장만하셧나요",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021204.1.6",
          "form": "간절기옷 읶어요",
          "original_form": "간절기옷 읶어요",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021204.1.7",
          "form": "전 입을만한게 별로 없네요",
          "original_form": "전 입을만한게 별로 없네요",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021204.1.8",
          "form": "구매 해야겠어요",
          "original_form": "구매 해야겠어요",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021204.1.9",
          "form": "후즐근하게 버티다가 겨울로 넘어갈지 몇벌 살지 고민중이에요 ㅋㅋㅋㅋ",
          "original_form": "후즐근하게 버티다가 겨울로 넘어갈지 몇벌 살지 고민중이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021204.1.10",
          "form": "가을은 금방지나가는 느낌이라",
          "original_form": "가을은 금방지나가는 느낌이라",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021204.1.11",
          "form": "가을이.거의.없죠",
          "original_form": "가을이.거의.없죠",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021204.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021204.1.13",
          "form": "기상이변이",
