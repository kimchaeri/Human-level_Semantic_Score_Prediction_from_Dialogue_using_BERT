      "utterance": [
        {
          "id": "MDRW2100025484.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025484.1.2",
          "form": "많이요",
          "original_form": "많이요",
          "speaker_id": "1",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025484.1.3",
          "form": "오늘 아침에 조금 오더니 지금은 안오네요",
          "original_form": "오늘 아침에 조금 오더니 지금은 안오네요",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025484.1.4",
          "form": "거기는 비와요?",
          "original_form": "거기는 비와요?",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025484.1.5",
          "form": "넴 많이",
          "original_form": "넴 많이",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100025484.1.6",
          "form": "지금도요?",
          "original_form": "지금도요?",
          "speaker_id": "2",
          "time": "20210901 13:28"
        },
        {
          "id": "MDRW2100025484.1.7",
          "form": "저희 지역은 내일도 비오고 다음주도 온대요",
          "original_form": "저희 지역은 내일도 비오고 다음주도 온대요",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025484.1.8",
          "form": "요즘 가을비가 참 많이오네요",
          "original_form": "요즘 가을비가 참 많이오네요",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025484.1.9",
          "form": "늦장마네요",
          "original_form": "늦장마네요",
          "speaker_id": "1",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025484.1.10",
          "form": "날씨도 많이 추워졌어요",
          "original_form": "날씨도 많이 추워졌어요",
          "speaker_id": "2",
          "time": "20210901 14:33"
        },
        {
          "id": "MDRW2100025484.1.11",
          "form": "근데 하늘이 너무 예쁜건 좋네요",
          "original_form": "근데 하늘이 너무 예쁜건 좋네요",
          "speaker_id": "2",
          "time": "20210901 14:33"
        },
        {
          "id": "MDRW2100025484.1.12",
          "form": "맑아오",
          "original_form": "맑아오",
          "speaker_id": "1",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100025484.1.13",
          "form": "좋아하는 계절이 뭐예요?",
          "original_form": "좋아하는 계절이 뭐예요?",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100025484.1.14",
          "form": "봄이요",
          "original_form": "봄이요",
          "speaker_id": "1",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025484.1.15",
          "form": "저는 가을이요",
          "original_form": "저는 가을이요",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025484.1.16",
          "form": "쓸쓸하잖아ㅛ",
          "original_form": "쓸쓸하잖아ㅛ",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100025484.1.17",
          "form": "ㅋㅋㅋ그런가요? 저는 가을날씨와 분위기가 너무 좋아요",
          "original_form": "ㅋㅋㅋ그런가요? 저는 가을날씨와 분위기가 너무 좋아요",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025484.1.18",
          "form": "괜히 있어보인달까 뭘하든 ㅋㅋㅋㅋ",
          "original_form": "괜히 있어보인달까 뭘하든 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025484.1.19",
          "form": "분위기 좋죠",
          "original_form": "분위기 좋죠",
          "speaker_id": "1",
          "time": "20210901 17:10"
        }
      ]
