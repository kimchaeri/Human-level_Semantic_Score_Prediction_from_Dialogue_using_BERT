      "utterance": [
        {
          "id": "MDRW2100013073.1.1",
          "form": "요즘 제일 사고싶은 물건 있으세요?",
          "original_form": "요즘 제일 사고싶은 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013073.1.2",
          "form": "네 냉장고요",
          "original_form": "네 냉장고요",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013073.1.3",
          "form": "뭐사고 싶으세요?",
          "original_form": "뭐사고 싶으세요?",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100013073.1.4",
          "form": "오 냉장고! 어떤걸로요?",
          "original_form": "오 냉장고! 어떤걸로요?",
          "speaker_id": "2",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100013073.1.5",
          "form": "전 요즘....핸드폰 바꾸고 싶어요 ㅎ",
          "original_form": "전 요즘....핸드폰 바꾸고 싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100013073.1.6",
          "form": "핸드폰은 맨날 바꾸고 싶어요 ㅜㅜ",
          "original_form": "핸드폰은 맨날 바꾸고 싶어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100013073.1.7",
          "form": "요새 나온 삼성 와 진짜 탐나요 폰이",
          "original_form": "요새 나온 삼성 와 진짜 탐나요 폰이",
          "speaker_id": "1",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100013073.1.8",
          "form": "냉장고는 솔직히 다르건 상관없고 고장만 안나면 돼요 ㅋㅋ",
          "original_form": "냉장고는 솔직히 다르건 상관없고 고장만 안나면 돼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100013073.1.9",
          "form": "냉장만 잘되면 되죠 ㅎㅎ",
          "original_form": "냉장만 잘되면 되죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100013073.1.10",
          "form": "핸드폰 그 전 위아래접는거 말고 옆으로 접는거요",
          "original_form": "핸드폰 그 전 위아래접는거 말고 옆으로 접는거요",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100013073.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ폴드!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ폴드!",
          "speaker_id": "2",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013073.1.12",
          "form": "전 플립 봤는데 직접 보니 생각보다 별로라 다음꺼 기다리려구요",
          "original_form": "전 플립 봤는데 직접 보니 생각보다 별로라 다음꺼 기다리려구요",
          "speaker_id": "2",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013073.1.13",
          "form": "네 이름이 요새 생각이 안나요 ㅋㅋ",
          "original_form": "네 이름이 요새 생각이 안나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013073.1.14",
          "form": "사전예약이 제일 할인폭도 크고 뭐도 많이 줘서",
          "original_form": "사전예약이 제일 할인폭도 크고 뭐도 많이 줘서",
          "speaker_id": "2",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013073.1.15",
          "form": "네 요건 좀 그렇고 좀있음 더 좋을듯해요",
          "original_form": "네 요건 좀 그렇고 좀있음 더 좋을듯해요",
          "speaker_id": "1",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013073.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013073.1.17",
          "form": "그거 해야해요",
          "original_form": "그거 해야해요",
          "speaker_id": "1",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013073.1.18",
          "form": "노트.다음거 보고 있는데 언제 나오련지ㅋㅋㅋ",
          "original_form": "노트.다음거 보고 있는데 언제 나오련지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013073.1.19",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013073.1.20",
          "form": "전 이사갈 계획 갖고 있는데 그러려면 애들 방도 좀 꾸며줘야 할 것 같고..",
          "original_form": "전 이사갈 계획 갖고 있는데 그러려면 애들 방도 좀 꾸며줘야 할 것 같고..",
          "speaker_id": "2",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013073.1.21",
          "form": "노트 생산 안한다고 하고",
          "original_form": "노트 생산 안한다고 하고",
          "speaker_id": "1",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013073.1.22",
          "form": "바꿀것도 좀 바꾸고..",
          "original_form": "바꿀것도 좀 바꾸고..",
          "speaker_id": "2",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013073.1.23",
          "form": "이사가면 애들방 필수죠",
          "original_form": "이사가면 애들방 필수죠",
          "speaker_id": "1",
          "time": "20210825 12:42"
        },
        {
          "id": "MDRW2100013073.1.24",
          "form": "전 이사 올때 침대바꿨어요",
          "original_form": "전 이사 올때 침대바꿨어요",
          "speaker_id": "1",
          "time": "20210825 12:42"
        },
        {
          "id": "MDRW2100013073.1.25",
          "form": "우왕 애들은 분리되는 이층침대 해주려구요ㅎㅎ",
          "original_form": "우왕 애들은 분리되는 이층침대 해주려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013073.1.26",
          "form": "일단은 그냥 매트리스 생활",
          "original_form": "일단은 그냥 매트리스 생활",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013073.1.27",
          "form": "네 살때 까진 매트리스죠",
          "original_form": "네 살때 까진 매트리스죠",
          "speaker_id": "1",
          "time": "20210825 12:44"
        },
        {
          "id": "MDRW2100013073.1.28",
          "form": "애들이 위아래 서로 선전 경쟁 하겠는데요 호",
          "original_form": "애들이 위아래 서로 선전 경쟁 하겠는데요 호",
          "speaker_id": "1",
          "time": "20210825 12:44"
        },
        {
          "id": "MDRW2100013073.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ동생이 양보해야죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ동생이 양보해야죠",
          "speaker_id": "2",
          "time": "20210825 12:44"
        },
        {
          "id": "MDRW2100013073.1.30",
          "form": "네 앙 윗녀석들 착해요",
          "original_form": "네 앙 윗녀석들 착해요",
          "speaker_id": "1",
          "time": "20210825 12:45"
        }
      ]
    }
