        },
        {
          "id": "MDRW2100045941.1.2",
          "form": "손들어갈정도로 ㅋㅋ",
          "original_form": "손들어갈정도로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045941.1.3",
          "form": "그레서 끝까지 다쓸수있어요",
          "original_form": "그레서 끝까지 다쓸수있어요",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045941.1.4",
          "form": "와 그게 좋네요",
          "original_form": "와 그게 좋네요",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045941.1.5",
          "form": "걍 치덕치덕 바르기도 좋고",
          "original_form": "걍 치덕치덕 바르기도 좋고",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045941.1.6",
          "form": "네 추천합니당",
          "original_form": "네 추천합니당",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045941.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045941.1.8",
          "form": "추천 감사해요!ㅋㅋㅋㅋ",
          "original_form": "추천 감사해요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.9",
          "form": "이제 겨울이라 마침 필요했는데",
          "original_form": "이제 겨울이라 마침 필요했는데",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.10",
          "form": "저는세타필은",
          "original_form": "저는세타필은",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.11",
          "form": "완전 좋아요ㅎㅎ",
          "original_form": "완전 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.12",
          "form": "팔뚝같이좀",
          "original_form": "팔뚝같이좀",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.14",
          "form": "아 그죠",
          "original_form": "아 그죠",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.15",
          "form": "오돌토돌한거나는데바르고",
          "original_form": "오돌토돌한거나는데바르고",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.16",
          "form": "싼거는 다리",
          "original_form": "싼거는 다리",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.17",
          "form": "오 그러네요",
          "original_form": "오 그러네요",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.18",
          "form": "그렇게 나누면 더 좋겠어요",
          "original_form": "그렇게 나누면 더 좋겠어요",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045941.1.19",
          "form": "네 ㅋㅋㅋ이것도",
          "original_form": "네 ㅋㅋㅋ이것도",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045941.1.20",
          "form": "돈아끼려고",
          "original_form": "돈아끼려고",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045941.1.21",
          "form": "ㅋㅋㅋ...",
          "original_form": "ㅋㅋㅋ...",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045941.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045941.1.23",
          "form": "아끼는 게 최고죠!",
          "original_form": "아끼는 게 최고죠!",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
