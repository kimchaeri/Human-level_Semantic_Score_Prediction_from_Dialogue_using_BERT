      "utterance": [
        {
          "id": "MDRW2100025503.1.1",
          "form": "집안일은 해도해도 끝이없어요",
          "original_form": "집안일은 해도해도 끝이없어요",
          "speaker_id": "1",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025503.1.2",
          "form": "어떤 집안일이 가장 귀찮으세요? 저는 설거지요",
          "original_form": "어떤 집안일이 가장 귀찮으세요? 저는 설거지요",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025503.1.3",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025503.1.4",
          "form": "요리보다 힘든 설거지",
          "original_form": "요리보다 힘든 설거지",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025503.1.5",
          "form": "저는 정리를 못해서",
          "original_form": "저는 정리를 못해서",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025503.1.6",
          "form": "집이 늘 너져분해요",
          "original_form": "집이 늘 너져분해요",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025503.1.7",
          "form": "저도요 바닥에는 머리카락이 흩날리고요",
          "original_form": "저도요 바닥에는 머리카락이 흩날리고요",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025503.1.8",
          "form": "고양이 키우는데 털이 뭉태기로 굴러다녀요",
          "original_form": "고양이 키우는데 털이 뭉태기로 굴러다녀요",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025503.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100025503.1.10",
          "form": "집안일중 제일 좋은건 뭔가요",
          "original_form": "집안일중 제일 좋은건 뭔가요",
          "speaker_id": "1",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100025503.1.11",
          "form": "집안일에 좋은게있을까요?",
          "original_form": "집안일에 좋은게있을까요?",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025503.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025503.1.13",
          "form": "어떤 사람은 빨래하는거 좋아하더라구요",
          "original_form": "어떤 사람은 빨래하는거 좋아하더라구요",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025503.1.14",
          "form": "저는 화장실 청소 ? 괜찮은거 같아요",
          "original_form": "저는 화장실 청소 ? 괜찮은거 같아요",
          "speaker_id": "1",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100025503.1.15",
          "form": "저는 다싫어요",
          "original_form": "저는 다싫어요",
          "speaker_id": "2",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100025503.1.16",
          "form": "그나마 괜찮은건 청소기돌리는거요",
          "original_form": "그나마 괜찮은건 청소기돌리는거요",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100025503.1.17",
          "form": "청소기는  먼지통 비우기 너무 싫어요",
          "original_form": "청소기는  먼지통 비우기 너무 싫어요",
          "speaker_id": "1",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025503.1.18",
          "form": "자동 비우는거 사고싶어요 ㅠㅠ",
          "original_form": "자동 비우는거 사고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025503.1.19",
          "form": "저도요 너무 좋아보이더라고요",
          "original_form": "저도요 너무 좋아보이더라고요",
          "speaker_id": "2",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025503.1.20",
          "form": "요즘 기술들이 정말 대단하네요",
          "original_form": "요즘 기술들이 정말 대단하네요",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025503.1.21",
          "form": "집안일 해주는 로봇이 필요하네요",
          "original_form": "집안일 해주는 로봇이 필요하네요",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025503.1.22",
          "form": "맞아요 식기세척기도 사고싶네요",
          "original_form": "맞아요 식기세척기도 사고싶네요",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025503.1.23",
          "form": "요즘 식세기 인기많더라구오",
          "original_form": "요즘 식세기 인기많더라구오",
          "speaker_id": "1",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100025503.1.24",
          "form": "저도 갖고싶어요",
          "original_form": "저도 갖고싶어요",
          "speaker_id": "1",
          "time": "20210901 13:16"
        }
      ]
    }
  ]
