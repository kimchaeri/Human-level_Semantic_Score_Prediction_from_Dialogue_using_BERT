      "utterance": [
        {
          "id": "MDRW2100046906.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211005 14:00"
        },
        {
          "id": "MDRW2100046906.1.2",
          "form": "틱톡 댄스 챌린지에 대해 어떻게 생각하시나요",
          "original_form": "틱톡 댄스 챌린지에 대해 어떻게 생각하시나요",
          "speaker_id": "1",
          "time": "20211005 14:00"
        },
        {
          "id": "MDRW2100046906.1.3",
          "form": "저 노래홍보하는데엔 괜찮은 방법이라고 생각해요",
          "original_form": "저 노래홍보하는데엔 괜찮은 방법이라고 생각해요",
          "speaker_id": "2",
          "time": "20211005 14:01"
        },
        {
          "id": "MDRW2100046906.1.4",
          "form": "name1님은 어떻게 생각하시나요",
          "original_form": "&name1&님은 어떻게 생각하시나요",
          "speaker_id": "2",
          "time": "20211005 14:01"
        },
        {
          "id": "MDRW2100046906.1.5",
          "form": "저도 댄스 챌린지 자체는 긍정적으로 옹호합니다.",
          "original_form": "저도 댄스 챌린지 자체는 긍정적으로 옹호합니다.",
          "speaker_id": "1",
          "time": "20211005 14:02"
        },
        {
          "id": "MDRW2100046906.1.6",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "1",
          "time": "20211005 14:02"
        },
        {
          "id": "MDRW2100046906.1.7",
          "form": "남성의 성상품화 문제는 걱정이 됩니다.",
          "original_form": "남성의 성상품화 문제는 걱정이 됩니다.",
          "speaker_id": "1",
          "time": "20211005 14:03"
        },
        {
          "id": "MDRW2100046906.1.8",
          "form": "남성의 인권이 너무약합니다.",
          "original_form": "남성의 인권이 너무약합니다.",
          "speaker_id": "1",
          "time": "20211005 14:03"
        },
        {
          "id": "MDRW2100046906.1.9",
          "form": "참여한 경험이 있으신가요?",
          "original_form": "참여한 경험이 있으신가요?",
          "speaker_id": "2",
          "time": "20211005 14:05"
        },
        {
          "id": "MDRW2100046906.1.10",
          "form": "예",
          "original_form": "예",
          "speaker_id": "1",
          "time": "20211005 14:15"
        },
        {
          "id": "MDRW2100046906.1.11",
          "form": "성작 수치심이 들었습니다",
          "original_form": "성작 수치심이 들었습니다",
          "speaker_id": "1",
          "time": "20211005 14:16"
        },
        {
          "id": "MDRW2100046906.1.12",
          "form": "남성인권을 위해 정진해야갰다고생각합니다.",
          "original_form": "남성인권을 위해 정진해야갰다고생각합니다.",
          "speaker_id": "1",
          "time": "20211005 14:16"
        },
        {
          "id": "MDRW2100046906.1.13",
          "form": "참여해보셨나요?",
          "original_form": "참여해보셨나요?",
          "speaker_id": "1",
          "time": "20211005 14:16"
        },
        {
          "id": "MDRW2100046906.1.14",
          "form": "무슨 춤을 추셨나요??",
          "original_form": "무슨 춤을 추셨나요??",
          "speaker_id": "2",
          "time": "20211005 14:18"
        },
        {
          "id": "MDRW2100046906.1.15",
          "form": "전 참여해본적이 없어서요 ㅎ",
          "original_form": "전 참여해본적이 없어서요 ㅎ",
          "speaker_id": "2",
          "time": "20211005 14:18"
        },
        {
          "id": "MDRW2100046906.1.16",
          "form": "오나나나 라는 영상 찍었습니다.",
          "original_form": "오나나나 라는 영상 찍었습니다.",
          "speaker_id": "1",
          "time": "20211005 14:24"
        },
        {
          "id": "MDRW2100046906.1.17",
          "form": "평소에 유투브로 댠스 챌린지가 많이뜨는데",
          "original_form": "평소에 유투브로 댠스 챌린지가 많이뜨는데",
          "speaker_id": "1",
          "time": "20211005 14:25"
        },
        {
          "id": "MDRW2100046906.1.18",
          "form": "기억나시는 챌린지먕 있으신가요?",
          "original_form": "기억나시는 챌린지먕 있으신가요?",
          "speaker_id": "1",
          "time": "20211005 14:25"
        },
        {
          "id": "MDRW2100046906.1.19",
          "form": "오나나나가 남성의 성상품화되는지는 모르겠네요 ...;",
          "original_form": "오나나나가 남성의 성상품화되는지는 모르겠네요 ...;",
          "speaker_id": "2",
          "time": "20211005 14:29"
        },
        {
          "id": "MDRW2100046906.1.20",
          "form": "기억나는 챌린지는 넥스트레벨 춤추는게 기억나네요",
          "original_form": "기억나는 챌린지는 넥스트레벨 춤추는게 기억나네요",
          "speaker_id": "2",
          "time": "20211005 14:30"
        },
        {
          "id": "MDRW2100046906.1.21",
          "form": "ㄷ춤",
          "original_form": "ㄷ춤",
          "speaker_id": "2",
          "time": "20211005 14:30"
        },
        {
          "id": "MDRW2100046906.1.22",
          "form": "그럴수 있습니다!",
          "original_form": "그럴수 있습니다!",
          "speaker_id": "1",
          "time": "20211005 14:51"
        },
        {
          "id": "MDRW2100046906.1.23",
          "form": "기준은 다르기 나름이니까요",
          "original_form": "기준은 다르기 나름이니까요",
          "speaker_id": "1",
          "time": "20211005 14:51"
        },
        {
          "id": "MDRW2100046906.1.24",
          "form": "앞으로 재미로라도 챙린지에 참여하실 생각은 있으신가요??",
          "original_form": "앞으로 재미로라도 챙린지에 참여하실 생각은 있으신가요??",
          "speaker_id": "1",
          "time": "20211005 14:52"
        },
        {
          "id": "MDRW2100046906.1.25",
          "form": "전혀없습니다!",
          "original_form": "전혀없습니다!",
          "speaker_id": "2",
          "time": "20211005 14:54"
        },
        {
          "id": "MDRW2100046906.1.26",
          "form": "왜냐면 몸치거든요!",
          "original_form": "왜냐면 몸치거든요!",
          "speaker_id": "2",
          "time": "20211005 14:54"
        },
        {
          "id": "MDRW2100046906.1.27",
          "form": "또 챌린지에 참여하실 의향있으신가요??",
          "original_form": "또 챌린지에 참여하실 의향있으신가요??",
          "speaker_id": "2",
          "time": "20211005 14:54"
        },
        {
          "id": "MDRW2100046906.1.28",
          "form": "옙!",
          "original_form": "옙!",
          "speaker_id": "1",
          "time": "20211006 03:46"
        },
        {
          "id": "MDRW2100046906.1.29",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20211006 03:46"
        },
        {
          "id": "MDRW2100046906.1.30",
          "form": "더멋지게 항수닛습니더",
          "original_form": "더멋지게 항수닛습니더",
          "speaker_id": "1",
          "time": "20211006 03:46"
        },
        {
          "id": "MDRW2100046906.1.31",
          "form": "오 도전정신",
          "original_form": "오 도전정신",
          "speaker_id": "2",
          "time": "20211006 09:37"
        },
        {
          "id": "MDRW2100046906.1.32",
          "form": "멋집니다",
          "original_form": "멋집니다",
          "speaker_id": "2",
          "time": "20211006 09:38"
        },
        {
          "id": "MDRW2100046906.1.33",
          "form": "젊잖아요!",
          "original_form": "젊잖아요!",
          "speaker_id": "1",
          "time": "20211006 10:23"
        },
        {
          "id": "MDRW2100046906.1.34",
          "form": "다해보디요뭐",
          "original_form": "다해보디요뭐",
          "speaker_id": "1",
          "time": "20211006 10:23"
        },
        {
          "id": "MDRW2100046906.1.35",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20211006 10:23"
        },
        {
          "id": "MDRW2100046906.1.36",
          "form": "조심하세요 요즘",
          "original_form": "조심하세요 요즘",
          "speaker_id": "1",
          "time": "20211006 10:23"
        },
        {
          "id": "MDRW2100046906.1.37",
          "form": "오토바에 사고로 다치는 사람 많은데",
          "original_form": "오토바에 사고로 다치는 사람 많은데",
          "speaker_id": "1",
          "time": "20211006 10:24"
        },
        {
          "id": "MDRW2100046906.1.38",
          "form": "다치면 챌린지같은거 못하잖아요",
          "original_form": "다치면 챌린지같은거 못하잖아요",
          "speaker_id": "1",
          "time": "20211006 10:24"
        },
        {
          "id": "MDRW2100046906.1.39",
          "form": "오토바이.. 무섭죠",
          "original_form": "오토바이.. 무섭죠",
          "speaker_id": "2",
          "time": "20211006 13:27"
        },
        {
          "id": "MDRW2100046906.1.40",
          "form": "틱톡 챌린지는 유형이 많아서 재밌는것같아요",
          "original_form": "틱톡 챌린지는 유형이 많아서 재밌는것같아요",
