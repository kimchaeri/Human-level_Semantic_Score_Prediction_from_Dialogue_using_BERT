      "utterance": [
        {
          "id": "MDRW2100026483.1.1",
          "form": "패션탱고티레몬에이드 피지오가 젤 존맛인거같아요",
          "original_form": "패션탱고티레몬에이드 피지오가 젤 존맛인거같아요",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026483.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이름이 어렵네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ이름이 어렵네요",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026483.1.3",
          "form": "에이드 들 중에 최애에요!",
          "original_form": "에이드 들 중에 최애에요!",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026483.1.4",
          "form": "저 처음 들어봐료",
          "original_form": "저 처음 들어봐료",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026483.1.5",
          "form": "저도 말할때 힘듭니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 말할때 힘듭니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026483.1.6",
          "form": "오 저 담에 한 번 꼭 먹어 볼래요",
          "original_form": "오 저 담에 한 번 꼭 먹어 볼래요",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026483.1.7",
          "form": "좋아요 ㅎㅎ",
          "original_form": "좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026483.1.8",
          "form": "다른 피지오도 인기많지만 제 최애 ㅎㅎ",
          "original_form": "다른 피지오도 인기많지만 제 최애 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026483.1.9",
          "form": "가서 이름 안씹게 얀습 좀 해야겠는데요",
          "original_form": "가서 이름 안씹게 얀습 좀 해야겠는데요",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026483.1.10",
          "form": "버벅이다가 혀 세번 씹을 것 같음데",
          "original_form": "버벅이다가 혀 세번 씹을 것 같음데",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026483.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026483.1.12",
          "form": "패션탱고티피지오라고하세욬ㅋㅋㅋㅋ",
          "original_form": "패션탱고티피지오라고하세욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026483.1.13",
          "form": "레몬에이드는 빼도갠춘합니닼ㅋㅋㅋㅋ",
          "original_form": "레몬에이드는 빼도갠춘합니닼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026483.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026483.1.15",
          "form": "팁 감사합니다",
          "original_form": "팁 감사합니다",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026483.1.16",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026483.1.17",
          "form": "혹시 추천하고싶은 음료가 있으신가용?",
          "original_form": "혹시 추천하고싶은 음료가 있으신가용?",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026483.1.18",
          "form": "추천 음료.. 제가 워낙 막입이라 가리는게 없는데",
          "original_form": "추천 음료.. 제가 워낙 막입이라 가리는게 없는데",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026483.1.19",
          "form": "이게 또 그만큼 유별나게 좋아하는 것도 없어요",
          "original_form": "이게 또 그만큼 유별나게 좋아하는 것도 없어요",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026483.1.20",
          "form": "아하 ㅠㅠㅋㅋ 아쉽네요",
          "original_form": "아하 ㅠㅠㅋㅋ 아쉽네요",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026483.1.21",
          "form": "최근에 뭔가 맛있다고 생각한건",
          "original_form": "최근에 뭔가 맛있다고 생각한건",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026483.1.22",
          "form": "호불호 갈릴것 갘긴한데 텐퍼센트에 크림치즈 딸게라떼??",
          "original_form": "호불호 갈릴것 갘긴한데 텐퍼센트에 크림치즈 딸게라떼??",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026483.1.23",
          "form": "오우.. 매우 호불호갈리겠어요",
          "original_form": "오우.. 매우 호불호갈리겠어요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.24",
          "form": "추천으로 먹어봤는데 생각보다 맛있어라구요",
          "original_form": "추천으로 먹어봤는데 생각보다 맛있어라구요",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.25",
          "form": "제가 딸기를 생딸기만 먹어서...",
          "original_form": "제가 딸기를 생딸기만 먹어서...",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇죠",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.27",
          "form": "절대안먹겠네욬ㅋㅋㅋㅋㅋ",
          "original_form": "절대안먹겠네욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.28",
          "form": "저도 처음엔 이게 무슨...? 했는데",
          "original_form": "저도 처음엔 이게 무슨...? 했는데",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.29",
          "form": "생각보단 괜찮았어요",
          "original_form": "생각보단 괜찮았어요",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.31",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.32",
          "form": "유일한 추천이...ㅠ",
          "original_form": "유일한 추천이...ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.33",
          "form": "오홍 친구먹여보고싶네요 ㅜㅜ",
          "original_form": "오홍 친구먹여보고싶네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.34",
          "form": "딸기 좋아하는 친구에게 추천할께요!",
          "original_form": "딸기 좋아하는 친구에게 추천할께요!",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026483.1.35",
          "form": "오! 좋아요!!",
          "original_form": "오! 좋아요!!",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026483.1.36",
          "form": "친구분이 맘에 들어하셨으면 좋겠네요",
          "original_form": "친구분이 맘에 들어하셨으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026483.1.37",
          "form": "네 ㅎㅎ 맘에들어할거같아요!",
          "original_form": "네 ㅎㅎ 맘에들어할거같아요!",
