      "utterance": [
        {
          "id": "MDRW2100047227.1.1",
          "form": "직장인이신가용?",
          "original_form": "직장인이신가용?",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100047227.1.2",
          "form": "네네 ㅎname1님은요?",
          "original_form": "네네 ㅎ&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100047227.1.3",
          "form": "전 아직 학생이욤ㅎ",
          "original_form": "전 아직 학생이욤ㅎ",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100047227.1.4",
          "form": "학교 선배림으로써 조언해주고 싶으신거 있나요?",
          "original_form": "학교 선배림으로써 조언해주고 싶으신거 있나요?",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100047227.1.5",
          "form": "아 좋을때네요ㅎㅎ",
          "original_form": "아 좋을때네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100047227.1.6",
          "form": "학교는 방학중이죠?",
          "original_form": "학교는 방학중이죠?",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100047227.1.7",
          "form": "네! 담주 개강이에요ㅠ",
          "original_form": "네! 담주 개강이에요ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100047227.1.8",
          "form": "아 우울하시겠어요ㅎ",
          "original_form": "아 우울하시겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100047227.1.9",
          "form": "대면 수업인가요",
          "original_form": "대면 수업인가요",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100047227.1.10",
          "form": "비대면이 연장되긴 했는데",
          "original_form": "비대면이 연장되긴 했는데",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100047227.1.11",
          "form": "후에 대면할지는 모르겠어요",
          "original_form": "후에 대면할지는 모르겠어요",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100047227.1.12",
          "form": "시험은 대면으로 보죠?",
          "original_form": "시험은 대면으로 보죠?",
          "speaker_id": "2",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100047227.1.13",
          "form": "교수님마다 다르실 것같아요",
          "original_form": "교수님마다 다르실 것같아요",
          "speaker_id": "1",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100047227.1.14",
          "form": "비대면이면 감독을어찌해요?",
          "original_form": "비대면이면 감독을어찌해요?",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100047227.1.15",
          "form": "줌으로 본인과 책상을 비춰서 하기도 하고요",
          "original_form": "줌으로 본인과 책상을 비춰서 하기도 하고요",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100047227.1.16",
          "form": "그냥 온라인으로 주어진 시간 안에 내거나",
          "original_form": "그냥 온라인으로 주어진 시간 안에 내거나",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100047227.1.17",
          "form": "줌으로 실시간 시험 치기도 해요",
          "original_form": "줌으로 실시간 시험 치기도 해요",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100047227.1.18",
          "form": "에어팟끼고 서로짜고그럴수도있자나요",
          "original_form": "에어팟끼고 서로짜고그럴수도있자나요",
          "speaker_id": "2",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100047227.1.19",
          "form": "ㅎㅎ 컨닝은 진짜 기상천외한방법다쓰던데",
          "original_form": "ㅎㅎ 컨닝은 진짜 기상천외한방법다쓰던데",
          "speaker_id": "2",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100047227.1.20",
          "form": "교수님도 아시는데 그냥 내시는 분도 있고 그거 방지하려고 줌 키는 것도 있고 그런 것같아요ㅋㅋㅋ",
          "original_form": "교수님도 아시는데 그냥 내시는 분도 있고 그거 방지하려고 줌 키는 것도 있고 그런 것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:54"
        }
      ]
