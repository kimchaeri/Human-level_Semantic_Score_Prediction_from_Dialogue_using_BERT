      "utterance": [
        {
          "id": "MDRW2100015747.1.1",
          "form": "최근에 구매한 물건 있으세요?",
          "original_form": "최근에 구매한 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210826 00:46"
        },
        {
          "id": "MDRW2100015747.1.2",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015747.1.3",
          "form": "태블릿샀어요",
          "original_form": "태블릿샀어요",
          "speaker_id": "2",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015747.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100015747.1.5",
          "form": "저는 아이패드 사고 싶어서 찾아보고 있어요",
          "original_form": "저는 아이패드 사고 싶어서 찾아보고 있어요",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100015747.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100015747.1.7",
          "form": "저는 애플꺼를 별로 안쓰다보니 태블릿도 그냥 갤럭시탭 샀어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 애플꺼를 별로 안쓰다보니 태블릿도 그냥 갤럭시탭 샀어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015747.1.8",
          "form": "어떤 용도로 사셨어요??",
          "original_form": "어떤 용도로 사셨어요??",
          "speaker_id": "1",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015747.1.9",
          "form": "원래 아이패드 쓰긴 했었는데",
          "original_form": "원래 아이패드 쓰긴 했었는데",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015747.1.10",
          "form": "아이폰을 안쓰니까... 굳이인거 같아서",
          "original_form": "아이폰을 안쓰니까... 굳이인거 같아서",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015747.1.11",
          "form": "저 그냥 영상보거나",
          "original_form": "저 그냥 영상보거나",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015747.1.12",
          "form": "pdf넣어보거나",
          "original_form": "pdf넣어보거나",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015747.1.13",
          "form": "필기용이요",
          "original_form": "필기용이요",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015747.1.14",
          "form": "그리고 가끔 들고다니면서 서류작성하기도 괜찮아요",
          "original_form": "그리고 가끔 들고다니면서 서류작성하기도 괜찮아요",
          "speaker_id": "2",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015747.1.15",
          "form": "아하 저도 공부는 안하는데..",
          "original_form": "아하 저도 공부는 안하는데..",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015747.1.16",
          "form": "공부용으로 사고 싶은? 아세요?..ㅎㅎ",
          "original_form": "공부용으로 사고 싶은? 아세요?..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015747.1.17",
          "form": "알죸ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "알죸ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015747.1.18",
          "form": "애플제품 많이 사시면 아이패드가 낫고.. 아니면 이번 갤탭 진짜 괜찮아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "애플제품 많이 사시면 아이패드가 낫고.. 아니면 이번 갤탭 진짜 괜찮아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015747.1.19",
          "form": "새로 신상 사셨나봐요?!",
          "original_form": "새로 신상 사셨나봐요?!",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015747.1.20",
          "form": "넹 s7+샀어요!",
          "original_form": "넹 s7+샀어요!",
          "speaker_id": "2",
          "time": "20210826 00:56"
        },
        {
          "id": "MDRW2100015747.1.21",
          "form": "우와 좋으시겠다!ㅎㅎ",
          "original_form": "우와 좋으시겠다!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:57"
        },
        {
          "id": "MDRW2100015747.1.22",
          "form": "어...그런데 용도가 공부쪽이시라",
          "original_form": "어...그런데 용도가 공부쪽이시라",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015747.1.23",
          "form": "필기 많이 하실거같으면",
          "original_form": "필기 많이 하실거같으면",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015747.1.24",
          "form": "아이패드쪽이 나을수도 있을거같긴해요...",
          "original_form": "아이패드쪽이 나을수도 있을거같긴해요...",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015747.1.25",
          "form": "애플펜슬이 뭔가 더 좋아보여서...",
          "original_form": "애플펜슬이 뭔가 더 좋아보여서...",
          "speaker_id": "2",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015747.1.26",
          "form": "아하ㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015747.1.27",
          "form": "공부많니 하고 싶은데..안해요ㅎㅎ",
          "original_form": "공부많니 하고 싶은데..안해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015747.1.28",
          "form": "중고 거래도 하신적 있으세요?",
          "original_form": "중고 거래도 하신적 있으세요?",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015747.1.29",
          "form": "ㅎㅎ... 저도....",
          "original_form": "ㅎㅎ... 저도....",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015747.1.30",
          "form": "음 중고거래는",
          "original_form": "음 중고거래는",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
