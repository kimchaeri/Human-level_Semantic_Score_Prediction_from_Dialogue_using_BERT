      "utterance": [
        {
          "id": "MDRW2100039506.1.1",
          "form": "안녕하세요! 전 마라탕 시켜먹었어요!",
          "original_form": "안녕하세요! 전 마라탕 시켜먹었어요!",
          "speaker_id": "1",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100039506.1.2",
          "form": "머라구요 몇단계 시켜드셨나요",
          "original_form": "머라구요 몇단계 시켜드셨나요",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100039506.1.3",
          "form": "4단계 먹었습니당",
          "original_form": "4단계 먹었습니당",
          "speaker_id": "1",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100039506.1.4",
          "form": "짱 맛있어용",
          "original_form": "짱 맛있어용",
          "speaker_id": "1",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100039506.1.5",
          "form": "헐 매운거 좋아하시나봐요 저는 1단계ㅋㅋㅋ",
          "original_form": "헐 매운거 좋아하시나봐요 저는 1단계ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039506.1.6",
          "form": "마라탕에 뭐뭐 넣어드세요?",
          "original_form": "마라탕에 뭐뭐 넣어드세요?",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039506.1.7",
          "form": "저는 떡종류랑 고기랑... 배추랑 청경채랑 새우 오징어요!",
          "original_form": "저는 떡종류랑 고기랑... 배추랑 청경채랑 새우 오징어요!",
          "speaker_id": "2",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039506.1.8",
          "form": "저 탄수화물처돌이라ㅋㅋㅋㅋ당면류 완전 많이 넣어요!!",
          "original_form": "저 탄수화물처돌이라ㅋㅋㅋㅋ당면류 완전 많이 넣어요!!",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039506.1.9",
          "form": "헐 당면도 맛있는데 불면...ㅋㅋㅋㅋ",
          "original_form": "헐 당면도 맛있는데 불면...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039506.1.10",
          "form": "밥도 말아드시나요...? 저는 말아먹어용 맛있더라구요",
          "original_form": "밥도 말아드시나요...? 저는 말아먹어용 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039506.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 청경채도 좋아해요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 청경채도 좋아해요!!",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039506.1.12",
          "form": "헉 당연하죠!!",
          "original_form": "헉 당연하죠!!",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039506.1.13",
          "form": "안 말아먹으면 진짜",
          "original_form": "안 말아먹으면 진짜",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039506.1.14",
          "form": "뭔가 부족해요ㅋㅋㅋㅋ",
          "original_form": "뭔가 부족해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100039506.1.15",
          "form": "맞아요 그래서 친구들이 그래서 마라탕이냐고ㅋㅋㅋ 그랬었어요ㅜ",
          "original_form": "맞아요 그래서 친구들이 그래서 마라탕이냐고ㅋㅋㅋ 그랬었어요ㅜ",
          "speaker_id": "2",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100039506.1.16",
          "form": "4단계는 근데 진짜 매울거같아요",
          "original_form": "4단계는 근데 진짜 매울거같아요",
          "speaker_id": "2",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100039506.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 그 향신료? 향이 너무 좋아서ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 그 향신료? 향이 너무 좋아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100039506.1.18",
          "form": "자꾸 매운거 먹게 되네용ㅋㅋㅋ",
          "original_form": "자꾸 매운거 먹게 되네용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100039506.1.19",
          "form": "헉 그렇구나ㅜㅜ... 그래도 위에 안 좋으니 적당히 드세용.. ",
          "original_form": "헉 그렇구나ㅜㅜ... 그래도 위에 안 좋으니 적당히 드세용.. ",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100039506.1.20",
          "form": "ㅋㅋㅋ안그래두 끊으려구요!!ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ안그래두 끊으려구요!!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039506.1.21",
          "form": "그래서 오늘저녁은",
          "original_form": "그래서 오늘저녁은",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039506.1.22",
          "form": "상추쌈 싸먹으려구요!!",
          "original_form": "상추쌈 싸먹으려구요!!",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100039506.1.23",
          "form": "안돼요 끊지는 마시구ㅋㅋㅋㅋㅋㅋ",
          "original_form": "안돼요 끊지는 마시구ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039506.1.24",
          "form": "마라탕을 상추쌈이요...?",
          "original_form": "마라탕을 상추쌈이요...?",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039506.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앗 아니 저녁밥이요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앗 아니 저녁밥이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039506.1.26",
          "form": "마라탕은 점심이었어요!!",
          "original_form": "마라탕은 점심이었어요!!",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039506.1.27",
          "form": "아 너무 웃겨요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 너무 웃겨요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100039506.1.28",
          "form": "아!! 맛있게 드세요 저는 김치찜ㅋㅋㅋㅋ",
          "original_form": "아!! 맛있게 드세요 저는 김치찜ㅋㅋㅋㅋ",
