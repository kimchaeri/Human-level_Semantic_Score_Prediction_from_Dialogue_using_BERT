      "utterance": [
        {
          "id": "MDRW2100037615.1.1",
          "form": "저는 오늘도 다이어트 및 디톡스 하겠다며 레디톡 48시간 먹었어요 하하",
          "original_form": "저는 오늘도 다이어트 및 디톡스 하겠다며 레디톡 48시간 먹었어요 하하",
          "speaker_id": "1",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100037615.1.2",
          "form": "ㅜㅇㅜ 진짜 48시간이 길기도 기네요",
          "original_form": "ㅜㅇㅜ 진짜 48시간이 길기도 기네요",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100037615.1.3",
          "form": "맛은 안 물리나요",
          "original_form": "맛은 안 물리나요",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100037615.1.4",
          "form": "맛은 계속 맛있어요",
          "original_form": "맛은 계속 맛있어요",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037615.1.5",
          "form": "아니 먹는게 이거뿐이라 당연한걸지도 ㅋㅋ",
          "original_form": "아니 먹는게 이거뿐이라 당연한걸지도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037615.1.6",
          "form": "아ㅋ  그래도 다행이다",
          "original_form": "아ㅋ  그래도 다행이다",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037615.1.7",
          "form": "ㅎㅎ 배고픈 건 좀 참을만하세요?",
          "original_form": "ㅎㅎ 배고픈 건 좀 참을만하세요?",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037615.1.8",
          "form": "너무 허기지면 생각이 없어진다고도 하던데",
          "original_form": "너무 허기지면 생각이 없어진다고도 하던데",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037615.1.9",
          "form": "낮에는 진짜 우울했고요",
          "original_form": "낮에는 진짜 우울했고요",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100037615.1.10",
          "form": "이제는 약간 제로의 영역을 넘어섰어요 ㅋㅋ",
          "original_form": "이제는 약간 제로의 영역을 넘어섰어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100037615.1.11",
          "form": "아ㅎㅎ 득도의 단계시군요",
          "original_form": "아ㅎㅎ 득도의 단계시군요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037615.1.12",
          "form": "48시간이 보는 제가 다 기네요",
          "original_form": "48시간이 보는 제가 다 기네요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037615.1.13",
          "form": "이제 보식을 고민중이에요",
          "original_form": "이제 보식을 고민중이에요",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037615.1.14",
          "form": "대체 어느정도만 먹어야할것인가 ㅋㅋ",
          "original_form": "대체 어느정도만 먹어야할것인가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037615.1.15",
          "form": "ㅎㅎ 치팅데이인가요",
          "original_form": "ㅎㅎ 치팅데이인가요",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037615.1.16",
          "form": "뭐 드실지 후보는 있나요!",
          "original_form": "뭐 드실지 후보는 있나요!",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037615.1.17",
          "form": "근데 일반식 먹으면 솔직히 그대로 돌아올거같아요",
          "original_form": "근데 일반식 먹으면 솔직히 그대로 돌아올거같아요",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037615.1.18",
          "form": "아직 몸무게 안재보긴했지만ㅜ",
          "original_form": "아직 몸무게 안재보긴했지만ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037615.1.19",
          "form": "어...그러면 업체에 고소해야 할 지경 아닌가요",
          "original_form": "어...그러면 업체에 고소해야 할 지경 아닌가요",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100037615.1.20",
          "form": "48시간짜리를 내놨으면 애프터 말로라도 안내해야지!",
          "original_form": "48시간짜리를 내놨으면 애프터 말로라도 안내해야지!",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100037615.1.21",
          "form": "몸보신하면서 살 안찌는 거 어렵네요ㅜ",
          "original_form": "몸보신하면서 살 안찌는 거 어렵네요ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100037615.1.22",
          "form": "사실 스스로도 크게 변화를 못느껴서ㄱㅋ",
          "original_form": "사실 스스로도 크게 변화를 못느껴서ㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037615.1.23",
          "form": "그래도 낼아침에 재보긴하려고요ㅜㅜ",
          "original_form": "그래도 낼아침에 재보긴하려고요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037615.1.24",
          "form": "ㅎㅎ 체중은 재보셨나요",
          "original_form": "ㅎㅎ 체중은 재보셨나요",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037615.1.25",
          "form": "아 아직",
          "original_form": "아 아직",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037615.1.26",
          "form": "시작전 껀 재봤어요 ㅋㅋ비포  몸무게랄까",
          "original_form": "시작전 껀 재봤어요 ㅋㅋ비포  몸무게랄까",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037615.1.27",
          "form": "효과 있길 빌어요!",
          "original_form": "효과 있길 빌어요!",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037615.1.28",
          "form": "아ㅎㅎ그건 재야죠ㅎ",
          "original_form": "아ㅎㅎ그건 재야죠ㅎ",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037615.1.29",
          "form": "이틀 완전 쫄쫄 굶었으니 이키로는 빠지지 않을까요",
          "original_form": "이틀 완전 쫄쫄 굶었으니 이키로는 빠지지 않을까요",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100037615.1.30",
          "form": "그정도를 원하고는 있어요ㅜ",
          "original_form": "그정도를 원하고는 있어요ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037615.1.31",
          "form": "하 김치전은 보식으로 안되겠져ㅡㅜㅜ",
          "original_form": "하 김치전은 보식으로 안되겠져ㅡㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037615.1.32",
          "form": "진짜 수영장 가기전 바짝 살빼기 용이네요",
          "original_form": "진짜 수영장 가기전 바짝 살빼기 용이네요",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037615.1.33",
          "form": "김치전ㅎㅎㅎ",
          "original_form": "김치전ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037615.1.34",
          "form": "아닠 그건 보식이 아니잖아욧1",
          "original_form": "아닠 그건 보식이 아니잖아욧1",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037615.1.35",
          "form": "역시 누룽지 끓여 먹어야하나",
          "original_form": "역시 누룽지 끓여 먹어야하나",
          "speaker_id": "1",
          "time": "20210901 21:30"
        }
      ]
