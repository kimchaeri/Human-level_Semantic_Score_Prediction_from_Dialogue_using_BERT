        {
          "id": "MDRW2100021748.1.1",
          "form": "필요할때 하는 편이죠",
          "original_form": "필요할때 하는 편이죠",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021748.1.2",
          "form": "쇼핑 자주 하세요?",
          "original_form": "쇼핑 자주 하세요?",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021748.1.3",
          "form": "쇼핑 좋아해요!",
          "original_form": "쇼핑 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021748.1.4",
          "form": "최근에 구매한 거 있나요?",
          "original_form": "최근에 구매한 거 있나요?",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021748.1.5",
          "form": "신발이요!",
          "original_form": "신발이요!",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021748.1.6",
          "form": "좋은거 사셨나요?",
          "original_form": "좋은거 사셨나요?",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021748.1.7",
          "form": "나이키꺼 샀어요ㅎㅎ",
          "original_form": "나이키꺼 샀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021748.1.8",
          "form": "좋은거네요ㅋㅋㅋ",
          "original_form": "좋은거네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021748.1.9",
          "form": "최근에 뭐사셨나요",
          "original_form": "최근에 뭐사셨나요",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021748.1.10",
          "form": "저는 옷이요",
          "original_form": "저는 옷이요",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021748.1.11",
          "form": "쓰던 쇼핑몰에서 할인하길래ㅋㅋ",
          "original_form": "쓰던 쇼핑몰에서 할인하길래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021748.1.12",
          "form": "오 가을옷 사셨군요",
          "original_form": "오 가을옷 사셨군요",
          "speaker_id": "1",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021748.1.13",
          "form": "네ㅎㅎ 근데 아직 입기엔 덥더라구요",
          "original_form": "네ㅎㅎ 근데 아직 입기엔 덥더라구요",
          "speaker_id": "2",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021748.1.14",
          "form": "이제 곧 서늘해지면ㅎㅎ",
          "original_form": "이제 곧 서늘해지면ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021748.1.15",
