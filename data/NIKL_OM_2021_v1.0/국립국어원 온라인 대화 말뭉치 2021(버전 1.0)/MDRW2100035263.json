      "utterance": [
        {
          "id": "MDRW2100035263.1.1",
          "form": "회사생활은 만족하시나요",
          "original_form": "회사생활은 만족하시나요",
          "speaker_id": "1",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100035263.1.2",
          "form": "전혀요ㅜㅜ",
          "original_form": "전혀요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100035263.1.3",
          "form": "가슴에 사표 품고 삽니다",
          "original_form": "가슴에 사표 품고 삽니다",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100035263.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100035263.1.5",
          "form": "비슷합니다..적성에 안맞으시나요?",
          "original_form": "비슷합니다..적성에 안맞으시나요?",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100035263.1.6",
          "form": "ㅎㅎ 일은 적성인데요",
          "original_form": "ㅎㅎ 일은 적성인데요",
          "speaker_id": "2",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100035263.1.7",
          "form": "위에서 적성 외 일까지 시켜서요",
          "original_form": "위에서 적성 외 일까지 시켜서요",
          "speaker_id": "2",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100035263.1.8",
          "form": "name1님 일은 적성인가요?",
          "original_form": "&name1&님 일은 적성인가요?",
          "speaker_id": "2",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100035263.1.9",
          "form": "음 저는 적성에 맞는면도 있은데 인간관계가 어렵네요",
          "original_form": "음 저는 적성에 맞는면도 있은데 인간관계가 어렵네요",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100035263.1.10",
          "form": "그쵸ㅜ사실 회사서 가장 힘든건 일이 아니라 관계성인 듯",
          "original_form": "그쵸ㅜ사실 회사서 가장 힘든건 일이 아니라 관계성인 듯",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100035263.1.11",
          "form": "이직 생각은 있으세요?",
          "original_form": "이직 생각은 있으세요?",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100035263.1.12",
          "form": "이직은 쉽지 않아서 마음만 가득하네요",
          "original_form": "이직은 쉽지 않아서 마음만 가득하네요",
          "speaker_id": "1",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035263.1.13",
          "form": "저도요...이달 말까지 공고가 있는데 아직도 고민이네요",
          "original_form": "저도요...이달 말까지 공고가 있는데 아직도 고민이네요",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100035263.1.14",
          "form": "어떤 공고요?",
          "original_form": "어떤 공고요?",
          "speaker_id": "1",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100035263.1.15",
          "form": "아 채용공고요ㅜ",
          "original_form": "아 채용공고요ㅜ",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035263.1.16",
          "form": "근데 연봉메리트가 별로 없는 상황서 이직 리스크를 짊어지는 게 부담이라 고민중이에요",
          "original_form": "근데 연봉메리트가 별로 없는 상황서 이직 리스크를 짊어지는 게 부담이라 고민중이에요",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035263.1.17",
          "form": "이직이 쉬운 일이 아니죠 잘 선택하시길 응원합니다",
          "original_form": "이직이 쉬운 일이 아니죠 잘 선택하시길 응원합니다",
          "speaker_id": "1",
          "time": "20210830 05:42"
        },
        {
          "id": "MDRW2100035263.1.18",
          "form": "감사합니다. 이틀간 더 고민해보려구요",
          "original_form": "감사합니다. 이틀간 더 고민해보려구요",
          "speaker_id": "2",
          "time": "20210830 10:04"
        },
        {
          "id": "MDRW2100035263.1.19",
          "form": "name1님은 그곳이 첫 직장이신가요?",
          "original_form": "&name1&님은 그곳이 첫 직장이신가요?",
          "speaker_id": "2",
          "time": "20210830 10:05"
        },
        {
          "id": "MDRW2100035263.1.20",
          "form": "네 첫 직장입니다",
          "original_form": "네 첫 직장입니다",
