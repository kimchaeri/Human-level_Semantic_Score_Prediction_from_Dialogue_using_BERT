      "utterance": [
        {
          "id": "MDRW2100039196.1.1",
          "form": "티비라;;; 지나간거",
          "original_form": "티비라;;; 지나간거",
          "speaker_id": "2",
          "time": "20210903 14:27"
        },
        {
          "id": "MDRW2100039196.1.2",
          "form": "지금 고백부부",
          "original_form": "지금 고백부부",
          "speaker_id": "2",
          "time": "20210903 14:27"
        },
        {
          "id": "MDRW2100039196.1.3",
          "form": "니는?",
          "original_form": "니는?",
          "speaker_id": "2",
          "time": "20210903 14:27"
        },
        {
          "id": "MDRW2100039196.1.4",
          "form": "ㅋㅋㅋ 고백부부",
          "original_form": "ㅋㅋㅋ 고백부부",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039196.1.5",
          "form": "나 슬의만 보고있지",
          "original_form": "나 슬의만 보고있지",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039196.1.6",
          "form": "꼬꼬무도 가끔씩 보고",
          "original_form": "꼬꼬무도 가끔씩 보고",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039196.1.7",
          "form": "그래ㅇ꼬꼬무는 본다",
          "original_form": "그래ㅇ꼬꼬무는 본다",
          "speaker_id": "2",
          "time": "20210903 14:47"
        },
        {
          "id": "MDRW2100039196.1.8",
          "form": "슬의생도보네ㅋ",
          "original_form": "슬의생도보네ㅋ",
          "speaker_id": "2",
          "time": "20210903 14:47"
        },
        {
          "id": "MDRW2100039196.1.9",
          "form": "다보는구만ㅋㅋ",
          "original_form": "다보는구만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100039196.1.10",
          "form": "다보네 ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다보네 ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100039196.1.11",
          "form": "뭐여 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "뭐여 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100039196.1.12",
          "form": "그렇네ㅡㅡ;;",
          "original_form": "그렇네ㅡㅡ;;",
          "speaker_id": "2",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100039196.1.13",
          "form": "어제 기기괴괴 영화봤느뉘",
          "original_form": "어제 기기괴괴 영화봤느뉘",
          "speaker_id": "2",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100039196.1.14",
          "form": "기분나쁜영화였음ㅋ",
          "original_form": "기분나쁜영화였음ㅋ",
          "speaker_id": "2",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100039196.1.15",
          "form": "이름부터 이상한데 언니",
          "original_form": "이름부터 이상한데 언니",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100039196.1.16",
          "form": "뭔지는 모르겠지만",
          "original_form": "뭔지는 모르겠지만",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100039196.1.17",
          "form": "어ㅋ 보지마라ㅋㅋㅋ",
          "original_form": "어ㅋ 보지마라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100039196.1.18",
          "form": "아 안 봐야지ㅋㅋㅋㅋ",
          "original_form": "아 안 봐야지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039196.1.19",
          "form": "어제 슬의 안해서 아쉽네",
          "original_form": "어제 슬의 안해서 아쉽네",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039196.1.20",
          "form": "그래 오늘은 뭐보꼬?",
          "original_form": "그래 오늘은 뭐보꼬?",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039196.1.21",
          "form": "닌 오늘 뭐볼끤데?",
          "original_form": "닌 오늘 뭐볼끤데?",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100039196.1.22",
          "form": "슬의빼고는 보는 게 없다 언니",
          "original_form": "슬의빼고는 보는 게 없다 언니",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039196.1.23",
          "form": "나는 티비 안 봐",
          "original_form": "나는 티비 안 봐",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039196.1.24",
          "form": "슬의도 넷플로 ㅎㅎ",
          "original_form": "슬의도 넷플로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039196.1.25",
          "form": "아하 그렇구먼",
          "original_form": "아하 그렇구먼",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039196.1.26",
          "form": "ㅋㅋ 불금인데 영화라도봐라",
          "original_form": "ㅋㅋ 불금인데 영화라도봐라",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039196.1.27",
          "form": "요새 이거 하느라 쉬지를 못했더니",
          "original_form": "요새 이거 하느라 쉬지를 못했더니",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039196.1.28",
          "form": "너무 피곤하다 언니",
          "original_form": "너무 피곤하다 언니",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039196.1.29",
          "form": "하루 종일 졸린 느낌",
          "original_form": "하루 종일 졸린 느낌",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100039196.1.30",
          "form": "영화는 커녕",
          "original_form": "영화는 커녕",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039196.1.31",
          "form": "유튜브 하나도 못보고 잠",
          "original_form": "유튜브 하나도 못보고 잠",
          "speaker_id": "1",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039196.1.32",
          "form": "그래?ㅋㅋㅈㅋ",
          "original_form": "그래?ㅋㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100039196.1.33",
          "form": "유튜브는 뭐 좋은거있나",
          "original_form": "유튜브는 뭐 좋은거있나",
          "speaker_id": "2",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100039196.1.34",
          "form": "난 오늘영화 또찾아봐야지",
          "original_form": "난 오늘영화 또찾아봐야지",
          "speaker_id": "2",
          "time": "20210903 18:40"
        }
      ]
    }
