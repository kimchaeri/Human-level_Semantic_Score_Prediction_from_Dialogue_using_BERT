      "utterance": [
        {
          "id": "MDRW2100005946.1.1",
          "form": "소개팅은어떤재미인가요",
          "original_form": "소개팅은어떤재미인가요",
          "speaker_id": "2",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100005946.1.2",
          "form": "저도 한번해봐서..",
          "original_form": "저도 한번해봐서..",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005946.1.3",
          "form": "소개팅은 노잼이에여",
          "original_form": "소개팅은 노잼이에여",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005946.1.4",
          "form": "미팅이 재밌지",
          "original_form": "미팅이 재밌지",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005946.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100005946.1.6",
          "form": "미팅해보셨나여",
          "original_form": "미팅해보셨나여",
          "speaker_id": "1",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100005946.1.7",
          "form": "저는 미팅할시기에",
          "original_form": "저는 미팅할시기에",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100005946.1.8",
          "form": "미성년자였어요",
          "original_form": "미성년자였어요",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100005946.1.9",
          "form": "네? 근데 나갔나요?",
          "original_form": "네? 근데 나갔나요?",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100005946.1.10",
          "form": "제가 준법정신이 투철해서요",
          "original_form": "제가 준법정신이 투철해서요",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100005946.1.11",
          "form": "오...멋있네요",
          "original_form": "오...멋있네요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100005946.1.12",
          "form": "롤하는 사람이 이상형인가요?",
          "original_form": "롤하는 사람이 이상형인가요?",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100005946.1.13",
          "form": "공유할 수 있으면 좋지만",
          "original_form": "공유할 수 있으면 좋지만",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100005946.1.14",
          "form": "제가 게임하는거 터치안하는사람이 더좋아요",
          "original_form": "제가 게임하는거 터치안하는사람이 더좋아요",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100005946.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100005946.1.16",
