      "utterance": [
        {
          "id": "MDRW2100035683.1.1",
          "form": "혹시 같이 세대 구성원이 어떻게 되세용?",
          "original_form": "혹시 같이 세대 구성원이 어떻게 되세용?",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035683.1.2",
          "form": "저는 엄마랑 살고있엉ㅅㅇ",
          "original_form": "저는 엄마랑 살고있엉ㅅㅇ",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035683.1.3",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035683.1.4",
          "form": "오! 엄마랑 둘이 사는거 좀 편해요ㅋㅋㅋㅋ",
          "original_form": "오! 엄마랑 둘이 사는거 좀 편해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035683.1.5",
          "form": "원래는 4명이서 살았는데",
          "original_form": "원래는 4명이서 살았는데",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035683.1.6",
          "form": "엄마가 거둬주시지 않았다면 월세살았겠죠ㅜ",
          "original_form": "엄마가 거둬주시지 않았다면 월세살았겠죠ㅜ",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035683.1.7",
          "form": "이런저런 이유로 흩어져서 지내요ㅋㅋㅋ",
          "original_form": "이런저런 이유로 흩어져서 지내요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035683.1.8",
          "form": "저는 혼자 나가살고싶은데",
          "original_form": "저는 혼자 나가살고싶은데",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035683.1.9",
          "form": "저도 누구는 타지역 가고 이러다보니ㅋㅋ",
          "original_form": "저도 누구는 타지역 가고 이러다보니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035683.1.10",
          "form": "안놔주시네요ㅋㅋㅋㅋ",
          "original_form": "안놔주시네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035683.1.11",
          "form": "ㅋㅋㅋ안놔주실때",
          "original_form": "ㅋㅋㅋ안놔주실때",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035683.1.12",
          "form": "최대한 눌러앉아 사십시오",
          "original_form": "최대한 눌러앉아 사십시오",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035683.1.13",
          "form": "이제 나이가 서른인데 좀 혼자 살아보고싶어요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "이제 나이가 서른인데 좀 혼자 살아보고싶어요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035683.1.14",
          "form": "약간 뼈있는 충고네욬ㅋㅋㅋㅋ",
          "original_form": "약간 뼈있는 충고네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035683.1.15",
          "form": "오 혹시 자취 안해보셨나요",
          "original_form": "오 혹시 자취 안해보셨나요",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035683.1.16",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.17",
          "form": "그럼 그러실수도 있죠!",
          "original_form": "그럼 그러실수도 있죠!",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.18",
          "form": "혼자 살아본적이",
          "original_form": "혼자 살아본적이",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.19",
          "form": "없어서",
          "original_form": "없어서",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.20",
          "form": "그런 로망이 있어요",
          "original_form": "그런 로망이 있어요",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.21",
          "form": "저는 9년정도 혼자 살았는데요",
          "original_form": "저는 9년정도 혼자 살았는데요",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.22",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.23",
          "form": "대단하시다",
          "original_form": "대단하시다",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.24",
          "form": "자립심이 많이 키워지셨나용?",
          "original_form": "자립심이 많이 키워지셨나용?",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.25",
          "form": "집안일 잘하시면 혼자 사는거 좋고요",
          "original_form": "집안일 잘하시면 혼자 사는거 좋고요",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035683.1.26",
          "form": "엇 청소하는거 좋아해서 나름 괜찮을것같네욬ㅋㅋㅋㅋ",
          "original_form": "엇 청소하는거 좋아해서 나름 괜찮을것같네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:23"
        }
      ]
    }
  ]
