        {
          "id": "MDRW2100045616.1.2",
          "form": "최근에 쇼핑 뭐했어?",
          "original_form": "최근에 쇼핑 뭐했어?",
          "speaker_id": "1",
          "time": "20210908 04:14"
        },
        {
          "id": "MDRW2100045616.1.3",
          "form": "나 어제 삼다수 잔뜩 샀어 ㅋㅋ",
          "original_form": "나 어제 삼다수 잔뜩 샀어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:14"
        },
        {
          "id": "MDRW2100045616.1.4",
          "form": "또 생수 쟁이기야?",
          "original_form": "또 생수 쟁이기야?",
          "speaker_id": "1",
          "time": "20210908 04:15"
        },
        {
          "id": "MDRW2100045616.1.5",
          "form": "응 그 많은걸 다마셨네",
          "original_form": "응 그 많은걸 다마셨네",
          "speaker_id": "2",
          "time": "20210908 04:15"
        },
        {
          "id": "MDRW2100045616.1.6",
          "form": "ㅋㅋㅋ삼다수 요즘 할인해?",
          "original_form": "ㅋㅋㅋ삼다수 요즘 할인해?",
          "speaker_id": "1",
          "time": "20210908 04:15"
        },
        {
          "id": "MDRW2100045616.1.7",
          "form": "응 무라벨 출시 기념으로",
          "original_form": "응 무라벨 출시 기념으로",
          "speaker_id": "2",
          "time": "20210908 04:16"
        },
        {
          "id": "MDRW2100045616.1.8",
          "form": "오 무라벨 나왔어?",
          "original_form": "오 무라벨 나왔어?",
          "speaker_id": "1",
          "time": "20210908 04:16"
        },
        {
          "id": "MDRW2100045616.1.9",
          "form": "어 분리수거 쉽겠더라",
          "original_form": "어 분리수거 쉽겠더라",
          "speaker_id": "2",
          "time": "20210908 04:16"
        },
        {
          "id": "MDRW2100045616.1.10",
          "form": "나도 그런 제품 사야겠다",
          "original_form": "나도 그런 제품 사야겠다",
          "speaker_id": "1",
          "time": "20210908 04:16"
        },
        {
          "id": "MDRW2100045616.1.11",
          "form": "환경에 진짜 좋겠다",
          "original_form": "환경에 진짜 좋겠다",
          "speaker_id": "2",
          "time": "20210908 04:16"
        },
        {
          "id": "MDRW2100045616.1.12",
          "form": "아이시스도 무라벨 판대!",
          "original_form": "아이시스도 무라벨 판대!",
          "speaker_id": "1",
          "time": "20210908 04:16"
        },
        {
          "id": "MDRW2100045616.1.13",
          "form": "근데 삼다수가 맛있긴하지",
          "original_form": "근데 삼다수가 맛있긴하지",
