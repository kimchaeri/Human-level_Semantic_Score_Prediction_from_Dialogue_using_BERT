      "utterance": [
        {
          "id": "MDRW2100017982.1.1",
          "form": "넷플릭스에서 떡볶이 영화 보셧나요?",
          "original_form": "넷플릭스에서 떡볶이 영화 보셧나요?",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017982.1.2",
          "form": "그런 영화가 있나요??!!",
          "original_form": "그런 영화가 있나요??!!",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017982.1.3",
          "form": "ㅋㅋㅋ 없어서 그냥 물어봤어여..",
          "original_form": "ㅋㅋㅋ 없어서 그냥 물어봤어여..",
          "speaker_id": "1",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017982.1.4",
          "form": "넷플릭스 하시나염?",
          "original_form": "넷플릭스 하시나염?",
          "speaker_id": "1",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017982.1.5",
          "form": "아 있는 줄 알았네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 있는 줄 알았네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017982.1.6",
          "form": "네 맨날 봐요 ㅋㅋㅋ",
          "original_form": "네 맨날 봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017982.1.7",
          "form": "어떤거 보세요? 도라마?",
          "original_form": "어떤거 보세요? 도라마?",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017982.1.8",
          "form": "네 영화도 보긴 하는데 드라마 위주로 봐요",
          "original_form": "네 영화도 보긴 하는데 드라마 위주로 봐요",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017982.1.9",
          "form": "모던패밀리아세요?",
          "original_form": "모던패밀리아세요?",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017982.1.10",
          "form": "미제? 아...",
          "original_form": "미제? 아...",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017982.1.11",
          "form": "저도 그거 앞 시즌 봤어요",
          "original_form": "저도 그거 앞 시즌 봤어요",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017982.1.12",
          "form": "엄청 웃기죠.. ㅋㅋㅋㅋ",
          "original_form": "엄청 웃기죠.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017982.1.13",
          "form": "네 ㅋㅋㅋㅋ 재밌더라구요",
          "original_form": "네 ㅋㅋㅋㅋ 재밌더라구요",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017982.1.14",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017982.1.15",
          "form": "저도 미제나 영국제 드라마 많이 봐요..",
          "original_form": "저도 미제나 영국제 드라마 많이 봐요..",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017982.1.16",
          "form": "최근엔....",
          "original_form": "최근엔....",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017982.1.17",
          "form": "미드가 재밌더라구여 저도 ㅋㅋㅋ",
          "original_form": "미드가 재밌더라구여 저도 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017982.1.18",
          "form": "아.. 그 fbi 심리학 태동기 이야기.. 제목이..",
          "original_form": "아.. 그 fbi 심리학 태동기 이야기.. 제목이..",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017982.1.19",
          "form": "시즌2까지 나오고.. 연쇄살인자 나오는..",
          "original_form": "시즌2까지 나오고.. 연쇄살인자 나오는..",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017982.1.20",
          "form": "미국거인가요??",
          "original_form": "미국거인가요??",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017982.1.21",
