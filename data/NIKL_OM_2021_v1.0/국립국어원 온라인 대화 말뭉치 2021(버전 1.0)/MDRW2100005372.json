      "utterance": [
        {
          "id": "MDRW2100005372.1.1",
          "form": "address은 비와요?",
          "original_form": "&address&은 비와요?",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.2",
          "form": "여긴 비 안오는데~",
          "original_form": "여긴 비 안오는데~",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.3",
          "form": "비그침",
          "original_form": "비그침",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.4",
          "form": "바람도그침",
          "original_form": "바람도그침",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.5",
          "form": "담주에 비가 장난없던데..",
          "original_form": "담주에 비가 장난없던데..",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.6",
          "form": "이제 가을오긋다ㅋㅋ",
          "original_form": "이제 가을오긋다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.7",
          "form": "벌씨로 걱정이네요ㅠ",
          "original_form": "벌씨로 걱정이네요ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.8",
          "form": "멘붕",
          "original_form": "멘붕",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.9",
          "form": "살빼야는데 ㅋㅋㅋㅋ",
          "original_form": "살빼야는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005372.1.10",
          "form": "가을되믄 청바지 입어야는데 ㅋㅋㅋ",
          "original_form": "가을되믄 청바지 입어야는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005372.1.11",
          "form": "나도ㅜ 추워지기전에",
          "original_form": "나도ㅜ 추워지기전에",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005372.1.12",
          "form": "난 겨울옷 안맞지싶다",
          "original_form": "난 겨울옷 안맞지싶다",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005372.1.13",
          "form": "살빼고샀는디ㅡㅡ",
          "original_form": "살빼고샀는디ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005372.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ옷을 두겹 입었다 생각하고 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ옷을 두겹 입었다 생각하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005372.1.15",
          "form": "ㅋㅋ 겨울되기전에 3키로 빼보자",
          "original_form": "ㅋㅋ 겨울되기전에 3키로 빼보자",
          "speaker_id": "2",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005372.1.16",
          "form": "진짜 안빠짐..",
          "original_form": "진짜 안빠짐..",
          "speaker_id": "1",
          "time": "20210821 22:06"
        },
        {
          "id": "MDRW2100005372.1.17",
          "form": "비오고나니 시원하네",
          "original_form": "비오고나니 시원하네",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005372.1.18",
          "form": "낮에 에어컨틀었었는데",
          "original_form": "낮에 에어컨틀었었는데",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005372.1.19",
          "form": "그니깐여~여긴 추워요ㅠ",
          "original_form": "그니깐여~여긴 추워요ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005372.1.20",
          "form": "난도 집에섬 에어컨키는데..",
          "original_form": "난도 집에섬 에어컨키는데..",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005372.1.21",
          "form": "ㅋ 설치니 더더움",
          "original_form": "ㅋ 설치니 더더움",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005372.1.22",
          "form": "난 여기서 방바닥에 붙어있음요 ㅋㅋㅋ",
          "original_form": "난 여기서 방바닥에 붙어있음요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005372.1.23",
          "form": "집에가믄 할일이 산더미ㅠ",
          "original_form": "집에가믄 할일이 산더미ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005372.1.24",
          "form": "아하;;;",
          "original_form": "아하;;;",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005372.1.25",
          "form": "난도 일은 많다",
          "original_form": "난도 일은 많다",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005372.1.26",
          "form": "가을옷도 사야되고~",
          "original_form": "가을옷도 사야되고~",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005372.1.27",
          "form": "옷정리하면 또 덥겠져?ㅋㅋ",
          "original_form": "옷정리하면 또 덥겠져?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005372.1.28",
          "form": "가을옷좋지ㅋㅋ",
          "original_form": "가을옷좋지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005372.1.29",
          "form": "이제 안속는다 캄서 ㅋㅋㅋ",
          "original_form": "이제 안속는다 캄서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005372.1.30",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005372.1.31",
          "form": "미리하면 꼭 ㄷ 번세번해야..ㅍ",
          "original_form": "미리하면 꼭 ㄷ 번세번해야..ㅍ",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005372.1.32",
          "form": "윽시 오타나는구만요 ㅋㅋ",
          "original_form": "윽시 오타나는구만요 ㅋㅋ",
