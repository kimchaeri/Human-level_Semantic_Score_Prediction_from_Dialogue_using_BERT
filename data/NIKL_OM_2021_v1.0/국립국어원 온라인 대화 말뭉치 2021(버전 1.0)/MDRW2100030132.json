      "utterance": [
        {
          "id": "MDRW2100030132.1.1",
          "form": "둘이 잘돌아 다녀요 ㅎㅎㅎ",
          "original_form": "둘이 잘돌아 다녀요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100030132.1.2",
          "form": "ㅎㅎ 왜요 같이가서 재미있게 드라이브도 하시고ㅎ",
          "original_form": "ㅎㅎ 왜요 같이가서 재미있게 드라이브도 하시고ㅎ",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100030132.1.3",
          "form": "아아~ㅎ 그래도 애기가 친구 같아서 좋겟어요ㅎㅎ",
          "original_form": "아아~ㅎ 그래도 애기가 친구 같아서 좋겟어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030132.1.4",
          "form": "네네 ㅎㅎ  요즘 땡깡애 늘어서",
          "original_form": "네네 ㅎㅎ  요즘 땡깡애 늘어서",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030132.1.5",
          "form": "조금 힘들때도 잇지만 ㅋ",
          "original_form": "조금 힘들때도 잇지만 ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030132.1.6",
          "form": "ㅎㅎ 다 그렇죠~ㅎ\r\n저희애도 그래요 ㅎㅎ",
          "original_form": "ㅎㅎ 다 그렇죠~ㅎ\r\n저희애도 그래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030132.1.7",
          "form": "요즘 울보ㅠㅠ",
          "original_form": "요즘 울보ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030132.1.8",
          "form": "어쩔수가없죠ㅎ 애들은 엄마가 가장 좋다고하잖아요ㅎ 가장편하고ㅎㅎ",
          "original_form": "어쩔수가없죠ㅎ 애들은 엄마가 가장 좋다고하잖아요ㅎ 가장편하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100030132.1.9",
          "form": "요즘 제일 잘하는말이",
          "original_form": "요즘 제일 잘하는말이",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030132.1.10",
          "form": "엄마는 엄마맘대로만하고",
          "original_form": "엄마는 엄마맘대로만하고",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030132.1.11",
          "form": "내가 하는건 다 안돼 ㅜㅁ ㅜ 엉엉",
          "original_form": "내가 하는건 다 안돼 ㅜㅁ ㅜ 엉엉",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030132.1.12",
          "form": "반복 ㅋ",
          "original_form": "반복 ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100030132.1.13",
          "form": "아이고 ...",
          "original_form": "아이고 ...",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030132.1.14",
          "form": "속상하시겠네요ㅠ 우리애도 조만간 그럴듯해요 ㅠ",
          "original_form": "속상하시겠네요ㅠ 우리애도 조만간 그럴듯해요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030132.1.15",
          "form": "글구 보니 어른들이.하지말라는게 많죠 ㅎㅎ",
          "original_form": "글구 보니 어른들이.하지말라는게 많죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030132.1.16",
          "form": "위험해서 안돼",
          "original_form": "위험해서 안돼",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100030132.1.17",
          "form": "더러운 ㄱ ㅓ야 안돼",
          "original_form": "더러운 ㄱ ㅓ야 안돼",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100030132.1.18",
          "form": "맞아요ㅎ 엄마는 걱정되서 하는건데 ..\r\n아이는 잘몰라주죠ㅠㅠ",
          "original_form": "맞아요ㅎ 엄마는 걱정되서 하는건데 ..\r\n아이는 잘몰라주죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100030132.1.19",
          "form": "저도 맨날 그래요ㅎ 그래도 남자아이라 잘삐치지는 않잖아요 ㅎ 근데 우리아이는 ... 엄청 삐침...ㅎㅎ 가서 달래줘야하고\r\n맨날 백설공주래요 ㅠ",
          "original_form": "저도 맨날 그래요ㅎ 그래도 남자아이라 잘삐치지는 않잖아요 ㅎ 근데 우리아이는 ... 엄청 삐침...ㅎㅎ 가서 달래줘야하고\r\n맨날 백설공주래요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030132.1.20",
          "form": "ㅎㅎㅎㅎ 귀엽네요",
          "original_form": "ㅎㅎㅎㅎ 귀엽네요",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030132.1.21",
          "form": "저 배터리 없는지도 모르구 ㅋㅋㅋ",
          "original_form": "저 배터리 없는지도 모르구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030132.1.22",
          "form": "휴대폰 꺼져서 당황요 ㅋ",
          "original_form": "휴대폰 꺼져서 당황요 ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030132.1.23",
          "form": "아아~ㅎㅎ 괜찬항요ㅎㅎ",
          "original_form": "아아~ㅎㅎ 괜찬항요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030132.1.24",
          "form": "저 수다떨고 있었음ㅎㅎ",
          "original_form": "저 수다떨고 있었음ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030132.1.25",
          "form": "ㅎㅎㅎ요즘 충전하는것도 잘 잊어버리네요 ㅋ",
