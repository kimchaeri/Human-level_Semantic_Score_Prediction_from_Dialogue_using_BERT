      "utterance": [
        {
          "id": "MDRW2100014202.1.1",
          "form": "한옥수영장이 뭐야",
          "original_form": "한옥수영장이 뭐야",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014202.1.2",
          "form": "여기가 예능프로에도 나왔다고 하더라",
          "original_form": "여기가 예능프로에도 나왔다고 하더라",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014202.1.3",
          "form": "그럼 사람들 많이 오겠다",
          "original_form": "그럼 사람들 많이 오겠다",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014202.1.4",
          "form": "경기도 양주에 있고",
          "original_form": "경기도 양주에 있고",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014202.1.5",
          "form": "전라도가 아니네",
          "original_form": "전라도가 아니네",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014202.1.6",
          "form": "응 전라도가 아닌 경기도",
          "original_form": "응 전라도가 아닌 경기도",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014202.1.7",
          "form": "잘못 갈뻔했네",
          "original_form": "잘못 갈뻔했네",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014202.1.8",
          "form": "ㅋㅋ여기가 전통한옥이랑 수영장을 하는데",
          "original_form": "ㅋㅋ여기가 전통한옥이랑 수영장을 하는데",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014202.1.9",
          "form": "펜션인가보네",
          "original_form": "펜션인가보네",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014202.1.10",
          "form": "한옥펜션이래 독특하다 가볼만 할듯",
          "original_form": "한옥펜션이래 독특하다 가볼만 할듯",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014202.1.11",
          "form": "응 완전 이쁘겠다",
          "original_form": "응 완전 이쁘겠다",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014202.1.12",
          "form": "그렇지 한옥도 이쁜데 수영장까지 있으니",
          "original_form": "그렇지 한옥도 이쁜데 수영장까지 있으니",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014202.1.13",
          "form": "여긴 진짜 한빈 가봐야겠다",
          "original_form": "여긴 진짜 한빈 가봐야겠다",
          "speaker_id": "2",
