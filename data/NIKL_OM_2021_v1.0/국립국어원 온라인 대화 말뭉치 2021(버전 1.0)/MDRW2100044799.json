      "utterance": [
        {
          "id": "MDRW2100044799.1.1",
          "form": "나 막걸리 좀 달달한거만 먹어ㅋㅋ",
          "original_form": "나 막걸리 좀 달달한거만 먹어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044799.1.2",
          "form": "꿀막걸리 이런거?",
          "original_form": "꿀막걸리 이런거?",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044799.1.3",
          "form": "엉엉ㅋㅋ 글고 사이다 타먹거나 그런거ㅋㅋ",
          "original_form": "엉엉ㅋㅋ 글고 사이다 타먹거나 그런거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044799.1.4",
          "form": "아 나는 요구르트 타먹는거 좋아해 ㅋㅋ",
          "original_form": "아 나는 요구르트 타먹는거 좋아해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044799.1.5",
          "form": "아 맛있겠다ㅋㅋ",
          "original_form": "아 맛있겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044799.1.6",
          "form": "그냥은 나도 잘 안마셔",
          "original_form": "그냥은 나도 잘 안마셔",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044799.1.7",
          "form": "근데 막걸리가 숙취가 심하지 않아?ㅋㅋ",
          "original_form": "근데 막걸리가 숙취가 심하지 않아?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044799.1.8",
          "form": "나는 숙취있을만큼 잘 안마셔서 모르겠어 ㅋㅋ 숙취가 심한가?",
          "original_form": "나는 숙취있을만큼 잘 안마셔서 모르겠어 ㅋㅋ 숙취가 심한가?",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044799.1.9",
          "form": "나는 막걸리만 먹으면 엄청 머리 아프더라구ㅋㅋ",
          "original_form": "나는 막걸리만 먹으면 엄청 머리 아프더라구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044799.1.10",
          "form": "사이다랑 섞어서 마셔서 그런걸수도",
          "original_form": "사이다랑 섞어서 마셔서 그런걸수도",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044799.1.11",
          "form": "그냥 막걸리만 먹었을때도 좀 그렇더라구",
          "original_form": "그냥 막걸리만 먹었을때도 좀 그렇더라구",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044799.1.12",
          "form": "아하..원래 좀 숙취를 발생시키나봐 곡주라 그런가?",
          "original_form": "아하..원래 좀 숙취를 발생시키나봐 곡주라 그런가?",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044799.1.13",
          "form": "엉엉 그런거같애ㅋㅋ",
          "original_form": "엉엉 그런거같애ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044799.1.14",
          "form": "나는 막걸리 먹으면 꼭 전이 땡기더라",
          "original_form": "나는 막걸리 먹으면 꼭 전이 땡기더라",
