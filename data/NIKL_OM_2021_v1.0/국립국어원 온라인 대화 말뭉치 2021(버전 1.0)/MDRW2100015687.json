        },
        {
          "id": "MDRW2100015687.1.2",
          "form": "네 강아지요ㅎ",
          "original_form": "네 강아지요ㅎ",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015687.1.3",
          "form": "이름이 뭐예요?",
          "original_form": "이름이 뭐예요?",
          "speaker_id": "2",
