      "utterance": [
        {
          "id": "MDRW2100009814.1.1",
          "form": "스포츠 좋아해여?",
          "original_form": "스포츠 좋아해여?",
          "speaker_id": "1",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009814.1.2",
          "form": "스포츠 에 따라 달라요",
          "original_form": "스포츠 에 따라 달라요",
          "speaker_id": "2",
          "time": "20210823 10:28"
        },
        {
          "id": "MDRW2100009814.1.3",
          "form": "축구는?",
          "original_form": "축구는?",
          "speaker_id": "1",
          "time": "20210823 10:28"
        },
        {
          "id": "MDRW2100009814.1.4",
          "form": "월드컵때만 가끔 봐요",
          "original_form": "월드컵때만 가끔 봐요",
          "speaker_id": "2",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009814.1.5",
          "form": "축구 좋아하세요?",
          "original_form": "축구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009814.1.6",
          "form": "보는것만요",
          "original_form": "보는것만요",
          "speaker_id": "1",
          "time": "20210823 10:44"
        },
        {
          "id": "MDRW2100009814.1.7",
          "form": "하는 건 너무 힘들죠\r\n특히 축구는...",
          "original_form": "하는 건 너무 힘들죠\r\n특히 축구는...",
          "speaker_id": "2",
          "time": "20210823 10:45"
        },
        {
          "id": "MDRW2100009814.1.8",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210823 10:45"
        },
        {
          "id": "MDRW2100009814.1.9",
          "form": "좋아하는 축구선수 있어요?",
          "original_form": "좋아하는 축구선수 있어요?",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100009814.1.10",
          "form": "이영표요",
          "original_form": "이영표요",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100009814.1.11",
          "form": "해설을 하니까 사람들과 적이 된다고 그만두신 분이시죠  많은 명언을 남기셨는데",
          "original_form": "해설을 하니까 사람들과 적이 된다고 그만두신 분이시죠  많은 명언을 남기셨는데",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100009814.1.12",
          "form": "대단해요",
          "original_form": "대단해요",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100009814.1.13",
          "form": "그죠 대단한 분이시죠\r\n실전을 바탕으로 예측도 잘하시고\r\n해결책도 제시하시고",
          "original_form": "그죠 대단한 분이시죠\r\n실전을 바탕으로 예측도 잘하시고\r\n해결책도 제시하시고",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100009814.1.14",
          "form": "네 정확하죠",
          "original_form": "네 정확하죠",
          "speaker_id": "1",
          "time": "20210825 16:02"
        }
      ]
