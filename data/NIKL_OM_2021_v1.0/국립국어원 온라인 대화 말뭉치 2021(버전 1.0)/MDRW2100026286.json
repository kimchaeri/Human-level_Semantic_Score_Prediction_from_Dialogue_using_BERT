      "utterance": [
        {
          "id": "MDRW2100026286.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026286.1.2",
          "form": "요즘 계속 비오지 않아?",
          "original_form": "요즘 계속 비오지 않아?",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026286.1.3",
          "form": "비 계속 와",
          "original_form": "비 계속 와",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026286.1.4",
          "form": "비오는날 좋아해?",
          "original_form": "비오는날 좋아해?",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026286.1.5",
          "form": "비올때 나는 냄새는 좋아하는데 비오는날 나가는건 안좋아해",
          "original_form": "비올때 나는 냄새는 좋아하는데 비오는날 나가는건 안좋아해",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026286.1.6",
          "form": "비맞는거 좋아해?",
          "original_form": "비맞는거 좋아해?",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026286.1.7",
          "form": "아니 안좋아하지 ㅋㅋ",
          "original_form": "아니 안좋아하지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026286.1.8",
          "form": "양말젖고 우산무거워서 안좋아해",
          "original_form": "양말젖고 우산무거워서 안좋아해",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026286.1.9",
          "form": "나도ㅋㅋㅋ",
          "original_form": "나도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026286.1.10",
          "form": "그래도 날씨는 안더워졌어",
          "original_form": "그래도 날씨는 안더워졌어",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026286.1.11",
          "form": "맞아 비오고나서는",
          "original_form": "맞아 비오고나서는",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026286.1.12",
          "form": "확 시원해졌어",
          "original_form": "확 시원해졌어",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026286.1.13",
          "form": "이제는 밤에 잘 때 춥더라",
          "original_form": "이제는 밤에 잘 때 춥더라",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026286.1.14",
          "form": "장마인가? 작년이 진짜 안더웠는데",
          "original_form": "장마인가? 작년이 진짜 안더웠는데",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026286.1.15",
          "form": "올해여름은 비안와서 덥더라",
          "original_form": "올해여름은 비안와서 덥더라",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026286.1.16",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026286.1.17",
          "form": "한여름에는 너무 더웠는데",
          "original_form": "한여름에는 너무 더웠는데",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026286.1.18",
          "form": "좀 빨리 비오지",
          "original_form": "좀 빨리 비오지",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026286.1.19",
          "form": "그니까 뭔가 기후가 이상해 ㅋㅋ",
          "original_form": "그니까 뭔가 기후가 이상해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026286.1.20",
          "form": "가을 좋아해?",
          "original_form": "가을 좋아해?",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026286.1.21",
          "form": "응 완전 좋아해",
          "original_form": "응 완전 좋아해",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026286.1.22",
          "form": "딱 중간 날씨라서 너는 가을 좋아해?",
          "original_form": "딱 중간 날씨라서 너는 가을 좋아해?",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026286.1.23",
          "form": "나도 좋아해 봄가을 좋아해",
          "original_form": "나도 좋아해 봄가을 좋아해",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026286.1.24",
          "form": "반팔이좋아 긴팔이 좋아",
          "original_form": "반팔이좋아 긴팔이 좋아",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026286.1.25",
          "form": "긴팔이 더 좋은거같아",
          "original_form": "긴팔이 더 좋은거같아",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026286.1.26",
          "form": "햇빛에 타는걸 안좋아해서",
          "original_form": "햇빛에 타는걸 안좋아해서",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026286.1.27",
          "form": "너는 긴팔이 좋아 반팔이 좋아?",
          "original_form": "너는 긴팔이 좋아 반팔이 좋아?",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026286.1.28",
          "form": "오 나도 긴팔",
          "original_form": "오 나도 긴팔",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026286.1.29",
          "form": "빨리 반팔입고싶어",
          "original_form": "빨리 반팔입고싶어",
          "speaker_id": "2",
          "time": "20210901 21:28"
        }
      ]
    }
  ]
