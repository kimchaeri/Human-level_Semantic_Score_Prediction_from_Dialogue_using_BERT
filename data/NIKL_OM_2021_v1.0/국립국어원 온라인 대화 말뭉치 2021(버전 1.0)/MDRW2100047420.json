      "utterance": [
        {
          "id": "MDRW2100047420.1.1",
          "form": "요세 극장 가셨어요?",
          "original_form": "요세 극장 가셨어요?",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100047420.1.2",
          "form": "아니요 코로나이후에 극장은 한번도 못갓어요 ㅠㅠ",
          "original_form": "아니요 코로나이후에 극장은 한번도 못갓어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047420.1.3",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047420.1.4",
          "form": "영화는굉장히 좋아하는데ㅠㅠ",
          "original_form": "영화는굉장히 좋아하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047420.1.5",
          "form": "저도 진짜 맨날갔어요",
          "original_form": "저도 진짜 맨날갔어요",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047420.1.6",
          "form": "vip였어요",
          "original_form": "vip였어요",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047420.1.7",
          "form": "이젠 일반 ㅜㅜ",
          "original_form": "이젠 일반 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047420.1.8",
          "form": "오 영화정말 좋아하셧구나ㅠㅠ",
          "original_form": "오 영화정말 좋아하셧구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100047420.1.9",
          "form": "극장못가는게 젤슬파요",
          "original_form": "극장못가는게 젤슬파요",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.11",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.12",
          "form": "액션요",
          "original_form": "액션요",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.13",
          "form": "저도극장너무좋아햇는데ㅠㅠ",
          "original_form": "저도극장너무좋아햇는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.14",
          "form": "오오 액션!!",
          "original_form": "오오 액션!!",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.15",
          "form": "네 요번에 샹치",
          "original_form": "네 요번에 샹치",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.16",
          "form": "기다려요",
          "original_form": "기다려요",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.17",
          "form": "마블",
          "original_form": "마블",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100047420.1.18",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100047420.1.19",
          "form": "저도 마블 좋아해요",
          "original_form": "저도 마블 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100047420.1.20",
          "form": "그런데 블랙위도우 아직 못봄 ㅠㅠ",
          "original_form": "그런데 블랙위도우 아직 못봄 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100047420.1.21",
          "form": "전 그거 보다 말았어요",
          "original_form": "전 그거 보다 말았어요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100047420.1.22",
          "form": "바빠서 중간보다가",
          "original_form": "바빠서 중간보다가",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100047420.1.23",
          "form": "앗 별로엿나요?",
          "original_form": "앗 별로엿나요?",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100047420.1.24",
          "form": "다시 주말에 보게요",
          "original_form": "다시 주말에 보게요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100047420.1.25",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100047420.1.26",
          "form": "음 그냥 그래요",
          "original_form": "음 그냥 그래요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100047420.1.27",
          "form": "마블시리즈중에 뭘 제일 좋아하세요?",
          "original_form": "마블시리즈중에 뭘 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.28",
          "form": "막엄청 재밌진 않아요 ㅋㅋ",
          "original_form": "막엄청 재밌진 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.29",
          "form": "어벤저스 가디언즈",
          "original_form": "어벤저스 가디언즈",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.30",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.31",
          "form": "저도 가오갤 좋아해요",
          "original_form": "저도 가오갤 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.32",
          "form": "아이언맨도 ㅋㅋ",
          "original_form": "아이언맨도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.33",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.34",
          "form": "아이언맨도 좋죠",
          "original_form": "아이언맨도 좋죠",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.35",
          "form": "저 다좋아해요 ㅋㅋ",
          "original_form": "저 다좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.36",
          "form": "저도 다 좋아해요",
          "original_form": "저도 다 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100047420.1.37",
          "form": "앤트맨도 좋아해요",
          "original_form": "앤트맨도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100047420.1.38",
          "form": "토르는 라그나로크",
          "original_form": "토르는 라그나로크",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100047420.1.39",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100047420.1.40",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100047420.1.41",
          "form": "라그나로크 너무 좋죠 ㅎㅎ",
          "original_form": "라그나로크 너무 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100047420.1.42",
          "form": "네 그전시리즈는 지루했어요",
          "original_form": "네 그전시리즈는 지루했어요",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100047420.1.43",
          "form": "캡틴마블도 그럭저럭 괜찮게봣는데",
          "original_form": "캡틴마블도 그럭저럭 괜찮게봣는데",
          "speaker_id": "1",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100047420.1.44",
          "form": "다른시리즈보다는 덜 재밋엇던것같아요",
          "original_form": "다른시리즈보다는 덜 재밋엇던것같아요",
          "speaker_id": "1",
          "time": "20210826 15:28"
