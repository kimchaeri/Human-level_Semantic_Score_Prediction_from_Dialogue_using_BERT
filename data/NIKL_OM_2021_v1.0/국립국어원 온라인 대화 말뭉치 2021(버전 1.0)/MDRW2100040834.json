      "utterance": [
        {
          "id": "MDRW2100040834.1.1",
          "form": "저는 사실 부모님과 살아서 요리할 일이 많진 않고",
          "original_form": "저는 사실 부모님과 살아서 요리할 일이 많진 않고",
          "speaker_id": "1",
          "time": "20210905 09:30"
        },
        {
          "id": "MDRW2100040834.1.2",
          "form": "가끔 베이킹하거나 밀키트 조리하는 정도",
          "original_form": "가끔 베이킹하거나 밀키트 조리하는 정도",
          "speaker_id": "1",
          "time": "20210905 09:30"
        },
        {
          "id": "MDRW2100040834.1.3",
          "form": "베이킹!!! 어떤거요??",
          "original_form": "베이킹!!! 어떤거요??",
          "speaker_id": "2",
          "time": "20210905 09:32"
        },
        {
          "id": "MDRW2100040834.1.4",
          "form": "지난주엔 큐브파운드 만들어봤어요",
          "original_form": "지난주엔 큐브파운드 만들어봤어요",
          "speaker_id": "1",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100040834.1.5",
          "form": "우왕 여기 금손님이 계셨네요!",
          "original_form": "우왕 여기 금손님이 계셨네요!",
          "speaker_id": "2",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040834.1.6",
          "form": "앗 아니예요",
          "original_form": "앗 아니예요",
          "speaker_id": "1",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040834.1.7",
          "form": "레시피 써있는대로",
          "original_form": "레시피 써있는대로",
          "speaker_id": "1",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040834.1.8",
          "form": "어설프게 따라서 베이킹하는 정도예요",
          "original_form": "어설프게 따라서 베이킹하는 정도예요",
          "speaker_id": "1",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040834.1.9",
          "form": "오븐이 없어 시도해보지 않았지만 일단... 요리를.. 못해요 ㅎㅎㅎ",
          "original_form": "오븐이 없어 시도해보지 않았지만 일단... 요리를.. 못해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040834.1.10",
          "form": "사과 깎기만 해도",
          "original_form": "사과 깎기만 해도",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040834.1.11",
          "form": "조각한다고 그러는걸요 ㅠ",
          "original_form": "조각한다고 그러는걸요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040834.1.12",
          "form": "칼질도 잘 못해서 베이킹 잘하는 분들 보면 부러워요",
          "original_form": "칼질도 잘 못해서 베이킹 잘하는 분들 보면 부러워요",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040834.1.13",
          "form": "또 어떤거 해보셨나요?",
          "original_form": "또 어떤거 해보셨나요?",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100040834.1.14",
          "form": "저도 과일 깎으면 잔소리 많이 들어요ㅜ",
          "original_form": "저도 과일 깎으면 잔소리 많이 들어요ㅜ",
          "speaker_id": "1",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100040834.1.15",
          "form": "그래서 귤을 제일 잘깝니다 ㅋㅋ",
          "original_form": "그래서 귤을 제일 잘깝니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040834.1.16",
          "form": "칼 안써도 되는 과일이 최고에요 ㅠㅠ",
          "original_form": "칼 안써도 되는 과일이 최고에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040834.1.17",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040834.1.18",
          "form": "바나나 귤",
          "original_form": "바나나 귤",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040834.1.19",
          "form": "딸기 포도",
          "original_form": "딸기 포도",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040834.1.20",
          "form": "역시 뭘 좀 아시는군요 ><ㅋㅋ",
          "original_form": "역시 뭘 좀 아시는군요 ><ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100040834.1.21",
          "form": "ㅎㅎ과일은 먹기 편한 게 최고죠",
          "original_form": "ㅎㅎ과일은 먹기 편한 게 최고죠",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100040834.1.22",
          "form": "어떤 과일 좋아하세요?",
          "original_form": "어떤 과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100040834.1.23",
          "form": "저는 과일 대부분 좋아해요",
          "original_form": "저는 과일 대부분 좋아해요",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100040834.1.24",
          "form": "딸기 자두 귤 망고",
          "original_form": "딸기 자두 귤 망고",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100040834.1.25",
          "form": "대부분 손질하기 편한 것들이네요ㅎㅎ",
          "original_form": "대부분 손질하기 편한 것들이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100040834.1.26",
          "form": "곧 제철이 다가오는 과일이 많네요 ㅎㅎ",
          "original_form": "곧 제철이 다가오는 과일이 많네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040834.1.27",
          "form": "네네~",
          "original_form": "네네~",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040834.1.28",
          "form": "겨울에 따뜻한 집에서 귤 까먹으면",
          "original_form": "겨울에 따뜻한 집에서 귤 까먹으면",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040834.1.29",
          "form": "천국이 따로 없죠",
          "original_form": "천국이 따로 없죠",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040834.1.30",
          "form": "생각만해도 벌써 좋아요 ㅋㅋ",
          "original_form": "생각만해도 벌써 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:47"
        },
        {
          "id": "MDRW2100040834.1.31",
          "form": "네네 얼른 귤 까먹는 계절이 오길",
          "original_form": "네네 얼른 귤 까먹는 계절이 오길",
          "speaker_id": "1",
          "time": "20210905 09:48"
        },
        {
          "id": "MDRW2100040834.1.32",
          "form": "아 sns에서 많이 나왔던",
          "original_form": "아 sns에서 많이 나왔던",
          "speaker_id": "1",
