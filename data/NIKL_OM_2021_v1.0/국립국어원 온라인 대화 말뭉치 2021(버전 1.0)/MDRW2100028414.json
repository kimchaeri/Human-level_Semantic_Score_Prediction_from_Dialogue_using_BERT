        },
        {
          "id": "MDRW2100028414.1.2",
          "form": "최근 다녀온 여행지 중에 마음에 드는 곳은 어디야",
          "original_form": "최근 다녀온 여행지 중에 마음에 드는 곳은 어디야",
          "speaker_id": "1",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028414.1.3",
          "form": "부산",
          "original_form": "부산",
          "speaker_id": "2",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028414.1.4",
          "form": "사유 아름답고 바다가 있음",
          "original_form": "사유 아름답고 바다가 있음",
          "speaker_id": "2",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028414.1.5",
          "form": "부산여행은 며칠이 적당함",
          "original_form": "부산여행은 며칠이 적당함",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028414.1.6",
          "form": "나 너무 짧게 다녀와서",
          "original_form": "나 너무 짧게 다녀와서",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028414.1.7",
          "form": "바다도 제대로 못보고",
          "original_form": "바다도 제대로 못보고",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028414.1.8",
          "form": "횟감골라서 먹은거 빼고는",
          "original_form": "횟감골라서 먹은거 빼고는",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028414.1.9",
          "form": "에바.............. 2박3일 적당한 것 같음 4일은 너무 길어",
          "original_form": "에바.............. 2박3일 적당한 것 같음 4일은 너무 길어",
          "speaker_id": "2",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028414.1.10",
          "form": "서울서 놀때랑 그다지 다를바x",
          "original_form": "서울서 놀때랑 그다지 다를바x",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.11",
          "form": "근데 관광지를",
          "original_form": "근데 관광지를",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.12",
          "form": "잘골라가야돼",
          "original_form": "잘골라가야돼",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.13",
          "form": "하....어렵다",
          "original_form": "하....어렵다",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.14",
          "form": "뭔가 어트랙션 이런거 잘 찾아가야됨",
          "original_form": "뭔가 어트랙션 이런거 잘 찾아가야됨",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.15",
          "form": "케이블카 해변열차 이런",
          "original_form": "케이블카 해변열차 이런",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.16",
          "form": "부산에도 어트랙션이 있음??",
          "original_form": "부산에도 어트랙션이 있음??",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.17",
          "form": "바다특화관광상품",
          "original_form": "바다특화관광상품",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.18",
          "form": "바다에서 배타고 그런건가",
          "original_form": "바다에서 배타고 그런건가",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.19",
          "form": "ㄴㄴ그냥 바다위의 케이블카타고",
          "original_form": "ㄴㄴ그냥 바다위의 케이블카타고",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028414.1.20",
          "form": "아~~ㅋㅋㅋㅋㅋ그런거 맞아 있던듯",
          "original_form": "아~~ㅋㅋㅋㅋㅋ그런거 맞아 있던듯",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.21",
          "form": "전에 탐라돌았던 바다를 잔뜩보는 열차? 뭐그런거",
          "original_form": "전에 탐라돌았던 바다를 잔뜩보는 열차? 뭐그런거",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.22",
          "form": "배타는거는... 비쌀듯...",
          "original_form": "배타는거는... 비쌀듯...",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.23",
          "form": "그거도있던데 바다따라도는 열차",
          "original_form": "그거도있던데 바다따라도는 열차",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.24",
          "form": "시간과정신의 방에 갇혀",
          "original_form": "시간과정신의 방에 갇혀",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.25",
          "form": "해변가를 끝없이 도는",
          "original_form": "해변가를 끝없이 도는",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.26",
          "form": "ㅋㅋㅋㅋㅋ아근데 재밌어ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아근데 재밌어ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.27",
          "form": "너무 비싸서 안탔던듯",
          "original_form": "너무 비싸서 안탔던듯",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.28",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.29",
          "form": "그리고 해동용궁사 갔다왔는데 즐겁더라",
          "original_form": "그리고 해동용궁사 갔다왔는데 즐겁더라",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.30",
          "form": "거기에 루지라고",
          "original_form": "거기에 루지라고",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028414.1.31",
          "form": "어른용 카트라이더같은거 있는데 그것도 재밌음",
          "original_form": "어른용 카트라이더같은거 있는데 그것도 재밌음",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
