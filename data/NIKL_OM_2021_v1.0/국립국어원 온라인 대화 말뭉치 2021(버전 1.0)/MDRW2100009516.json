          "id": "MDRW2100009516.1.1",
          "form": "가사 및 가족",
          "original_form": "가사 및 가족",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009516.1.2",
          "form": "내가 또 집안일은 잘하지",
          "original_form": "내가 또 집안일은 잘하지",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009516.1.3",
          "form": "?? 그건 좀..",
          "original_form": "?? 그건 좀..",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009516.1.4",
          "form": "왜 어떤면에서",
          "original_form": "왜 어떤면에서",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009516.1.5",
          "form": "나 정리빼고 청소빼고 물쓰는 거빼고",
          "original_form": "나 정리빼고 청소빼고 물쓰는 거빼고",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009516.1.6",
          "form": "잘함",
          "original_form": "잘함",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009516.1.7",
          "form": "그게 집안일 아님?",
          "original_form": "그게 집안일 아님?",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009516.1.8",
          "form": "집에서 누워있는게 일이면 그것도 집안일이지",
          "original_form": "집에서 누워있는게 일이면 그것도 집안일이지",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009516.1.9",
          "form": "난 매일 집안일을 하고 있었네",
          "original_form": "난 매일 집안일을 하고 있었네",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100009516.1.10",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100009516.1.11",
          "form": "나도 집안일 잘한다니까",
          "original_form": "나도 집안일 잘한다니까",
          "speaker_id": "2",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100009516.1.12",
          "form": "ㅠㅠ 어휴 방청소 해야 되는데",
          "original_form": "ㅠㅠ 어휴 방청소 해야 되는데",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100009516.1.13",
          "form": "너무 하기 싫군",
          "original_form": "너무 하기 싫군",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100009516.1.14",
          "form": "찐집안일이군",
          "original_form": "찐집안일이군",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100009516.1.15",
          "form": "나도 정리 더 해야하는데 귀찮",
          "original_form": "나도 정리 더 해야하는데 귀찮",
          "speaker_id": "2",
          "time": "20210825 11:26"
        },
        {
          "id": "MDRW2100009516.1.16",
          "form": "ㅠㅠ 기숙사 가기 전에 해야되는데",
          "original_form": "ㅠㅠ 기숙사 가기 전에 해야되는데",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100009516.1.17",
          "form": "긱사... 그렇네",
          "original_form": "긱사... 그렇네",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100009516.1.18",
