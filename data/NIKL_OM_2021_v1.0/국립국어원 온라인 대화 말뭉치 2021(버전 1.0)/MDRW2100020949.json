      "utterance": [
        {
          "id": "MDRW2100020949.1.1",
          "form": "가을된줄 알았는데 햇빛 장난 아니네요...",
          "original_form": "가을된줄 알았는데 햇빛 장난 아니네요...",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020949.1.2",
          "form": "맞아요 오늘 날씨 무슨일이래요...!?",
          "original_form": "맞아요 오늘 날씨 무슨일이래요...!?",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100020949.1.3",
          "form": "덥네요 아직",
          "original_form": "덥네요 아직",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100020949.1.4",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100020949.1.5",
          "form": "저는 가을 젤 좋아합니다~",
          "original_form": "저는 가을 젤 좋아합니다~",
          "speaker_id": "2",
          "time": "20210829 06:36"
        },
        {
          "id": "MDRW2100020949.1.6",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100020949.1.7",
          "form": "덥지도 않고 춥지도 않아서요ㅋㅋ",
          "original_form": "덥지도 않고 춥지도 않아서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100020949.1.8",
          "form": "맞아요~ 선선하니넘좋죠",
          "original_form": "맞아요~ 선선하니넘좋죠",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100020949.1.9",
          "form": "앗..비가 엄청 오네요",
          "original_form": "앗..비가 엄청 오네요",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100020949.1.10",
          "form": "아정말요??",
          "original_form": "아정말요??",
          "speaker_id": "2",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100020949.1.11",
          "form": "어디지역에사시나요? 여긴 부산인데 비는안와요",
          "original_form": "어디지역에사시나요? 여긴 부산인데 비는안와요",
          "speaker_id": "2",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100020949.1.12",
          "form": "여긴 서울인데 아직도 오네요",
          "original_form": "여긴 서울인데 아직도 오네요",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100020949.1.13",
          "form": "부산 날씨는 어떤가요 지금??",
          "original_form": "부산 날씨는 어떤가요 지금??",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100020949.1.14",
          "form": "부산은 그냥 되게 덥구 습했어요!",
          "original_form": "부산은 그냥 되게 덥구 습했어요!",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100020949.1.15",
          "form": "그렇군요 여긴 좀 시원하네요",
          "original_form": "그렇군요 여긴 좀 시원하네요",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100020949.1.16",
          "form": "여름이랑 겨울중에 뭐가 더 힘드세요??",
          "original_form": "여름이랑 겨울중에 뭐가 더 힘드세요??",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100020949.1.17",
          "form": "저는 겨울이더힘들어요 ㅜㅜ추위를 느무느무많이타서요 ㅠㅠ",
          "original_form": "저는 겨울이더힘들어요 ㅜㅜ추위를 느무느무많이타서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100020949.1.18",
          "form": "앗 저도요",
          "original_form": "앗 저도요",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100020949.1.19",
          "form": "집 밖으로 나가기 싫어요ㅋㅋ",
          "original_form": "집 밖으로 나가기 싫어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100020949.1.20",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋ 그래서 겨울이안왓으면 좋겟어요 오더라도 좀 빨리지나가기를...",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋ 그래서 겨울이안왓으면 좋겟어요 오더라도 좀 빨리지나가기를...",
          "speaker_id": "2",
