      "utterance": [
        {
          "id": "MDRW2100033820.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 11:32"
        },
        {
          "id": "MDRW2100033820.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210825 11:32"
        },
        {
          "id": "MDRW2100033820.1.3",
          "form": "설거지랑 빨래중에",
          "original_form": "설거지랑 빨래중에",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033820.1.4",
          "form": "뭐가 그나마낫나요?",
          "original_form": "뭐가 그나마낫나요?",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033820.1.5",
          "form": "음 설거지는 식세기 이모님이",
          "original_form": "음 설거지는 식세기 이모님이",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033820.1.6",
          "form": "빨래는 세탁기가",
          "original_form": "빨래는 세탁기가",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033820.1.7",
          "form": "건조는 건조기가ㅋㅋㅋㅋㅋ",
          "original_form": "건조는 건조기가ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033820.1.8",
          "form": "굳이 고르자면 빨래가 더 편하겠네요!",
          "original_form": "굳이 고르자면 빨래가 더 편하겠네요!",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033820.1.9",
          "form": "근데 식세기에",
          "original_form": "근데 식세기에",
          "speaker_id": "2",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100033820.1.10",
          "form": "넣어도 한계가있지않나요?ㅠㅠ",
          "original_form": "넣어도 한계가있지않나요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100033820.1.11",
          "form": "저희는 4인가족인데",
          "original_form": "저희는 4인가족인데",
          "speaker_id": "1",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100033820.1.12",
          "form": "12인용 쓰고 있거든요^^",
          "original_form": "12인용 쓰고 있거든요^^",
          "speaker_id": "1",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100033820.1.13",
          "form": "그래서 그런가\r\n불편함 없이 쓰는중요ㅋ",
          "original_form": "그래서 그런가\r\n불편함 없이 쓰는중요ㅋ",
          "speaker_id": "1",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100033820.1.14",
          "form": "와우 12 인용",
          "original_form": "와우 12 인용",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100033820.1.15",
          "form": "기름때같은건",
          "original_form": "기름때같은건",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100033820.1.16",
          "form": "안닦이죠??",
          "original_form": "안닦이죠??",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100033820.1.17",
          "form": "아뇽ㅋㅋ 아주 잘 닦여요ㅋㅋ",
          "original_form": "아뇽ㅋㅋ 아주 잘 닦여요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100033820.1.18",
          "form": "고온세척 가능하니\r\n오히려 손설거지 하는것보다 나아요",
          "original_form": "고온세척 가능하니\r\n오히려 손설거지 하는것보다 나아요",
          "speaker_id": "1",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100033820.1.19",
          "form": "헐 진짜요? 모르는새에 많이발전했네요",
          "original_form": "헐 진짜요? 모르는새에 많이발전했네요",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100033820.1.20",
          "form": "고기 먹은 그릇도되나요?",
          "original_form": "고기 먹은 그릇도되나요?",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100033820.1.21",
          "form": "따로 수세미로안닦아도?",
          "original_form": "따로 수세미로안닦아도?",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100033820.1.22",
          "form": "식세기에 넣기 전에",
          "original_form": "식세기에 넣기 전에",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100033820.1.23",
          "form": "애.벌 세척하긴 하는데요",
          "original_form": "애.벌 세척하긴 하는데요",
          "speaker_id": "1",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100033820.1.24",
          "form": "그냥 수세미로 한번씩 휙 해주기만 해요ㅋ",
          "original_form": "그냥 수세미로 한번씩 휙 해주기만 해요ㅋ",
          "speaker_id": "1",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100033820.1.25",
          "form": "진짜 세상 편해요",
          "original_form": "진짜 세상 편해요",
          "speaker_id": "1",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100033820.1.26",
          "form": "와우 좋네요",
          "original_form": "와우 좋네요",
          "speaker_id": "2",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100033820.1.27",
          "form": "얼마쯤하나요 12인용은??",
          "original_form": "얼마쯤하나요 12인용은??",
          "speaker_id": "2",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100033820.1.28",
          "form": "저는 임직원가로 구입해서\r\n100만원 조금 안되게 구입했던거 같아요:)",
          "original_form": "저는 임직원가로 구입해서\r\n100만원 조금 안되게 구입했던거 같아요:)",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100033820.1.29",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100033820.1.30",
          "form": "생각보다 엄청비싸네요????",
          "original_form": "생각보다 엄청비싸네요????",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100033820.1.31",
          "form": "오잉?ㅋㅋㅋㅋ완전 핫딜인데요",
          "original_form": "오잉?ㅋㅋㅋㅋ완전 핫딜인데요",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
