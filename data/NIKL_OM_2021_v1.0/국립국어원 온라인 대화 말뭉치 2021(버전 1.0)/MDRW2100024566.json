      "utterance": [
        {
          "id": "MDRW2100024566.1.1",
          "form": "재택근무 하시나요?",
          "original_form": "재택근무 하시나요?",
          "speaker_id": "1",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100024566.1.2",
          "form": "오! 네 ㅋㅋ 근데 한낱 알바생이랍니다ㅜㅋㅋ",
          "original_form": "오! 네 ㅋㅋ 근데 한낱 알바생이랍니다ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100024566.1.3",
          "form": "아 알바하시는구나",
          "original_form": "아 알바하시는구나",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100024566.1.4",
          "form": "저도 예전에 했었어요ㅎ",
          "original_form": "저도 예전에 했었어요ㅎ",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100024566.1.5",
          "form": "아하! 어떤알바하셧나요??",
          "original_form": "아하! 어떤알바하셧나요??",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100024566.1.6",
          "form": "전 피자집서빙이랑",
          "original_form": "전 피자집서빙이랑",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100024566.1.7",
          "form": "과외했었어요",
          "original_form": "과외했었어요",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100024566.1.8",
          "form": "우왕 피자집서빙!!! 어디피자였나여??",
          "original_form": "우왕 피자집서빙!!! 어디피자였나여??",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100024566.1.9",
          "form": "브랜드아닌 수제피자였는데",
          "original_form": "브랜드아닌 수제피자였는데",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100024566.1.10",
          "form": "나름 재미있었던ㅎ",
          "original_form": "나름 재미있었던ㅎ",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100024566.1.11",
          "form": "아하~ ㅋㅋㅋ 오 나름 성격에맞으셨나봐여!",
          "original_form": "아하~ ㅋㅋㅋ 오 나름 성격에맞으셨나봐여!",
          "speaker_id": "2",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100024566.1.12",
          "form": "갑질은 힘들었는데 동료들이랑 재밌었어요ㅎ",
          "original_form": "갑질은 힘들었는데 동료들이랑 재밌었어요ㅎ",
          "speaker_id": "1",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100024566.1.13",
          "form": "아ㅜㅜ갑질 ㅜㅜㅜ오 동료들이랑 친한거는 진짜 편하고좋을거같아요!",
          "original_form": "아ㅜㅜ갑질 ㅜㅜㅜ오 동료들이랑 친한거는 진짜 편하고좋을거같아요!",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100024566.1.14",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100024566.1.15",
          "form": "그덕에 할수있었던거같아요",
          "original_form": "그덕에 할수있었던거같아요",
          "speaker_id": "1",
          "time": "20210902 00:46"
        },
        {
          "id": "MDRW2100024566.1.16",
          "form": "맞아요 동료복도 참 중요한거같아요!",
          "original_form": "맞아요 동료복도 참 중요한거같아요!",
