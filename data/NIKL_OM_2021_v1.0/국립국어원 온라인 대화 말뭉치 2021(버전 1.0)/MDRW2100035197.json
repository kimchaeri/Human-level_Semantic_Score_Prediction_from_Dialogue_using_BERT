      "utterance": [
        {
          "id": "MDRW2100035197.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100035197.1.2",
          "form": "안녕하세요 식사하셨나요?",
          "original_form": "안녕하세요 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100035197.1.3",
          "form": "네 이제 점심 먹으려고 합니니다",
          "original_form": "네 이제 점심 먹으려고 합니니다",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035197.1.4",
          "form": "식사 하셨어요?",
          "original_form": "식사 하셨어요?",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035197.1.5",
          "form": "넵 아침을 늦게 먹어서 점심은 패쓰합니다ㅎㅎ",
          "original_form": "넵 아침을 늦게 먹어서 점심은 패쓰합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035197.1.6",
          "form": "맛있는거 드셨나요 ㅎㅎ",
          "original_form": "맛있는거 드셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035197.1.7",
          "form": "김볶먹었어요!",
          "original_form": "김볶먹었어요!",
          "speaker_id": "2",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100035197.1.8",
          "form": "점심메뉴 뭐예요?",
          "original_form": "점심메뉴 뭐예요?",
          "speaker_id": "2",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100035197.1.9",
          "form": "국밥 포장해올까 싶어요 ㅎㅎ",
          "original_form": "국밥 포장해올까 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100035197.1.10",
          "form": "국밥 좋아하시나요?",
          "original_form": "국밥 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100035197.1.11",
          "form": "국밥 좋죠! 종류 어떤 거예요???",
          "original_form": "국밥 좋죠! 종류 어떤 거예요???",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100035197.1.12",
          "form": "돼지국밥 먹을 것 같아요!",
          "original_form": "돼지국밥 먹을 것 같아요!",
          "speaker_id": "1",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100035197.1.13",
          "form": "날이 흐릿흐릿한게 국밥이 먹고싶어서요 ㅎㅎ",
          "original_form": "날이 흐릿흐릿한게 국밥이 먹고싶어서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100035197.1.14",
          "form": "아 진짜 맛있겠어요! 저도 볶음밥에 오리탕 먹었어요 나이들수록 국물이 꼭 필요해요",
          "original_form": "아 진짜 맛있겠어요! 저도 볶음밥에 오리탕 먹었어요 나이들수록 국물이 꼭 필요해요",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100035197.1.15",
          "form": "요즘에 아침은 또 쌀쌀해서 국물 먹으면 좋은 것 같아요 ㅎㅎ",
          "original_form": "요즘에 아침은 또 쌀쌀해서 국물 먹으면 좋은 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100035197.1.16",
          "form": "복숭아 좋아하시나요?",
          "original_form": "복숭아 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100035197.1.17",
          "form": "복숭아 너무 좋아요 딱복파세요?",
          "original_form": "복숭아 너무 좋아요 딱복파세요?",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100035197.1.18",
          "form": "딱복 물복 안가려요 ㅎㅎ\n있으면 다 먹습니다!",
          "original_form": "딱복 물복 안가려요 ㅎㅎ\n있으면 다 먹습니다!",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100035197.1.19",
          "form": "이제 복숭아철 마지막 시기라서 딱복도 물복처럼 물렁하네요 ㅎㅎ",
          "original_form": "이제 복숭아철 마지막 시기라서 딱복도 물복처럼 물렁하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:30"
        }
      ]
