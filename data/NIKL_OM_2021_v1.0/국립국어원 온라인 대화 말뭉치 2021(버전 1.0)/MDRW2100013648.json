      "utterance": [
        {
          "id": "MDRW2100013648.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013648.1.2",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013648.1.3",
          "form": "전 점심은 시리얼먹어요",
          "original_form": "전 점심은 시리얼먹어요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013648.1.4",
          "form": "스페셜케이ㅋㅋ",
          "original_form": "스페셜케이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013648.1.5",
          "form": "ㅋㅋ전 아침에 시리얼 먹는데",
          "original_form": "ㅋㅋ전 아침에 시리얼 먹는데",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013648.1.6",
          "form": "전 회사에서 간단하게ㅋㅋ",
          "original_form": "전 회사에서 간단하게ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013648.1.7",
          "form": "아 스페셜케이만 드시나요?ㅎㅎ",
          "original_form": "아 스페셜케이만 드시나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013648.1.8",
          "form": "시리얼 추천해주실 거 있으세요?ㅋㅋ",
          "original_form": "시리얼 추천해주실 거 있으세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013648.1.9",
          "form": "크랜베리 들어간거 맛있던데\r\n근데 요즘은 스페셜케이만 먹어요ㅋㅋ",
          "original_form": "크랜베리 들어간거 맛있던데\r\n근데 요즘은 스페셜케이만 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013648.1.10",
          "form": "대용량 사서 사료처럼ㅋㅋ",
          "original_form": "대용량 사서 사료처럼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013648.1.11",
          "form": "ㅋㅋ스페셜케이도 맛있나봐요ㅋㅋ",
          "original_form": "ㅋㅋ스페셜케이도 맛있나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013648.1.12",
          "form": "전 현미 후레이크 이런거 건강식인척 먹어요ㅋㅋㅋ",
          "original_form": "전 현미 후레이크 이런거 건강식인척 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013648.1.13",
          "form": "제친구는 그거 두꺼운도화지 씹는거같대요ㅋㅋ",
          "original_form": "제친구는 그거 두꺼운도화지 씹는거같대요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013648.1.14",
          "form": "네 저도 한끼는 건강식이다 생각하고 그냥먹는거에여ㅋㅋ",
          "original_form": "네 저도 한끼는 건강식이다 생각하고 그냥먹는거에여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013648.1.15",
          "form": "ㅋㅋㅋㅋ도화지라니 ㅋㅋ네 우유랑 같이 먹으면 꽤 맛도 있어요ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ도화지라니 ㅋㅋ네 우유랑 같이 먹으면 꽤 맛도 있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013648.1.16",
          "form": "네 우유랑먹음 단맛나서 먹을만해요",
          "original_form": "네 우유랑먹음 단맛나서 먹을만해요",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013648.1.17",
          "form": "맞아요ㅋㅋ전 지겨우면 초코맛 시리얼도 좀 섞어서 먹고 그래요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ전 지겨우면 초코맛 시리얼도 좀 섞어서 먹고 그래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013648.1.18",
          "form": "근데 점심만 그렇게 먹지 아침적녁은 많이먹어서 의미가 없네요ㅋㅋ",
          "original_form": "근데 점심만 그렇게 먹지 아침적녁은 많이먹어서 의미가 없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013648.1.19",
          "form": "아침에는 뭐드세요?",
          "original_form": "아침에는 뭐드세요?",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013648.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013648.1.21",
          "form": "아침 저녁은 밥으로 야무지게 먹습니다",
          "original_form": "아침 저녁은 밥으로 야무지게 먹습니다",
          "speaker_id": "2",
          "time": "20210825 16:44"
        }
      ]
    }
