        },
        {
          "id": "MDRW2100028855.1.2",
          "form": "마마무 소속사에서갑자기",
          "original_form": "마마무 소속사에서갑자기",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.3",
          "form": "공식포스트를 다 밀어서",
          "original_form": "공식포스트를 다 밀어서",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.4",
          "form": "엥?????",
          "original_form": "엥?????",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.5",
          "form": "알비덥소속가수 포스팅 다밀렸대...",
          "original_form": "알비덥소속가수 포스팅 다밀렸대...",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.6",
          "form": "멤1명 소속사 나갔지만 해체 아니잖아",
          "original_form": "멤1명 소속사 나갔지만 해체 아니잖아",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.7",
          "form": "이유를 모르겠대...",
          "original_form": "이유를 모르겠대...",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.8",
          "form": "아 알비덥 소속 다???",
          "original_form": "아 알비덥 소속 다???",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.9",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.10",
          "form": "거기도 참 일 못해...",
          "original_form": "거기도 참 일 못해...",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.11",
          "form": "원어스 원위 퍼플키스",
          "original_form": "원어스 원위 퍼플키스",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.12",
          "form": "다날아갔다는데..",
          "original_form": "다날아갔다는데..",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028855.1.13",
          "form": "친구가 그 휘인 덬이라",
          "original_form": "친구가 그 휘인 덬이라",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028855.1.14",
          "form": "솔로 나간거는 맘에들어하던데",
          "original_form": "솔로 나간거는 맘에들어하던데",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028855.1.15",
          "form": "라비네 소속사 갔는데",
          "original_form": "라비네 소속사 갔는데",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028855.1.16",
          "form": "직원이 일을 지인짜 잘하나봐",
          "original_form": "직원이 일을 지인짜 잘하나봐",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100028855.1.17",
          "form": "아 새로 간 곳이 라비네야????",
          "original_form": "아 새로 간 곳이 라비네야????",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028855.1.18",
          "form": "거기 영입 많이 하더라",
          "original_form": "거기 영입 많이 하더라",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028855.1.19",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100028855.1.20",
          "form": "거기 에일리랑 휘인 이렇게 나오는데",
          "original_form": "거기 에일리랑 휘인 이렇게 나오는데",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028855.1.21",
          "form": "둘다한테 잘하겠다고 다짐하ㅣ도하고",
          "original_form": "둘다한테 잘하겠다고 다짐하ㅣ도하고",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028855.1.22",
          "form": "공식 인스타 생겼는데",
          "original_form": "공식 인스타 생겼는데",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028855.1.23",
          "form": "주접이 장난아니더랔ㅋㅋㅋ팬보다 잘떰",
          "original_form": "주접이 장난아니더랔ㅋㅋㅋ팬보다 잘떰",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028855.1.24",
          "form": "라비가 힙한씬에서 자리 잡믈려고 힙합하는 애들 많이 영입하던데",
          "original_form": "라비가 힙한씬에서 자리 잡믈려고 힙합하는 애들 많이 영입하던데",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028855.1.25",
          "form": "에일리도 있구낭",
          "original_form": "에일리도 있구낭",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028855.1.26",
          "form": "열일하겠네",
          "original_form": "열일하겠네",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028855.1.27",
          "form": "ㅇㅇㅇ 가수는 에일리랑 휘인",
          "original_form": "ㅇㅇㅇ 가수는 에일리랑 휘인",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028855.1.28",
          "form": "가수 출신 사장 소속사가 허접한 중소보다 신경 잘써줘서 좋은거 같앙",
          "original_form": "가수 출신 사장 소속사가 허접한 중소보다 신경 잘써줘서 좋은거 같앙",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028855.1.29",
          "form": "잘갔네",
          "original_form": "잘갔네",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028855.1.30",
          "form": "ㅇㅇㅇ게다가",
          "original_form": "ㅇㅇㅇ게다가",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028855.1.31",
          "form": "라비가 ㄱ그룹하면서 솔로는 따로하는 딱 그 상황이라",
          "original_form": "라비가 ㄱ그룹하면서 솔로는 따로하는 딱 그 상황이라",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028855.1.32",
          "form": "상황이 같아서",
          "original_form": "상황이 같아서",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028855.1.33",
          "form": "이해 잘해주겠네",
          "original_form": "이해 잘해주겠네",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028855.1.34",
          "form": "그룹도 신경잘써줄거같다고 하더라",
          "original_form": "그룹도 신경잘써줄거같다고 하더라",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028855.1.35",
          "form": "탈알비덤하고 최선으로 간듯",
          "original_form": "탈알비덤하고 최선으로 간듯",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028855.1.36",
