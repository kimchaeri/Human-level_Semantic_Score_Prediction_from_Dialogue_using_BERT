      "utterance": [
        {
          "id": "MDRW2100028855.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ오....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ오....",
