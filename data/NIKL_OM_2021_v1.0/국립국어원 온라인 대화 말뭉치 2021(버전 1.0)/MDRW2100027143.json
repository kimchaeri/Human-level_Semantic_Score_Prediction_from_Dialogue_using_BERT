      "utterance": [
        {
          "id": "MDRW2100027143.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100027143.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027143.1.3",
          "form": "환절기에 아픈데 없이 건강하시죠??",
          "original_form": "환절기에 아픈데 없이 건강하시죠??",
          "speaker_id": "1",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027143.1.4",
          "form": "네 저는 건강합니다.",
          "original_form": "네 저는 건강합니다.",
          "speaker_id": "2",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100027143.1.5",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100027143.1.6",
          "form": "네 맞고 아파서 누워 있어요",
          "original_form": "네 맞고 아파서 누워 있어요",
          "speaker_id": "1",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100027143.1.7",
          "form": "헐 많이 아프신가요?",
          "original_form": "헐 많이 아프신가요?",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027143.1.8",
          "form": "근육통에 너무 심하네요 속도 메스껍고",
          "original_form": "근육통에 너무 심하네요 속도 메스껍고",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027143.1.9",
          "form": "머리도 띵하게 울리는 두통이 계속ㅠㅠ",
          "original_form": "머리도 띵하게 울리는 두통이 계속ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027143.1.10",
          "form": "백신접종하셨어요?",
          "original_form": "백신접종하셨어요?",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027143.1.11",
          "form": "네 저는 지난주에 맞았는데 팔만 가끔 뻐근한거 빼면 괜찮아요",
          "original_form": "네 저는 지난주에 맞았는데 팔만 가끔 뻐근한거 빼면 괜찮아요",
          "speaker_id": "2",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027143.1.12",
          "form": "지금 몇일째세요?",
          "original_form": "지금 몇일째세요?",
          "speaker_id": "2",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027143.1.13",
          "form": "저 이틀째요",
          "original_form": "저 이틀째요",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027143.1.14",
          "form": "ㅜㅜ 어제부터 계속 아프셨어요?",
          "original_form": "ㅜㅜ 어제부터 계속 아프셨어요?",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027143.1.15",
          "form": "아니요 어제는 괜찮았는데 오늘 많이 심하네요",
          "original_form": "아니요 어제는 괜찮았는데 오늘 많이 심하네요",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027143.1.16",
          "form": "에구ㅜㅜ 타이레놀 드셨나요?",
          "original_form": "에구ㅜㅜ 타이레놀 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027143.1.17",
          "form": "아직 안 먹었어요 속이 메스꺼워서",
          "original_form": "아직 안 먹었어요 속이 메스꺼워서",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027143.1.18",
          "form": "식사 좀 대충이라도 하고 먹으려고요",
          "original_form": "식사 좀 대충이라도 하고 먹으려고요",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027143.1.19",
          "form": "네 입맛없으셔도 죽이라도 꼭 챙겨드세요",
          "original_form": "네 입맛없으셔도 죽이라도 꼭 챙겨드세요",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027143.1.20",
          "form": "공복에 드시면 더 안좋을까봐 걱정이네요",
          "original_form": "공복에 드시면 더 안좋을까봐 걱정이네요",
          "speaker_id": "2",
          "time": "20210902 15:03"
        }
