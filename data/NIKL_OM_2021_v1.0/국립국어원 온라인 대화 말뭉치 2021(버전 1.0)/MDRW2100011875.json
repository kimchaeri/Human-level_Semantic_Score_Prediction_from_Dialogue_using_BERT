      "utterance": [
        {
          "id": "MDRW2100011875.1.1",
          "form": "저는 개키워서 개 치약잉8ㅋㅋㅋ",
          "original_form": "저는 개키워서 개 치약잉{emoji:‥}8ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011875.1.2",
          "form": "이요ㅋㅋ",
          "original_form": "이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011875.1.3",
          "form": "애견치약 구매하셨구나",
          "original_form": "애견치약 구매하셨구나",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011875.1.4",
          "form": "네ㅋㅋ바디워시는 어디껀가요",
          "original_form": "네ㅋㅋ바디워시는 어디껀가요",
          "speaker_id": "2",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011875.1.5",
          "form": "저 해피바스꺼 구매했어요",
          "original_form": "저 해피바스꺼 구매했어요",
          "speaker_id": "1",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011875.1.6",
          "form": "자몽향이 좋더라구요",
          "original_form": "자몽향이 좋더라구요",
          "speaker_id": "1",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011875.1.7",
          "form": "좋아하는 향 있으신가요",
          "original_form": "좋아하는 향 있으신가요",
          "speaker_id": "1",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011875.1.8",
          "form": "저는 코코넛이요ㅋㅋ 코코넛 좋아하세요?",
          "original_form": "저는 코코넛이요ㅋㅋ 코코넛 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011875.1.9",
          "form": "약간 달달한쪽 좋아하시나부다",
          "original_form": "약간 달달한쪽 좋아하시나부다",
          "speaker_id": "1",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011875.1.10",
          "form": "저도 좋아해요 코코넛향",
          "original_form": "저도 좋아해요 코코넛향",
          "speaker_id": "1",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011875.1.11",
          "form": "강아지 치약은 어디브랜드꺼 사셨나요",
          "original_form": "강아지 치약은 어디브랜드꺼 사셨나요",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100011875.1.12",
          "form": "버박인가 거기꺼가 유명하던데",
          "original_form": "버박인가 거기꺼가 유명하던데",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100011875.1.13",
          "form": "오 맞아요 어떻게 아세요??",
          "original_form": "오 맞아요 어떻게 아세요??",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100011875.1.14",
          "form": "저는 오라틴 샀어요ㅋㅋ",
          "original_form": "저는 오라틴 샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100011875.1.15",
          "form": "아 고모가 강아지를 키우셔서ㅋㅋ",
          "original_form": "아 고모가 강아지를 키우셔서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100011875.1.16",
          "form": "오란틴도 좋은가요",
          "original_form": "오란틴도 좋은가요",
          "speaker_id": "1",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100011875.1.17",
          "form": "네 많이들 쓰더라고요",
          "original_form": "네 많이들 쓰더라고요",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100011875.1.18",
          "form": "근데 비싸네요ㅋㅋㅋㅋㅜㅜ",
          "original_form": "근데 비싸네요ㅋㅋㅋㅋㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100011875.1.19",
          "form": "강아지들 치약 비싸더라구요",
          "original_form": "강아지들 치약 비싸더라구요",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100011875.1.20",
          "form": "그래도 치아관리해주려면 어쩔 수 없는 거 같아요",
          "original_form": "그래도 치아관리해주려면 어쩔 수 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100011875.1.21",
          "form": "네 사람거보다 비싸지만...어쩔 수 없네요ㅜ",
          "original_form": "네 사람거보다 비싸지만...어쩔 수 없네요ㅜ",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100011875.1.22",
          "form": "또 살거 없으신가요",
          "original_form": "또 살거 없으신가요",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100011875.1.23",
          "form": "병원비보다는 싸니까요ㅠ",
          "original_form": "병원비보다는 싸니까요ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100011875.1.24",
          "form": "어 당분간은 딱히 살 거 없을거같아요",
          "original_form": "어 당분간은 딱히 살 거 없을거같아요",
          "speaker_id": "1",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100011875.1.25",
          "form": "당장은 필요한게 없어서..",
          "original_form": "당장은 필요한게 없어서..",
          "speaker_id": "1",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100011875.1.26",
          "form": "생쥐님은요?",
          "original_form": "생쥐님은요?",
          "speaker_id": "1",
          "time": "20210824 21:48"
        },
        {
          "id": "MDRW2100011875.1.27",
          "form": "저는 데스크탑 사야 되는데 넘 귀찮네요ㄱㅋ",
          "original_form": "저는 데스크탑 사야 되는데 넘 귀찮네요ㄱㅋ",
          "speaker_id": "2",
          "time": "20210824 21:51"
        }
      ]
