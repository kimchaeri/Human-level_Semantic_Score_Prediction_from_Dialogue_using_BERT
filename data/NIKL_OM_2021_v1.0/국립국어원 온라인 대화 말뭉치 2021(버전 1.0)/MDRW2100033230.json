      "utterance": [
        {
          "id": "MDRW2100033230.1.1",
          "form": "오 내 친구가 이번에 여군으로 들어갔어요",
          "original_form": "오 내 친구가 이번에 여군으로 들어갔어요",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100033230.1.2",
          "form": "그래서 요즘 계속 편지 보내줬었는데ㅋㅋㅋㅋ",
          "original_form": "그래서 요즘 계속 편지 보내줬었는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100033230.1.3",
          "form": "주변 사람 중에 군대 간 사람 있어요?",
          "original_form": "주변 사람 중에 군대 간 사람 있어요?",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100033230.1.4",
          "form": "네 동생이요",
          "original_form": "네 동생이요",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100033230.1.5",
          "form": "여군이면 부사관인가요~??",
          "original_form": "여군이면 부사관인가요~??",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100033230.1.6",
          "form": "네 맞아요! 내가 아는 사람이 군대라니 신기하기도 하고ㅋㅋㅋㅋ",
          "original_form": "네 맞아요! 내가 아는 사람이 군대라니 신기하기도 하고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100033230.1.7",
          "form": "면회도 가보고 싶은데 코로나라서...ㅠㅜ 아쉬워요",
          "original_form": "면회도 가보고 싶은데 코로나라서...ㅠㅜ 아쉬워요",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100033230.1.8",
          "form": "ㅜㅜㅠㅠ그렇겠네요",
          "original_form": "ㅜㅜㅠㅠ그렇겠네요",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100033230.1.9",
          "form": "주변에 군대간사람이",
          "original_form": "주변에 군대간사람이",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100033230.1.10",
          "form": "처음인가요??",
          "original_form": "처음인가요??",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100033230.1.11",
          "form": "네! 주변에 남사친이 없어서요ㅋㅋㅋ",
          "original_form": "네! 주변에 남사친이 없어서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100033230.1.12",
          "form": "과 동기애들은 친해질 틈도 없이 그냥 군대 가버려서",
          "original_form": "과 동기애들은 친해질 틈도 없이 그냥 군대 가버려서",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100033230.1.13",
          "form": "ㅜㅠㅠㅠㅠㅠ아 그렇겠네요",
          "original_form": "ㅜㅠㅠㅠㅠㅠ아 그렇겠네요",
          "speaker_id": "2",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100033230.1.14",
          "form": "전 이번에 동생도 군대가고...주변에 간 애들이 많네요ㅎㅎ근데 가서 다들 잘 지내요",
          "original_form": "전 이번에 동생도 군대가고...주변에 간 애들이 많네요ㅎㅎ근데 가서 다들 잘 지내요",
          "speaker_id": "2",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100033230.1.15",
          "form": "자격증 공부하더라구요",
          "original_form": "자격증 공부하더라구요",
          "speaker_id": "2",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100033230.1.16",
          "form": "오 진짜 알차게 사네요!",
          "original_form": "오 진짜 알차게 사네요!",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100033230.1.17",
          "form": "애가 고등학교때 공부를 안해서",
          "original_form": "애가 고등학교때 공부를 안해서",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100033230.1.18",
          "form": "어쩔구없어요",
          "original_form": "어쩔구없어요",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100033230.1.19",
          "form": "어쩔수없어요",
          "original_form": "어쩔수없어요",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100033230.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ성인돼서라도 하는게 어디예요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ성인돼서라도 하는게 어디예요!",
          "speaker_id": "1",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100033230.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ음...도피했죠 뭐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ음...도피했죠 뭐",
          "speaker_id": "2",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100033230.1.22",
          "form": "제발 정신좀 차리고 나오면 좋겠어요",
          "original_form": "제발 정신좀 차리고 나오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100033230.1.23",
          "form": "ㅜㅜㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅜㅜㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100033230.1.24",
          "form": "군대 다녀오면 사람 된다잖아요",
          "original_form": "군대 다녀오면 사람 된다잖아요",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100033230.1.25",
          "form": "막 청소랑 빨래도 도맡아한다그러고",
          "original_form": "막 청소랑 빨래도 도맡아한다그러고",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100033230.1.26",
          "form": "어휴 제발 그랬으면 좋겠네요!!!",
          "original_form": "어휴 제발 그랬으면 좋겠네요!!!",
          "speaker_id": "2",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100033230.1.27",
          "form": "진짜 집에서 할 줄 아는게",
          "original_form": "진짜 집에서 할 줄 아는게",
          "speaker_id": "2",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100033230.1.28",
          "form": "하나도 없었거든요",
          "original_form": "하나도 없었거든요",
          "speaker_id": "2",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100033230.1.29",
          "form": "아이고야! 게임만 했군요!!",
          "original_form": "아이고야! 게임만 했군요!!",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100033230.1.30",
          "form": "어떻게 아셨죠?",
          "original_form": "어떻게 아셨죠?",
          "speaker_id": "2",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100033230.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇ0ㅇ",
