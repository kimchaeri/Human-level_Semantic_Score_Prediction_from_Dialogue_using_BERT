      "utterance": [
        {
          "id": "MDRW2100040801.1.1",
          "form": "학교다닐때 진짜 한의원도가보고",
          "original_form": "학교다닐때 진짜 한의원도가보고",
          "speaker_id": "2",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040801.1.2",
          "form": "따로 치료받아야되죠?",
          "original_form": "따로 치료받아야되죠?",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040801.1.3",
          "form": "고치려 노력많이했는데",
          "original_form": "고치려 노력많이했는데",
          "speaker_id": "2",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040801.1.4",
          "form": "완치란건 없더라구요ㅠ",
          "original_form": "완치란건 없더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040801.1.5",
          "form": "안돼나요?",
          "original_form": "안돼나요?",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040801.1.6",
          "form": "비염 세척도 많이쑤고 좋다고 하던데",
          "original_form": "비염 세척도 많이쑤고 좋다고 하던데",
          "speaker_id": "2",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100040801.1.7",
          "form": "꼭 가을되면 같이찾아와요",
          "original_form": "꼭 가을되면 같이찾아와요",
          "speaker_id": "1",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040801.1.8",
          "form": "전 몇번해보고 실패해서 그냥 약에 의존해요ㅠ",
          "original_form": "전 몇번해보고 실패해서 그냥 약에 의존해요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040801.1.9",
          "form": "식염수로 하고 하는데 끙끙",
          "original_form": "식염수로 하고 하는데 끙끙",
          "speaker_id": "1",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040801.1.10",
          "form": "뭐 너무 당연한 말이겠지만 면역력기르는게ㅠㅠ",
          "original_form": "뭐 너무 당연한 말이겠지만 면역력기르는게ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040801.1.11",
          "form": "병원가자고해도 괜찮다고하고",
          "original_form": "병원가자고해도 괜찮다고하고",
          "speaker_id": "1",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040801.1.12",
          "form": "아..그렇죠",
          "original_form": "아..그렇죠",
          "speaker_id": "1",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040801.1.13",
          "form": "알러지 검사를 해봤는데 전 다 반응이있더라구요",
          "original_form": "알러지 검사를 해봤는데 전 다 반응이있더라구요",
          "speaker_id": "2",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100040801.1.14",
          "form": "면역력을키워야겠네요",
          "original_form": "면역력을키워야겠네요",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040801.1.15",
          "form": "계절 옷정리할때도 혼자 휴지한통다쓰면서 해야해여ㅠ",
          "original_form": "계절 옷정리할때도 혼자 휴지한통다쓰면서 해야해여ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040801.1.16",
          "form": "진짜 농담반으로 ㅋㅋ의사샘이 청소도 하지말라햇어요 집먼지에도 반응을 하니",
          "original_form": "진짜 농담반으로 ㅋㅋ의사샘이 청소도 하지말라햇어요 집먼지에도 반응을 하니",
          "speaker_id": "2",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040801.1.17",
          "form": "아..그정도예요?",
          "original_form": "아..그정도예요?",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040801.1.18",
          "form": "여름엔 안나타 나죠?",
          "original_form": "여름엔 안나타 나죠?",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100040801.1.19",
          "form": "전 찬바람이랑 먼지에 좀 강하게 와요",
          "original_form": "전 찬바람이랑 먼지에 좀 강하게 와요",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040801.1.20",
          "form": "네네 ㅠ 그리고 몇년동안은 봄가을에도 괜찮았는데",
          "original_form": "네네 ㅠ 그리고 몇년동안은 봄가을에도 괜찮았는데",
          "speaker_id": "2",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040801.1.21",
          "form": "아들도 여름엔 괜찮다가 딱 이런날씨에",
          "original_form": "아들도 여름엔 괜찮다가 딱 이런날씨에",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100040801.1.22",
          "form": "올해 몸이 영 안좋운가봐요 ㅠ 전모르겟지만 제몸이 그런지",
          "original_form": "올해 몸이 영 안좋운가봐요 ㅠ 전모르겟지만 제몸이 그런지",
          "speaker_id": "2",
          "time": "20210905 07:49"
        }
      ]
    }
