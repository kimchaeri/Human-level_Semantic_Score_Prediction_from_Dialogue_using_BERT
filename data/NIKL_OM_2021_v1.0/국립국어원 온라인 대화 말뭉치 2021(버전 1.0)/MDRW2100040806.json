      "utterance": [
        {
          "id": "MDRW2100040806.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100040806.1.2",
          "form": "전 사실 다좋아해요ㅠㅠ",
          "original_form": "전 사실 다좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 08:03"
        },
        {
          "id": "MDRW2100040806.1.3",
          "form": "가리는 것 없이 잘 드시는군요",
          "original_form": "가리는 것 없이 잘 드시는군요",
          "speaker_id": "1",
          "time": "20210905 08:03"
        },
        {
          "id": "MDRW2100040806.1.4",
          "form": "배달음식도 자주 드시나요?",
          "original_form": "배달음식도 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210905 08:03"
        },
        {
          "id": "MDRW2100040806.1.5",
          "form": "네ㅠㅠ~ 어제도 굽네치킨먹었네요ㅠ",
          "original_form": "네ㅠㅠ~ 어제도 굽네치킨먹었네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100040806.1.6",
          "form": "와 맛있었겠네요",
          "original_form": "와 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040806.1.7",
          "form": "굽네면 고 추 바사삭",
          "original_form": "굽네면 고 추 바사삭",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040806.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040806.1.9",
          "form": "오랫만에 먹으니 맛잇엇어요",
          "original_form": "오랫만에 먹으니 맛잇엇어요",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040806.1.10",
          "form": "그냥 오리지날 먹었어요!",
          "original_form": "그냥 오리지날 먹었어요!",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040806.1.11",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040806.1.12",
          "form": "저도 한동안 배달음식 많이 먹다가",
          "original_form": "저도 한동안 배달음식 많이 먹다가",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040806.1.13",
          "form": "요즘 좀 자제중이에요",
          "original_form": "요즘 좀 자제중이에요",
          "speaker_id": "1",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100040806.1.14",
          "form": "맞아요ㅠㅠ저도 그래야하는데ㅠ",
          "original_form": "맞아요ㅠㅠ저도 그래야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100040806.1.15",
          "form": "더울땐 밥 챙겨먹기도 귀찮아서 배달 많이 시켰는데",
          "original_form": "더울땐 밥 챙겨먹기도 귀찮아서 배달 많이 시켰는데",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100040806.1.16",
          "form": "지출도 많이 되고",
          "original_form": "지출도 많이 되고",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100040806.1.17",
          "form": "살도 찌고ㅜ",
          "original_form": "살도 찌고ㅜ",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100040806.1.18",
          "form": "맞아요ㅠㅠ 시키면 기본 값이 있으니",
          "original_form": "맞아요ㅠㅠ 시키면 기본 값이 있으니",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040806.1.19",
          "form": "네 양도 많이 시키게 되고요",
          "original_form": "네 양도 많이 시키게 되고요",
          "speaker_id": "1",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040806.1.20",
          "form": "맞아여ㅠㅠ 진짜 배달비생각하면 추가할때도잇고ㅠ",
          "original_form": "맞아여ㅠㅠ 진짜 배달비생각하면 추가할때도잇고ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040806.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040806.1.22",
          "form": "혼자 사시나요?",
          "original_form": "혼자 사시나요?",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040806.1.23",
          "form": "혼자인데 남친이 거의 같이있어요 ㅎ",
          "original_form": "혼자인데 남친이 거의 같이있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100040806.1.24",
          "form": "아 그러시구나~",
          "original_form": "아 그러시구나~",
          "speaker_id": "1",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100040806.1.25",
          "form": "둘이 먹음 좀 낫죠ㅎㅎ",
          "original_form": "둘이 먹음 좀 낫죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100040806.1.26",
          "form": "혼자서도...뭐 잘시키지만 ㅎㅎ",
          "original_form": "혼자서도...뭐 잘시키지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:52"
        }
      ]
    }
