      "utterance": [
        {
          "id": "MDRW2100021118.1.1",
          "form": "직장다니시나요?",
          "original_form": "직장다니시나요?",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100021118.1.2",
          "form": "전 무직장인입니다.. ㅋㅋㅋ \n어떤일 하시나요?",
          "original_form": "전 무직장인입니다.. ㅋㅋㅋ \n어떤일 하시나요?",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100021118.1.3",
          "form": "저두요 .ㅋ",
          "original_form": "저두요 .ㅋ",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100021118.1.4",
          "form": "구직활동도 안하고 있어요",
          "original_form": "구직활동도 안하고 있어요",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100021118.1.5",
          "form": "사람이.. 집에서 놀기도해야..",
          "original_form": "사람이.. 집에서 놀기도해야..",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100021118.1.6",
          "form": "경제가 돌고... 마~~~",
          "original_form": "경제가 돌고... 마~~~",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100021118.1.7",
          "form": "부자도 돈쓰고... 그런거아니겠슺니까.. ㅋㅋ",
          "original_form": "부자도 돈쓰고... 그런거아니겠슺니까.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100021118.1.8",
          "form": "그쵸. 돈버는 사람도 있고 쓰는 사람도 있고",
          "original_form": "그쵸. 돈버는 사람도 있고 쓰는 사람도 있고",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100021118.1.9",
          "form": "계속 백수신거에요? 쭉? ... ㅋㅋ",
          "original_form": "계속 백수신거에요? 쭉? ... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100021118.1.10",
          "form": "쭉은 아니고  일하다 논지 몇년 되었어요",
          "original_form": "쭉은 아니고  일하다 논지 몇년 되었어요",
          "speaker_id": "2",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100021118.1.11",
          "form": "아.. 그전엔 직장인이셧구나.. \n그거 모지?",
          "original_form": "아.. 그전엔 직장인이셧구나.. \n그거 모지?",
          "speaker_id": "1",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100021118.1.12",
          "form": "경력단절상태에요",
          "original_form": "경력단절상태에요",
          "speaker_id": "2",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100021118.1.13",
          "form": "시집준비중 그런거이신가여?",
          "original_form": "시집준비중 그런거이신가여?",
          "speaker_id": "1",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100021118.1.14",
          "form": "ㅋㅋ 결혼했어요",
          "original_form": "ㅋㅋ 결혼했어요",
          "speaker_id": "2",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100021118.1.15",
          "form": "아.. 그럼 주부이신거네요..",
          "original_form": "아.. 그럼 주부이신거네요..",
          "speaker_id": "1",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021118.1.16",
          "form": "주부도 직업인뎅.. ㅋㅋ",
          "original_form": "주부도 직업인뎅.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021118.1.17",
          "form": "아...그렇죠 ㅋ",
          "original_form": "아...그렇죠 ㅋ",
          "speaker_id": "2",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100021118.1.18",
          "form": "전업주부가 직업인건 싫으네요.",
          "original_form": "전업주부가 직업인건 싫으네요.",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021118.1.19",
          "form": "뭔가 살림이 내 주된 업무인거잖아요. 백수 할래요 ㅋ",
          "original_form": "뭔가 살림이 내 주된 업무인거잖아요. 백수 할래요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021118.1.20",
          "form": "아르바이트는 하시나요?",
          "original_form": "아르바이트는 하시나요?",
