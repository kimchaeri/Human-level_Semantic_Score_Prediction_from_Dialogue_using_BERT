        {
          "id": "MDRW2100007929.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100007929.1.2",
          "form": "흐리기만 하고 비는 안와요",
          "original_form": "흐리기만 하고 비는 안와요",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007929.1.3",
          "form": "거긴 날씨 어때요?",
          "original_form": "거긴 날씨 어때요?",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007929.1.4",
          "form": "맑아요",
          "original_form": "맑아요",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007929.1.5",
          "form": "혹시 덥진 않나요?",
          "original_form": "혹시 덥진 않나요?",
          "speaker_id": "2",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007929.1.6",
          "form": "더워요",
          "original_form": "더워요",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007929.1.7",
          "form": "아직 기온이 크게 떨어지진 않았더라구요",
          "original_form": "아직 기온이 크게 떨어지진 않았더라구요",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007929.1.8",
          "form": "마지막 더위 조심하세요",
          "original_form": "마지막 더위 조심하세요",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007929.1.9",
          "form": "늦장마 무서워요",
          "original_form": "늦장마 무서워요",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007929.1.10",
          "form": "호우주의보 자주 뜨더라구요",
          "original_form": "호우주의보 자주 뜨더라구요",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007929.1.11",
          "form": "태풍도요",
          "original_form": "태풍도요",
          "speaker_id": "1",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007929.1.12",
          "form": "조심하세요 기상이변이 심해서",
          "original_form": "조심하세요 기상이변이 심해서",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007929.1.13",
          "form": "자연이 대단해요",
          "original_form": "자연이 대단해요",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007929.1.14",
          "form": "인간은 자연을 이길 수 없나봐요",
          "original_form": "인간은 자연을 이길 수 없나봐요",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
