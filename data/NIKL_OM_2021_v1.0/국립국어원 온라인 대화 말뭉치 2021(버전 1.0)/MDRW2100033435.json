      "utterance": [
        {
          "id": "MDRW2100033435.1.1",
          "form": "코로나 끝나면",
          "original_form": "코로나 끝나면",
          "speaker_id": "2",
          "time": "20210823 22:08"
        },
        {
          "id": "MDRW2100033435.1.2",
          "form": "여행 어디 가고싶으세요?",
          "original_form": "여행 어디 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100033435.1.3",
          "form": "저는 해외요 ㅠㅠㅠ",
          "original_form": "저는 해외요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 08:05"
        },
        {
          "id": "MDRW2100033435.1.4",
          "form": "오 해외 좋죠",
          "original_form": "오 해외 좋죠",
          "speaker_id": "2",
          "time": "20210824 09:44"
        },
        {
          "id": "MDRW2100033435.1.5",
          "form": "해외 어디요??",
          "original_form": "해외 어디요??",
          "speaker_id": "2",
          "time": "20210824 09:44"
        },
        {
          "id": "MDRW2100033435.1.6",
          "form": "프랑스 가고싶어용 !",
          "original_form": "프랑스 가고싶어용 !",
          "speaker_id": "1",
          "time": "20210824 09:48"
        },
        {
          "id": "MDRW2100033435.1.7",
          "form": "가고싶은곳 있으세요?",
          "original_form": "가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210824 09:48"
        },
        {
          "id": "MDRW2100033435.1.8",
          "form": "오 전 미국이요",
          "original_form": "오 전 미국이요",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100033435.1.9",
          "form": "듣기만해도 프랑스 너무 좋네요",
          "original_form": "듣기만해도 프랑스 너무 좋네요",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100033435.1.10",
          "form": "아니면 캐나다도 가고싶어요ㅋㅋ",
          "original_form": "아니면 캐나다도 가고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100033435.1.11",
          "form": "친구가 코로나전에 다녀왓는데",
          "original_form": "친구가 코로나전에 다녀왓는데",
          "speaker_id": "1",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100033435.1.12",
          "form": "그렇게 극찬을 하더라구요 ㅠㅠ",
          "original_form": "그렇게 극찬을 하더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100033435.1.13",
          "form": "ㅠㅠㅠㅠㅠ 아 너무 좋을 거 같아요",
          "original_form": "ㅠㅠㅠㅠㅠ 아 너무 좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100033435.1.14",
          "form": "이름만 들어도 부럽네요",
          "original_form": "이름만 들어도 부럽네요",
          "speaker_id": "2",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100033435.1.15",
          "form": "그쵸 ㅠㅠ 내년엔 제주도라도 가려구용",
          "original_form": "그쵸 ㅠㅠ 내년엔 제주도라도 가려구용",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033435.1.16",
          "form": "제주도 좋죠 ㅠㅠㅠㅠ",
          "original_form": "제주도 좋죠 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100033435.1.17",
          "form": "집에만 있으니",
          "original_form": "집에만 있으니",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100033435.1.18",
          "form": "넘 우울해영 ㅠ",
          "original_form": "넘 우울해영 ㅠ",
          "speaker_id": "1",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100033435.1.19",
          "form": "알아요 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "알아요 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100033435.1.20",
          "form": "여행 가고싶죠 ㅠㅠㅠ",
          "original_form": "여행 가고싶죠 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100033435.1.21",
          "form": "기억나는 여행지 있나요?",
          "original_form": "기억나는 여행지 있나요?",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100033435.1.22",
          "form": "저도 제주도요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 제주도요ㅋㅋㅋㅋㅋㅋㅋ",
