      "utterance": [
        {
          "id": "MDRW2100042425.1.1",
          "form": "맛집 다니는 것 좋아하세요?",
          "original_form": "맛집 다니는 것 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042425.1.2",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042425.1.3",
          "form": "맛집 탐방",
          "original_form": "맛집 탐방",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042425.1.4",
          "form": "근데 사실 다 맛있어서... ㅋㅋ",
          "original_form": "근데 사실 다 맛있어서... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042425.1.5",
          "form": "저도 여행 가면 맛집 리스트 찾아서 방문하는 게 낙이예요",
          "original_form": "저도 여행 가면 맛집 리스트 찾아서 방문하는 게 낙이예요",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042425.1.6",
          "form": "친구들이랑 검색해서 가면 오히려 실망했었는데",
          "original_form": "친구들이랑 검색해서 가면 오히려 실망했었는데",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042425.1.7",
          "form": "사실 저도 크게 가리지 않고 잘 먹어요ㅎㅎ",
          "original_form": "사실 저도 크게 가리지 않고 잘 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042425.1.8",
          "form": "여행지는 기본이상해요",
          "original_form": "여행지는 기본이상해요",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042425.1.9",
          "form": "진짜 충격적인 맛집은 사실 서울... ㅋㅋ",
          "original_form": "진짜 충격적인 맛집은 사실 서울... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042425.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042425.1.11",
          "form": "어디가 제일 맛있으셨어요?",
          "original_form": "어디가 제일 맛있으셨어요?",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042425.1.12",
          "form": "전국 맛집의 분점이 서울에 있기도 하고요ㅎㅎ",
          "original_form": "전국 맛집의 분점이 서울에 있기도 하고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042425.1.13",
          "form": "지금 딱 생각나는 맛집은 없는데",
          "original_form": "지금 딱 생각나는 맛집은 없는데",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042425.1.14",
          "form": "맞아요ㅜㅎㅎ",
          "original_form": "맞아요ㅜㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042425.1.15",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042425.1.16",
          "form": "추천맛집 있으신가요?ㅎㅎ",
          "original_form": "추천맛집 있으신가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042425.1.17",
          "form": "체인이긴한데 오늘그대와???",
          "original_form": "체인이긴한데 오늘그대와???",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042425.1.18",
          "form": "파스타집인데 맛있었어요!",
          "original_form": "파스타집인데 맛있었어요!",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042425.1.19",
          "form": "홍대쪽에도 있다더라고요 전 부천으로 갔어요",
          "original_form": "홍대쪽에도 있다더라고요 전 부천으로 갔어요",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042425.1.20",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042425.1.21",
          "form": "찾아봐야겠네요ㅎㅎ",
          "original_form": "찾아봐야겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042425.1.22",
          "form": "네 그날 배가 고프긴했어요ㅋㅋ",
          "original_form": "네 그날 배가 고프긴했어요ㅋㅋ",
