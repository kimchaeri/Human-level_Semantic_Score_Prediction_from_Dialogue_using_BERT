      "utterance": [
        {
          "id": "MDRW2100022548.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100022548.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022548.1.3",
          "form": "요즘 날씨가 넘 흐리죠",
          "original_form": "요즘 날씨가 넘 흐리죠",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022548.1.4",
          "form": "네 날씨 흐려서 맨날 우산갖고 다녀요ㅋㅋ",
          "original_form": "네 날씨 흐려서 맨날 우산갖고 다녀요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022548.1.5",
          "form": "그니깐요 기상청 예보도 맨날 안맞구",
          "original_form": "그니깐요 기상청 예보도 맨날 안맞구",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022548.1.6",
          "form": "그쵸 우산 들고 다니는거도 일인데 비안오면 얼마나 허탈하던지ㅋㅋ",
          "original_form": "그쵸 우산 들고 다니는거도 일인데 비안오면 얼마나 허탈하던지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:05"
        },
        {
          "id": "MDRW2100022548.1.7",
          "form": "오늘은 비 안올것 같아 좋네요!",
          "original_form": "오늘은 비 안올것 같아 좋네요!",
          "speaker_id": "1",
          "time": "20210830 08:11"
        },
        {
          "id": "MDRW2100022548.1.8",
          "form": "네 저희 지역도 오늘은 비가 안온다네요ㅎ",
          "original_form": "네 저희 지역도 오늘은 비가 안온다네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 15:50"
        },
        {
          "id": "MDRW2100022548.1.9",
          "form": "이번 여름은 태풍이 별로 안온거같지 않나요?",
          "original_form": "이번 여름은 태풍이 별로 안온거같지 않나요?",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100022548.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100022548.1.11",
          "form": "다행히 태풍피해 적은것같아요",
          "original_form": "다행히 태풍피해 적은것같아요",
          "speaker_id": "1",
          "time": "20210830 16:59"
        },
        {
          "id": "MDRW2100022548.1.12",
          "form": "네 태풍피해 무섭죠ㅠ 휩쓸리는거 순식간이라서ㅜㅜ",
          "original_form": "네 태풍피해 무섭죠ㅠ 휩쓸리는거 순식간이라서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 17:13"
        },
        {
          "id": "MDRW2100022548.1.13",
          "form": "미세먼지없는 이 좋은 계절에",
          "original_form": "미세먼지없는 이 좋은 계절에",
          "speaker_id": "1",
          "time": "20210830 17:16"
        },
        {
          "id": "MDRW2100022548.1.14",
          "form": "마스크라 아쉬워요",
          "original_form": "마스크라 아쉬워요",
          "speaker_id": "1",
          "time": "20210830 17:16"
        },
        {
          "id": "MDRW2100022548.1.15",
          "form": "그쵸 마스크쓰면 숨쉬기 답답하고요ㅠㅠ",
          "original_form": "그쵸 마스크쓰면 숨쉬기 답답하고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 17:17"
        }
      ]
