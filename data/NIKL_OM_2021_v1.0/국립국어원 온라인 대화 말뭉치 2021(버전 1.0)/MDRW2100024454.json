      "utterance": [
        {
          "id": "MDRW2100024454.1.1",
          "form": "건강위해서 하는거 있으세요?",
          "original_form": "건강위해서 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024454.1.2",
          "form": "운동이라든지?",
          "original_form": "운동이라든지?",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024454.1.3",
          "form": "영양제 챙겨먹기?",
          "original_form": "영양제 챙겨먹기?",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024454.1.4",
          "form": "오... 어떤거 드세요?",
          "original_form": "오... 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024454.1.5",
          "form": "저는 좋다는 영양제 이것저것 다 챙겨먹다가",
          "original_form": "저는 좋다는 영양제 이것저것 다 챙겨먹다가",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024454.1.6",
          "form": "이제는 그만뒀어요ㅎㅎ",
          "original_form": "이제는 그만뒀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024454.1.7",
          "form": "종합비타민이랑",
          "original_form": "종합비타민이랑",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024454.1.8",
          "form": "프로폴리스요",
          "original_form": "프로폴리스요",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024454.1.9",
          "form": "그거 면역력 영양제 맞죠?",
          "original_form": "그거 면역력 영양제 맞죠?",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024454.1.10",
          "form": "들어본적 있어요!",
          "original_form": "들어본적 있어요!",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024454.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024454.1.12",
          "form": "그거랑 면역력 영양제 하나 더 먹어요",
          "original_form": "그거랑 면역력 영양제 하나 더 먹어요",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024454.1.13",
          "form": "알로에로만든건데 얼마전에 샀어요",
          "original_form": "알로에로만든건데 얼마전에 샀어요",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024454.1.14",
          "form": "오.... 건강 엄청 챙기시나봐요",
          "original_form": "오.... 건강 엄청 챙기시나봐요",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024454.1.15",
          "form": "최근에 많이 피곤하고",
          "original_form": "최근에 많이 피곤하고",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100024454.1.16",
          "form": "여기저기 아파서요",
          "original_form": "여기저기 아파서요",
          "speaker_id": "2",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024454.1.17",
          "form": "아하.. 건강한게 최고죠ㅠ",
          "original_form": "아하.. 건강한게 최고죠ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024454.1.18",
          "form": "비타민은 어떤거 드세요? 효과 좋아요?!",
          "original_form": "비타민은 어떤거 드세요? 효과 좋아요?!",
          "speaker_id": "1",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100024454.1.19",
          "form": "선물받은거라 잘 모르겠어요",
          "original_form": "선물받은거라 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024454.1.20",
          "form": "먹은지 얼마안돼서 효과는 잘 모르지만",
          "original_form": "먹은지 얼마안돼서 효과는 잘 모르지만",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024454.1.21",
          "form": "안먹을때보단 기운이 쌩쌩해요",
          "original_form": "안먹을때보단 기운이 쌩쌩해요",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100024454.1.22",
          "form": "저도 요즘 건강 챙겨야겠다 싶었는데",
          "original_form": "저도 요즘 건강 챙겨야겠다 싶었는데",
          "speaker_id": "1",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100024454.1.23",
          "form": "비타민 다시 먹어야겠네요ㅋㅋ",
          "original_form": "비타민 다시 먹어야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:46"
        },
        {
          "id": "MDRW2100024454.1.24",
          "form": "요즘 같은 때에",
          "original_form": "요즘 같은 때에",
          "speaker_id": "2",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100024454.1.25",
          "form": "건강이 중요해요 정말",
          "original_form": "건강이 중요해요 정말",
          "speaker_id": "2",
          "time": "20210831 10:47"
        },
        {
          "id": "MDRW2100024454.1.26",
          "form": "저는 건강때문에 다이어트 해야하는데ㅜ",
          "original_form": "저는 건강때문에 다이어트 해야하는데ㅜ",
          "speaker_id": "1",
          "time": "20210831 10:48"
        },
        {
          "id": "MDRW2100024454.1.27",
          "form": "운동 너무 싫어요..",
          "original_form": "운동 너무 싫어요..",
          "speaker_id": "1",
          "time": "20210831 10:48"
        }
      ]
    }
  ]
