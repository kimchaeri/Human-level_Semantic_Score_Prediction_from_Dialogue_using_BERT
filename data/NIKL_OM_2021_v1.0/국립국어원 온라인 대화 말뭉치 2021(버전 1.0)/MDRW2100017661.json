      "utterance": [
        {
          "id": "MDRW2100017661.1.1",
          "form": "머리로는 알지만 혀와 입이 계속 찾아서",
          "original_form": "머리로는 알지만 혀와 입이 계속 찾아서",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017661.1.2",
          "form": "빵도 얼마나 설탕이 많이 들어가는데요",
          "original_form": "빵도 얼마나 설탕이 많이 들어가는데요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017661.1.3",
          "form": "그 얘기 전에 들엇어요",
          "original_form": "그 얘기 전에 들엇어요",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017661.1.4",
          "form": "설탕이 포대로 들어가요 정말",
          "original_form": "설탕이 포대로 들어가요 정말",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017661.1.5",
          "form": "포대ㅋㅋㄱㅋㅋㅋ",
          "original_form": "포대ㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017661.1.6",
          "form": "정말로",
          "original_form": "정말로",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017661.1.7",
          "form": "빵은 진짜 끊기 어려운데",
          "original_form": "빵은 진짜 끊기 어려운데",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017661.1.8",
          "form": "왠지 줄이기는 해야할듯요",
          "original_form": "왠지 줄이기는 해야할듯요",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017661.1.9",
          "form": "떡을 먹어요",
          "original_form": "떡을 먹어요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017661.1.10",
          "form": "얼마나 맛있고 몸에 좋은지 몰라요",
          "original_form": "얼마나 맛있고 몸에 좋은지 몰라요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017661.1.11",
          "form": "저 떡도 좋아해요",
          "original_form": "저 떡도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017661.1.12",
          "form": "호박인절미 진짜 좋아하는데",
          "original_form": "호박인절미 진짜 좋아하는데",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017661.1.13",
          "form": "혹시 광주에 창억떡집이라고 알아요?",
          "original_form": "혹시 광주에 창억떡집이라고 알아요?",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017661.1.14",
          "form": "네네네!!! 거기요",
          "original_form": "네네네!!! 거기요",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017661.1.15",
          "form": "재료도 좋은거 쓰고 믿을 수 있는 집이에요",
          "original_form": "재료도 좋은거 쓰고 믿을 수 있는 집이에요",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017661.1.16",
          "form": "잘 아시나봐요",
          "original_form": "잘 아시나봐요",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017661.1.17",
          "form": "건너건너 조금 들은 이야기가 있어요",
          "original_form": "건너건너 조금 들은 이야기가 있어요",
          "speaker_id": "1",
          "time": "20210826 20:40"
        }
