      "utterance": [
        {
          "id": "MDRW2100005313.1.1",
          "form": "우리 티비에는 스크린이 없나보다",
          "original_form": "우리 티비에는 스크린이 없나보다",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005313.1.2",
          "form": "이거 오래된 영화만 보여주던데",
          "original_form": "이거 오래된 영화만 보여주던데",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005313.1.3",
          "form": "스크린 엣날에 채널 봤는데 아무리 채널 옮겨봐도 없음",
          "original_form": "스크린 엣날에 채널 봤는데 아무리 채널 옮겨봐도 없음",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005313.1.4",
          "form": "ㅋㅋㅋ 영화는 끝났다 또 채널 돌림",
          "original_form": "ㅋㅋㅋ 영화는 끝났다 또 채널 돌림",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005313.1.5",
          "form": "펜트하우스 오늘 막장이네",
          "original_form": "펜트하우스 오늘 막장이네",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005313.1.6",
          "form": "아 그런 드라마는 안 보신다ㅋ",
          "original_form": "아 그런 드라마는 안 보신다ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005313.1.7",
          "form": "엄청 길게 썻는데 나쁜말이다 ㅋㅋ",
          "original_form": "엄청 길게 썻는데 나쁜말이다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005313.1.8",
          "form": "다 망하고 경찰한테 잡혀간다",
          "original_form": "다 망하고 경찰한테 잡혀간다",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005313.1.9",
          "form": "도대체가 전혀 아닌데 나쁜말ㅋ 어느 채널에 정착 할지 모르겠다",
          "original_form": "도대체가 전혀 아닌데 나쁜말ㅋ 어느 채널에 정착 할지 모르겠다",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005313.1.10",
          "form": "단어가 그렇나봐 쓰 레 기 같은거",
          "original_form": "단어가 그렇나봐 쓰 레 기 같은거",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005313.1.11",
          "form": "존 트라볼타 나오는 그리스 영화 보시네",
          "original_form": "존 트라볼타 나오는 그리스 영화 보시네",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005313.1.12",
          "form": "난 신서유기6 재방이나 볼까",
          "original_form": "난 신서유기6 재방이나 볼까",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005313.1.13",
          "form": "존 트라볼타는 진짜 연기파 배우",
          "original_form": "존 트라볼타는 진짜 연기파 배우",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005313.1.14",
          "form": "고등학생 얼굴이 아니다ㅋ",
          "original_form": "고등학생 얼굴이 아니다ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005313.1.15",
          "form": "ㅋㅋ어렸을떼 노안이 난중에 동안이더라",
          "original_form": "ㅋㅋ어렸을떼 노안이 난중에 동안이더라",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005313.1.16",
          "form": "저때 얼굴이나 지금이나 똑같자나",
          "original_form": "저때 얼굴이나 지금이나 똑같자나",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005313.1.17",
          "form": "일본편인갑다 이게",
          "original_form": "일본편인갑다 이게",
