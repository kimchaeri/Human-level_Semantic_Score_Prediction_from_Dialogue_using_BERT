      "utterance": [
        {
          "id": "MDRW2100011984.1.1",
          "form": "저녁은 뭐드실 예정이에요?",
          "original_form": "저녁은 뭐드실 예정이에요?",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011984.1.2",
          "form": "저녁 삼겹살 생각하고있어요",
          "original_form": "저녁 삼겹살 생각하고있어요",
          "speaker_id": "1",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011984.1.3",
          "form": "삼겹살좋아하시나요",
          "original_form": "삼겹살좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011984.1.4",
          "form": "와~~~비오는날제대로네요",
          "original_form": "와~~~비오는날제대로네요",
          "speaker_id": "2",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011984.1.5",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011984.1.6",
          "form": "삼겹살 먹을 때",
          "original_form": "삼겹살 먹을 때",
          "speaker_id": "1",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011984.1.7",
          "form": "쌈 싸드시나요",
          "original_form": "쌈 싸드시나요",
          "speaker_id": "1",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011984.1.8",
          "form": "쌈싸서 한입에는 잘안먹고 따로 먹어요ㅎㅎ",
          "original_form": "쌈싸서 한입에는 잘안먹고 따로 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011984.1.9",
          "form": "쌈 좋아하세요?",
          "original_form": "쌈 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011984.1.10",
          "form": "저는 있으면 먹고 없으면 안먹고",
          "original_form": "저는 있으면 먹고 없으면 안먹고",
          "speaker_id": "1",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.11",
          "form": "상관없어요",
          "original_form": "상관없어요",
          "speaker_id": "1",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.12",
          "form": "그럼 뻔한질문",
          "original_form": "그럼 뻔한질문",
          "speaker_id": "1",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.13",
          "form": "진짜 열린 분이시다",
          "original_form": "진짜 열린 분이시다",
          "speaker_id": "2",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.14",
          "form": "나는 상추다 깻잎이다",
          "original_form": "나는 상추다 깻잎이다",
          "speaker_id": "1",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.15",
          "form": "음 상추....?",
          "original_form": "음 상추....?",
          "speaker_id": "2",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.16",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.17",
          "form": "왜요?ㅋㅋㅋㅋ",
          "original_form": "왜요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.18",
          "form": "깻잎파인가요?",
          "original_form": "깻잎파인가요?",
          "speaker_id": "2",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.19",
          "form": "저는 깻잎좋아해요 깻잎저만먹을 수 있게됐어요",
          "original_form": "저는 깻잎좋아해요 깻잎저만먹을 수 있게됐어요",
          "speaker_id": "1",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011984.1.20",
          "form": "완벽하네요",
          "original_form": "완벽하네요",
          "speaker_id": "2",
          "time": "20210824 18:07"
        }
      ]
