      "utterance": [
        {
          "id": "MDRW2100023736.1.1",
          "form": "물 택스쳐가 제일 그래픽 많이 먹는댔는데",
          "original_form": "물 택스쳐가 제일 그래픽 많이 먹는댔는데",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023736.1.2",
          "form": "그래도 모바일 게임에 이 용량은 너무해....",
          "original_form": "그래도 모바일 게임에 이 용량은 너무해....",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023736.1.3",
          "form": "그니까......",
          "original_form": "그니까......",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023736.1.4",
          "form": "게임 자체가 무거워서",
          "original_form": "게임 자체가 무거워서",
          "speaker_id": "1",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100023736.1.5",
          "form": "정작 그래픽 풀로 돌려보지도 못하는데...",
          "original_form": "정작 그래픽 풀로 돌려보지도 못하는데...",
          "speaker_id": "1",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100023736.1.6",
          "form": "ㄹㅇ....",
          "original_form": "ㄹㅇ....",
          "speaker_id": "2",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100023736.1.7",
          "form": "나 지금",
          "original_form": "나 지금",
          "speaker_id": "2",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100023736.1.8",
          "form": "한번씩 멈추는거 짜증나서",
          "original_form": "한번씩 멈추는거 짜증나서",
          "speaker_id": "2",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100023736.1.9",
          "form": "다 최저치로 맞춰놨어",
          "original_form": "다 최저치로 맞춰놨어",
          "speaker_id": "2",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100023736.1.10",
          "form": "나도 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100023736.1.11",
          "form": "랜더링이랑 음양 그래픽 사양 다 낮음인데",
          "original_form": "랜더링이랑 음양 그래픽 사양 다 낮음인데",
          "speaker_id": "1",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100023736.1.12",
          "form": "맨날 비행하다가 착륙하면",
          "original_form": "맨날 비행하다가 착륙하면",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023736.1.13",
          "form": "성벽이 덩어리된다구",
          "original_form": "성벽이 덩어리된다구",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023736.1.14",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ랜더링 낮으면",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ랜더링 낮으면",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023736.1.15",
          "form": "ㄹㅇ 죄다 덩어리야",
          "original_form": "ㄹㅇ 죄다 덩어리야",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023736.1.16",
          "form": "마비노기 하는 느낌이야",
          "original_form": "마비노기 하는 느낌이야",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023736.1.17",
          "form": "나 방금 받기 시작했는데",
          "original_form": "나 방금 받기 시작했는데",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100023736.1.18",
          "form": "와이파이로는 얼마나 걸리나 보자",
          "original_form": "와이파이로는 얼마나 걸리나 보자",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100023736.1.19",
          "form": "내가 데이터+속도제한이라 좀 심각하게 느린 경우일 수 있어 ㅋㅋㅋㅋ",
          "original_form": "내가 데이터+속도제한이라 좀 심각하게 느린 경우일 수 있어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:49"
        },
        {
          "id": "MDRW2100023736.1.20",
          "form": "나 한 15분 걸렸어",
          "original_form": "나 한 15분 걸렸어",
          "speaker_id": "2",
          "time": "20210830 16:50"
        },
        {
          "id": "MDRW2100023736.1.21",
          "form": "아니 난 3메가 제한인데",
          "original_form": "아니 난 3메가 제한인데",
          "speaker_id": "1",
          "time": "20210830 16:51"
        },
        {
          "id": "MDRW2100023736.1.22",
          "form": "오 빠르네",
          "original_form": "오 빠르네",
          "speaker_id": "1",
          "time": "20210830 16:51"
        },
        {
          "id": "MDRW2100023736.1.23",
          "form": "다운로드 속도가 왜 키바가 나오는거야;;",
          "original_form": "다운로드 속도가 왜 키바가 나오는거야;;",
          "speaker_id": "1",
          "time": "20210830 16:51"
        },
        {
          "id": "MDRW2100023736.1.24",
          "form": "그런거 보통 제한이라고 써있는것보다",
          "original_form": "그런거 보통 제한이라고 써있는것보다",
          "speaker_id": "2",
          "time": "20210830 16:51"
        },
        {
          "id": "MDRW2100023736.1.25",
          "form": "느리더라구...",
          "original_form": "느리더라구...",
          "speaker_id": "2",
          "time": "20210830 16:51"
        },
        {
          "id": "MDRW2100023736.1.26",
          "form": "이거 약간",
          "original_form": "이거 약간",
          "speaker_id": "1",
          "time": "20210830 16:51"
        },
        {
          "id": "MDRW2100023736.1.27",
          "form": "근데 게임 플레이 할때보다도",
          "original_form": "근데 게임 플레이 할때보다도",
          "speaker_id": "1",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.28",
          "form": "심각하게 느린 느낌",
          "original_form": "심각하게 느린 느낌",
          "speaker_id": "1",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.29",
          "form": "인게임은 오히려",
          "original_form": "인게임은 오히려",
          "speaker_id": "2",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.30",
          "form": "데이터 많이 안먹어",
          "original_form": "데이터 많이 안먹어",
          "speaker_id": "2",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.31",
          "form": "게임 플레이 할 때도",
          "original_form": "게임 플레이 할 때도",
          "speaker_id": "1",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.32",
          "form": "오른쪽 상단에",
          "original_form": "오른쪽 상단에",
          "speaker_id": "1",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.33",
          "form": "겁나 렉걸려서 그렇지",
          "original_form": "겁나 렉걸려서 그렇지",
          "speaker_id": "2",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.34",
          "form": "데이터 속도 뜨지않아?",
          "original_form": "데이터 속도 뜨지않아?",
          "speaker_id": "1",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.35",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "2",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.36",
          "form": "확인 안해봤어",
          "original_form": "확인 안해봤어",
          "speaker_id": "2",
          "time": "20210830 16:52"
        },
        {
          "id": "MDRW2100023736.1.37",
          "form": "일반 플레이 할 때는 속도가",
          "original_form": "일반 플레이 할 때는 속도가",
          "speaker_id": "1",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100023736.1.38",
          "form": "221메가 정도 나왔던거같은데",
          "original_form": "221메가 정도 나왔던거같은데",
          "speaker_id": "1",
          "time": "20210830 16:53"
        },
        {
          "id": "MDRW2100023736.1.39",
          "form": "패치파일 다운로드 속도 무슨일이야",
          "original_form": "패치파일 다운로드 속도 무슨일이야",
          "speaker_id": "1",
          "time": "20210830 16:53"
        }
      ]
