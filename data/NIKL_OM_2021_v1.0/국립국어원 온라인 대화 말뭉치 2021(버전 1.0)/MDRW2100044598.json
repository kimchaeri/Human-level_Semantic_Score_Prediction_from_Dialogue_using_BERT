      "utterance": [
        {
          "id": "MDRW2100044598.1.1",
          "form": "온콘은 3만원 넘어가면 돈아까워",
          "original_form": "온콘은 3만원 넘어가면 돈아까워",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044598.1.2",
          "form": "한성수 똑바로해",
          "original_form": "한성수 똑바로해",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.3",
          "form": "뉴이어콘같이 하면",
          "original_form": "뉴이어콘같이 하면",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.4",
          "form": "가만안둔다",
          "original_form": "가만안둔다",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.5",
          "form": "멱살잡아야함",
          "original_form": "멱살잡아야함",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.6",
          "form": "나 한때 너 테러하려고 ㅇ연장 모아놓은 사람이야....",
          "original_form": "나 한때 너 테러하려고 ㅇ연장 모아놓은 사람이야....",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.7",
          "form": "ㅋㅋㅋㅋ조심해...",
          "original_form": "ㅋㅋㅋㅋ조심해...",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.8",
          "form": "밤길 조심해 진짜",
          "original_form": "밤길 조심해 진짜",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.9",
          "form": "아직도 성수와의 만찬 하면 갈 의향있다",
          "original_form": "아직도 성수와의 만찬 하면 갈 의향있다",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.10",
          "form": "수수깡으로 확마 그냥",
          "original_form": "수수깡으로 확마 그냥",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.11",
          "form": "이제는",
          "original_form": "이제는",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044598.1.12",
          "form": "어디서 방돼지같은거랑 손을 잡아갸지고",
          "original_form": "어디서 방돼지같은거랑 손을 잡아갸지고",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.13",
          "form": "주 쟁점이",
          "original_form": "주 쟁점이",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.14",
          "form": "왜 하이브 들어갔냐",
          "original_form": "왜 하이브 들어갔냐",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.15",
          "form": "겠지만",
          "original_form": "겠지만",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아니 왜 일본시스템을 들여왔지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아니 왜 일본시스템을 들여왔지",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.17",
          "form": "일본을 사랑해서...",
          "original_form": "일본을 사랑해서...",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.18",
          "form": "어디서 그런 거지같은 시스템만 골라왔지",
          "original_form": "어디서 그런 거지같은 시스템만 골라왔지",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.19",
          "form": "정말 끔찎하다 끔찍해",
          "original_form": "정말 끔찎하다 끔찍해",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.20",
          "form": "추첨제 환멸이라고",
          "original_form": "추첨제 환멸이라고",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.21",
          "form": "아니그니까",
          "original_form": "아니그니까",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.22",
          "form": "뭐가공평한데",
          "original_form": "뭐가공평한데",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044598.1.23",
          "form": "티켓팅잘하는 사람은 티켓팅해서 가고싶다구요",
          "original_form": "티켓팅잘하는 사람은 티켓팅해서 가고싶다구요",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044598.1.24",
          "form": "망할 하느님석 가는것도 싫고",
          "original_form": "망할 하느님석 가는것도 싫고",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044598.1.25",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044598.1.26",
          "form": "내가 그자리에없는것도 싫어",
          "original_form": "내가 그자리에없는것도 싫어",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044598.1.27",
          "form": "그렇다고",
          "original_form": "그렇다고",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044598.1.28",
          "form": "가격차등제 있는것도 아니잖어",
          "original_form": "가격차등제 있는것도 아니잖어",
          "speaker_id": "2",
          "time": "20210907 23:13"
        }
      ]
    }
  ]
