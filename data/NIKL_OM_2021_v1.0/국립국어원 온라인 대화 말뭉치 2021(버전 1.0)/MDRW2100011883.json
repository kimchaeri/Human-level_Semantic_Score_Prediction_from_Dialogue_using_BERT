      "utterance": [
        {
          "id": "MDRW2100011883.1.1",
          "form": "올림픽 보셨어요?",
          "original_form": "올림픽 보셨어요?",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011883.1.2",
          "form": "네~여자배구 봤엉ㆍ",
          "original_form": "네~여자배구 봤엉ㆍ",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011883.1.3",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011883.1.4",
          "form": "아 저는 쫄려서 결과만 봤어요",
          "original_form": "아 저는 쫄려서 결과만 봤어요",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011883.1.5",
          "form": "선수 누구 좋아하세요?",
          "original_form": "선수 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011883.1.6",
          "form": "저는 농구선수 이상민 좋아했어요 님은요?",
          "original_form": "저는 농구선수 이상민 좋아했어요 님은요?",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100011883.1.7",
          "form": "아 저는 양궁선수들 좋아해요",
          "original_form": "아 저는 양궁선수들 좋아해요",
          "speaker_id": "1",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100011883.1.8",
          "form": "농구 좋아하시나봐요!",
          "original_form": "농구 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100011883.1.9",
          "form": "네~양궁선수 누구요?",
          "original_form": "네~양궁선수 누구요?",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011883.1.10",
          "form": "안산이랑 김제덕이용",
          "original_form": "안산이랑 김제덕이용",
          "speaker_id": "1",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011883.1.11",
          "form": "농구 직관 가보신적있나요?",
          "original_form": "농구 직관 가보신적있나요?",
          "speaker_id": "1",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011883.1.12",
          "form": "네~아주 예전이네요ㅜ",
          "original_form": "네~아주 예전이네요ㅜ",
          "speaker_id": "2",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011883.1.13",
          "form": "김제덕 귀엽죠 안산은 멋있고",
          "original_form": "김제덕 귀엽죠 안산은 멋있고",
          "speaker_id": "2",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011883.1.14",
          "form": "그쵸 둘다 애기인데 활쏠땐 너무 멋지더라구요",
          "original_form": "그쵸 둘다 애기인데 활쏠땐 너무 멋지더라구요",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011883.1.15",
          "form": "네 완전 아가인데 너무 멋져요~",
          "original_form": "네 완전 아가인데 너무 멋져요~",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100011883.1.16",
          "form": "김연경도 멋졌죠",
          "original_form": "김연경도 멋졌죠",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100011883.1.17",
          "form": "네 은퇴해서 너무 아쉬워요ㅠㅠ",
          "original_form": "네 은퇴해서 너무 아쉬워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100011883.1.18",
          "form": "그러게요ㅜ연경님도 언니죠",
          "original_form": "그러게요ㅜ연경님도 언니죠",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100011883.1.19",
          "form": "멋있으니깐~",
          "original_form": "멋있으니깐~",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100011883.1.20",
          "form": "마음같아선 더보고 싶지만 너무 힘들어하는게 눈에보이더라구요ㅠ",
          "original_form": "마음같아선 더보고 싶지만 너무 힘들어하는게 눈에보이더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210824 18:45"
        }
      ]
