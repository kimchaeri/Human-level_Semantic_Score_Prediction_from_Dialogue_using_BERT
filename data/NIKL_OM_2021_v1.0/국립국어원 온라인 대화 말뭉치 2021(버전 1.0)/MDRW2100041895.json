      },
      "utterance": [
        {
          "id": "MDRW2100041895.1.1",
          "form": "궁금하긴 하네요 그거 ㅋㅋ",
          "original_form": "궁금하긴 하네요 그거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041895.1.2",
          "form": "달여마시는거정도는 괜찮지 않을까 싶구",
          "original_form": "달여마시는거정도는 괜찮지 않을까 싶구",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041895.1.3",
          "form": "대추는 아직 싫지만 그래두 건강이구",
          "original_form": "대추는 아직 싫지만 그래두 건강이구",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041895.1.4",
          "form": "집에 가면 그럴게요 ㅋㅋㅋㅋ",
          "original_form": "집에 가면 그럴게요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041895.1.5",
          "form": "종강전에 함 가긴 하려구요 저두 가구싶으니까 ㅠ",
          "original_form": "종강전에 함 가긴 하려구요 저두 가구싶으니까 ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041895.1.6",
          "form": "대추차는 그렇게 기대 안되긴 하지만 마실게요",
          "original_form": "대추차는 그렇게 기대 안되긴 하지만 마실게요",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041895.1.7",
          "form": "보리차랑 대추차는 좀 다른 느낌이라ˋˋˋ",
          "original_form": "보리차랑 대추차는 좀 다른 느낌이라ˋˋˋ",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041895.1.8",
          "form": "갑자기 안눌려서 한번 해봤어요",
          "original_form": "갑자기 안눌려서 한번 해봤어요",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041895.1.9",
          "form": "이상하다 하지마라",
          "original_form": "이상하다 하지마라",
          "speaker_id": "1",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041895.1.10",
          "form": "네 ㅎ ㅠ",
          "original_form": "네 ㅎ ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041895.1.11",
          "form": "하여튼 둥굴레차도 보리차도 괜찬흥ㄴ데 대추차는 향이 너무 세요",
          "original_form": "하여튼 둥굴레차도 보리차도 괜찬흥ㄴ데 대추차는 향이 너무 세요",
          "speaker_id": "2",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041895.1.12",
          "form": "몸에 좋아서 쓴거 아냐?",
          "original_form": "몸에 좋아서 쓴거 아냐?",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041895.1.13",
          "form": "향긋하기만 하던데",
          "original_form": "향긋하기만 하던데",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041895.1.14",
          "form": "사람 취향이라는게 있다구요 ㅠ",
          "original_form": "사람 취향이라는게 있다구요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041895.1.15",
          "form": "편식하면 안돼 스물셋이나 돼서",
          "original_form": "편식하면 안돼 스물셋이나 돼서",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041895.1.16",
          "form": "편식 아니구 그냥 취향인데",
          "original_form": "편식 아니구 그냥 취향인데",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041895.1.17",
          "form": "그게 편식이지",
          "original_form": "그게 편식이지",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041895.1.18",
          "form": "아무튼 생수보다 보리차가 맛있긴 하니까요",
          "original_form": "아무튼 생수보다 보리차가 맛있긴 하니까요",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041895.1.19",
          "form": "그래? 입맛 다 길들여졌네 다행이다",
          "original_form": "그래? 입맛 다 길들여졌네 다행이다",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041895.1.20",
          "form": "길들여지다니 ㅋㅋㅋ 건강에 좋은가 보리차가",
          "original_form": "길들여지다니 ㅋㅋㅋ 건강에 좋은가 보리차가",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041895.1.21",
          "form": "맛은 있는데요",
          "original_form": "맛은 있는데요",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041895.1.22",
          "form": "엄마도 나이 드시니까 건강 챙기시네요 신기해",
          "original_form": "엄마도 나이 드시니까 건강 챙기시네요 신기해",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041895.1.23",
          "form": "원래 건강 챙겼지",
          "original_form": "원래 건강 챙겼지",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041895.1.24",
          "form": "맨날 라면 끓여먹으라고 하셨으면서",
          "original_form": "맨날 라면 끓여먹으라고 하셨으면서",
          "speaker_id": "2",
          "time": "20210906 17:22"
        }
      ]
    }
  ]
