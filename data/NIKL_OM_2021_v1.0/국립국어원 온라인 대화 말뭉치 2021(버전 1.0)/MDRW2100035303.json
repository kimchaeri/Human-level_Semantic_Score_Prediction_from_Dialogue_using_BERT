      "utterance": [
        {
          "id": "MDRW2100035303.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100035303.1.2",
          "form": "넵! 드셨어요?",
          "original_form": "넵! 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100035303.1.3",
          "form": "네 국밥 먹었어요!",
          "original_form": "네 국밥 먹었어요!",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100035303.1.4",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100035303.1.5",
          "form": "저는 골벵이 열무국수요!!",
          "original_form": "저는 골벵이 열무국수요!!",
          "speaker_id": "1",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100035303.1.6",
          "form": "오 오늘 대화하신분들 중 밥은 처음이에요",
          "original_form": "오 오늘 대화하신분들 중 밥은 처음이에요",
          "speaker_id": "1",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100035303.1.7",
          "form": "밥이 제일 무난한 것 같아서요 ㅎㅎ",
          "original_form": "밥이 제일 무난한 것 같아서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100035303.1.8",
          "form": "열무국수 시원하고 맛있겠어요!",
          "original_form": "열무국수 시원하고 맛있겠어요!",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100035303.1.9",
          "form": "그렇긴해용ㅋㅋ",
          "original_form": "그렇긴해용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035303.1.10",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035303.1.11",
          "form": "오랜만에 먹으니 더욱 맛있더라구요",
          "original_form": "오랜만에 먹으니 더욱 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035303.1.12",
          "form": "열무국수 잘하는 집 있는데 못 간지 오래 됐네요ㅠ",
          "original_form": "열무국수 잘하는 집 있는데 못 간지 오래 됐네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100035303.1.13",
          "form": "맛집 자주 가시나요?",
          "original_form": "맛집 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100035303.1.14",
          "form": "아.. 그거는 제가 다 아쉽네요ㅠ",
          "original_form": "아.. 그거는 제가 다 아쉽네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 14:48"
        },
        {
          "id": "MDRW2100035303.1.15",
          "form": "맛집 가고 싶은 친구있을때 따라가요!",
          "original_form": "맛집 가고 싶은 친구있을때 따라가요!",
          "speaker_id": "1",
          "time": "20210829 14:48"
        },
        {
          "id": "MDRW2100035303.1.16",
          "form": "맛집 자주 가세요?",
          "original_form": "맛집 자주 가세요?",
          "speaker_id": "1",
          "time": "20210829 14:48"
        },
        {
          "id": "MDRW2100035303.1.17",
          "form": "저는 가던 곳만 가서 많이는 몰라요 ㅎㅎ",
          "original_form": "저는 가던 곳만 가서 많이는 몰라요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:50"
        },
        {
          "id": "MDRW2100035303.1.18",
          "form": "주위에 맛집 좋아하는 친구 있으면 좋죠!\r\n새로운 문명 접하는 기분이고 ㅋㅋㅋ",
          "original_form": "주위에 맛집 좋아하는 친구 있으면 좋죠!\r\n새로운 문명 접하는 기분이고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:51"
        },
        {
          "id": "MDRW2100035303.1.19",
          "form": "맞아요ㅋㅋ 저도 가던곳만 가서 새로운걸 알려주는 사람이 참 고맙더라구요~",
          "original_form": "맞아요ㅋㅋ 저도 가던곳만 가서 새로운걸 알려주는 사람이 참 고맙더라구요~",
          "speaker_id": "1",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100035303.1.20",
          "form": "맛집 다녀보신 곳 중에 제일 신기한 곳이 있나요?",
          "original_form": "맛집 다녀보신 곳 중에 제일 신기한 곳이 있나요?",
          "speaker_id": "2",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100035303.1.21",
          "form": "음.. 신기한곳..",
          "original_form": "음.. 신기한곳..",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100035303.1.22",
          "form": "삼합에 버섯이였던게 제일 신기했었어요!",
          "original_form": "삼합에 버섯이였던게 제일 신기했었어요!",
          "speaker_id": "1",
          "time": "20210829 15:04"
        }
      ]
