      "utterance": [
        {
          "id": "MDRW2100033710.1.1",
          "form": "까오업헤어지고 그 폐인된 업 보려고.",
          "original_form": "까오업헤어지고 그 폐인된 업 보려고.",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.2",
          "form": "네네 스포밟앗는데",
          "original_form": "네네 스포밟앗는데",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.3",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.4",
          "form": "허어어억",
          "original_form": "허어어억",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.5",
          "form": "하 씁..",
          "original_form": "하 씁..",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.6",
          "form": "오케이.",
          "original_form": "오케이.",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.7",
          "form": "이미 그거부터 봐버린걸 어떡하냐고요.",
          "original_form": "이미 그거부터 봐버린걸 어떡하냐고요.",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.8",
          "form": "혹시 그거 보셨나요",
          "original_form": "혹시 그거 보셨나요",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네?",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.10",
          "form": "제 첫 중드인",
          "original_form": "제 첫 중드인",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.11",
          "form": "상은.",
          "original_form": "상은.",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.12",
          "form": "한때 핫했던 그 드라마",
          "original_form": "한때 핫했던 그 드라마",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033710.1.13",
          "form": "상은? 저 얘기만 듣고 못봤다고요,,,",
          "original_form": "상은? 저 얘기만 듣고 못봤다고요,,,",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033710.1.14",
          "form": "전정령 전에 상은",
          "original_form": "전정령 전에 상은",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033710.1.15",
          "form": "진정령인가 암튼",
          "original_form": "진정령인가 암튼",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033710.1.16",
          "form": "근데 드라마",
          "original_form": "근데 드라마",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033710.1.17",
          "form": "저 사실 중드는 저게 처음이에요 진정령",
          "original_form": "저 사실 중드는 저게 처음이에요 진정령",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033710.1.18",
          "form": "쓰레기거든요",
          "original_form": "쓰레기거든요",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033710.1.19",
          "form": "ㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아놔",
          "original_form": "ㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아놔",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033710.1.20",
          "form": "근데 비하인드 씬이..",
          "original_form": "근데 비하인드 씬이..",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033710.1.21",
          "form": "그 엔지컷이 찐이에요",
          "original_form": "그 엔지컷이 찐이에요",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033710.1.22",
          "form": "하 씁...",
          "original_form": "하 씁...",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033710.1.23",
          "form": "상은 드라마? 안봐도 된다.",
          "original_form": "상은 드라마? 안봐도 된다.",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033710.1.24",
          "form": "아니 중국애들은 왜 드라마 다 잘라먹고",
          "original_form": "아니 중국애들은 왜 드라마 다 잘라먹고",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033710.1.25",
          "form": "중드 특 : 드라마는 재미없고 오프레가 찐이다",
          "original_form": "중드 특 : 드라마는 재미없고 오프레가 찐이다",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033710.1.26",
          "form": "비하인드 씬에서는 찐인걸 티내요?",
          "original_form": "비하인드 씬에서는 찐인걸 티내요?",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033710.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033710.1.28",
          "form": "근데 진짜 진정령도",
          "original_form": "근데 진짜 진정령도",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033710.1.29",
          "form": "그렇지 않나요?",
          "original_form": "그렇지 않나요?",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033710.1.30",
          "form": "사람 망상갖게하고...나참;",
          "original_form": "사람 망상갖게하고...나참;",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033710.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033710.1.32",
          "form": "나 사실 진정령 친구들이 보라고 울부짖어도 안봒어요",
          "original_form": "나 사실 진정령 친구들이 보라고 울부짖어도 안봒어요",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033710.1.33",
          "form": "전 name2님이 러블리라이터 봐주신게 넘 감사해요",
          "original_form": "전 &name2&님이 러블리라이터 봐주신게 넘 감사해요",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033710.1.34",
          "form": "물론 태국애라 제가 영업한다고 뭐 달라지는 건 업ㄱ겠지만",
          "original_form": "물론 태국애라 제가 영업한다고 뭐 달라지는 건 업ㄱ겠지만",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033710.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ님 업 얼굴보고 안보는 사람이....있을리가...?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ님 업 얼굴보고 안보는 사람이....있을리가...?",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033710.1.36",
          "form": "업<얼굴보면 솔직히 그 독자좋아하는애들은 거의 볼 거 같은데...",
          "original_form": "업<얼굴보면 솔직히 그 독자좋아하는애들은 거의 볼 거 같은데...",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033710.1.37",
          "form": "근데 태국드라마는 처음보거든요",
          "original_form": "근데 태국드라마는 처음보거든요",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033710.1.38",
          "form": "하 근데 재밌네요 약간 뭐라해야하지 그 내용이 귀엽다고 해야하나",
          "original_form": "하 근데 재밌네요 약간 뭐라해야하지 그 내용이 귀엽다고 해야하나",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033710.1.39",
          "form": "중간에 까오랑 드라마 찍는애가 헛짓하잖아요?",
          "original_form": "중간에 까오랑 드라마 찍는애가 헛짓하잖아요?",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033710.1.40",
          "form": "그게 어이없어서 귀여운;",
          "original_form": "그게 어이없어서 귀여운;",
