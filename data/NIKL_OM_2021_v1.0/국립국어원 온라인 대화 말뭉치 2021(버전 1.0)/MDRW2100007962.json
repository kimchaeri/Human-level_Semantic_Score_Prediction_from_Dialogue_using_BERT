      "utterance": [
        {
          "id": "MDRW2100007962.1.1",
          "form": "당 대체품",
          "original_form": "당 대체품",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007962.1.2",
          "form": "그 당없는 아이스티 봤는데",
          "original_form": "그 당없는 아이스티 봤는데",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007962.1.3",
          "form": "그거 사볼려고",
          "original_form": "그거 사볼려고",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007962.1.4",
          "form": "뭐였지 이름이...아스파탐이나...",
          "original_form": "뭐였지 이름이...아스파탐이나...",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.6",
          "form": "그거 나도 사고싶더라",
          "original_form": "그거 나도 사고싶더라",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.7",
          "form": "ㄹㅇㄹ",
          "original_form": "ㄹㅇㄹ",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.8",
          "form": "탄산수만 마시는 거 질렸어",
          "original_form": "탄산수만 마시는 거 질렸어",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.9",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.10",
          "form": "탄산수 맛없어ㅠ",
          "original_form": "탄산수 맛없어ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.11",
          "form": "탄산수보다 보리차같은게 맛있지않아?",
          "original_form": "탄산수보다 보리차같은게 맛있지않아?",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.12",
          "form": "보리차 맨날 타먹기도 귀찮아서\r\n그래서 그거 사려고",
          "original_form": "보리차 맨날 타먹기도 귀찮아서\r\n그래서 그거 사려고",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007962.1.13",
          "form": "보리가루인데 커피맛 나는거",
          "original_form": "보리가루인데 커피맛 나는거",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007962.1.14",
          "form": "디카페인ㅎㅎ",
          "original_form": "디카페인ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007962.1.15",
          "form": "지금 마시는 보리차는",
          "original_form": "지금 마시는 보리차는",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007962.1.16",
          "form": "내가 8시간동안 냉침해서 우려야한단 말이야",
          "original_form": "내가 8시간동안 냉침해서 우려야한단 말이야",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007962.1.17",
          "form": "그건 그냥 물에 타기만 하면 된대",
          "original_form": "그건 그냥 물에 타기만 하면 된대",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007962.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007962.1.19",
          "form": "아스파탐은 뭐야?",
          "original_form": "아스파탐은 뭐야?",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007962.1.20",
          "form": "당 대체품",
          "original_form": "당 대체품",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007962.1.21",
          "form": "근데 아스파탐은 뒷맛이 별로라서",
          "original_form": "근데 아스파탐은 뒷맛이 별로라서",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007962.1.22",
          "form": "다른 게 더 좋댔어\r\n아까 그 아이스티가 더 좋은거 쓴댔어",
          "original_form": "다른 게 더 좋댔어\r\n아까 그 아이스티가 더 좋은거 쓴댔어",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007962.1.23",
          "form": "먹어보고 맛있으면 나도 사먹어볼까 싶어",
          "original_form": "먹어보고 맛있으면 나도 사먹어볼까 싶어",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007962.1.24",
          "form": "그랭",
          "original_form": "그랭",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007962.1.25",
          "form": "아스파탐은 안사도될것같아",
          "original_form": "아스파탐은 안사도될것같아",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007962.1.26",
          "form": "응ㅋㅋ",
          "original_form": "응ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007962.1.27",
          "form": "과자는 많이 먹어? 군것질류?",
          "original_form": "과자는 많이 먹어? 군것질류?",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007962.1.28",
          "form": "애당초 나는 요리할때 설탕안써",
          "original_form": "애당초 나는 요리할때 설탕안써",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007962.1.29",
          "form": "그건 좋다",
          "original_form": "그건 좋다",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007962.1.30",
          "form": "밖에서 사먹는 것들이 설탕넣으니깐ㅠ",
          "original_form": "밖에서 사먹는 것들이 설탕넣으니깐ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007962.1.31",
          "form": "그치. 굳이 집밥에는 설탕 안넣게 돼",
          "original_form": "그치. 굳이 집밥에는 설탕 안넣게 돼",
          "speaker_id": "2",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007962.1.32",
          "form": "유튜브에 레시피 찾아보다가도 설탕 들어간다싶으면 꺼버림ㅋㅋ",
          "original_form": "유튜브에 레시피 찾아보다가도 설탕 들어간다싶으면 꺼버림ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:32"
        }
      ]
    }
