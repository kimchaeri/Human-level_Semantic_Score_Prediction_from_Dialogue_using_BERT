      "utterance": [
        {
          "id": "MDRW2100018769.1.1",
          "form": "점심시간이네요",
          "original_form": "점심시간이네요",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018769.1.2",
          "form": "점심드셧나여",
          "original_form": "점심드셧나여",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018769.1.3",
          "form": "지금 먹고 있어요",
          "original_form": "지금 먹고 있어요",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018769.1.4",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018769.1.5",
          "form": "도시락 배달시켜 먹고있어요",
          "original_form": "도시락 배달시켜 먹고있어요",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018769.1.6",
          "form": "매일 메뉴가 바뀌나요?",
          "original_form": "매일 메뉴가 바뀌나요?",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018769.1.7",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018769.1.8",
          "form": "좋아하는 음식은?",
          "original_form": "좋아하는 음식은?",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018769.1.9",
          "form": "어떤건가요",
          "original_form": "어떤건가요",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018769.1.10",
          "form": "갈비 좋아해요",
          "original_form": "갈비 좋아해요",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018769.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018769.1.12",
          "form": "저는 다 잘먹는데",
          "original_form": "저는 다 잘먹는데",
          "speaker_id": "1",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018769.1.13",
          "form": "특히 면 종류",
          "original_form": "특히 면 종류",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018769.1.14",
          "form": "제일 좋아하는 면요리는 어떤거에요?",
          "original_form": "제일 좋아하는 면요리는 어떤거에요?",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018769.1.15",
          "form": "라면..칼국수 다...  고를수없어요",
          "original_form": "라면..칼국수 다...  고를수없어요",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018769.1.16",
          "form": "카페가면 뭐드세요?",
          "original_form": "카페가면 뭐드세요?",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018769.1.17",
          "form": "저는 케이크랑 레몬에이드 조합을 좋아해요",
          "original_form": "저는 케이크랑 레몬에이드 조합을 좋아해요",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018769.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018769.1.19",
          "form": "저는 무조건 아아",
          "original_form": "저는 무조건 아아",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018769.1.20",
          "form": "얼죽아 에요",
          "original_form": "얼죽아 에요",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018769.1.21",
          "form": "오ㅋㅋ 엄청 애정하시나봐요",
          "original_form": "오ㅋㅋ 엄청 애정하시나봐요",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018769.1.22",
          "form": "출근하자마자 때려넣어요",
          "original_form": "출근하자마자 때려넣어요",
          "speaker_id": "1",
          "time": "20210827 12:34"
