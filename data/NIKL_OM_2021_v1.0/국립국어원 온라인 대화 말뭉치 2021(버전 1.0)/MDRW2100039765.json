      "utterance": [
        {
          "id": "MDRW2100039765.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039765.1.2",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039765.1.3",
          "form": "아뇨 취준생입니다....ㅋㅋㅋㅋ큐ㅠㅠ",
          "original_form": "아뇨 취준생입니다....ㅋㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039765.1.4",
          "form": "큐ㅠㅠㅠ 그러시군요",
          "original_form": "큐ㅠㅠㅠ 그러시군요",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039765.1.5",
          "form": "저는 알바생이예요",
          "original_form": "저는 알바생이예요",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039765.1.6",
          "form": "앗 무슨알바하세요??",
          "original_form": "앗 무슨알바하세요??",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039765.1.7",
          "form": "맘스처치 알바해요 ㅎㅎ",
          "original_form": "맘스처치 알바해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039765.1.8",
          "form": "맘스터치요",
          "original_form": "맘스터치요",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039765.1.9",
          "form": "헉 맘스터치...뭔가... 힘드시겠다...",
          "original_form": "헉 맘스터치...뭔가... 힘드시겠다...",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039765.1.10",
          "form": "절대 하지마세요",
          "original_form": "절대 하지마세요",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039765.1.11",
          "form": "힘드니까요 ㅎㅎ",
          "original_form": "힘드니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039765.1.12",
          "form": "손님 엄청 많이 올 거 같아요...",
          "original_form": "손님 엄청 많이 올 거 같아요...",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039765.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ넵 안할게요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ넵 안할게요...",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039765.1.14",
          "form": "ㅋㅋㅋㅋ몰릴때 진짜 몰리더라구요",
          "original_form": "ㅋㅋㅋㅋ몰릴때 진짜 몰리더라구요",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039765.1.15",
          "form": "저도 관둘 타이밍 놓쳐서 하고있긴 한데 ㅋㅋㅋㅋ",
          "original_form": "저도 관둘 타이밍 놓쳐서 하고있긴 한데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039765.1.16",
          "form": "진짜 점심 저녁 엄청 힘들 거 같아요",
          "original_form": "진짜 점심 저녁 엄청 힘들 거 같아요",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039765.1.17",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039765.1.18",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039765.1.19",
          "form": "되게 많이 그만두더라구요",
          "original_form": "되게 많이 그만두더라구요",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039765.1.20",
          "form": "진짜 시켜만 드세요 ㅎㅎ",
          "original_form": "진짜 시켜만 드세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039765.1.21",
          "form": "어느 학과 나오셨어요?",
          "original_form": "어느 학과 나오셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039765.1.22",
          "form": "ㅋㅋㅋㅋㅋ저는 사범대 나왔어요",
          "original_form": "ㅋㅋㅋㅋㅋ저는 사범대 나왔어요",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039765.1.23",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039765.1.24",
          "form": "오오 멋지다",
          "original_form": "오오 멋지다",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039765.1.25",
          "form": "저는 물리치료학과 다니고 있어요",
          "original_form": "저는 물리치료학과 다니고 있어요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039765.1.26",
          "form": "물리치료!! 우왕 그게 더 멋진데요",
          "original_form": "물리치료!! 우왕 그게 더 멋진데요",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039765.1.27",
          "form": "그럼...막.......뼈와...근육과...관절을 완전 잘 아시겠네요",
          "original_form": "그럼...막.......뼈와...근육과...관절을 완전 잘 아시겠네요",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039765.1.28",
          "form": "저는 사범대가 더 멋있어요!! 앞에서 가르치는거!!",
          "original_form": "저는 사범대가 더 멋있어요!! 앞에서 가르치는거!!",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039765.1.29",
          "form": "ㅎㅎ열심히 뼈와 근육들을 암기하고 있습니다 ㅎㅎ",
          "original_form": "ㅎㅎ열심히 뼈와 근육들을 암기하고 있습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039765.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ근데 애들 무서워해요^^...쫄보라서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ근데 애들 무서워해요^^...쫄보라서",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039765.1.31",
          "form": "물리치료사 유튜브 짱 많이 보거든요 요즘ㅋㅋㅋㅋ",
          "original_form": "물리치료사 유튜브 짱 많이 보거든요 요즘ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039765.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ고등학생 가르치세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ고등학생 가르치세요?",
          "speaker_id": "1",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039765.1.33",
          "form": "아뇨 아직 일은 안해서...",
          "original_form": "아뇨 아직 일은 안해서...",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039765.1.34",
          "form": "고딩 중딩 초딩 다 가르쳐본 경험이 있긴 해요 알바같은 걸루",
          "original_form": "고딩 중딩 초딩 다 가르쳐본 경험이 있긴 해요 알바같은 걸루",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039765.1.35",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039765.1.36",
          "form": "근데 고딩은 좀 무서워요...ㅋㅋㅋㅋㅋ",
          "original_form": "근데 고딩은 좀 무서워요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039765.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039765.1.38",
          "form": "저는 초딩 가르쳐본 적 있는데",
          "original_form": "저는 초딩 가르쳐본 적 있는데",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039765.1.39",
          "form": "제가 키가 작다보니...요즘 남자애들 소나무만 하더라구요",
          "original_form": "제가 키가 작다보니...요즘 남자애들 소나무만 하더라구요",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039765.1.40",
