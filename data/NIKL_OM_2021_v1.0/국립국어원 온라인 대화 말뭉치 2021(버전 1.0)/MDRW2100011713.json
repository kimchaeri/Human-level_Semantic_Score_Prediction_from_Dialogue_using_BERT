      "utterance": [
        {
          "id": "MDRW2100011713.1.1",
          "form": "백종원 호텔도 좋다하데",
          "original_form": "백종원 호텔도 좋다하데",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011713.1.2",
          "form": "가족끼리 같이가면 좋을듯",
          "original_form": "가족끼리 같이가면 좋을듯",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011713.1.3",
          "form": "집안일 생각안하고 나가고 싶네",
          "original_form": "집안일 생각안하고 나가고 싶네",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011713.1.4",
          "form": "제주도에 가족끼리 가면 좋지",
          "original_form": "제주도에 가족끼리 가면 좋지",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011713.1.5",
          "form": "어 \n지금 베란다 엉망",
          "original_form": "어 \n지금 베란다 엉망",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011713.1.6",
          "form": "청소할라꼬?",
          "original_form": "청소할라꼬?",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011713.1.7",
          "form": "청소해야는데.. 제일 하기싫은곳",
          "original_form": "청소해야는데.. 제일 하기싫은곳",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011713.1.8",
          "form": "그치ㅜ",
          "original_form": "그치ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011713.1.9",
          "form": "집안일은 싫다ㅜ",
          "original_form": "집안일은 싫다ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011713.1.10",
          "form": "퐁퐁뿌려서 밀고싶다 베란다",
          "original_form": "퐁퐁뿌려서 밀고싶다 베란다",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011713.1.11",
          "form": "집안일은 티가안난다",
          "original_form": "집안일은 티가안난다",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011713.1.12",
          "form": "제일 힘든 일..끝이 없다",
          "original_form": "제일 힘든 일..끝이 없다",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011713.1.13",
          "form": "안하면 티가 엄청나고",
          "original_form": "안하면 티가 엄청나고",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011713.1.14",
          "form": "냉장고 청소도 언젠간 해야하는데",
          "original_form": "냉장고 청소도 언젠간 해야하는데",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011713.1.15",
          "form": "냉장고청소도 참 싫다ㅜ",
          "original_form": "냉장고청소도 참 싫다ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011713.1.16",
          "form": "어 반찬통 정리며",
          "original_form": "어 반찬통 정리며",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011713.1.17",
          "form": "난 한번도 안 해봤는데ㅜ",
          "original_form": "난 한번도 안 해봤는데ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011713.1.18",
          "form": "냉장고 엉망",
          "original_form": "냉장고 엉망",
          "speaker_id": "2",
          "time": "20210824 15:21"
        }
      ]
