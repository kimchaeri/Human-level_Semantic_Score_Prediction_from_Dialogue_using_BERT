      "utterance": [
        {
          "id": "MDRW2100039731.1.1",
          "form": "요즘 보고 있는 드라마나 영화 있으세요?",
          "original_form": "요즘 보고 있는 드라마나 영화 있으세요?",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100039731.1.2",
          "form": "네 어제 펜트하우스3봤어요",
          "original_form": "네 어제 펜트하우스3봤어요",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100039731.1.3",
          "form": "너무재미있어요ㅋ",
          "original_form": "너무재미있어요ㅋ",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100039731.1.4",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100039731.1.5",
          "form": "펜트하우스 핫하죠 ㅎㅎ",
          "original_form": "펜트하우스 핫하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100039731.1.6",
          "form": "전 해외쪽 좋아해요",
          "original_form": "전 해외쪽 좋아해요",
          "speaker_id": "2",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100039731.1.7",
          "form": "요번에 마블 영화 개봉해서 고민되요",
          "original_form": "요번에 마블 영화 개봉해서 고민되요",
          "speaker_id": "2",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100039731.1.8",
          "form": "마블 영화 좋아하세요?",
          "original_form": "마블 영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100039731.1.9",
          "form": "아 마블ㅋㅋ좋아해요ㅋㅋ",
          "original_form": "아 마블ㅋㅋ좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100039731.1.10",
          "form": "해외쪽 좋아하시는군요ㅋㅋ",
          "original_form": "해외쪽 좋아하시는군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100039731.1.11",
          "form": "저는 한국 히히ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 한국 히히ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100039731.1.12",
          "form": "역시 마블 인기가 많네요ㅎㅎ",
          "original_form": "역시 마블 인기가 많네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100039731.1.13",
          "form": "영국배우 좋아하게 되서 해외쪽 작품 보게 되더라고요",
          "original_form": "영국배우 좋아하게 되서 해외쪽 작품 보게 되더라고요",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100039731.1.14",
          "form": "아 그렇구나ㅋㅋ",
          "original_form": "아 그렇구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100039731.1.15",
          "form": "마블은 뭐 진짜",
          "original_form": "마블은 뭐 진짜",
          "speaker_id": "1",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100039731.1.16",
          "form": "누구나 다 좋아하는것같아요ㅋㅋ",
          "original_form": "누구나 다 좋아하는것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100039731.1.17",
          "form": "네 그냥 마블인 느낌이죠  ㅎㅎ",
          "original_form": "네 그냥 마블인 느낌이죠  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100039731.1.18",
          "form": "하나의 장르로 느껴져요",
          "original_form": "하나의 장르로 느껴져요",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100039731.1.19",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100039731.1.20",
          "form": "저도 개봉하면 보러가야겠어요ㅋㅋ",
          "original_form": "저도 개봉하면 보러가야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100039731.1.21",
          "form": "유튜브는 뭐 보세요?",
          "original_form": "유튜브는 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100039731.1.22",
          "form": "요즘 샹치 개봉했어요ㅋㅋ",
          "original_form": "요즘 샹치 개봉했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100039731.1.23",
          "form": "유튜는 주로 음악감상해요 주로 한방이나 기능의학 검색해서 봐요 아니면 미니멀이나 정리쪽이요",
          "original_form": "유튜는 주로 음악감상해요 주로 한방이나 기능의학 검색해서 봐요 아니면 미니멀이나 정리쪽이요",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100039731.1.24",
          "form": "아 저도 음악감상,정리쪽봐요ㅋㅋ",
          "original_form": "아 저도 음악감상,정리쪽봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100039731.1.25",
          "form": "아니면재테크ㅋ",
          "original_form": "아니면재테크ㅋ",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100039731.1.26",
          "form": "재테크 영상 어때요?",
          "original_form": "재테크 영상 어때요?",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100039731.1.27",
          "form": "저는 도움조금되는것같아요.",
          "original_form": "저는 도움조금되는것같아요.",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100039731.1.28",
          "form": "그냥 이거저거 보고있어요ㅋ",
          "original_form": "그냥 이거저거 보고있어요ㅋ",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100039731.1.29",
          "form": "보이는대로요",
          "original_form": "보이는대로요",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100039731.1.30",
          "form": "저도 좀 봐야갰어요 책보다 나을 것 같네요",
          "original_form": "저도 좀 봐야갰어요 책보다 나을 것 같네요",
          "speaker_id": "2",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100039731.1.31",
          "form": "네네ㅋ",
          "original_form": "네네ㅋ",
          "speaker_id": "1",
          "time": "20210904 14:26"
        }
      ]
