      "utterance": [
        {
          "id": "MDRW2100015679.1.1",
          "form": "요즘 앵무들은 어떠신지",
          "original_form": "요즘 앵무들은 어떠신지",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015679.1.2",
          "form": "빠르군",
          "original_form": "빠르군",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015679.1.3",
          "form": "앵짱들은 요즘 맘스터치",
          "original_form": "앵짱들은 요즘 맘스터치",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015679.1.4",
          "form": "받고 계십니다",
          "original_form": "받고 계십니다",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015679.1.5",
          "form": "어째서인지 밥을 잘 챙겨줘서 그런가",
          "original_form": "어째서인지 밥을 잘 챙겨줘서 그런가",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015679.1.6",
          "form": "둘째 빨강이는 털이 아주 반짝 거려",
          "original_form": "둘째 빨강이는 털이 아주 반짝 거려",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015679.1.7",
          "form": "미안 부족한 주인이라....",
          "original_form": "미안 부족한 주인이라....",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015679.1.8",
          "form": "맘스터치 최곤데?",
          "original_form": "맘스터치 최곤데?",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015679.1.9",
          "form": "신기해",
          "original_form": "신기해",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015679.1.10",
          "form": "이게 역시 내공이라는건가",
          "original_form": "이게 역시 내공이라는건가",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015679.1.11",
          "form": "원래 엄마손에 크면 뭐든 살찌고 포둥포둥",
          "original_form": "원래 엄마손에 크면 뭐든 살찌고 포둥포둥",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015679.1.12",
          "form": "식물도 똑같아",
          "original_form": "식물도 똑같아",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015679.1.13",
          "form": "그러니깐 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러니깐 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015679.1.14",
          "form": "내가하면 다 죽는데",
          "original_form": "내가하면 다 죽는데",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015679.1.15",
          "form": "앵무들은 장난감 필요없나?",
          "original_form": "앵무들은 장난감 필요없나?",
          "speaker_id": "1",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015679.1.16",
          "form": "아 장난감~",
          "original_form": "아 장난감~",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015679.1.17",
          "form": "작은애들은 그래도 사주면 몆 개월은 가는데",
          "original_form": "작은애들은 그래도 사주면 몆 개월은 가는데",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015679.1.18",
          "form": "큰애는 한시간 컷",
          "original_form": "큰애는 한시간 컷",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015679.1.19",
          "form": "몇 만원이 한시간만에 사라지는 매직",
          "original_form": "몇 만원이 한시간만에 사라지는 매직",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100015679.1.20",
          "form": "아 막 망가트려?ㅋㅋㅋ",
          "original_form": "아 막 망가트려?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015679.1.21",
          "form": "그래도 쓰는거가 다행이여",
          "original_form": "그래도 쓰는거가 다행이여",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015679.1.22",
          "form": "고양이들 보면 박스에만 들어가드만ㅋㅋ",
          "original_form": "고양이들 보면 박스에만 들어가드만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015679.1.23",
          "form": "응 뭐... 그렇져.. 벽지랑 장판도 해드심 ,ㅋㅋㅋㅋ",
          "original_form": "응 뭐... 그렇져.. 벽지랑 장판도 해드심 ,ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015679.1.24",
          "form": "거의 퍼즐조각 수준으로 아주 잘 씹어드셔서",
          "original_form": "거의 퍼즐조각 수준으로 아주 잘 씹어드셔서",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015679.1.25",
          "form": "에어팟도 드셨던가?",
          "original_form": "에어팟도 드셨던가?",
          "speaker_id": "1",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015679.1.26",
          "form": "진짜 반려동물 키우는사람들 대단해",
          "original_form": "진짜 반려동물 키우는사람들 대단해",
          "speaker_id": "1",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015679.1.27",
          "form": "그건.... 제가... 세탁기에",
          "original_form": "그건.... 제가... 세탁기에",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015679.1.28",
          "form": "바다 간접체험시켜드렸습니다",
          "original_form": "바다 간접체험시켜드렸습니다",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015679.1.29",
          "form": "나 앵짱집사 대단해",
          "original_form": "나 앵짱집사 대단해",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015679.1.30",
          "form": "좋은 주인이야 바다 구경도 시켜주고ㅋㅋㅋ",
          "original_form": "좋은 주인이야 바다 구경도 시켜주고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015679.1.31",
          "form": "진짜 앵무새 키우는것도 대단혀",
          "original_form": "진짜 앵무새 키우는것도 대단혀",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015679.1.32",
          "form": "소리 맨날 지르잖아...",
          "original_form": "소리 맨날 지르잖아...",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015679.1.33",
          "form": "난 못버텨",
          "original_form": "난 못버텨",
          "speaker_id": "1",
          "time": "20210826 00:24"
        }
      ]
    }
