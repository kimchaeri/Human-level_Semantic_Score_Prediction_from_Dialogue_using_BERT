      "utterance": [
        {
          "id": "MDRW2100034544.1.1",
          "form": "집에서 영화보면 딱인 하루네요",
          "original_form": "집에서 영화보면 딱인 하루네요",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100034544.1.2",
          "form": "네 ㅋㅋㅋㅋ안그래도 이제 하나 틀어보려구요",
          "original_form": "네 ㅋㅋㅋㅋ안그래도 이제 하나 틀어보려구요",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034544.1.3",
          "form": "심심이 너무 오래했더니 손목아파요ㅋㅋㅋㅋ",
          "original_form": "심심이 너무 오래했더니 손목아파요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100034544.1.4",
          "form": "앗ㅋㅋ 저두요ㅋㅋㅋ",
          "original_form": "앗ㅋㅋ 저두요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100034544.1.5",
          "form": "잠시 쉬고 있었어요",
          "original_form": "잠시 쉬고 있었어요",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100034544.1.6",
          "form": "뭐보세요?",
          "original_form": "뭐보세요?",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100034544.1.7",
          "form": "저 지난주 대탈출이요 ㅋㅋㅋ",
          "original_form": "저 지난주 대탈출이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034544.1.8",
          "form": "오! 저 챙겨보고있어요",
          "original_form": "오! 저 챙겨보고있어요",
          "speaker_id": "1",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100034544.1.9",
          "form": "1ㅡ2화에서 실망했는데말이죠",
          "original_form": "1ㅡ2화에서 실망했는데말이죠",
          "speaker_id": "1",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100034544.1.10",
          "form": "지난주껀 진짜 엄청나네요 세트장이 ㅋㅋㅋ",
          "original_form": "지난주껀 진짜 엄청나네요 세트장이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100034544.1.11",
          "form": "진짜요ㅋㅋ",
          "original_form": "진짜요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100034544.1.12",
          "form": "체험해보고싶어요",
          "original_form": "체험해보고싶어요",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034544.1.13",
          "form": "그러게여 아쉽지만 방탈출이라도 하고싶네요 ㅋㅋㅋ",
          "original_form": "그러게여 아쉽지만 방탈출이라도 하고싶네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100034544.1.14",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:21"
        },
        {
          "id": "MDRW2100034544.1.15",
          "form": "집에서 폰으로라도 해야겠어요ㅋㅋ",
          "original_form": "집에서 폰으로라도 해야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:21"
        },
        {
          "id": "MDRW2100034544.1.16",
          "form": "앜ㅋㅋ폰게임 방탈출도 은근 자주했었는데 재밌져",
          "original_form": "앜ㅋㅋ폰게임 방탈출도 은근 자주했었는데 재밌져",
          "speaker_id": "2",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100034544.1.17",
          "form": "넹ㅋㅋ 시간가는줄 모르고 했어요ㅋㅋㅋ",
          "original_form": "넹ㅋㅋ 시간가는줄 모르고 했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100034544.1.18",
          "form": "코로나 전에 친구랑 둘이 롯데리아 앉아서 자정까지 했던 기억나요",
          "original_form": "코로나 전에 친구랑 둘이 롯데리아 앉아서 자정까지 했던 기억나요",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100034544.1.19",
          "form": "우와ㅋㅋ 알거같아요",
          "original_form": "우와ㅋㅋ 알거같아요",
          "speaker_id": "1",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034544.1.20",
          "form": "그정도 파급력이있죠",
          "original_form": "그정도 파급력이있죠",
          "speaker_id": "1",
