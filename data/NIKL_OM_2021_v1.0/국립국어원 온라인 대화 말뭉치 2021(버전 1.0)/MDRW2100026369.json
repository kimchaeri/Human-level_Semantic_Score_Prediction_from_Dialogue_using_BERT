      "utterance": [
        {
          "id": "MDRW2100026369.1.1",
          "form": "혹시 공연 같은 것도 보세요?",
          "original_form": "혹시 공연 같은 것도 보세요?",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026369.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026369.1.3",
          "form": "재밌게 보셨던 공연 추천 부탁드려도 될까요?",
          "original_form": "재밌게 보셨던 공연 추천 부탁드려도 될까요?",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026369.1.4",
          "form": "헤드윅 많이 봤어요",
          "original_form": "헤드윅 많이 봤어요",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026369.1.5",
          "form": "name1은요",
          "original_form": "&name1&은요",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026369.1.6",
          "form": "저는 가장 최근에 본게 그날들이네요",
          "original_form": "저는 가장 최근에 본게 그날들이네요",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026369.1.7",
          "form": "어? 헤드윅 이번에 다시 시작하지 않나요?",
          "original_form": "어? 헤드윅 이번에 다시 시작하지 않나요?",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026369.1.8",
          "form": "아 그렇군요\r\n하고있는걸로알고있어요",
          "original_form": "아 그렇군요\r\n하고있는걸로알고있어요",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026369.1.9",
          "form": "저도 한번쯤 보고싶은 공연이에요!",
          "original_form": "저도 한번쯤 보고싶은 공연이에요!",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026369.1.10",
          "form": "좋아하시는 장르는 있나요?",
          "original_form": "좋아하시는 장르는 있나요?",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026369.1.11",
          "form": "공연 장르요?",
          "original_form": "공연 장르요?",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026369.1.12",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026369.1.13",
          "form": "뮤지컬? 좋아하세요?",
          "original_form": "뮤지컬? 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026369.1.14",
          "form": "저는 연극은 본적이 없거든요",
          "original_form": "저는 연극은 본적이 없거든요",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026369.1.15",
          "form": "아 저는 뮤지컬 연극 둘다 좋아해요",
          "original_form": "아 저는 뮤지컬 연극 둘다 좋아해요",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026369.1.16",
          "form": "가리는거 없어요 연주회도 좋아해요",
          "original_form": "가리는거 없어요 연주회도 좋아해요",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026369.1.17",
          "form": "우와 연주회도 가시는군요!",
          "original_form": "우와 연주회도 가시는군요!",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026369.1.18",
          "form": "피아노연주회를 좋아해서요",
          "original_form": "피아노연주회를 좋아해서요",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026369.1.19",
          "form": "오 좋아하시는 연주자가 있으세요?",
          "original_form": "오 좋아하시는 연주자가 있으세요?",
          "speaker_id": "1",
          "time": "20210901 22:16"
        }
