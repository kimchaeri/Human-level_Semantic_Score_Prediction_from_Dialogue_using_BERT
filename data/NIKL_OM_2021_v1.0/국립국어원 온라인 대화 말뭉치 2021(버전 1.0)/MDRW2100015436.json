      "utterance": [
        {
          "id": "MDRW2100015436.1.1",
          "form": "종교는 무었인가요?",
          "original_form": "종교는 무었인가요?",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015436.1.2",
          "form": "저는 무교입니다",
          "original_form": "저는 무교입니다",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015436.1.3",
          "form": "엇ㅎ 저도 무교ㅎㅎ",
          "original_form": "엇ㅎ 저도 무교ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015436.1.4",
          "form": "무교인 이유가?ㅎ",
          "original_form": "무교인 이유가?ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015436.1.5",
          "form": "공감이 안 가더라구요",
          "original_form": "공감이 안 가더라구요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015436.1.6",
          "form": "전 제자신을 믿어서 그래요ㅎㅎㅎ",
          "original_form": "전 제자신을 믿어서 그래요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015436.1.7",
          "form": "너무 신격화 시키기도 했고",
          "original_form": "너무 신격화 시키기도 했고",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015436.1.8",
          "form": "결국 선택은 제 몫이니까요",
          "original_form": "결국 선택은 제 몫이니까요",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015436.1.9",
          "form": "무슨 박혁거세 같이 소설을 써놨으니ㅎ 믿기질 않아서ㅎ",
          "original_form": "무슨 박혁거세 같이 소설을 써놨으니ㅎ 믿기질 않아서ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015436.1.10",
          "form": "맞아요ㅎ 종교은 본인의 선택이니깐요ㅎ",
          "original_form": "맞아요ㅎ 종교은 본인의 선택이니깐요ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015436.1.11",
          "form": "제 의지는 아니고 어쩌다 교회도 성당도 가봤었는데 말씀에 공감이 안 가더라구요",
          "original_form": "제 의지는 아니고 어쩌다 교회도 성당도 가봤었는데 말씀에 공감이 안 가더라구요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015436.1.12",
          "form": "맞아요ㅠ 전 왜 기도를 하는지모르겍어요ㅍ",
          "original_form": "맞아요ㅠ 전 왜 기도를 하는지모르겍어요ㅍ",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015436.1.13",
          "form": "기도하면 뭐 주나요? 뭐가 생기나요?",
          "original_form": "기도하면 뭐 주나요? 뭐가 생기나요?",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015436.1.14",
          "form": "마음의 안정일까요?ㅋㅋ",
          "original_form": "마음의 안정일까요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015436.1.15",
          "form": "그럼 눈감고 쉬면 되죠ㅎㄹ",
          "original_form": "그럼 눈감고 쉬면 되죠ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015436.1.16",
          "form": "아무것도안하고ㅎ 모여서기도하지말고ㅎㅎ",
          "original_form": "아무것도안하고ㅎ 모여서기도하지말고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015436.1.17",
          "form": "이거 어디가서 누가캡쳐하면 욕먹겟네요ㅎㄹ",
          "original_form": "이거 어디가서 누가캡쳐하면 욕먹겟네요ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015436.1.18",
          "form": "꼭 교회에 가야만 하는지는 모르겠어요",
          "original_form": "꼭 교회에 가야만 하는지는 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015436.1.19",
          "form": "그냥 집에서 각자 마음으로 챙겨도 될 것 같은데",
          "original_form": "그냥 집에서 각자 마음으로 챙겨도 될 것 같은데",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015436.1.20",
          "form": "맞아요ㅎㅎ 공감해요ㅎ",
          "original_form": "맞아요ㅎㅎ 공감해요ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015436.1.21",
          "form": "종교는 제가봤을땐 공동체의 산물같애요",
          "original_form": "종교는 제가봤을땐 공동체의 산물같애요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015436.1.22",
          "form": "전쟁이든 뭐든 사람들을 하나로 뭉쳐야 하는데 구실이 필요했던거죠",
          "original_form": "전쟁이든 뭐든 사람들을 하나로 뭉쳐야 하는데 구실이 필요했던거죠",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015436.1.23",
          "form": "사람 만나러 가는 것도 있는 것 같아요",
          "original_form": "사람 만나러 가는 것도 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015436.1.24",
          "form": "예로부터 샤머니즘 처럼",
          "original_form": "예로부터 샤머니즘 처럼",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015436.1.25",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015436.1.26",
          "form": "한국은 종교 없어도 사는 데 문제없어서 다행이에요",
