        },
        {
          "id": "MDRW2100027653.1.7",
          "form": "감사합니다 ^^",
          "original_form": "감사합니다 ^^",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027653.1.8",
          "form": "발표할까요 감상문 쓸까요",
          "original_form": "발표할까요 감상문 쓸까요",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027653.1.9",
          "form": "발표하면 종교에 대해 발표해야 하고",
          "original_form": "발표하면 종교에 대해 발표해야 하고",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027653.1.10",
          "form": "감상문은 종교 관련 책을 읽고 써야해요",
          "original_form": "감상문은 종교 관련 책을 읽고 써야해요",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027653.1.11",
          "form": "발표가 그럼 자기 종교에 대해 발표하는거에요??",
          "original_form": "발표가 그럼 자기 종교에 대해 발표하는거에요??",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027653.1.12",
          "form": "아니면 종교 조사해서 발표하는 거?",
          "original_form": "아니면 종교 조사해서 발표하는 거?",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027653.1.13",
          "form": "종교 조사요!!",
          "original_form": "종교 조사요!!",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027653.1.14",
          "form": "근데 가끔 자기 종교 발표하다가 전도하규 포교하려는 사람들도 있대요",
          "original_form": "근데 가끔 자기 종교 발표하다가 전도하규 포교하려는 사람들도 있대요",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027653.1.15",
          "form": "그러지 말라고 하셨음",
          "original_form": "그러지 말라고 하셨음",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027653.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100027653.1.17",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027653.1.18",
          "form": "어차피 조사하는건 같은데 무조건 감상문이죠~",
          "original_form": "어차피 조사하는건 같은데 무조건 감상문이죠~",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027653.1.19",
          "form": "근디 대학교에 진짜 포교하는 사람 많은듯",
          "original_form": "근디 대학교에 진짜 포교하는 사람 많은듯",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027653.1.20",
          "form": "맞아요 특히 사이비 종교 같은 곳은...특히 대학생 여자 상대로 많이 붙잡는 것 같어요",
          "original_form": "맞아요 특히 사이비 종교 같은 곳은...특히 대학생 여자 상대로 많이 붙잡는 것 같어요",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027653.1.21",
          "form": "길거리에서 그런거 얘기 들어주고 있는 여자 보면 안쓰러운데 뭔가 껴들기도 애매하고",
          "original_form": "길거리에서 그런거 얘기 들어주고 있는 여자 보면 안쓰러운데 뭔가 껴들기도 애매하고",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100027653.1.22",
          "form": "사이비는 죄악이에여",
          "original_form": "사이비는 죄악이에여",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100027653.1.23",
          "form": "근데 이런얘기 해도 되나",
          "original_form": "근데 이런얘기 해도 되나",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100027653.1.24",
          "form": "종교 얘기니까 괜찮겠죠",
          "original_form": "종교 얘기니까 괜찮겠죠",
          "speaker_id": "1",
          "time": "20210902 14:45"
        },
        {
          "id": "MDRW2100027653.1.25",
          "form": "종교 얘기니까 괜찮죠 어쩌겠어요 사이비인데ㅡㅡ",
          "original_form": "종교 얘기니까 괜찮죠 어쩌겠어요 사이비인데ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100027653.1.26",
          "form": "학교 앞에서 밥먹을때도",
          "original_form": "학교 앞에서 밥먹을때도",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100027653.1.27",
          "form": "뭔가 처음 만난 것 같은데",
          "original_form": "뭔가 처음 만난 것 같은데",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100027653.1.28",
          "form": "심층적인 대화를 한다? 싶으면",
          "original_form": "심층적인 대화를 한다? 싶으면",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100027653.1.29",
          "form": "사이비임.",
          "original_form": "사이비임.",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100027653.1.30",
          "form": "ㄹㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ갑작이 다가와서 설문조사해달라고 시작해서",
          "original_form": "ㄹㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ갑작이 다가와서 설문조사해달라고 시작해서",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100027653.1.31",
          "form": "종교 있냐고 물어보고",
          "original_form": "종교 있냐고 물어보고",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100027653.1.32",
          "form": "성경공부하자고 함",
          "original_form": "성경공부하자고 함",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100027653.1.33",
          "form": "백퍼 사이비죠",
          "original_form": "백퍼 사이비죠",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100027653.1.34",
          "form": "근데 왜 주변에 가까운 사이비는",
          "original_form": "근데 왜 주변에 가까운 사이비는",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100027653.1.35",
          "form": "다 성경으로 시작할까요",
          "original_form": "다 성경으로 시작할까요",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100027653.1.36",
          "form": "성경 말고 뭐 할 게 그렇게 없나",
          "original_form": "성경 말고 뭐 할 게 그렇게 없나",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100027653.1.37",
          "form": "오해하지 말고 들어주세요",
          "original_form": "오해하지 말고 들어주세요",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100027653.1.38",
          "form": "저 친구중에 사이비 종교 다니는애 있었는데",
          "original_form": "저 친구중에 사이비 종교 다니는애 있었는데",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100027653.1.39",
          "form": "저한테 한 번 더 성경얘기하면 손절한다고 했거든요",
          "original_form": "저한테 한 번 더 성경얘기하면 손절한다고 했거든요",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100027653.1.40",
          "form": "근데 머라하지..자기네들 성경에 자부심이 있다 해야하나",
          "original_form": "근데 머라하지..자기네들 성경에 자부심이 있다 해야하나",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100027653.1.41",
          "form": "어차피 저는 무교라서 성경 얘기 해봤자 거기서 거기라 영업도 안당하는데 쩝",
          "original_form": "어차피 저는 무교라서 성경 얘기 해봤자 거기서 거기라 영업도 안당하는데 쩝",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100027653.1.42",
          "form": "그래서 오히려 무교보다 교인이 더 넘어가기 쉽대요",
          "original_form": "그래서 오히려 무교보다 교인이 더 넘어가기 쉽대요",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100027653.1.43",
          "form": "엥!!!!",
          "original_form": "엥!!!!",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027653.1.44",
          "form": "근데 확실히 성경이 해석따라 다 달라지는 부분이 있대니까",
          "original_form": "근데 확실히 성경이 해석따라 다 달라지는 부분이 있대니까",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027653.1.45",
          "form": "그렇게될지도 몰겠네요....",
          "original_form": "그렇게될지도 몰겠네요....",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027653.1.46",
          "form": "무교는 성경 자체를 안 믿고 교인은 일단 성경을 믿으니까 그런가",
          "original_form": "무교는 성경 자체를 안 믿고 교인은 일단 성경을 믿으니까 그런가",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027653.1.47",
          "form": "네 그런 맥락인 것 같아요 무교는 아예 안 믿으니까 딴거 들어도 뭐어쩔? 되는 거고",
          "original_form": "네 그런 맥락인 것 같아요 무교는 아예 안 믿으니까 딴거 들어도 뭐어쩔? 되는 거고",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100027653.1.48",
          "form": "종교가 있는 사람은 솔깃할 여지가 있는? 그정도",
          "original_form": "종교가 있는 사람은 솔깃할 여지가 있는? 그정도",
