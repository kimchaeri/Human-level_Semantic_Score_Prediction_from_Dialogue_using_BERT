      "utterance": [
        {
          "id": "MDRW2100006052.1.1",
          "form": "날씨 어떤가요?",
          "original_form": "날씨 어떤가요?",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100006052.1.2",
          "form": "오늘 하루종일 비가 오네요",
          "original_form": "오늘 하루종일 비가 오네요",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006052.1.3",
          "form": "그러시군요 여긴 갰다 왔다 변덕스러웠어요",
          "original_form": "그러시군요 여긴 갰다 왔다 변덕스러웠어요",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100006052.1.4",
          "form": "여기는 계속 오고 있어요",
          "original_form": "여기는 계속 오고 있어요",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006052.1.5",
          "form": "비가 넘 자주 오네요",
          "original_form": "비가 넘 자주 오네요",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006052.1.6",
          "form": "벌써 2주째인가요...3주째인가요..가을장마같아요",
          "original_form": "벌써 2주째인가요...3주째인가요..가을장마같아요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006052.1.7",
          "form": "채소 과일값이 올라 가겠죠",
          "original_form": "채소 과일값이 올라 가겠죠",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006052.1.8",
          "form": "오늘 복숭아 한박스 사는데 3만2천원이더라구요 비쌌어요",
          "original_form": "오늘 복숭아 한박스 사는데 3만2천원이더라구요 비쌌어요",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100006052.1.9",
          "form": "비가 많이와서 떨어진 과일도 병든 과일도 많아서 그렇겠죠",
          "original_form": "비가 많이와서 떨어진 과일도 병든 과일도 많아서 그렇겠죠",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100006052.1.10",
          "form": "그렇죠 비가와서 맛도 더 싱거운듯했어요 ㅠㅠ",
          "original_form": "그렇죠 비가와서 맛도 더 싱거운듯했어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006052.1.11",
          "form": "추석이 다가오니 더 가격이 올라가겠죠",
          "original_form": "추석이 다가오니 더 가격이 올라가겠죠",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006052.1.12",
          "form": "아 그러겠네요 명절 특수...",
          "original_form": "아 그러겠네요 명절 특수...",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100006052.1.13",
          "form": "제사지내기가 점점 힘들어지네요",
          "original_form": "제사지내기가 점점 힘들어지네요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006052.1.14",
          "form": "좋아하는 계절은요?",
          "original_form": "좋아하는 계절은요?",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006052.1.15",
          "form": "전 추운것보단 더운 계절이 좋아요",
          "original_form": "전 추운것보단 더운 계절이 좋아요",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006052.1.16",
          "form": "봄 여름이 좋아요",
          "original_form": "봄 여름이 좋아요",
          "speaker_id": "2",
          "time": "20210907 21:46"
