        },
        {
          "id": "MDRW2100045565.1.2",
          "form": "저 보고있습니다",
          "original_form": "저 보고있습니다",
          "speaker_id": "1",
          "time": "20210908 06:23"
        },
        {
          "id": "MDRW2100045565.1.3",
          "form": "오래 살아계시네요",
          "original_form": "오래 살아계시네요",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045565.1.4",
          "form": "그런 듯요",
          "original_form": "그런 듯요",
          "speaker_id": "1",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045565.1.5",
          "form": "차 고르시는즁이네요",
          "original_form": "차 고르시는즁이네요",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045565.1.6",
          "form": "부품이 많이 망가졌네요",
          "original_form": "부품이 많이 망가졌네요",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045565.1.7",
          "form": "고치기는 힘들 듯",
          "original_form": "고치기는 힘들 듯",
          "speaker_id": "2",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045565.1.8",
          "form": "유리창도 없대요",
          "original_form": "유리창도 없대요",
          "speaker_id": "1",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045565.1.9",
          "form": "많은 일이 있었군요",
          "original_form": "많은 일이 있었군요",
          "speaker_id": "2",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045565.1.10",
          "form": "유리를 옮기시는 중",
          "original_form": "유리를 옮기시는 중",
          "speaker_id": "1",
          "time": "20210908 06:28"
        },
        {
          "id": "MDRW2100045565.1.11",
          "form": "그런 것도 되는군요",
          "original_form": "그런 것도 되는군요",
          "speaker_id": "2",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045565.1.12",
          "form": "버그 걸려서 할까맒가 하시는중",
