      "utterance": [
        {
          "id": "MDRW2100025823.1.1",
          "form": "요즘 뭔 음료 좋아하세요?",
          "original_form": "요즘 뭔 음료 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025823.1.2",
          "form": "커피?",
          "original_form": "커피?",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025823.1.3",
          "form": "탄산?",
          "original_form": "탄산?",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025823.1.4",
          "form": "커피좋아요",
          "original_form": "커피좋아요",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025823.1.5",
          "form": "커피 어느 브랜드 좋아해요?",
          "original_form": "커피 어느 브랜드 좋아해요?",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025823.1.6",
          "form": "음 브랜드커피는 너무 쓰더라구요",
          "original_form": "음 브랜드커피는 너무 쓰더라구요",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025823.1.7",
          "form": "아니면 익숙하지않거나..",
          "original_form": "아니면 익숙하지않거나..",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025823.1.8",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025823.1.9",
          "form": "그럼 뭐 드세요?",
          "original_form": "그럼 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025823.1.10",
          "form": "커피는 개인카페로 가는거같고",
          "original_form": "커피는 개인카페로 가는거같고",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025823.1.11",
          "form": "브랜드커피는.. 특별한거먹는거같아요",
          "original_form": "브랜드커피는.. 특별한거먹는거같아요",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025823.1.12",
          "form": "특별한 날이...언제에요?????",
          "original_form": "특별한 날이...언제에요?????",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025823.1.13",
          "form": "특별한메뉴를 먹어요",
          "original_form": "특별한메뉴를 먹어요",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025823.1.14",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025823.1.15",
          "form": "아 케이크요??ㅋㅋㅋ",
          "original_form": "아 케이크요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025823.1.16",
          "form": "어떤 케이크 좋아하세요?",
          "original_form": "어떤 케이크 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025823.1.17",
          "form": "저는 생크림케이크아좋아요",
          "original_form": "저는 생크림케이크아좋아요",
          "speaker_id": "2",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100025823.1.18",
          "form": "저는 모카케익요!",
          "original_form": "저는 모카케익요!",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025823.1.19",
          "form": "파는데가 별루없어요 ㅍ",
          "original_form": "파는데가 별루없어요 ㅍ",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025823.1.20",
          "form": "모카케익 좋아하시는구나",
          "original_form": "모카케익 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025823.1.21",
          "form": "네 정항우 존맛인거같아요",
          "original_form": "네 정항우 존맛인거같아요",
          "speaker_id": "1",
          "time": "20210901 17:44"
        }
      ]
