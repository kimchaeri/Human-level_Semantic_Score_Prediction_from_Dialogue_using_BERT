        },
        {
          "id": "MDRW2100033887.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100033887.1.3",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100033887.1.4",
          "form": "네 주로 다시보기용으로 쓰는 것 같아요",
          "original_form": "네 주로 다시보기용으로 쓰는 것 같아요",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100033887.1.5",
          "form": "넷플릭스 보세요?",
          "original_form": "넷플릭스 보세요?",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100033887.1.6",
          "form": "이번달에 해지했어요",
          "original_form": "이번달에 해지했어요",
          "speaker_id": "1",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100033887.1.7",
          "form": "뭐 주로 보세요?",
          "original_form": "뭐 주로 보세요?",
          "speaker_id": "1",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100033887.1.8",
          "form": "드라마 다시보기용으로 거의 쓰는 것 같아요",
          "original_form": "드라마 다시보기용으로 거의 쓰는 것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100033887.1.9",
          "form": "날 잡아서 오리지널들도 봐야지 하는데 시간이 안 나네요 ㅎ",
          "original_form": "날 잡아서 오리지널들도 봐야지 하는데 시간이 안 나네요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100033887.1.10",
          "form": "해지하신 이유가 있나요?",
          "original_form": "해지하신 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100033887.1.11",
          "form": "오리지널들중 관심있는건 몇개봤었는데",
          "original_form": "오리지널들중 관심있는건 몇개봤었는데",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100033887.1.12",
          "form": "좀 결말이 흐지부지 인경우가 많아서 실망했구요",
          "original_form": "좀 결말이 흐지부지 인경우가 많아서 실망했구요",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100033887.1.13",
          "form": "메인화면만 주구장창 보는거같아서 해지했어요",
          "original_form": "메인화면만 주구장창 보는거같아서 해지했어요",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100033887.1.14",
          "form": "저도 지금 그 기로에 놓인 것 같아요",
          "original_form": "저도 지금 그 기로에 놓인 것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100033887.1.15",
          "form": "챙겨 볼 드라마 없어지면 해지할 것 같아요",
          "original_form": "챙겨 볼 드라마 없어지면 해지할 것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100033887.1.16",
          "form": "그나마 이건 좋았다하는 작품 있나요?",
          "original_form": "그나마 이건 좋았다하는 작품 있나요?",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100033887.1.17",
          "form": "음... 오티스의 상담실인가 그거 가볍게 재있게 봤었구요",
          "original_form": "음... 오티스의 상담실인가 그거 가볍게 재있게 봤었구요",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100033887.1.18",
          "form": "종이의집 볼만했어요",
          "original_form": "종이의집 볼만했어요",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100033887.1.19",
          "form": "드라마 뭐뭐 보셨나요?",
          "original_form": "드라마 뭐뭐 보셨나요?",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100033887.1.20",
          "form": "비밀의 숲이랑",
          "original_form": "비밀의 숲이랑",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100033887.1.21",
          "form": "슬기로운 의사생활 보고있어요",
          "original_form": "슬기로운 의사생활 보고있어요",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100033887.1.22",
          "form": "아맞아 저도 넷플에서 비숲 재밌게 봤어요",
          "original_form": "아맞아 저도 넷플에서 비숲 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100033887.1.23",
          "form": "다보셨나요?",
          "original_form": "다보셨나요?",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100033887.1.24",
          "form": "네 비밀의 숲 때문에 넷플릭스 시작하게 됐어요!",
          "original_form": "네 비밀의 숲 때문에 넷플릭스 시작하게 됐어요!",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100033887.1.25",
          "form": "배우분들이 다들 연기를 너무 잘하셔서 진짜 너무 몰입해서 잘봤어요",
          "original_form": "배우분들이 다들 연기를 너무 잘하셔서 진짜 너무 몰입해서 잘봤어요",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100033887.1.26",
          "form": "맞아요 엄한 사람 끝까지 계속 의심하고",
          "original_form": "맞아요 엄한 사람 끝까지 계속 의심하고",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100033887.1.27",
          "form": "흑막이어야하는데 왜 아무짓도 안하지?그러고 ㅋㅋㅋ",
          "original_form": "흑막이어야하는데 왜 아무짓도 안하지?그러고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:26"
        }
      ]
