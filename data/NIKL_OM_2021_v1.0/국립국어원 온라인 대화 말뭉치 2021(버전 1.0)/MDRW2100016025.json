      "utterance": [
        {
          "id": "MDRW2100016025.1.1",
          "form": "즐겨듣는 노래 있어요?",
          "original_form": "즐겨듣는 노래 있어요?",
          "speaker_id": "1",
          "time": "20210826 08:08"
        },
        {
          "id": "MDRW2100016025.1.2",
          "form": "왕빠르네요",
          "original_form": "왕빠르네요",
          "speaker_id": "2",
          "time": "20210826 08:08"
        },
        {
          "id": "MDRW2100016025.1.3",
          "form": "저는.",
          "original_form": "저는.",
          "speaker_id": "2",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.4",
          "form": "오늘 자우림 노래들었어요",
          "original_form": "오늘 자우림 노래들었어요",
          "speaker_id": "2",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.5",
          "form": "자주듣지는 않은데",
          "original_form": "자주듣지는 않은데",
          "speaker_id": "2",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.6",
          "form": "잎새들에게 적는 노래인가",
          "original_form": "잎새들에게 적는 노래인가",
          "speaker_id": "2",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.7",
          "form": "자우림  있지 좋아해요 저 어떤 노래 좋아하세요",
          "original_form": "자우림  있지 좋아해요 저 어떤 노래 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.8",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.9",
          "form": "오오옹ㆍ우",
          "original_form": "오오옹ㆍ우",
          "speaker_id": "2",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.10",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.11",
          "form": "잎새들에게 적는 노래",
          "original_form": "잎새들에게 적는 노래",
          "speaker_id": "1",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.12",
          "form": "들어봐아겠어요",
          "original_form": "들어봐아겠어요",
          "speaker_id": "1",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.13",
          "form": "있지 너무좋아요",
          "original_form": "있지 너무좋아요",
          "speaker_id": "2",
          "time": "20210826 08:09"
        },
        {
          "id": "MDRW2100016025.1.14",
          "form": "동성애",
          "original_form": "동성애",
          "speaker_id": "2",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.15",
          "form": "노래 말씀하시는거 맞으시져",
          "original_form": "노래 말씀하시는거 맞으시져",
          "speaker_id": "1",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.16",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.17",
          "form": "신기했어요",
          "original_form": "신기했어요",
          "speaker_id": "2",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.18",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.19",
          "form": "이런소재로 쓸수도있구나",
          "original_form": "이런소재로 쓸수도있구나",
          "speaker_id": "2",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.21",
          "form": "되게 가사 잘 썼더라구요",
          "original_form": "되게 가사 잘 썼더라구요",
          "speaker_id": "1",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.22",
          "form": "밑에는 막 댓글로 사랑했던 언니 막 이렇게",
          "original_form": "밑에는 막 댓글로 사랑했던 언니 막 이렇게",
          "speaker_id": "2",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.23",
          "form": "그거 노래만들으면",
          "original_form": "그거 노래만들으면",
          "speaker_id": "2",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.24",
          "form": "아련한가요",
          "original_form": "아련한가요",
          "speaker_id": "1",
          "time": "20210826 08:10"
        },
        {
          "id": "MDRW2100016025.1.25",
          "form": "가슴이 찡해지면서 나도 좋아하는 언니가 있던것처럼",
          "original_form": "가슴이 찡해지면서 나도 좋아하는 언니가 있던것처럼",
          "speaker_id": "2",
          "time": "20210826 08:11"
        },
        {
          "id": "MDRW2100016025.1.26",
          "form": "기억왜곡돼오",
          "original_form": "기억왜곡돼오",
          "speaker_id": "2",
          "time": "20210826 08:11"
        },
        {
          "id": "MDRW2100016025.1.27",
          "form": "ㅋㅋㄱㅈㄱ",
          "original_form": "ㅋㅋㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210826 08:11"
        }
      ]
