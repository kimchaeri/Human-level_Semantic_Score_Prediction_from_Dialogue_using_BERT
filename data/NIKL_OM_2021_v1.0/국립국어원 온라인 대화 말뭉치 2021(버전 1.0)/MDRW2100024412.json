      "utterance": [
        {
          "id": "MDRW2100024412.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210831 07:12"
        },
        {
          "id": "MDRW2100024412.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 07:14"
        },
        {
          "id": "MDRW2100024412.1.3",
          "form": "ㅎㅎ name2님은 무슨 일 하세요?",
          "original_form": "ㅎㅎ &name2&님은 무슨 일 하세요?",
          "speaker_id": "1",
          "time": "20210831 07:14"
        },
        {
          "id": "MDRW2100024412.1.4",
          "form": "직장 다니시나요?",
          "original_form": "직장 다니시나요?",
          "speaker_id": "2",
          "time": "20210831 07:14"
        },
        {
          "id": "MDRW2100024412.1.5",
          "form": "저는 발전소에서 근무해요",
          "original_form": "저는 발전소에서 근무해요",
          "speaker_id": "2",
          "time": "20210831 07:14"
        },
        {
          "id": "MDRW2100024412.1.6",
          "form": "오늘은 휴가라 출근을 안해요",
          "original_form": "오늘은 휴가라 출근을 안해요",
          "speaker_id": "2",
          "time": "20210831 07:14"
        },
        {
          "id": "MDRW2100024412.1.7",
          "form": "오 발전소 멋지네요!!",
          "original_form": "오 발전소 멋지네요!!",
          "speaker_id": "1",
          "time": "20210831 07:15"
        },
        {
          "id": "MDRW2100024412.1.8",
          "form": "name1님은 어떤일을 하시나요?",
          "original_form": "&name1&님은 어떤일을 하시나요?",
          "speaker_id": "2",
          "time": "20210831 07:15"
        },
        {
          "id": "MDRW2100024412.1.9",
          "form": "전 아직 학생이라 아르바이트만 하고 있어요!",
          "original_form": "전 아직 학생이라 아르바이트만 하고 있어요!",
          "speaker_id": "1",
          "time": "20210831 07:15"
        },
        {
          "id": "MDRW2100024412.1.10",
          "form": "학생이시구나",
          "original_form": "학생이시구나",
          "speaker_id": "2",
          "time": "20210831 07:16"
        },
        {
          "id": "MDRW2100024412.1.11",
          "form": "발전소일은 어떠세요?",
          "original_form": "발전소일은 어떠세요?",
          "speaker_id": "1",
          "time": "20210831 07:16"
        },
        {
          "id": "MDRW2100024412.1.12",
          "form": "힘드신일 있으신가요?",
          "original_form": "힘드신일 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 07:16"
        },
        {
          "id": "MDRW2100024412.1.13",
          "form": "가끔 제가 담당하는 주요설비가 고장나거나 문제 발생하면",
          "original_form": "가끔 제가 담당하는 주요설비가 고장나거나 문제 발생하면",
          "speaker_id": "2",
          "time": "20210831 07:17"
        },
        {
          "id": "MDRW2100024412.1.14",
          "form": "새벽이나 주말에도 연락오는점",
          "original_form": "새벽이나 주말에도 연락오는점",
          "speaker_id": "2",
          "time": "20210831 07:17"
        },
        {
          "id": "MDRW2100024412.1.15",
          "form": "이런게 조금 스트레스죠",
          "original_form": "이런게 조금 스트레스죠",
          "speaker_id": "2",
          "time": "20210831 07:17"
        },
        {
          "id": "MDRW2100024412.1.16",
          "form": "그 외에는 만족해요",
          "original_form": "그 외에는 만족해요",
          "speaker_id": "2",
          "time": "20210831 07:17"
        },
        {
          "id": "MDRW2100024412.1.17",
          "form": "헉 새벽에도 일을 해야 하나요?",
          "original_form": "헉 새벽에도 일을 해야 하나요?",
          "speaker_id": "1",
          "time": "20210831 07:17"
        },
        {
          "id": "MDRW2100024412.1.18",
          "form": "작년 태풍왔을때",
          "original_form": "작년 태풍왔을때",
          "speaker_id": "2",
          "time": "20210831 07:18"
        },
        {
          "id": "MDRW2100024412.1.19",
          "form": "딱 한번 그런경우 있었어요",
          "original_form": "딱 한번 그런경우 있었어요",
          "speaker_id": "2",
          "time": "20210831 07:18"
        },
        {
          "id": "MDRW2100024412.1.20",
          "form": "그런점은 좀 힘들겠네요ㅜㅠ",
          "original_form": "그런점은 좀 힘들겠네요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 07:18"
        },
        {
          "id": "MDRW2100024412.1.21",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "2",
          "time": "20210831 07:18"
        },
        {
          "id": "MDRW2100024412.1.22",
          "form": "저도 만족할만한 직업을 찾고 싶네요ㅎㅎ",
          "original_form": "저도 만족할만한 직업을 찾고 싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 07:18"
        }
