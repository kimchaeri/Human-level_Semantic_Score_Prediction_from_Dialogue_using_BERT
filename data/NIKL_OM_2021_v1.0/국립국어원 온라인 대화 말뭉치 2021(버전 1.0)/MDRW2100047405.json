        {
          "id": "MDRW2100047405.1.2",
          "form": "어디가고싶으세요",
          "original_form": "어디가고싶으세요",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047405.1.3",
          "form": "가고싶은 여행지있나요",
          "original_form": "가고싶은 여행지있나요",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100047405.1.4",
          "form": "저 제주도요",
          "original_form": "저 제주도요",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100047405.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100047405.1.6",
          "form": "저도용 ㅎㅎ 제주도 한번 갈까 고민중이네요",
          "original_form": "저도용 ㅎㅎ 제주도 한번 갈까 고민중이네요",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047405.1.7",
          "form": "ㅋㅋ ㅋㅋㅋㅋ 예전에 간",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋ 예전에 간",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100047405.1.8",
          "form": "제주도가 너무 좋아사",
          "original_form": "제주도가 너무 좋아사",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100047405.1.9",
          "form": "또 가고싶어요",
          "original_form": "또 가고싶어요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100047405.1.10",
          "form": "저도요 넘 재밌던 기억이있네용 ㅎㅎ",
          "original_form": "저도요 넘 재밌던 기억이있네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100047405.1.11",
          "form": "ㅎ ㅎㅎㅋ 그쵸",
          "original_form": "ㅎ ㅎㅎㅋ 그쵸",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100047405.1.12",
          "form": "바다도 좋았어요",
          "original_form": "바다도 좋았어요",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100047405.1.13",
          "form": "남해도 가보셨나요?",
          "original_form": "남해도 가보셨나요?",
