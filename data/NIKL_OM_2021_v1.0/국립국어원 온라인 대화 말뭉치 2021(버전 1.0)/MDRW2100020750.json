        {
          "id": "MDRW2100020750.1.1",
          "form": "친구들 생일 때 보통 선물 뭐 해주세요?",
          "original_form": "친구들 생일 때 보통 선물 뭐 해주세요?",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100020750.1.2",
          "form": "생일 이제 잘 안챙기는거 같네요 ㅋㅋ",
          "original_form": "생일 이제 잘 안챙기는거 같네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100020750.1.3",
          "form": "서로 축하만 해줘요",
          "original_form": "서로 축하만 해줘요",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020750.1.4",
          "form": "고마운일 있음 기프티콘 보내주긴하네요",
          "original_form": "고마운일 있음 기프티콘 보내주긴하네요",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020750.1.5",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020750.1.6",
          "form": "저도 좀 축하만 해주는 편이에요",
          "original_form": "저도 좀 축하만 해주는 편이에요",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020750.1.7",
          "form": "안주고 안받기가 편하더라구요 ㅋㅋ",
          "original_form": "안주고 안받기가 편하더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020750.1.8",
          "form": "네 맞아요 이것도 은근 신경쓰여서 ㅋㅋ",
          "original_form": "네 맞아요 이것도 은근 신경쓰여서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100020750.1.9",
          "form": "요즘 사고 싶은거 있으신가요?",
          "original_form": "요즘 사고 싶은거 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020750.1.10",
          "form": "식기세척기랑 건조기요 ㅋㅋㅋ",
          "original_form": "식기세척기랑 건조기요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020750.1.11",
          "form": "근데 자취해서 놓을 자리가 없네요",
          "original_form": "근데 자취해서 놓을 자리가 없네요",
          "speaker_id": "1",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020750.1.12",
          "form": "둘다 진짜 좋아보이더라구요 ㅎㅎ",
          "original_form": "둘다 진짜 좋아보이더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020750.1.13",
          "form": "아 ㅋㅋ 그리고 제빙기요",
          "original_form": "아 ㅋㅋ 그리고 제빙기요",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020750.1.14",
          "form": "자취방 냉장고 냉동실이 서랍형이여서 물 얼리기 힘들어요",
          "original_form": "자취방 냉장고 냉동실이 서랍형이여서 물 얼리기 힘들어요",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020750.1.15",
          "form": "헐 정말 그러시겠네요",
          "original_form": "헐 정말 그러시겠네요",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020750.1.16",
          "form": "제빙기 전 얼음 자주 먹어서 그것도 좋아보이네요",
          "original_form": "제빙기 전 얼음 자주 먹어서 그것도 좋아보이네요",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020750.1.17",
          "form": "ㅠㅠ 그쵸 얼음 부족이에요 항상",
          "original_form": "ㅠㅠ 그쵸 얼음 부족이에요 항상",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020750.1.18",
          "form": "name2님은 사고싶은 물건 없어요?",
          "original_form": "&name2&님은 사고싶은 물건 없어요?",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020750.1.19",
          "form": "저는 커다란 티비요!",
          "original_form": "저는 커다란 티비요!",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020750.1.20",
          "form": "지금 티비는 십년정도 되서요 바꾸고 싶네요ㅜㅜ",
          "original_form": "지금 티비는 십년정도 되서요 바꾸고 싶네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020750.1.21",
          "form": "티비 ㅋㅋㅋㅋ 어디꺼요?",
          "original_form": "티비 ㅋㅋㅋㅋ 어디꺼요?",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020750.1.22",
          "form": "삼성 엘지 둘다 좋죠!",
          "original_form": "삼성 엘지 둘다 좋죠!",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020750.1.23",
          "form": "요즘 전세계에서 삼성 엘지 티비 알아준다네요",
          "original_form": "요즘 전세계에서 삼성 엘지 티비 알아준다네요",
          "speaker_id": "1",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020750.1.24",
          "form": "엘지에서 그 스탠바이미 나온거 봤어요?",
          "original_form": "엘지에서 그 스탠바이미 나온거 봤어요?",
          "speaker_id": "1",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020750.1.25",
          "form": "오 맞아요 엘지가 업계 1위 아니었나요? 어디서 본거 같아요",
          "original_form": "오 맞아요 엘지가 업계 1위 아니었나요? 어디서 본거 같아요",
          "speaker_id": "2",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020750.1.26",
          "form": "스탠바이미? 처음들어봐요!",
          "original_form": "스탠바이미? 처음들어봐요!",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020750.1.27",
          "form": "이동식 티비래요!!",
          "original_form": "이동식 티비래요!!",
          "speaker_id": "1",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020750.1.28",
          "form": "진짜 좋은 것 같아요 ㅠㅠ 침대에 누워서도 보고 방에 인강 들을때도 보고 ㅜㅜ",
          "original_form": "진짜 좋은 것 같아요 ㅠㅠ 침대에 누워서도 보고 방에 인강 들을때도 보고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100020750.1.29",
          "form": "무선이래요 무선!!",
          "original_form": "무선이래요 무선!!",
          "speaker_id": "1",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100020750.1.30",
          "form": "아!!!! 그거봤어요!!!!",
          "original_form": "아!!!! 그거봤어요!!!!",
          "speaker_id": "2",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100020750.1.31",
          "form": "스탠바이미? 그거 인터넷에서 봤어요",
