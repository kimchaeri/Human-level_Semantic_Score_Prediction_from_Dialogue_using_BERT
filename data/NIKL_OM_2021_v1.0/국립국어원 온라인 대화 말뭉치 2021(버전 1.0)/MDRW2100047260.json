      "utterance": [
        {
          "id": "MDRW2100047260.1.1",
          "form": "게임 하시는거 있으세요?",
          "original_form": "게임 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100047260.1.2",
          "form": "예전에 했어요",
          "original_form": "예전에 했어요",
          "speaker_id": "1",
          "time": "20210826 03:03"
        },
        {
          "id": "MDRW2100047260.1.3",
          "form": "어떤 게임요?",
          "original_form": "어떤 게임요?",
          "speaker_id": "2",
          "time": "20210826 03:03"
        },
        {
          "id": "MDRW2100047260.1.4",
          "form": "데바데요",
          "original_form": "데바데요",
          "speaker_id": "1",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100047260.1.5",
          "form": "앗 그거 공포게임 아닌가요?",
          "original_form": "앗 그거 공포게임 아닌가요?",
          "speaker_id": "2",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100047260.1.6",
          "form": "오 아시네요",
          "original_form": "오 아시네요",
          "speaker_id": "1",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100047260.1.7",
          "form": "저는 제5인격이라는 게임 했었거든요",
          "original_form": "저는 제5인격이라는 게임 했었거든요",
          "speaker_id": "2",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100047260.1.8",
          "form": "그게 약간 데바데랑 비슷하다고 해서 알고잇어요! ㅋㅋ",
          "original_form": "그게 약간 데바데랑 비슷하다고 해서 알고잇어요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100047260.1.9",
          "form": "5인격 들어봤어요",
          "original_form": "5인격 들어봤어요",
          "speaker_id": "1",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100047260.1.10",
          "form": "5인격 데바데랑 비슷하대요!",
          "original_form": "5인격 데바데랑 비슷하대요!",
          "speaker_id": "2",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100047260.1.11",
          "form": "5인격도 재밌어요 ㅋㅋ",
          "original_form": "5인격도 재밌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100047260.1.12",
          "form": "근데 하나도 안무서운 공포게임이에요",
          "original_form": "근데 하나도 안무서운 공포게임이에요",
          "speaker_id": "1",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100047260.1.13",
          "form": "맞아요ㅋㅋㅋㅋ 데바데에 비하면",
          "original_form": "맞아요ㅋㅋㅋㅋ 데바데에 비하면",
          "speaker_id": "2",
          "time": "20210826 03:09"
        },
        {
          "id": "MDRW2100047260.1.14",
          "form": "완전 애교수준일거에요",
          "original_form": "완전 애교수준일거에요",
          "speaker_id": "2",
          "time": "20210826 03:09"
        },
        {
          "id": "MDRW2100047260.1.15",
          "form": "데바데도 한두번해보면 웃겨요ㅋㅋ",
          "original_form": "데바데도 한두번해보면 웃겨요ㅋㅋ",
