      "utterance": [
        {
          "id": "MDRW2100033800.1.1",
          "form": "안녕하세요ㅎㅎㅎ",
          "original_form": "안녕하세요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100033800.1.2",
          "form": "네네 버섯 좋아하는 name1님 안녕하세요",
          "original_form": "네네 버섯 좋아하는 &name1&님 안녕하세요",
          "speaker_id": "2",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100033800.1.3",
          "form": "버섯은 어떤게 가장 좋더이까",
          "original_form": "버섯은 어떤게 가장 좋더이까",
          "speaker_id": "2",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100033800.1.4",
          "form": "팽이버섯이랑 새송이요ㅋㅋㅋ",
          "original_form": "팽이버섯이랑 새송이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100033800.1.5",
          "form": "청경채도 넣어야죠!",
          "original_form": "청경채도 넣어야죠!",
          "speaker_id": "1",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100033800.1.6",
          "form": "표고는 별로이신가요",
          "original_form": "표고는 별로이신가요",
          "speaker_id": "2",
          "time": "20210825 02:48"
        },
        {
          "id": "MDRW2100033800.1.7",
          "form": "으음",
          "original_form": "으음",
          "speaker_id": "1",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100033800.1.8",
          "form": "표고는 빼야겠어욬ㅋㅋㅋ",
          "original_form": "표고는 빼야겠어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100033800.1.9",
          "form": "저는 버섯중 표고와 목이 이 두개가 먼저인데 저와 반대네요ㅎ",
          "original_form": "저는 버섯중 표고와 목이 이 두개가 먼저인데 저와 반대네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 08:33"
        },
        {
          "id": "MDRW2100033800.1.10",
          "form": "ㅋㅋㅋ  저는",
          "original_form": "ㅋㅋㅋ  저는",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100033800.1.11",
          "form": "이상하게 목이는 추가하기싫더라구요ㅠ",
          "original_form": "이상하게 목이는 추가하기싫더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100033800.1.12",
          "form": "저는 잡채도 목이와 표고파라ㅎ",
          "original_form": "저는 잡채도 목이와 표고파라ㅎ",
          "speaker_id": "2",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100033800.1.13",
          "form": "같은 버섯 카테고리서도 취향이 이렇게 다르니 신기하네요",
          "original_form": "같은 버섯 카테고리서도 취향이 이렇게 다르니 신기하네요",
          "speaker_id": "2",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100033800.1.14",
          "form": "아ㅋㅋㅋ전근데",
          "original_form": "아ㅋㅋㅋ전근데",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100033800.1.15",
          "form": "잡채에들어간 목이는 좋아해요",
          "original_form": "잡채에들어간 목이는 좋아해요",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100033800.1.16",
          "form": "제가 잡채귀신이랍니다",
          "original_form": "제가 잡채귀신이랍니다",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100033800.1.17",
          "form": "ㅎㅎ 잡채는 사랑입니다",
          "original_form": "ㅎㅎ 잡채는 사랑입니다",
          "speaker_id": "2",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100033800.1.18",
          "form": "부산인가? 어디는 잡채밥에 자장소스 얹어서 준댕ㆍ",
          "original_form": "부산인가? 어디는 잡채밥에 자장소스 얹어서 준댕ㆍ",
          "speaker_id": "2",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100033800.1.19",
          "form": "볶음밥처럼요",
          "original_form": "볶음밥처럼요",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100033800.1.20",
          "form": "헉 자장소스라니",
          "original_form": "헉 자장소스라니",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100033800.1.21",
          "form": "조금 낯선잡채밥이네요ㅠ",
          "original_form": "조금 낯선잡채밥이네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100033800.1.22",
          "form": "거기에 계란프라이도요ㅎ",
          "original_form": "거기에 계란프라이도요ㅎ",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100033800.1.23",
          "form": "보니 거기는 중국집 잡채밥이 수도권처럼 매콤하지 않은가봐요",
          "original_form": "보니 거기는 중국집 잡채밥이 수도권처럼 매콤하지 않은가봐요",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100033800.1.24",
          "form": "가정집 잡채처럼 순한 맛이려나 추측만 해봅니다",
          "original_form": "가정집 잡채처럼 순한 맛이려나 추측만 해봅니다",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100033800.1.25",
          "form": "계란주는건 진짜좋네요ㅋㅋㅋㅋ",
          "original_form": "계란주는건 진짜좋네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:13"
        }
      ]
    }
  ]
