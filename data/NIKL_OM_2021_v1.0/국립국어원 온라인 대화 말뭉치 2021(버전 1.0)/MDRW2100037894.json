      "utterance": [
        {
          "id": "MDRW2100037894.1.1",
          "form": "고양이 한 마리 키우고 있습니다!",
          "original_form": "고양이 한 마리 키우고 있습니다!",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037894.1.2",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037894.1.3",
          "form": "우와 name2님 집사셨구나!",
          "original_form": "우와 &name2&님 집사셨구나!",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100037894.1.4",
          "form": "아니요ㅠ 키우고 싶은데 못키우는 중이에요!!",
          "original_form": "아니요ㅠ 키우고 싶은데 못키우는 중이에요!!",
          "speaker_id": "1",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100037894.1.5",
          "form": "비염 때문에 힘드시겠죠ㅠㅠ",
          "original_form": "비염 때문에 힘드시겠죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037894.1.6",
          "form": "사실 엄마가",
          "original_form": "사실 엄마가",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100037894.1.7",
          "form": "기관지가 안좋으셔서요ㅠㅠ",
          "original_form": "기관지가 안좋으셔서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100037894.1.8",
          "form": "저 근데 예전에 길고양이한테 간택받은 적 있어요!",
          "original_form": "저 근데 예전에 길고양이한테 간택받은 적 있어요!",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037894.1.9",
          "form": "간택 받으셨어요?!",
          "original_form": "간택 받으셨어요?!",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037894.1.10",
          "form": "애기한테 잘해주셨나보네요ㅠㅠ",
          "original_form": "애기한테 잘해주셨나보네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037894.1.11",
          "form": "아쉽지만 기관지에 털은 많이 안좋으니까 어쩔 수 없었겠어요ㅠㅠ",
          "original_form": "아쉽지만 기관지에 털은 많이 안좋으니까 어쩔 수 없었겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100037894.1.12",
          "form": "정말 신기한 일이었어요!",
          "original_form": "정말 신기한 일이었어요!",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037894.1.13",
          "form": "추운 겨울날이었는데",
          "original_form": "추운 겨울날이었는데",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037894.1.14",
          "form": "귀여운 삼색이가 아파트 공동현관 앞에 있길래",
          "original_form": "귀여운 삼색이가 아파트 공동현관 앞에 있길래",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037894.1.15",
          "form": "\"들어올래?\" 이러면서 제가 자동문 열어줬거든요?",
          "original_form": "\"들어올래?\" 이러면서 제가 자동문 열어줬거든요?",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100037894.1.16",
          "form": "들어왔나요 우리 똑똑이가ㅠㅠㅠㅠㅠ",
          "original_form": "들어왔나요 우리 똑똑이가ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100037894.1.17",
          "form": "아뇨아뇨 반전이 있어요..",
          "original_form": "아뇨아뇨 반전이 있어요..",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037894.1.18",
          "form": "제가 그렇게 말하면서 봤는데",
          "original_form": "제가 그렇게 말하면서 봤는데",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037894.1.19",
          "form": "아파트에 안들어오고 그냥 저만 빤히 보는거예요",
          "original_form": "아파트에 안들어오고 그냥 저만 빤히 보는거예요",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037894.1.20",
          "form": "그래서 들어올 생각은 없나보다, 하고 그냥 계단으로 저희 집까지 올라갔는데",
          "original_form": "그래서 들어올 생각은 없나보다, 하고 그냥 계단으로 저희 집까지 올라갔는데",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037894.1.21",
          "form": "4층이었거든요 저희 집이?",
          "original_form": "4층이었거든요 저희 집이?",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037894.1.22",
          "form": "근데 다음날 아침에",
          "original_form": "근데 다음날 아침에",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037894.1.23",
          "form": "저는 늦게 일어났는데 엄마가 외출하고 돌아오셔서",
          "original_form": "저는 늦게 일어났는데 엄마가 외출하고 돌아오셔서",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037894.1.24",
          "form": "문앞에 고양이가 있다는거예요?",
          "original_form": "문앞에 고양이가 있다는거예요?",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037894.1.25",
          "form": "아이고 애기가ㅠㅠㅠ 거길 어떻게 찾았을까요ㅠㅠ",
          "original_form": "아이고 애기가ㅠㅠㅠ 거길 어떻게 찾았을까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037894.1.26",
          "form": "냄새도 잘 맡고ㅠㅠ",
          "original_form": "냄새도 잘 맡고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037894.1.27",
          "form": "그쵸!!!",
          "original_form": "그쵸!!!",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037894.1.28",
          "form": "저는 처음에 당연히 아파트 공동현관인 줄 알았는데",
          "original_form": "저는 처음에 당연히 아파트 공동현관인 줄 알았는데",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037894.1.29",
          "form": "정말 딱 저희 집 현관 앞에서",
          "original_form": "정말 딱 저희 집 현관 앞에서",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037894.1.30",
          "form": "밤을 새서 기다린거예요...!!!",
          "original_form": "밤을 새서 기다린거예요...!!!",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037894.1.31",
          "form": "그래서 저희 식구들이 문 드나들 때마다 저희 집에 들어오려고... 신발장에 들어오려고 그랬는데...",
          "original_form": "그래서 저희 식구들이 문 드나들 때마다 저희 집에 들어오려고... 신발장에 들어오려고 그랬는데...",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037894.1.32",
          "form": "심지어 뱃속에 아기가 있는 삼색이었어요ㅠ",
          "original_form": "심지어 뱃속에 아기가 있는 삼색이었어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037894.1.33",
          "form": "아이고 애기가ㅠㅠㅠㅠ\r\nname1님 좋은 사람인거 딱 알고 왔나봐요ㅠㅠ",
          "original_form": "아이고 애기가ㅠㅠㅠㅠ\r\n&name1&님 좋은 사람인거 딱 알고 왔나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037894.1.34",
          "form": "믿음이 갔나봐요ㅠㅠ",
          "original_form": "믿음이 갔나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037894.1.35",
          "form": "그렇게 말씀해주셔서 감사해요ㅠ",
          "original_form": "그렇게 말씀해주셔서 감사해요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037894.1.36",
          "form": "너무 추워서 뭐라도 주고싶어서",
          "original_form": "너무 추워서 뭐라도 주고싶어서",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037894.1.37",
          "form": "사과 갈아서 줬는데 안먹더라구요.,,",
          "original_form": "사과 갈아서 줬는데 안먹더라구요.,,",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037894.1.38",
          "form": "사과는 원래 안먹나요?!",
          "original_form": "사과는 원래 안먹나요?!",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037894.1.39",
          "form": "나름 검색해서 먹어도 되는거 준건데ㅠ",
          "original_form": "나름 검색해서 먹어도 되는거 준건데ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037894.1.40",
          "form": "생각해보니 물을 줄걸 그랬어요ㅠ",
          "original_form": "생각해보니 물을 줄걸 그랬어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037894.1.41",
          "form": "고양이가 일단은 육식과라서 그랬던 것 같아요ㅠㅠ",
          "original_form": "고양이가 일단은 육식과라서 그랬던 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037894.1.42",
          "form": "사료도 거의 고기성분이거든요ㅠㅠ",
          "original_form": "사료도 거의 고기성분이거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:37"
        }
      ]
