      "utterance": [
        {
          "id": "MDRW2100023823.1.1",
          "form": "네네 실빼는게 이빼는거보다 아팠어요..ㅋㅋㅋㅋㅋ",
          "original_form": "네네 실빼는게 이빼는거보다 아팠어요..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100023823.1.2",
          "form": "약간 따끔따끔하더라구요",
          "original_form": "약간 따끔따끔하더라구요",
          "speaker_id": "2",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100023823.1.3",
          "form": "으악 너무 고통스러울 것 같아요",
          "original_form": "으악 너무 고통스러울 것 같아요",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023823.1.4",
          "form": "두개 한번에 빼셨어요?",
          "original_form": "두개 한번에 빼셨어요?",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023823.1.5",
          "form": "아뇨 저는 따로 뺐어요",
          "original_form": "아뇨 저는 따로 뺐어요",
          "speaker_id": "2",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023823.1.6",
          "form": "진짜 징그러운게 잇몸에 구멍이 나있거든요",
          "original_form": "진짜 징그러운게 잇몸에 구멍이 나있거든요",
          "speaker_id": "2",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023823.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023823.1.8",
          "form": "악",
          "original_form": "악",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023823.1.9",
          "form": "음식물 끼지않아요?",
          "original_form": "음식물 끼지않아요?",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023823.1.10",
          "form": "칫솔질은 어케해요?",
          "original_form": "칫솔질은 어케해요?",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023823.1.11",
          "form": "네 껴요",
          "original_form": "네 껴요",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023823.1.12",
          "form": "칫솔질 그녕 평범하게 했는데",
          "original_form": "칫솔질 그녕 평범하게 했는데",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023823.1.13",
          "form": "물가글을 열심히 했어요",
          "original_form": "물가글을 열심히 했어요",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023823.1.14",
          "form": "음식물이 자꾸 들어가더라구요",
          "original_form": "음식물이 자꾸 들어가더라구요",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023823.1.15",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "1",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100023823.1.16",
          "form": "어떻게 빼요?",
          "original_form": "어떻게 빼요?",
          "speaker_id": "1",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100023823.1.17",
          "form": "그냥 물로 우글우글 가글가글 하면 저절로 빠져요",
          "original_form": "그냥 물로 우글우글 가글가글 하면 저절로 빠져요",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100023823.1.18",
          "form": "신기하죠ㅋㅋㅋㅋㅋ",
          "original_form": "신기하죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100023823.1.19",
          "form": "그게 빠지긴 하더라구요",
          "original_form": "그게 빠지긴 하더라구요",
          "speaker_id": "2",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.20",
          "form": "우와 네 엄청신기해욬ㅋㅋㅋ",
          "original_form": "우와 네 엄청신기해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.21",
          "form": "영원히 걸려있을거같은데",
          "original_form": "영원히 걸려있을거같은데",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.22",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "2",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.23",
          "form": "잇몸 아물면",
          "original_form": "잇몸 아물면",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.24",
          "form": "그 구멍이 메꿔지나요?",
          "original_form": "그 구멍이 메꿔지나요?",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.25",
          "form": "저도 그게 무서워서 양치 열심히 했어요",
          "original_form": "저도 그게 무서워서 양치 열심히 했어요",
          "speaker_id": "2",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.26",
          "form": "네 다 메꿔지고",
          "original_form": "네 다 메꿔지고",
          "speaker_id": "2",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.27",
          "form": "그냥 잇몸이 되더라그요",
          "original_form": "그냥 잇몸이 되더라그요",
          "speaker_id": "2",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023823.1.28",
          "form": "우와 회복력 진짜 신기해요",
          "original_form": "우와 회복력 진짜 신기해요",
          "speaker_id": "1",
          "time": "20210830 18:11"
        },
        {
          "id": "MDRW2100023823.1.29",
          "form": "ㅎㅏ긴 교정할때 발치하는것도 다 메꿔지더라구요",
          "original_form": "ㅎㅏ긴 교정할때 발치하는것도 다 메꿔지더라구요",
          "speaker_id": "1",
          "time": "20210830 18:11"
        },
        {
          "id": "MDRW2100023823.1.30",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210830 18:11"
        },
        {
          "id": "MDRW2100023823.1.31",
          "form": "입안 살이 회복력이 빠른가봐요",
          "original_form": "입안 살이 회복력이 빠른가봐요",
