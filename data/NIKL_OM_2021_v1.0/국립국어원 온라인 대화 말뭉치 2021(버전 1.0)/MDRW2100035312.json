      "utterance": [
        {
          "id": "MDRW2100035312.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100035312.1.2",
          "form": "아직요 name1님은요",
          "original_form": "아직요 &name1&님은요",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100035312.1.3",
          "form": "저는 볶음밥 먹었어요 중국집 배달해서ㅎㅎ",
          "original_form": "저는 볶음밥 먹었어요 중국집 배달해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035312.1.4",
          "form": "오우 배달음식 드셨군요 맛나겠당",
          "original_form": "오우 배달음식 드셨군요 맛나겠당",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035312.1.5",
          "form": "전 맨밥에 물말아서 먹으려구용",
          "original_form": "전 맨밥에 물말아서 먹으려구용",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035312.1.6",
          "form": "입맛이 없으신가요??",
          "original_form": "입맛이 없으신가요??",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100035312.1.7",
          "form": "그냥 간단하게 먹으려구요ㅎㅎ빨리 간식먹으려구요",
          "original_form": "그냥 간단하게 먹으려구요ㅎㅎ빨리 간식먹으려구요",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035312.1.8",
          "form": "간식 좋죠ㅋㅋ 전 간식으로 기정떡 먹었어요",
          "original_form": "간식 좋죠ㅋㅋ 전 간식으로 기정떡 먹었어요",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100035312.1.9",
          "form": "오 기정떡은뭐에요? 앙꼬있어요?",
          "original_form": "오 기정떡은뭐에요? 앙꼬있어요?",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100035312.1.10",
          "form": "그냥 술떡이에요ㅋㅋ 안에 앙꼬있는걸로 먹었네요 맛나요",
          "original_form": "그냥 술떡이에요ㅋㅋ 안에 앙꼬있는걸로 먹었네요 맛나요",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100035312.1.11",
          "form": "오 맛있겠네요 저녁겸용인가요?",
          "original_form": "오 맛있겠네요 저녁겸용인가요?",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100035312.1.12",
          "form": "생각보다 많이 먹게되서 저녁겸용이 되버렸네요ㅋㅋ",
          "original_form": "생각보다 많이 먹게되서 저녁겸용이 되버렸네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100035312.1.13",
          "form": "name2님은 어떤 간식 드셨어요?",
          "original_form": "&name2&님은 어떤 간식 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100035312.1.14",
          "form": "저는 말차라떼마셨어요 크로플하구요",
          "original_form": "저는 말차라떼마셨어요 크로플하구요",
          "speaker_id": "2",
          "time": "20210829 18:12"
        },
        {
          "id": "MDRW2100035312.1.15",
          "form": "말차라떼 드셨군요ㅎㅎ 기대만큼 맛있었나요?",
          "original_form": "말차라떼 드셨군요ㅎㅎ 기대만큼 맛있었나요?",
          "speaker_id": "1",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100035312.1.16",
          "form": "네 까다로운 친구가 맛있다고 알려줘서 신뢰가는 정보를 가지고 갔는데",
          "original_form": "네 까다로운 친구가 맛있다고 알려줘서 신뢰가는 정보를 가지고 갔는데",
          "speaker_id": "2",
          "time": "20210829 19:59"
        },
        {
          "id": "MDRW2100035312.1.17",
          "form": "역시 잘하는집이더래요ㅎㅎ",
          "original_form": "역시 잘하는집이더래요ㅎㅎ",
