      "utterance": [
        {
          "id": "MDRW2100047144.1.1",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100047144.1.2",
          "form": "해물도좋죠ㅎㅎ",
          "original_form": "해물도좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100047144.1.3",
          "form": "전 다좋아해요ㅎ진짜",
          "original_form": "전 다좋아해요ㅎ진짜",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100047144.1.4",
          "form": "해물찜는 해물탕은 냠냠 잘도 먹어요 ㅎㅎ",
          "original_form": "해물찜는 해물탕은 냠냠 잘도 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100047144.1.5",
          "form": "잘먹는게 좋아요",
          "original_form": "잘먹는게 좋아요",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100047144.1.6",
          "form": "살쪗었던거보면 아실꺼에요ㅎㅎ",
          "original_form": "살쪗었던거보면 아실꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100047144.1.7",
          "form": "엄청 많이먹어서ㅠ",
          "original_form": "엄청 많이먹어서ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100047144.1.8",
          "form": "저처럼 가리면 안좋음",
          "original_form": "저처럼 가리면 안좋음",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100047144.1.9",
          "form": "그래서 점심을 안먹어요ㅎ",
          "original_form": "그래서 점심을 안먹어요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100047144.1.10",
          "form": "전 가려도 살찌던데요 ㅋㅋ",
          "original_form": "전 가려도 살찌던데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100047144.1.11",
          "form": "그래도 가리시면 날씬하잖아요ㅎ",
          "original_form": "그래도 가리시면 날씬하잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100047144.1.12",
          "form": "저도 관리 할땐 두끼",
          "original_form": "저도 관리 할땐 두끼",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100047144.1.13",
          "form": "세상부럽ㅎㅎ",
          "original_form": "세상부럽ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100047144.1.14",
          "form": "전 한끼요ㅎ",
          "original_form": "전 한끼요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100047144.1.15",
          "form": "요즘 관리안하니깐 보기가흉하네요ㅎ",
          "original_form": "요즘 관리안하니깐 보기가흉하네요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100047144.1.16",
          "form": "저도 최고치는  남자만큼 나갓어여 ㅎㅎ",
          "original_form": "저도 최고치는  남자만큼 나갓어여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100047144.1.17",
          "form": "진짜 결혼하고 나서 엄청 쨨네요ㅎ",
          "original_form": "진짜 결혼하고 나서 엄청 쨨네요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100047144.1.18",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100047144.1.19",
          "form": "그러실때가 있으셨구나",
          "original_form": "그러실때가 있으셨구나",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100047144.1.20",
          "form": "남자몸무게 많이나가는데ㅎㅎㅎ",
          "original_form": "남자몸무게 많이나가는데ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100047144.1.21",
          "form": "남자들 보통 결혼하면 찌는게 공식 같든데",
          "original_form": "남자들 보통 결혼하면 찌는게 공식 같든데",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100047144.1.22",
          "form": "이제 전 아내지만",
          "original_form": "이제 전 아내지만",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100047144.1.23",
          "form": "밥을 잘 안차려줬어요ㅎㅎ",
          "original_form": "밥을 잘 안차려줬어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100047144.1.24",
          "form": "앗 ㅡㅁ ㅡ",
          "original_form": "앗 ㅡㅁ ㅡ",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100047144.1.25",
          "form": "그래서 인스턴트 위주로 식사하고ㅎ",
          "original_form": "그래서 인스턴트 위주로 식사하고ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100047144.1.26",
          "form": "에공 ㅜㅜ",
          "original_form": "에공 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100047144.1.27",
          "form": "아무래도 애기키우다보니 스트레스도 많고 사이도 그렇게 좋지가 않아거",
          "original_form": "아무래도 애기키우다보니 스트레스도 많고 사이도 그렇게 좋지가 않아거",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100047144.1.28",
          "form": "지금은 좀 챙겨 먹어요?",
          "original_form": "지금은 좀 챙겨 먹어요?",
          "speaker_id": "1",
          "time": "20210828 00:06"
        }
      ]
    }
