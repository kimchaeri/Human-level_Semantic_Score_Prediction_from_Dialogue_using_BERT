      "utterance": [
        {
          "id": "MDRW2100013386.1.1",
          "form": "다이어트 하시나요?",
          "original_form": "다이어트 하시나요?",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013386.1.2",
          "form": "이제 하려고요 ㅠㅠㅠ",
          "original_form": "이제 하려고요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013386.1.3",
          "form": "name1도 하시나요",
          "original_form": "&name1&도 하시나요",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013386.1.4",
          "form": "전 평생 해야죠ㅋ",
          "original_form": "전 평생 해야죠ㅋ",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013386.1.5",
          "form": "간헐적 단식으로 3키로 정도 뺐어요",
          "original_form": "간헐적 단식으로 3키로 정도 뺐어요",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013386.1.6",
          "form": "와 그럼 1식 하시는 건가요?",
          "original_form": "와 그럼 1식 하시는 건가요?",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013386.1.7",
          "form": "2식하고 있어요. 아침만 안 먹어요",
          "original_form": "2식하고 있어요. 아침만 안 먹어요",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013386.1.8",
          "form": "아하 그러시구나 그렇게 해도 효과가 괜찮네요 3키로면",
          "original_form": "아하 그러시구나 그렇게 해도 효과가 괜찮네요 3키로면",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013386.1.9",
          "form": "근데 2끼라고 엄청 많이 먹음 안되요ㅋ",
          "original_form": "근데 2끼라고 엄청 많이 먹음 안되요ㅋ",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013386.1.10",
          "form": "아 글쵸글쵸 ㅋㅋㅋ 그럼 먹는 것도 종류를 바꾸나요? 닭가슴살이나 샐러드같은?",
          "original_form": "아 글쵸글쵸 ㅋㅋㅋ 그럼 먹는 것도 종류를 바꾸나요? 닭가슴살이나 샐러드같은?",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013386.1.11",
          "form": "아뇨 먹는건 그냥 원래 그대로 했죠.",
          "original_form": "아뇨 먹는건 그냥 원래 그대로 했죠.",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013386.1.12",
          "form": "아 그럼 식사시간도 그대로고 아침만 안먹는 거 맞나요?",
          "original_form": "아 그럼 식사시간도 그대로고 아침만 안먹는 거 맞나요?",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013386.1.13",
          "form": "네 글고 8시 이후엔 안 먹었어요",
          "original_form": "네 글고 8시 이후엔 안 먹었어요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013386.1.14",
          "form": "그게 중요하겠네요!!! 저도 참고해야겠어요 요새 체중이 불어서 ㅋㅋ",
          "original_form": "그게 중요하겠네요!!! 저도 참고해야겠어요 요새 체중이 불어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013386.1.15",
          "form": "네 꾸준히 하는게 중요해요ㅋㅋ",
          "original_form": "네 꾸준히 하는게 중요해요ㅋㅋ",
