      "utterance": [
        {
          "id": "MDRW2100024835.1.1",
          "form": "안녕하세요 저녁식사하셨나요?",
          "original_form": "안녕하세요 저녁식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024835.1.2",
          "form": "네 비도 오고 감자전먹었어요",
          "original_form": "네 비도 오고 감자전먹었어요",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024835.1.3",
          "form": "저녁식사 하셨나요?",
          "original_form": "저녁식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024835.1.4",
          "form": "네 저도 먹었어요",
          "original_form": "네 저도 먹었어요",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024835.1.5",
          "form": "감자전만 드셨어요?",
          "original_form": "감자전만 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024835.1.6",
          "form": "네 저녁 뭐드셨나요?",
          "original_form": "네 저녁 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024835.1.7",
          "form": "전 고기 먹었어요",
          "original_form": "전 고기 먹었어요",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024835.1.8",
          "form": "감자전은 직접 만드셨나요?",
          "original_form": "감자전은 직접 만드셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024835.1.9",
          "form": "아니요 부모님이 만들어주셨어요!",
          "original_form": "아니요 부모님이 만들어주셨어요!",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024835.1.10",
          "form": "무슨 고기 드셨나요?",
          "original_form": "무슨 고기 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024835.1.11",
          "form": "삼겹살이요ㅎㅎ",
          "original_form": "삼겹살이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024835.1.12",
          "form": "비와서 전 드신거예요?ㅎ",
          "original_form": "비와서 전 드신거예요?ㅎ",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024835.1.13",
          "form": "맞아요 비와서 먹었어요 ㅋㅋ 삼겹살.. 삼겹살도 비올때 딱이죠",
          "original_form": "맞아요 비와서 먹었어요 ㅋㅋ 삼겹살.. 삼겹살도 비올때 딱이죠",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024835.1.14",
          "form": "그쵸 기름튀기는 소리가 좋아요",
          "original_form": "그쵸 기름튀기는 소리가 좋아요",
          "speaker_id": "1",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100024835.1.15",
          "form": "평소엔 무슨음식 좋아하세요?",
          "original_form": "평소엔 무슨음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 19:37"
        },
        {
          "id": "MDRW2100024835.1.16",
          "form": "음 저는 고기도 좋아하고 빵도 좋아하고 ㅋㅋ 먹는거 다좋아해요",
          "original_form": "음 저는 고기도 좋아하고 빵도 좋아하고 ㅋㅋ 먹는거 다좋아해요",
          "speaker_id": "2",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024835.1.17",
          "form": "그쵸 세상에 맛있는게 너무 많아요",
          "original_form": "그쵸 세상에 맛있는게 너무 많아요",
          "speaker_id": "1",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100024835.1.18",
          "form": "맞아요 ㅋㅋ 민초 좋아하시나여",
          "original_form": "맞아요 ㅋㅋ 민초 좋아하시나여",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024835.1.19",
          "form": "아 아뇨 민초 불호입니다",
          "original_form": "아 아뇨 민초 불호입니다",
          "speaker_id": "1",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024835.1.20",
          "form": "단호한 민초싫어파 이시군요!!",
          "original_form": "단호한 민초싫어파 이시군요!!",
