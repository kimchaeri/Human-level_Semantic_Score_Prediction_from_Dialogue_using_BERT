      "utterance": [
        {
          "id": "MDRW2100041210.1.1",
          "form": "부산 가서 맛있는 것 드시고 오셔요",
          "original_form": "부산 가서 맛있는 것 드시고 오셔요",
          "speaker_id": "2",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100041210.1.2",
          "form": "부산도 맛있는 음식 많잖아요ㅎㅎ",
          "original_form": "부산도 맛있는 음식 많잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100041210.1.3",
          "form": "아마 코로나 때문에 밥은 안먹을거 같아요 뭐 사가서 차 안에서 먹으려구요 ㅎㅎ",
          "original_form": "아마 코로나 때문에 밥은 안먹을거 같아요 뭐 사가서 차 안에서 먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100041210.1.4",
          "form": "부산도 심각하더라구요ㅠㅠ",
          "original_form": "부산도 심각하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041210.1.5",
          "form": "아 그렇죠ㅜ",
          "original_form": "아 그렇죠ㅜ",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041210.1.6",
          "form": "어디 가서 먹기는 곤란하겠네요ㅜ",
          "original_form": "어디 가서 먹기는 곤란하겠네요ㅜ",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041210.1.7",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041210.1.8",
          "form": "네 점심 든든히 먹어서",
          "original_form": "네 점심 든든히 먹어서",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041210.1.9",
          "form": "저녁은 간단히 과일이랑 계란 먹었어요",
          "original_form": "저녁은 간단히 과일이랑 계란 먹었어요",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041210.1.10",
          "form": "네 혹시몰라서 식당은 안가려구요",
          "original_form": "네 혹시몰라서 식당은 안가려구요",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100041210.1.11",
          "form": "좋네요! 저도 저녁은 간단하게 먹었어야됐는데...",
          "original_form": "좋네요! 저도 저녁은 간단하게 먹었어야됐는데...",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100041210.1.12",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100041210.1.13",
          "form": "맘스터치 햄버거랑 순살치킨요 ㅋㅋ",
          "original_form": "맘스터치 햄버거랑 순살치킨요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100041210.1.14",
          "form": "배가 아직 빵빵해요",
          "original_form": "배가 아직 빵빵해요",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100041210.1.15",
          "form": "와 맘스터치 맛있죠",
          "original_form": "와 맘스터치 맛있죠",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100041210.1.16",
          "form": "네 싸이버거랑 싸이순살 먹었어요",
          "original_form": "네 싸이버거랑 싸이순살 먹었어요",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100041210.1.17",
          "form": "싸이버거는 여전히 맛있었는데 순살은 그냥그랬네요",
          "original_form": "싸이버거는 여전히 맛있었는데 순살은 그냥그랬네요",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100041210.1.18",
          "form": "아 맘스터치는 싸이버거가 최고인 것 같아요",
          "original_form": "아 맘스터치는 싸이버거가 최고인 것 같아요",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100041210.1.19",
          "form": "가성비도 좋고",
          "original_form": "가성비도 좋고",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100041210.1.20",
          "form": "네 맞아요 싸이가 확실히 맛있죠",
          "original_form": "네 맞아요 싸이가 확실히 맛있죠",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100041210.1.21",
          "form": "네 맘스터치는 싸이버거!",
          "original_form": "네 맘스터치는 싸이버거!",
          "speaker_id": "2",
          "time": "20210905 20:29"
        }
      ]
