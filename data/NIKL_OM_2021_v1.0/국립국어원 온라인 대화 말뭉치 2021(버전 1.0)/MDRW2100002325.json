      "utterance": [
        {
          "id": "MDRW2100002325.1.1",
          "form": "전 어제 아들 가방 사줬어요",
          "original_form": "전 어제 아들 가방 사줬어요",
          "speaker_id": "2",
          "time": "20210820 09:28"
        },
        {
          "id": "MDRW2100002325.1.2",
          "form": "남자들 뭔가방을 메는지 모르겠어요",
          "original_form": "남자들 뭔가방을 메는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210820 09:28"
        },
        {
          "id": "MDRW2100002325.1.3",
          "form": "남자들껀 확실히 잘모르겠어요 ㅎㅎㅎ",
          "original_form": "남자들껀 확실히 잘모르겠어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100002325.1.4",
          "form": "아이들이랑 영화 보러 다니셔요?",
          "original_form": "아이들이랑 영화 보러 다니셔요?",
          "speaker_id": "1",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100002325.1.5",
          "form": "사줘도 싫어해요",
          "original_form": "사줘도 싫어해요",
          "speaker_id": "2",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100002325.1.6",
          "form": "아뇨 요새 안가요",
          "original_form": "아뇨 요새 안가요",
          "speaker_id": "2",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100002325.1.7",
          "form": "극장갔다 노마스크 경험하고 안가요 ㅜㅜ",
          "original_form": "극장갔다 노마스크 경험하고 안가요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100002325.1.8",
          "form": "헉 노 마스크\r\n요즘같은 시대에 노마스크 극혐이네요",
          "original_form": "헉 노 마스크\r\n요즘같은 시대에 노마스크 극혐이네요",
          "speaker_id": "1",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100002325.1.9",
          "form": "지방이라 째려보면 절 째려봐요",
          "original_form": "지방이라 째려보면 절 째려봐요",
          "speaker_id": "2",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100002325.1.10",
          "form": "그래서 직원불렀는데 소용없었어요",
          "original_form": "그래서 직원불렀는데 소용없었어요",
          "speaker_id": "2",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100002325.1.11",
          "form": "헉 못됐다 ㅜㅜ\r\n안하무인 많은거 같아요",
          "original_form": "헉 못됐다 ㅜㅜ\r\n안하무인 많은거 같아요",
          "speaker_id": "1",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100002325.1.12",
          "form": "마지막으로 본게 기억도 안나요",
          "original_form": "마지막으로 본게 기억도 안나요",
          "speaker_id": "2",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100002325.1.13",
          "form": "요즘 모가디슈 넘 보고싶은데\r\n못가서 슬프네요",
          "original_form": "요즘 모가디슈 넘 보고싶은데\r\n못가서 슬프네요",
          "speaker_id": "1",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100002325.1.14",
          "form": "그러게요 저도 보고 싶더라고요",
          "original_form": "그러게요 저도 보고 싶더라고요",
          "speaker_id": "2",
          "time": "20210820 09:33"
        },
        {
          "id": "MDRW2100002325.1.15",
          "form": "평일에 가면 사람없긴해요",
          "original_form": "평일에 가면 사람없긴해요",
          "speaker_id": "2",
          "time": "20210820 09:33"
        },
        {
          "id": "MDRW2100002325.1.16",
          "form": "평일엔 시간이 안되고 저녘엔 사람이 많더라구요\r\n경기도쪽인데",
          "original_form": "평일엔 시간이 안되고 저녘엔 사람이 많더라구요\r\n경기도쪽인데",
          "speaker_id": "1",
          "time": "20210820 09:34"
        },
        {
          "id": "MDRW2100002325.1.17",
          "form": "경기도는 그렇죠",
          "original_form": "경기도는 그렇죠",
          "speaker_id": "2",
          "time": "20210820 09:35"
        },
        {
          "id": "MDRW2100002325.1.18",
          "form": "여긴 사람이 별로 없어요 특정 극장말고는",
          "original_form": "여긴 사람이 별로 없어요 특정 극장말고는",
          "speaker_id": "2",
          "time": "20210820 09:36"
        },
        {
          "id": "MDRW2100002325.1.19",
          "form": "우아 좋겠어요 \r\n저희쪽은\r\n동네 극장은 다 안하는 추세더라구요",
          "original_form": "우아 좋겠어요 \r\n저희쪽은\r\n동네 극장은 다 안하는 추세더라구요",
          "speaker_id": "1",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002325.1.20",
          "form": "아 그렇군요 안하다니 기회도 없네요",
          "original_form": "아 그렇군요 안하다니 기회도 없네요",
          "speaker_id": "2",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002325.1.21",
          "form": "영화시장 요새 힘들겠어요",
          "original_form": "영화시장 요새 힘들겠어요",
          "speaker_id": "2",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002325.1.22",
          "form": "그쵸  적자나면 누가 영화 제작도 덜할거 같고",
          "original_form": "그쵸  적자나면 누가 영화 제작도 덜할거 같고",
          "speaker_id": "1",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002325.1.23",
          "form": "그래서 대작보기 힘들어요",
          "original_form": "그래서 대작보기 힘들어요",
          "speaker_id": "2",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002325.1.24",
          "form": "보는 저희도 선택 폭이 적어지고요",
          "original_form": "보는 저희도 선택 폭이 적어지고요",
          "speaker_id": "1",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002325.1.25",
          "form": "아쉬워요 다 코로나가 문제임 ㅜㅜ",
          "original_form": "아쉬워요 다 코로나가 문제임 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 09:39"
        },
        {
          "id": "MDRW2100002325.1.26",
          "form": "맞아요..저 영화 vip 였는데 일반됐어요",
          "original_form": "맞아요..저 영화 vip 였는데 일반됐어요",
          "speaker_id": "2",
          "time": "20210820 09:39"
        },
        {
          "id": "MDRW2100002325.1.27",
          "form": "아예 안보니",
          "original_form": "아예 안보니",
          "speaker_id": "2",
          "time": "20210820 09:39"
        },
        {
          "id": "MDRW2100002325.1.28",
          "form": "아쉽겠어요 ㅠㅠ",
          "original_form": "아쉽겠어요 ㅠㅠ",
