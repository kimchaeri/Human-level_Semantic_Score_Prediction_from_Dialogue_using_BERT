      "utterance": [
        {
          "id": "MDRW2100025519.1.1",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100025519.1.2",
          "form": "여기 비도 엄청왔어",
          "original_form": "여기 비도 엄청왔어",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025519.1.3",
          "form": "오늘 두꺼운 긴팔티입었오",
          "original_form": "오늘 두꺼운 긴팔티입었오",
          "speaker_id": "1",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100025519.1.4",
          "form": "반팔 보내줘야하나봐",
          "original_form": "반팔 보내줘야하나봐",
          "speaker_id": "1",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100025519.1.5",
          "form": "나더 긴팔입고 얇은 남방입음",
          "original_form": "나더 긴팔입고 얇은 남방입음",
          "speaker_id": "2",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100025519.1.6",
          "form": "그리고 갑자기 뜨아 땡겨",
          "original_form": "그리고 갑자기 뜨아 땡겨",
          "speaker_id": "2",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025519.1.7",
          "form": "오 나도 나 도라지작두콩차먹으려고",
          "original_form": "오 나도 나 도라지작두콩차먹으려고",
          "speaker_id": "1",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025519.1.8",
          "form": "오 ㅋㅋㅋ 나 저번에 산 생강차 배달왔어 감기걸리면먹게",
          "original_form": "오 ㅋㅋㅋ 나 저번에 산 생강차 배달왔어 감기걸리면먹게",
          "speaker_id": "2",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100025519.1.9",
          "form": "헤에엑",
          "original_form": "헤에엑",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025519.1.10",
          "form": "생가차가 추울 때 좋다는데",
          "original_form": "생가차가 추울 때 좋다는데",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025519.1.11",
          "form": "나는 생각차싫어",
          "original_form": "나는 생각차싫어",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025519.1.12",
          "form": "ㅎㅎ나도",
          "original_form": "ㅎㅎ나도",
          "speaker_id": "2",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100025519.1.13",
          "form": "ㅎㅎ유자차만 품어 겨울엔 유자라구",
          "original_form": "ㅎㅎ유자차만 품어 겨울엔 유자라구",
          "speaker_id": "2",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100025519.1.14",
          "form": "아 인정 겨울은 유자차지",
          "original_form": "아 인정 겨울은 유자차지",
          "speaker_id": "1",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100025519.1.15",
          "form": "아직도 하늘 구리다",
          "original_form": "아직도 하늘 구리다",
          "speaker_id": "2",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025519.1.16",
          "form": "요즘 맑은날이없어",
          "original_form": "요즘 맑은날이없어",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100025519.1.17",
          "form": "그니까요 비는 그쳤는게 바람안부네..",
          "original_form": "그니까요 비는 그쳤는게 바람안부네..",
          "speaker_id": "2",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025519.1.18",
          "form": "저녁에는 제법춥더라",
          "original_form": "저녁에는 제법춥더라",
          "speaker_id": "1",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025519.1.19",
          "form": "그래도 왠지모르게 선풍기는 틀게돼",
          "original_form": "그래도 왠지모르게 선풍기는 틀게돼",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025519.1.20",
          "form": "오 나는 아닌데",
          "original_form": "오 나는 아닌데",
          "speaker_id": "1",
          "time": "20210901 16:43"
        }
      ]
