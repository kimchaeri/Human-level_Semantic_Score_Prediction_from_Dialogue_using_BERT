        },
        {
          "id": "MDRW2100045074.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045074.1.33",
          "form": "난 라이덴을 빨리 키워야해......",
          "original_form": "난 라이덴을 빨리 키워야해......",
          "speaker_id": "2",
