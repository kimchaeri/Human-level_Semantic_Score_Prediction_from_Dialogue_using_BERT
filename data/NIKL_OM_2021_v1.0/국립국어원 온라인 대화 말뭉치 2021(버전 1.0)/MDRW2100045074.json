      "utterance": [
        {
          "id": "MDRW2100045074.1.1",
          "form": "ㅎ ㅏ 진짜",
          "original_form": "ㅎ ㅏ 진짜",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045074.1.2",
          "form": "아니 좀 고유한 설정값을 넣으면 안되나..?",
          "original_form": "아니 좀 고유한 설정값을 넣으면 안되나..?",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045074.1.3",
          "form": "배경이 라노벨오딱구놈들의 성지인 일본이라고",
          "original_form": "배경이 라노벨오딱구놈들의 성지인 일본이라고",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045074.1.4",
          "form": "신난듯ㅎ",
          "original_form": "신난듯ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045074.1.5",
          "form": "스토리 상태도 그렇고",
          "original_form": "스토리 상태도 그렇고",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045074.1.6",
          "form": "ㅎ... 걍 빨리 다음 도시로 넘기고싶다",
          "original_form": "ㅎ... 걍 빨리 다음 도시로 넘기고싶다",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045074.1.7",
          "form": "ㅇㅇ설마 수메르에서",
          "original_form": "ㅇㅇ설마 수메르에서",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045074.1.8",
          "form": "오따구짓을 하진 않겠지",
          "original_form": "오따구짓을 하진 않겠지",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045074.1.9",
          "form": "자중해라 좀",
          "original_form": "자중해라 좀",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045074.1.10",
          "form": "ㅎ ㅏ 근데 이번에 매출 역대급이라며",
          "original_form": "ㅎ ㅏ 근데 이번에 매출 역대급이라며",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045074.1.11",
          "form": "??왜???",
          "original_form": "??왜???",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045074.1.12",
          "form": "같이 라노벨에 눈돌아간 오타쿠들이",
          "original_form": "같이 라노벨에 눈돌아간 오타쿠들이",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045074.1.13",
          "form": "돈써줌???",
          "original_form": "돈써줌???",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045074.1.14",
          "form": "1주년 기념으로 과금 두배 리셋",
          "original_form": "1주년 기념으로 과금 두배 리셋",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045074.1.15",
          "form": "+ 간만에 신규 신 등장",
          "original_form": "+ 간만에 신규 신 등장",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045074.1.16",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045074.1.17",
          "form": "아니 그럼",
          "original_form": "아니 그럼",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045074.1.18",
          "form": "라노벨 스토리라서가 아니잖아",
          "original_form": "라노벨 스토리라서가 아니잖아",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045074.1.19",
          "form": "제발 고려 해라........",
          "original_form": "제발 고려 해라........",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045074.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그니까...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그니까...",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045074.1.21",
          "form": "특히 한섭 이번에",
          "original_form": "특히 한섭 이번에",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045074.1.22",
          "form": "번역도 난리던대",
          "original_form": "번역도 난리던대",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045074.1.23",
          "form": "그 돈으로 검수나 더 해...",
          "original_form": "그 돈으로 검수나 더 해...",
