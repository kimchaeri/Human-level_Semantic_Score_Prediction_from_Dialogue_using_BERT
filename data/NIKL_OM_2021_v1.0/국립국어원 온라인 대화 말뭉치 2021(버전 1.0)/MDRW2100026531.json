      "utterance": [
        {
          "id": "MDRW2100026531.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026531.1.2",
          "form": "좋아하는 메뉴 있어요?",
          "original_form": "좋아하는 메뉴 있어요?",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026531.1.3",
          "form": "김치수제비요",
          "original_form": "김치수제비요",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026531.1.4",
          "form": "김치볶음밥도 좋아하고 김치들어간건 거의다 좋아해요",
          "original_form": "김치볶음밥도 좋아하고 김치들어간건 거의다 좋아해요",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026531.1.5",
          "form": "님은요??",
          "original_form": "님은요??",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026531.1.6",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026531.1.7",
          "form": "저도 떡볶이는 거의 안가리는거 같아요",
          "original_form": "저도 떡볶이는 거의 안가리는거 같아요",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026531.1.8",
          "form": "아 떡볶이 맛있죠",
          "original_form": "아 떡볶이 맛있죠",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026531.1.9",
          "form": "좋아하는 체인점있어요?",
          "original_form": "좋아하는 체인점있어요?",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026531.1.10",
          "form": "못먹는건 없는데 엽떡 좋아해요",
          "original_form": "못먹는건 없는데 엽떡 좋아해요",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026531.1.11",
          "form": "매운거 잘드시나보다",
          "original_form": "매운거 잘드시나보다",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026531.1.12",
          "form": "먹고나면.. 화장실에서 너무 고통스럽던데",
          "original_form": "먹고나면.. 화장실에서 너무 고통스럽던데",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026531.1.13",
          "form": "괜찮으신가봐요",
          "original_form": "괜찮으신가봐요",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026531.1.14",
          "form": "아 저도 화장실은 고통스러운데",
          "original_form": "아 저도 화장실은 고통스러운데",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026531.1.15",
          "form": "순간의 입에서의 행복에 중독되는바람에...",
          "original_form": "순간의 입에서의 행복에 중독되는바람에...",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026531.1.16",
          "form": "먹을땐 너무 맛있어서 끊을 수가 없네요ㅠㅠ",
          "original_form": "먹을땐 너무 맛있어서 끊을 수가 없네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026531.1.17",
          "form": "ㅋㅋㅋ중독적인 맛이죠.. 로제 나왔을때 시켜먹었는데 그건 덜매워서 잘먹었던거 같아요",
          "original_form": "ㅋㅋㅋ중독적인 맛이죠.. 로제 나왔을때 시켜먹었는데 그건 덜매워서 잘먹었던거 같아요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026531.1.18",
          "form": "집에서 요리하세요?",
          "original_form": "집에서 요리하세요?",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026531.1.19",
          "form": "집에서도 떡볶이해먹어요ㅋㅋㄲ",
          "original_form": "집에서도 떡볶이해먹어요ㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026531.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ떡볶이 사랑이 대단하신데요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ떡볶이 사랑이 대단하신데요",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026531.1.21",
          "form": "저는 떡볶이에 양배추 엄청 넣어먹어요",
          "original_form": "저는 떡볶이에 양배추 엄청 넣어먹어요",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026531.1.22",
          "form": "떡보다 더 넣는듯..",
          "original_form": "떡보다 더 넣는듯..",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026531.1.23",
          "form": "떡볶이 뭐 넣어 드시는지 궁금",
          "original_form": "떡볶이 뭐 넣어 드시는지 궁금",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026531.1.24",
          "form": "음 그런거 같더라고요",
          "original_form": "음 그런거 같더라고요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026531.1.25",
          "form": "저도 양배추 좋아해요",
          "original_form": "저도 양배추 좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026531.1.26",
          "form": "양배추가 위장에도 좋대요?!",
          "original_form": "양배추가 위장에도 좋대요?!",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026531.1.27",
          "form": "맞아요! 굿굿",
          "original_form": "맞아요! 굿굿",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026531.1.28",
          "form": "양배추 쌈도 좋구",
          "original_form": "양배추 쌈도 좋구",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026531.1.29",
          "form": "양배추 김치 ㅋㅋㅋㅋ도 해먹어요",
          "original_form": "양배추 김치 ㅋㅋㅋㅋ도 해먹어요",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026531.1.30",
          "form": "보통 양배추 안좋아하는 사람도 있던데",
          "original_form": "보통 양배추 안좋아하는 사람도 있던데",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026531.1.31",
          "form": "저는 양배추 피클이요!",
          "original_form": "저는 양배추 피클이요!",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026531.1.32",
          "form": "아 완전 맛나죠!!",
          "original_form": "아 완전 맛나죠!!",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026531.1.33",
          "form": "지금 우유에 더위사냥 섞어먹는 중인데 이거 완전 맛있어요",
          "original_form": "지금 우유에 더위사냥 섞어먹는 중인데 이거 완전 맛있어요",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026531.1.34",
          "form": "텀블러에 우유넣고 더위사냥 넣고 흔들면 짱 맛있는 쉐이크가 됩니다",
          "original_form": "텀블러에 우유넣고 더위사냥 넣고 흔들면 짱 맛있는 쉐이크가 됩니다",
          "speaker_id": "2",
          "time": "20210901 23:06"
        }
      ]
