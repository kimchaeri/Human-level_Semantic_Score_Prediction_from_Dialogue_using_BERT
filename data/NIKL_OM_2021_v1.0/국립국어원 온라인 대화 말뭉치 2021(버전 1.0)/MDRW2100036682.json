      "utterance": [
        {
          "id": "MDRW2100036682.1.1",
          "form": "아 아예 드가는게 안되나요?",
          "original_form": "아 아예 드가는게 안되나요?",
          "speaker_id": "2",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100036682.1.2",
          "form": "들어갈수 있긴해요",
          "original_form": "들어갈수 있긴해요",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036682.1.3",
          "form": "그 줄쳐놓은거 넘어가거나",
          "original_form": "그 줄쳐놓은거 넘어가거나",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036682.1.4",
          "form": "안쳐진곳도 있긴해요 ㅋㅋ",
          "original_form": "안쳐진곳도 있긴해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036682.1.5",
          "form": "아.ㅋㅋ들어가는거 원래 안되는거 아녜요?ㅋ",
          "original_form": "아.ㅋㅋ들어가는거 원래 안되는거 아녜요?ㅋ",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036682.1.6",
          "form": "뭐 원칙은? ㅋㅋ",
          "original_form": "뭐 원칙은? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036682.1.7",
          "form": "ㅋㅋㅋ. 이런",
          "original_form": "ㅋㅋㅋ. 이런",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036682.1.8",
          "form": "그래도 간간히 해변 걷는사람 간간히 있긴해요 ㅋㅋ",
          "original_form": "그래도 간간히 해변 걷는사람 간간히 있긴해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036682.1.9",
          "form": "맘 먹고가는사람들은ㅠㅠ조용한 해변이라도 이때다싶어ㅋㄱ",
          "original_form": "맘 먹고가는사람들은ㅠㅠ조용한 해변이라도 이때다싶어ㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036682.1.10",
          "form": "멀리서 온다기보다",
          "original_form": "멀리서 온다기보다",
          "speaker_id": "1",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036682.1.11",
          "form": "동네주민일수도요 ㅋㅋ",
          "original_form": "동네주민일수도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036682.1.12",
          "form": "그냥 산책겸 데이트겸 해변 걷는?",
          "original_form": "그냥 산책겸 데이트겸 해변 걷는?",
          "speaker_id": "1",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100036682.1.13",
          "form": "와~ 좋겠어요ㅡㅠ바다를 매일걸을수있다니 ㅠ저희는큼맘 먹고 가야하는데",
          "original_form": "와~ 좋겠어요ㅡㅠ바다를 매일걸을수있다니 ㅠ저희는큼맘 먹고 가야하는데",
          "speaker_id": "2",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100036682.1.14",
          "form": "약간 그런거죠",
          "original_form": "약간 그런거죠",
          "speaker_id": "1",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100036682.1.15",
          "form": "경주사는친구는",
          "original_form": "경주사는친구는",
          "speaker_id": "1",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100036682.1.16",
          "form": "석가탑 매일 저녁산책가고",
          "original_form": "석가탑 매일 저녁산책가고",
          "speaker_id": "1",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100036682.1.17",
          "form": "황성공원에 개산책시키던데 ㅋㅋ그런느낌이랄까",
          "original_form": "황성공원에 개산책시키던데 ㅋㅋ그런느낌이랄까",
          "speaker_id": "1",
          "time": "20210831 10:02"
        },
        {
          "id": "MDRW2100036682.1.18",
          "form": "아ㅋㅋ너무웃기네요 ㄱ",
          "original_form": "아ㅋㅋ너무웃기네요 ㄱ",
          "speaker_id": "2",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100036682.1.19",
          "form": "전 그래서인지 별로 그 바다에대한 환상이없어요",
          "original_form": "전 그래서인지 별로 그 바다에대한 환상이없어요",
          "speaker_id": "1",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100036682.1.20",
          "form": "그럴수도있겠네요",
          "original_form": "그럴수도있겠네요",
          "speaker_id": "2",
          "time": "20210831 10:28"
        }
      ]
