      "utterance": [
        {
          "id": "MDRW2100000892.1.1",
          "form": "게임하는거 좋아하세요?",
          "original_form": "게임하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000892.1.2",
          "form": "네 요즘은 모바일게임 하나만하네요 님은요",
          "original_form": "네 요즘은 모바일게임 하나만하네요 님은요",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000892.1.3",
          "form": "전 게임 잘 안해요.. 중독이 무서워서",
          "original_form": "전 게임 잘 안해요.. 중독이 무서워서",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000892.1.4",
          "form": "어떤 게임하세요? 전 쿠키런 좋아했었는데",
          "original_form": "어떤 게임하세요? 전 쿠키런 좋아했었는데",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000892.1.5",
          "form": "전 금방금방질려해서 중독되기전에 삭제해요 ㅋㅋㅋㅋ요즘은 쿠킹덤하고있어요",
          "original_form": "전 금방금방질려해서 중독되기전에 삭제해요 ㅋㅋㅋㅋ요즘은 쿠킹덤하고있어요",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000892.1.6",
          "form": "근데 슬슬질려서 이것도 지워야하나 생각중이에요",
          "original_form": "근데 슬슬질려서 이것도 지워야하나 생각중이에요",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000892.1.7",
          "form": "쿠킹덤.. 재밌어보이더라고요",
          "original_form": "쿠킹덤.. 재밌어보이더라고요",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000892.1.8",
          "form": "전 게임 꽂히면 며칠 밤샘이 기본이라",
          "original_form": "전 게임 꽂히면 며칠 밤샘이 기본이라",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000892.1.9",
          "form": "아하 전 집중력이 그만큼 안돼요ㅜ ㅋㅋㅋㅋㅋㅜ",
          "original_form": "아하 전 집중력이 그만큼 안돼요ㅜ ㅋㅋㅋㅋㅋㅜ",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000892.1.10",
          "form": "전 짧고 빠르게 불태우는..! ㅋㅋㅋㅋㅋ",
          "original_form": "전 짧고 빠르게 불태우는..! ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000892.1.11",
          "form": "유튜버 hahaha님네 고양이 나오는 게임이 새로 나왔다던ㄷ0",
          "original_form": "유튜버 hahaha님네 고양이 나오는 게임이 새로 나왔다던ㄷ0",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000892.1.12",
          "form": "나왔다던데.. 고양이 좋아해서 한번 해볼까싶어요",
          "original_form": "나왔다던데.. 고양이 좋아해서 한번 해볼까싶어요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000892.1.13",
          "form": "아 맞아요 봤어요 얼마나 귀여울지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 봤어요 얼마나 귀여울지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000892.1.14",
          "form": "그건 짧게할수있을것같더라구여",
          "original_form": "그건 짧게할수있을것같더라구여",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000892.1.15",
          "form": "그쵸!! 레벨이나 스토리가 엄청 많고.. 그런게 아니라",
          "original_form": "그쵸!! 레벨이나 스토리가 엄청 많고.. 그런게 아니라",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000892.1.16",
          "form": "짧게 클리어할수있을것같은 게임이예요 ㅋㅋㅋ",
          "original_form": "짧게 클리어할수있을것같은 게임이예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000892.1.17",
          "form": "네 귀여운 냥이들도 잔뜩보고요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 귀여운 냥이들도 잔뜩보고요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000892.1.18",
          "form": "맞아요.. 중독보단 힐링에 가까운 게임..ㅎㅎ",
          "original_form": "맞아요.. 중독보단 힐링에 가까운 게임..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000892.1.19",
          "form": "사실 오늘 너무 심심해서 메이플 깔려고했는데",
          "original_form": "사실 오늘 너무 심심해서 메이플 깔려고했는데",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000892.1.20",
          "form": "닉네임 고민하다가 포기했어요",
          "original_form": "닉네임 고민하다가 포기했어요",
          "speaker_id": "1",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000892.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ메이플은 닉넴짓는게 젤오래걸리는것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ메이플은 닉넴짓는게 젤오래걸리는것같아요",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000892.1.22",
          "form": "웬만하면 다있고",
          "original_form": "웬만하면 다있고",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000892.1.23",
          "form": "이상한건또 짓기싫고 그러다보면 한참걸려서 하기도전에 진빠지는",
          "original_form": "이상한건또 짓기싫고 그러다보면 한참걸려서 하기도전에 진빠지는",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000892.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000892.1.25",
          "form": "전 메이플도 오래해봤자 일이주일인데",
          "original_form": "전 메이플도 오래해봤자 일이주일인데",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000892.1.26",
          "form": "닉네임 고민하는데 꼬박 하루씩 걸려요..",
          "original_form": "닉네임 고민하는데 꼬박 하루씩 걸려요..",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000892.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 그런게임류는 캐릭터만드는게 젤오래걸려요 그리고 금방질리는 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 그런게임류는 캐릭터만드는게 젤오래걸려요 그리고 금방질리는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000892.1.28",
          "form": "메이플 아직도 사람들 많이하더만여",
          "original_form": "메이플 아직도 사람들 많이하더만여",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
