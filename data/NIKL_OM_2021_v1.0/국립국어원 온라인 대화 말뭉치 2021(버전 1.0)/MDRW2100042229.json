      "utterance": [
        {
          "id": "MDRW2100042229.1.1",
          "form": "네 잘 챙겨먹진 못하구요",
          "original_form": "네 잘 챙겨먹진 못하구요",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042229.1.2",
          "form": "뭐드세요",
          "original_form": "뭐드세요",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042229.1.3",
          "form": "비타민 정도요",
          "original_form": "비타민 정도요",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042229.1.4",
          "form": "드시는거 있어요?",
          "original_form": "드시는거 있어요?",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042229.1.5",
          "form": "저 비타민하고 콜라겐 오메가용",
          "original_form": "저 비타민하고 콜라겐 오메가용",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042229.1.6",
          "form": "효과 보는것 같나요?",
          "original_form": "효과 보는것 같나요?",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042229.1.7",
          "form": "아뇨 ㅋㅋㅋㅋ 잘 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋㅋ 잘 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042229.1.8",
          "form": "ㅋㅋㅋ먹다가 안먹으면 확 느껴진다던데요",
          "original_form": "ㅋㅋㅋ먹다가 안먹으면 확 느껴진다던데요",
          "speaker_id": "2",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042229.1.9",
          "form": "아 그래요? 맞아 무조건 먹어야해...",
          "original_form": "아 그래요? 맞아 무조건 먹어야해...",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042229.1.10",
          "form": "먹다 안먹다 하니까 솔직히 잘 모르겠긴 하더라구요",
          "original_form": "먹다 안먹다 하니까 솔직히 잘 모르겠긴 하더라구요",
          "speaker_id": "2",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042229.1.11",
          "form": "그렇군요 저는 녹즙 먹다가 끊었는데",
          "original_form": "그렇군요 저는 녹즙 먹다가 끊었는데",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042229.1.12",
          "form": "별차이 모르겠어요",
          "original_form": "별차이 모르겠어요",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042229.1.13",
          "form": "녹즙은 맛없구 양도 많아서 못먹겠어요. 차라리 약 삼키는게 나은거 같아요ㅋㅋ",
          "original_form": "녹즙은 맛없구 양도 많아서 못먹겠어요. 차라리 약 삼키는게 나은거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042229.1.14",
          "form": "아정말요 풀무원 녹즙은 야구르트 처럼 맛있는데",
          "original_form": "아정말요 풀무원 녹즙은 야구르트 처럼 맛있는데",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042229.1.15",
          "form": "그래요? 녹즙은 다 맛없을줄 알았네요ㅋㅋ",
          "original_form": "그래요? 녹즙은 다 맛없을줄 알았네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:23"
        }
      ]
