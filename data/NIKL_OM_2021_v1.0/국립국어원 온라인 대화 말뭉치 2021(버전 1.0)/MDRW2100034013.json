      "utterance": [
        {
          "id": "MDRW2100034013.1.1",
          "form": "머지포인트 있나?",
          "original_form": "머지포인트 있나?",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100034013.1.2",
          "form": "아까 뉴스보니 난리났던데",
          "original_form": "아까 뉴스보니 난리났던데",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100034013.1.3",
          "form": "난 당연히 없지 안한다 저번부터 난리였음",
          "original_form": "난 당연히 없지 안한다 저번부터 난리였음",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034013.1.4",
          "form": "근데 은근 많이 가지고 있드라",
          "original_form": "근데 은근 많이 가지고 있드라",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034013.1.5",
          "form": "저번에 우리 손예진 나오는 드라마 이름을 몰랐잖아  사랑의 불시착이다 갑자기 생각남",
          "original_form": "저번에 우리 손예진 나오는 드라마 이름을 몰랐잖아  사랑의 불시착이다 갑자기 생각남",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034013.1.6",
          "form": "ㅋㅋㅋ 난 북한만 생각났음",
          "original_form": "ㅋㅋㅋ 난 북한만 생각났음",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100034013.1.7",
          "form": "배우 이름도 기억 아나더니 이제는 드라마 이름도 가물가물",
          "original_form": "배우 이름도 기억 아나더니 이제는 드라마 이름도 가물가물",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100034013.1.8",
          "form": "우리가 나이를 먹긴 먹나보다",
          "original_form": "우리가 나이를 먹긴 먹나보다",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100034013.1.9",
          "form": "유퀴즈에 황정민 영화 홍보하러 나오나보다",
          "original_form": "유퀴즈에 황정민 영화 홍보하러 나오나보다",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034013.1.10",
          "form": "조승우 지진희 황정민 찍은 사진 때문에 나올거 같드라",
          "original_form": "조승우 지진희 황정민 찍은 사진 때문에 나올거 같드라",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034013.1.11",
          "form": "오늘 방송분이네",
          "original_form": "오늘 방송분이네",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034013.1.12",
          "form": "안그래도 나와서 그 사진 얘기햇나보다",
          "original_form": "안그래도 나와서 그 사진 얘기햇나보다",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034013.1.13",
          "form": "조승우 지진희 좀 4차원이던데ㅋ",
          "original_form": "조승우 지진희 좀 4차원이던데ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034013.1.14",
          "form": "연예인들은 거의 다 그런듯 일반인랑 틀린거 같더라",
          "original_form": "연예인들은 거의 다 그런듯 일반인랑 틀린거 같더라",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034013.1.15",
          "form": "오히려 황정민 정상인 같고 둘이 사차원ㅋ",
          "original_form": "오히려 황정민 정상인 같고 둘이 사차원ㅋ",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100034013.1.16",
          "form": "ㅋㅋ 낼 재방송으로 봐야겠다",
          "original_form": "ㅋㅋ 낼 재방송으로 봐야겠다",
          "speaker_id": "2",
          "time": "20210825 22:50"
        }
      ]
