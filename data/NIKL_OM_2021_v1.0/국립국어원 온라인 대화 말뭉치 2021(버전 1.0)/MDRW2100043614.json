      "utterance": [
        {
          "id": "MDRW2100043614.1.1",
          "form": "아 엄마표 갈비찜 먹구싶어",
          "original_form": "아 엄마표 갈비찜 먹구싶어",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.2",
          "form": "와맞아",
          "original_form": "와맞아",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.3",
          "form": "생일에만 먹을수잇자나",
          "original_form": "생일에만 먹을수잇자나",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.4",
          "form": "내말이...슬프다",
          "original_form": "내말이...슬프다",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.5",
          "form": "언제쯤 먹을수잇을까ㅠㅡ",
          "original_form": "언제쯤 먹을수잇을까ㅠㅡ",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.6",
          "form": "아빠생일?",
          "original_form": "아빠생일?",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.7",
          "form": "제일 가까워서??",
          "original_form": "제일 가까워서??",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.9",
          "form": "갈비찜은 그래도 돼지야",
          "original_form": "갈비찜은 그래도 돼지야",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.10",
          "form": "돼지고기가 제일좋야",
          "original_form": "돼지고기가 제일좋야",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.11",
          "form": "학 나두나두",
          "original_form": "학 나두나두",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.12",
          "form": "감자도 좋아?",
          "original_form": "감자도 좋아?",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043614.1.13",
          "form": "감자도 좋아하지",
          "original_form": "감자도 좋아하지",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043614.1.14",
          "form": "당군은 어때",
          "original_form": "당군은 어때",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043614.1.15",
          "form": "당근은 좀....",
          "original_form": "당근은 좀....",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043614.1.16",
          "form": "그건 데코용이야",
          "original_form": "그건 데코용이야",
          "speaker_id": "1",
          "time": "20210907 19:44"
