        },
        {
          "id": "MDRW2100040585.1.2",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040585.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040585.1.4",
          "form": "늙은호박으로 전 부쳐먹었어요",
          "original_form": "늙은호박으로 전 부쳐먹었어요",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040585.1.5",
          "form": "저녁 뭐 드셨어요?ㅎㅎ",
          "original_form": "저녁 뭐 드셨어요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040585.1.6",
          "form": "저는 곱창이요",
          "original_form": "저는 곱창이요",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040585.1.7",
          "form": "오 맛있는 것 드셨네요",
          "original_form": "오 맛있는 것 드셨네요",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040585.1.8",
          "form": "곱창구이 드신건가요?",
          "original_form": "곱창구이 드신건가요?",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040585.1.9",
          "form": "네!! 그럼 name2님운 호박전 드신건가요?",
          "original_form": "네!! 그럼 &name2&님운 호박전 드신건가요?",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040585.1.10",
          "form": "네 호박전이요",
          "original_form": "네 호박전이요",
          "speaker_id": "2",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100040585.1.11",
          "form": "갑자기 늙은호박이 생겨서",
          "original_form": "갑자기 늙은호박이 생겨서",
          "speaker_id": "2",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100040585.1.12",
          "form": "며칠은 호박 활용한 요리를 해먹어야해요",
          "original_form": "며칠은 호박 활용한 요리를 해먹어야해요",
          "speaker_id": "2",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100040585.1.13",
          "form": "누구한테서 받으신건가요?",
          "original_form": "누구한테서 받으신건가요?",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040585.1.14",
          "form": "어머니 아는 분이 농사지으셨다고",
          "original_form": "어머니 아는 분이 농사지으셨다고",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040585.1.15",
          "form": "주고 가셨어요",
          "original_form": "주고 가셨어요",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040585.1.16",
          "form": "그렇군요\r\n그럼 호박으로 만드시고 싶은 음식 있으신가요?",
          "original_form": "그렇군요\r\n그럼 호박으로 만드시고 싶은 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040585.1.17",
          "form": "일단 유튜브에 늙은호박 요리 검색해보니",
          "original_form": "일단 유튜브에 늙은호박 요리 검색해보니",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040585.1.18",
          "form": "호박죽 호박전이 제일 많이 나오더라고요",
          "original_form": "호박죽 호박전이 제일 많이 나오더라고요",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040585.1.19",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040585.1.20",
          "form": "호박으로 만들 수 있는게 많네요",
          "original_form": "호박으로 만들 수 있는게 많네요",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040585.1.21",
