      "utterance": [
        {
          "id": "MDRW2100006116.1.1",
          "form": "음식얘기했었어요",
          "original_form": "음식얘기했었어요",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006116.1.2",
          "form": "혹시 백신맞으셨어요?",
          "original_form": "혹시 백신맞으셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006116.1.3",
          "form": "백신 목요일에 맞으러가요!",
          "original_form": "백신 목요일에 맞으러가요!",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006116.1.4",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006116.1.5",
          "form": "네! 저는 잔여백신으로 2차까지 완료했습니닷",
          "original_form": "네! 저는 잔여백신으로 2차까지 완료했습니닷",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006116.1.6",
          "form": "1차신가요?",
          "original_form": "1차신가요?",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006116.1.7",
          "form": "네네! 1차입니다!",
          "original_form": "네네! 1차입니다!",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006116.1.8",
          "form": "맞으실때 안아팠나요? 아플까바 무서워요 ㅠㅠ",
          "original_form": "맞으실때 안아팠나요? 아플까바 무서워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006116.1.9",
          "form": "맞을 때는 간호사분이 잘 놓으시는분인지 하나도 안아팠어요...!",
          "original_form": "맞을 때는 간호사분이 잘 놓으시는분인지 하나도 안아팠어요...!",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006116.1.10",
          "form": "1차때는 그냥 졸음이 엄청 쏟아지는정도...?",
          "original_form": "1차때는 그냥 졸음이 엄청 쏟아지는정도...?",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006116.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006116.1.12",
          "form": "원래도 잠이 많은데... 큰일났다 ㅋㅋ",
          "original_form": "원래도 잠이 많은데... 큰일났다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006116.1.13",
          "form": "당일만 잠이 엄청 쏟아져요?",
          "original_form": "당일만 잠이 엄청 쏟아져요?",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006116.1.14",
          "form": "ㅋㅋㅋ제주변에도 그렇고 1차 때는 다들 졸립댔어요",
          "original_form": "ㅋㅋㅋ제주변에도 그렇고 1차 때는 다들 졸립댔어요",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006116.1.15",
          "form": "저는 1시간 뒤에 졸음이 막 쏟아져서 잤어요 그냥",
          "original_form": "저는 1시간 뒤에 졸음이 막 쏟아져서 잤어요 그냥",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006116.1.16",
          "form": "당일이 제일 심하고 3일정도 갔어요 미세하게 피곤한게...",
          "original_form": "당일이 제일 심하고 3일정도 갔어요 미세하게 피곤한게...",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006116.1.17",
          "form": "안대 ㅠㅠ 그날 가을잠 준비해야겠네요..",
          "original_form": "안대 ㅠㅠ 그날 가을잠 준비해야겠네요..",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006116.1.18",
          "form": "그럼 아픈건 따로 없었나요?",
          "original_form": "그럼 아픈건 따로 없었나요?",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006116.1.19",
          "form": "전 2차때 하루정도 열나고 몸살났었어요",
          "original_form": "전 2차때 하루정도 열나고 몸살났었어요",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006116.1.20",
          "form": "헛! 지금은 괜찮으세요?",
          "original_form": "헛! 지금은 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006116.1.21",
          "form": "네!! 지금은 말짱합니다ㅋㅋㅋ",
          "original_form": "네!! 지금은 말짱합니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006116.1.22",
          "form": "걱정했었는데 의외로? 괜찮았어요",
          "original_form": "걱정했었는데 의외로? 괜찮았어요",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006116.1.23",
          "form": "다행이네요ㅎㅎ",
          "original_form": "다행이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006116.1.24",
          "form": "제가 지금 엄청 걱정중인데 별일 없겠죠?",
          "original_form": "제가 지금 엄청 걱정중인데 별일 없겠죠?",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006116.1.25",
          "form": "제가 꽤 약한편인데 괜찮은거 보면 name2님은 하나도 안아프시고 지나갈 수 있을거에요!",
          "original_form": "제가 꽤 약한편인데 괜찮은거 보면 &name2&님은 하나도 안아프시고 지나갈 수 있을거에요!",
          "speaker_id": "1",
          "time": "20210907 21:19"
        }
      ]
    }
