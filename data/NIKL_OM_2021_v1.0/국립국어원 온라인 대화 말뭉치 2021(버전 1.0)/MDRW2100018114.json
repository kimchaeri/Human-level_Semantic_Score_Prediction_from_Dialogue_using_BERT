      "utterance": [
        {
          "id": "MDRW2100018114.1.1",
          "form": "군인들 마스크 벗을 거라더라",
          "original_form": "군인들 마스크 벗을 거라더라",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018114.1.2",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018114.1.3",
          "form": "그래도 위험하지 않나?",
          "original_form": "그래도 위험하지 않나?",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018114.1.4",
          "form": "그러니까 괜찮으려나 모르겠네",
          "original_form": "그러니까 괜찮으려나 모르겠네",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018114.1.5",
          "form": "그러다 걸리기라도 하면",
          "original_form": "그러다 걸리기라도 하면",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018114.1.6",
          "form": "맞아 휴가도 통제 당하고 그럴 텐데",
          "original_form": "맞아 휴가도 통제 당하고 그럴 텐데",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018114.1.7",
          "form": "넘 불쌍해",
          "original_form": "넘 불쌍해",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018114.1.8",
          "form": "그치 완전 불쌍하지 실험 대상이 되는 거잖아",
          "original_form": "그치 완전 불쌍하지 실험 대상이 되는 거잖아",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018114.1.9",
          "form": "그니까 군인들이 젤 불쌍해",
          "original_form": "그니까 군인들이 젤 불쌍해",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018114.1.10",
          "form": "대우도 그렇고",
          "original_form": "대우도 그렇고",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018114.1.11",
          "form": "에휴 밥이라도 좀 제대로 잘 주지",
          "original_form": "에휴 밥이라도 좀 제대로 잘 주지",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018114.1.12",
          "form": "저러니까 불만이 터지지",
          "original_form": "저러니까 불만이 터지지",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018114.1.13",
          "form": "그러니까 어떻게 밥을 가지고 장난을 치냐",
          "original_form": "그러니까 어떻게 밥을 가지고 장난을 치냐",
          "speaker_id": "1",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018114.1.14",
          "form": "먹을거 가지고 그러는건 진짜 아니지",
          "original_form": "먹을거 가지고 그러는건 진짜 아니지",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018114.1.15",
          "form": "나 같으면 서러워서 맨날 울었을 거야",
          "original_form": "나 같으면 서러워서 맨날 울었을 거야",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018114.1.16",
