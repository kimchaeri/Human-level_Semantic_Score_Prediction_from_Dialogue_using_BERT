      "utterance": [
        {
          "id": "MDRW2100018120.1.1",
          "form": "주말에 영화 넷플로보시나요?",
          "original_form": "주말에 영화 넷플로보시나요?",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018120.1.2",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018120.1.3",
          "form": "넷플왓차 다있거든요",
          "original_form": "넷플왓차 다있거든요",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018120.1.4",
          "form": "룸메이트가..",
          "original_form": "룸메이트가..",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018120.1.5",
          "form": "전없고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전없고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018120.1.6",
          "form": "앜ㅋㅋㅋㅋㅋ버스타시는군요",
          "original_form": "앜ㅋㅋㅋㅋㅋ버스타시는군요",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018120.1.7",
          "form": "근데 그거 사실 저두 ㅋㅋㅋㅋㄱㅋ",
          "original_form": "근데 그거 사실 저두 ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018120.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ핫 룸메 잘 둔 느낌 큼큼",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ핫 룸메 잘 둔 느낌 큼큼",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018120.1.9",
          "form": "혼자서는 잘 안 보게 되더라고요",
          "original_form": "혼자서는 잘 안 보게 되더라고요",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018120.1.10",
          "form": "그쵸.... 저흰 전자레인지 팝콘 쟁겨두고 돌려서 보면서 먹기도하는데 어떠세요?",
          "original_form": "그쵸.... 저흰 전자레인지 팝콘 쟁겨두고 돌려서 보면서 먹기도하는데 어떠세요?",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018120.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ완전 제대로네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ완전 제대로네요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018120.1.12",
          "form": "저희는 전자렌지 없는데 룸메가 팝콘 좋아해서",
          "original_form": "저희는 전자렌지 없는데 룸메가 팝콘 좋아해서",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018120.1.13",
          "form": "편의점에서 돌려와서 먹었단 적은 있어요",
          "original_form": "편의점에서 돌려와서 먹었단 적은 있어요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018120.1.14",
          "form": "룸메 귀엽죠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "룸메 귀엽죠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018120.1.15",
          "form": "평소에는 그냥 배달음식 시켜서 먹으면서 봐요",
          "original_form": "평소에는 그냥 배달음식 시켜서 먹으면서 봐요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018120.1.16",
          "form": "앜ㅋㅋㅋㄱ귀여우시다",
          "original_form": "앜ㅋㅋㅋㄱ귀여우시다",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018120.1.17",
          "form": "드라마도 보시나요?",
          "original_form": "드라마도 보시나요?",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018120.1.18",
          "form": "드라마는 미드 굿플레이스 재밌게봤어요",
          "original_form": "드라마는 미드 굿플레이스 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018120.1.19",
          "form": "미드 저녁먹을때 시간맞으면 같이 한편씩 틀어두고 보기 좋죠",
          "original_form": "미드 저녁먹을때 시간맞으면 같이 한편씩 틀어두고 보기 좋죠",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018120.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 저녁먹을때라고 하니가 생각났는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 저녁먹을때라고 하니가 생각났는데",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018120.1.21",
          "form": "템테이션 아일랜드 봤거든요",
          "original_form": "템테이션 아일랜드 봤거든요",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018120.1.22",
          "form": "아주완전막장..",
          "original_form": "아주완전막장..",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018120.1.23",
          "form": "앗 그거ㅋㅋㅋㅋ",
          "original_form": "앗 그거ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018120.1.24",
          "form": "너무 막장이죠ㅋㅋㅋㅋ",
          "original_form": "너무 막장이죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018120.1.25",
          "form": "못된놈도 하나나오지 않나요",
          "original_form": "못된놈도 하나나오지 않나요",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018120.1.26",
          "form": "헐 아시는구나",
          "original_form": "헐 아시는구나",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018120.1.27",
          "form": "남자패널중에..",
          "original_form": "남자패널중에..",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018120.1.28",
          "form": "그그",
          "original_form": "그그",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018120.1.29",
          "form": "에번스?",
          "original_form": "에번스?",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018120.1.30",
          "form": "완전 사기꾼이라면서요",
          "original_form": "완전 사기꾼이라면서요",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018120.1.31",
          "form": "모건 선택했는데 걀국 그사람한케도 사기챢대뇨",
          "original_form": "모건 선택했는데 걀국 그사람한케도 사기챢대뇨",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018120.1.32",
          "form": "맞는거 같아요 후기 폭로까지봐야",
          "original_form": "맞는거 같아요 후기 폭로까지봐야",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018120.1.33",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018120.1.34",
          "form": "그거요 저도봤어요",
          "original_form": "그거요 저도봤어요",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018120.1.35",
          "form": "앗 그래서 저녁드시면서 템테이션 보셨는데",
          "original_form": "앗 그래서 저녁드시면서 템테이션 보셨는데",
          "speaker_id": "1",
          "time": "20210826 22:58"
        }
      ]
