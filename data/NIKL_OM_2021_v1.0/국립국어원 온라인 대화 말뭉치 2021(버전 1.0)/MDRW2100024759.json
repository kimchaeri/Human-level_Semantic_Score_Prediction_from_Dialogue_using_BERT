      "utterance": [
        {
          "id": "MDRW2100024759.1.1",
          "form": "스포츠경기 자주 보시나요?",
          "original_form": "스포츠경기 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024759.1.2",
          "form": "아뇽 ㅋㅋㅋ 요즘은 딱히...",
          "original_form": "아뇽 ㅋㅋㅋ 요즘은 딱히...",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024759.1.3",
          "form": "재밌는게없네요",
          "original_form": "재밌는게없네요",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024759.1.4",
          "form": "혹시 직접가서 보신  경기는  있으세요?",
          "original_form": "혹시 직접가서 보신  경기는  있으세요?",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024759.1.5",
          "form": "직관하신거요~",
          "original_form": "직관하신거요~",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024759.1.6",
          "form": "음...진짜 옛날에 야구?ㅋㅋㅋ",
          "original_form": "음...진짜 옛날에 야구?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024759.1.7",
          "form": "직관이 진짜 재밌는것 같긴해요",
          "original_form": "직관이 진짜 재밌는것 같긴해요",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024759.1.8",
          "form": "그쵸~~~?",
          "original_form": "그쵸~~~?",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024759.1.9",
          "form": "야구 직관이 제일 재밌는거 같아요",
          "original_form": "야구 직관이 제일 재밌는거 같아요",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024759.1.10",
          "form": "응원가도 재미있고~~!",
          "original_form": "응원가도 재미있고~~!",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024759.1.11",
          "form": "마자요마자요 응원가 ㅋㅋㅋㅋ",
          "original_form": "마자요마자요 응원가 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024759.1.12",
          "form": "ㅎㅎㅎ 야구장 못간지 꽤 됐는데",
          "original_form": "ㅎㅎㅎ 야구장 못간지 꽤 됐는데",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024759.1.13",
          "form": "규칙을 잘 알지는 못해도",
          "original_form": "규칙을 잘 알지는 못해도",
          "speaker_id": "1",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024759.1.14",
          "form": "같이 응원하고 치킨먹으면 너무 즐겁더라구요",
          "original_form": "같이 응원하고 치킨먹으면 너무 즐겁더라구요",
          "speaker_id": "1",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024759.1.15",
          "form": "ㅋㅋㅋㅋ 맞아요...근데 못간지 넘 오래라ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 맞아요...근데 못간지 넘 오래라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024759.1.16",
          "form": "핑계일 수도 있는데 쉴 때는 야구장보다 집콕하고 싶어져서ㅎㅎ",
          "original_form": "핑계일 수도 있는데 쉴 때는 야구장보다 집콕하고 싶어져서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024759.1.17",
          "form": "그춍ㅋㅋㅋ 야구장 가는거도 일이긴해요",
          "original_form": "그춍ㅋㅋㅋ 야구장 가는거도 일이긴해요",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024759.1.18",
          "form": "네! 그리고 요새 야구선수들을 잘 몰라서ㅎㅎ",
          "original_form": "네! 그리고 요새 야구선수들을 잘 몰라서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024759.1.19",
          "form": "야구하면 치맥 응원가만 떠오르네요ㅎㅎ",
          "original_form": "야구하면 치맥 응원가만 떠오르네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024759.1.20",
          "form": "ㅋㅋㅋ 진짜..계속 바뀌니까요...이정후..박에모름",
          "original_form": "ㅋㅋㅋ 진짜..계속 바뀌니까요...이정후..박에모름",
          "speaker_id": "2",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024759.1.21",
          "form": "네 선수들도 너무 많고ㅋㅋ",
          "original_form": "네 선수들도 너무 많고ㅋㅋ",
