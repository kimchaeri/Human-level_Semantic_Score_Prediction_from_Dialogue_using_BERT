      "utterance": [
        {
          "id": "MDRW2100040950.1.1",
          "form": "그렇구나 염증이 있으세요?ㅠ",
          "original_form": "그렇구나 염증이 있으세요?ㅠ",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040950.1.2",
          "form": "항상 있는 건 아닌데",
          "original_form": "항상 있는 건 아닌데",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040950.1.3",
          "form": "가끔씩 귀에",
          "original_form": "가끔씩 귀에",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040950.1.4",
          "form": "뾰루지같은게 올라오거든요?!",
          "original_form": "뾰루지같은게 올라오거든요?!",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040950.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.6",
          "form": "근데 이게 염증성 체질이라 그렇다 하더라구요",
          "original_form": "근데 이게 염증성 체질이라 그렇다 하더라구요",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.7",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.8",
          "form": "귀 뚫고 싶은데",
          "original_form": "귀 뚫고 싶은데",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.9",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.10",
          "form": "이거 때문에 고생할까봐요ㅠㅠㅠ",
          "original_form": "이거 때문에 고생할까봐요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.11",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.12",
          "form": "ㅠㅠㅠ 그럼 걱정되겠어요 진짜",
          "original_form": "ㅠㅠㅠ 그럼 걱정되겠어요 진짜",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.13",
          "form": "인바디 기능 괜찮대요 어차피 참고 정도 하는거고",
          "original_form": "인바디 기능 괜찮대요 어차피 참고 정도 하는거고",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100040950.1.14",
          "form": "대신 같은 시간에 같은 부위 같은자세로 하면 된대요",
          "original_form": "대신 같은 시간에 같은 부위 같은자세로 하면 된대요",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040950.1.15",
          "form": "그럼 관찰하기에는 충분하다고 하더라구요",
          "original_form": "그럼 관찰하기에는 충분하다고 하더라구요",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040950.1.16",
          "form": "사시는거 추천입니다",
          "original_form": "사시는거 추천입니다",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100040950.1.17",
          "form": "우와우와",
          "original_form": "우와우와",
          "speaker_id": "2",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100040950.1.18",
          "form": "name1님 진짜 아는 거 많으시네요...",
          "original_form": "&name1&님 진짜 아는 거 많으시네요...",
          "speaker_id": "2",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100040950.1.19",
          "form": "멋져요!!! 좋은 정보 감사해요!!",
          "original_form": "멋져요!!! 좋은 정보 감사해요!!",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100040950.1.20",
          "form": "아잇 아니네요ㅠ ㅋㅋㅋ 감사합니당",
          "original_form": "아잇 아니네요ㅠ ㅋㅋㅋ 감사합니당",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040950.1.21",
          "form": "그럼 name2님 운동도 하시나요!",
          "original_form": "그럼 &name2&님 운동도 하시나요!",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100040950.1.22",
          "form": "네네 저 지난주부터",
          "original_form": "네네 저 지난주부터",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100040950.1.23",
          "form": "필라테스 다니고 있어요!!!",
          "original_form": "필라테스 다니고 있어요!!!",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100040950.1.24",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100040950.1.25",
          "form": "헐헐 대박 운동하시는 분이셨군요",
          "original_form": "헐헐 대박 운동하시는 분이셨군요",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100040950.1.26",
          "form": "저는 게으름뱅이에옹..ㅋㅋㅋㅋㅋ",
          "original_form": "저는 게으름뱅이에옹..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100040950.1.27",
          "form": "앜ㅋㅋ 운동하는 사람이라기엔",
          "original_form": "앜ㅋㅋ 운동하는 사람이라기엔",
          "speaker_id": "2",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100040950.1.28",
          "form": "이제 겨우 2회만 가서,, 쭈뼛",
          "original_form": "이제 겨우 2회만 가서,, 쭈뼛",
          "speaker_id": "2",
          "time": "20210905 14:16"
        },
        {
          "id": "MDRW2100040950.1.29",
          "form": "ㅋㅋ어때요? 재밌나요?",
          "original_form": "ㅋㅋ어때요? 재밌나요?",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100040950.1.30",
          "form": "뭔가 재미를 느끼기 전에 고통이 큰데",
          "original_form": "뭔가 재미를 느끼기 전에 고통이 큰데",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100040950.1.31",
          "form": "강사님한테 어쩌다 칭찬 받으면",
          "original_form": "강사님한테 어쩌다 칭찬 받으면",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100040950.1.32",
          "form": "그렇게 기분이 좋을 수 없어요 ㅋㅋㅋㅋ",
          "original_form": "그렇게 기분이 좋을 수 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100040950.1.33",
          "form": "ㅋㅋㅋㅋㅋ 칭찬은 만능인것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 칭찬은 만능인것 같아요",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100040950.1.34",
          "form": "일주일에 몇번 가시는건가요?",
          "original_form": "일주일에 몇번 가시는건가요?",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100040950.1.35",
          "form": "11월까지 25회 가는 건데",
          "original_form": "11월까지 25회 가는 건데",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100040950.1.36",
          "form": "제가 스케줄이랑 컨디션 봐가면서 가거든요!",
          "original_form": "제가 스케줄이랑 컨디션 봐가면서 가거든요!",
          "speaker_id": "2",
