      "utterance": [
        {
          "id": "MDRW2100017100.1.1",
          "form": "꽃게탕 조리법은 어디서 보고 하실거에요?",
          "original_form": "꽃게탕 조리법은 어디서 보고 하실거에요?",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017100.1.2",
          "form": "네이버에서요 ㅋ 봤는데 저는 다른재료는 없어서.. 된장풀고 고추장 조금 첨가하는 레시피가 있길래 \r\n마늘 파 넣고 그렇게 끓여보려구요",
          "original_form": "네이버에서요 ㅋ 봤는데 저는 다른재료는 없어서.. 된장풀고 고추장 조금 첨가하는 레시피가 있길래 \r\n마늘 파 넣고 그렇게 끓여보려구요",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017100.1.3",
          "form": "그렇게만 넣으면 되나요?",
          "original_form": "그렇게만 넣으면 되나요?",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017100.1.4",
          "form": "그렇게 대충 된장국처럼 끓여보려구요 ㅋ",
          "original_form": "그렇게 대충 된장국처럼 끓여보려구요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017100.1.5",
          "form": "오~생각보다 간단하네요~!",
          "original_form": "오~생각보다 간단하네요~!",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017100.1.6",
          "form": "버섯도 콩나물도 숙주도 양파도 없네요",
          "original_form": "버섯도 콩나물도 숙주도 양파도 없네요",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017100.1.7",
          "form": "그런건 안사고 맨날 과자 빵만 사놔서",
          "original_form": "그런건 안사고 맨날 과자 빵만 사놔서",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017100.1.8",
          "form": "아~그런것도 곁들이면 더 맛있겠어요",
          "original_form": "아~그런것도 곁들이면 더 맛있겠어요",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017100.1.9",
          "form": "된장찌개에는 뭐뭐넣고 끓이셔요?",
          "original_form": "된장찌개에는 뭐뭐넣고 끓이셔요?",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017100.1.10",
          "form": "감자 버섯 두부 양파 이렇게요",
          "original_form": "감자 버섯 두부 양파 이렇게요",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017100.1.11",
          "form": "아하 감자~^^ 좋은 팁 감사해요",
          "original_form": "아하 감자~^^ 좋은 팁 감사해요",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017100.1.12",
          "form": "집에 감자가 있는데 잊고 있었어요",
          "original_form": "집에 감자가 있는데 잊고 있었어요",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017100.1.13",
          "form": "아~있으시면 감자도 넣으면 좋겠네요",
          "original_form": "아~있으시면 감자도 넣으면 좋겠네요",
