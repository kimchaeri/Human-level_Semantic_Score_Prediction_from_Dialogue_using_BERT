      "utterance": [
        {
          "id": "MDRW2100022283.1.1",
          "form": "응응 좋지?",
          "original_form": "응응 좋지?",
          "speaker_id": "2",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100022283.1.2",
          "form": "근데 그게 끝이 아니더라..ㅠ?",
          "original_form": "근데 그게 끝이 아니더라..ㅠ?",
          "speaker_id": "1",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100022283.1.3",
          "form": "엥 무슨 소리야?",
          "original_form": "엥 무슨 소리야?",
          "speaker_id": "2",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100022283.1.4",
          "form": "추가로 사야하는게 엄청 많더라...",
          "original_form": "추가로 사야하는게 엄청 많더라...",
          "speaker_id": "1",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100022283.1.5",
          "form": "케이스나 매직키보드 이런거?",
          "original_form": "케이스나 매직키보드 이런거?",
          "speaker_id": "2",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100022283.1.6",
          "form": "액정필름이랑 케이스도 그렇고",
          "original_form": "액정필름이랑 케이스도 그렇고",
          "speaker_id": "1",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100022283.1.7",
          "form": "또 사야되는게 있어?",
          "original_form": "또 사야되는게 있어?",
          "speaker_id": "2",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022283.1.8",
          "form": "그림그리려니까 거치대에 단선방지용 케이블도 사고 하니까",
          "original_form": "그림그리려니까 거치대에 단선방지용 케이블도 사고 하니까",
          "speaker_id": "1",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022283.1.9",
          "form": "단선방지용 케이블이 뭐야?",
          "original_form": "단선방지용 케이블이 뭐야?",
          "speaker_id": "2",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022283.1.10",
          "form": "거의 15만원쯤 나오더라구",
          "original_form": "거의 15만원쯤 나오더라구",
          "speaker_id": "1",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022283.1.11",
          "form": "아 그거 아이폰이나 아이패드 충전선",
          "original_form": "아 그거 아이폰이나 아이패드 충전선",
          "speaker_id": "1",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022283.1.12",
          "form": "아아 그거 알아 나도 있어",
          "original_form": "아아 그거 알아 나도 있어",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022283.1.13",
          "form": "엄청 약하잖아 그거 안끊어지게 하는 보호 선",
          "original_form": "엄청 약하잖아 그거 안끊어지게 하는 보호 선",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022283.1.14",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022283.1.15",
          "form": "그니까 그런거 다 사니까 추가지출이 어마어마해ㅠㅠ",
          "original_form": "그니까 그런거 다 사니까 추가지출이 어마어마해ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022283.1.16",
