      "utterance": [
        {
          "id": "MDRW2100043119.1.1",
          "form": "학생이신가요",
          "original_form": "학생이신가요",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043119.1.2",
          "form": "아뇨 음.. 발령대기중인 사회초년생?ㅋㅋㅋ",
          "original_form": "아뇨 음.. 발령대기중인 사회초년생?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100043119.1.3",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "2",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100043119.1.4",
          "form": "ㅎㅎ저는졸업한지2년됬어요",
          "original_form": "ㅎㅎ저는졸업한지2년됬어요",
          "speaker_id": "1",
          "time": "20210907 14:37"
        },
        {
          "id": "MDRW2100043119.1.5",
          "form": "같이과거를더듬어볼까요",
          "original_form": "같이과거를더듬어볼까요",
          "speaker_id": "1",
          "time": "20210907 14:37"
        },
        {
          "id": "MDRW2100043119.1.6",
          "form": "아 저는 대학 중퇴랔ㅋㅋㅋㅋ 대학경험은 거의 없어요...",
          "original_form": "아 저는 대학 중퇴랔ㅋㅋㅋㅋ 대학경험은 거의 없어요...",
          "speaker_id": "2",
          "time": "20210907 14:38"
        },
        {
          "id": "MDRW2100043119.1.7",
          "form": "어떤 전공하셨어요?",
          "original_form": "어떤 전공하셨어요?",
          "speaker_id": "2",
          "time": "20210907 14:38"
        },
        {
          "id": "MDRW2100043119.1.8",
          "form": "앗그러시군요",
          "original_form": "앗그러시군요",
          "speaker_id": "1",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043119.1.9",
          "form": "저는보건계열이요ㅎㅎ",
          "original_form": "저는보건계열이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043119.1.10",
          "form": "와우 공부진짜 많이하셨겠어요.",
          "original_form": "와우 공부진짜 많이하셨겠어요.",
          "speaker_id": "2",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100043119.1.11",
          "form": "해부..? 그런 실습도 하셨나요?",
          "original_form": "해부..? 그런 실습도 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100043119.1.12",
          "form": "ㅎㅎㅎ했죠",
          "original_form": "ㅎㅎㅎ했죠",
          "speaker_id": "1",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100043119.1.13",
          "form": "해부교수님이빡셌던기억이있네요",
          "original_form": "해부교수님이빡셌던기억이있네요",
          "speaker_id": "1",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100043119.1.14",
          "form": "시험점수에 엄격하셨나요 ㄷㄷ",
          "original_form": "시험점수에 엄격하셨나요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043119.1.15",
          "form": "태도와시험다..ㅎㄷㄷ",
          "original_form": "태도와시험다..ㅎㄷㄷ",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043119.1.16",
          "form": "어이고.. 해부 자체로도 이미 고단할거 같은데 더 힘드셨겠어요 ㄷㄷ",
          "original_form": "어이고.. 해부 자체로도 이미 고단할거 같은데 더 힘드셨겠어요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043119.1.17",
          "form": "가장 흥미로웠던 수업은 뭐에요?",
          "original_form": "가장 흥미로웠던 수업은 뭐에요?",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043119.1.18",
          "form": "ㅎㅎ저는",
          "original_form": "ㅎㅎ저는",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043119.1.19",
          "form": "으음..",
          "original_form": "으음..",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043119.1.20",
          "form": "ㅋㅋㅋㅋ재활의학 재밌었어요",
          "original_form": "ㅋㅋㅋㅋ재활의학 재밌었어요",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043119.1.21",
          "form": "재활의학에선 어떤거를 배우나요?",
          "original_form": "재활의학에선 어떤거를 배우나요?",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043119.1.22",
          "form": "근육을다외우는데",
          "original_form": "근육을다외우는데",
          "speaker_id": "1",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100043119.1.23",
          "form": "눈에보여서좋더라고요ㅎㅎ",
          "original_form": "눈에보여서좋더라고요ㅎㅎ",
