      "utterance": [
        {
          "id": "MDRW2100028659.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028659.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028659.1.3",
          "form": "오늘 계신 곳에 비가 왔나요?",
          "original_form": "오늘 계신 곳에 비가 왔나요?",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028659.1.4",
          "form": "오늘은 비가 안온거같아요",
          "original_form": "오늘은 비가 안온거같아요",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028659.1.5",
          "form": "어제까지 비오고 난리도아니었어요",
          "original_form": "어제까지 비오고 난리도아니었어요",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028659.1.6",
          "form": "어제는 여기도 많이 왔어요",
          "original_form": "어제는 여기도 많이 왔어요",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028659.1.7",
          "form": "새벽되면 좀 추워진거 같아요ㅎ",
          "original_form": "새벽되면 좀 추워진거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028659.1.8",
          "form": "맞아요 계속 축축해서 에어컨 켜도 뭔가 찌뿌둥하고",
          "original_form": "맞아요 계속 축축해서 에어컨 켜도 뭔가 찌뿌둥하고",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028659.1.9",
          "form": "좀 선선해져서 좋네요",
          "original_form": "좀 선선해져서 좋네요",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028659.1.10",
          "form": "네 어제만 해도 습했는데 오늘은 시원하네요",
          "original_form": "네 어제만 해도 습했는데 오늘은 시원하네요",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028659.1.11",
          "form": "나가서 산책하기 좋은 날씨에요",
          "original_form": "나가서 산책하기 좋은 날씨에요",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028659.1.12",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028659.1.13",
          "form": "얼른 가을이되면 좋겠어요",
          "original_form": "얼른 가을이되면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028659.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028659.1.15",
          "form": "요즘 미세먼지는 없어서 좋네요",
          "original_form": "요즘 미세먼지는 없어서 좋네요",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028659.1.16",
          "form": "어떤계절 좋아하세요?",
          "original_form": "어떤계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028659.1.17",
          "form": "저는 겨울 좋아합니다",
          "original_form": "저는 겨울 좋아합니다",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028659.1.18",
          "form": "해가 짧은걸 좋아해요",
          "original_form": "해가 짧은걸 좋아해요",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028659.1.19",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028659.1.20",
          "form": "ㅋㅋㅋ왜 해가짧은게 좋으세요?",
          "original_form": "ㅋㅋㅋ왜 해가짧은게 좋으세요?",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028659.1.21",
          "form": "저도 여름보다는 겨울이좋은거같아요",
          "original_form": "저도 여름보다는 겨울이좋은거같아요",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100028659.1.22",
          "form": "조용한 추운 겨운 밤이 좋더라구요ㅎㅎ",
          "original_form": "조용한 추운 겨운 밤이 좋더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028659.1.23",
          "form": "이불 밖은 비록 추워서 힘들지만 \r\nㅎ",
          "original_form": "이불 밖은 비록 추워서 힘들지만 \r\nㅎ",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028659.1.24",
          "form": "아..알거같아요",
          "original_form": "아..알거같아요",
          "speaker_id": "2",
          "time": "20210902 21:38"
        }
