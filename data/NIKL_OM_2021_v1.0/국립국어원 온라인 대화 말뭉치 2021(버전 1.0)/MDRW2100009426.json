      "utterance": [
        {
          "id": "MDRW2100009426.1.1",
          "form": "안녕하세요 대학생이세요?",
          "original_form": "안녕하세요 대학생이세요?",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009426.1.2",
          "form": "아뇨 졸업햇죠 ㅎㅎ학생이세요?",
          "original_form": "아뇨 졸업햇죠 ㅎㅎ학생이세요?",
          "speaker_id": "2",
          "time": "20210823 08:38"
        },
        {
          "id": "MDRW2100009426.1.3",
          "form": "아뇨 저도 졸업했어요",
          "original_form": "아뇨 저도 졸업했어요",
          "speaker_id": "1",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009426.1.4",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "2",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009426.1.5",
          "form": "아뇨 유튜버에요 ㅎㅎ 직장다니세요?",
          "original_form": "아뇨 유튜버에요 ㅎㅎ 직장다니세요?",
          "speaker_id": "1",
          "time": "20210823 09:38"
        },
        {
          "id": "MDRW2100009426.1.6",
          "form": "오 유투브 잘되세용?",
          "original_form": "오 유투브 잘되세용?",
          "speaker_id": "2",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009426.1.7",
          "form": "넹 전 치위생과나와서 치과다녀용",
          "original_form": "넹 전 치위생과나와서 치과다녀용",
          "speaker_id": "2",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009426.1.8",
          "form": "잘되진않아요 ㅜㅜ",
          "original_form": "잘되진않아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009426.1.9",
          "form": "치위생사시군요",
          "original_form": "치위생사시군요",
          "speaker_id": "1",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009426.1.10",
          "form": "저도 도전해볼까했는데 어렵더라구용 ㅠ",
          "original_form": "저도 도전해볼까했는데 어렵더라구용 ㅠ",
          "speaker_id": "2",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100009426.1.11",
          "form": "치위생과 학창생활은 어때요? 재밌어요?",
          "original_form": "치위생과 학창생활은 어때요? 재밌어요?",
          "speaker_id": "1",
          "time": "20210823 12:39"
        },
        {
          "id": "MDRW2100009426.1.12",
          "form": "아뇨 ㅋㅋ 저희는 대학때 선배들이 군기잡아서 진짜 재미없게다녔어요",
          "original_form": "아뇨 ㅋㅋ 저희는 대학때 선배들이 군기잡아서 진짜 재미없게다녔어요",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009426.1.13",
          "form": "학생때 재밌으셨어요?",
          "original_form": "학생때 재밌으셨어요?",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009426.1.14",
          "form": "아뇨 저도 별로 재미없었어요",
          "original_form": "아뇨 저도 별로 재미없었어요",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100009426.1.15",
          "form": "과제도 엄청많아서",
          "original_form": "과제도 엄청많아서",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100009426.1.16",
          "form": "ㅋㅋㅋ짜증나서 잘안하고 땡땡이치고했네요",
          "original_form": "ㅋㅋㅋ짜증나서 잘안하고 땡땡이치고했네요",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100009426.1.17",
          "form": "과제가 많군요.. 치위생과 나온 분이랑 첨 얘기해봐요",
          "original_form": "과제가 많군요.. 치위생과 나온 분이랑 첨 얘기해봐요",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100009426.1.18",
          "form": "요즘에 학교생활 소재로 한 웹툰 많던데요",
          "original_form": "요즘에 학교생활 소재로 한 웹툰 많던데요",
          "speaker_id": "1",
          "time": "20210823 13:47"
        }
      ]
