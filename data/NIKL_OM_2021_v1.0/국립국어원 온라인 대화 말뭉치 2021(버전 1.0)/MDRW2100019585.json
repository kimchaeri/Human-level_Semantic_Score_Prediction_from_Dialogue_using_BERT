      "utterance": [
        {
          "id": "MDRW2100019585.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100019585.1.2",
          "form": "쇼핑 좋아하는 편이에요?",
          "original_form": "쇼핑 좋아하는 편이에요?",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100019585.1.3",
          "form": "네 쇼핑 좋아해요!",
          "original_form": "네 쇼핑 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019585.1.4",
          "form": "최근에 어떤거 샀어요?",
          "original_form": "최근에 어떤거 샀어요?",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100019585.1.5",
          "form": "컵밥샀어요",
          "original_form": "컵밥샀어요",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100019585.1.6",
          "form": "컵밥 ㅎㅎㅎ",
          "original_form": "컵밥 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100019585.1.7",
          "form": "무슨 컵밥이용?",
          "original_form": "무슨 컵밥이용?",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100019585.1.8",
          "form": "낙지콩나물비빔밥이용",
          "original_form": "낙지콩나물비빔밥이용",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100019585.1.9",
          "form": "어? 그거 그 안에 햇반 들어있는거 맞죠?",
          "original_form": "어? 그거 그 안에 햇반 들어있는거 맞죠?",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100019585.1.10",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100019585.1.11",
          "form": "ㅎㅎ 몇개 샀어요?",
          "original_form": "ㅎㅎ 몇개 샀어요?",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100019585.1.12",
          "form": "18개 한박스 샀네요ㅎㅎ 자취생이라...",
          "original_form": "18개 한박스 샀네요ㅎㅎ 자취생이라...",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100019585.1.13",
          "form": "와 ㅋㅋ 컵밥 종류가 다양하던데 그게 제일 나아요?",
          "original_form": "와 ㅋㅋ 컵밥 종류가 다양하던데 그게 제일 나아요?",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100019585.1.14",
          "form": "저 예전에 뭐 하나 먹고 양도 적고 그래서 실망했거든요ㅠ",
          "original_form": "저 예전에 뭐 하나 먹고 양도 적고 그래서 실망했거든요ㅠ",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100019585.1.15",
          "form": "가격이 제일 싸서 사봤는데 맛있어서 다행이었어요",
          "original_form": "가격이 제일 싸서 사봤는데 맛있어서 다행이었어요",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100019585.1.16",
          "form": "얼마 주고 샀어요 한개당?",
          "original_form": "얼마 주고 샀어요 한개당?",
          "speaker_id": "1",
          "time": "20210828 15:14"
        }
      ]
