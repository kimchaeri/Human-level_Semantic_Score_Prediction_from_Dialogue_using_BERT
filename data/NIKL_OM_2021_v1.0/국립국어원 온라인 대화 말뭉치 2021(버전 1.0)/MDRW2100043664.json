      "utterance": [
        {
          "id": "MDRW2100043664.1.1",
          "form": "저는드립은하는데",
          "original_form": "저는드립은하는데",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043664.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043664.1.3",
          "form": "갈때귀찮드라고요",
          "original_form": "갈때귀찮드라고요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043664.1.4",
          "form": "ㅇㅎㅠㅠ",
          "original_form": "ㅇㅎㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043664.1.5",
          "form": "저는 카페 알바나 그런걸 해본적이없어서",
          "original_form": "저는 카페 알바나 그런걸 해본적이없어서",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043664.1.6",
          "form": "저도용ㅎㅎ",
          "original_form": "저도용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043664.1.7",
          "form": "숙달되신분은 뚝딱뚝딱 하시던데",
          "original_form": "숙달되신분은 뚝딱뚝딱 하시던데",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043664.1.8",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043664.1.9",
          "form": "그러게요ㅎㅎ",
          "original_form": "그러게요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043664.1.10",
          "form": "캡슐은어떠세요",
          "original_form": "캡슐은어떠세요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043664.1.11",
          "form": "기계를 사야하구",
          "original_form": "기계를 사야하구",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043664.1.12",
          "form": "설거지가 귀찮아서....ㅎㅎ",
          "original_form": "설거지가 귀찮아서....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043664.1.13",
          "form": "제가 긱사살아서요!!",
          "original_form": "제가 긱사살아서요!!",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043664.1.14",
          "form": "아하ㅜ",
          "original_form": "아하ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043664.1.15",
          "form": "대학생이세요",
          "original_form": "대학생이세요",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043664.1.16",
          "form": "네네 대학생이에요",
          "original_form": "네네 대학생이에요",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043664.1.17",
          "form": "그래서 사먹거나",
          "original_form": "그래서 사먹거나",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043664.1.18",
          "form": "설거지거리가 없는 음식을 많이 먹는답니다",
          "original_form": "설거지거리가 없는 음식을 많이 먹는답니다",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043664.1.19",
          "form": "ㅎㅎㅎ어떤게있나요",
          "original_form": "ㅎㅎㅎ어떤게있나요",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043664.1.20",
          "form": "식당가서 먹거나",
          "original_form": "식당가서 먹거나",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043664.1.21",
          "form": "배달시키거나",
          "original_form": "배달시키거나",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043664.1.22",
          "form": "테이크아웃 ㅋㅋㅋ",
          "original_form": "테이크아웃 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
