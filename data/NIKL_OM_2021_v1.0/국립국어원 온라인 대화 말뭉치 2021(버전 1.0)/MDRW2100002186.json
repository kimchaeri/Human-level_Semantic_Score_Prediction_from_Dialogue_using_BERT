        {
          "id": "MDRW2100002186.1.1",
          "form": "요즘 산거 있어?",
          "original_form": "요즘 산거 있어?",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002186.1.2",
          "form": "요즘 육아용품ㅋ",
          "original_form": "요즘 육아용품ㅋ",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002186.1.3",
          "form": "육아용품을 사는구나 애기가 몇살이야?",
          "original_form": "육아용품을 사는구나 애기가 몇살이야?",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002186.1.4",
          "form": "이제 19개월",
          "original_form": "이제 19개월",
          "speaker_id": "2",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002186.1.5",
          "form": "그렇구나 육아용품은 뭐뭐사야해 기저귀같은거?",
          "original_form": "그렇구나 육아용품은 뭐뭐사야해 기저귀같은거?",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002186.1.6",
          "form": "육아용품은 끝이없어",
          "original_form": "육아용품은 끝이없어",
          "speaker_id": "2",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002186.1.7",
          "form": "그렇구나 ㅠㅠ 고생이 많다",
          "original_form": "그렇구나 ㅠㅠ 고생이 많다",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002186.1.8",
          "form": "그냥 다 사야해ㅋㄲㅋ",
          "original_form": "그냥 다 사야해ㅋㄲㅋ",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002186.1.9",
          "form": "ㅠㅠ 물려줄 사람 없어?",
          "original_form": "ㅠㅠ 물려줄 사람 없어?",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002186.1.10",
          "form": "둘째도 곧이라ㅋ그대로 써야해",
          "original_form": "둘째도 곧이라ㅋ그대로 써야해",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002186.1.11",
          "form": "그렇구나 축하해 ㅋㅋㅋㅋㅋ",
          "original_form": "그렇구나 축하해 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002186.1.12",
          "form": "ㅋㅋ고마워ㅋㅋ살게 태산이라",
          "original_form": "ㅋㅋ고마워ㅋㅋ살게 태산이라",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002186.1.13",
          "form": "그렇겠지 ㅠㅠ 집앞에 이마트만 가도 애기들먹는거 입히는거 재우는거 다 따로 팔더라고 ㅠㅠ",
          "original_form": "그렇겠지 ㅠㅠ 집앞에 이마트만 가도 애기들먹는거 입히는거 재우는거 다 따로 팔더라고 ㅠㅠ",
