      "utterance": [
        {
          "id": "MDRW2100000547.1.1",
          "form": "반려동물키우시나요?",
          "original_form": "반려동물키우시나요?",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000547.1.2",
          "form": "저는 집사지망생이요!ㅠ",
          "original_form": "저는 집사지망생이요!ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000547.1.3",
          "form": "아하 ㅋㅋㅋㅋ전 집사입니당",
          "original_form": "아하 ㅋㅋㅋㅋ전 집사입니당",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000547.1.4",
          "form": "코숏어쩌다보니 간택당해서",
          "original_form": "코숏어쩌다보니 간택당해서",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000547.1.5",
          "form": "지금5년째 키우고있어요 궁금하신거 물어보십셔",
          "original_form": "지금5년째 키우고있어요 궁금하신거 물어보십셔",
          "speaker_id": "2",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000547.1.6",
          "form": "헐.. 부러우ㅏ요",
          "original_form": "헐.. 부러우ㅏ요",
          "speaker_id": "1",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000547.1.7",
          "form": "코숏..!",
          "original_form": "코숏..!",
          "speaker_id": "1",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000547.1.8",
          "form": "길에서 간택 당하신거예요?",
          "original_form": "길에서 간택 당하신거예요?",
          "speaker_id": "1",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000547.1.9",
          "form": "네 제가 집에없었을때 동생이 아픈냥이를 데리고와서 치료해주고 그러다가 키우게됐어요",
          "original_form": "네 제가 집에없었을때 동생이 아픈냥이를 데리고와서 치료해주고 그러다가 키우게됐어요",
          "speaker_id": "2",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000547.1.10",
          "form": "1년만에 집에오니  고양이가! ㅋㅋㅋㅋㅋ",
          "original_form": "1년만에 집에오니  고양이가! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000547.1.11",
          "form": "ㅋㅋ진짜 부러워요",
          "original_form": "ㅋㅋ진짜 부러워요",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000547.1.12",
          "form": "강아지는 멍때리면서 산책하는 거 볼 수 있지만 냥이들은 쉽지 않아서!",
          "original_form": "강아지는 멍때리면서 산책하는 거 볼 수 있지만 냥이들은 쉽지 않아서!",
          "speaker_id": "1",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000547.1.13",
          "form": "맞아요 길에서가끔봐도 경계심심한애들은 도망가기바쁘니까요",
          "original_form": "맞아요 길에서가끔봐도 경계심심한애들은 도망가기바쁘니까요",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000547.1.14",
          "form": "애들이 달려듭니당",
          "original_form": "애들이 달려듭니당",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000547.1.15",
          "form": "고양이는 처음에 키울 때 경계심하죠?",
          "original_form": "고양이는 처음에 키울 때 경계심하죠?",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000547.1.16",
          "form": "네 근데 아예 애기이면 경계도없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 근데 아예 애기이면 경계도없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000547.1.17",
          "form": "오히려 다 크니까 자기 영역에대해 확고해지면 경계심이생기는것같아요",
          "original_form": "오히려 다 크니까 자기 영역에대해 확고해지면 경계심이생기는것같아요",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000547.1.18",
          "form": "외부사람들오면 아주난리난리",
          "original_form": "외부사람들오면 아주난리난리",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000547.1.19",
          "form": "오.. 그렇군요.",
          "original_form": "오.. 그렇군요.",
          "speaker_id": "1",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000547.1.20",
          "form": "산책냥이는 거의 없죠?",
          "original_form": "산책냥이는 거의 없죠?",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000547.1.21",
          "form": "네 특히 코숏은...밖에서태어난 스트릿출신냥이들은 산책시키면",
          "original_form": "네 특히 코숏은...밖에서태어난 스트릿출신냥이들은 산책시키면",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000547.1.22",
          "form": "난리납니다ㅜ",
          "original_form": "난리납니다ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000547.1.23",
          "form": "산책이아니라 영역탐방이되는데 그럼 매일 자기영역둘러보러 나가야되고 그러다가 애 잃어버렸다",
          "original_form": "산책이아니라 영역탐방이되는데 그럼 매일 자기영역둘러보러 나가야되고 그러다가 애 잃어버렸다",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000547.1.24",
          "form": "잃어버려요",
          "original_form": "잃어버려요",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000547.1.25",
          "form": "그리고 리드줄같은것도 고양이들 유연하기도하고 싫어해서 잘빠지거든요",
          "original_form": "그리고 리드줄같은것도 고양이들 유연하기도하고 싫어해서 잘빠지거든요",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000547.1.26",
          "form": "새로운곳의 새로운자극에대한 스트레스도 많고요",
          "original_form": "새로운곳의 새로운자극에대한 스트레스도 많고요",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000547.1.27",
          "form": "동병갈때도 난리나는데ㅜㅜ",
          "original_form": "동병갈때도 난리나는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000547.1.28",
          "form": "어이쿠..ㅠ",
          "original_form": "어이쿠..ㅠ",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000547.1.29",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000547.1.30",
          "form": "오히려 스트릿출신냥이가 산책에 취약하군요",
          "original_form": "오히려 스트릿출신냥이가 산책에 취약하군요",
          "speaker_id": "1",
          "time": "20210819 17:14"
        }
      ]
    }
