      "utterance": [
        {
          "id": "MDRW2100043249.1.1",
          "form": "요즘 프로그램 뭐보세요?",
          "original_form": "요즘 프로그램 뭐보세요?",
          "speaker_id": "2",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043249.1.2",
          "form": "놀뭐랑 대탈출 맛녀석 요렇게 주기적으로 보는거 같아요 ㅋㅋ",
          "original_form": "놀뭐랑 대탈출 맛녀석 요렇게 주기적으로 보는거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:17"
        },
        {
          "id": "MDRW2100043249.1.3",
          "form": "맛녀석ㅋㅋㅋㅋㅋ저도 참 좋아해요",
          "original_form": "맛녀석ㅋㅋㅋㅋㅋ저도 참 좋아해요",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043249.1.4",
          "form": "밥친구밥친구 ㅋㅋㅋㅋㅋ",
          "original_form": "밥친구밥친구 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043249.1.5",
          "form": "네 저도 밥먹을때 주로 봐요 ㅋㅋ",
          "original_form": "네 저도 밥먹을때 주로 봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043249.1.6",
          "form": "드라마나 영화는 안보시나요?",
          "original_form": "드라마나 영화는 안보시나요?",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043249.1.7",
          "form": "드라마 영화 다 좋죠 요즘 방영하는건 안보고 오티티에 있는거 보고 있어요 ㅋㅋ",
          "original_form": "드라마 영화 다 좋죠 요즘 방영하는건 안보고 오티티에 있는거 보고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100043249.1.8",
          "form": "저도 오티티 ㅋㅋㅋㅋ",
          "original_form": "저도 오티티 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:02"
        },
        {
          "id": "MDRW2100043249.1.9",
          "form": "요즘 오티티가 짱이죠",
          "original_form": "요즘 오티티가 짱이죠",
          "speaker_id": "2",
          "time": "20210907 17:02"
        },
        {
          "id": "MDRW2100043249.1.10",
          "form": "저는 최근에 알고잇지만이랑 디피 다 보고 지금 굿걸스 보고잇어요",
          "original_form": "저는 최근에 알고잇지만이랑 디피 다 보고 지금 굿걸스 보고잇어요",
          "speaker_id": "2",
          "time": "20210907 17:02"
        },
        {
          "id": "MDRW2100043249.1.11",
          "form": "굿걸스 첨 들어요! 로맨스물인가요?",
          "original_form": "굿걸스 첨 들어요! 로맨스물인가요?",
          "speaker_id": "1",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100043249.1.12",
          "form": "아니요 미국드라만데",
          "original_form": "아니요 미국드라만데",
          "speaker_id": "2",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100043249.1.13",
          "form": "세 친구들이 돈 벌기위해 마트를 털었다가",
          "original_form": "세 친구들이 돈 벌기위해 마트를 털었다가",
          "speaker_id": "2",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100043249.1.14",
          "form": "갱단에 연류되어 겉잡을 수 없이 일이 커지게되는",
          "original_form": "갱단에 연류되어 겉잡을 수 없이 일이 커지게되는",
          "speaker_id": "2",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100043249.1.15",
          "form": "우당탕탕 어쩌고..",
          "original_form": "우당탕탕 어쩌고..",
          "speaker_id": "2",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100043249.1.16",
          "form": "ㅋㅋㅋㅋㅋ재밌어욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ재밌어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100043249.1.17",
          "form": "아 그거 알거같아요! 인터넷에서 그 갱단 남자",
          "original_form": "아 그거 알거같아요! 인터넷에서 그 갱단 남자",
          "speaker_id": "1",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043249.1.18",
          "form": "맞아요핫가이라고",
          "original_form": "맞아요핫가이라고",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043249.1.19",
          "form": "ㅋㅋㅋㅋㅋ그 날개타투잇고",
          "original_form": "ㅋㅋㅋㅋㅋ그 날개타투잇고",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043249.1.20",
          "form": "빡빡이",
          "original_form": "빡빡이",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043249.1.21",
          "form": "네네 핫가이!!!",
          "original_form": "네네 핫가이!!!",
          "speaker_id": "1",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043249.1.22",
          "form": "아니 근데 어이없는게 드라마보면",
          "original_form": "아니 근데 어이없는게 드라마보면",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043249.1.23",
          "form": "진짜 핫해요;",
          "original_form": "진짜 핫해요;",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043249.1.24",
          "form": "어이없어;",
          "original_form": "어이없어;",
          "speaker_id": "2",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043249.1.25",
          "form": "나 저런 취향 좋아하나;",
          "original_form": "나 저런 취향 좋아하나;",
          "speaker_id": "2",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100043249.1.26",
          "form": "ㅋㅋㅋ 전에 댓글보면 다 그런 반응이었어요",
          "original_form": "ㅋㅋㅋ 전에 댓글보면 다 그런 반응이었어요",
          "speaker_id": "1",
          "time": "20210907 17:06"
        },
        {
          "id": "MDRW2100043249.1.27",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210907 17:06"
        }
      ]
    }
  ]
