      "utterance": [
        {
          "id": "MDRW2100031324.1.1",
          "form": "어제 간만에 맥주 한 캔 마셨넹",
          "original_form": "어제 간만에 맥주 한 캔 마셨넹",
          "speaker_id": "2",
          "time": "20210904 20:32"
        },
        {
          "id": "MDRW2100031324.1.2",
          "form": "혼술했니 안주는 뭐였니",
          "original_form": "혼술했니 안주는 뭐였니",
          "speaker_id": "1",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031324.1.3",
          "form": "반건조 오징어 불닭마요에 찍어먹었단다",
          "original_form": "반건조 오징어 불닭마요에 찍어먹었단다",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031324.1.4",
          "form": "반건조 오징어 어디서 샀어!",
          "original_form": "반건조 오징어 어디서 샀어!",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031324.1.5",
          "form": "내가 공구하자할땐 맨날 까더니!!",
          "original_form": "내가 공구하자할땐 맨날 까더니!!",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031324.1.6",
          "form": "죽도시장...",
          "original_form": "죽도시장...",
          "speaker_id": "2",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031324.1.7",
          "form": "언니가 사고싶대서 엄마가 사주고 몇장 받았다 ㅋㅋㅋ",
          "original_form": "언니가 사고싶대서 엄마가 사주고 몇장 받았다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031324.1.8",
          "form": "몇마리에 얼마주고 샀니",
          "original_form": "몇마리에 얼마주고 샀니",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031324.1.9",
          "form": "기억 안남....",
          "original_form": "기억 안남....",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031324.1.10",
          "form": "근데 난 걍 그랬어 굳이 안 사먹...",
          "original_form": "근데 난 걍 그랬어 굳이 안 사먹...",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031324.1.11",
          "form": "...그래? 그래도 먹고싶은디....",
          "original_form": "...그래? 그래도 먹고싶은디....",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100031324.1.12",
          "form": "재난지원금 받음 죽도시장서 사볼까ㅋㅋ",
          "original_form": "재난지원금 받음 죽도시장서 사볼까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100031324.1.13",
          "form": "아 타시도에선 못쓰나?",
          "original_form": "아 타시도에선 못쓰나?",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100031324.1.14",
          "form": "경주시만 되는걸로 안다 ㅋㅋ",
          "original_form": "경주시만 되는걸로 안다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100031324.1.15",
          "form": "에잇.....",
          "original_form": "에잇.....",
          "speaker_id": "1",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100031324.1.16",
          "form": "아마도 ㅋㅋㅋ 나도 포항서 쓰고싶은데",
          "original_form": "아마도 ㅋㅋㅋ 나도 포항서 쓰고싶은데",
          "speaker_id": "2",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031324.1.17",
          "form": "하...경주에도 건어물 가게가 있긴하겠지만 포항죽도시장서 사고싶은디ㅋㅋㅋ",
          "original_form": "하...경주에도 건어물 가게가 있긴하겠지만 포항죽도시장서 사고싶은디ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100031324.1.18",
          "form": "그니께 ㅋㅋㅋ 근데 뭐 안강에 파는것도 포항서 오는거 아니겠냐고 ㅋㅋㅋ",
          "original_form": "그니께 ㅋㅋㅋ 근데 뭐 안강에 파는것도 포항서 오는거 아니겠냐고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031324.1.19",
          "form": "그 오징어가 그 오징어일거야 ㅋㅋㅋ 아무데서나 사자",
          "original_form": "그 오징어가 그 오징어일거야 ㅋㅋㅋ 아무데서나 사자",
          "speaker_id": "2",
          "time": "20210904 21:50"
        }
      ]
