        },
        {
          "id": "MDRW2100019178.1.2",
          "form": "콜라를 안준다",
          "original_form": "콜라를 안준다",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019178.1.3",
          "form": "실망이에요",
          "original_form": "실망이에요",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019178.1.4",
          "form": "태연씨 그렇게 웃고잇으면 다에요",
          "original_form": "태연씨 그렇게 웃고잇으면 다에요",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019178.1.5",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019178.1.6",
          "form": "너무해",
          "original_form": "너무해",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019178.1.7",
          "form": "콜라주세요...",
          "original_form": "콜라주세요...",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019178.1.8",
          "form": "홍당무 켜주세요",
          "original_form": "홍당무 켜주세요",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019178.1.9",
          "form": "진자 게살피자됨",
          "original_form": "진자 게살피자됨",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019178.1.10",
          "form": "콜라없이피자를 어케먹어요~!",
          "original_form": "콜라없이피자를 어케먹어요~!",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019178.1.11",
          "form": "그니깐..?",
          "original_form": "그니깐..?",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019178.1.12",
          "form": "무슨 생각이지",
          "original_form": "무슨 생각이지",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019178.1.13",
          "form": "이거 이번주안에",
          "original_form": "이거 이번주안에",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019178.1.14",
          "form": "바사삭 얻겟다",
          "original_form": "바사삭 얻겟다",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019178.1.15",
          "form": "주 소보냄",
          "original_form": "주 소보냄",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019178.1.16",
          "form": "감사",
          "original_form": "감사",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019178.1.17",
          "form": "피자 또 먹는중 ^^",
          "original_form": "피자 또 먹는중 ^^",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100019178.1.18",
          "form": "할라불고기",
          "original_form": "할라불고기",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019178.1.19",
          "form": "어케됨?",
          "original_form": "어케됨?",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019178.1.20",
          "form": "식으니까 목울만해요",
          "original_form": "식으니까 목울만해요",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019178.1.21",
          "form": "헐 다행이다",
          "original_form": "헐 다행이다",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019178.1.22",
          "form": "뜨듯피자 맛잇겟네",
          "original_form": "뜨듯피자 맛잇겟네",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019178.1.23",
          "form": "뜨뜻피자는 이재 없음",
          "original_form": "뜨뜻피자는 이재 없음",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019178.1.24",
          "form": "..",
          "original_form": "..",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019178.1.25",
          "form": "난 차갑피자",
          "original_form": "난 차갑피자",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019178.1.26",
          "form": "별로에요",
          "original_form": "별로에요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019178.1.27",
          "form": "근데 주면 먹어요",
          "original_form": "근데 주면 먹어요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019178.1.28",
          "form": "데워먹우면되지",
          "original_form": "데워먹우면되지",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019178.1.29",
          "form": "난 다 잘먹는다 ㅎㅎ",
          "original_form": "난 다 잘먹는다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019178.1.30",
          "form": "데워주세요 나는",
