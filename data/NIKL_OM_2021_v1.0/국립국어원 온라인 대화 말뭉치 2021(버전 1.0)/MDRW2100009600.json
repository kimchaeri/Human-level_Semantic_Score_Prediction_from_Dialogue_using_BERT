      "utterance": [
        {
          "id": "MDRW2100009600.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 01:28"
        },
        {
          "id": "MDRW2100009600.1.2",
          "form": "안녕하세요 온라인 커뮤니티 하시나요?",
          "original_form": "안녕하세요 온라인 커뮤니티 하시나요?",
          "speaker_id": "2",
          "time": "20210823 01:34"
        },
        {
          "id": "MDRW2100009600.1.3",
          "form": "저는 네이버 블로그 해요",
          "original_form": "저는 네이버 블로그 해요",
          "speaker_id": "1",
          "time": "20210823 01:35"
        },
        {
          "id": "MDRW2100009600.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210823 01:35"
        },
        {
          "id": "MDRW2100009600.1.5",
          "form": "네이버 블로그 저도 해요!",
          "original_form": "네이버 블로그 저도 해요!",
          "speaker_id": "2",
          "time": "20210823 01:37"
        },
        {
          "id": "MDRW2100009600.1.6",
          "form": "어떤 글을 쓰시나요?",
          "original_form": "어떤 글을 쓰시나요?",
          "speaker_id": "2",
          "time": "20210823 01:37"
        },
        {
          "id": "MDRW2100009600.1.7",
          "form": "저는 리뷰 같은 거 많이 썼고",
          "original_form": "저는 리뷰 같은 거 많이 썼고",
          "speaker_id": "1",
          "time": "20210823 01:38"
        },
        {
          "id": "MDRW2100009600.1.8",
          "form": "올림픽 기간에는 올림픽 관한 거 많이 썼었어요!",
          "original_form": "올림픽 기간에는 올림픽 관한 거 많이 썼었어요!",
          "speaker_id": "1",
          "time": "20210823 01:38"
        },
        {
          "id": "MDRW2100009600.1.9",
          "form": "바게트님은요?",
          "original_form": "바게트님은요?",
          "speaker_id": "1",
          "time": "20210823 01:38"
        },
        {
          "id": "MDRW2100009600.1.10",
          "form": "우와 저는 그냥 지역 맛집 이런거 써요",
          "original_form": "우와 저는 그냥 지역 맛집 이런거 써요",
          "speaker_id": "2",
          "time": "20210823 01:40"
        },
        {
          "id": "MDRW2100009600.1.11",
          "form": "전문적인 정보는 못쓰겠더라고요ㅠㅠ 방문자 얼마나 되시나요?",
          "original_form": "전문적인 정보는 못쓰겠더라고요ㅠㅠ 방문자 얼마나 되시나요?",
          "speaker_id": "2",
          "time": "20210823 01:40"
        },
        {
          "id": "MDRW2100009600.1.12",
          "form": "와 지역 맛집...사실 제가 제일 어려워하는 분야예요",
          "original_form": "와 지역 맛집...사실 제가 제일 어려워하는 분야예요",
          "speaker_id": "1",
          "time": "20210823 01:41"
        },
        {
          "id": "MDRW2100009600.1.13",
          "form": "맛표현이 힘들어서 ㅠㅠ",
          "original_form": "맛표현이 힘들어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 01:41"
        },
        {
          "id": "MDRW2100009600.1.14",
          "form": "앜ㅋㅋㅋ 저는 먹는거에 열중해서 그런게 낫더라구요",
          "original_form": "앜ㅋㅋㅋ 저는 먹는거에 열중해서 그런게 낫더라구요",
          "speaker_id": "2",
          "time": "20210823 01:41"
        },
        {
          "id": "MDRW2100009600.1.15",
          "form": "오히려 정보글이나 이런걸 쓰는게 어려운 것 같아요",
          "original_form": "오히려 정보글이나 이런걸 쓰는게 어려운 것 같아요",
          "speaker_id": "2",
          "time": "20210823 01:41"
        },
        {
          "id": "MDRW2100009600.1.16",
          "form": "저는 방문자가 많은 편은 아니라",
          "original_form": "저는 방문자가 많은 편은 아니라",
          "speaker_id": "1",
          "time": "20210823 01:42"
        },
        {
          "id": "MDRW2100009600.1.17",
          "form": "하루에 50...? 요즘 그 정도인 거 같아요",
          "original_form": "하루에 50...? 요즘 그 정도인 거 같아요",
          "speaker_id": "1",
          "time": "20210823 01:42"
        },
        {
          "id": "MDRW2100009600.1.18",
          "form": "그렇군요 저는 150~200 정도인데",
          "original_form": "그렇군요 저는 150~200 정도인데",
          "speaker_id": "2",
          "time": "20210823 01:42"
        },
        {
          "id": "MDRW2100009600.1.19",
          "form": "올해 안에 400찍는게 목표예요!",
          "original_form": "올해 안에 400찍는게 목표예요!",
          "speaker_id": "2",
          "time": "20210823 01:43"
        },
        {
          "id": "MDRW2100009600.1.20",
          "form": "우리 열심히 해봐요!",
          "original_form": "우리 열심히 해봐요!",
          "speaker_id": "2",
          "time": "20210823 01:43"
        },
        {
