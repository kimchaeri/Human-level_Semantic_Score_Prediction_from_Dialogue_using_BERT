      "utterance": [
        {
          "id": "MDRW2100010300.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100010300.1.2",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010300.1.3",
          "form": "아니요 무교랍니다",
          "original_form": "아니요 무교랍니다",
          "speaker_id": "1",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010300.1.4",
          "form": "종교있으신가요?",
          "original_form": "종교있으신가요?",
          "speaker_id": "1",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010300.1.5",
          "form": "네 저는 종교 있어요 ㅎㅎ",
          "original_form": "네 저는 종교 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:41"
        },
        {
          "id": "MDRW2100010300.1.6",
          "form": "아 요즘 아프가니스탄 때문에 난린데...",
          "original_form": "아 요즘 아프가니스탄 때문에 난린데...",
          "speaker_id": "2",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100010300.1.7",
          "form": "거긴 이슬람을 믿는 사람들인가요?",
          "original_form": "거긴 이슬람을 믿는 사람들인가요?",
          "speaker_id": "2",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100010300.1.8",
          "form": "아 어떤종교인가요?",
          "original_form": "아 어떤종교인가요?",
          "speaker_id": "1",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010300.1.9",
          "form": "맞아요 이슬람..",
          "original_form": "맞아요 이슬람..",
          "speaker_id": "1",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010300.1.10",
          "form": "저는 기독교 입니다 ㅎㅎ",
          "original_form": "저는 기독교 입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010300.1.11",
          "form": "오 기독교시군요",
          "original_form": "오 기독교시군요",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010300.1.12",
          "form": "저도교회에 가봤어요",
          "original_form": "저도교회에 가봤어요",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010300.1.13",
          "form": "아아 교회는 접근성이 좋아서 한번씩은 경험이 있더라구요 ㅎㅎ",
          "original_form": "아아 교회는 접근성이 좋아서 한번씩은 경험이 있더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100010300.1.14",
          "form": "맞아요ㅎㅎ 친구가",
          "original_form": "맞아요ㅎㅎ 친구가",
          "speaker_id": "1",
          "time": "20210823 16:56"
        },
        {
          "id": "MDRW2100010300.1.15",
          "form": "다녀서 같이가봤었어요",
          "original_form": "다녀서 같이가봤었어요",
          "speaker_id": "1",
          "time": "20210823 16:56"
        },
        {
          "id": "MDRW2100010300.1.16",
          "form": "그랬군요 ㅎㅎㅎ 저도 친구랑 어릴때 교회다니고 친구교회도 놀러가고 했었어요",
          "original_form": "그랬군요 ㅎㅎㅎ 저도 친구랑 어릴때 교회다니고 친구교회도 놀러가고 했었어요",
          "speaker_id": "2",
          "time": "20210823 16:57"
        },
        {
          "id": "MDRW2100010300.1.17",
          "form": "교회학교 다닐땐 어려서 그랬는지 재미있었는데 ㅎㅎ",
          "original_form": "교회학교 다닐땐 어려서 그랬는지 재미있었는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:57"
        },
        {
          "id": "MDRW2100010300.1.18",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:02"
        },
        {
          "id": "MDRW2100010300.1.19",
          "form": "달란트시장?이",
          "original_form": "달란트시장?이",
          "speaker_id": "1",
          "time": "20210823 17:02"
        },
        {
          "id": "MDRW2100010300.1.20",
          "form": "기억나네요",
          "original_form": "기억나네요",
          "speaker_id": "1",
          "time": "20210823 17:02"
        },
        {
          "id": "MDRW2100010300.1.21",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010300.1.22",
          "form": "몇번 다니셨었나봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "몇번 다니셨었나봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:10"
        },
        {
          "id": "MDRW2100010300.1.23",
          "form": "달란트시장ㅋㅋㅋㅋㅋ",
          "original_form": "달란트시장ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010300.1.24",
          "form": "거기서 샌드위치도 사먹고 인형도 사고... 지우개도 샀었는데.. 아련아련",
          "original_form": "거기서 샌드위치도 사먹고 인형도 사고... 지우개도 샀었는데.. 아련아련",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010300.1.25",
          "form": "요즘에는 줌으로 달란트시장해요...ㅎㅎ",
          "original_form": "요즘에는 줌으로 달란트시장해요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010300.1.26",
          "form": "맞아요ㅋㅋㅋ 진짜초딩때 달란트시장있었는데",
          "original_form": "맞아요ㅋㅋㅋ 진짜초딩때 달란트시장있었는데",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010300.1.27",
          "form": "헐 줌으로라니",
          "original_form": "헐 줌으로라니",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010300.1.28",
          "form": "코로나시대의 현황이네요ㅠㅠ",
          "original_form": "코로나시대의 현황이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010300.1.29",
          "form": "그르니까요ㅜㅜ name1님은 달란트시장때 뭐 사셨어요?",
          "original_form": "그르니까요ㅜㅜ &name1&님은 달란트시장때 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010300.1.30",
          "form": "교회마다 물품은 비슷할것 같긴하지만 ㅎㅎ",
          "original_form": "교회마다 물품은 비슷할것 같긴하지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010300.1.31",
          "form": "거의한",
          "original_form": "거의한",
          "speaker_id": "1",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010300.1.32",
          "form": "15년도 더된일이라 기억이 잘 안나네요ㅋㅋㅋ",
          "original_form": "15년도 더된일이라 기억이 잘 안나네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010300.1.33",
          "form": "노트이런거였을까요?",
          "original_form": "노트이런거였을까요?",
          "speaker_id": "1",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010300.1.34",
          "form": "ㅋㅋㅋㅋ저도 기억을 더듬어봤어요.",
          "original_form": "ㅋㅋㅋㅋ저도 기억을 더듬어봤어요.",
          "speaker_id": "2",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010300.1.35",
          "form": "어릴땐 교회에서 성가대도 하고..",
          "original_form": "어릴땐 교회에서 성가대도 하고..",
          "speaker_id": "2",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010300.1.36",
          "form": "여름성경학교 이런 수련회도 가고 그랬는데",
          "original_form": "여름성경학교 이런 수련회도 가고 그랬는데",
          "speaker_id": "2",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010300.1.37",
          "form": "맞아요 교회는참",
          "original_form": "맞아요 교회는참",
          "speaker_id": "1",
          "time": "20210823 17:47"
        },
        {
          "id": "MDRW2100010300.1.38",
          "form": "그런활동이 많은것같아요ㅋㅋㅋ",
          "original_form": "그런활동이 많은것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:47"
        },
        {
          "id": "MDRW2100010300.1.39",
          "form": "그쵸 ㅎㅎㅎ 거의 초딩들의 방학생활을 담당하는..",
          "original_form": "그쵸 ㅎㅎㅎ 거의 초딩들의 방학생활을 담당하는..",
