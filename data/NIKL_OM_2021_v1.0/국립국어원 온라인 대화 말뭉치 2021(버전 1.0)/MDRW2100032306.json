      "utterance": [
        {
          "id": "MDRW2100032306.1.1",
          "form": "ㅎㅎ 진짜 많이배우네요ㅎ",
          "original_form": "ㅎㅎ 진짜 많이배우네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032306.1.2",
          "form": "아직전 초보라 많이가르켜 주세요ㅎㅎ",
          "original_form": "아직전 초보라 많이가르켜 주세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032306.1.3",
          "form": "근데 답은 없으니 시도해보고 ㅎㅎ 아니아 싶으면 말으셔요 ㅎㅎ",
          "original_form": "근데 답은 없으니 시도해보고 ㅎㅎ 아니아 싶으면 말으셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032306.1.4",
          "form": "아이.성향이 달라서",
          "original_form": "아이.성향이 달라서",
          "speaker_id": "1",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032306.1.5",
          "form": "네네ㅎㅎ 그래야죠ㅎㅎ",
          "original_form": "네네ㅎㅎ 그래야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032306.1.6",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032306.1.7",
          "form": "저희애는 마음에드는 장난감이 있으면 바로 사달라하고ㅎ",
          "original_form": "저희애는 마음에드는 장난감이 있으면 바로 사달라하고ㅎ",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032306.1.8",
          "form": "안사주면 삐치고그래요ㅎㅎ",
          "original_form": "안사주면 삐치고그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032306.1.9",
          "form": "그래서 어쩔수없이 사주는 것같애요ㅎ",
          "original_form": "그래서 어쩔수없이 사주는 것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032306.1.10",
          "form": "아 ㅎㅎ 근데 그걸 마냥 해줄수 없으니",
          "original_form": "아 ㅎㅎ 근데 그걸 마냥 해줄수 없으니",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032306.1.11",
          "form": "아이가 더크면 클수록 원하는 가격대가",
          "original_form": "아이가 더크면 클수록 원하는 가격대가",
          "speaker_id": "1",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032306.1.12",
          "form": "넘 높아지니  조절하는 방버을 살짝 배우는것도 괜찮은거 같아요 ㅎㅎ",
          "original_form": "넘 높아지니  조절하는 방버을 살짝 배우는것도 괜찮은거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032306.1.13",
          "form": "맞네요ㅎ 지금도 하나에 10만원씩하는대ㅎ",
          "original_form": "맞네요ㅎ 지금도 하나에 10만원씩하는대ㅎ",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032306.1.14",
          "form": "나중에는 더 커지겟내요ㅎㅎ",
          "original_form": "나중에는 더 커지겟내요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032306.1.15",
          "form": "지금도 원피스입고싶다고하네요ㅎㅎ",
          "original_form": "지금도 원피스입고싶다고하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
