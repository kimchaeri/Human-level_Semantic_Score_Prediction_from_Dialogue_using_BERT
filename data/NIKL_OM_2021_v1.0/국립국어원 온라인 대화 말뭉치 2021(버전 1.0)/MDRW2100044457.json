      "utterance": [
        {
          "id": "MDRW2100044457.1.1",
          "form": "모두 유명ㅋㅋㅋㅋ",
          "original_form": "모두 유명ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044457.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ대박 어디브랜드야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ대박 어디브랜드야",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044457.1.3",
          "form": "무슨 생갇으로 출시햇나 요런 느낌",
          "original_form": "무슨 생갇으로 출시햇나 요런 느낌",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044457.1.4",
          "form": "나도 짤만 봐가지곸ㅋㅋㅋ",
          "original_form": "나도 짤만 봐가지곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044457.1.5",
          "form": "참 치킨이 다양하구나..",
          "original_form": "참 치킨이 다양하구나..",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044457.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044457.1.7",
          "form": "바른치킨하니까 바른치킨땡긴다",
          "original_form": "바른치킨하니까 바른치킨땡긴다",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044457.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ아 바른치킨 함 무봐야하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 바른치킨 함 무봐야하는데",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044457.1.9",
          "form": "존막탱",
          "original_form": "존막탱",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044457.1.10",
          "form": "양이 좀 애매해",
          "original_form": "양이 좀 애매해",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044457.1.11",
          "form": "아 혼자먹으면 충분ㅋㅋㅋㅋ",
          "original_form": "아 혼자먹으면 충분ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044457.1.12",
          "form": "아 그정도? ㅋㅋㅋㅋㅋ",
          "original_form": "아 그정도? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044457.1.13",
          "form": "엄청 많은 줄",
          "original_form": "엄청 많은 줄",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044457.1.14",
          "form": "딱 적당한 한마리크기??ㅋㅋㅋㅋㅋ",
          "original_form": "딱 적당한 한마리크기??ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044457.1.15",
          "form": "교촌보다는 많음?",
          "original_form": "교촌보다는 많음?",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044457.1.16",
          "form": "그런거같음ㅋㅋㅋㅋ",
          "original_form": "그런거같음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044457.1.17",
          "form": "집에서 가족이랑 먹기엔 좀 작더라",
          "original_form": "집에서 가족이랑 먹기엔 좀 작더라",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044457.1.18",
          "form": "배부른느낌은 없어",
          "original_form": "배부른느낌은 없어",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044457.1.19",
          "form": "ㅋㅋㅋㅋㅋ그럼 혼자 시키무야겟네",
          "original_form": "ㅋㅋㅋㅋㅋ그럼 혼자 시키무야겟네",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044457.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ무조건 반반시켜",
          "original_form": "ㅋㅋㅋㅋㅋㅋ무조건 반반시켜",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044457.1.21",
          "form": "ㅋㅋㅋ반반이먄 머머야",
          "original_form": "ㅋㅋㅋ반반이먄 머머야",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044457.1.22",
          "form": "양념 후라이드?....",
          "original_form": "양념 후라이드?....",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044457.1.23",
          "form": "현미후라이드랑 그 대세레듴ㅋㅋㅋㅋ",
          "original_form": "현미후라이드랑 그 대세레듴ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044457.1.24",
          "form": "대세레드랑 양념이랑 맛달라 조심",
          "original_form": "대세레드랑 양념이랑 맛달라 조심",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044457.1.25",
          "form": "아 ㅋㅋㅋㅋ귯 좋은 가이드다",
          "original_form": "아 ㅋㅋㅋㅋ귯 좋은 가이드다",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044457.1.26",
          "form": "ㅋㅋㅋㅋ후라이드가 현미후라이드여",
          "original_form": "ㅋㅋㅋㅋ후라이드가 현미후라이드여",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044457.1.27",
          "form": "주말에 시켜서 맥주 때려야지...",
          "original_form": "주말에 시켜서 맥주 때려야지...",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044457.1.28",
          "form": "현미후라이드라서 완전 고소함",
          "original_form": "현미후라이드라서 완전 고소함",
