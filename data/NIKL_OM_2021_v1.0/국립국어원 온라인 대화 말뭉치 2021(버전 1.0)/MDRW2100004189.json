      "utterance": [
        {
          "id": "MDRW2100004189.1.1",
          "form": "키우는 반려동물 있어?",
          "original_form": "키우는 반려동물 있어?",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004189.1.2",
          "form": "지금은 키우는 반려동물은 없어!",
          "original_form": "지금은 키우는 반려동물은 없어!",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004189.1.3",
          "form": "오 그럼 전에는 키웠던 거야?",
          "original_form": "오 그럼 전에는 키웠던 거야?",
          "speaker_id": "2",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004189.1.4",
          "form": "이전에는 구피랑 고슴도치 키웠었어!",
          "original_form": "이전에는 구피랑 고슴도치 키웠었어!",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004189.1.5",
          "form": "우와 구피.. 우리 할머니집 가면 그런 작은 물고기들 많았어",
          "original_form": "우와 구피.. 우리 할머니집 가면 그런 작은 물고기들 많았어",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004189.1.6",
          "form": "고슴도치도 귀엽지 내 친구 중에 키우던 애 있었어 쓰다듬기 어렵더라",
          "original_form": "고슴도치도 귀엽지 내 친구 중에 키우던 애 있었어 쓰다듬기 어렵더라",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004189.1.7",
          "form": "핸들링이 중요한 동물이라ㅠ 그래도 엄청 귀여웠다ㅋㅋ",
          "original_form": "핸들링이 중요한 동물이라ㅠ 그래도 엄청 귀여웠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004189.1.8",
          "form": "맞아.. 이름은 어떻게 지어줬었어?",
          "original_form": "맞아.. 이름은 어떻게 지어줬었어?",
          "speaker_id": "2",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004189.1.9",
          "form": "수컷은 name3, 암컷은 name4로 지었어!",
          "original_form": "수컷은 &name3&, 암컷은 &name4&로 지었어!",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004189.1.10",
          "form": "오 name3name4 잘 어울린다ㅋㅋㅋ",
          "original_form": "오 &name3&&name4& 잘 어울린다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004189.1.11",
          "form": "간식은 어떤 걸 줘?",
          "original_form": "간식은 어떤 걸 줘?",
          "speaker_id": "2",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004189.1.12",
          "form": "주식은 사료였고, 가끔 고슴도치 전용 소시지랑 밀웜?",
          "original_form": "주식은 사료였고, 가끔 고슴도치 전용 소시지랑 밀웜?",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004189.1.13",
          "form": "와우 밀웜 보다보면 적응 되는 거겠지",
          "original_form": "와우 밀웜 보다보면 적응 되는 거겠지",
          "speaker_id": "2",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004189.1.14",
          "form": "나는 끝까지 적응이 안 되더라...",
          "original_form": "나는 끝까지 적응이 안 되더라...",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004189.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 키운 사람이 그런 말 하니까 되게 웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 키운 사람이 그런 말 하니까 되게 웃기다",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004189.1.16",
          "form": "고양이 키우는 사람들은 집사라고 하잖아",
          "original_form": "고양이 키우는 사람들은 집사라고 하잖아",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004189.1.17",
          "form": "고슴도치 키우는 사람은 뭐라고 해?",
          "original_form": "고슴도치 키우는 사람은 뭐라고 해?",
          "speaker_id": "2",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004189.1.18",
          "form": "음 딱히 호칭을 나눴던 기억은 없어! 집사라는 호칭이 나오기 전에 길렀던 거라서ㅎㅎ",
          "original_form": "음 딱히 호칭을 나눴던 기억은 없어! 집사라는 호칭이 나오기 전에 길렀던 거라서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:48"
        },
        {
          "id": "MDRW2100004189.1.19",
          "form": "집사라는 호칭이 나온지 별로 안됐나?",
          "original_form": "집사라는 호칭이 나온지 별로 안됐나?",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004189.1.20",
          "form": "암튼 귀엽구만 고슴도치",
          "original_form": "암튼 귀엽구만 고슴도치",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004189.1.21",
          "form": "나는 강아지 키워보고 싶더라",
          "original_form": "나는 강아지 키워보고 싶더라",
          "speaker_id": "2",
          "time": "20210821 15:52"
        }
      ]
