      "utterance": [
        {
          "id": "MDRW2100011831.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011831.1.2",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011831.1.3",
          "form": "네! 좋아하는데 요즘은 자주 못가죠ㅠㅠ",
          "original_form": "네! 좋아하는데 요즘은 자주 못가죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011831.1.4",
          "form": "최근에 다녀오신 곳 있으세요?",
          "original_form": "최근에 다녀오신 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011831.1.5",
          "form": "최근이라고 하긴 어렵지만 5월에 부산 갔었어요!",
          "original_form": "최근이라고 하긴 어렵지만 5월에 부산 갔었어요!",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011831.1.6",
          "form": "제일 좋았던 여행지 있으세요?",
          "original_form": "제일 좋았던 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011831.1.7",
          "form": "저도 부산 좋아해요!",
          "original_form": "저도 부산 좋아해요!",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011831.1.8",
          "form": "근데 최근에 간적이없네요",
          "original_form": "근데 최근에 간적이없네요",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011831.1.9",
          "form": "바다있는 곳 좋아해요",
          "original_form": "바다있는 곳 좋아해요",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011831.1.10",
          "form": "부산에서 뭐 드셨었나요?",
          "original_form": "부산에서 뭐 드셨었나요?",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011831.1.11",
          "form": "떡볶이랑 국밥이 기억에 남네요",
          "original_form": "떡볶이랑 국밥이 기억에 남네요",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011831.1.12",
          "form": "딱히 유명한곳 간 건 아닌데 맛있었어요!",
          "original_form": "딱히 유명한곳 간 건 아닌데 맛있었어요!",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011831.1.13",
          "form": "저는 아직 부산에서 국밥 먹어본적이 없어요",
          "original_form": "저는 아직 부산에서 국밥 먹어본적이 없어요",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011831.1.14",
          "form": "정말 다른지역이랑 맛이 다른가요?",
          "original_form": "정말 다른지역이랑 맛이 다른가요?",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011831.1.15",
          "form": "음.. 뭐 특별하게 다른건 사실 잘 모르겠어요",
          "original_form": "음.. 뭐 특별하게 다른건 사실 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011831.1.16",
          "form": "그래도 맛은 있더라구요!",
          "original_form": "그래도 맛은 있더라구요!",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011831.1.17",
          "form": "해외여행도 좋아하시나요?",
          "original_form": "해외여행도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011831.1.18",
          "form": "네네 아직 몇번 가본적 없지만 좋아해요",
          "original_form": "네네 아직 몇번 가본적 없지만 좋아해요",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011831.1.19",
          "form": "다녀오신 해외 있으세요?",
          "original_form": "다녀오신 해외 있으세요?",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100011831.1.20",
          "form": "네 옛날에 뉴욕 가봤는데 제일 좋았고, 대만이랑 일본도 좋았어요",
          "original_form": "네 옛날에 뉴욕 가봤는데 제일 좋았고, 대만이랑 일본도 좋았어요",
          "speaker_id": "1",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011831.1.21",
          "form": "우와 뉴욕은 정말 한번 가보고싶어요",
          "original_form": "우와 뉴욕은 정말 한번 가보고싶어요",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011831.1.22",
          "form": "영어권이라 그나마 말하기도 편할 것 같아요",
          "original_form": "영어권이라 그나마 말하기도 편할 것 같아요",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011831.1.23",
          "form": "사실 코로나 직전에 또 갈 뻔 했는데",
          "original_form": "사실 코로나 직전에 또 갈 뻔 했는데",
          "speaker_id": "1",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011831.1.24",
          "form": "셧다운해서 취소했었어요..",
          "original_form": "셧다운해서 취소했었어요..",
          "speaker_id": "1",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100011831.1.25",
          "form": "아ㅠ 아쉽네요",
          "original_form": "아ㅠ 아쉽네요",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011831.1.26",
          "form": "타이밍이 안맞으셨군요",
          "original_form": "타이밍이 안맞으셨군요",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011831.1.27",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011831.1.28",
          "form": "코로나 끝나면",
          "original_form": "코로나 끝나면",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011831.1.29",
          "form": "가고싶은 곳 있으신가요?",
          "original_form": "가고싶은 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100011831.1.30",
          "form": "저는 코로나 끝나면 유니버셜스튜디오를 가보고싶어요",
          "original_form": "저는 코로나 끝나면 유니버셜스튜디오를 가보고싶어요",
          "speaker_id": "2",
          "time": "20210824 16:33"
        }
      ]
    }
