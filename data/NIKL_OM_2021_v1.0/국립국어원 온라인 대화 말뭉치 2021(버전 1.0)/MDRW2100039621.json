      "utterance": [
        {
          "id": "MDRW2100039621.1.1",
          "form": "요즘 운동 하시나요",
          "original_form": "요즘 운동 하시나요",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039621.1.2",
          "form": "제가 입운동만 하는 사람이라...",
          "original_form": "제가 입운동만 하는 사람이라...",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039621.1.3",
          "form": "name1님은 운동 하세요?",
          "original_form": "&name1&님은 운동 하세요?",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039621.1.4",
          "form": "저도 코로나 심해지고 숨쉬기운동만 하고 있어요",
          "original_form": "저도 코로나 심해지고 숨쉬기운동만 하고 있어요",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039621.1.5",
          "form": "살이 너무 쪄서 운동하려고요",
          "original_form": "살이 너무 쪄서 운동하려고요",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100039621.1.6",
          "form": "무슨 운동 하실거에요?",
          "original_form": "무슨 운동 하실거에요?",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039621.1.7",
          "form": "일단 걷기부터 해보려고요",
          "original_form": "일단 걷기부터 해보려고요",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039621.1.8",
          "form": "name2님은 운동 계획은 없으신가요?",
          "original_form": "&name2&님은 운동 계획은 없으신가요?",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039621.1.9",
          "form": "저는 걷는건 좋아해서요 근데 요즘엔 잘 걷지를 못하네요",
          "original_form": "저는 걷는건 좋아해서요 근데 요즘엔 잘 걷지를 못하네요",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100039621.1.10",
          "form": "만약에 운동을 배운다면 필라테스 해보고 싶어요",
          "original_form": "만약에 운동을 배운다면 필라테스 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039621.1.11",
          "form": "저도 이사오고나서 운동을 더 안하게 되더라고요",
          "original_form": "저도 이사오고나서 운동을 더 안하게 되더라고요",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039621.1.12",
          "form": "걷기부터 시작해야 무리없이 운동량을 늘릴거같아요",
          "original_form": "걷기부터 시작해야 무리없이 운동량을 늘릴거같아요",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039621.1.13",
          "form": "필라테스 좋아요 살은 안빠지지만 몸이 단단해지는 기분이예요",
          "original_form": "필라테스 좋아요 살은 안빠지지만 몸이 단단해지는 기분이예요",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039621.1.14",
          "form": "제가 진짜 근육량이 적어서 흐물흐물하거든요ㅋㅋ",
          "original_form": "제가 진짜 근육량이 적어서 흐물흐물하거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039621.1.15",
          "form": "몸이 단단해진다니 진짜 해봐야겠어요",
          "original_form": "몸이 단단해진다니 진짜 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039621.1.16",
          "form": "아 근육은 는다는걸 느껴요\r\n근데 살빼려면 비추예요ㅋㅋ",
          "original_form": "아 근육은 는다는걸 느껴요\r\n근데 살빼려면 비추예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039621.1.17",
          "form": "살은 어차피 신의 영역 같기도 해섴ㅋㅋㅋ",
          "original_form": "살은 어차피 신의 영역 같기도 해섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039621.1.18",
          "form": "뱃살이 참 고민이에요",
          "original_form": "뱃살이 참 고민이에요",
          "speaker_id": "2",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039621.1.19",
          "form": "그쵸 전 모든 살이 다 걱정이예요ㅋㅋ\r\n수영가고싶은데 코로나로 무서워서 못가겠어요",
          "original_form": "그쵸 전 모든 살이 다 걱정이예요ㅋㅋ\r\n수영가고싶은데 코로나로 무서워서 못가겠어요",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039621.1.20",
          "form": "뱃살에 필라테스 효과 좋아요",
          "original_form": "뱃살에 필라테스 효과 좋아요",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039621.1.21",
          "form": "운동도 운동인데 밀가루를 좀 덜 먹어야 빠질거 같긴 해요ㅋㅋ",
          "original_form": "운동도 운동인데 밀가루를 좀 덜 먹어야 빠질거 같긴 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039621.1.22",
          "form": "근데 지금도 도넛을 먹고 있네요",
          "original_form": "근데 지금도 도넛을 먹고 있네요",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039621.1.23",
          "form": "맞아요 가장 중요한게 먹는거라더라고요",
          "original_form": "맞아요 가장 중요한게 먹는거라더라고요",
          "speaker_id": "1",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039621.1.24",
          "form": "먹는거 행복해요...",
          "original_form": "먹는거 행복해요...",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039621.1.25",
          "form": "저도 운동해야지 하면서 과자 먹고 있어요ㅋㅋ",
          "original_form": "저도 운동해야지 하면서 과자 먹고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039621.1.26",
          "form": "불금이니까 합리화시키고 있어요",
          "original_form": "불금이니까 합리화시키고 있어요",
          "speaker_id": "1",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039621.1.27",
          "form": "맞아요 금요일엔 괜찮아욬ㅋㅋ",
          "original_form": "맞아요 금요일엔 괜찮아욬ㅋㅋ",
          "speaker_id": "2",
