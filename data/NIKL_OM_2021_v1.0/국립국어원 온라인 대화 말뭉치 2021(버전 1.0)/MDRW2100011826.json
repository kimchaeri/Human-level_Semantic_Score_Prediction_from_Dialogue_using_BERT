      "utterance": [
        {
          "id": "MDRW2100011826.1.1",
          "form": "많아 참는 중",
          "original_form": "많아 참는 중",
          "speaker_id": "2",
          "time": "20210824 16:21"
        },
        {
          "id": "MDRW2100011826.1.2",
          "form": "아 그래?난 딱히 없는데.뭐 사고 싶은데?",
          "original_form": "아 그래?난 딱히 없는데.뭐 사고 싶은데?",
          "speaker_id": "1",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011826.1.3",
          "form": "컵이랑 반지랑 등등?",
          "original_form": "컵이랑 반지랑 등등?",
          "speaker_id": "2",
          "time": "20210824 16:22"
        },
        {
          "id": "MDRW2100011826.1.4",
          "form": "반지?",
          "original_form": "반지?",
          "speaker_id": "1",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011826.1.5",
          "form": "유기놋쇠로 만든 반지가 있더라고",
          "original_form": "유기놋쇠로 만든 반지가 있더라고",
          "speaker_id": "2",
          "time": "20210824 16:23"
        },
        {
          "id": "MDRW2100011826.1.6",
          "form": "의왼데?",
          "original_form": "의왼데?",
          "speaker_id": "1",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011826.1.7",
          "form": "예쁘더라고",
          "original_form": "예쁘더라고",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011826.1.8",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "1",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011826.1.9",
          "form": "사고싶은 거 없다고?",
          "original_form": "사고싶은 거 없다고?",
          "speaker_id": "2",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011826.1.10",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011826.1.11",
          "form": "좋은 마인드네",
          "original_form": "좋은 마인드네",
          "speaker_id": "2",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011826.1.12",
          "form": "ㅋㅋㅋㅋ 요즘 쇼핑 많이 하는것 같아.스트레스 풀려고",
          "original_form": "ㅋㅋㅋㅋ 요즘 쇼핑 많이 하는것 같아.스트레스 풀려고",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011826.1.13",
          "form": "그치 뭐 나갈 수도 없으니까",
          "original_form": "그치 뭐 나갈 수도 없으니까",
          "speaker_id": "2",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011826.1.14",
          "form": "얼마 전에 주문한 그릇 얼른 도착했으면 좋겠다",
          "original_form": "얼마 전에 주문한 그릇 얼른 도착했으면 좋겠다",
