      "utterance": [
        {
          "id": "MDRW2100036057.1.1",
          "form": "백신은 많으셨어요?",
          "original_form": "백신은 많으셨어요?",
          "speaker_id": "1",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100036057.1.2",
          "form": "네 얀센요",
          "original_form": "네 얀센요",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100036057.1.3",
          "form": "맞으셨어요?",
          "original_form": "맞으셨어요?",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100036057.1.4",
          "form": "전 십부제  신청해서 담달에 맞아요",
          "original_form": "전 십부제  신청해서 담달에 맞아요",
          "speaker_id": "1",
          "time": "20210830 15:30"
        },
        {
          "id": "MDRW2100036057.1.5",
          "form": "얀센이면 한번에  끝났겠네요",
          "original_form": "얀센이면 한번에  끝났겠네요",
          "speaker_id": "1",
          "time": "20210830 15:30"
        },
        {
          "id": "MDRW2100036057.1.6",
          "form": "네 근데 쫌 그래요",
          "original_form": "네 근데 쫌 그래요",
          "speaker_id": "2",
          "time": "20210830 15:30"
        },
        {
          "id": "MDRW2100036057.1.7",
          "form": "부스터샷 맞고 싶네요",
          "original_form": "부스터샷 맞고 싶네요",
          "speaker_id": "2",
          "time": "20210830 15:30"
        },
        {
          "id": "MDRW2100036057.1.8",
          "form": "효과가 별로라 해서요",
          "original_form": "효과가 별로라 해서요",
          "speaker_id": "2",
          "time": "20210830 15:30"
        },
        {
          "id": "MDRW2100036057.1.9",
          "form": "부스터샷도 있을거 같아요.. 2차까지 대충 돌아가면요..",
          "original_form": "부스터샷도 있을거 같아요.. 2차까지 대충 돌아가면요..",
          "speaker_id": "1",
          "time": "20210830 15:32"
        },
        {
          "id": "MDRW2100036057.1.10",
          "form": "네 빨리 후딱 맞고 싶어요",
          "original_form": "네 빨리 후딱 맞고 싶어요",
          "speaker_id": "2",
          "time": "20210830 15:33"
        },
        {
          "id": "MDRW2100036057.1.11",
          "form": "효과가 9배 된데요",
          "original_form": "효과가 9배 된데요",
          "speaker_id": "2",
          "time": "20210830 15:33"
        },
        {
          "id": "MDRW2100036057.1.12",
          "form": "오~ 전 아직 1차도 안맞아서요",
          "original_form": "오~ 전 아직 1차도 안맞아서요",
          "speaker_id": "1",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100036057.1.13",
          "form": "네 1차는 뭐로 맞아요?",
          "original_form": "네 1차는 뭐로 맞아요?",
          "speaker_id": "2",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100036057.1.14",
          "form": "미정이래요.. 맞을때 알려준데요..ㅋㅋㅋ",
          "original_form": "미정이래요.. 맞을때 알려준데요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100036057.1.15",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100036057.1.16",
          "form": "50대는 화이자 맞던데 그후는 유동적인가봐요",
          "original_form": "50대는 화이자 맞던데 그후는 유동적인가봐요",
          "speaker_id": "2",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100036057.1.17",
          "form": "뭐 백신 수급에 따라 달라져서 그런가봐요.. 아무튼 화이자 아니면 모더나라고 쓰여있네요",
          "original_form": "뭐 백신 수급에 따라 달라져서 그런가봐요.. 아무튼 화이자 아니면 모더나라고 쓰여있네요",
          "speaker_id": "1",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100036057.1.18",
          "form": "네 둘다 좋죠 ㅎㅎ",
          "original_form": "네 둘다 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036057.1.19",
          "form": "맞고 나면 든든해요",
          "original_form": "맞고 나면 든든해요",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036057.1.20",
          "form": "저희가족 다 멎았어요 ㅎㅎ",
          "original_form": "저희가족 다 멎았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036057.1.21",
          "form": "막내딸은 어려서 못맞소",
          "original_form": "막내딸은 어려서 못맞소",
          "speaker_id": "2",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100036057.1.22",
          "form": "고",
          "original_form": "고",
          "speaker_id": "2",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100036057.1.23",
          "form": "저는 가족들 다 맞고 저만 아직이요..ㅎㅎ",
          "original_form": "저는 가족들 다 맞고 저만 아직이요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036057.1.24",
          "form": "얼른 맞고 싶네요",
          "original_form": "얼른 맞고 싶네요",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036057.1.25",
          "form": "아휴 진짜 빨리 맞고 싶으시죠 ㅜㅜ",
          "original_form": "아휴 진짜 빨리 맞고 싶으시죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100036057.1.26",
          "form": "맞고나면 불안이 좀 사라져요",
          "original_form": "맞고나면 불안이 좀 사라져요",
