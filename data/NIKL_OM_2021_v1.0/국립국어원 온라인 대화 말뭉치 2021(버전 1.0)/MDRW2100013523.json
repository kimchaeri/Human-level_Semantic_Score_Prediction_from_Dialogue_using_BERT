      "utterance": [
        {
          "id": "MDRW2100013523.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013523.1.2",
          "form": "안녕하세요 무교입니다 ㅎㅎㅎ",
          "original_form": "안녕하세요 무교입니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013523.1.3",
          "form": "있으신지..",
          "original_form": "있으신지..",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013523.1.4",
          "form": "어릴때 친구들이랑 우르르 교회 다닌적은 있는데",
          "original_form": "어릴때 친구들이랑 우르르 교회 다닌적은 있는데",
          "speaker_id": "1",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013523.1.5",
          "form": "무교예요 저도ㅎㅎ",
          "original_form": "무교예요 저도ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013523.1.6",
          "form": "아 저도 어릴때 교회 ㅋㅋㅋㅋ",
          "original_form": "아 저도 어릴때 교회 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013523.1.7",
          "form": "간식받으러..",
          "original_form": "간식받으러..",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013523.1.8",
          "form": "ㅋㅋㅋㅋ맞아요.. 거기서 점심도 주고",
          "original_form": "ㅋㅋㅋㅋ맞아요.. 거기서 점심도 주고",
          "speaker_id": "1",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013523.1.9",
          "form": "달란트도 받아서 학용품 사고",
          "original_form": "달란트도 받아서 학용품 사고",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013523.1.10",
          "form": "아 달란트마켓 생각나욬ㅋㅋㅋㅋ",
          "original_form": "아 달란트마켓 생각나욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013523.1.11",
          "form": "크리스마스때 행사해서 문화상품권 주고 그랚ㅅ는데..",
          "original_form": "크리스마스때 행사해서 문화상품권 주고 그랚ㅅ는데..",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013523.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그쵸그쵸 그거 받으려고 엄총 열심히 다닌 기억이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그쵸그쵸 그거 받으려고 엄총 열심히 다닌 기억이",
          "speaker_id": "1",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013523.1.13",
          "form": "성경책 필사도 하고..",
          "original_form": "성경책 필사도 하고..",
          "speaker_id": "1",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013523.1.14",
          "form": "맞아ㅇㅛ 저도 성경퀴즈 막 풀었던 기억이 나요",
          "original_form": "맞아ㅇㅛ 저도 성경퀴즈 막 풀었던 기억이 나요",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013523.1.15",
          "form": "기독교도 아니면서..",
          "original_form": "기독교도 아니면서..",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013523.1.16",
          "form": "그때는 신앙심이라기버다..... 콩고물에 더 관심이 많았던ㅎㅎㅎ",
          "original_form": "그때는 신앙심이라기버다..... 콩고물에 더 관심이 많았던ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013523.1.17",
          "form": "콩고물이 원체 매력적이어야..",
          "original_form": "콩고물이 원체 매력적이어야..",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013523.1.18",
          "form": "맛있는거 주면 절도 다녓을거같아요",
          "original_form": "맛있는거 주면 절도 다녓을거같아요",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013523.1.19",
          "form": "전 집안자체가 불교라 절도 자주 갔었는데",
          "original_form": "전 집안자체가 불교라 절도 자주 갔었는데",
          "speaker_id": "1",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013523.1.20",
          "form": "절밥 맛있어요 ㅣㅎㅎ 산채비빔밥..",
          "original_form": "절밥 맛있어요 ㅣㅎㅎ 산채비빔밥..",
          "speaker_id": "1",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100013523.1.21",
          "form": "싹싹 긁어먹었어요",
          "original_form": "싹싹 긁어먹었어요",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013523.1.22",
          "form": "아 절밥 !! ㄹㅇ 저도 템플스테이 여러번갔는데",
          "original_form": "아 절밥 !! ㄹㅇ 저도 템플스테이 여러번갔는데",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013523.1.23",
          "form": "ㅈㅓ도 템플스테이 자주갔거든요",
          "original_form": "ㅈㅓ도 템플스테이 자주갔거든요",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013523.1.24",
          "form": "절밥 진짜 ㅜ",
          "original_form": "절밥 진짜 ㅜ",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013523.1.25",
          "form": "유자에 버무린 연근 먹은적있는데 진짜맛있었어요",
          "original_form": "유자에 버무린 연근 먹은적있는데 진짜맛있었어요",
          "speaker_id": "2",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013523.1.26",
          "form": "헛 최근엔 가본적 없는데 유자에 버무린 연근이라니!!",
          "original_form": "헛 최근엔 가본적 없는데 유자에 버무린 연근이라니!!",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013523.1.27",
          "form": "레시피가 굉장히 세련되어진것 같은 느낌이에요ㅋㅋㅋㅋ",
