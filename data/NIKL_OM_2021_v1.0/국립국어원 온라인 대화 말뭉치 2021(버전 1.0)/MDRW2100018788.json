      "utterance": [
        {
          "id": "MDRW2100018788.1.1",
          "form": "점심은 주로 모 드세요?",
          "original_form": "점심은 주로 모 드세요?",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018788.1.2",
          "form": "요즘엔 다이어트식 먹으려해요",
          "original_form": "요즘엔 다이어트식 먹으려해요",
          "speaker_id": "2",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018788.1.3",
          "form": "밀키트 같은거요? 아님 샐러드?",
          "original_form": "밀키트 같은거요? 아님 샐러드?",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018788.1.4",
          "form": "만들어먹어요!",
          "original_form": "만들어먹어요!",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018788.1.5",
          "form": "오늘은 통밀또띠아요!",
          "original_form": "오늘은 통밀또띠아요!",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018788.1.6",
          "form": "그런 고급스러운걸 직접 만드세요?",
          "original_form": "그런 고급스러운걸 직접 만드세요?",
          "speaker_id": "1",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018788.1.7",
          "form": "한번 도전해봤어요!ㅋㅋ",
          "original_form": "한번 도전해봤어요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018788.1.8",
          "form": "꽤 먹을만해서 만족스러워요",
          "original_form": "꽤 먹을만해서 만족스러워요",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018788.1.9",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018788.1.10",
          "form": "전 1시 넘어서 먹어요.. \r\n아침을 늦게 먹고.. \r\n\r\n또띠아 빵은 파는거에요?",
          "original_form": "전 1시 넘어서 먹어요.. \r\n아침을 늦게 먹고.. \r\n\r\n또띠아 빵은 파는거에요?",
          "speaker_id": "1",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018788.1.11",
          "form": "늦게 드시는편이시군요",
          "original_form": "늦게 드시는편이시군요",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018788.1.12",
          "form": "네 통밀또띠아 인터넷에서 구매해서 다이어트 책보고 만들어 먹고 있어요",
          "original_form": "네 통밀또띠아 인터넷에서 구매해서 다이어트 책보고 만들어 먹고 있어요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018788.1.13",
          "form": "빵은 생지같은거에요?",
          "original_form": "빵은 생지같은거에요?",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018788.1.14",
          "form": "구워진걸 냉동판매하는거 같아요",
          "original_form": "구워진걸 냉동판매하는거 같아요",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018788.1.15",
          "form": "아.. 그럼 해동만하면 되능?",
          "original_form": "아.. 그럼 해동만하면 되능?",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018788.1.16",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018788.1.17",
          "form": "저는 더 맛있으면해서 후라이팬에 구워먹어요",
          "original_form": "저는 더 맛있으면해서 후라이팬에 구워먹어요",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018788.1.18",
          "form": "맛잇겟다... 저도 샐러드가 주식이라..",
          "original_form": "맛잇겟다... 저도 샐러드가 주식이라..",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018788.1.19",
          "form": "해보고 싶네요",
          "original_form": "해보고 싶네요",
          "speaker_id": "1",
          "time": "20210827 12:24"
        }
      ]
