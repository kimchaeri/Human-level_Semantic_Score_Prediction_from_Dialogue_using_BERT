      "utterance": [
        {
          "id": "MDRW2100019310.1.1",
          "form": "저녁 메뉴 정하셨나요?",
          "original_form": "저녁 메뉴 정하셨나요?",
          "speaker_id": "1",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100019310.1.2",
          "form": "지금 치킨 먹고 있습니다!",
          "original_form": "지금 치킨 먹고 있습니다!",
          "speaker_id": "2",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019310.1.3",
          "form": "후라이드 치킨이요~",
          "original_form": "후라이드 치킨이요~",
          "speaker_id": "2",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019310.1.4",
          "form": "배달시키신건가요?",
          "original_form": "배달시키신건가요?",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019310.1.5",
          "form": "아뇽 집 가는 길에 포장했어요",
          "original_form": "아뇽 집 가는 길에 포장했어요",
          "speaker_id": "2",
          "time": "20210827 18:08"
        },
        {
          "id": "MDRW2100019310.1.6",
          "form": "테이크아웃 하니까 할인해주더라구요!",
          "original_form": "테이크아웃 하니까 할인해주더라구요!",
          "speaker_id": "2",
          "time": "20210827 18:08"
        },
        {
          "id": "MDRW2100019310.1.7",
          "form": "저도 저녁 픽업해서 집 가고 있어요ㅎㅎ",
          "original_form": "저도 저녁 픽업해서 집 가고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019310.1.8",
          "form": "어떤거 드세요?",
          "original_form": "어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019310.1.9",
          "form": "보쌈과 막국수요!",
          "original_form": "보쌈과 막국수요!",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019310.1.10",
          "form": "방금 보쌈하고 막국수 먹고 오셨나봐요 ㅋㅋㅋ",
          "original_form": "방금 보쌈하고 막국수 먹고 오셨나봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019310.1.11",
          "form": "집에서 방금 다 먹었어요!",
          "original_form": "집에서 방금 다 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019310.1.12",
          "form": "메뉴 선택 진짜 훌륭하네요..",
          "original_form": "메뉴 선택 진짜 훌륭하네요..",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019310.1.13",
          "form": "남김 없이 다 먹었어요?",
          "original_form": "남김 없이 다 먹었어요?",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019310.1.14",
          "form": "네네! 막숙수에 풀떼기 조금 남겼네요ㅋㅋㅌ",
          "original_form": "네네! 막숙수에 풀떼기 조금 남겼네요ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019310.1.15",
          "form": "대단하세요.. 얼마 지불하셨어요?",
          "original_form": "대단하세요.. 얼마 지불하셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019310.1.16",
          "form": "5만원 좀 안 됐어요!",
          "original_form": "5만원 좀 안 됐어요!",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019310.1.17",
          "form": "와우 포장할인 있을텐데 그거 해도 5만원이에요?ㅠㅠ",
          "original_form": "와우 포장할인 있을텐데 그거 해도 5만원이에요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019310.1.18",
          "form": "대짜에 고기를 더 추가해서여....ㅎㅎ",
          "original_form": "대짜에 고기를 더 추가해서여....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019310.1.19",
          "form": "그러니까.. 지금 대짜에 고기 추가해서.. 그걸 막국수까지 혼자 다 드셨다는 거죠?",
          "original_form": "그러니까.. 지금 대짜에 고기 추가해서.. 그걸 막국수까지 혼자 다 드셨다는 거죠?",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019310.1.20",
          "form": "풀떼기만 조금 남기고..?",
          "original_form": "풀떼기만 조금 남기고..?",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019310.1.21",
          "form": "아뇨아뇨!! 가족과 먹은거에요.!!! 물론 제가 가장 많이 먹긴 했지만요!!!",
          "original_form": "아뇨아뇨!! 가족과 먹은거에요.!!! 물론 제가 가장 많이 먹긴 했지만요!!!",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019310.1.22",
          "form": "아 진짜 깜짝 놀랐어요",
          "original_form": "아 진짜 깜짝 놀랐어요",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019310.1.23",
          "form": "무슨 푸드파이터인 줄 알고...",
          "original_form": "무슨 푸드파이터인 줄 알고...",
