      "utterance": [
        {
          "id": "MDRW2100034218.1.1",
          "form": "혹시 이상형있으세요?",
          "original_form": "혹시 이상형있으세요?",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100034218.1.2",
          "form": "ㅋㅋ저는 웃을때 예쁜사람이랑 착한사람 좋아해요!ㅎㅎ",
          "original_form": "ㅋㅋ저는 웃을때 예쁜사람이랑 착한사람 좋아해요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100034218.1.3",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100034218.1.4",
          "form": "음 저는 그냥 잘생긴 사람이요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "음 저는 그냥 잘생긴 사람이요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034218.1.5",
          "form": "이상형은 이상형이니..ㅎㅎ",
          "original_form": "이상형은 이상형이니..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034218.1.6",
          "form": "잘생긴게 최고긴하죠ㅋㅋㅋㅋ",
          "original_form": "잘생긴게 최고긴하죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034218.1.7",
          "form": "키는 안보세요?",
          "original_form": "키는 안보세요?",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034218.1.8",
          "form": "맞아요 ㅋㅋㅋㅋ 흔치않아서 그렇지 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ 흔치않아서 그렇지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034218.1.9",
          "form": "음 크면 좋긴한데 제가 작은편인지라",
          "original_form": "음 크면 좋긴한데 제가 작은편인지라",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034218.1.10",
          "form": "키는 작아도 차라리 잘생긴게 좋더라구요 ㅋㅋㅋ",
          "original_form": "키는 작아도 차라리 잘생긴게 좋더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034218.1.11",
          "form": "키가 몇이신데요?",
          "original_form": "키가 몇이신데요?",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034218.1.12",
          "form": "아ㅋㅋㅋㅋ저는 작으면 잘생겨도 좀 그렇더라구요ㅠ",
          "original_form": "아ㅋㅋㅋㅋ저는 작으면 잘생겨도 좀 그렇더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034218.1.13",
          "form": "전 156이에요 ㅋㅋㅋ",
          "original_form": "전 156이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034218.1.14",
          "form": "그런분이 많더라구요!",
          "original_form": "그런분이 많더라구요!",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034218.1.15",
          "form": "전 다 저같을줄 알았는데 ㅋㅋㅋ",
          "original_form": "전 다 저같을줄 알았는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034218.1.16",
          "form": "근데 꼭 잘생기신분들이 작은편이시더라구요ㅠ",
          "original_form": "근데 꼭 잘생기신분들이 작은편이시더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034218.1.17",
          "form": "제 키가 조금 큰편이여서 키를 더 보는걸수도 있나봐요",
          "original_form": "제 키가 조금 큰편이여서 키를 더 보는걸수도 있나봐요",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034218.1.18",
          "form": "사람마다 보는게 다르니까요 뭐!",
          "original_form": "사람마다 보는게 다르니까요 뭐!",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100034218.1.19",
          "form": "헉 그래요? ㅋㅋㅋㅋ",
          "original_form": "헉 그래요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100034218.1.20",
          "form": "어느정도부터가 크다고 생각하세요?",
          "original_form": "어느정도부터가 크다고 생각하세요?",
          "speaker_id": "1",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100034218.1.21",
          "form": "제가봤던 잘생긴사람은 거의 그렇더라구요ㅋㅋ",
          "original_form": "제가봤던 잘생긴사람은 거의 그렇더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100034218.1.22",
          "form": "음..크다라는 말이 나오려면 그래도 180은 넘어야 크게 보는거같아요..ㅋㅋ",
          "original_form": "음..크다라는 말이 나오려면 그래도 180은 넘어야 크게 보는거같아요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100034218.1.23",
          "form": "그건 그래요 ㅋㅋㅋ8자가 붙어야 ㅋㅋㅋ",
          "original_form": "그건 그래요 ㅋㅋㅋ8자가 붙어야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100034218.1.24",
          "form": "손은 혹시 안보시나요?",
          "original_form": "손은 혹시 안보시나요?",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034218.1.25",
          "form": "저는 손도 봐가지구ㅋㅋ\r\n손이 너무 안예쁘면 그것도 또 마음이 안가더라구요ㅋㅋ",
          "original_form": "저는 손도 봐가지구ㅋㅋ\r\n손이 너무 안예쁘면 그것도 또 마음이 안가더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034218.1.26",
          "form": "이러니까  엄청 눈 높아보이네요ㅋㅋ",
          "original_form": "이러니까  엄청 눈 높아보이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100034218.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 손예쁜 사람 좋아하는 분들 많으시더라고요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 손예쁜 사람 좋아하는 분들 많으시더라고요",
          "speaker_id": "1",
          "time": "20210827 21:30"
        }
      ]
