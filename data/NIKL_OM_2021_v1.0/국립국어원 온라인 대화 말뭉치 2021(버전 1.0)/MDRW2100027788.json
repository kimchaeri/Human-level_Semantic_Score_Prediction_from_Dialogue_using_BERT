      "utterance": [
        {
          "id": "MDRW2100027788.1.1",
          "form": "안녕하세요 파르페 좋아하시나용?",
          "original_form": "안녕하세요 파르페 좋아하시나용?",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027788.1.2",
          "form": "아뇨 특별히 좋아하진 않아요",
          "original_form": "아뇨 특별히 좋아하진 않아요",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027788.1.3",
          "form": "앗 그렇군요 좋아하는 과일 있으신가요?",
          "original_form": "앗 그렇군요 좋아하는 과일 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027788.1.4",
          "form": "살구 제일 좋아해요",
          "original_form": "살구 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027788.1.5",
          "form": "어떤 과일 좋아하세요?",
          "original_form": "어떤 과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027788.1.6",
          "form": "살구맛있죠 저는 포도요!",
          "original_form": "살구맛있죠 저는 포도요!",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027788.1.7",
          "form": "저도 청포도 좋아해요! 포도 종류는 다 좋아하시나요?",
          "original_form": "저도 청포도 좋아해요! 포도 종류는 다 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027788.1.8",
          "form": "건포도 빼고는 다좋은거같아요ㅋㅋㅋ",
          "original_form": "건포도 빼고는 다좋은거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100027788.1.9",
          "form": "아 저도 건포도는... 너무 달아서 못 먹겠어요ㅋㅋㅋ",
          "original_form": "아 저도 건포도는... 너무 달아서 못 먹겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100027788.1.10",
          "form": "그쵸ㅋㅋㅋ 사실 식감도 별로에요ㅎ",
          "original_form": "그쵸ㅋㅋㅋ 사실 식감도 별로에요ㅎ",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100027788.1.11",
          "form": "저도요ㅋㅋㅋ 빵에 박혀 있는 것도 다 빼서 먹어요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ 빵에 박혀 있는 것도 다 빼서 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100027788.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도요ㅋㅋㅋ아예 안들은 빵이 제일이죠!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도요ㅋㅋㅋ아예 안들은 빵이 제일이죠!",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100027788.1.13",
          "form": "맞아요ㅋㅋ 그래도 가끔 건포도 박힌 모카빵 받으면 열심히 골라 먹어요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋ 그래도 가끔 건포도 박힌 모카빵 받으면 열심히 골라 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100027788.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ그쵸 안먹을순 없으니깐!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그쵸 안먹을순 없으니깐!",
