      "utterance": [
        {
          "id": "MDRW2100038883.1.1",
          "form": "네네 그럼요",
          "original_form": "네네 그럼요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038883.1.2",
          "form": "개의치마세요",
          "original_form": "개의치마세요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038883.1.3",
          "form": "감사합니다^^",
          "original_form": "감사합니다^^",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038883.1.4",
          "form": "좋아하는 공연 있으세요?",
          "original_form": "좋아하는 공연 있으세요?",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100038883.1.5",
          "form": "저 얼마 전에 트로이의 여인들 인터넷으로 봤어요",
          "original_form": "저 얼마 전에 트로이의 여인들 인터넷으로 봤어요",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100038883.1.6",
          "form": "공연이나 전시회 좋아하세요?",
          "original_form": "공연이나 전시회 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100038883.1.7",
          "form": "공연도 즐겨보시네요~",
          "original_form": "공연도 즐겨보시네요~",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038883.1.8",
          "form": "전 정말 옛날에",
          "original_form": "전 정말 옛날에",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038883.1.9",
          "form": "연극을 봤는데 엄청 재미있었어",
          "original_form": "연극을 봤는데 엄청 재미있었어",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038883.1.10",
          "form": "그러셨군요",
          "original_form": "그러셨군요",
          "speaker_id": "2",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038883.1.11",
          "form": "연극 매력있죠",
          "original_form": "연극 매력있죠",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038883.1.12",
          "form": "너무 재미있기 몇 번 보다가",
          "original_form": "너무 재미있기 몇 번 보다가",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038883.1.13",
          "form": "어느 순간 사는 게 바빠",
          "original_form": "어느 순간 사는 게 바빠",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038883.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038883.1.15",
          "form": "안 가게 되더라구요ㅠ",
          "original_form": "안 가게 되더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038883.1.16",
          "form": "바쁘면 못 챙겨봐요",
          "original_form": "바쁘면 못 챙겨봐요",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100038883.1.17",
          "form": "뭔가 월급이 쪼들리면 문화생활부터 끊게 되잖아요ㅋㅋ",
          "original_form": "뭔가 월급이 쪼들리면 문화생활부터 끊게 되잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100038883.1.18",
          "form": "그때부터 못 봤던 것 같아요;;",
          "original_form": "그때부터 못 봤던 것 같아요;;",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100038883.1.19",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100038883.1.20",
          "form": "시간적 금적적 여유가 있어야",
          "original_form": "시간적 금적적 여유가 있어야",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100038883.1.21",
          "form": "지금은 또 시절이 이러니..",
          "original_form": "지금은 또 시절이 이러니..",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100038883.1.22",
          "form": "문화생활이 가능해요",
          "original_form": "문화생활이 가능해요",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100038883.1.23",
          "form": "이래저래 문화생활이랑 안 맞나 봐요ㅋㅋ",
          "original_form": "이래저래 문화생활이랑 안 맞나 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100038883.1.24",
          "form": "맞아요 더 힘들죠",
          "original_form": "맞아요 더 힘들죠",
          "speaker_id": "2",
          "time": "20210902 22:41"
