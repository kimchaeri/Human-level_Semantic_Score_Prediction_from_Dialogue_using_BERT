      "utterance": [
        {
          "id": "MDRW2100033728.1.1",
          "form": "난 오늘 녹차를 마셨어",
          "original_form": "난 오늘 녹차를 마셨어",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033728.1.2",
          "form": "넌 녹차를 싫어하지?",
          "original_form": "넌 녹차를 싫어하지?",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033728.1.3",
          "form": "녹차는 무슨맛으로 먹니",
          "original_form": "녹차는 무슨맛으로 먹니",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033728.1.4",
          "form": "녹차맛.",
          "original_form": "녹차맛.",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.5",
          "form": "으 정말 나는 그 찻잎맛이 너무 싫어",
          "original_form": "으 정말 나는 그 찻잎맛이 너무 싫어",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.6",
          "form": "떫지않아?",
          "original_form": "떫지않아?",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.7",
          "form": "흠 너의 취향 존중해",
          "original_form": "흠 너의 취향 존중해",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.8",
          "form": "많이 떫지 않아",
          "original_form": "많이 떫지 않아",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.9",
          "form": "너는 아기혀구나",
          "original_form": "너는 아기혀구나",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.10",
          "form": "하지만 아주 달콤한 녹차아이스크림 정도는 나도 먹어",
          "original_form": "하지만 아주 달콤한 녹차아이스크림 정도는 나도 먹어",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.11",
          "form": "가짜녹차는 좋아해",
          "original_form": "가짜녹차는 좋아해",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.12",
          "form": "달콤하니까.",
          "original_form": "달콤하니까.",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.13",
          "form": "아무래도.",
          "original_form": "아무래도.",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.14",
          "form": "우리네는 그런것도 다 포용해.",
          "original_form": "우리네는 그런것도 다 포용해.",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033728.1.15",
          "form": "우리네는 녹차 사랑모임이니?",
          "original_form": "우리네는 녹차 사랑모임이니?",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.16",
          "form": "응. 암암리에 전국에 퍼져있어.",
          "original_form": "응. 암암리에 전국에 퍼져있어.",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.17",
          "form": "그럼 너는 녹차로만든 어떤걸 좋아해?",
          "original_form": "그럼 너는 녹차로만든 어떤걸 좋아해?",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.18",
          "form": "물론 거짓말이야 하하.",
          "original_form": "물론 거짓말이야 하하.",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.19",
          "form": "나는 녹차로 만든 건 시중에 나온건 다 좋아.",
          "original_form": "나는 녹차로 만든 건 시중에 나온건 다 좋아.",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.20",
          "form": "흠 정말 녹차를 좋아하는구나",
          "original_form": "흠 정말 녹차를 좋아하는구나",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.21",
          "form": "제품 하나만 제시해줘.",
          "original_form": "제품 하나만 제시해줘.",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.22",
          "form": "나중에 보성에 같이 가보자",
          "original_form": "나중에 보성에 같이 가보자",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.23",
          "form": "응 싫지않아.",
          "original_form": "응 싫지않아.",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.24",
          "form": "보성 덥더라",
          "original_form": "보성 덥더라",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033728.1.25",
          "form": "가본적있니?",
          "original_form": "가본적있니?",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100033728.1.26",
          "form": "찻잎 따서 가져가지도 못해",
          "original_form": "찻잎 따서 가져가지도 못해",
          "speaker_id": "2",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100033728.1.27",
          "form": "응.",
          "original_form": "응.",
          "speaker_id": "2",
          "time": "20210824 23:47"
        }
      ]
    }
  ]
