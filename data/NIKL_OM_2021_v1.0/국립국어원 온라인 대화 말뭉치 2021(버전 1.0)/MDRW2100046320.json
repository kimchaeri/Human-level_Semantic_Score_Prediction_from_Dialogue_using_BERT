      "utterance": [
        {
          "id": "MDRW2100046320.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 22:05"
        },
        {
          "id": "MDRW2100046320.1.2",
          "form": "안녕하세요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "안녕하세요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 09:10"
        },
        {
          "id": "MDRW2100046320.1.3",
          "form": "오늘 출근하면서 느낀거지만",
          "original_form": "오늘 출근하면서 느낀거지만",
          "speaker_id": "2",
          "time": "20211020 09:10"
        },
        {
          "id": "MDRW2100046320.1.4",
          "form": "가을은 갔네요..",
          "original_form": "가을은 갔네요..",
          "speaker_id": "2",
          "time": "20211020 09:10"
        },
        {
          "id": "MDRW2100046320.1.5",
          "form": "크크",
          "original_form": "크크",
          "speaker_id": "2",
          "time": "20211020 09:10"
        },
        {
          "id": "MDRW2100046320.1.6",
          "form": "좋은 아침이예요",
          "original_form": "좋은 아침이예요",
          "speaker_id": "1",
          "time": "20211020 10:04"
        },
        {
          "id": "MDRW2100046320.1.7",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 10:04"
        },
        {
          "id": "MDRW2100046320.1.8",
          "form": "아침에 안추우셨나요~?",
          "original_form": "아침에 안추우셨나요~?",
          "speaker_id": "2",
          "time": "20211020 10:04"
        },
        {
          "id": "MDRW2100046320.1.9",
          "form": "날씨가 추워져서 가을인지 겨울인지 헷갈리네요",
          "original_form": "날씨가 추워져서 가을인지 겨울인지 헷갈리네요",
          "speaker_id": "1",
          "time": "20211020 10:04"
        },
        {
          "id": "MDRW2100046320.1.10",
          "form": "확실히 가을은 아닌거같아요...",
          "original_form": "확실히 가을은 아닌거같아요...",
          "speaker_id": "2",
          "time": "20211020 10:05"
        },
        {
          "id": "MDRW2100046320.1.11",
          "form": "오늘 패딩입었는데 적당하더라구요",
          "original_form": "오늘 패딩입었는데 적당하더라구요",
          "speaker_id": "2",
          "time": "20211020 10:06"
        },
        {
          "id": "MDRW2100046320.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 10:06"
        },
        {
          "id": "MDRW2100046320.1.13",
          "form": "10월달인데 패딩이라니ㅠㅠ",
          "original_form": "10월달인데 패딩이라니ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 10:06"
        },
        {
          "id": "MDRW2100046320.1.14",
          "form": "전 가을옷 꺼내놓고 입지도 못했어요 하나도",
          "original_form": "전 가을옷 꺼내놓고 입지도 못했어요 하나도",
          "speaker_id": "1",
          "time": "20211020 10:19"
        },
        {
          "id": "MDRW2100046320.1.15",
          "form": "저두..",
          "original_form": "저두..",
          "speaker_id": "2",
          "time": "20211020 10:19"
        },
        {
          "id": "MDRW2100046320.1.16",
          "form": "제 트렌치는..",
          "original_form": "제 트렌치는..",
          "speaker_id": "2",
          "time": "20211020 10:19"
        },
        {
          "id": "MDRW2100046320.1.17",
          "form": "내년에 개시해야할듯..",
          "original_form": "내년에 개시해야할듯..",
          "speaker_id": "2",
          "time": "20211020 10:19"
        },
        {
          "id": "MDRW2100046320.1.18",
          "form": "흑ㅎ그",
          "original_form": "흑ㅎ그",
          "speaker_id": "2",
          "time": "20211020 10:19"
        },
        {
          "id": "MDRW2100046320.1.19",
          "form": "가장 좋아하는",
          "original_form": "가장 좋아하는",
          "speaker_id": "2",
          "time": "20211020 10:20"
        },
        {
          "id": "MDRW2100046320.1.20",
          "form": "계절이 있나요?",
          "original_form": "계절이 있나요?",
          "speaker_id": "2",
          "time": "20211020 10:20"
        },
        {
          "id": "MDRW2100046320.1.21",
          "form": "저는 겨울에 태어나서 겨울을 좋아합니다",
          "original_form": "저는 겨울에 태어나서 겨울을 좋아합니다",
          "speaker_id": "1",
          "time": "20211020 10:20"
        },
        {
          "id": "MDRW2100046320.1.22",
          "form": "몸에 열이 많아서 추운 겨울이 좋아요",
          "original_form": "몸에 열이 많아서 추운 겨울이 좋아요",
          "speaker_id": "1",
          "time": "20211020 10:20"
        },
        {
          "id": "MDRW2100046320.1.23",
          "form": "다이어트하기에 가장 적당하기도 하고요",
          "original_form": "다이어트하기에 가장 적당하기도 하고요",
          "speaker_id": "1",
          "time": "20211020 10:20"
        },
        {
          "id": "MDRW2100046320.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20211020 10:21"
        },
        {
          "id": "MDRW2100046320.1.25",
          "form": "추위를 많이 안타시나봐요!",
          "original_form": "추위를 많이 안타시나봐요!",
          "speaker_id": "2",
          "time": "20211020 10:21"
        },
        {
          "id": "MDRW2100046320.1.26",
          "form": "지방이 두꺼워서 ㅠㅠ",
          "original_form": "지방이 두꺼워서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20211020 10:22"
        },
        {
          "id": "MDRW2100046320.1.27",
          "form": "더운것보단 추운게 낫네요",
          "original_form": "더운것보단 추운게 낫네요",
          "speaker_id": "1",
          "time": "20211020 10:22"
        },
        {
          "id": "MDRW2100046320.1.28",
          "form": "하긴 저두",
          "original_form": "하긴 저두",
          "speaker_id": "2",
          "time": "20211020 10:22"
        },
        {
          "id": "MDRW2100046320.1.29",
          "form": "겨울이 막 좋은건아닌데",
          "original_form": "겨울이 막 좋은건아닌데",
          "speaker_id": "2",
          "time": "20211020 10:22"
        },
        {
          "id": "MDRW2100046320.1.30",
          "form": "여름보다 겨울이 나은거같아요",
          "original_form": "여름보다 겨울이 나은거같아요",
          "speaker_id": "2",
          "time": "20211020 10:22"
        },
        {
          "id": "MDRW2100046320.1.31",
          "form": "더운건 정말...일상생활이 불가능하게만들어요 ㅋㅋ",
          "original_form": "더운건 정말...일상생활이 불가능하게만들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 10:23"
        },
        {
          "id": "MDRW2100046320.1.32",
          "form": "추울때는 이불속에 들어가서 전기장판 키고 몸을 지지는게 가장 행복해요",
          "original_form": "추울때는 이불속에 들어가서 전기장판 키고 몸을 지지는게 가장 행복해요",
          "speaker_id": "1",
          "time": "20211020 10:23"
        },
        {
          "id": "MDRW2100046320.1.33",
          "form": "햐...... 전기장판 최고죠...........",
          "original_form": "햐...... 전기장판 최고죠...........",
          "speaker_id": "2",
          "time": "20211020 10:23"
        }
      ]
    }
