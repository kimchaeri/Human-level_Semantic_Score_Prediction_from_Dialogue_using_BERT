      "utterance": [
        {
          "id": "MDRW2100004595.1.1",
          "form": "학교 생활 즐거우신가요?",
          "original_form": "학교 생활 즐거우신가요?",
          "speaker_id": "1",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004595.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004595.1.3",
          "form": "다른 과 동기들은 별로인데",
          "original_form": "다른 과 동기들은 별로인데",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004595.1.4",
          "form": "친구들이랑은 친하고",
          "original_form": "친구들이랑은 친하고",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004595.1.5",
          "form": "과제가 많지만 않으면",
          "original_form": "과제가 많지만 않으면",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004595.1.6",
          "form": "강의도 들을 만 해요",
          "original_form": "강의도 들을 만 해요",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004595.1.7",
          "form": "학교 생활 즐거우세요?",
          "original_form": "학교 생활 즐거우세요?",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004595.1.8",
          "form": "저는 그저그럭이네요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "저는 그저그럭이네요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004595.1.9",
          "form": "학생때 공부 좀 더 할걸그랬나봐요",
          "original_form": "학생때 공부 좀 더 할걸그랬나봐요",
          "speaker_id": "1",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004595.1.10",
          "form": "나쁘진않은데 타 학교랑 보면 조금 차이가 나는 것 같기도하고",
          "original_form": "나쁘진않은데 타 학교랑 보면 조금 차이가 나는 것 같기도하고",
          "speaker_id": "1",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004595.1.11",
          "form": "아쉬운 부분이 있더라구요",
          "original_form": "아쉬운 부분이 있더라구요",
          "speaker_id": "1",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004595.1.12",
          "form": "아 대학생이시구나",
          "original_form": "아 대학생이시구나",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004595.1.13",
          "form": "근데 편입까지 할 생각은 없어서",
          "original_form": "근데 편입까지 할 생각은 없어서",
          "speaker_id": "1",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004595.1.14",
          "form": "졸업장따려고 다니구있어요",
          "original_form": "졸업장따려고 다니구있어요",
          "speaker_id": "1",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004595.1.15",
          "form": "학리적이네요",
          "original_form": "학리적이네요",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004595.1.16",
          "form": "과는 괜찮으세요?",
          "original_form": "과는 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004595.1.17",
          "form": "과는 괜찮은것같아요 그나마 다행이죠 ㅋㅋㅋ",
          "original_form": "과는 괜찮은것같아요 그나마 다행이죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004595.1.18",
          "form": "과까지 별로였으면 정말 바로 편입하거나 자퇴했을거같네요",
          "original_form": "과까지 별로였으면 정말 바로 편입하거나 자퇴했을거같네요",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004595.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.20",
          "form": "그럼 다행이에요",
          "original_form": "그럼 다행이에요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.21",
          "form": "수업이 그나마 들을 만 했나봐요",
          "original_form": "수업이 그나마 들을 만 했나봐요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.22",
          "form": "주로 뭐 배웠어요?",
          "original_form": "주로 뭐 배웠어요?",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.23",
          "form": "전 디자인과라 디자인 위주로 배웠어요 재밌더라구요",
          "original_form": "전 디자인과라 디자인 위주로 배웠어요 재밌더라구요",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.24",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.25",
          "form": "프로그램도 배우고 마케팅쪽도 배우고",
          "original_form": "프로그램도 배우고 마케팅쪽도 배우고",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.26",
          "form": "예대 다니셨네요",
          "original_form": "예대 다니셨네요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.27",
          "form": "재밌었네요",
          "original_form": "재밌었네요",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004595.1.28",
          "form": "맞아요?",
          "original_form": "맞아요?",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004595.1.29",
          "form": "멋있어요",
          "original_form": "멋있어요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004595.1.30",
          "form": "넵 ㅎㅎ",
          "original_form": "넵 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004595.1.31",
          "form": "전 그림 잘 그리는 사람들이 제일 신기해요",
          "original_form": "전 그림 잘 그리는 사람들이 제일 신기해요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004595.1.32",
          "form": "제가 진짜 못 그려서",
          "original_form": "제가 진짜 못 그려서",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004595.1.33",
          "form": "가장 재밌었던 수업이 뭐예요?",
          "original_form": "가장 재밌었던 수업이 뭐예요?",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004595.1.34",
          "form": "근데 생각보다 그림이랑 디자인이랑 별개인거같더라구요",
          "original_form": "근데 생각보다 그림이랑 디자인이랑 별개인거같더라구요",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004595.1.35",
          "form": "도움은 되겠지만!",
          "original_form": "도움은 되겠지만!",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004595.1.36",
          "form": "저는 요새 서비스디자인이란게 재밌더라구요",
          "original_form": "저는 요새 서비스디자인이란게 재밌더라구요",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004595.1.37",
          "form": "사람들에게 무슨 경험을 줄 지 디자인하는게 어렵고 재밌었네요 ㅎㅎ",
          "original_form": "사람들에게 무슨 경험을 줄 지 디자인하는게 어렵고 재밌었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004595.1.38",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004595.1.39",
          "form": "전문적이네요",
          "original_form": "전문적이네요",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004595.1.40",
          "form": "어려운데 재밌는 게",
          "original_form": "어려운데 재밌는 게",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004595.1.41",
          "form": "진짜 적성인가 봐요",
          "original_form": "진짜 적성인가 봐요",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004595.1.42",
          "form": "그럼 전공 살려서 취직하세요?",
          "original_form": "그럼 전공 살려서 취직하세요?",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.43",
          "form": "네 그럴려구요!",
          "original_form": "네 그럴려구요!",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.44",
          "form": "근데 취직이 될까 모르겠네요",
          "original_form": "근데 취직이 될까 모르겠네요",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.45",
          "form": "암담하네요 ㅠㅠ",
          "original_form": "암담하네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.46",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.47",
          "form": "요즘 취직이 힘들잖아요ㅜㅜㅜㅜ",
          "original_form": "요즘 취직이 힘들잖아요ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.48",
          "form": "맞아요 ㅠㅠ 가뜩이나 일자리가 없는데",
          "original_form": "맞아요 ㅠㅠ 가뜩이나 일자리가 없는데",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.49",
          "form": "그럼 지금 방학이세요?",
          "original_form": "그럼 지금 방학이세요?",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.50",
          "form": "더 없어진거같아요",
          "original_form": "더 없어진거같아요",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004595.1.51",
          "form": "네 ㅎㅎ 이제 일주일 남았지만요 ㅠ",
          "original_form": "네 ㅎㅎ 이제 일주일 남았지만요 ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004595.1.52",
          "form": "ㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004595.1.53",
          "form": "특히 인문계가",
          "original_form": "특히 인문계가",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004595.1.54",
          "form": "진짜 안 뽑아요 요즘",
          "original_form": "진짜 안 뽑아요 요즘",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004595.1.55",
          "form": "언제 개강하세요?",
          "original_form": "언제 개강하세요?",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004595.1.56",
          "form": "아이고 ㅠㅠ..  큰일났네요 곧 취업인데",
          "original_form": "아이고 ㅠㅠ..  큰일났네요 곧 취업인데",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004595.1.57",
          "form": "휴학이라도 해야하나",
          "original_form": "휴학이라도 해야하나",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004595.1.58",
          "form": "30일날 개강해요",
          "original_form": "30일날 개강해요",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004595.1.59",
          "form": "하..ㅠㅠ",
          "original_form": "하..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004595.1.60",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004595.1.61",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004595.1.62",
