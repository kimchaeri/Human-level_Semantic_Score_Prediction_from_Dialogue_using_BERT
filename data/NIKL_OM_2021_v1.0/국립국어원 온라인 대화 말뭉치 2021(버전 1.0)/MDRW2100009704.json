        {
          "id": "MDRW2100009704.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210823 07:21"
        },
        {
          "id": "MDRW2100009704.1.2",
          "form": "네 무척많이오네요 거긴어떤가요",
          "original_form": "네 무척많이오네요 거긴어떤가요",
          "speaker_id": "2",
          "time": "20210823 07:38"
        },
        {
          "id": "MDRW2100009704.1.3",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210823 07:39"
        },
        {
          "id": "MDRW2100009704.1.4",
          "form": "이번주내내 비소식이던데...",
          "original_form": "이번주내내 비소식이던데...",
          "speaker_id": "2",
          "time": "20210823 07:39"
        },
        {
          "id": "MDRW2100009704.1.5",
          "form": "태풍도요",
          "original_form": "태풍도요",
          "speaker_id": "1",
          "time": "20210823 07:40"
        },
        {
          "id": "MDRW2100009704.1.6",
          "form": "아 태풍까지요...심란하네요",
          "original_form": "아 태풍까지요...심란하네요",
          "speaker_id": "2",
          "time": "20210823 07:45"
        },
        {
          "id": "MDRW2100009704.1.7",
          "form": "피해없이지나갔으면",
          "original_form": "피해없이지나갔으면",
          "speaker_id": "1",
          "time": "20210823 07:46"
        },
        {
          "id": "MDRW2100009704.1.8",
          "form": "출퇴근길도 걱정이네요",
          "original_form": "출퇴근길도 걱정이네요",
          "speaker_id": "2",
          "time": "20210823 08:05"
        },
        {
          "id": "MDRW2100009704.1.9",
          "form": "저번에는 앞이 안보이더라구요",
          "original_form": "저번에는 앞이 안보이더라구요",
          "speaker_id": "1",
          "time": "20210823 08:06"
        },
        {
          "id": "MDRW2100009704.1.10",
          "form": "이번주 지나면 가을이 훅 다가오겠네요",
          "original_form": "이번주 지나면 가을이 훅 다가오겠네요",
          "speaker_id": "2",
          "time": "20210823 08:21"
