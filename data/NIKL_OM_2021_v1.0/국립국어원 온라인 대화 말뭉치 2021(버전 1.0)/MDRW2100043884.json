      "utterance": [
        {
          "id": "MDRW2100043884.1.1",
          "form": "좋네",
          "original_form": "좋네",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.2",
          "form": "이제 식이조절도 좀 해야겠다 ㅋㅋ",
          "original_form": "이제 식이조절도 좀 해야겠다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.3",
          "form": "그래 살찌는 거 먹지 마라",
          "original_form": "그래 살찌는 거 먹지 마라",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.4",
          "form": "웅웅 곤약같은거 좀 사야겠다",
          "original_form": "웅웅 곤약같은거 좀 사야겠다",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.5",
          "form": "곤약을 왜",
          "original_form": "곤약을 왜",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.6",
          "form": "곤약이 칼로리 없으니까 살빠지는데 좋지",
          "original_form": "곤약이 칼로리 없으니까 살빠지는데 좋지",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.7",
          "form": "곤약 맛이 없잖아",
          "original_form": "곤약 맛이 없잖아",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.8",
          "form": "요리 잘 하면 맛나지",
          "original_form": "요리 잘 하면 맛나지",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.9",
          "form": "곤약 밥도 할 수 있나",
          "original_form": "곤약 밥도 할 수 있나",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.10",
          "form": "하는방법 찾아서 하면되지 ㅋ",
          "original_form": "하는방법 찾아서 하면되지 ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043884.1.11",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043884.1.12",
          "form": "웅웅 같이 먹고 살빼자 ㅎㅎ",
          "original_form": "웅웅 같이 먹고 살빼자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043884.1.13",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043884.1.14",
          "form": "컵라면에 끓인 물을 좀 늦게 부었더니 잘안익네",
          "original_form": "컵라면에 끓인 물을 좀 늦게 부었더니 잘안익네",
