      "utterance": [
        {
          "id": "MDRW2100011033.1.1",
          "form": "너 하나님의교회라는 곳 아니?",
          "original_form": "너 하나님의교회라는 곳 아니?",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.2",
          "form": "세상에 거기 사이비잖아",
          "original_form": "세상에 거기 사이비잖아",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.3",
          "form": "친구가 권하더라도 절대 가면안돼!",
          "original_form": "친구가 권하더라도 절대 가면안돼!",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.4",
          "form": "어머. 이미 알고있었구나.",
          "original_form": "어머. 이미 알고있었구나.",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.5",
          "form": "나는 근데 한번 가본적있어",
          "original_form": "나는 근데 한번 가본적있어",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.6",
          "form": "아무리 종교의 자유가 있다고해도 포교활동이 너무하지않아?",
          "original_form": "아무리 종교의 자유가 있다고해도 포교활동이 너무하지않아?",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.7",
          "form": "어릴때 친구가 나를 꼬아낸 적 있어",
          "original_form": "어릴때 친구가 나를 꼬아낸 적 있어",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.8",
          "form": "맞아 정말 위험했지",
          "original_form": "맞아 정말 위험했지",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.9",
          "form": "이상한..친구구나.",
          "original_form": "이상한..친구구나.",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.10",
          "form": "어릴때라서 잘 몰랐지만",
          "original_form": "어릴때라서 잘 몰랐지만",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.11",
          "form": "친구가 널 배배꼰거야?",
          "original_form": "친구가 널 배배꼰거야?",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011033.1.12",
          "form": "ㅠㅠ아니 미안해 오타야",
          "original_form": "ㅠㅠ아니 미안해 오타야",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011033.1.13",
          "form": "꾀어냈어",
          "original_form": "꾀어냈어",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011033.1.14",
          "form": "응.. 놀랐어!",
          "original_form": "응.. 놀랐어!",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011033.1.15",
          "form": "아무리 사이비래도 사람을 꼬아선 안 돼.",
          "original_form": "아무리 사이비래도 사람을 꼬아선 안 돼.",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011033.1.16",
          "form": "근데 사이비들중에 사람들을 때리는 곳도 있댜",
          "original_form": "근데 사이비들중에 사람들을 때리는 곳도 있댜",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011033.1.17",
          "form": "조심하자",
          "original_form": "조심하자",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100011033.1.18",
          "form": "구마하려고..?",
          "original_form": "구마하려고..?",
