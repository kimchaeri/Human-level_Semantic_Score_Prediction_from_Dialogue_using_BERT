      "utterance": [
        {
          "id": "MDRW2100040824.1.1",
          "form": "네~ 김치찌개에 냠냠",
          "original_form": "네~ 김치찌개에 냠냠",
          "speaker_id": "2",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100040824.1.2",
          "form": "와 김치찌개 맛있었겠어요!!",
          "original_form": "와 김치찌개 맛있었겠어요!!",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100040824.1.3",
          "form": "갈치구어서 냠냠햇어여 ㅎㅎ",
          "original_form": "갈치구어서 냠냠햇어여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100040824.1.4",
          "form": "아침부터 잘 차려드셨네요",
          "original_form": "아침부터 잘 차려드셨네요",
          "speaker_id": "1",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100040824.1.5",
          "form": "아침식사에 갈치구이까지!!",
          "original_form": "아침식사에 갈치구이까지!!",
          "speaker_id": "1",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100040824.1.6",
          "form": "집에 어린 상전님이 계셔서",
          "original_form": "집에 어린 상전님이 계셔서",
          "speaker_id": "2",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100040824.1.7",
          "form": "생선 픽수에요 ㅎㅎ",
          "original_form": "생선 픽수에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100040824.1.8",
          "form": "필수",
          "original_form": "필수",
          "speaker_id": "2",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100040824.1.9",
          "form": "아 아기가 있으시구나",
          "original_form": "아 아기가 있으시구나",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100040824.1.10",
          "form": "아이 있으면 식사 챙기는 것 더 신경쓰이시겠네요",
          "original_form": "아이 있으면 식사 챙기는 것 더 신경쓰이시겠네요",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100040824.1.11",
          "form": "ㅎㅎ 아무래도 아직 매운거 못먹어서 더그러네요",
          "original_form": "ㅎㅎ 아무래도 아직 매운거 못먹어서 더그러네요",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040824.1.12",
          "form": "그럼 어른 음식/아이 음식 또 따로 준비해안 하잖아요",
          "original_form": "그럼 어른 음식/아이 음식 또 따로 준비해안 하잖아요",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100040824.1.13",
          "form": "준비해야",
          "original_form": "준비해야",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100040824.1.14",
          "form": "아이가 밥은 잘 먹나요?",
          "original_form": "아이가 밥은 잘 먹나요?",
          "speaker_id": "1",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100040824.1.15",
          "form": "아뇨  잘안먹어요 ㅎ 속상",
          "original_form": "아뇨  잘안먹어요 ㅎ 속상",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100040824.1.16",
          "form": "ㅠㅠ아이가 잘 안먹으면 속상하죠",
          "original_form": "ㅠㅠ아이가 잘 안먹으면 속상하죠",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100040824.1.17",
          "form": "식사 메뉴 고민도 되고",
          "original_form": "식사 메뉴 고민도 되고",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100040824.1.18",
          "form": "먹는게 몇가지 안되서 \nㅎ 잘먹엇늠 한데 맘대로 안되네요",
          "original_form": "먹는게 몇가지 안되서 \nㅎ 잘먹엇늠 한데 맘대로 안되네요",
          "speaker_id": "2",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100040824.1.19",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100040824.1.20",
          "form": "크면 나아지겟죠 ㅎㅎ",
          "original_form": "크면 나아지겟죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100040824.1.21",
          "form": "제출",
          "original_form": "제출",
          "speaker_id": "2",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100040824.1.22",
          "form": "제 주변에도 아이 식사 때문에 걱정하는 분들이 많아서",
          "original_form": "제 주변에도 아이 식사 때문에 걱정하는 분들이 많아서",
