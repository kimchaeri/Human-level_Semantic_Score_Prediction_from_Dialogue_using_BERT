        {
          "id": "MDRW2100031366.1.1",
          "form": "전 안 먹어 봤어요",
          "original_form": "전 안 먹어 봤어요",
          "speaker_id": "2",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100031366.1.2",
          "form": "밀키스 아시나요",
          "original_form": "밀키스 아시나요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.3",
          "form": "그건 알아요",
          "original_form": "그건 알아요",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.4",
          "form": "그거 보다 원조에요",
          "original_form": "그거 보다 원조에요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.5",
          "form": "원 주라고요",
          "original_form": "원 주라고요",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.6",
          "form": "내 롯데가 삐 꼈어요",
          "original_form": "내 롯데가 삐 꼈어요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.7",
          "form": "롯데가 그럼 그렇죠",
          "original_form": "롯데가 그럼 그렇죠",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.8",
          "form": "훨씬 더 부드럽고 맛있어요",
          "original_form": "훨씬 더 부드럽고 맛있어요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.9",
          "form": "응 탄산은 안 센가요",
          "original_form": "응 탄산은 안 센가요",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.10",
          "form": "어 맞아요 탄산이 부드러워요",
          "original_form": "어 맞아요 탄산이 부드러워요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.11",
          "form": "마셔 보고 싶네요",
          "original_form": "마셔 보고 싶네요",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.12",
          "form": "향이 세지 않아서 너무 맛있어요",
          "original_form": "향이 세지 않아서 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031366.1.13",
          "form": "추천해 주셔서 감사해요",
          "original_form": "추천해 주셔서 감사해요",
