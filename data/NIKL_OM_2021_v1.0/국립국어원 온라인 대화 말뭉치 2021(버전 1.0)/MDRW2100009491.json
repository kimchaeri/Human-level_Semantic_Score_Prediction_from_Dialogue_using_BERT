        {
          "id": "MDRW2100009491.1.16",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009491.1.17",
          "form": "아박 맛있죠ㅋㅋㅋ",
          "original_form": "아박 맛있죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009491.1.18",
          "form": "헉 대박!",
          "original_form": "헉 대박!",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009491.1.19",
          "form": "남은 거 싸가지고 가고 그래요?",
          "original_form": "남은 거 싸가지고 가고 그래요?",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009491.1.20",
          "form": "으학ㅋㅋㅋ완전 가끔이예요",
          "original_form": "으학ㅋㅋㅋ완전 가끔이예요",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009491.1.21",
          "form": "케이크도 편의점처럼 유통기한이 있거등요",
          "original_form": "케이크도 편의점처럼 유통기한이 있거등요",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009491.1.22",
          "form": "그걸 상미라고 부르는데",
          "original_form": "그걸 상미라고 부르는데",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009491.1.23",
          "form": "주로 마감하는 사람이 가져간다고 하더라구요ㅋㅋ",
          "original_form": "주로 마감하는 사람이 가져간다고 하더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009491.1.24",
          "form": "상미 지나면 매니저님들이 챙겨주시기도 해요",
          "original_form": "상미 지나면 매니저님들이 챙겨주시기도 해요",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009491.1.25",
          "form": "저 마감이에요 히히",
          "original_form": "저 마감이에요 히히",
