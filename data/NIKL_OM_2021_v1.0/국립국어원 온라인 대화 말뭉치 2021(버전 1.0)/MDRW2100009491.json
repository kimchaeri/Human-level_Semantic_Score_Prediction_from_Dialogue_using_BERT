        },
        {
          "id": "MDRW2100009491.1.2",
          "form": "무슨 음식 좋아하시요?",
          "original_form": "무슨 음식 좋아하시요?",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009491.1.3",
          "form": "저는.. 피자?",
          "original_form": "저는.. 피자?",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009491.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009491.1.5",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009491.1.6",
          "form": "굽네치킨 고.추.바사삭 썼더니",
          "original_form": "굽네치킨 고.추.바사삭 썼더니",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009491.1.7",
          "form": "나쁜말은 안된다네요 ㅋㅋㅋㅋ",
          "original_form": "나쁜말은 안된다네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009491.1.8",
          "form": "아ㅋㅋㅋㅋ 그런ㅜㅜ",
          "original_form": "아ㅋㅋㅋㅋ 그런ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009491.1.9",
          "form": "카페 가는 건 좋아하세요?",
          "original_form": "카페 가는 건 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009491.1.10",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009491.1.11",
          "form": "빵도 좋아하구..",
          "original_form": "빵도 좋아하구..",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009491.1.12",
          "form": "투썸 좋아하세용?",
          "original_form": "투썸 좋아하세용?",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009491.1.13",
          "form": "네 좋아해요ㅋㅋㅋ",
          "original_form": "네 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009491.1.14",
          "form": "떠먹는 아이스박스!",
          "original_form": "떠먹는 아이스박스!",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009491.1.15",
          "form": "저 투썸 알바생이에욤",
          "original_form": "저 투썸 알바생이에욤",
          "speaker_id": "2",
          "time": "20210822 23:54"
