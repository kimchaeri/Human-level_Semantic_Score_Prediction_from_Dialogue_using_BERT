      "utterance": [
        {
          "id": "MDRW2100036364.1.1",
          "form": "시원하니 좋네요",
          "original_form": "시원하니 좋네요",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036364.1.2",
          "form": "전 국물을 넘좋아해요ㅠㅠ",
          "original_form": "전 국물을 넘좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036364.1.3",
          "form": "그러니 배가 문제가심각하지만 ㅠㅠ",
          "original_form": "그러니 배가 문제가심각하지만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100036364.1.4",
          "form": "찔리네요 하하하",
          "original_form": "찔리네요 하하하",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100036364.1.5",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100036364.1.6",
          "form": "많이 먹고 운동하면 되죵!",
          "original_form": "많이 먹고 운동하면 되죵!",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100036364.1.7",
          "form": "디저트는뭐좃아하세요?",
          "original_form": "디저트는뭐좃아하세요?",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100036364.1.8",
          "form": "운동넘시러요ㅠㅠㅍ ㅋㅋㅋㅋ",
          "original_form": "운동넘시러요ㅠㅠㅍ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100036364.1.9",
          "form": "다 좋아해요.... 진짜 다",
          "original_form": "다 좋아해요.... 진짜 다",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100036364.1.10",
          "form": "어머나ㅜㅜ",
          "original_form": "어머나ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100036364.1.11",
          "form": "그래도 특히 좋아하는건",
          "original_form": "그래도 특히 좋아하는건",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 뭘까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 뭘까요",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.13",
          "form": "타르트랑",
          "original_form": "타르트랑",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.14",
          "form": "오 ㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.15",
          "form": "아 그 프랑스 디저트인것같은데",
          "original_form": "아 그 프랑스 디저트인것같은데",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.16",
          "form": "저도 에그타르트 따뜻하게 먹우몀",
          "original_form": "저도 에그타르트 따뜻하게 먹우몀",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.17",
          "form": "진짜 맛잇어요ㅠㅠ",
          "original_form": "진짜 맛잇어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.18",
          "form": "컵에다가 겹겹이 쌓아서 먹는 음식 뭐죠",
          "original_form": "컵에다가 겹겹이 쌓아서 먹는 음식 뭐죠",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.19",
          "form": "파르페?",
          "original_form": "파르페?",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.20",
          "form": "맞아요ㅋㅋㅋ 에프에 돌려먹기!",
          "original_form": "맞아요ㅋㅋㅋ 에프에 돌려먹기!",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.21",
          "form": "어오오오오오",
          "original_form": "어오오오오오",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036364.1.23",
          "form": "갑자기 기억이 안나서ㅋㅋㅋㅋ",
          "original_form": "갑자기 기억이 안나서ㅋㅋㅋㅋ",
          "speaker_id": "1",
