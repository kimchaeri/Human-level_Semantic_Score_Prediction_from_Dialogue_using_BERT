        {
          "id": "MDRW2100041003.1.1",
          "form": "네 그게 좋을것같아요",
          "original_form": "네 그게 좋을것같아요",
          "speaker_id": "2",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100041003.1.2",
          "form": "어떤 게 고민이세요?",
          "original_form": "어떤 게 고민이세요?",
          "speaker_id": "2",
          "time": "20210905 15:06"
        },
        {
          "id": "MDRW2100041003.1.3",
          "form": "그럼 파운데이션은 뭐 쓰시나요?",
          "original_form": "그럼 파운데이션은 뭐 쓰시나요?",
          "speaker_id": "1",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100041003.1.4",
          "form": "아 저는 쿠션 써요",
          "original_form": "아 저는 쿠션 써요",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100041003.1.5",
          "form": "어디꺼요? 저 쿠션 유목민이예요",
          "original_form": "어디꺼요? 저 쿠션 유목민이예요",
          "speaker_id": "1",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100041003.1.6",
          "form": "아 저는 투쿨포스쿨 쓰는데 괜찮은거같아요",
          "original_form": "아 저는 투쿨포스쿨 쓰는데 괜찮은거같아요",
          "speaker_id": "2",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100041003.1.7",
          "form": "투쿨포스쿨이요? 찾아봐야겠어요 어떤면이 좋나요?",
          "original_form": "투쿨포스쿨이요? 찾아봐야겠어요 어떤면이 좋나요?",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041003.1.8",
          "form": "가볍고 커버도 잘되어요",
          "original_form": "가볍고 커버도 잘되어요",
          "speaker_id": "2",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100041003.1.9",
          "form": "가벼운데 커버도 잘되면 최고인데요",
          "original_form": "가벼운데 커버도 잘되면 최고인데요",
          "speaker_id": "1",
          "time": "20210906 09:48"
        },
        {
          "id": "MDRW2100041003.1.10",
          "form": "그쵸 한번 찾아보셔요~~~",
          "original_form": "그쵸 한번 찾아보셔요~~~",
          "speaker_id": "2",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100041003.1.11",
          "form": "지금 비비만 바르고 다녀서 쿠션이 시급합니다 ㅋㅋ",
          "original_form": "지금 비비만 바르고 다녀서 쿠션이 시급합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100041003.1.12",
          "form": "오 그러시구나 쿠션 좋아요",
          "original_form": "오 그러시구나 쿠션 좋아요",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041003.1.13",
          "form": "다릉거 또 뭐쓰세요 화장할때?",
          "original_form": "다릉거 또 뭐쓰세요 화장할때?",
