      "utterance": [
        {
          "id": "MDRW2100013501.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100013501.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100013501.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100013501.1.4",
          "form": "네 간단하게 빵먹었어요",
          "original_form": "네 간단하게 빵먹었어요",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100013501.1.5",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100013501.1.6",
          "form": "저는 국밥 먹었어요~",
          "original_form": "저는 국밥 먹었어요~",
          "speaker_id": "2",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100013501.1.7",
          "form": "빵에 잼 발라 드셨나요?",
          "original_form": "빵에 잼 발라 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100013501.1.8",
          "form": "아뇨. 앙금빵 먹었어요ㅋㅋ 커피랑해서요",
          "original_form": "아뇨. 앙금빵 먹었어요ㅋㅋ 커피랑해서요",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100013501.1.9",
          "form": "국밥 맛있겠네요",
          "original_form": "국밥 맛있겠네요",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100013501.1.10",
          "form": "갑자기 앙금빵 먹고싶네요 ㅎㅎ\r\n흰우유랑 먹는 거 좋아해서요 ㅎㅎ",
          "original_form": "갑자기 앙금빵 먹고싶네요 ㅎㅎ\r\n흰우유랑 먹는 거 좋아해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100013501.1.11",
          "form": "국밥 좋아하세요?",
          "original_form": "국밥 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100013501.1.12",
          "form": "네 저 한식 좋아해요",
          "original_form": "네 저 한식 좋아해요",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100013501.1.13",
          "form": "무슨 국밥 드셨어요?",
          "original_form": "무슨 국밥 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100013501.1.14",
          "form": "돼지국밥 먹었습니다!",
          "original_form": "돼지국밥 먹었습니다!",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100013501.1.15",
          "form": "맛있죠ㅋㅋ 전 순대국밥 좋아해요",
          "original_form": "맛있죠ㅋㅋ 전 순대국밥 좋아해요",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100013501.1.16",
          "form": "순대국밥도 순대가 탱글탱글해서 맛있죠 ㅋㅋㅋㅋ",
          "original_form": "순대국밥도 순대가 탱글탱글해서 맛있죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100013501.1.17",
          "form": "오늘 순대국밥이랑 같이 고민하긴 했어요 ㅋㅋㅋ",
          "original_form": "오늘 순대국밥이랑 같이 고민하긴 했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100013501.1.18",
          "form": "순대만 떠먹어도 좋은데 으깨서 풀어먹어도 맛있더라구요",
          "original_form": "순대만 떠먹어도 좋은데 으깨서 풀어먹어도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100013501.1.19",
          "form": "맞아요! 저도 으깨서 먹는 편이거든요 ㅋㅋ",
          "original_form": "맞아요! 저도 으깨서 먹는 편이거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100013501.1.20",
          "form": "으깨 먹는 분 잘 못봤는데 반갑네요 ㅎㅎ",
          "original_form": "으깨 먹는 분 잘 못봤는데 반갑네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:13"
        }
      ]
