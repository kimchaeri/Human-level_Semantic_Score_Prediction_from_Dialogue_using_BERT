      "utterance": [
        {
          "id": "MDRW2100034646.1.1",
          "form": "전 원래 늦게 자는 편이긴하지만",
          "original_form": "전 원래 늦게 자는 편이긴하지만",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034646.1.2",
          "form": "커피마시면 날새야해서 부담스럽더라구요",
          "original_form": "커피마시면 날새야해서 부담스럽더라구요",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034646.1.3",
          "form": "그 정도면 너무 곤란하겠어요ㅜㅜ",
          "original_form": "그 정도면 너무 곤란하겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100034646.1.4",
          "form": "네 ㅜㅜ 차라리",
          "original_form": "네 ㅜㅜ 차라리",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100034646.1.5",
          "form": "디카페인으로 바꿔서 마셔보셨나요?",
          "original_form": "디카페인으로 바꿔서 마셔보셨나요?",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100034646.1.6",
          "form": "새벽 서너시까진 나은데..",
          "original_form": "새벽 서너시까진 나은데..",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100034646.1.7",
          "form": "디카페인은 잘 와요!ㅋㅋㅋㅋ",
          "original_form": "디카페인은 잘 와요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100034646.1.8",
          "form": "근데 주변에 잘 안팔아서 슬퍼요 ㅠ",
          "original_form": "근데 주변에 잘 안팔아서 슬퍼요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100034646.1.9",
          "form": "전 그래서 원두 사서 직접 내려마시고 있어요",
          "original_form": "전 그래서 원두 사서 직접 내려마시고 있어요",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100034646.1.10",
          "form": "오 어떠세요??",
          "original_form": "오 어떠세요??",
          "speaker_id": "1",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100034646.1.11",
          "form": "디카페인으로 바꾸고 나니 좋더라구요!",
          "original_form": "디카페인으로 바꾸고 나니 좋더라구요!",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100034646.1.12",
          "form": "원두 선택의 종류는 제한적이지만요",
          "original_form": "원두 선택의 종류는 제한적이지만요",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100034646.1.13",
          "form": "오 저는 내려마신 적은 없는데 좋을거같아요!",
          "original_form": "오 저는 내려마신 적은 없는데 좋을거같아요!",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100034646.1.14",
          "form": "저는 사실 커피맛은 잘 몰라서 아무거나 마셔가지구 ㅋㅋㅋㅋ괜찮을거같아요",
          "original_form": "저는 사실 커피맛은 잘 몰라서 아무거나 마셔가지구 ㅋㅋㅋㅋ괜찮을거같아요",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034646.1.15",
          "form": "원할 때, 원하는 만큼 먹을 수 있으니 좋아요",
          "original_form": "원할 때, 원하는 만큼 먹을 수 있으니 좋아요",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034646.1.16",
          "form": "그게 특장점이겠네요 !",
          "original_form": "그게 특장점이겠네요 !",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034646.1.17",
          "form": "디카페인으로 하니 저녁에 마실 수 있는것도 좋구요",
          "original_form": "디카페인으로 하니 저녁에 마실 수 있는것도 좋구요",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034646.1.18",
          "form": "뭔가 직접 내려마신다하면 어려울거같아서 도전을 못해봤거든여",
          "original_form": "뭔가 직접 내려마신다하면 어려울거같아서 도전을 못해봤거든여",
          "speaker_id": "1",
          "time": "20210828 16:58"
        }
      ]
