      "utterance": [
        {
          "id": "MDRW2100043088.1.1",
          "form": "최근쇼핑한거있으세요",
          "original_form": "최근쇼핑한거있으세요",
          "speaker_id": "1",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100043088.1.2",
          "form": "진짜 오늘 아침에 산 따끈따끈한 상품은",
          "original_form": "진짜 오늘 아침에 산 따끈따끈한 상품은",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043088.1.3",
          "form": "커튼봉이요",
          "original_form": "커튼봉이요",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043088.1.4",
          "form": "근데 아직 커튼을 구매안했어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 아직 커튼을 구매안했어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043088.1.5",
          "form": "ㅋㄴㄱㅋㄱㄴㅋㅋ",
          "original_form": "ㅋㄴㄱㅋㄱㄴㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043088.1.6",
          "form": "설치하셨나요",
          "original_form": "설치하셨나요",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100043088.1.7",
          "form": "오늘 아침에 결제한거라 !! 아직 안받았어요 ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 아침에 결제한거라 !! 아직 안받았어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100043088.1.8",
          "form": "앜ㅋㅋㅋ오고있겠네요",
          "original_form": "앜ㅋㅋㅋ오고있겠네요",
          "speaker_id": "1",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100043088.1.9",
          "form": "네 ㅋㅋㅋ이제 커튼을 사야하는데요.. 너무 많아서 못고르겠네요",
          "original_form": "네 ㅋㅋㅋ이제 커튼을 사야하는데요.. 너무 많아서 못고르겠네요",
          "speaker_id": "2",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043088.1.10",
          "form": "누가 걍 정해서 이거 사라고 해줬으면 좋겠어요",
          "original_form": "누가 걍 정해서 이거 사라고 해줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043088.1.11",
          "form": "ㅎㅎㅎ이쁜커튼인가요 햇빛가리개인가요",
          "original_form": "ㅎㅎㅎ이쁜커튼인가요 햇빛가리개인가요",
          "speaker_id": "1",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100043088.1.12",
          "form": "암막커튼이요!! 근데 너무 싼거하자니 재질이 보면 싼티난다는 후기도 있고 ㅋㅋㅋ비싼거하자니 햇빛가리개인데 돈이 좀 아깝고 ㅋㅋㅋㅋㅋ",
          "original_form": "암막커튼이요!! 근데 너무 싼거하자니 재질이 보면 싼티난다는 후기도 있고 ㅋㅋㅋ비싼거하자니 햇빛가리개인데 돈이 좀 아깝고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100043088.1.13",
          "form": "직접 보고 사고싶은데 ㅠㅠ 걍 대충 시켜야죠 뭐",
          "original_form": "직접 보고 사고싶은데 ㅠㅠ 걍 대충 시켜야죠 뭐",
          "speaker_id": "2",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100043088.1.14",
          "form": "ㅎㅎㅎㅎ암막커튼있으면",
          "original_form": "ㅎㅎㅎㅎ암막커튼있으면",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043088.1.15",
          "form": "진짜밤같더라고요",
          "original_form": "진짜밤같더라고요",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043088.1.16",
          "form": "언넝 사서 달아보고싶네요 후후..",
          "original_form": "언넝 사서 달아보고싶네요 후후..",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043088.1.17",
          "form": "name1님은 최근에 쇼핑한거 있으세요?",
          "original_form": "&name1&님은 최근에 쇼핑한거 있으세요?",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043088.1.18",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:39"
        },
        {
          "id": "MDRW2100043088.1.19",
          "form": "저는올리브영세일해서",
          "original_form": "저는올리브영세일해서",
