      "utterance": [
        {
          "id": "MDRW2100006382.1.1",
          "form": "이번 생일선물",
          "original_form": "이번 생일선물",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006382.1.2",
          "form": "뭐사줄까?",
          "original_form": "뭐사줄까?",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006382.1.3",
          "form": "가방?ㅋㅋㅋㅋ",
          "original_form": "가방?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100006382.1.4",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100006382.1.5",
          "form": "우리는 생일이 한달간격이라",
          "original_form": "우리는 생일이 한달간격이라",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100006382.1.6",
          "form": "주고받는 느낌이 강해가지고 ㅋㅋㅋ",
          "original_form": "주고받는 느낌이 강해가지고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006382.1.7",
          "form": "주면 한달뒤 다시 반납 ㅋㅋㅋ",
          "original_form": "주면 한달뒤 다시 반납 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006382.1.8",
          "form": "난 아이패드 ㅋㅋㅋ",
          "original_form": "난 아이패드 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006382.1.9",
          "form": "전자기기만 좋아하는 남친이 이상하겠지만 ㅋㅋㅋㅋ",
          "original_form": "전자기기만 좋아하는 남친이 이상하겠지만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100006382.1.10",
          "form": "난 이런것만 눈에들어온다 ㅋㅋㅋㅋ",
          "original_form": "난 이런것만 눈에들어온다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100006382.1.11",
          "form": "자기 요즘 뭐 사려고 보는거 있어?",
          "original_form": "자기 요즘 뭐 사려고 보는거 있어?",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006382.1.12",
          "form": "폰사고나서 자중하고 있어 ㅋㅋㅋ",
          "original_form": "폰사고나서 자중하고 있어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006382.1.13",
          "form": "사전예약하고 받은 사은품 팔아서",
          "original_form": "사전예약하고 받은 사은품 팔아서",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006382.1.14",
          "form": "총알 장전해야지 ㅋㅋ",
          "original_form": "총알 장전해야지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100006382.1.15",
          "form": "중고나라에 팔라구?",
          "original_form": "중고나라에 팔라구?",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006382.1.16",
          "form": "아닝",
          "original_form": "아닝",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006382.1.17",
          "form": "당근마켓 히히",
          "original_form": "당근마켓 히히",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006382.1.18",
          "form": "이번에 팔때 나도 같이 따라가볼래 ㅋㅋㅋ",
          "original_form": "이번에 팔때 나도 같이 따라가볼래 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006382.1.19",
          "form": "중고로 파는법을 배워야겠어",
          "original_form": "중고로 파는법을 배워야겠어",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006382.1.20",
          "form": "저번에 한번 하더니 자신감 붙었네",
          "original_form": "저번에 한번 하더니 자신감 붙었네",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006382.1.21",
          "form": "있는물건중에 안쓰는거 정리도 할겸 해서",
          "original_form": "있는물건중에 안쓰는거 정리도 할겸 해서",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006382.1.22",
          "form": "다 팔아볼까 해서",
          "original_form": "다 팔아볼까 해서",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006382.1.23",
          "form": "그럼 팔아서 맛난거사죠",
          "original_form": "그럼 팔아서 맛난거사죠",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
