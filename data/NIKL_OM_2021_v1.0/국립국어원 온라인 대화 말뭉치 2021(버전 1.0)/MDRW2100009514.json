      "utterance": [
        {
          "id": "MDRW2100009514.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009514.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009514.1.3",
          "form": "결혼과 집안일 육아에 대해",
          "original_form": "결혼과 집안일 육아에 대해",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009514.1.4",
          "form": "어떤 생각을 하고 계시나요?",
          "original_form": "어떤 생각을 하고 계시나요?",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009514.1.5",
          "form": "하기싫지만 안할순 없는일들이죠ㅜ",
          "original_form": "하기싫지만 안할순 없는일들이죠ㅜ",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009514.1.6",
          "form": "누군가는 꼭해야하는.ㅠ",
          "original_form": "누군가는 꼭해야하는.ㅠ",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009514.1.7",
          "form": "힘든데 대우는 못받고",
          "original_form": "힘든데 대우는 못받고",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009514.1.8",
          "form": "그런일같아요",
          "original_form": "그런일같아요",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009514.1.9",
          "form": "맞아요ㅋ알아주는이 없고",
          "original_form": "맞아요ㅋ알아주는이 없고",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009514.1.10",
          "form": "정말 필요하고 힘도많이드는데",
          "original_form": "정말 필요하고 힘도많이드는데",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009514.1.11",
          "form": "주부한테 집에서 논다고 하잖아요",
          "original_form": "주부한테 집에서 논다고 하잖아요",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009514.1.12",
          "form": "돈을 못받는거지 노는일이 아닌데요",
          "original_form": "돈을 못받는거지 노는일이 아닌데요",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009514.1.13",
          "form": "저희집에도 그렇게 생각하는 사람이 있어서 공감 입니다ㅠ",
          "original_form": "저희집에도 그렇게 생각하는 사람이 있어서 공감 입니다ㅠ",
          "speaker_id": "2",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100009514.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009514.1.15",
          "form": "차라리 일하는게 낮죠ㅜ",
          "original_form": "차라리 일하는게 낮죠ㅜ",
          "speaker_id": "2",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009514.1.16",
          "form": "네 다들 그렇게 생각하죠",
          "original_form": "네 다들 그렇게 생각하죠",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009514.1.17",
          "form": "그러니 결혼잘안하고 애도안낳구요",
          "original_form": "그러니 결혼잘안하고 애도안낳구요",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009514.1.18",
          "form": "저도 다음생에는 안하려구요ㅋㄱ혼자편하게 살고싶어요",
          "original_form": "저도 다음생에는 안하려구요ㅋㄱ혼자편하게 살고싶어요",
          "speaker_id": "2",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009514.1.19",
          "form": "결혼하셨나봐요",
          "original_form": "결혼하셨나봐요",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009514.1.20",
          "form": "저는 아직 안했긴한데 엄두는 안나요",
          "original_form": "저는 아직 안했긴한데 엄두는 안나요",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009514.1.21",
          "form": "네ㅠㅠ애도둘이에요",
          "original_form": "네ㅠㅠ애도둘이에요",
          "speaker_id": "2",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009514.1.22",
          "form": "키우느라 힘드시겠어요",
          "original_form": "키우느라 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009514.1.23",
          "form": "아직 둘째가 어려서 좀 그런 시기에요",
          "original_form": "아직 둘째가 어려서 좀 그런 시기에요",
          "speaker_id": "2",
          "time": "20210823 00:15"
        }
      ]
