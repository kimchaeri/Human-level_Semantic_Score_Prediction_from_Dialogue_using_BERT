      "utterance": [
        {
          "id": "MDRW2100012296.1.1",
          "form": "아하 저는 동물농장이요ㅋㅋ",
          "original_form": "아하 저는 동물농장이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012296.1.2",
          "form": "좋아하는거 있으세요?",
          "original_form": "좋아하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012296.1.3",
          "form": "저는 여행유튜브 자주봅니다",
          "original_form": "저는 여행유튜브 자주봅니다",
          "speaker_id": "1",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012296.1.4",
          "form": "오 요즘은 여행가기가 힘드니 더 자주보시겠어요",
          "original_form": "오 요즘은 여행가기가 힘드니 더 자주보시겠어요",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012296.1.5",
          "form": "어디 여행이요? 해외? 국내여행?",
          "original_form": "어디 여행이요? 해외? 국내여행?",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012296.1.6",
          "form": "요즘은 해외도 백신맞고 나가더라구요ㅎㅎ",
          "original_form": "요즘은 해외도 백신맞고 나가더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012296.1.7",
          "form": "아 대단하네요 그분들!",
          "original_form": "아 대단하네요 그분들!",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012296.1.8",
          "form": "대리만족 되겠어요ㅋㅋ",
          "original_form": "대리만족 되겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012296.1.9",
          "form": "오맞아요 대리만족 중입니다ㅎㅎ",
          "original_form": "오맞아요 대리만족 중입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012296.1.10",
          "form": "ㅋㅋ저도 애완동물이 없어서 동물농장보면서 대리만족하거든요",
          "original_form": "ㅋㅋ저도 애완동물이 없어서 동물농장보면서 대리만족하거든요",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012296.1.11",
          "form": "어떤 여행컨텐츠 좋아하세요?",
          "original_form": "어떤 여행컨텐츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012296.1.12",
          "form": "휴양? 관광?",
          "original_form": "휴양? 관광?",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012296.1.13",
          "form": "관광이요!",
          "original_form": "관광이요!",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012296.1.14",
          "form": "막 배낭 매고 여기저기 다니는것도 좋아하세요?",
          "original_form": "막 배낭 매고 여기저기 다니는것도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012296.1.15",
          "form": "오 네네 막 돈만원으로 하루버티기 이런것도 재밌더라구요",
          "original_form": "오 네네 막 돈만원으로 하루버티기 이런것도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012296.1.16",
          "form": "ㅋㅋ그럴때 막 돌발상황이나 친절한 사람 만나고 이런거 보면 재밌더라구요",
          "original_form": "ㅋㅋ그럴때 막 돌발상황이나 친절한 사람 만나고 이런거 보면 재밌더라구요",
          "speaker_id": "2",
          "time": "20210824 21:36"
        }
      ]
