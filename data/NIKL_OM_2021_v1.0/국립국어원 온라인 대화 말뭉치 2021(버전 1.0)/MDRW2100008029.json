      "utterance": [
        {
          "id": "MDRW2100008029.1.1",
          "form": "오늘날씨.. 괜찮은가?",
          "original_form": "오늘날씨.. 괜찮은가?",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100008029.1.2",
          "form": "아직까지는 괜찮구만",
          "original_form": "아직까지는 괜찮구만",
          "speaker_id": "2",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100008029.1.3",
          "form": "선선하고",
          "original_form": "선선하고",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100008029.1.4",
          "form": "아침에 좀 흐린거 같더니",
          "original_form": "아침에 좀 흐린거 같더니",
          "speaker_id": "1",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100008029.1.5",
          "form": "지금도 맑은건 아니지만 좀 괜찮은가 싶네",
          "original_form": "지금도 맑은건 아니지만 좀 괜찮은가 싶네",
          "speaker_id": "1",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100008029.1.6",
          "form": "오히려 햇빛 쨍쨍이 아니라 더 좋아",
          "original_form": "오히려 햇빛 쨍쨍이 아니라 더 좋아",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100008029.1.7",
          "form": "덥지 않아서",
          "original_form": "덥지 않아서",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008029.1.8",
          "form": "아 그러네",
          "original_form": "아 그러네",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008029.1.9",
          "form": "눈도 별로 안부시고 좋은 점이 있네",
          "original_form": "눈도 별로 안부시고 좋은 점이 있네",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008029.1.10",
          "form": "응 좋은 점이 있지",
          "original_form": "응 좋은 점이 있지",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008029.1.11",
          "form": "오늘은 비가 안오려나",
          "original_form": "오늘은 비가 안오려나",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008029.1.12",
          "form": "요즘은 갑자기 비가 오는 일이 잦아서",
          "original_form": "요즘은 갑자기 비가 오는 일이 잦아서",
          "speaker_id": "2",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008029.1.13",
          "form": "오늘도 오지 않을까?",
          "original_form": "오늘도 오지 않을까?",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008029.1.14",
          "form": "공기가 습한 느낌인데",
          "original_form": "공기가 습한 느낌인데",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008029.1.15",
          "form": "또 갑자기 비가 내릴지도 모르겠네",
          "original_form": "또 갑자기 비가 내릴지도 모르겠네",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008029.1.16",
          "form": "그러게 요새는 뭐 예고하고 비가 오는게 아니니까",
          "original_form": "그러게 요새는 뭐 예고하고 비가 오는게 아니니까",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008029.1.17",
          "form": "이제 바람이 선선한 것이 곧 가을이 오려나 싶군",
          "original_form": "이제 바람이 선선한 것이 곧 가을이 오려나 싶군",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008029.1.18",
          "form": "그러게 지금 외부온도는 몇도인지 모르겠는데 바람이 시원하네",
          "original_form": "그러게 지금 외부온도는 몇도인지 모르겠는데 바람이 시원하네",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008029.1.19",
          "form": "휴대폰으로 확인하니 26도래",
          "original_form": "휴대폰으로 확인하니 26도래",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008029.1.20",
          "form": "확실히 기온이 내려갔네",
          "original_form": "확실히 기온이 내려갔네",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008029.1.21",
          "form": "26도면 뭐 겨울이네(농담)",
          "original_form": "26도면 뭐 겨울이네(농담)",
          "speaker_id": "1",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008029.1.22",
          "form": "생활하기 좋은 온도가 됐구만 한 23도 되면 좋겠다",
          "original_form": "생활하기 좋은 온도가 됐구만 한 23도 되면 좋겠다",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008029.1.23",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008029.1.24",
          "form": "확실히 광복절 지나니 가을이야",
          "original_form": "확실히 광복절 지나니 가을이야",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008029.1.25",
          "form": "응 20도 초반대가 활동하기 딱 좋지",
          "original_form": "응 20도 초반대가 활동하기 딱 좋지",
          "speaker_id": "2",
          "time": "20210822 14:34"
        }
      ]
    }
  ]
