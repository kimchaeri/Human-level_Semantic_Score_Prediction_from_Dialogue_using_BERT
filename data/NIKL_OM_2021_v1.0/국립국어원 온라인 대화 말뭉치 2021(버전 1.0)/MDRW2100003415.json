      "utterance": [
        {
          "id": "MDRW2100003415.1.1",
          "form": "주말엔 쉬시지요?",
          "original_form": "주말엔 쉬시지요?",
          "speaker_id": "1",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003415.1.2",
          "form": "네네 주5일 근무요",
          "original_form": "네네 주5일 근무요",
          "speaker_id": "2",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003415.1.3",
          "form": "생태씨는요?",
          "original_form": "생태씨는요?",
          "speaker_id": "2",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003415.1.4",
          "form": "네 주5일이라 주말에 폰도 안하고 놀았네요ㅎㅎ",
          "original_form": "네 주5일이라 주말에 폰도 안하고 놀았네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100003415.1.5",
          "form": "저도 주말내내 톡만",
          "original_form": "저도 주말내내 톡만",
          "speaker_id": "2",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100003415.1.6",
          "form": "월요일 출근 힘드셨지요?",
          "original_form": "월요일 출근 힘드셨지요?",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100003415.1.7",
          "form": "네 오늘 반차쓸뻔했어요",
          "original_form": "네 오늘 반차쓸뻔했어요",
          "speaker_id": "2",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100003415.1.8",
          "form": "늦게인나서ㅋ",
          "original_form": "늦게인나서ㅋ",
          "speaker_id": "2",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100003415.1.9",
          "form": "출근하셨어요?",
          "original_form": "출근하셨어요?",
          "speaker_id": "2",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100003415.1.10",
          "form": "ㅋㅋㅋㅋ 와 반차유혹 뿌리치기 힘든데ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 와 반차유혹 뿌리치기 힘든데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:57"
        },
        {
          "id": "MDRW2100003415.1.11",
          "form": "좀만늦었음 쓸뻔했는데 후딱준비하고 지각은안해서요ㅎㅎ",
          "original_form": "좀만늦었음 쓸뻔했는데 후딱준비하고 지각은안해서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003415.1.12",
          "form": "생태씨는 무슨일 하세요?",
          "original_form": "생태씨는 무슨일 하세요?",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003415.1.13",
          "form": "전 연구관련 일이에요ㅎㅎ",
          "original_form": "전 연구관련 일이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100003415.1.14",
          "form": "카롱씨는요?",
          "original_form": "카롱씨는요?",
          "speaker_id": "1",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100003415.1.15",
          "form": "오 좋은일하시네요 전 원자력관련일",
          "original_form": "오 좋은일하시네요 전 원자력관련일",
          "speaker_id": "2",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100003415.1.16",
          "form": "우와 아주 중요한 일을 하시네요.",
          "original_form": "우와 아주 중요한 일을 하시네요.",
          "speaker_id": "1",
          "time": "20210823 11:58"
        },
        {
          "id": "MDRW2100003415.1.17",
          "form": "그런데 정책에 영향을 많이 받으시겠어요ㅜㄴ",
          "original_form": "그런데 정책에 영향을 많이 받으시겠어요ㅜㄴ",
          "speaker_id": "1",
          "time": "20210823 11:58"
        },
        {
          "id": "MDRW2100003415.1.18",
          "form": "네 사장님이 그래서 맨날 욕해요ㅎㅎ",
          "original_form": "네 사장님이 그래서 맨날 욕해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100003415.1.19",
          "form": "큰회사는아니고 작은회산데 타격이 크죠",
          "original_form": "큰회사는아니고 작은회산데 타격이 크죠",
          "speaker_id": "2",
          "time": "20210823 12:53"
        },
        {
          "id": "MDRW2100003415.1.20",
          "form": "무슨연구 하시나요?",
          "original_form": "무슨연구 하시나요?",
          "speaker_id": "2",
          "time": "20210823 12:53"
        },
        {
          "id": "MDRW2100003415.1.21",
          "form": "저는 독서교육이요ㅎㅎ",
          "original_form": "저는 독서교육이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100003415.1.22",
          "form": "정작 책은 많이 안 읽으면서ㅎㅎ",
          "original_form": "정작 책은 많이 안 읽으면서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100003415.1.23",
          "form": "독서교육  특이하네요ㅎ 그런연구하는곳도 있다니",
          "original_form": "독서교육  특이하네요ㅎ 그런연구하는곳도 있다니",
          "speaker_id": "2",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100003415.1.24",
          "form": "주로 독서교육이면 어떤연구인가요?",
          "original_form": "주로 독서교육이면 어떤연구인가요?",
          "speaker_id": "2",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100003415.1.25",
          "form": "어케하면 책을 더많이 읽게하나 그런건가요ㅎ",
          "original_form": "어케하면 책을 더많이 읽게하나 그런건가요ㅎ",
          "speaker_id": "2",
          "time": "20210823 13:26"
        }
      ]
    }
