      "utterance": [
        {
          "id": "MDRW2100000689.1.1",
          "form": "치킨은 언제나 진리지",
          "original_form": "치킨은 언제나 진리지",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000689.1.2",
          "form": "네네가 아니구 비에이치씨 순살로 변경",
          "original_form": "네네가 아니구 비에이치씨 순살로 변경",
          "speaker_id": "1",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000689.1.3",
          "form": "픽업주문 완료",
          "original_form": "픽업주문 완료",
          "speaker_id": "1",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000689.1.4",
          "form": "아 비에이치는ㅋㅋ 야구경품?",
          "original_form": "아 비에이치는ㅋㅋ 야구경품?",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000689.1.5",
          "form": "응ㅋㅋㅋㅋ 알뜰하게 잘쓰고있다ㅋㅋ",
          "original_form": "응ㅋㅋㅋㅋ 알뜰하게 잘쓰고있다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000689.1.6",
          "form": "좋구만ㅋㅋ우리는뭐먹지",
          "original_form": "좋구만ㅋㅋ우리는뭐먹지",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000689.1.7",
          "form": "네네배달 해야되나",
          "original_form": "네네배달 해야되나",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000689.1.8",
          "form": "픽업은 누가감?",
          "original_form": "픽업은 누가감?",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000689.1.9",
          "form": "내가 가야지 ㅋㅋㄱ배달비있으니",
          "original_form": "내가 가야지 ㅋㅋㄱ배달비있으니",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000689.1.10",
          "form": "그니까 비싸ㅜ",
          "original_form": "그니까 비싸ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000689.1.11",
          "form": "울집앞이 비에치씨자나",
          "original_form": "울집앞이 비에치씨자나",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000689.1.12",
          "form": "아 좋다",
          "original_form": "아 좋다",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000689.1.13",
          "form": "애들보고 가져오라캐야지",
          "original_form": "애들보고 가져오라캐야지",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000689.1.14",
          "form": "아 추가결제해야되서 내가 가야하네",
          "original_form": "아 추가결제해야되서 내가 가야하네",
          "speaker_id": "1",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000689.1.15",
          "form": "켁 ㅡ송금ㅋ",
          "original_form": "켁 ㅡ송금ㅋ",
          "speaker_id": "2",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100000689.1.16",
          "form": "이미 애들 들어왔더라 나간김에 씨유가서 좀바꿔오지모",
          "original_form": "이미 애들 들어왔더라 나간김에 씨유가서 좀바꿔오지모",
          "speaker_id": "1",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100000689.1.17",
          "form": "ㅋㅋㅋ뭐바꾸는데ㅎ",
          "original_form": "ㅋㅋㅋ뭐바꾸는데ㅎ",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100000689.1.18",
          "form": "뚱바랑 깊콘잔액남은거 쓰러 ㅋ 바로 옆이니깐 간식거리 사와야제",
          "original_form": "뚱바랑 깊콘잔액남은거 쓰러 ㅋ 바로 옆이니깐 간식거리 사와야제",
