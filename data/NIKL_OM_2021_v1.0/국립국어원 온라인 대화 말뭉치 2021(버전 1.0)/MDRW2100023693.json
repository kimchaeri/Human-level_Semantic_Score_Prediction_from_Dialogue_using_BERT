        {
          "id": "MDRW2100023693.1.1",
          "form": "너 대만 갔을때",
          "original_form": "너 대만 갔을때",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023693.1.2",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023693.1.3",
          "form": "그 패션후르츠 아이스크림 먹었었나?",
          "original_form": "그 패션후르츠 아이스크림 먹었었나?",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023693.1.4",
          "form": "아닝 그건 너네 셋이",
          "original_form": "아닝 그건 너네 셋이",
          "speaker_id": "1",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023693.1.5",
          "form": "왜 너는 없었던 것 같지?",
          "original_form": "왜 너는 없었던 것 같지?",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023693.1.6",
          "form": "그치?",
          "original_form": "그치?",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023693.1.7",
          "form": "맞아 나 없었어ㅋㅋ",
          "original_form": "맞아 나 없었어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023693.1.8",
          "form": "너 어디갔었어?ㅋㅋ",
          "original_form": "너 어디갔었어?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023693.1.9",
          "form": "나 그냥 주변구경ㅋㅋ",
          "original_form": "나 그냥 주변구경ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023693.1.10",
          "form": "그래서 거기 바닷가에서 만난건가?",
          "original_form": "그래서 거기 바닷가에서 만난건가?",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023693.1.11",
          "form": "거기 올라가는거였나? 무튼 그냥 쉬고싶어서ㅋㅋ",
          "original_form": "거기 올라가는거였나? 무튼 그냥 쉬고싶어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023693.1.12",
          "form": "맞아ㅋㅋ",
          "original_form": "맞아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023693.1.13",
          "form": "마자 더웠지ㅠㅠㅠㅠ",
          "original_form": "마자 더웠지ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023693.1.14",
          "form": "그니까 사진찍은거 봐도 없더라고",
          "original_form": "그니까 사진찍은거 봐도 없더라고",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023693.1.15",
          "form": "그러고 거기 노을보러 섬같은데 간건가? 배타고",
          "original_form": "그러고 거기 노을보러 섬같은데 간건가? 배타고",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023693.1.16",
          "form": "아 거기서 배탄거구나?",
          "original_form": "아 거기서 배탄거구나?",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023693.1.17",
          "form": "그날이었는지 다른날이었는지 기억이 잘 안나",
          "original_form": "그날이었는지 다른날이었는지 기억이 잘 안나",
