      "utterance": [
        {
          "id": "MDRW2100018224.1.1",
          "form": "날씨 요새 그래도 선선해졌죠",
          "original_form": "날씨 요새 그래도 선선해졌죠",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018224.1.2",
          "form": "뭔가 빨라진 느낌이에요",
          "original_form": "뭔가 빨라진 느낌이에요",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018224.1.3",
          "form": "네 가을이 금방왔죠",
          "original_form": "네 가을이 금방왔죠",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018224.1.4",
          "form": "막 덥진 않은데 습도땜시 땀나요",
          "original_form": "막 덥진 않은데 습도땜시 땀나요",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018224.1.5",
          "form": "에어컨 켰다 껐어요",
          "original_form": "에어컨 켰다 껐어요",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018224.1.6",
          "form": "습도 낮추게",
          "original_form": "습도 낮추게",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018224.1.7",
          "form": "어라 답이 안갔나",
          "original_form": "어라 답이 안갔나",
          "speaker_id": "2",
          "time": "20210827 09:32"
        },
        {
          "id": "MDRW2100018224.1.8",
          "form": "맞아요 막 덥지는 않은데",
          "original_form": "맞아요 막 덥지는 않은데",
          "speaker_id": "2",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100018224.1.9",
          "form": "끈적끈적",
          "original_form": "끈적끈적",
          "speaker_id": "2",
          "time": "20210827 09:33"
        },
        {
          "id": "MDRW2100018224.1.10",
          "form": "저희 남편 어제 오자마자 에어컨 좀 켜말을 안하고 끈적하지 않아 ㅋㅋ",
          "original_form": "저희 남편 어제 오자마자 에어컨 좀 켜말을 안하고 끈적하지 않아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100018224.1.11",
          "form": "돌려서 마하는데 귀여웠어요",
          "original_form": "돌려서 마하는데 귀여웠어요",
          "speaker_id": "1",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100018224.1.12",
          "form": "뭘 그런걸 돌려서 켜면되지 ㅎㅎ",
          "original_form": "뭘 그런걸 돌려서 켜면되지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100018224.1.13",
          "form": "요새 에어컨 켜긴 선선해서 좀그렇지만 습도 때문에 끈적해서",
          "original_form": "요새 에어컨 켜긴 선선해서 좀그렇지만 습도 때문에 끈적해서",
          "speaker_id": "1",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100018224.1.14",
          "form": "켜야해요",
          "original_form": "켜야해요",
          "speaker_id": "1",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100018224.1.15",
          "form": "어제 다시 켰어요",
          "original_form": "어제 다시 켰어요",
          "speaker_id": "1",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018224.1.16",
          "form": "집으로 돌아 오는길 잠깐 갈을때도 끈적하죠 ㅜㅜ날씨가 참",
          "original_form": "집으로 돌아 오는길 잠깐 갈을때도 끈적하죠 ㅜㅜ날씨가 참",
          "speaker_id": "1",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018224.1.17",
          "form": "걸을때도",
          "original_form": "걸을때도",
          "speaker_id": "1",
          "time": "20210827 09:41"
        },
        {
          "id": "MDRW2100018224.1.18",
          "form": "저도 ㅎㅎ",
          "original_form": "저도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:41"
        },
        {
          "id": "MDRW2100018224.1.19",
          "form": "더운건 괜찮은데 습도가 ㅎㅎ",
          "original_form": "더운건 괜찮은데 습도가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:41"
        },
        {
          "id": "MDRW2100018224.1.20",
          "form": "맞아요 더우면 오히려 땀이 덜나요",
          "original_form": "맞아요 더우면 오히려 땀이 덜나요",
          "speaker_id": "1",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018224.1.21",
          "form": "덜끈적하고",
          "original_form": "덜끈적하고",
          "speaker_id": "1",
          "time": "20210827 09:43"
        },
        {
          "id": "MDRW2100018224.1.22",
          "form": "동남아 놀러온 기분 ㅎㅎ",
          "original_form": "동남아 놀러온 기분 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018224.1.23",
          "form": "홍콩 같아요",
          "original_form": "홍콩 같아요",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018224.1.24",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018224.1.25",
          "form": "전  대만",
          "original_form": "전  대만",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018224.1.26",
          "form": "어제 남편이랑 대만 되간다 했는데 ㅋㅋ",
          "original_form": "어제 남편이랑 대만 되간다 했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018224.1.27",
          "form": "대만 야시장 진짜",
          "original_form": "대만 야시장 진짜",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018224.1.28",
          "form": "행복인데 ㅋㅋㅋ",
          "original_form": "행복인데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018224.1.29",
          "form": "대만 가고 싶다~~~~",
          "original_form": "대만 가고 싶다~~~~",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018224.1.30",
          "form": "맞아요 ㅋㅋ 젤 가고싶어요",
          "original_form": "맞아요 ㅋㅋ 젤 가고싶어요",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018224.1.31",
          "form": "마지막 여행지 여서 그런가봐요",
          "original_form": "마지막 여행지 여서 그런가봐요",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018224.1.32",
          "form": "물가도 아주 비싸지 않고",
          "original_form": "물가도 아주 비싸지 않고",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018224.1.33",
          "form": "음식도 입맛에 맞고",
          "original_form": "음식도 입맛에 맞고",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018224.1.34",
          "form": "자유여행 딱좋았어요 ㅎㅎ",
          "original_form": "자유여행 딱좋았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:57"
        },
        {
          "id": "MDRW2100018224.1.35",
          "form": "ㅋㅋ좀 더운거 빼면",
          "original_form": "ㅋㅋ좀 더운거 빼면",
          "speaker_id": "2",
          "time": "20210827 10:00"
        },
        {
          "id": "MDRW2100018224.1.36",
          "form": "사람들도 친절하고 ㅎㅎ",
          "original_form": "사람들도 친절하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018224.1.37",
          "form": "맞아요 ㅎㅎ대충 말해도 잘알아 듣고",
          "original_form": "맞아요 ㅎㅎ대충 말해도 잘알아 듣고",
          "speaker_id": "1",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018224.1.38",
          "form": "친절하시죠 ㅎㅎ",
          "original_form": "친절하시죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:02"
        }
