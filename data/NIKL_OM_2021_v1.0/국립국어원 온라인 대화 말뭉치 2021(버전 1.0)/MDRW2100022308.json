      "utterance": [
        {
          "id": "MDRW2100022308.1.1",
          "form": "홈런볼 얼려먹어봤어?",
          "original_form": "홈런볼 얼려먹어봤어?",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022308.1.2",
          "form": "엉 쩔어..",
          "original_form": "엉 쩔어..",
          "speaker_id": "1",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022308.1.3",
          "form": "찰떡파이는..?",
          "original_form": "찰떡파이는..?",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022308.1.4",
          "form": "내 픽은 누드빼빼로 오독오독 최고..",
          "original_form": "내 픽은 누드빼빼로 오독오독 최고..",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022308.1.5",
          "form": "찰떡파이는 안해봤는데..",
          "original_form": "찰떡파이는 안해봤는데..",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022308.1.6",
          "form": "찰떡파이 한번 해먹어보고싶어",
          "original_form": "찰떡파이 한번 해먹어보고싶어",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022308.1.7",
          "form": "근데 그럼 또 쫀쫀한 맛이 없어져서 별로이려나?",
          "original_form": "근데 그럼 또 쫀쫀한 맛이 없어져서 별로이려나?",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022308.1.8",
          "form": "안해본거냐구 ㅋㅋㅋ해본줄",
          "original_form": "안해본거냐구 ㅋㅋㅋ해본줄",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022308.1.9",
          "form": "웅 안해봄ㅋㅋㅋ",
          "original_form": "웅 안해봄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022308.1.10",
          "form": "초코파이도 맛있으니까 찰떡파이도 맛있을 거 같아",
          "original_form": "초코파이도 맛있으니까 찰떡파이도 맛있을 거 같아",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022308.1.11",
          "form": "초코파이 얼려봣어?",
          "original_form": "초코파이 얼려봣어?",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022308.1.12",
          "form": "응 완전 맛있음",
          "original_form": "응 완전 맛있음",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022308.1.13",
          "form": "마시멜로우는 식감이 어때? 얼리면?",
          "original_form": "마시멜로우는 식감이 어때? 얼리면?",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022308.1.14",
          "form": "내가 마시멜로우를 별로안좋아해서 얼린게 더 맛있던데 바삭해",
          "original_form": "내가 마시멜로우를 별로안좋아해서 얼린게 더 맛있던데 바삭해",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022308.1.15",
          "form": "아하 신기하다 나도 해봐야지",
          "original_form": "아하 신기하다 나도 해봐야지",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022308.1.16",
          "form": "몽쉘도 얼리면 맛있어",
          "original_form": "몽쉘도 얼리면 맛있어",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022308.1.17",
          "form": "아 요즘 만두 빚고싶어",
          "original_form": "아 요즘 만두 빚고싶어",
