        },
        {
          "id": "MDRW2100042606.1.2",
          "form": "엉 토요일 여행계모임했는데",
          "original_form": "엉 토요일 여행계모임했는데",
          "speaker_id": "1",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042606.1.3",
          "form": "어디로 가는데",
          "original_form": "어디로 가는데",
          "speaker_id": "2",
          "time": "20210906 23:23"
        },
        {
          "id": "MDRW2100042606.1.4",
          "form": "그냥 일단 내년 5월 여행가자는거만 하고",
          "original_form": "그냥 일단 내년 5월 여행가자는거만 하고",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100042606.1.5",
          "form": "아그래",
          "original_form": "아그래",
          "speaker_id": "2",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100042606.1.6",
          "form": "엉 내년 되어봐야 알지 뭐",
          "original_form": "엉 내년 되어봐야 알지 뭐",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100042606.1.7",
          "form": "계속 이러네",
          "original_form": "계속 이러네",
          "speaker_id": "1",
          "time": "20210906 23:39"
        },
        {
          "id": "MDRW2100042606.1.8",
          "form": "뭐지",
          "original_form": "뭐지",
          "speaker_id": "1",
          "time": "20210906 23:40"
        },
        {
          "id": "MDRW2100042606.1.9",
          "form": "해외 못가면 국내라도",
          "original_form": "해외 못가면 국내라도",
          "speaker_id": "1",
          "time": "20210906 23:41"
        },
        {
          "id": "MDRW2100042606.1.10",
          "form": "일행이휴가쓰는게 힘들다며",
          "original_form": "일행이휴가쓰는게 힘들다며",
          "speaker_id": "2",
          "time": "20210906 23:41"
        },
        {
          "id": "MDRW2100042606.1.11",
          "form": "맞아 비행기값 쓰고빨리와야함",
          "original_form": "맞아 비행기값 쓰고빨리와야함",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100042606.1.12",
          "form": "돈아깝다",
          "original_form": "돈아깝다",
          "speaker_id": "2",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100042606.1.13",
          "form": "좀그런면이있어",
          "original_form": "좀그런면이있어",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100042606.1.14",
          "form": "그냥 5월이지만 안갈수도 있는거네",
          "original_form": "그냥 5월이지만 안갈수도 있는거네",
          "speaker_id": "2",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042606.1.15",
          "form": "엉 가면 또 트러블생길까봐",
          "original_form": "엉 가면 또 트러블생길까봐",
          "speaker_id": "1",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042606.1.16",
          "form": "그럼 가지마라",
          "original_form": "그럼 가지마라",
          "speaker_id": "2",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042606.1.17",
          "form": "매달 보는데 안간다고하면 무슨 이유를",
          "original_form": "매달 보는데 안간다고하면 무슨 이유를",
