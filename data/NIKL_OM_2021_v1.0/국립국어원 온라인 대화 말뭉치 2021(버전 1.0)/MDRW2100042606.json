          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042606.1.18",
          "form": "난처하다",
          "original_form": "난처하다",
          "speaker_id": "2",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042606.1.19",
