      "utterance": [
        {
          "id": "MDRW2100001932.1.1",
          "form": "남자친구분은",
          "original_form": "남자친구분은",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001932.1.2",
          "form": "이제 전역하셨나요?",
          "original_form": "이제 전역하셨나요?",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001932.1.3",
          "form": "어제했어염",
          "original_form": "어제했어염",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001932.1.4",
          "form": "이제 민간인이져",
          "original_form": "이제 민간인이져",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001932.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001932.1.6",
          "form": "자유에여",
          "original_form": "자유에여",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001932.1.7",
          "form": "제 남친",
          "original_form": "제 남친",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001932.1.8",
          "form": "군대에서",
          "original_form": "군대에서",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001932.1.9",
          "form": "요리만해서",
          "original_form": "요리만해서",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001932.1.10",
          "form": "취사병이었거든요",
          "original_form": "취사병이었거든요",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001932.1.11",
          "form": "어머어머 취사병ㅋㅋㅋㅋ",
          "original_form": "어머어머 취사병ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001932.1.12",
          "form": "어제 전역하셨으면은\r\n꽃신 신겨주셨나영?ㅋㅋ",
          "original_form": "어제 전역하셨으면은\r\n꽃신 신겨주셨나영?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001932.1.13",
          "form": "ㅋㅋㅋㅋㅋ주말에 이제 만나려고용",
          "original_form": "ㅋㅋㅋㅋㅋ주말에 이제 만나려고용",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001932.1.14",
          "form": "요새는 입대 기간이 얼마나 되죠?",
          "original_form": "요새는 입대 기간이 얼마나 되죠?",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001932.1.15",
          "form": "1년 반인뎅",
          "original_form": "1년 반인뎅",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001932.1.16",
          "form": "요즘 코로나땜시",
          "original_form": "요즘 코로나땜시",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001932.1.17",
          "form": "휴가못나가서",
          "original_form": "휴가못나가서",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001932.1.18",
          "form": "그만큼 일찍 전역하긴 했어염 ㅋㅋㅋ",
          "original_form": "그만큼 일찍 전역하긴 했어염 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:46"
        },
        {
          "id": "MDRW2100001932.1.19",
          "form": "오 그럼 누적된 휴가일수만큼",
          "original_form": "오 그럼 누적된 휴가일수만큼",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001932.1.20",
          "form": "앞당겨서 전역하는거?ㅋㅋ",
          "original_form": "앞당겨서 전역하는거?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001932.1.21",
          "form": "넹넹 ㅋㅋㅋㅋ",
          "original_form": "넹넹 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100001932.1.22",
          "form": "강쥐 데리고전역 ㅋㅋㅋ",
          "original_form": "강쥐 데리고전역 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100001932.1.23",
          "form": "전역할때 강쥐 어떻게 델고 나왔어여?",
          "original_form": "전역할때 강쥐 어떻게 델고 나왔어여?",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100001932.1.24",
          "form": "걍 데리고 나오던데염..?",
          "original_form": "걍 데리고 나오던데염..?",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100001932.1.25",
          "form": "아거기 군대에사",
          "original_form": "아거기 군대에사",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100001932.1.26",
          "form": "강쥐들..다 보호소로 간다해소 ㅠ",
          "original_form": "강쥐들..다 보호소로 간다해소 ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100001932.1.27",
          "form": "홀ㅜㅜ 불쌍한 강쥐들",
          "original_form": "홀ㅜㅜ 불쌍한 강쥐들",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100001932.1.28",
          "form": "지금 갱아쥐 남치닝 대신돌보고있는데 힘들뎈ㅋㅋ 계속 놀아줘야된다고 ㅋㅋ",
          "original_form": "지금 갱아쥐 남치닝 대신돌보고있는데 힘들뎈ㅋㅋ 계속 놀아줘야된다고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 07:22"
        },
        {
          "id": "MDRW2100001932.1.29",
          "form": "ㅋㅋ군생활보다 힘들다던가요?ㅋㅋ",
          "original_form": "ㅋㅋ군생활보다 힘들다던가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 07:22"
        },
        {
          "id": "MDRW2100001932.1.30",
          "form": "그소리하면",
          "original_form": "그소리하면",
          "speaker_id": "1",
          "time": "20210820 08:06"
        },
        {
          "id": "MDRW2100001932.1.31",
          "form": "큰일납니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "큰일납니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:06"
        },
        {
          "id": "MDRW2100001932.1.32",
          "form": "재미는 있었는데",
          "original_form": "재미는 있었는데",
          "speaker_id": "1",
          "time": "20210820 08:07"
        },
        {
          "id": "MDRW2100001932.1.33",
          "form": "다시는 가거싶지않대옄ㅋㅋㅋㅋ",
          "original_form": "다시는 가거싶지않대옄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:07"
        },
        {
          "id": "MDRW2100001932.1.34",
          "form": "ㅋㅋ군생활은 그렇게 기억되나 봅니다ㅋㅋ",
          "original_form": "ㅋㅋ군생활은 그렇게 기억되나 봅니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 08:26"
        },
        {
          "id": "MDRW2100001932.1.35",
          "form": "그쳨ㅋㅋ 그렇게 추억이되는거쥬 ㅋㅋㅋ",
          "original_form": "그쳨ㅋㅋ 그렇게 추억이되는거쥬 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100001932.1.36",
          "form": "그래도 요즘 군대",
          "original_form": "그래도 요즘 군대",
          "speaker_id": "1",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100001932.1.37",
          "form": "핸드폰 쓸 시간도 주고",
          "original_form": "핸드폰 쓸 시간도 주고",
          "speaker_id": "1",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100001932.1.38",
          "form": "막 야식도 시켜먹고 그르등데염",
          "original_form": "막 야식도 시켜먹고 그르등데염",
          "speaker_id": "1",
          "time": "20210820 08:30"
        },
        {
          "id": "MDRW2100001932.1.39",
          "form": "우왕ㅋㅋ 좋아지긴 좋아졌네",
          "original_form": "우왕ㅋㅋ 좋아지긴 좋아졌네",
          "speaker_id": "2",
          "time": "20210820 08:32"
        },
        {
          "id": "MDRW2100001932.1.40",
          "form": "핸드폰은 밤에 사용 가능한건가?",
          "original_form": "핸드폰은 밤에 사용 가능한건가?",
          "speaker_id": "2",
          "time": "20210820 08:32"
        },
        {
          "id": "MDRW2100001932.1.41",
          "form": "점호하고나서? ㅋㅋ",
          "original_form": "점호하고나서? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 08:32"
        },
        {
          "id": "MDRW2100001932.1.42",
          "form": "한 10시전?? 까진 사용하던뎅",
          "original_form": "한 10시전?? 까진 사용하던뎅",
          "speaker_id": "1",
          "time": "20210820 08:41"
        },
        {
          "id": "MDRW2100001932.1.43",
          "form": "저녁 먹고나서",
          "original_form": "저녁 먹고나서",
          "speaker_id": "1",
          "time": "20210820 08:41"
        },
        {
          "id": "MDRW2100001932.1.44",
          "form": "오 진짜 꿀이네요 군생활 ㅋㅋㅋ",
          "original_form": "오 진짜 꿀이네요 군생활 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100001932.1.45",
          "form": "라떼는 말이야...ㅋㅋㅋ",
          "original_form": "라떼는 말이야...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100001932.1.46",
          "form": "줄서서 공중전화기로 전화했는데ㅋㅋ",
          "original_form": "줄서서 공중전화기로 전화했는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100001932.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100001932.1.48",
          "form": "맞엌ㅋㅋ",
          "original_form": "맞엌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100001932.1.49",
          "form": "근데 아직 공중전화기있을걸???",
          "original_form": "근데 아직 공중전화기있을걸???",
          "speaker_id": "1",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100001932.1.50",
          "form": "막 군대 초반엔 폰 잘못받으니까",
          "original_form": "막 군대 초반엔 폰 잘못받으니까",
          "speaker_id": "1",
          "time": "20210820 08:54"
        },
        {
          "id": "MDRW2100001932.1.51",
          "form": "군대에서 사용하는 폰도",
          "original_form": "군대에서 사용하는 폰도",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100001932.1.52",
          "form": "우리가 일반적으로 사용하는 폰이랑 같은건가?ㅋㅋ",
          "original_form": "우리가 일반적으로 사용하는 폰이랑 같은건가?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100001932.1.53",
          "form": "인터넷 전화 문자 다 가능하고?ㅋㅋ",
          "original_form": "인터넷 전화 문자 다 가능하고?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:46"
        },
        {
          "id": "MDRW2100001932.1.54",
          "form": "움..",
          "original_form": "움..",
          "speaker_id": "1",
          "time": "20210820 09:46"
        },
        {
          "id": "MDRW2100001932.1.55",
          "form": "카메라만 막아둔걸로 알고있어욤 ㅋㅋ",
          "original_form": "카메라만 막아둔걸로 알고있어욤 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:46"
        },
        {
