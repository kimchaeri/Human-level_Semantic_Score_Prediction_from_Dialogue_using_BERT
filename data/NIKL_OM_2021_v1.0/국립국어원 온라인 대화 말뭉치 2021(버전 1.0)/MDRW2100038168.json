        },
        {
          "id": "MDRW2100038168.1.2",
          "form": "ㅇㅑ타용왕처럼",
          "original_form": "ㅇㅑ타용왕처럼",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100038168.1.3",
          "form": "뭔가 조건이 있나",
          "original_form": "뭔가 조건이 있나",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100038168.1.4",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100038168.1.5",
          "form": "아냐 그런 스토리가 있는 몹이 아녀",
          "original_form": "아냐 그런 스토리가 있는 몹이 아녀",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100038168.1.6",
          "form": "물의 정령 알지?",
          "original_form": "물의 정령 알지?",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100038168.1.7",
          "form": "그거 속성만 바뀐거",
          "original_form": "그거 속성만 바뀐거",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100038168.1.8",
          "form": "글킨한데",
          "original_form": "글킨한데",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100038168.1.9",
          "form": "왜 안보이지",
          "original_form": "왜 안보이지",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100038168.1.10",
          "form": "보통 위치는 숨겨져있더라도",
          "original_form": "보통 위치는 숨겨져있더라도",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100038168.1.11",
          "form": "맵에 단편적으로는",
          "original_form": "맵에 단편적으로는",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100038168.1.12",
          "form": "뜨던데",
          "original_form": "뜨던데",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100038168.1.13",
          "form": "ㅇㅎ 검색해보니까",
          "original_form": "ㅇㅎ 검색해보니까",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038168.1.14",
          "form": "세이라이섬 퀘 끝내면",
          "original_form": "세이라이섬 퀘 끝내면",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038168.1.15",
          "form": "나온대",
          "original_form": "나온대",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100038168.1.16",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100038168.1.17",
          "form": "ㄴ ㅏ",
          "original_form": "ㄴ ㅏ",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100038168.1.18",
          "form": "멘스 미니게임에서 막혀서",
          "original_form": "멘스 미니게임에서 막혀서",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100038168.1.19",
          "form": "걍 드랍하고",
          "original_form": "걍 드랍하고",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100038168.1.20",
          "form": "새로 추가된 낚시나 함...",
          "original_form": "새로 추가된 낚시나 함...",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100038168.1.21",
          "form": "된다",
          "original_form": "된다",
          "speaker_id": "2",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100038168.1.22",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100038168.1.23",
          "form": "낚시 할만해?",
          "original_form": "낚시 할만해?",
          "speaker_id": "2",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100038168.1.24",
          "form": "이거",
          "original_form": "이거",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100038168.1.25",
          "form": "어려워;;;",
          "original_form": "어려워;;;",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100038168.1.26",
          "form": "검은사막보다 어려운데???",
          "original_form": "검은사막보다 어려운데???",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100038168.1.27",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038168.1.28",
          "form": "어떻게 하길래....",
          "original_form": "어떻게 하길래....",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038168.1.29",
          "form": "ㄹㅇ쉽게 가는게 없네",
          "original_form": "ㄹㅇ쉽게 가는게 없네",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038168.1.30",
          "form": "고인물들 할거없으면 낚싯대나 붙잡고 놀아라",
          "original_form": "고인물들 할거없으면 낚싯대나 붙잡고 놀아라",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038168.1.31",
          "form": "그건ㄱㆍ",
          "original_form": "그건ㄱㆍ",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038168.1.32",
          "form": "그런듯 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런듯 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038168.1.33",
          "form": "낚시 포인트마다 자잘하게 종 다르고",
          "original_form": "낚시 포인트마다 자잘하게 종 다르고",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038168.1.34",
          "form": "섬세하고 할거 많은건 좋은데",
          "original_form": "섬세하고 할거 많은건 좋은데",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038168.1.35",
          "form": "같은 종이어도 하우징에 풀어줄 수 있는 관상어는 희귀확률로 나타남",
          "original_form": "같은 종이어도 하우징에 풀어줄 수 있는 관상어는 희귀확률로 나타남",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038168.1.36",
          "form": "멘스도 그만큼 좀 신경써줬으면",
          "original_form": "멘스도 그만큼 좀 신경써줬으면",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038168.1.37",
          "form": "얼마나 좋아",
          "original_form": "얼마나 좋아",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038168.1.38",
          "form": "배보다 배꼽이 더 크네",
          "original_form": "배보다 배꼽이 더 크네",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038168.1.39",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
