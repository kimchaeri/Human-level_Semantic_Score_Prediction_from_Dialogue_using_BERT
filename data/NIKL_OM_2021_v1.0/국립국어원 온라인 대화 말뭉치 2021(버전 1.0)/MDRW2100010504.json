      "utterance": [
        {
          "id": "MDRW2100010504.1.1",
          "form": "홈트로 다이어트하는 사람처럼",
          "original_form": "홈트로 다이어트하는 사람처럼",
          "speaker_id": "2",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010504.1.2",
          "form": "혼자 체지방 빼시는 거 진짜 대단하십니다",
          "original_form": "혼자 체지방 빼시는 거 진짜 대단하십니다",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010504.1.3",
          "form": "저 질문 웜톤이세요 쿨톤이세요?!",
          "original_form": "저 질문 웜톤이세요 쿨톤이세요?!",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010504.1.4",
          "form": "웜톤인 거 같아요",
          "original_form": "웜톤인 거 같아요",
          "speaker_id": "1",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010504.1.5",
          "form": "저는 지금까지 쿨톤인줄 알고 살아왔는데 웜톤일수도 있다하더라고요..쓰읍..",
          "original_form": "저는 지금까지 쿨톤인줄 알고 살아왔는데 웜톤일수도 있다하더라고요..쓰읍..",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010504.1.6",
          "form": "저도 사실 검사 같은 걸 안 받아봐서 잘 모르겠어요",
          "original_form": "저도 사실 검사 같은 걸 안 받아봐서 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010504.1.7",
          "form": "혹시 전문적인 퍼스널 컬러도 받아",
          "original_form": "혹시 전문적인 퍼스널 컬러도 받아",
          "speaker_id": "2",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010504.1.8",
          "form": "아아 저도요",
          "original_form": "아아 저도요",
          "speaker_id": "2",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010504.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010504.1.10",
          "form": "혹시 화장은 좋아하시는 편이신가여?!",
          "original_form": "혹시 화장은 좋아하시는 편이신가여?!",
          "speaker_id": "2",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010504.1.11",
          "form": "친구들이랑 한 번 받아보려고요",
          "original_form": "친구들이랑 한 번 받아보려고요",
          "speaker_id": "1",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010504.1.12",
          "form": "피부가 예민해서 잘 안 하는 편이에요ㅜㅠ",
          "original_form": "피부가 예민해서 잘 안 하는 편이에요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010504.1.13",
          "form": "순한 걸 발라도 뭐가 나고 잘 안 받더라고요",
          "original_form": "순한 걸 발라도 뭐가 나고 잘 안 받더라고요",
          "speaker_id": "1",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010504.1.14",
          "form": "앗 ㅠㅠㅠㅠ",
          "original_form": "앗 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010504.1.15",
          "form": "저는 화장 잘 못하는 편이라",
          "original_form": "저는 화장 잘 못하는 편이라",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010504.1.16",
          "form": "안하기는 하는데",
          "original_form": "안하기는 하는데",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010504.1.17",
          "form": "그러면 그 기초 사실때도",
          "original_form": "그러면 그 기초 사실때도",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010504.1.18",
          "form": "많이 체크하고 사시겠어요ㅠㅠ",
          "original_form": "많이 체크하고 사시겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010504.1.19",
          "form": "최대한 순하고 아무것도 안 들어 간 거 사요ㅎㅎ",
          "original_form": "최대한 순하고 아무것도 안 들어 간 거 사요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010504.1.20",
          "form": "요즘은 특히 마스크 때문에 다들 화장 잘 안 하더라고요",
          "original_form": "요즘은 특히 마스크 때문에 다들 화장 잘 안 하더라고요",
          "speaker_id": "1",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010504.1.21",
          "form": "맞아요 사실 그래서 살짝 ㅋㅋㅋㅋ마스크 코로나가 없어져도 계속 썼으면 좋겠어욬ㅋㅋㅋ",
          "original_form": "맞아요 사실 그래서 살짝 ㅋㅋㅋㅋ마스크 코로나가 없어져도 계속 썼으면 좋겠어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010504.1.22",
          "form": "저는 제가 화장을 안 해서 그런지 다들 화장 안 하는 지금이 좀 좋더라고요ㅎㅎ",
          "original_form": "저는 제가 화장을 안 해서 그런지 다들 화장 안 하는 지금이 좀 좋더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010504.1.23",
          "form": "헤헤 화장 넘 어렵고 돈도 많이 들어요ㅠ 시간도 들고",
          "original_form": "헤헤 화장 넘 어렵고 돈도 많이 들어요ㅠ 시간도 들고",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010504.1.24",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010504.1.25",
          "form": "진짜 제일 후회하는게 화장품 산 돈이에요",
          "original_form": "진짜 제일 후회하는게 화장품 산 돈이에요",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010504.1.26",
          "form": "너무 비싼 거 같아요ㅠㅠ",
          "original_form": "너무 비싼 거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010504.1.27",
          "form": "몇 번 안 쓰면 더 아깝죠",
          "original_form": "몇 번 안 쓰면 더 아깝죠",
          "speaker_id": "1",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010504.1.28",
          "form": "맞아요 특히 색조ㅜㅠ",
          "original_form": "맞아요 특히 색조ㅜㅠ",
          "speaker_id": "2",
          "time": "20210823 19:24"
        }
      ]
