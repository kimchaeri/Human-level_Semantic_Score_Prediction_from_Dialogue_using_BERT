      "utterance": [
        {
          "id": "MDRW2100042360.1.1",
          "form": "행복한돼지는 먹을거얘기를제일좋아하죠..",
          "original_form": "행복한돼지는 먹을거얘기를제일좋아하죠..",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042360.1.2",
          "form": "ㅋㅈㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㅋㅈㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042360.1.3",
          "form": "가장좋아하는반찬은?!",
          "original_form": "가장좋아하는반찬은?!",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042360.1.4",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042360.1.5",
          "form": "양념게장??",
          "original_form": "양념게장??",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042360.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042360.1.7",
          "form": "오호 !!! 저는 떡볶이요!!!!!",
          "original_form": "오호 !!! 저는 떡볶이요!!!!!",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042360.1.8",
          "form": "헉 떡볶이 맛있죠",
          "original_form": "헉 떡볶이 맛있죠",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042360.1.9",
          "form": "로제떡볶이 드셔보셨나요 유행하던데",
          "original_form": "로제떡볶이 드셔보셨나요 유행하던데",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042360.1.10",
          "form": "로제여기저기먹어봤는데\r\n역시 그냥떡볶이가짱인듯합니다ㅋㅋㅋㅋㄱ",
          "original_form": "로제여기저기먹어봤는데\r\n역시 그냥떡볶이가짱인듯합니다ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042360.1.11",
          "form": "뭔가느끼하더라구요저는!",
          "original_form": "뭔가느끼하더라구요저는!",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042360.1.12",
          "form": "드셔보셧나용",
          "original_form": "드셔보셧나용",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042360.1.13",
          "form": "전 안먹어봤어요ㅋㅋㅠ그래서 궁금하더라구요",
          "original_form": "전 안먹어봤어요ㅋㅋㅠ그래서 궁금하더라구요",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042360.1.14",
          "form": "느끼한거 별로시면 강추하진않아요!  그냥새로운맛궁금하시면도전하실만하지만ㅎㅎ",
          "original_form": "느끼한거 별로시면 강추하진않아요!  그냥새로운맛궁금하시면도전하실만하지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042360.1.15",
          "form": "한식파이신가융",
          "original_form": "한식파이신가융",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042360.1.16",
          "form": "전 그냥 다먹어요ㅎㅎ.....",
          "original_form": "전 그냥 다먹어요ㅎㅎ.....",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042360.1.17",
          "form": "일식도 먹고 중식도먹고 한식도먹고",
          "original_form": "일식도 먹고 중식도먹고 한식도먹고",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042360.1.18",
          "form": "가리는거없으셔용?",
          "original_form": "가리는거없으셔용?",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042360.1.19",
          "form": "어... 버섯싫어해요!",
          "original_form": "어... 버섯싫어해요!",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042360.1.20",
          "form": "아 그리고 마라탕 불호예요ㅠ",
          "original_form": "아 그리고 마라탕 불호예요ㅠ",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042360.1.21",
          "form": "아 저도마라탕 진짜불호ㅜㅜㅜㅜㅜㅠ",
          "original_form": "아 저도마라탕 진짜불호ㅜㅜㅜㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042360.1.22",
          "form": "가리는것도많지만...ㅎㅎ",
          "original_form": "가리는것도많지만...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042360.1.23",
          "form": "하...친구들 다 마라탕먹으러가는데",
          "original_form": "하...친구들 다 마라탕먹으러가는데",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042360.1.24",
          "form": "저만 덩그러니..",
          "original_form": "저만 덩그러니..",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042360.1.25",
          "form": "아.. 전정말싫던데..;-;",
          "original_form": "아.. 전정말싫던데..;-;",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042360.1.26",
          "form": "혀가마비되는것도너무싫구..허허",
          "original_form": "혀가마비되는것도너무싫구..허허",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042360.1.27",
          "form": "다 먹고나면 그냥 너무 느글거려요... 비싼건드럽게 비싼데 돈아까워요",
          "original_form": "다 먹고나면 그냥 너무 느글거려요... 비싼건드럽게 비싼데 돈아까워요",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042360.1.28",
          "form": "맞아요가격도세고",
          "original_form": "맞아요가격도세고",
          "speaker_id": "1",
