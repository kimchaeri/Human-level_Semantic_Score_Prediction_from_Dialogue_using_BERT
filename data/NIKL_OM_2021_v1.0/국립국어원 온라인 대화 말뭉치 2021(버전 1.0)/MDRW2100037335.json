      "utterance": [
        {
          "id": "MDRW2100037335.1.1",
          "form": "너는 여러번 본 영화가잇니?",
          "original_form": "너는 여러번 본 영화가잇니?",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037335.1.2",
          "form": "음 나는 아저씨를 많이 봣어",
          "original_form": "음 나는 아저씨를 많이 봣어",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.3",
          "form": "아저씨 봤어?",
          "original_form": "아저씨 봤어?",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.4",
          "form": "원빈이 나오는 영화야",
          "original_form": "원빈이 나오는 영화야",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.5",
          "form": "요즘은 원빈이 뜸한데 그당시엔 굉장히 핫했어",
          "original_form": "요즘은 원빈이 뜸한데 그당시엔 굉장히 핫했어",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.6",
          "form": "응응 근데 나는 한번만봤어!",
          "original_form": "응응 근데 나는 한번만봤어!",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.7",
          "form": "원빈이 마지막으로 찍은 영화지?",
          "original_form": "원빈이 마지막으로 찍은 영화지?",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.8",
          "form": "나는 아저씨라는 영화 대사도 외워",
          "original_form": "나는 아저씨라는 영화 대사도 외워",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.9",
          "form": "음. 잘 몰라",
          "original_form": "음. 잘 몰라",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.10",
          "form": "세상에 어떤대화야?",
          "original_form": "세상에 어떤대화야?",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.11",
          "form": "요즘 내 기억에서 잊혔거든",
          "original_form": "요즘 내 기억에서 잊혔거든",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.12",
          "form": "어떤 대사야??",
          "original_form": "어떤 대사야??",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.13",
          "form": "새론이가 장난식으로 하는 말이야",
          "original_form": "새론이가 장난식으로 하는 말이야",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.14",
          "form": "원빈대사는 솔직히 오글거려",
          "original_form": "원빈대사는 솔직히 오글거려",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.15",
          "form": "너는 그영화가 재개봉하면 보러갈꺼니?",
          "original_form": "너는 그영화가 재개봉하면 보러갈꺼니?",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.16",
          "form": "그냥 영화가 원빈 얼굴보려고 보는 영화야",
          "original_form": "그냥 영화가 원빈 얼굴보려고 보는 영화야",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.17",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037335.1.18",
          "form": "나는 돈이 소중해",
          "original_form": "나는 돈이 소중해",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037335.1.19",
          "form": "단호하구나",
          "original_form": "단호하구나",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037335.1.20",
          "form": "물론 시간도 소중해",
          "original_form": "물론 시간도 소중해",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037335.1.21",
          "form": "만약에 오천원이라면??",
          "original_form": "만약에 오천원이라면??",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037335.1.22",
          "form": "오백원이라도 안가.",
          "original_form": "오백원이라도 안가.",
          "speaker_id": "1",
          "time": "20210831 22:54"
        }
      ]
