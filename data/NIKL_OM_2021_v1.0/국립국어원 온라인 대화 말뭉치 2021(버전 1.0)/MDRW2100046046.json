          "original_form": "한국인 종특 발언",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046046.1.28",
          "form": "ㅋ ㅋ ㅋ ㅋ  ㅋ ㅋ ㅋ ㅋ ㅋ",
          "original_form": "ㅋ ㅋ ㅋ ㅋ  ㅋ ㅋ ㅋ ㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
