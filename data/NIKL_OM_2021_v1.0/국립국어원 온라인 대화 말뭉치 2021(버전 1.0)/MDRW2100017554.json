      "utterance": [
        {
          "id": "MDRW2100017554.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017554.1.2",
          "form": "안녕하세요! 다음 학기 다니세요?",
          "original_form": "안녕하세요! 다음 학기 다니세요?",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017554.1.3",
          "form": "네 ㅋㅋ 비대면이에요",
          "original_form": "네 ㅋㅋ 비대면이에요",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017554.1.4",
          "form": "오 전면 비대면인가요!",
          "original_form": "오 전면 비대면인가요!",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017554.1.5",
          "form": "좋네요ㅎㅎ",
          "original_form": "좋네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017554.1.6",
          "form": "네~ 사실 전 유학생이라 어쩔수없는선택이었어요 ㅋㅋ",
          "original_form": "네~ 사실 전 유학생이라 어쩔수없는선택이었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017554.1.7",
          "form": "헉 정말요???",
          "original_form": "헉 정말요???",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017554.1.8",
          "form": "한국으로 유학 오신 거에요? 아니면 타국으로 유학 가신 건가요?",
          "original_form": "한국으로 유학 오신 거에요? 아니면 타국으로 유학 가신 건가요?",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017554.1.9",
          "form": "타국으로 유학가는가는거요 ㅎㅎ전 한국인이에용",
          "original_form": "타국으로 유학가는가는거요 ㅎㅎ전 한국인이에용",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017554.1.10",
          "form": "와 저 주변에 유학가는 친구는 없어서 신기해요",
          "original_form": "와 저 주변에 유학가는 친구는 없어서 신기해요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017554.1.11",
          "form": "멋져요!!",
          "original_form": "멋져요!!",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017554.1.12",
          "form": "아하~ 아녀요 ㅎㅎ 외국어 실력 살리려구 가는거죠 뭐~",
          "original_form": "아하~ 아녀요 ㅎㅎ 외국어 실력 살리려구 가는거죠 뭐~",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017554.1.13",
          "form": "어떤 나라로 가세요?",
          "original_form": "어떤 나라로 가세요?",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017554.1.14",
          "form": "저는 중국이요 ㅎㅎ",
          "original_form": "저는 중국이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017554.1.15",
          "form": "저 중국어 좋아해요ㅜㅠ",
          "original_form": "저 중국어 좋아해요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017554.1.16",
          "form": "나중에 제대로 공부하고 싶어요",
          "original_form": "나중에 제대로 공부하고 싶어요",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017554.1.17",
          "form": "혹시 전공이 중국어세요?",
          "original_form": "혹시 전공이 중국어세요?",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017554.1.18",
          "form": "아정말요?? 저는 요새 하루종일 중드만보고살아요 ㅋㅋ",
          "original_form": "아정말요?? 저는 요새 하루종일 중드만보고살아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017554.1.19",
          "form": "아뇨 ㅋㅋ 전공은 경영이에여~",
          "original_form": "아뇨 ㅋㅋ 전공은 경영이에여~",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017554.1.20",
          "form": "와 중드..! 저 추천해주세요!!",
          "original_form": "와 중드..! 저 추천해주세요!!",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017554.1.21",
          "form": "저두 전공 경영이에요 뭔가 반가워요ㅎㅎ",
