      "utterance": [
        {
          "id": "MDRW2100012369.1.1",
          "form": "주위에 군대 가신 분",
          "original_form": "주위에 군대 가신 분",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100012369.1.2",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100012369.1.3",
          "form": "아뇨.. 이미 다 예비군 끝난 사람들.. ㅋㅋㅋ",
          "original_form": "아뇨.. 이미 다 예비군 끝난 사람들.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012369.1.4",
          "form": "군대갔다오셧나여?",
          "original_form": "군대갔다오셧나여?",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012369.1.5",
          "form": "아니용..",
          "original_form": "아니용..",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012369.1.6",
          "form": "군대를 안가요",
          "original_form": "군대를 안가요",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012369.1.7",
          "form": "부럽다.. 안갈수있음 안가야죠..",
          "original_form": "부럽다.. 안갈수있음 안가야죠..",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012369.1.8",
          "form": "군대 갔다왔어쿄?",
          "original_form": "군대 갔다왔어쿄?",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012369.1.9",
          "form": "전 의경 갔다왔어요..",
          "original_form": "전 의경 갔다왔어요..",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012369.1.10",
          "form": "아하 어땠나요",
          "original_form": "아하 어땠나요",
          "speaker_id": "2",
          "time": "20210824 22:10"
        },
        {
          "id": "MDRW2100012369.1.11",
          "form": "생각하기 싫죠.. 너무 힘들고 가혹행위도 많았고..",
          "original_form": "생각하기 싫죠.. 너무 힘들고 가혹행위도 많았고..",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012369.1.12",
          "form": "의경이라 세상 더러운거 많이 보고..",
          "original_form": "의경이라 세상 더러운거 많이 보고..",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012369.1.13",
          "form": "가혹행위요????",
          "original_form": "가혹행위요????",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012369.1.14",
          "form": "군대네 구타 폭언 이런거 많은 시절이었어서..",
          "original_form": "군대네 구타 폭언 이런거 많은 시절이었어서..",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012369.1.15",
          "form": "헉.. 신고는 못하나요?",
          "original_form": "헉.. 신고는 못하나요?",
          "speaker_id": "2",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012369.1.16",
          "form": "회사에서 부장 싫다고 신고 못하잖아요..",
          "original_form": "회사에서 부장 싫다고 신고 못하잖아요..",
          "speaker_id": "1",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012369.1.17",
          "form": "폐쇄된 곳에서 도망갈곳없음.. 아무거도 못하죠..",
          "original_form": "폐쇄된 곳에서 도망갈곳없음.. 아무거도 못하죠..",
          "speaker_id": "1",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012369.1.18",
          "form": "아하 ㅠㅠㅠㅠㅠ",
          "original_form": "아하 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012369.1.19",
          "form": "제 가족도 군대 가있거든요",
          "original_form": "제 가족도 군대 가있거든요",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012369.1.20",
          "form": "요즘은 스마트폰도 있고 많이 바뀌었을거에요..",
          "original_form": "요즘은 스마트폰도 있고 많이 바뀌었을거에요..",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012369.1.21",
          "form": "요즘 제대한 친구들 얘기 들어보면 그냥 학교같은 분위기라 그러더라고요",
          "original_form": "요즘 제대한 친구들 얘기 들어보면 그냥 학교같은 분위기라 그러더라고요",
          "speaker_id": "1",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012369.1.22",
          "form": "네 연락도 자주 하더라구요",
          "original_form": "네 연락도 자주 하더라구요",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012369.1.23",
          "form": "군대도 변해야죠... 사람사는곳인데..",
          "original_form": "군대도 변해야죠... 사람사는곳인데..",
          "speaker_id": "1",
          "time": "20210824 22:36"
        }
      ]
    }
