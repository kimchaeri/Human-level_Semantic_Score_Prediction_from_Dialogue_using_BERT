      "utterance": [
        {
          "id": "MDRW2100023699.1.1",
          "form": "가족이 무엇이라고 생각하나요?",
          "original_form": "가족이 무엇이라고 생각하나요?",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023699.1.2",
          "form": "룸메는 주간가셨나요?",
          "original_form": "룸메는 주간가셨나요?",
          "speaker_id": "2",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100023699.1.3",
          "form": "아주머니",
          "original_form": "아주머니",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100023699.1.4",
          "form": "name2~자동차는 잘 받았습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "&name2&~자동차는 잘 받았습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 07:27"
        },
        {
          "id": "MDRW2100023699.1.5",
          "form": "싸울꺼라고 예상은했지만..참담했네여ㅠ ㅋㅋㅋㅋ",
          "original_form": "싸울꺼라고 예상은했지만..참담했네여ㅠ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 07:27"
        },
        {
          "id": "MDRW2100023699.1.6",
          "form": "맘에 드나보네요",
          "original_form": "맘에 드나보네요",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100023699.1.7",
          "form": "싸게 잘샀나요?",
          "original_form": "싸게 잘샀나요?",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100023699.1.8",
          "form": "가족끼리는 싸우고 크는거죠 ㅋㅋㅋ",
          "original_form": "가족끼리는 싸우고 크는거죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100023699.1.9",
          "form": "싸우고 크는 거 맞죠??",
          "original_form": "싸우고 크는 거 맞죠??",
          "speaker_id": "1",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100023699.1.10",
          "form": "등지는거 아니겠죠ㅋㅋㅋ",
          "original_form": "등지는거 아니겠죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100023699.1.11",
          "form": "싸우고 큰다네요",
          "original_form": "싸우고 큰다네요",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100023699.1.12",
          "form": "저는 몰라요",
          "original_form": "저는 몰라요",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100023699.1.13",
          "form": "독자입니다",
          "original_form": "독자입니다",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100023699.1.14",
          "form": "옛말 아닌가 싶습니다~요즘은 싸우다 갈라설듯요ㅠ ㅋㅋㅋㅋ",
          "original_form": "옛말 아닌가 싶습니다~요즘은 싸우다 갈라설듯요ㅠ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100023699.1.15",
          "form": "독자가 좋습니다요#",
          "original_form": "독자가 좋습니다요#",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100023699.1.16",
          "form": "하나만 낳을라공ㅂ",
          "original_form": "하나만 낳을라공ㅂ",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100023699.1.17",
          "form": "합니다",
          "original_form": "합니다",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100023699.1.18",
          "form": "둘 안낳을려구요",
          "original_form": "둘 안낳을려구요",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100023699.1.19",
          "form": "다행이 월경시작",
          "original_form": "다행이 월경시작",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100023699.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ하나 잘 키우는게 낫심더 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ하나 잘 키우는게 낫심더 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100023699.1.21",
          "form": "내 편하자고 포기하는거죠",
          "original_form": "내 편하자고 포기하는거죠",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100023699.1.22",
          "form": "심심하면 만나면 됩니다 ㅋㅋㅋㅋㅋ",
          "original_form": "심심하면 만나면 됩니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100023699.1.23",
          "form": "아들생각하면 미안합니다",
          "original_form": "아들생각하면 미안합니다",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100023699.1.24",
          "form": "오 어디사세요?",
          "original_form": "오 어디사세요?",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100023699.1.25",
          "form": "말투가 시골같네요",
          "original_form": "말투가 시골같네요",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100023699.1.26",
          "form": "가사일 바쁜데 오실수 있나요",
          "original_form": "가사일 바쁜데 오실수 있나요",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100023699.1.27",
          "form": "시골입니더 ㅋㅋㅋㅋㅋ",
          "original_form": "시골입니더 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100023699.1.28",
          "form": "주말엔 후리하지요 ㅋㅋㅋㅋ",
          "original_form": "주말엔 후리하지요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100023699.1.29",
          "form": "주말엔 주로 뭐하시나요?가족과 여행가시나요?",
          "original_form": "주말엔 주로 뭐하시나요?가족과 여행가시나요?",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100023699.1.30",
          "form": "신랑 출근 안하면 거의 밖으로 다닙니더~큰애 두발 자전거 연습해야하는데..시간이 없네요;;",
          "original_form": "신랑 출근 안하면 거의 밖으로 다닙니더~큰애 두발 자전거 연습해야하는데..시간이 없네요;;",
          "speaker_id": "1",
          "time": "20210831 10:49"
        },
        {
          "id": "MDRW2100023699.1.31",
          "form": "외발자전거는 어떳습니까?",
          "original_form": "외발자전거는 어떳습니까?",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100023699.1.32",
          "form": "서커스 보내야 합니까??ㅋㅋㅋㅋ",
          "original_form": "서커스 보내야 합니까??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100023699.1.33",
          "form": "석커스",
          "original_form": "석커스",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100023699.1.34",
          "form": "두발자전거 타면 진짜 다 키웟네요",
          "original_form": "두발자전거 타면 진짜 다 키웟네요",
