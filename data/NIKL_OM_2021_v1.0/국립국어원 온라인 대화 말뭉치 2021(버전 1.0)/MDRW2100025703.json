      "utterance": [
        {
          "id": "MDRW2100025703.1.1",
          "form": "맞아ㅋㅋ 너 차차차 봤어?",
          "original_form": "맞아ㅋㅋ 너 차차차 봤어?",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025703.1.2",
          "form": "응 뻔하고 올드하긴한데ㅠ그랴도 볼거없을때 볼만은해ㅋㅋㅋ",
          "original_form": "응 뻔하고 올드하긴한데ㅠ그랴도 볼거없을때 볼만은해ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025703.1.3",
          "form": "엄청 재밌진 않구만..",
          "original_form": "엄청 재밌진 않구만..",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025703.1.4",
          "form": "김선호는 확실히 정장보단 후줄근한게 잘 어울려",
          "original_form": "김선호는 확실히 정장보단 후줄근한게 잘 어울려",
          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025703.1.5",
          "form": "ㅋㅋㅋ마자 좀 너드미라고 해야하나?ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ마자 좀 너드미라고 해야하나?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025703.1.6",
          "form": "맞아 엄청 재밌진않아ㅋㅋ\r\n그냥 1박보고 차차차보고 대탈출보고 일케 봐ㅋㅋ",
          "original_form": "맞아 엄청 재밌진않아ㅋㅋ\r\n그냥 1박보고 차차차보고 대탈출보고 일케 봐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025703.1.7",
          "form": "딱 일요일 코스네ㅋㅋ",
          "original_form": "딱 일요일 코스네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025703.1.8",
          "form": "아니근데 겉모습만 후줄근하고 성격은 좀 별로던데?\r\n스타텁때도 그랴ㅛ고\r\n동네 할머니들한테는 서글서글하긴 한데",
          "original_form": "아니근데 겉모습만 후줄근하고 성격은 좀 별로던데?\r\n스타텁때도 그랴ㅛ고\r\n동네 할머니들한테는 서글서글하긴 한데",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025703.1.9",
          "form": "아 그래? 난 고분고분게 좋은데...",
          "original_form": "아 그래? 난 고분고분게 좋은데...",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025703.1.10",
          "form": "진짜 할머니랑 잘 어울리는 친구야",
          "original_form": "진짜 할머니랑 잘 어울리는 친구야",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025703.1.11",
          "form": "마자 딱 손자같아 잘어울려",
          "original_form": "마자 딱 손자같아 잘어울려",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025703.1.12",
          "form": "그치ㅋㅋ 그게 영화 홍반장 리메이크한거라는데",
          "original_form": "그치ㅋㅋ 그게 영화 홍반장 리메이크한거라는데",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025703.1.13",
          "form": "그니까 리메이크 했다며?",
          "original_form": "그니까 리메이크 했다며?",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025703.1.14",
          "form": "그래서근가 올드한게있어",
          "original_form": "그래서근가 올드한게있어",
          "speaker_id": "2",
          "time": "20210901 16:13"
        }
