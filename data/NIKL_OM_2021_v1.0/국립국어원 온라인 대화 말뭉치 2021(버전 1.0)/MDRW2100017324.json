      "utterance": [
        {
          "id": "MDRW2100017324.1.1",
          "form": "아무래도 힘이 많이 들어가니까",
          "original_form": "아무래도 힘이 많이 들어가니까",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017324.1.2",
          "form": "안전한게 좋을거같네요",
          "original_form": "안전한게 좋을거같네요",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017324.1.3",
          "form": "네네 ㅎㅎ 저는 일단 지금은 괜찮은데",
          "original_form": "네네 ㅎㅎ 저는 일단 지금은 괜찮은데",
          "speaker_id": "1",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017324.1.4",
          "form": "혹시 몰라서",
          "original_form": "혹시 몰라서",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.5",
          "form": "철봉대를 하나 살까 고민 중이에요",
          "original_form": "철봉대를 하나 살까 고민 중이에요",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.6",
          "form": "철봉대요?",
          "original_form": "철봉대요?",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.7",
          "form": "그거 있음 좀 더 안정적인가요",
          "original_form": "그거 있음 좀 더 안정적인가요",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.8",
          "form": "네네 그냥 아예 철봉을 설치하는거죠",
          "original_form": "네네 그냥 아예 철봉을 설치하는거죠",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.9",
          "form": "아.. 철봉을? 집에다?",
          "original_form": "아.. 철봉을? 집에다?",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.10",
          "form": "네 그건 문 사이에 끼우는 게 아니라",
          "original_form": "네 그건 문 사이에 끼우는 게 아니라",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.11",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.12",
          "form": "철봉 자체를 설치!",
          "original_form": "철봉 자체를 설치!",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.13",
          "form": "기구죠 기구!",
          "original_form": "기구죠 기구!",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017324.1.14",
          "form": "와 홈트용품의 세계란",
          "original_form": "와 홈트용품의 세계란",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017324.1.15",
          "form": "설치하면 완전 헬스장 느낌나요 ㅎㅎ",
          "original_form": "설치하면 완전 헬스장 느낌나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017324.1.16",
