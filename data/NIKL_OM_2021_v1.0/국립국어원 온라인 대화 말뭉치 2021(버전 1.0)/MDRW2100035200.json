      "utterance": [
        {
          "id": "MDRW2100035200.1.1",
          "form": "여행좋아하시나욤",
          "original_form": "여행좋아하시나욤",
          "speaker_id": "1",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100035200.1.2",
          "form": "엄청 좋아했죠.. 작년부터 묶인 몸이지만요",
          "original_form": "엄청 좋아했죠.. 작년부터 묶인 몸이지만요",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100035200.1.3",
          "form": "저두요ㅜ",
          "original_form": "저두요ㅜ",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100035200.1.4",
          "form": "여름휴가는 다녀오셧나요",
          "original_form": "여름휴가는 다녀오셧나요",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100035200.1.5",
          "form": "작년부터 아무데도 안갔어요.. 우리식구 모두 그냥 집이랑 동네공원 정도",
          "original_form": "작년부터 아무데도 안갔어요.. 우리식구 모두 그냥 집이랑 동네공원 정도",
          "speaker_id": "2",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100035200.1.6",
          "form": "아이고ㅜㅜ 대단하셔요",
          "original_form": "아이고ㅜㅜ 대단하셔요",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100035200.1.7",
          "form": "많이 갑갑하시져ㅜㅜ",
          "original_form": "많이 갑갑하시져ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035200.1.8",
          "form": "기억남는 여행지잇으신가요",
          "original_form": "기억남는 여행지잇으신가요",
          "speaker_id": "1",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100035200.1.9",
          "form": "국내는 제주도가 기억에 남아요~ 2번  갔었는데.. 2번째때 인생 노을을 만났거든요..ㅎㅎ",
          "original_form": "국내는 제주도가 기억에 남아요~ 2번  갔었는데.. 2번째때 인생 노을을 만났거든요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100035200.1.10",
          "form": "오 저도 노을덕후에여",
          "original_form": "오 저도 노을덕후에여",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100035200.1.11",
          "form": "제주도 진짜 많이갓는데 ㅎ",
          "original_form": "제주도 진짜 많이갓는데 ㅎ",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100035200.1.12",
          "form": "좋져 ㅎㅎ",
          "original_form": "좋져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100035200.1.13",
          "form": "많이 가보셨나요? 전 2번 갔는데.. 첫번째는 그냥 일행들 따라다닌거라 큰 기억은 안나고 녹차밭하고 잠수함탄거 기억나네요..ㅎㅎ",
          "original_form": "많이 가보셨나요? 전 2번 갔는데.. 첫번째는 그냥 일행들 따라다닌거라 큰 기억은 안나고 녹차밭하고 잠수함탄거 기억나네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100035200.1.14",
          "form": "ㅋㅋ많이갔어요",
          "original_form": "ㅋㅋ많이갔어요",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100035200.1.15",
          "form": "올해만 두번다녀왓어요",
          "original_form": "올해만 두번다녀왓어요",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100035200.1.16",
          "form": "한라산 등산햇는데 진짜 경치가 넘 넘 좋앗어요",
          "original_form": "한라산 등산햇는데 진짜 경치가 넘 넘 좋앗어요",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100035200.1.17",
          "form": "한라산은 도전 못했어요.. 등산을 안좋아해서요..ㅋㅋㅋ 평지만 찾아다녔죠..ㅎ",
          "original_form": "한라산은 도전 못했어요.. 등산을 안좋아해서요..ㅋㅋㅋ 평지만 찾아다녔죠..ㅎ",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100035200.1.18",
          "form": "ㅎㅎ저도 싫어하는데 윗세오름은 경치가넘좋아서 참고 다녀왓어요 흑흑",
          "original_form": "ㅎㅎ저도 싫어하는데 윗세오름은 경치가넘좋아서 참고 다녀왓어요 흑흑",
          "speaker_id": "1",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100035200.1.19",
          "form": "언제쯤 코로나 사라질까유",
          "original_form": "언제쯤 코로나 사라질까유",
          "speaker_id": "1",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100035200.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100035200.1.21",
          "form": "그니까요.. 해외는 커녕 국내도 맘편히 다니지도 못하는 이 상황이 싫으네요..",
          "original_form": "그니까요.. 해외는 커녕 국내도 맘편히 다니지도 못하는 이 상황이 싫으네요..",
          "speaker_id": "2",
          "time": "20210829 12:12"
        },
        {
          "id": "MDRW2100035200.1.22",
          "form": "제주도 다음에 가게 되면 한라산도 도전해봐야겠어요",
          "original_form": "제주도 다음에 가게 되면 한라산도 도전해봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 12:12"
        },
        {
          "id": "MDRW2100035200.1.23",
          "form": "힘들지만 경치가 오져요 ㅎㅎㅎ",
          "original_form": "힘들지만 경치가 오져요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100035200.1.24",
          "form": "이대로가다간 감기처럼 같이 살아야하나여",
          "original_form": "이대로가다간 감기처럼 같이 살아야하나여",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100035200.1.25",
          "form": "끔찍해요 흑",
          "original_form": "끔찍해요 흑",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100035200.1.26",
          "form": "왠시 그럴거 같지만 그래도 국내여행 정도는 내년쯤 눈치 안보고 다닐 수 있을 거 같아요",
          "original_form": "왠시 그럴거 같지만 그래도 국내여행 정도는 내년쯤 눈치 안보고 다닐 수 있을 거 같아요",
          "speaker_id": "2",
          "time": "20210829 12:41"
        }
      ]
