      "utterance": [
        {
          "id": "MDRW2100032260.1.1",
          "form": "볼 때마다 미어 터지더라구요",
          "original_form": "볼 때마다 미어 터지더라구요",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032260.1.2",
          "form": "고양이 키우실 때",
          "original_form": "고양이 키우실 때",
          "speaker_id": "2",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032260.1.3",
          "form": "밥을 어떤 걸로 주셨어요",
          "original_form": "밥을 어떤 걸로 주셨어요",
          "speaker_id": "2",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032260.1.4",
          "form": "요즘에는 강아지들도 유치원 데리고 가고 호텔도 가고 하더라고요",
          "original_form": "요즘에는 강아지들도 유치원 데리고 가고 호텔도 가고 하더라고요",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032260.1.5",
          "form": "전에 아침 홍보 행사 갔다가 강아지 유치원 찾아 보았습니다",
          "original_form": "전에 아침 홍보 행사 갔다가 강아지 유치원 찾아 보았습니다",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032260.1.6",
          "form": "사료를 줬나요",
          "original_form": "사료를 줬나요",
          "speaker_id": "2",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032260.1.7",
          "form": "너무 오래전이라 기억이 잘 안나요",
          "original_form": "너무 오래전이라 기억이 잘 안나요",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.8",
          "form": "강아지들 낮잠도 자더라구요",
          "original_form": "강아지들 낮잠도 자더라구요",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.9",
          "form": "멸치도 주고 했던 기억이 있네요",
          "original_form": "멸치도 주고 했던 기억이 있네요",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.10",
          "form": "너무 귀엽더라고요 옹기종기",
          "original_form": "너무 귀엽더라고요 옹기종기",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.11",
          "form": "멸치를 골라 먹었던 기억이 나네요",
          "original_form": "멸치를 골라 먹었던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.12",
          "form": "좋은 거 먹었네요",
          "original_form": "좋은 거 먹었네요",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.13",
          "form": "반장도 없지요",
          "original_form": "반장도 없지요",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.14",
          "form": "없지요",
          "original_form": "없지요",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.15",
          "form": "뽑지요",
          "original_form": "뽑지요",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.16",
          "form": "그런 발상이 신기해요",
          "original_form": "그런 발상이 신기해요",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.17",
          "form": "그러면 개과천선 하나요",
          "original_form": "그러면 개과천선 하나요",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.18",
          "form": "다 같이 낮잠을 자다니 시간 맞춰 자는게 너무 신기하더라고요",
          "original_form": "다 같이 낮잠을 자다니 시간 맞춰 자는게 너무 신기하더라고요",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.19",
          "form": "나름 규칙이 있더라구요",
          "original_form": "나름 규칙이 있더라구요",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032260.1.20",
          "form": "이런 귀요미들",
          "original_form": "이런 귀요미들",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032260.1.21",
          "form": "정말 너무너무 귀염뽀짝 하지 않나요 유치원 이라니",
          "original_form": "정말 너무너무 귀염뽀짝 하지 않나요 유치원 이라니",
          "speaker_id": "1",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032260.1.22",
          "form": "거기서 아르바이트 하고 싶어요",
          "original_form": "거기서 아르바이트 하고 싶어요",
