          "speaker_id": "2",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100021623.1.10",
          "form": "아침이요?",
          "original_form": "아침이요?",
          "speaker_id": "2",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100021623.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 08:04"
        },
        {
          "id": "MDRW2100021623.1.12",
          "form": "아 전 계란 두알",
          "original_form": "아 전 계란 두알",
          "speaker_id": "2",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100021623.1.13",
          "form": "아침은 간단히요 ㅎㅎ",
          "original_form": "아침은 간단히요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100021623.1.14",
          "form": "넹 과하게 먹으니 좀 힘들어요",
          "original_form": "넹 과하게 먹으니 좀 힘들어요",
          "speaker_id": "2",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100021623.1.15",
          "form": "더부룩하고",
          "original_form": "더부룩하고",
          "speaker_id": "1",
          "time": "20210902 08:08"
        },
        {
          "id": "MDRW2100021623.1.16",
          "form": "그리고 커피 테이크아웃 해서 출근해용",
          "original_form": "그리고 커피 테이크아웃 해서 출근해용",
          "speaker_id": "2",
          "time": "20210902 08:09"
        },
        {
          "id": "MDRW2100021623.1.17",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100021623.1.18",
          "form": "네 근데 너무 단답하시니까",
          "original_form": "네 근데 너무 단답하시니까",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100021623.1.19",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "2",
