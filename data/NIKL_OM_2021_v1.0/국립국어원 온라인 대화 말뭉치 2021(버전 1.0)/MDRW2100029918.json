      },
      "utterance": [
        {
          "id": "MDRW2100029918.1.1",
          "form": "그런 의도로 있는건 아닐듯?ㅋㅋㅋㅋ",
          "original_form": "그런 의도로 있는건 아닐듯?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029918.1.2",
          "form": "엥 그럼 종이 낭비하면서 왜 있어?",
          "original_form": "엥 그럼 종이 낭비하면서 왜 있어?",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029918.1.3",
          "form": "작가님한테 물어바 출판사나...",
          "original_form": "작가님한테 물어바 출판사나...",
          "speaker_id": "2",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100029918.1.4",
          "form": "등장인물 까먹는 사람때문에 있는것은 아닐것",
          "original_form": "등장인물 까먹는 사람때문에 있는것은 아닐것",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029918.1.5",
          "form": "요즘은 나와서 모를 것 같야",
          "original_form": "요즘은 나와서 모를 것 같야",
          "speaker_id": "1",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029918.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029918.1.7",
          "form": "책은 계속 생기는데",
          "original_form": "책은 계속 생기는데",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029918.1.8",
          "form": "읽지는 않는중... 언제 다읽어",
          "original_form": "읽지는 않는중... 언제 다읽어",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029918.1.9",
          "form": "그르게",
          "original_form": "그르게",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029918.1.10",
          "form": "아직도 회지도 다 못 읽었는디",
          "original_form": "아직도 회지도 다 못 읽었는디",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029918.1.11",
          "form": "맞아 읽어야지 해놓고... 몇권 밀렸지ㅋㅋㅋ",
          "original_form": "맞아 읽어야지 해놓고... 몇권 밀렸지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029918.1.12",
          "form": "목표가 없으면 안 하는 인간 같애",
          "original_form": "목표가 없으면 안 하는 인간 같애",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029918.1.13",
          "form": "지금 읽는 것도 끝이 정해져있어서",
          "original_form": "지금 읽는 것도 끝이 정해져있어서",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100029918.1.14",
          "form": "읽는 듯",
          "original_form": "읽는 듯",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100029918.1.15",
          "form": "김연아랑 유재석은 목표없이 산댔는데...",
          "original_form": "김연아랑 유재석은 목표없이 산댔는데...",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029918.1.16",
          "form": "그런데도 그렇게 부지런하다니",
          "original_form": "그런데도 그렇게 부지런하다니",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029918.1.17",
          "form": "종이 다른 듷",
          "original_form": "종이 다른 듷",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029918.1.18",
          "form": "외계인인가? 믿을수있음ㅋㅋㅋㅋ",
          "original_form": "외계인인가? 믿을수있음ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029918.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ때가 되면 걍 하는 건가.... 어떻게 사람이....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ때가 되면 걍 하는 건가.... 어떻게 사람이....",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029918.1.20",
          "form": "역시... 외계인...",
          "original_form": "역시... 외계인...",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100029918.1.21",
          "form": "신문도 읽고 책도 읽고 공부도 하고...",
          "original_form": "신문도 읽고 책도 읽고 공부도 하고...",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029918.1.22",
          "form": "난 수험서도 별로 못봤는디",
          "original_form": "난 수험서도 별로 못봤는디",
          "speaker_id": "2",
          "time": "20210903 17:02"
        }
      ]
