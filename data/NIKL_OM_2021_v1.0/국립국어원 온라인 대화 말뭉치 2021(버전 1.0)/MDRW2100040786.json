      "utterance": [
        {
          "id": "MDRW2100040786.1.1",
          "form": "아니요 백신 안 맞았어요 안 맞고 싶어요",
          "original_form": "아니요 백신 안 맞았어요 안 맞고 싶어요",
          "speaker_id": "2",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100040786.1.2",
          "form": "이유가 있으신가요?",
          "original_form": "이유가 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100040786.1.3",
          "form": "밖에 잘 안 나가는데 굳이 죽을 위한 무릅쓰면서 맞은 이유를 못 느끼겠어요",
          "original_form": "밖에 잘 안 나가는데 굳이 죽을 위한 무릅쓰면서 맞은 이유를 못 느끼겠어요",
          "speaker_id": "2",
          "time": "20210905 07:06"
        },
        {
          "id": "MDRW2100040786.1.4",
          "form": "그쵸 아무래도 걱정되죠ㅜ",
          "original_form": "그쵸 아무래도 걱정되죠ㅜ",
          "speaker_id": "1",
          "time": "20210905 07:06"
        },
        {
          "id": "MDRW2100040786.1.5",
          "form": "저는 엊그제 2차 접종까지 했는데",
          "original_form": "저는 엊그제 2차 접종까지 했는데",
          "speaker_id": "1",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040786.1.6",
          "form": "맞기 전엔 걱정 많이 되더라고요",
          "original_form": "맞기 전엔 걱정 많이 되더라고요",
          "speaker_id": "1",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040786.1.7",
          "form": "지금 어떠세요",
          "original_form": "지금 어떠세요",
          "speaker_id": "2",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040786.1.8",
          "form": "별다른 증상은 없으신가요?",
          "original_form": "별다른 증상은 없으신가요?",
          "speaker_id": "2",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040786.1.9",
          "form": "어제는 몸살기도 느껴지고 그랬는데",
          "original_form": "어제는 몸살기도 느껴지고 그랬는데",
          "speaker_id": "1",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040786.1.10",
          "form": "오늘은 괜찮아요",
          "original_form": "오늘은 괜찮아요",
          "speaker_id": "1",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040786.1.11",
          "form": "다이어트는 자주 하시는 편이세요?",
          "original_form": "다이어트는 자주 하시는 편이세요?",
          "speaker_id": "2",
          "time": "20210905 07:07"
        },
        {
          "id": "MDRW2100040786.1.12",
          "form": "ㅎㅎ늘 하는 편이죠",
          "original_form": "ㅎㅎ늘 하는 편이죠",
          "speaker_id": "1",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040786.1.13",
          "form": "저도 요즘에 살이 쪄서 하고 있는데요",
          "original_form": "저도 요즘에 살이 쪄서 하고 있는데요",
          "speaker_id": "2",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100040786.1.14",
          "form": "식이조절 하시는 거예요?",
          "original_form": "식이조절 하시는 거예요?",
          "speaker_id": "1",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040786.1.15",
          "form": "네 밥을 약간 적게 먹고 있어요",
          "original_form": "네 밥을 약간 적게 먹고 있어요",
          "speaker_id": "2",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100040786.1.16",
          "form": "다이어트는 식이가 중요하다고 하더라고요",
          "original_form": "다이어트는 식이가 중요하다고 하더라고요",
          "speaker_id": "1",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100040786.1.17",
          "form": "운동도 하시나요?",
          "original_form": "운동도 하시나요?",
          "speaker_id": "1",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100040786.1.18",
          "form": "이제 운동도 하려고요",
          "original_form": "이제 운동도 하려고요",
          "speaker_id": "2",
          "time": "20210905 07:13"
        }
      ]
