      "utterance": [
        {
          "id": "MDRW2100032733.1.1",
          "form": "혹시 하는 동호회같은 거 있어??",
          "original_form": "혹시 하는 동호회같은 거 있어??",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032733.1.2",
          "form": "음...딱히 없어!",
          "original_form": "음...딱히 없어!",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032733.1.3",
          "form": "name1는 있어??",
          "original_form": "&name1&는 있어??",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032733.1.4",
          "form": "나도 딱히 없는 거 같네....??",
          "original_form": "나도 딱히 없는 거 같네....??",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032733.1.5",
          "form": "아님 뭐 해보고 싶은 동호회라던가??",
          "original_form": "아님 뭐 해보고 싶은 동호회라던가??",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032733.1.6",
          "form": "나 약간 토론모임 그런거 해보고싶어...",
          "original_form": "나 약간 토론모임 그런거 해보고싶어...",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032733.1.7",
          "form": "헐랭!!나도~!",
          "original_form": "헐랭!!나도~!",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032733.1.8",
          "form": "뭐야 ㅋㅋㅋ",
          "original_form": "뭐야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032733.1.9",
          "form": "우리가 만들자!",
          "original_form": "우리가 만들자!",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032733.1.10",
          "form": "조아~!!",
          "original_form": "조아~!!",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032733.1.11",
          "form": "오 동호회 멤버는 어떻게 모으지...?",
          "original_form": "오 동호회 멤버는 어떻게 모으지...?",
          "speaker_id": "1",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032733.1.12",
          "form": "음...어떡하지...",
          "original_form": "음...어떡하지...",
          "speaker_id": "2",
          "time": "20210906 11:16"
        },
        {
          "id": "MDRW2100032733.1.13",
          "form": "일단 보통 먹을거로 꼬시던데...(?)",
          "original_form": "일단 보통 먹을거로 꼬시던데...(?)",
          "speaker_id": "2",
          "time": "20210906 11:16"
        },
        {
          "id": "MDRW2100032733.1.14",
          "form": "좋네 먹을 걸로 꼬시기...",
          "original_form": "좋네 먹을 걸로 꼬시기...",
          "speaker_id": "1",
          "time": "20210906 11:20"
        },
        {
          "id": "MDRW2100032733.1.15",
          "form": "그거 말고는 하고싶은 동호회 없어??",
          "original_form": "그거 말고는 하고싶은 동호회 없어??",
          "speaker_id": "1",
          "time": "20210906 11:20"
        },
        {
          "id": "MDRW2100032733.1.16",
          "form": "음...보드게임 동호회 같은거?",
          "original_form": "음...보드게임 동호회 같은거?",
          "speaker_id": "2",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100032733.1.17",
          "form": "혼자서는 못하니까ㅋㅋ큐ㅜㅠㅠ",
          "original_form": "혼자서는 못하니까ㅋㅋ큐ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100032733.1.18",
          "form": "나도 보드게임 좋아하는데!!",
          "original_form": "나도 보드게임 좋아하는데!!",
          "speaker_id": "1",
          "time": "20210906 11:28"
        },
        {
          "id": "MDRW2100032733.1.19",
          "form": "우리 만들 동호회 많네",
          "original_form": "우리 만들 동호회 많네",
          "speaker_id": "1",
          "time": "20210906 11:28"
        },
        {
          "id": "MDRW2100032733.1.20",
          "form": "응ㅋㅋㄱㅋㅋㄱㅋ",
          "original_form": "응ㅋㅋㄱㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100032733.1.21",
          "form": "그냥 한곳에서",
          "original_form": "그냥 한곳에서",
          "speaker_id": "2",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100032733.1.22",
          "form": "월요일 보드게임\r\n화요일 토론\r\n수요일 ...\r\n.\r\n.\r\n.\r\n.",
          "original_form": "월요일 보드게임\r\n화요일 토론\r\n수요일 ...\r\n.\r\n.\r\n.\r\n.",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100032733.1.23",
          "form": "하는건 어때?ㅋㅋ",
          "original_form": "하는건 어때?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100032733.1.24",
          "form": "좋아 동호회 만들자 ㅋㅋㅋ",
          "original_form": "좋아 동호회 만들자 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100032733.1.25",
          "form": "재밌겠다..",
          "original_form": "재밌겠다..",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100032733.1.26",
          "form": "막 하루만에 없어지는거 아냐?ㅋㅋ",
          "original_form": "막 하루만에 없어지는거 아냐?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100032733.1.27",
          "form": "아니야 그럼 안 돼 ㅠㅠ",
          "original_form": "아니야 그럼 안 돼 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100032733.1.28",
          "form": "그럼 계속 이어나가자~",
