      "utterance": [
        {
          "id": "MDRW2100024811.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024811.1.2",
          "form": "자취해서...그냥 최소만해요 ㅋㅋㅋ",
          "original_form": "자취해서...그냥 최소만해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024811.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024811.1.4",
          "form": "청소가 젤 어렵던데 ㅋㅋ 청소잘하시나요",
          "original_form": "청소가 젤 어렵던데 ㅋㅋ 청소잘하시나요",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024811.1.5",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024811.1.6",
          "form": "아 ! ㅋㅋㅋ 락스최고아닌가요",
          "original_form": "아 ! ㅋㅋㅋ 락스최고아닌가요",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024811.1.7",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100024811.1.8",
          "form": "그렇긴 한데 효과가 그만한게없는것같아요",
          "original_form": "그렇긴 한데 효과가 그만한게없는것같아요",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100024811.1.9",
          "form": "락스사용안하시나요?",
          "original_form": "락스사용안하시나요?",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100024811.1.10",
          "form": "하죠",
          "original_form": "하죠",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100024811.1.11",
          "form": "머리가 지끈지끈",
          "original_form": "머리가 지끈지끈",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100024811.1.12",
          "form": "그래도 냄새 덜한 락스 사용하면 조금이나마 낫더라구요 문다열고 환기돌리고 ㅠㅠ",
          "original_form": "그래도 냄새 덜한 락스 사용하면 조금이나마 낫더라구요 문다열고 환기돌리고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100024811.1.13",
          "form": "마스크해더 들어완여",
          "original_form": "마스크해더 들어완여",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100024811.1.14",
          "form": "심하네요",
          "original_form": "심하네요",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100024811.1.15",
          "form": "청소하시나요",
