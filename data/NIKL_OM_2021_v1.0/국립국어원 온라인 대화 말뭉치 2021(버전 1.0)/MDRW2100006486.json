      "utterance": [
        {
          "id": "MDRW2100006486.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ아깝네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아깝네",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006486.1.2",
          "form": "슬슬 시즌 마무리지?",
          "original_form": "슬슬 시즌 마무리지?",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006486.1.3",
          "form": "겨울 동안 야구 안해서 심심하겠네",
          "original_form": "겨울 동안 야구 안해서 심심하겠네",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006486.1.4",
          "form": "ㅋㅋㅋㅋㅋ아닠ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아닠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006486.1.5",
          "form": "머가 아까워..ㅎ",
          "original_form": "머가 아까워..ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006486.1.6",
          "form": "후후.. 겨울에 야구 안해?",
          "original_form": "후후.. 겨울에 야구 안해?",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006486.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ응응 가을야구가 시즌 마무리일걸",
          "original_form": "ㅋㅋㅋㅋㅋㅋ응응 가을야구가 시즌 마무리일걸",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006486.1.8",
          "form": "아 맞다 가을 야구 들어는 봤다",
          "original_form": "아 맞다 가을 야구 들어는 봤다",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006486.1.9",
          "form": "그거 나 제도도 몰라 ㅎㅎ",
          "original_form": "그거 나 제도도 몰라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006486.1.10",
          "form": "가을 야구가 잘하는 팀끼리 하는거겠지?",
          "original_form": "가을 야구가 잘하는 팀끼리 하는거겠지?",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006486.1.11",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006486.1.12",
          "form": "나도 정확히는 기억 안난다",
          "original_form": "나도 정확히는 기억 안난다",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006486.1.13",
          "form": "아무래도 한화는 없을 것 같애",
          "original_form": "아무래도 한화는 없을 것 같애",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006486.1.14",
          "form": "그치..아무래도 힘들지",
          "original_form": "그치..아무래도 힘들지",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006486.1.15",
          "form": "지금 꼴찌거든 그리고 오늘 왜 꼴지인지 보여준 경기를 했지 ㅎ",
          "original_form": "지금 꼴찌거든 그리고 오늘 왜 꼴지인지 보여준 경기를 했지 ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006486.1.16",
          "form": "내년을 기대해봐 ㅋㅋㅋㅋㅋ",
          "original_form": "내년을 기대해봐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006486.1.17",
          "form": "그래도 오늘 홈런 2개였어",
          "original_form": "그래도 오늘 홈런 2개였어",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006486.1.18",
          "form": "심지어 한사람은 데뷔 첫 홈런",
          "original_form": "심지어 한사람은 데뷔 첫 홈런",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006486.1.19",
          "form": "그치만 상대편은  만루 홈런을 햇지..ㅎㅎㅋㅋㅋㅋㅋ",
          "original_form": "그치만 상대편은  만루 홈런을 햇지..ㅎㅎㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006486.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ열심히 봤네 그래도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ열심히 봤네 그래도",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006486.1.21",
          "form": "대충봤어 공부해야해성",
          "original_form": "대충봤어 공부해야해성",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100006486.1.22",
          "form": "그치만 내용은 알지 흐름을 봤으니",
          "original_form": "그치만 내용은 알지 흐름을 봤으니",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100006486.1.23",
          "form": "아하 그래그래 내년에 열심히 봐",
          "original_form": "아하 그래그래 내년에 열심히 봐",
