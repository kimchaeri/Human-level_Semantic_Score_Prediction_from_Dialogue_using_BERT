      "utterance": [
        {
          "id": "MDRW2100037071.1.1",
          "form": "카페에서 케이크 먹었는데 맛있더라",
          "original_form": "카페에서 케이크 먹었는데 맛있더라",
          "speaker_id": "2",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100037071.1.2",
          "form": "꿈파 딸기 케이크 같이 생겼어",
          "original_form": "꿈파 딸기 케이크 같이 생겼어",
          "speaker_id": "2",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100037071.1.3",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100037071.1.4",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100037071.1.5",
          "form": "그건 타르트지 않아?",
          "original_form": "그건 타르트지 않아?",
          "speaker_id": "1",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100037071.1.6",
          "form": "꿈파에 딸기케이크도 나왔을껄?",
          "original_form": "꿈파에 딸기케이크도 나왔을껄?",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100037071.1.7",
          "form": "아 뭔지 알겠다",
          "original_form": "아 뭔지 알겠다",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100037071.1.8",
          "form": "할머니가 해주신 거(?)",
          "original_form": "할머니가 해주신 거(?)",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100037071.1.9",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100037071.1.10",
          "form": "그걸꺼야",
          "original_form": "그걸꺼야",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100037071.1.11",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100037071.1.12",
          "form": "나 노티드 도넛인가",
          "original_form": "나 노티드 도넛인가",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100037071.1.13",
          "form": "그거 먹어보고 싶어",
          "original_form": "그거 먹어보고 싶어",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100037071.1.14",
          "form": "들어 봤어?",
          "original_form": "들어 봤어?",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100037071.1.15",
          "form": "어엉",
          "original_form": "어엉",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100037071.1.16",
          "form": "들어봤지",
          "original_form": "들어봤지",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100037071.1.17",
          "form": "유명하잖아",
          "original_form": "유명하잖아",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100037071.1.18",
          "form": "먹어보고 싶어",
          "original_form": "먹어보고 싶어",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100037071.1.19",
          "form": "근데 그 가격만큼 맛있지는 않다고 들음..",
          "original_form": "근데 그 가격만큼 맛있지는 않다고 들음..",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100037071.1.20",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100037071.1.21",
          "form": "허엉 글쿠만",
          "original_form": "허엉 글쿠만",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100037071.1.22",
          "form": "엉얻",
          "original_form": "엉얻",
          "speaker_id": "2",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100037071.1.23",
          "form": "엄청 비싸댜",
          "original_form": "엄청 비싸댜",
          "speaker_id": "2",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100037071.1.24",
          "form": "그럼 별로 안 먹고 싶다...ㅋㅋ",
          "original_form": "그럼 별로 안 먹고 싶다...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100037071.1.25",
          "form": "그냥 다이어트 해서 빵이 먹고 싶은 거 같긴 해",
          "original_form": "그냥 다이어트 해서 빵이 먹고 싶은 거 같긴 해",
          "speaker_id": "1",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100037071.1.26",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100037071.1.27",
          "form": "다이어트하면 빵 엄첟 먹고싶지",
          "original_form": "다이어트하면 빵 엄첟 먹고싶지",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100037071.1.28",
          "form": "얍",
          "original_form": "얍",
