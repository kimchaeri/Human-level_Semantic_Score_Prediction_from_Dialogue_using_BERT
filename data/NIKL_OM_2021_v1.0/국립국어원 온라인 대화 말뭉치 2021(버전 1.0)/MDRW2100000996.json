      "utterance": [
        {
          "id": "MDRW2100000996.1.1",
          "form": "오늘은 구름끼어서 날씨가 흐려요",
          "original_form": "오늘은 구름끼어서 날씨가 흐려요",
          "speaker_id": "1",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000996.1.2",
          "form": "여긴 맑아요",
          "original_form": "여긴 맑아요",
          "speaker_id": "2",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000996.1.3",
          "form": "매미가 엄청 신났네요",
          "original_form": "매미가 엄청 신났네요",
          "speaker_id": "2",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000996.1.4",
          "form": "아 날씨 쨍한가봐요 ㅎㅎㄹ",
          "original_form": "아 날씨 쨍한가봐요 ㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000996.1.5",
          "form": "여긴 꾸리꾸리 비 곧 쏟아질 분우기 ㅎㅎ",
          "original_form": "여긴 꾸리꾸리 비 곧 쏟아질 분우기 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000996.1.6",
          "form": "네 ㅎㅎ 덥네요 오후라",
          "original_form": "네 ㅎㅎ 덥네요 오후라",
          "speaker_id": "2",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000996.1.7",
          "form": "주말에 비온데요",
          "original_form": "주말에 비온데요",
          "speaker_id": "2",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000996.1.8",
          "form": "전 장마 싫은데",
          "original_form": "전 장마 싫은데",
          "speaker_id": "2",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000996.1.9",
          "form": "저도 싫어요  가을장마 는 더싫어요",
          "original_form": "저도 싫어요  가을장마 는 더싫어요",
          "speaker_id": "1",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100000996.1.10",
          "form": "신발 젖으면 찝찝해요",
          "original_form": "신발 젖으면 찝찝해요",
          "speaker_id": "2",
          "time": "20210820 11:38"
        },
        {
          "id": "MDRW2100000996.1.11",
          "form": "저는 엄마 과수원 하셔서 피해때문에",
          "original_form": "저는 엄마 과수원 하셔서 피해때문에",
          "speaker_id": "1",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100000996.1.12",
          "form": "더싫어요 ㅠㅠ",
          "original_form": "더싫어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100000996.1.13",
          "form": "아휴 그렇죠 과수원 하시면",
          "original_form": "아휴 그렇죠 과수원 하시면",
          "speaker_id": "2",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100000996.1.14",
          "form": "비가 피해없이 지나가면 좋겠어요",
          "original_form": "비가 피해없이 지나가면 좋겠어요",
          "speaker_id": "2",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100000996.1.15",
          "form": "가뭄이라 필요하지만 피해가 오면 넘 속상하죠",
          "original_form": "가뭄이라 필요하지만 피해가 오면 넘 속상하죠",
          "speaker_id": "2",
          "time": "20210820 11:39"
        },
        {
          "id": "MDRW2100000996.1.16",
          "form": "날씨따라 정말 맘이 조마조마 하시겠어요",
          "original_form": "날씨따라 정말 맘이 조마조마 하시겠어요",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100000996.1.17",
          "form": "가뭄도 힘들고 장마도 힘들고 ㅎㅎㅎ",
          "original_form": "가뭄도 힘들고 장마도 힘들고 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100000996.1.18",
          "form": "네 다힘들죠",
          "original_form": "네 다힘들죠",
          "speaker_id": "2",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100000996.1.19",
          "form": "날씨에 민감할수 밖에 없죠",
          "original_form": "날씨에 민감할수 밖에 없죠",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100000996.1.20",
          "form": "과일은 해도 잘받고 물도 잘먹어야 할텐데",
          "original_form": "과일은 해도 잘받고 물도 잘먹어야 할텐데",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100000996.1.21",
          "form": "ㅎㅎ 그래서 늘 여름 가을엔 걱정이에요 ㅎㅎ",
          "original_form": "ㅎㅎ 그래서 늘 여름 가을엔 걱정이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100000996.1.22",
          "form": "날씨가 좋아도 어차피 코로나로 맘편히 오딜 못가요",
          "original_form": "날씨가 좋아도 어차피 코로나로 맘편히 오딜 못가요",
          "speaker_id": "2",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100000996.1.23",
          "form": "그렇죠 여름 가을에 기상이 안좋아서",
          "original_form": "그렇죠 여름 가을에 기상이 안좋아서",
          "speaker_id": "2",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100000996.1.24",
          "form": "전 생각도 못했는데 고생이 많으 시네요",
          "original_form": "전 생각도 못했는데 고생이 많으 시네요",
          "speaker_id": "2",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100000996.1.25",
          "form": "미세먼지는 요새 없어서 좋아요",
          "original_form": "미세먼지는 요새 없어서 좋아요",
          "speaker_id": "2",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100000996.1.26",
          "form": "겨울되면 항시 안좋아서",
          "original_form": "겨울되면 항시 안좋아서",
          "speaker_id": "2",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100000996.1.27",
          "form": "미세 먼지도 싫긴해요 ㅎㅎㅎ\n4계절이 뚜렷한 우리나라는 옛말이네요 ㅎㅎ",
          "original_form": "미세 먼지도 싫긴해요 ㅎㅎㅎ\n4계절이 뚜렷한 우리나라는 옛말이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100000996.1.28",
          "form": "네 이젠 애매해요 미세먼지가 젤싫어요",
          "original_form": "네 이젠 애매해요 미세먼지가 젤싫어요",
          "speaker_id": "2",
          "time": "20210820 12:04"
        },
        {
          "id": "MDRW2100000996.1.29",
          "form": "저도 기관지가 안좋아서  티가 확 나더라구오",
          "original_form": "저도 기관지가 안좋아서  티가 확 나더라구오",
