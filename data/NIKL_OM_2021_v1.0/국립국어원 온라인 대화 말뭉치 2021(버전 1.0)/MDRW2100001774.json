      "utterance": [
        {
          "id": "MDRW2100001774.1.1",
          "form": "요즘엔 당근거래 많이들 하시는 거 같아요~",
          "original_form": "요즘엔 당근거래 많이들 하시는 거 같아요~",
          "speaker_id": "1",
          "time": "20210820 10:26"
        },
        {
          "id": "MDRW2100001774.1.2",
          "form": "저도 당근에 팔고싶은거 많은데 \r\n시골살아서 어려워요",
          "original_form": "저도 당근에 팔고싶은거 많은데 \r\n시골살아서 어려워요",
          "speaker_id": "2",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100001774.1.3",
          "form": "해봣어요?",
          "original_form": "해봣어요?",
          "speaker_id": "2",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100001774.1.4",
          "form": "많이는 안 해봤고 두번 정도 팔아봤어요~",
          "original_form": "많이는 안 해봤고 두번 정도 팔아봤어요~",
          "speaker_id": "1",
          "time": "20210820 10:49"
        },
        {
          "id": "MDRW2100001774.1.5",
          "form": "거래는 원만하게 했어요?",
          "original_form": "거래는 원만하게 했어요?",
          "speaker_id": "2",
          "time": "20210820 10:55"
        },
        {
          "id": "MDRW2100001774.1.6",
          "form": "네~ 네고나 진상 없이 깔끔하게 진행됐어요~",
          "original_form": "네~ 네고나 진상 없이 깔끔하게 진행됐어요~",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100001774.1.7",
          "form": "팔고 싶은 물건 많으세요?",
          "original_form": "팔고 싶은 물건 많으세요?",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100001774.1.8",
          "form": "네 애들 자전거부터 시작해서",
          "original_form": "네 애들 자전거부터 시작해서",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100001774.1.9",
          "form": "사놓고 모셔놓은게 많은데",
          "original_form": "사놓고 모셔놓은게 많은데",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100001774.1.10",
          "form": "무료드림하자니 아까워요 ㅠ",
          "original_form": "무료드림하자니 아까워요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100001774.1.11",
          "form": "아..ㅠㅠㅠ 올려보신적은 있으신거에요?",
          "original_form": "아..ㅠㅠㅠ 올려보신적은 있으신거에요?",
          "speaker_id": "1",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100001774.1.12",
          "form": "네 한번요",
          "original_form": "네 한번요",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100001774.1.13",
          "form": "답이 없어요 ㅎ",
          "original_form": "답이 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100001774.1.14",
          "form": "안되면 아름다운가게 갖다주려고요 ㅠ",
          "original_form": "안되면 아름다운가게 갖다주려고요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100001774.1.15",
          "form": "아! 아름다운 가게// 좋은 생각이시네요~~",
          "original_form": "아! 아름다운 가게// 좋은 생각이시네요~~",
          "speaker_id": "1",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100001774.1.16",
          "form": "기부영수증 된다고 하니까 ㅎ",
          "original_form": "기부영수증 된다고 하니까 ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100001774.1.17",
          "form": "또 연말정산 생각해야죠 ㅎ",
          "original_form": "또 연말정산 생각해야죠 ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100001774.1.18",
          "form": "최고네요~ 저도 안 팔리는 건 아름다운가게로 가져가봐야겠어요~",
          "original_form": "최고네요~ 저도 안 팔리는 건 아름다운가게로 가져가봐야겠어요~",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100001774.1.19",
          "form": "나름 깨끗한 물건들인데",
          "original_form": "나름 깨끗한 물건들인데",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100001774.1.20",
          "form": "인기없는것도 있더라고요~",
          "original_form": "인기없는것도 있더라고요~",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100001774.1.21",
          "form": "맞아요 안팔리면 그것도 스트레스",
          "original_form": "맞아요 안팔리면 그것도 스트레스",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100001774.1.22",
          "form": "얼른 처리해야하는데 맘처럼 안되요",
          "original_form": "얼른 처리해야하는데 맘처럼 안되요",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100001774.1.23",
          "form": "작년엔 사놓고 못입은 타미 옷들때문에 좀 그랫어요",
          "original_form": "작년엔 사놓고 못입은 타미 옷들때문에 좀 그랫어요",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100001774.1.24",
          "form": "샀을때 쓴 돈 생각하니 헐값엔 못넘기겠고",
          "original_form": "샀을때 쓴 돈 생각하니 헐값엔 못넘기겠고",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100001774.1.25",
          "form": "중고로는 안팔리고 다들 더 깎아달래니",
          "original_form": "중고로는 안팔리고 다들 더 깎아달래니",
          "speaker_id": "2",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100001774.1.26",
          "form": "그래서 그냥 조카 줘버렸어요",
          "original_form": "그래서 그냥 조카 줘버렸어요",
          "speaker_id": "2",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100001774.1.27",
          "form": "차라리 그게 좋겠네요~\r\n진짜.. 커뮤니티 같은데 보면 깎아달라하는 사람 많은 거 같더라고요~",
          "original_form": "차라리 그게 좋겠네요~\r\n진짜.. 커뮤니티 같은데 보면 깎아달라하는 사람 많은 거 같더라고요~",
          "speaker_id": "1",
