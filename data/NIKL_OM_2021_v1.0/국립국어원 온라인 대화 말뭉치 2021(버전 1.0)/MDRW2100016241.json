      "utterance": [
        {
          "id": "MDRW2100016241.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016241.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016241.1.3",
          "form": "다이어트같은거 관심 있으신가요?",
          "original_form": "다이어트같은거 관심 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016241.1.4",
          "form": "다이어트라기보다 근육량 늘리고싶어요",
          "original_form": "다이어트라기보다 근육량 늘리고싶어요",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016241.1.5",
          "form": "피티받았었는데 근육이 진짜 없더라구요",
          "original_form": "피티받았었는데 근육이 진짜 없더라구요",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016241.1.6",
          "form": "다이어트 하시나요?",
          "original_form": "다이어트 하시나요?",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016241.1.7",
          "form": "아하 근육량 늘리고싶으시군요!",
          "original_form": "아하 근육량 늘리고싶으시군요!",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016241.1.8",
          "form": "저도 다이어트는 안하구 근육량늘리고싶어서 운동 열심히 하고있어요!",
          "original_form": "저도 다이어트는 안하구 근육량늘리고싶어서 운동 열심히 하고있어요!",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016241.1.9",
          "form": "근육운동하면 근육도 늘고 살도 빠지면 좋을텐데 유산소안해서 그런지 그냥 근육만 붙더라구요",
          "original_form": "근육운동하면 근육도 늘고 살도 빠지면 좋을텐데 유산소안해서 그런지 그냥 근육만 붙더라구요",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016241.1.10",
          "form": "유산소랑 무산소운동이 둘다 중요한거같아요!",
          "original_form": "유산소랑 무산소운동이 둘다 중요한거같아요!",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016241.1.11",
          "form": "저는 근육운동을 엄청 열심히 하고있어용ㅎㅎ",
          "original_form": "저는 근육운동을 엄청 열심히 하고있어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016241.1.12",
          "form": "피티는 얼마나 받으셨어요?",
          "original_form": "피티는 얼마나 받으셨어요?",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016241.1.13",
          "form": "한 6개월정도 받았어요",
          "original_form": "한 6개월정도 받았어요",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016241.1.14",
          "form": "그니까요 두 개를 적절히 잘 해야 되는거같아요",
          "original_form": "그니까요 두 개를 적절히 잘 해야 되는거같아요",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016241.1.15",
          "form": "피티 꽤 오래 받으셨네용! 저는 비싸서 피티는 받기가 좀 부담이더라구요ㅠ",
          "original_form": "피티 꽤 오래 받으셨네용! 저는 비싸서 피티는 받기가 좀 부담이더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016241.1.16",
          "form": "그니까요 저도 비싸서 그만뒀어요",
          "original_form": "그니까요 저도 비싸서 그만뒀어요",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100016241.1.17",
          "form": "그래서 아예 운동기구 몇개사서 집에서 하고있어요",
          "original_form": "그래서 아예 운동기구 몇개사서 집에서 하고있어요",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100016241.1.18",
          "form": "집에서 운동하는것도 좋은거같아요! 이동시간도 줄이고 장점이 많죠",
          "original_form": "집에서 운동하는것도 좋은거같아요! 이동시간도 줄이고 장점이 많죠",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100016241.1.19",
          "form": "운동기구는 어떤거 있으세요?",
          "original_form": "운동기구는 어떤거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100016241.1.20",
          "form": "덤벨이랑 바벨이랑 케틀벨 이런거있어요!",
          "original_form": "덤벨이랑 바벨이랑 케틀벨 이런거있어요!",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100016241.1.21",
          "form": "오 바벨도 있으세요? 공간차지도 꽤 할텐데 열정이 있으시네요!!",
          "original_form": "오 바벨도 있으세요? 공간차지도 꽤 할텐데 열정이 있으시네요!!",
          "speaker_id": "1",
          "time": "20210826 18:41"
        }
      ]
    }
