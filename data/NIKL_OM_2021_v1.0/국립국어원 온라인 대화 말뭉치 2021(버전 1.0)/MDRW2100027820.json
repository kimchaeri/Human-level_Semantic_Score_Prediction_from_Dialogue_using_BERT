      "utterance": [
        {
          "id": "MDRW2100027820.1.1",
          "form": "아 ㅋㅋ 맞아 나 들고 싶은 동아리 있어서",
          "original_form": "아 ㅋㅋ 맞아 나 들고 싶은 동아리 있어서",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027820.1.2",
          "form": "3월부터 존버했는데",
          "original_form": "3월부터 존버했는데",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027820.1.3",
          "form": "(1년에 두 번 모집함)",
          "original_form": "(1년에 두 번 모집함)",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100027820.1.4",
          "form": "보통 9월에 2차 모집하길래 기다렸는데",
          "original_form": "보통 9월에 2차 모집하길래 기다렸는데",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027820.1.5",
          "form": "올해는 8월에 모집했더라",
          "original_form": "올해는 8월에 모집했더라",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027820.1.6",
          "form": "허망하다",
          "original_form": "허망하다",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027820.1.7",
          "form": "웃기다 그런 일이",
          "original_form": "웃기다 그런 일이",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027820.1.8",
          "form": "무슨 동아린데",
          "original_form": "무슨 동아린데",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027820.1.9",
          "form": "진짜 말도 안됨",
          "original_form": "진짜 말도 안됨",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027820.1.10",
          "form": "마케팅 동아리 . ..",
          "original_form": "마케팅 동아리 . ..",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027820.1.11",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027820.1.12",
          "form": "근데 동아리까지 하면",
          "original_form": "근데 동아리까지 하면",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027820.1.13",
          "form": "살아남을 수 있는 거냐고",
          "original_form": "살아남을 수 있는 거냐고",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027820.1.14",
          "form": "생각보다 살만혀",
          "original_form": "생각보다 살만혀",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027820.1.15",
          "form": "하... ...",
          "original_form": "하... ...",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027820.1.16",
          "form": "그냥 종강해서 미화된 거 아냐",
          "original_form": "그냥 종강해서 미화된 거 아냐",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027820.1.17",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027820.1.18",
          "form": "...근데 진짜 하고 싶었는데",
          "original_form": "...근데 진짜 하고 싶었는데",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027820.1.19",
          "form": "아니 분명 9월에 모집하셨잖아요.",
          "original_form": "아니 분명 9월에 모집하셨잖아요.",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027820.1.20",
          "form": "안타깝다",
          "original_form": "안타깝다",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027820.1.21",
          "form": "왜 확인 안 해봤어",
          "original_form": "왜 확인 안 해봤어",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027820.1.22",
