      "utterance": [
        {
          "id": "MDRW2100020391.1.1",
          "form": "아뇨 ㅠㅠ 전공은 화공이라 처음엔 화공쪽으로 갔었구",
          "original_form": "아뇨 ㅠㅠ 전공은 화공이라 처음엔 화공쪽으로 갔었구",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020391.1.2",
          "form": "지금은 it이긴한데",
          "original_form": "지금은 it이긴한데",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020391.1.3",
          "form": "it중의 쓰리디같아요..",
          "original_form": "it중의 쓰리디같아요..",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020391.1.4",
          "form": "멋있어요 !",
          "original_form": "멋있어요 !",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020391.1.5",
          "form": "제가 근데 또 그 쪽은 처음이니까 참고 다니고있긴한데",
          "original_form": "제가 근데 또 그 쪽은 처음이니까 참고 다니고있긴한데",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020391.1.6",
          "form": "사람이 기술이있어야 ,",
          "original_form": "사람이 기술이있어야 ,",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020391.1.7",
          "form": "너무 힘들어요ㅠㅠㅠ",
          "original_form": "너무 힘들어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020391.1.8",
          "form": "ㅠㅠ 2년이 진짜 제일 힘들때죠?",
          "original_form": "ㅠㅠ 2년이 진짜 제일 힘들때죠?",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020391.1.9",
          "form": "그니깐여 그리고 최근에 주변에서 한 3명이 주르륵 퇴사했거든요",
          "original_form": "그니깐여 그리고 최근에 주변에서 한 3명이 주르륵 퇴사했거든요",
          "speaker_id": "2",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020391.1.10",
          "form": "마음의 동요가 .. ㄷㄷ",
          "original_form": "마음의 동요가 .. ㄷㄷ",
          "speaker_id": "2",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020391.1.11",
          "form": "ㅋㅋㅋㅋㅋ맞아요 ! 옆에서 퇴사하면 진짜 마음에 파도가 치죠,,",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 ! 옆에서 퇴사하면 진짜 마음에 파도가 치죠,,",
          "speaker_id": "1",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020391.1.12",
          "form": "퇴사자들이 행복하게 떠나는 모습",
          "original_form": "퇴사자들이 행복하게 떠나는 모습",
          "speaker_id": "1",
          "time": "20210828 02:42"
        },
        {
          "id": "MDRW2100020391.1.13",
          "form": "맞아요 ㅠㅜ 근데 뭐 퇴사해도 어떤걸 하고싶다 ! 이런거도 없고 ㅋㅋㅋ정말 ㅋㅋㅋ",
          "original_form": "맞아요 ㅠㅜ 근데 뭐 퇴사해도 어떤걸 하고싶다 ! 이런거도 없고 ㅋㅋㅋ정말 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:43"
        },
        {
          "id": "MDRW2100020391.1.14",
          "form": "하고싶은건 마냥 쉬는것 ,,",
          "original_form": "하고싶은건 마냥 쉬는것 ,,",
          "speaker_id": "1",
          "time": "20210828 02:43"
        },
        {
          "id": "MDRW2100020391.1.15",
          "form": "이렇게 평생을 일하면서 살아야한다니",
          "original_form": "이렇게 평생을 일하면서 살아야한다니",
          "speaker_id": "1",
          "time": "20210828 02:43"
        },
        {
          "id": "MDRW2100020391.1.16",
          "form": "아찔해요",
          "original_form": "아찔해요",
          "speaker_id": "1",
          "time": "20210828 02:43"
        },
        {
          "id": "MDRW2100020391.1.17",
          "form": "그쵸!! 그것도 정말... 이젠 한 70까지는 일해야할텐데",
          "original_form": "그쵸!! 그것도 정말... 이젠 한 70까지는 일해야할텐데",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020391.1.18",
          "form": "와 생각만해도",
          "original_form": "와 생각만해도",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020391.1.19",
          "form": "막막하네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "막막하네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020391.1.20",
          "form": "70까지 일해서 간신히 집살것같아요 ,,",
          "original_form": "70까지 일해서 간신히 집살것같아요 ,,",
          "speaker_id": "1",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020391.1.21",
          "form": "70도 아닌가 80일까요..",
          "original_form": "70도 아닌가 80일까요..",
          "speaker_id": "2",
          "time": "20210828 02:44"
        },
        {
          "id": "MDRW2100020391.1.22",
          "form": "맞아요.. 인구는 줄고있다는데 !! 왜 내집은 없는건지!!",
          "original_form": "맞아요.. 인구는 줄고있다는데 !! 왜 내집은 없는건지!!",
          "speaker_id": "2",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020391.1.23",
          "form": "월급여는 어느정도가 맞다고 생각하세요?!",
          "original_form": "월급여는 어느정도가 맞다고 생각하세요?!",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020391.1.24",
          "form": "물론 많으면 많을수록 좋지만,,",
          "original_form": "물론 많으면 많을수록 좋지만,,",
          "speaker_id": "1",
          "time": "20210828 02:45"
        }
      ]
