      "utterance": [
        {
          "id": "MDRW2100032266.1.1",
          "form": "역류성 식도염....정말 힘들답니다ㅠ",
          "original_form": "역류성 식도염....정말 힘들답니다ㅠ",
          "speaker_id": "1",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032266.1.2",
          "form": "식도염이신가요??",
          "original_form": "식도염이신가요??",
          "speaker_id": "2",
          "time": "20210905 14:57"
        },
        {
          "id": "MDRW2100032266.1.3",
          "form": "네ㅜㅠㅠ",
          "original_form": "네ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100032266.1.4",
          "form": "야식 많이 먹어서인것같아요ㅠ",
          "original_form": "야식 많이 먹어서인것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100032266.1.5",
          "form": "아ㅠㅠㅠㅠ그거 진짜 불편한데ㅜㅠ",
          "original_form": "아ㅠㅠㅠㅠ그거 진짜 불편한데ㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100032266.1.6",
          "form": "네ㅜㅠ",
          "original_form": "네ㅜㅠ",
          "speaker_id": "1",
          "time": "20210905 15:33"
        },
        {
          "id": "MDRW2100032266.1.7",
          "form": "name2님은 신경써야하는",
          "original_form": "&name2&님은 신경써야하는",
          "speaker_id": "1",
          "time": "20210905 15:33"
        },
        {
          "id": "MDRW2100032266.1.8",
          "form": "부분은 없으신가요?",
          "original_form": "부분은 없으신가요?",
          "speaker_id": "1",
          "time": "20210905 15:33"
        },
        {
          "id": "MDRW2100032266.1.9",
          "form": "전 식도염하고 비염...ㅎ이 심해요ㅜ",
          "original_form": "전 식도염하고 비염...ㅎ이 심해요ㅜ",
          "speaker_id": "1",
          "time": "20210905 15:33"
        },
        {
          "id": "MDRW2100032266.1.10",
          "form": "아 저도 비염이요ㅜㅜ",
          "original_form": "아 저도 비염이요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032266.1.11",
          "form": "근데 마스크 많이 써서 그런지 요새는 안심해욬ㅋㅋㅋㅋ",
          "original_form": "근데 마스크 많이 써서 그런지 요새는 안심해욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032266.1.12",
          "form": "아ㅋㅋㅋㅋㅋ저도요",
          "original_form": "아ㅋㅋㅋㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032266.1.13",
          "form": "그죠??",
          "original_form": "그죠??",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032266.1.14",
          "form": "역시 호흡기질환에는 마스크ㅎㅎ",
          "original_form": "역시 호흡기질환에는 마스크ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032266.1.15",
          "form": "잘때도 힘들었는데 신기해요",
          "original_form": "잘때도 힘들었는데 신기해요",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032266.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032266.1.17",
          "form": "근데 답답해서 마스크는",
          "original_form": "근데 답답해서 마스크는",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032266.1.18",
          "form": "그만끼고 싶네요ㅜ",
          "original_form": "그만끼고 싶네요ㅜ",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032266.1.19",
          "form": "피부트러블이 엄청 심해요ㅜㅜㅠ",
          "original_form": "피부트러블이 엄청 심해요ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032266.1.20",
          "form": "그건그래요ㅠㅠ",
          "original_form": "그건그래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032266.1.21",
          "form": "아 저두",
          "original_form": "아 저두",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032266.1.22",
          "form": "턱드름..",
          "original_form": "턱드름..",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032266.1.23",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032266.1.24",
          "form": "ㅠㅜㅜㅜㅜ신경쓰이죠...ㅠㅜ",
          "original_form": "ㅠㅜㅜㅜㅜ신경쓰이죠...ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032266.1.25",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100032266.1.26",
          "form": "마스크 안끼면 좋은데",
          "original_form": "마스크 안끼면 좋은데",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032266.1.27",
