      "utterance": [
        {
          "id": "MDRW2100012868.1.1",
          "form": "안녕하세요 선선한 아침이네요",
          "original_form": "안녕하세요 선선한 아침이네요",
          "speaker_id": "1",
          "time": "20210825 05:14"
        },
        {
          "id": "MDRW2100012868.1.2",
          "form": "지금 아침인가요 새벽 아닌가요",
          "original_form": "지금 아침인가요 새벽 아닌가요",
          "speaker_id": "2",
          "time": "20210825 05:15"
        },
        {
          "id": "MDRW2100012868.1.3",
          "form": "아침인건가...?",
          "original_form": "아침인건가...?",
          "speaker_id": "2",
          "time": "20210825 05:15"
        },
        {
          "id": "MDRW2100012868.1.4",
          "form": "아 그런가요 일어나면 아침이라고 생각했어요",
          "original_form": "아 그런가요 일어나면 아침이라고 생각했어요",
          "speaker_id": "1",
          "time": "20210825 05:15"
        },
        {
          "id": "MDRW2100012868.1.5",
          "form": "맞아요 일어나면 아침이죠 하하",
          "original_form": "맞아요 일어나면 아침이죠 하하",
          "speaker_id": "2",
          "time": "20210825 05:15"
        },
        {
          "id": "MDRW2100012868.1.6",
          "form": "날씨 어때요",
          "original_form": "날씨 어때요",
          "speaker_id": "2",
          "time": "20210825 05:15"
        },
        {
          "id": "MDRW2100012868.1.7",
          "form": "여긴 오늘도 비올거같은데 그쪽날씨는 어떠세요 ?",
          "original_form": "여긴 오늘도 비올거같은데 그쪽날씨는 어떠세요 ?",
          "speaker_id": "1",
          "time": "20210825 05:16"
        },
        {
          "id": "MDRW2100012868.1.8",
          "form": "계속 비가 오고 있어요",
          "original_form": "계속 비가 오고 있어요",
          "speaker_id": "2",
          "time": "20210825 05:16"
        },
        {
          "id": "MDRW2100012868.1.9",
          "form": "아 여긴 지금은 안오는데. 거긴 많이 오나요?",
          "original_form": "아 여긴 지금은 안오는데. 거긴 많이 오나요?",
          "speaker_id": "1",
          "time": "20210825 05:17"
        },
        {
          "id": "MDRW2100012868.1.10",
          "form": "네 새벽내내 쏟아지고 그러네용..\r\n요즘 계속 비가 오는 거 같아요",
          "original_form": "네 새벽내내 쏟아지고 그러네용..\r\n요즘 계속 비가 오는 거 같아요",
          "speaker_id": "2",
          "time": "20210825 05:17"
        },
        {
          "id": "MDRW2100012868.1.11",
          "form": "가을 장마랑 태풍이랑 겹쳐서 그런가 봐요",
          "original_form": "가을 장마랑 태풍이랑 겹쳐서 그런가 봐요",
          "speaker_id": "2",
          "time": "20210825 05:18"
        },
        {
          "id": "MDRW2100012868.1.12",
          "form": "많이 오는 곳은 길도 끊기게 비왔더라구요. 여름 폭우처럼요.",
          "original_form": "많이 오는 곳은 길도 끊기게 비왔더라구요. 여름 폭우처럼요.",
          "speaker_id": "1",
          "time": "20210825 05:18"
        },
        {
          "id": "MDRW2100012868.1.13",
          "form": "날씨가 점점 동남아 처럼 변해가는 것 같아요",
          "original_form": "날씨가 점점 동남아 처럼 변해가는 것 같아요",
          "speaker_id": "2",
          "time": "20210825 05:18"
        },
        {
          "id": "MDRW2100012868.1.14",
          "form": "비 오는 날 어떠세요 좋아하시나요",
          "original_form": "비 오는 날 어떠세요 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 05:19"
        },
        {
          "id": "MDRW2100012868.1.15",
          "form": "더운 여름에 비오는거 실내에서만 좋아요 발 젖는거 싫어서요. 비 좋아하세요?",
          "original_form": "더운 여름에 비오는거 실내에서만 좋아요 발 젖는거 싫어서요. 비 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 05:20"
        },
        {
          "id": "MDRW2100012868.1.16",
          "form": "가을비 좋아하시분도 많죠.",
          "original_form": "가을비 좋아하시분도 많죠.",
          "speaker_id": "1",
          "time": "20210825 05:20"
        },
        {
          "id": "MDRW2100012868.1.17",
          "form": "아니요 저도 비를 별로 좋아하지 않아요",
          "original_form": "아니요 저도 비를 별로 좋아하지 않아요",
          "speaker_id": "2",
          "time": "20210825 05:20"
        },
        {
          "id": "MDRW2100012868.1.18",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "2",
          "time": "20210825 05:20"
        },
        {
          "id": "MDRW2100012868.1.19",
          "form": "약간 습한 걸 안 좋아해서..",
          "original_form": "약간 습한 걸 안 좋아해서..",
          "speaker_id": "2",
          "time": "20210825 05:21"
        },
        {
          "id": "MDRW2100012868.1.20",
          "form": "오 그쵸 고양이 마음 알거같은 느낌!",
          "original_form": "오 그쵸 고양이 마음 알거같은 느낌!",
          "speaker_id": "1",
          "time": "20210825 05:21"
        }
      ]
