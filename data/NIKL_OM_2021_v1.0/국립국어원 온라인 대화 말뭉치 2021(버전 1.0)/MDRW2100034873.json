      "utterance": [
        {
          "id": "MDRW2100034873.1.1",
          "form": "안녕하세용 립이랑 파데 뭐 쓰세용?",
          "original_form": "안녕하세용 립이랑 파데 뭐 쓰세용?",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034873.1.2",
          "form": "립은 그냥 틴트 써용",
          "original_form": "립은 그냥 틴트 써용",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034873.1.3",
          "form": "파데는 제가 피부화장을 안 해서 ㅎㅎ...",
          "original_form": "파데는 제가 피부화장을 안 해서 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034873.1.4",
          "form": "저는 롬앤 틴트 잘쓰는데 틴트 어디꺼 쓰세용?",
          "original_form": "저는 롬앤 틴트 잘쓰는데 틴트 어디꺼 쓰세용?",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034873.1.5",
          "form": "롬앤 쥬시래스팅 써요ㅋㅋ",
          "original_form": "롬앤 쥬시래스팅 써요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034873.1.6",
          "form": "오 롬앤 쓰시는군여",
          "original_form": "오 롬앤 쓰시는군여",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034873.1.7",
          "form": "저는 에뛰드 써요 허헛",
          "original_form": "저는 에뛰드 써요 허헛",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034873.1.8",
          "form": "에뛰드 새로나온거중에 픽싱틴트 그거",
          "original_form": "에뛰드 새로나온거중에 픽싱틴트 그거",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034873.1.9",
          "form": "마스크에 안묻고 좋다고하던데",
          "original_form": "마스크에 안묻고 좋다고하던데",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034873.1.10",
          "form": "!",
          "original_form": "!",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034873.1.11",
          "form": "오 그런가요? 화장에 관심 많으신가봐요",
          "original_form": "오 그런가요? 화장에 관심 많으신가봐요",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100034873.1.12",
          "form": "네네!ㅎㅎ 관심이 좀 많아요",
          "original_form": "네네!ㅎㅎ 관심이 좀 많아요",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034873.1.13",
          "form": "파운데이션 안쓰시는거보니 피부가 좋으신가봐요ㅠㅠ",
          "original_form": "파운데이션 안쓰시는거보니 피부가 좋으신가봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034873.1.14",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034873.1.15",
          "form": "10대 때만 해도 아무 트러블 없어서 안 썼는데",
          "original_form": "10대 때만 해도 아무 트러블 없어서 안 썼는데",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034873.1.16",
          "form": "딱 20살 되면서 갑자기 블랙헤드가 생겼어요ㅠㅠ",
          "original_form": "딱 20살 되면서 갑자기 블랙헤드가 생겼어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034873.1.17",
          "form": "그래서 화장 제대로 시작하고 싶은데 안 해봐서 ㅎㅎ....ㅠㅠ",
          "original_form": "그래서 화장 제대로 시작하고 싶은데 안 해봐서 ㅎㅎ....ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034873.1.18",
          "form": "아하ㅠㅠ 저도 고3 수능보고 딱 스무살때부터",
          "original_form": "아하ㅠㅠ 저도 고3 수능보고 딱 스무살때부터",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034873.1.19",
          "form": "화장시작했어요!",
          "original_form": "화장시작했어요!",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034873.1.20",
          "form": "블랙헤드 저도 고민이네요ㅠ",
          "original_form": "블랙헤드 저도 고민이네요ㅠ",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100034873.1.21",
          "form": "그래서 클렌징오일? 사서 한동안 써봤는데",
          "original_form": "그래서 클렌징오일? 사서 한동안 써봤는데",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100034873.1.22",
          "form": "생전 안 나던 여드름만 나서",
          "original_form": "생전 안 나던 여드름만 나서",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100034873.1.23",
          "form": "다시 안 쓰고 있어요ㅠㅠ",
          "original_form": "다시 안 쓰고 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034873.1.24",
          "form": "아 클렌징오일로 롤링하면 피지나 블랙헤드 제거된다고",
          "original_form": "아 클렌징오일로 롤링하면 피지나 블랙헤드 제거된다고",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034873.1.25",
          "form": "저도 들었는데",
          "original_form": "저도 들었는데",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034873.1.26",
          "form": "오일류가 안맞는 피부면 트러블 난다고 하더라구요ㅠㅠ",
          "original_form": "오일류가 안맞는 피부면 트러블 난다고 하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034873.1.27",
          "form": "네ㅜ 제가 원래 건성 타입인데",
          "original_form": "네ㅜ 제가 원래 건성 타입인데",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034873.1.28",
          "form": "코만 블랙헤드 때문에 클렌징오일 좀 쓴 뒤로",
          "original_form": "코만 블랙헤드 때문에 클렌징오일 좀 쓴 뒤로",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034873.1.29",
          "form": "약간 기름기가 계속 올라오고 있어요",
          "original_form": "약간 기름기가 계속 올라오고 있어요",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034873.1.30",
          "form": "몇달 됐는데도...ㅠㅠ",
          "original_form": "몇달 됐는데도...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034873.1.31",
          "form": "아하ㅠㅠㅠ 그러면 좀 산뜻한걸로 클렌징워터쪽으로 쓰셔야겠네요ㅠㅠ",
          "original_form": "아하ㅠㅠㅠ 그러면 좀 산뜻한걸로 클렌징워터쪽으로 쓰셔야겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034873.1.32",
          "form": "저도 블랙헤드 한번 올라온 뒤로는 안없어져서 슬파요ㅠㅠ",
          "original_form": "저도 블랙헤드 한번 올라온 뒤로는 안없어져서 슬파요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        }
      ]
