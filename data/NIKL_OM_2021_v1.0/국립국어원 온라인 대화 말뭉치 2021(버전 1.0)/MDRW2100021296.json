      "utterance": [
        {
          "id": "MDRW2100021296.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021296.1.2",
          "form": "어디 가보고 싶으세요?",
          "original_form": "어디 가보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021296.1.3",
          "form": "미국이요!",
          "original_form": "미국이요!",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021296.1.4",
          "form": "미국 갈데 많은데 종주하고 싶으신가요?",
          "original_form": "미국 갈데 많은데 종주하고 싶으신가요?",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021296.1.5",
          "form": "일단 뉴욕이 가보고싶네요ㅎㅎ",
          "original_form": "일단 뉴욕이 가보고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021296.1.6",
          "form": "저 썸머썸머나 다른 뉴요커 유튜브 보면서 엄청 친근한ㅋㅋㅋㅋㅋ",
          "original_form": "저 썸머썸머나 다른 뉴요커 유튜브 보면서 엄청 친근한ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021296.1.7",
          "form": "센트럴파크 가고싶네여",
          "original_form": "센트럴파크 가고싶네여",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021296.1.8",
          "form": "오오 가고싶은 스팟이 있는거 좋아요",
          "original_form": "오오 가고싶은 스팟이 있는거 좋아요",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021296.1.9",
          "form": "어디가보고싶으세요",
          "original_form": "어디가보고싶으세요",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021296.1.10",
          "form": "대만 좋다고 들어서요 큰 비용 안쓰고 만족도가 높은...",
          "original_form": "대만 좋다고 들어서요 큰 비용 안쓰고 만족도가 높은...",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021296.1.11",
          "form": "저 가봤는데 대만 너무좋았어여",
          "original_form": "저 가봤는데 대만 너무좋았어여",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021296.1.12",
          "form": "특히 음식 좋다고ㅋㅋㅋㅋ",
          "original_form": "특히 음식 좋다고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021296.1.13",
          "form": "우육면 맛 못잊겠어요ㅠ",
          "original_form": "우육면 맛 못잊겠어요ㅠ",
          "speaker_id": "1",
