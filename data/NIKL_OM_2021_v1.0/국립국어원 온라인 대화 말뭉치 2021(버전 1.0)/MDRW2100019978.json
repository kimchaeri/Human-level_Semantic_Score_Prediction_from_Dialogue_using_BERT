      "utterance": [
        {
          "id": "MDRW2100019978.1.1",
          "form": "쌍둥이 어떻게 생각해",
          "original_form": "쌍둥이 어떻게 생각해",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019978.1.2",
          "form": "쌍둥이들 보면 부럽더라",
          "original_form": "쌍둥이들 보면 부럽더라",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019978.1.3",
          "form": "어떤 점에서",
          "original_form": "어떤 점에서",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019978.1.4",
          "form": "서로 잘 맞는 부분이",
          "original_form": "서로 잘 맞는 부분이",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019978.1.5",
          "form": "나 중학생 때 쌍둥이인 친구가 있어서",
          "original_form": "나 중학생 때 쌍둥이인 친구가 있어서",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019978.1.6",
          "form": "그 둘도 사이가 좋았어",
          "original_form": "그 둘도 사이가 좋았어",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019978.1.7",
          "form": "엄청 좋았지",
          "original_form": "엄청 좋았지",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019978.1.8",
          "form": "서언 서준이 같은 쌍둥이 좋겠다",
          "original_form": "서언 서준이 같은 쌍둥이 좋겠다",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019978.1.9",
          "form": "이휘재네 아이들이지?",
          "original_form": "이휘재네 아이들이지?",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019978.1.10",
          "form": "엄청 귀 귀여워 그 쌍둥이",
          "original_form": "엄청 귀 귀여워 그 쌍둥이",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019978.1.11",
          "form": "내가 아까 말한 그 친구인 쌍둥이는 지금 같이 카페를 해",
          "original_form": "내가 아까 말한 그 친구인 쌍둥이는 지금 같이 카페를 해",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019978.1.12",
          "form": "오 그런 면에서는 쌍둥이가 좋구나",
          "original_form": "오 그런 면에서는 쌍둥이가 좋구나",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019978.1.13",
          "form": "쌍둥이가 좋은 거 같아",
          "original_form": "쌍둥이가 좋은 거 같아",
          "speaker_id": "2",
          "time": "20210827 23:12"
