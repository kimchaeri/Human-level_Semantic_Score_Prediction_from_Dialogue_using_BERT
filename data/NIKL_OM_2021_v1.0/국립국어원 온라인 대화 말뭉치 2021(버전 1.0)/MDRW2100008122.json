      "utterance": [
        {
          "id": "MDRW2100008122.1.1",
          "form": "안녕하세요 반려동물 키우시나요",
          "original_form": "안녕하세요 반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008122.1.2",
          "form": "네 강아지 키워요!",
          "original_form": "네 강아지 키워요!",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100008122.1.3",
          "form": "와 좋겠어요 전 못키워서ㅜ 혹시 이름이 두부인가요ㅋㅋ",
          "original_form": "와 좋겠어요 전 못키워서ㅜ 혹시 이름이 두부인가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008122.1.4",
          "form": "앗 맞아요ㅋㅋㅋ",
          "original_form": "앗 맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008122.1.5",
          "form": "하얀색이라 두부예요",
          "original_form": "하얀색이라 두부예요",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008122.1.6",
          "form": "귀엽겠어요ㅠ 저는 책임질 자신이 없어서 영상만 봐요",
          "original_form": "귀엽겠어요ㅠ 저는 책임질 자신이 없어서 영상만 봐요",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008122.1.7",
          "form": "저도 하하하 영상봐요",
          "original_form": "저도 하하하 영상봐요",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008122.1.8",
          "form": "즐겨보는채널있으세요?",
          "original_form": "즐겨보는채널있으세요?",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008122.1.9",
          "form": "저 모카우유랑 메탈남이요",
          "original_form": "저 모카우유랑 메탈남이요",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008122.1.10",
          "form": "두부님은 뭐보시나요",
          "original_form": "두부님은 뭐보시나요",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008122.1.11",
          "form": "오 저도한번 찾아봐야겠네요",
          "original_form": "오 저도한번 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008122.1.12",
          "form": "저는 알고리즘에뜨는 영상으로봐요ㅎㅎ",
          "original_form": "저는 알고리즘에뜨는 영상으로봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008122.1.13",
          "form": "전 키우지도 않으면서 그런거 열심히 봐서 강아지 용품도 찾아보고 그래요",
          "original_form": "전 키우지도 않으면서 그런거 열심히 봐서 강아지 용품도 찾아보고 그래요",
          "speaker_id": "1",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008122.1.14",
          "form": "개는 훌륭하다도 보고요",
          "original_form": "개는 훌륭하다도 보고요",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008122.1.15",
          "form": "앗 본적있어요ㅋㅋㅋ 모카우유한번찾아볼게요",
          "original_form": "앗 본적있어요ㅋㅋㅋ 모카우유한번찾아볼게요",
          "speaker_id": "2",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008122.1.16",
          "form": "강아지 키울때 제일 힘든점은 뭐가있나요",
          "original_form": "강아지 키울때 제일 힘든점은 뭐가있나요",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008122.1.17",
          "form": "아파서 병원데려갈때가 제일 힘들죠ㅠㅠ",
          "original_form": "아파서 병원데려갈때가 제일 힘들죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008122.1.18",
          "form": "그렇겠네요 사람처럼 어디 아프다 말도 못하고 병원비는 대부분 비급여고요ㅜ",
          "original_form": "그렇겠네요 사람처럼 어디 아프다 말도 못하고 병원비는 대부분 비급여고요ㅜ",
          "speaker_id": "1",
          "time": "20210822 13:33"
        }
      ]
