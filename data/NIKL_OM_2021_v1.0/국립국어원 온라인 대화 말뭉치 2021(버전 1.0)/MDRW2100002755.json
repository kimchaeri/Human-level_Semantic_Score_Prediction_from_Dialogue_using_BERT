      "utterance": [
        {
          "id": "MDRW2100002755.1.1",
          "form": "요즘 무슨 게임해?",
          "original_form": "요즘 무슨 게임해?",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002755.1.2",
          "form": "모바일 게임이라면 마이홈이랑 랜덤 다이스 많이 해",
          "original_form": "모바일 게임이라면 마이홈이랑 랜덤 다이스 많이 해",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002755.1.3",
          "form": "마이홈은 어떤 게임이야?",
          "original_form": "마이홈은 어떤 게임이야?",
          "speaker_id": "1",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002755.1.4",
          "form": "농사하고 가축 기르고",
          "original_form": "농사하고 가축 기르고",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002755.1.5",
          "form": "옷, 가구 만들고",
          "original_form": "옷, 가구 만들고",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002755.1.6",
          "form": "캐릭터 꾸미는 게임?",
          "original_form": "캐릭터 꾸미는 게임?",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002755.1.7",
          "form": "재미있을거 같다",
          "original_form": "재미있을거 같다",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002755.1.8",
          "form": "나는 그런 성장형 게임이 좋아",
          "original_form": "나는 그런 성장형 게임이 좋아",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002755.1.9",
          "form": "내가 컨트롤하는것보다",
          "original_form": "내가 컨트롤하는것보다",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002755.1.10",
          "form": "랜덤다이스는 어떤 게임이야?",
          "original_form": "랜덤다이스는 어떤 게임이야?",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002755.1.11",
          "form": "이거는 운 게임?",
          "original_form": "이거는 운 게임?",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002755.1.12",
          "form": "주사위로 상대랑 싸우는건데",
          "original_form": "주사위로 상대랑 싸우는건데",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002755.1.13",
          "form": "시간 잘가",
          "original_form": "시간 잘가",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002755.1.14",
          "form": "혹시 모두의마블같은 게임이야?",
          "original_form": "혹시 모두의마블같은 게임이야?",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002755.1.15",
          "form": "아니 많이 달라",
          "original_form": "아니 많이 달라",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002755.1.16",
          "form": "주사위로 싸운다는게",
          "original_form": "주사위로 싸운다는게",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002755.1.17",
          "form": "주사위에 능력이 있어서",
          "original_form": "주사위에 능력이 있어서",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002755.1.18",
          "form": "그걸로 몹 무찌르는 거야",
          "original_form": "그걸로 몹 무찌르는 거야",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002755.1.19",
          "form": "그거 재미있겠다",
          "original_form": "그거 재미있겠다",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002755.1.20",
          "form": "나도 게임 할까 싶다가도",
          "original_form": "나도 게임 할까 싶다가도",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002755.1.21",
          "form": "시간 너무 쓸까봐 시작을 못해",
          "original_form": "시간 너무 쓸까봐 시작을 못해",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002755.1.22",
          "form": "맞아 나도 게임에 시간을 너무 써서",
          "original_form": "맞아 나도 게임에 시간을 너무 써서",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002755.1.23",
          "form": "결국 삭제한 게임이 많아",
          "original_form": "결국 삭제한 게임이 많아",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002755.1.24",
          "form": "그렇게 되더라 나도",
          "original_form": "그렇게 되더라 나도",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002755.1.25",
          "form": "게임을 하다보면 재밌는데",
          "original_form": "게임을 하다보면 재밌는데",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002755.1.26",
          "form": "게임을 끄면 우울해짐",
          "original_form": "게임을 끄면 우울해짐",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002755.1.27",
          "form": "슬퍼",
          "original_form": "슬퍼",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002755.1.28",
          "form": "맞아 적당히 하는게 좋은데",
          "original_form": "맞아 적당히 하는게 좋은데",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002755.1.29",
          "form": "그게 쉽지 않지",
          "original_form": "그게 쉽지 않지",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002755.1.30",
          "form": "나중에 돈 많이 벌어서",
          "original_form": "나중에 돈 많이 벌어서",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002755.1.31",
          "form": "게임 즐겁게 하고 살아야지",
          "original_form": "게임 즐겁게 하고 살아야지",
          "speaker_id": "1",
          "time": "20210820 15:38"
        }
      ]
    }
  ]
