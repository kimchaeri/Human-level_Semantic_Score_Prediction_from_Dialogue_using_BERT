      "utterance": [
        {
          "id": "MDRW2100005197.1.1",
          "form": "저는 이번 여름에",
          "original_form": "저는 이번 여름에",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005197.1.2",
          "form": "전주에 여행을 갈 계획이었답니다",
          "original_form": "전주에 여행을 갈 계획이었답니다",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005197.1.3",
          "form": "어머낫...",
          "original_form": "어머낫...",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005197.1.4",
          "form": "저도 전주 가려고 했었는데...",
          "original_form": "저도 전주 가려고 했었는데...",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005197.1.5",
          "form": "코로나 덕분에 ^^",
          "original_form": "코로나 덕분에 ^^",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005197.1.6",
          "form": "ㅋㅋㅋㅋname2님도 코로나 때문에 못가셨구나...^^...",
          "original_form": "ㅋㅋㅋㅋ&name2&님도 코로나 때문에 못가셨구나...^^...",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005197.1.7",
          "form": "전 원래 한옥숙소에서 머무를 계획이었어요",
          "original_form": "전 원래 한옥숙소에서 머무를 계획이었어요",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005197.1.8",
          "form": "전주니까 한복도 대여해서요!",
          "original_form": "전주니까 한복도 대여해서요!",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005197.1.9",
          "form": "하지만 못갔네요^^",
          "original_form": "하지만 못갔네요^^",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005197.1.10",
          "form": "전주는 한복이죠",
          "original_form": "전주는 한복이죠",
          "speaker_id": "2",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005197.1.11",
          "form": "앗 12번 티키타카 하면 대화제출이 된다는데",
          "original_form": "앗 12번 티키타카 하면 대화제출이 된다는데",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005197.1.12",
          "form": "대화제출이 아직 안되내요...",
          "original_form": "대화제출이 아직 안되내요...",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005197.1.13",
          "form": "좀더 노력해봅시다",
          "original_form": "좀더 노력해봅시다",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005197.1.14",
          "form": "노동력 착취가 꽤...",
          "original_form": "노동력 착취가 꽤...",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005197.1.15",
          "form": "해외여행은 어디어디 가보셨나요",
          "original_form": "해외여행은 어디어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005197.1.16",
          "form": "보라카이 여행이 기억에 남습니다",
          "original_form": "보라카이 여행이 기억에 남습니다",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005197.1.17",
          "form": "정말 인상깊으셨나 본데요",
          "original_form": "정말 인상깊으셨나 본데요",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005197.1.18",
          "form": "저는 베트남이랑 홍콩은 여행으롱",
          "original_form": "저는 베트남이랑 홍콩은 여행으롱",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005197.1.19",
          "form": "일본은 뭐랄까 교수님과 학우들과 함께",
          "original_form": "일본은 뭐랄까 교수님과 학우들과 함께",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005197.1.20",
          "form": "오 name2님은 베트남 홍콩만 가보셨나요? 더 가보셨을 것 같은 닉네임이셔요",
          "original_form": "오 &name2&님은 베트남 홍콩만 가보셨나요? 더 가보셨을 것 같은 닉네임이셔요",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005197.1.21",
          "form": "대만?이런데도 가지않으셨나요?",
          "original_form": "대만?이런데도 가지않으셨나요?",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005197.1.22",
          "form": "대만 못 가봤어용",
          "original_form": "대만 못 가봤어용",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005197.1.23",
          "form": "가보고 싶슴다",
          "original_form": "가보고 싶슴다",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005197.1.24",
          "form": "터키도 가보고 싶네요",
          "original_form": "터키도 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.25",
          "form": "아~ 그럼 해외는 베트남 홍콩 일본에 가보신건가요?",
          "original_form": "아~ 그럼 해외는 베트남 홍콩 일본에 가보신건가요?",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.26",
          "form": "터키도 좋죠",
          "original_form": "터키도 좋죠",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.27",
          "form": "목숨이 두개라면 도전",
          "original_form": "목숨이 두개라면 도전",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.28",
          "form": "제 대학 친구가 터키에 다녀왔는데 좋았다더라구요",
          "original_form": "제 대학 친구가 터키에 다녀왔는데 좋았다더라구요",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.29",
          "form": "넹, 아시아만 가봤네요",
          "original_form": "넹, 아시아만 가봤네요",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.30",
          "form": "오오, 터키를요?",
          "original_form": "오오, 터키를요?",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.31",
          "form": "쩐다...",
          "original_form": "쩐다...",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.32",
          "form": "카파도키아 열기구...",
          "original_form": "카파도키아 열기구...",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.33",
          "form": "아닌가요? 대학원을 나온 친구가",
          "original_form": "아닌가요? 대학원을 나온 친구가",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.34",
          "form": "파묵칼레...",
          "original_form": "파묵칼레...",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.35",
          "form": "터키도 가지않았나요?",
          "original_form": "터키도 가지않았나요?",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.36",
          "form": "터키를 갔던거같아요",
          "original_form": "터키를 갔던거같아요",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005197.1.37",
          "form": "음, 그 분은 싱가포르 아니었나요",
          "original_form": "음, 그 분은 싱가포르 아니었나요",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005197.1.38",
          "form": "아 아니네요",
          "original_form": "아 아니네요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005197.1.39",
          "form": "터키는",
          "original_form": "터키는",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005197.1.40",
          "form": "터키는 제 지인 중엔 없었는데",
          "original_form": "터키는 제 지인 중엔 없었는데",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005197.1.41",
          "form": "제 대학친구는요",
          "original_form": "제 대학친구는요",
