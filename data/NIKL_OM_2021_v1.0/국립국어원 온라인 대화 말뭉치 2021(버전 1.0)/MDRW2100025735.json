      "utterance": [
        {
          "id": "MDRW2100025735.1.1",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025735.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025735.1.3",
          "form": "게임좋아하세요?",
          "original_form": "게임좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025735.1.4",
          "form": "네 저도 좋아해요!",
          "original_form": "네 저도 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025735.1.5",
          "form": "최근에 무슨 게임을 하셨나요?",
          "original_form": "최근에 무슨 게임을 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025735.1.6",
          "form": "저는 최근에 스위치로 동숲이랑 스팀으로 레데리를 했어요",
          "original_form": "저는 최근에 스위치로 동숲이랑 스팀으로 레데리를 했어요",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025735.1.7",
          "form": "최근에 어떤거하셨어요?",
          "original_form": "최근에 어떤거하셨어요?",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025735.1.8",
          "form": "저도 최근에 스위치로 링피트와 동숲을 했어요!",
          "original_form": "저도 최근에 스위치로 링피트와 동숲을 했어요!",
          "speaker_id": "1",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025735.1.9",
          "form": "열심히 빚을 갚고 있어요.. ㅎㅎ",
          "original_form": "열심히 빚을 갚고 있어요.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025735.1.10",
          "form": "저도요ㅋㅋ ㅋㅋ 어느빚 갚고계신가요?",
          "original_form": "저도요ㅋㅋ ㅋㅋ 어느빚 갚고계신가요?",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025735.1.11",
          "form": "저는 지하실빚 갚는중이예요",
          "original_form": "저는 지하실빚 갚는중이예요",
          "speaker_id": "2",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025735.1.12",
          "form": "집 확장을 해서 갚고 있는데",
          "original_form": "집 확장을 해서 갚고 있는데",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025735.1.13",
          "form": "지금은 2번째 방?을 갚고 있어요",
          "original_form": "지금은 2번째 방?을 갚고 있어요",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025735.1.14",
          "form": "와 지하실 벌써 만드셨군요 부럽네요..!",
          "original_form": "와 지하실 벌써 만드셨군요 부럽네요..!",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025735.1.15",
          "form": "시작한지 그래도 두달은 되어가지구요 !",
          "original_form": "시작한지 그래도 두달은 되어가지구요 !",
          "speaker_id": "2",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100025735.1.16",
          "form": "오늘 9월이라고 잡히는 생물들도 바뀌었는데 보셨나요?",
          "original_form": "오늘 9월이라고 잡히는 생물들도 바뀌었는데 보셨나요?",
          "speaker_id": "2",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100025735.1.17",
          "form": "와 최근에 못들어갔는데 알려주셔서 감사해요 들어가봐야겠어요!",
          "original_form": "와 최근에 못들어갔는데 알려주셔서 감사해요 들어가봐야겠어요!",
          "speaker_id": "1",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100025735.1.18",
          "form": "어떤 생물로 바뀌었는지 아시나요?",
          "original_form": "어떤 생물로 바뀌었는지 아시나요?",
          "speaker_id": "1",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100025735.1.19",
          "form": "조금씩 바뀌었는데 물고기중에 연어가 잡히기 시작했구요 고추잠자리가 자주보여요",
          "original_form": "조금씩 바뀌었는데 물고기중에 연어가 잡히기 시작했구요 고추잠자리가 자주보여요",
          "speaker_id": "2",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100025735.1.20",
          "form": "나비종류도 조금 다르구요",
          "original_form": "나비종류도 조금 다르구요",
          "speaker_id": "2",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100025735.1.21",
          "form": "연어 잡아본적은 없는데 한 번 잡아봐야겠어요",
          "original_form": "연어 잡아본적은 없는데 한 번 잡아봐야겠어요",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100025735.1.22",
          "form": "다시 들어가서 빚도 갚고 박물관도 채워봐야겠어요 ㅎㅎ",
          "original_form": "다시 들어가서 빚도 갚고 박물관도 채워봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100025735.1.23",
          "form": "박물관채우는 재미가 있죠!",
          "original_form": "박물관채우는 재미가 있죠!",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025735.1.24",
          "form": "주민들 데려오는 재미도 있구요",
          "original_form": "주민들 데려오는 재미도 있구요",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025735.1.25",
          "form": "주민 몇명하구 살고 계세요?",
          "original_form": "주민 몇명하구 살고 계세요?",
          "speaker_id": "2",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100025735.1.26",
          "form": "한 명만 더 데려오면 다 차요!",
          "original_form": "한 명만 더 데려오면 다 차요!",
          "speaker_id": "1",
          "time": "20210901 16:43"
        }
      ]
