      "utterance": [
        {
          "id": "MDRW2100040632.1.1",
          "form": "name1님덕분 ㅎㅎ",
          "original_form": "&name1&님덕분 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040632.1.2",
          "form": "히히히히",
          "original_form": "히히히히",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040632.1.3",
          "form": "네네 저는",
          "original_form": "네네 저는",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040632.1.4",
          "form": "주로 친구들 취향을 따르는데",
          "original_form": "주로 친구들 취향을 따르는데",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040632.1.5",
          "form": "술 좋아하는 친구들 만나면",
          "original_form": "술 좋아하는 친구들 만나면",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040632.1.6",
          "form": "술 마시러 가고",
          "original_form": "술 마시러 가고",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040632.1.7",
          "form": "아니면 아닌대로 다니고~",
          "original_form": "아니면 아닌대로 다니고~",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040632.1.8",
          "form": "배려심 넘치시군여!",
          "original_form": "배려심 넘치시군여!",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040632.1.9",
          "form": "술을 싫어하는정도는 아니신거죠?",
          "original_form": "술을 싫어하는정도는 아니신거죠?",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040632.1.10",
          "form": "그럼요 그럼요!",
          "original_form": "그럼요 그럼요!",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040632.1.11",
          "form": "저도 맛있는 술은 좋아해요",
          "original_form": "저도 맛있는 술은 좋아해요",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040632.1.12",
          "form": "그 취하는 기분이 좋아서 ㅎㅎ",
          "original_form": "그 취하는 기분이 좋아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.13",
          "form": "오 그기분 느끼시는구나 ㅋㅋㅋㅋ",
          "original_form": "오 그기분 느끼시는구나 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.14",
          "form": "근데 소주는 이제 못마시깄어요ㅠ",
          "original_form": "근데 소주는 이제 못마시깄어요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.15",
          "form": "술 잘안먹늠 제 친구는",
          "original_form": "술 잘안먹늠 제 친구는",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.16",
          "form": "네네 막",
          "original_form": "네네 막",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.17",
          "form": "그 기분을 못느끼더라구요",
          "original_form": "그 기분을 못느끼더라구요",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.18",
          "form": "기분이 너무 좋아져요",
          "original_form": "기분이 너무 좋아져요",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.19",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.20",
          "form": "그리고 술자리 분위기...너무 좋아요....",
          "original_form": "그리고 술자리 분위기...너무 좋아요....",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.21",
          "form": "네네 술마시며누기분좋아지는 그 느낌을",
          "original_form": "네네 술마시며누기분좋아지는 그 느낌을",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.22",
          "form": "몰라요..",
          "original_form": "몰라요..",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040632.1.23",
          "form": "저도 소주는",
          "original_form": "저도 소주는",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.24",
          "form": "으으 싫어합니다",
          "original_form": "으으 싫어합니다",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.25",
          "form": "그 공업용알콜을 왜..먹지",
          "original_form": "그 공업용알콜을 왜..먹지",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.26",
          "form": "그료!!!!",
          "original_form": "그료!!!!",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.27",
          "form": "그쵸!!!!!",
          "original_form": "그쵸!!!!!",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.28",
          "form": "저는 그냥 알콜분해효소가 적을뿐",
          "original_form": "저는 그냥 알콜분해효소가 적을뿐",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.29",
          "form": "술은 좋아해요 ㅎㅎㅎ",
          "original_form": "술은 좋아해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.30",
          "form": "ㅎㅎ다행입니다",
          "original_form": "ㅎㅎ다행입니다",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.31",
          "form": "그래서 더 대학교 신입생 때",
          "original_form": "그래서 더 대학교 신입생 때",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040632.1.32",
          "form": "전에 다니던 대학이 충무로에 있었는데",
          "original_form": "전에 다니던 대학이 충무로에 있었는데",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.33",
          "form": "이대는 편입한거거든요!",
          "original_form": "이대는 편입한거거든요!",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.34",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.35",
          "form": "그래서 그 때 남자애들이랑 술마실 땨",
          "original_form": "그래서 그 때 남자애들이랑 술마실 땨",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.36",
          "form": "그 땐 가성비",
          "original_form": "그 땐 가성비",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.37",
          "form": "소주만 마시니까",
          "original_form": "소주만 마시니까",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.38",
          "form": "술이 싫었어요ㅠ",
          "original_form": "술이 싫었어요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.39",
          "form": "마장요 오히려 대딩때는",
          "original_form": "마장요 오히려 대딩때는",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.40",
          "form": "싼 술만 마시니까 소주 엄청먹엇는데",
          "original_form": "싼 술만 마시니까 소주 엄청먹엇는데",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.41",
          "form": "우욱 지금은 굳이?",
          "original_form": "우욱 지금은 굳이?",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040632.1.42",
          "form": "글고 위스킼ㅋㅋㅋ저는 그맛 몰랏는데",
          "original_form": "글고 위스킼ㅋㅋㅋ저는 그맛 몰랏는데",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.43",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.44",
          "form": "오 위스키!!!",
          "original_form": "오 위스키!!!",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.45",
          "form": "싱가폴가서 만난 친구가 위스키만 먹는거예요 글고 맨날 위스키만 사주고",
          "original_form": "싱가폴가서 만난 친구가 위스키만 먹는거예요 글고 맨날 위스키만 사주고",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.46",
          "form": "그때 입덕..",
          "original_form": "그때 입덕..",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.47",
          "form": "저는 안마셔봤어요 비싼 술 아닌가요?!",
          "original_form": "저는 안마셔봤어요 비싼 술 아닌가요?!",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.48",
          "form": "먹다보니 세상 향긋..",
          "original_form": "먹다보니 세상 향긋..",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.49",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.50",
          "form": "향긋해요?!",
          "original_form": "향긋해요?!",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.51",
          "form": "저렴한것도 잇어요 조니워커같은!",
          "original_form": "저렴한것도 잇어요 조니워커같은!",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.52",
          "form": "저 또 향 좋은거 못참는데",
          "original_form": "저 또 향 좋은거 못참는데",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040632.1.53",
          "form": "네 그 오크향이 있거든요?",
          "original_form": "네 그 오크향이 있거든요?",
          "speaker_id": "2",
          "time": "20210904 23:30"
        }
      ]
    }
