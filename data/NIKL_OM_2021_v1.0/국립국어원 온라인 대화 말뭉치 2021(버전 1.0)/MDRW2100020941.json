      "utterance": [
        {
          "id": "MDRW2100020941.1.1",
          "form": "저녁 뭐 드실건가요",
          "original_form": "저녁 뭐 드실건가요",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020941.1.2",
          "form": "배불러서 생각도 안나요",
          "original_form": "배불러서 생각도 안나요",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020941.1.3",
          "form": "ㅜㅜ 어제 먹다만 삼겹살도 생각나고",
          "original_form": "ㅜㅜ 어제 먹다만 삼겹살도 생각나고",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020941.1.4",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020941.1.5",
          "form": "전 보글보글 된장찌개",
          "original_form": "전 보글보글 된장찌개",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020941.1.6",
          "form": "하하하하하",
          "original_form": "하하하하하",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020941.1.7",
          "form": "헉 맛있겠다",
          "original_form": "헉 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020941.1.8",
          "form": "된찌와 김치찌개 고르면?",
          "original_form": "된찌와 김치찌개 고르면?",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020941.1.9",
          "form": "김치찌개죠.",
          "original_form": "김치찌개죠.",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020941.1.10",
          "form": "찌개에 두부 넣으시나요?",
          "original_form": "찌개에 두부 넣으시나요?",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020941.1.11",
          "form": "네네 두부는 필수예요",
          "original_form": "네네 두부는 필수예요",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020941.1.12",
          "form": "전 좀 텁텁해서 직전에 넣어서 바로 다 먹는걸 선호해요",
          "original_form": "전 좀 텁텁해서 직전에 넣어서 바로 다 먹는걸 선호해요",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020941.1.13",
          "form": "고기 넣는건 어떠세요?",
          "original_form": "고기 넣는건 어떠세요?",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020941.1.14",
          "form": "아...그럼 맑은 찌개를 선호하는군요",
          "original_form": "아...그럼 맑은 찌개를 선호하는군요",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020941.1.15",
          "form": "고기는 사랑이죠",
          "original_form": "고기는 사랑이죠",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020941.1.16",
          "form": "네. 좀 맑은 느낌 좋아해요",
          "original_form": "네. 좀 맑은 느낌 좋아해요",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020941.1.17",
          "form": "참치?  고기? 뭘 넣으세요",
          "original_form": "참치?  고기? 뭘 넣으세요",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020941.1.18",
          "form": "전 고기 참치 둘 다 별로 안 좋아해요ㅜㅜ 굳이 넣으면 고기네요. 깊은 맛 때문에",
          "original_form": "전 고기 참치 둘 다 별로 안 좋아해요ㅜㅜ 굳이 넣으면 고기네요. 깊은 맛 때문에",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020941.1.19",
          "form": "아니 어떻게 그럴수가ㄷㄷ",
          "original_form": "아니 어떻게 그럴수가ㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020941.1.20",
          "form": "그럼 김치만 넣나요ㅜ",
          "original_form": "그럼 김치만 넣나요ㅜ",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020941.1.21",
          "form": "파? ㅎㅎㅎㅎ",
          "original_form": "파? ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020941.1.22",
          "form": "고기를 넣는다면 조금만 넣어요",
          "original_form": "고기를 넣는다면 조금만 넣어요",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020941.1.23",
          "form": "아ㅎㅎㅎ 피도 맑아지겠네요",
          "original_form": "아ㅎㅎㅎ 피도 맑아지겠네요",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020941.1.24",
          "form": "저와는 겸상하지 맙시다ㅎㅎ",
          "original_form": "저와는 겸상하지 맙시다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:26"
        }
      ]
