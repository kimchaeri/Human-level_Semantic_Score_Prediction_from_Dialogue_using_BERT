      "utterance": [
        {
          "id": "MDRW2100015473.1.1",
          "form": "ㅜ아프가니스탄 사람들 그만왔음 좋겠다..",
          "original_form": "ㅜ아프가니스탄 사람들 그만왔음 좋겠다..",
          "speaker_id": "1",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100015473.1.2",
          "form": "왜 그사람들도 살아야지",
          "original_form": "왜 그사람들도 살아야지",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100015473.1.3",
          "form": "그 사람들 종교적특성인지 민족특성인지 모르겠는데",
          "original_form": "그 사람들 종교적특성인지 민족특성인지 모르겠는데",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100015473.1.4",
          "form": "사는 장소만 바뀐거지 자기네들 살던대로 그대로살텐데",
          "original_form": "사는 장소만 바뀐거지 자기네들 살던대로 그대로살텐데",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100015473.1.5",
          "form": "한국 와서 적응하고 살면 괜찮겠지",
          "original_form": "한국 와서 적응하고 살면 괜찮겠지",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100015473.1.6",
          "form": "다른사회에 가도 자기들 고집대로만 사는 것 같던데 무서움",
          "original_form": "다른사회에 가도 자기들 고집대로만 사는 것 같던데 무서움",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100015473.1.7",
          "form": "이슬람인가?",
          "original_form": "이슬람인가?",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100015473.1.8",
          "form": "그렇지 않나?",
          "original_form": "그렇지 않나?",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100015473.1.9",
          "form": "잘 모르겠다 근데 일단 엄청 무서운 이단느낌이야ㅠㅜ",
          "original_form": "잘 모르겠다 근데 일단 엄청 무서운 이단느낌이야ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100015473.1.10",
          "form": "그사람들은 삶이 그런거지",
          "original_form": "그사람들은 삶이 그런거지",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100015473.1.11",
          "form": "세뇌 수준인것 같음..",
          "original_form": "세뇌 수준인것 같음..",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100015473.1.12",
          "form": "잘 살겠지 머",
          "original_form": "잘 살겠지 머",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100015473.1.13",
          "form": "우리국민들 입장에선 넘 부담인데",
          "original_form": "우리국민들 입장에선 넘 부담인데",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100015473.1.14",
          "form": "그만데러옴 좋겠다 제발ㅜ",
          "original_form": "그만데러옴 좋겠다 제발ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100015473.1.15",
          "form": "하긴 외국은 난민때매 골치라곤 하더라",
          "original_form": "하긴 외국은 난민때매 골치라곤 하더라",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100015473.1.16",
          "form": "맞아 난민 받아서 잘 된 사례가 별로 없잖아",
          "original_form": "맞아 난민 받아서 잘 된 사례가 별로 없잖아",
          "speaker_id": "1",
          "time": "20210828 22:42"
        }
