      "utterance": [
        {
          "id": "MDRW2100033616.1.1",
          "form": "저는 이제 딸데리러가요",
          "original_form": "저는 이제 딸데리러가요",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100033616.1.2",
          "form": "네 조심히 다녀오세요",
          "original_form": "네 조심히 다녀오세요",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100033616.1.3",
          "form": "집에가서 개봉해보고 후기알려드릴께요 ㅎㅎ",
          "original_form": "집에가서 개봉해보고 후기알려드릴께요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100033616.1.4",
          "form": "네 아이폰 워치도 좋아보이던데",
          "original_form": "네 아이폰 워치도 좋아보이던데",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100033616.1.5",
          "form": "후기들려주세요",
          "original_form": "후기들려주세요",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100033616.1.6",
          "form": "워치4와서 개봉했어요",
          "original_form": "워치4와서 개봉했어요",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100033616.1.7",
          "form": "근데 블랙으로 했더니 이쁜지 모르겠네요 ㅎ",
          "original_form": "근데 블랙으로 했더니 이쁜지 모르겠네요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100033616.1.8",
          "form": "색상이 몇가지예요?",
          "original_form": "색상이 몇가지예요?",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100033616.1.9",
          "form": "연보라 있음 이쁠거같은데",
          "original_form": "연보라 있음 이쁠거같은데",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100033616.1.10",
          "form": "전 저녁먹으러왔어요ㅋ",
          "original_form": "전 저녁먹으러왔어요ㅋ",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100033616.1.11",
          "form": "핑크 블랙 카키였나?",
          "original_form": "핑크 블랙 카키였나?",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100033616.1.12",
          "form": "핑쿠 이쁘긴한데 때탈꺼같아",
          "original_form": "핑쿠 이쁘긴한데 때탈꺼같아",
          "speaker_id": "2",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100033616.1.13",
          "form": "그냥 블랙했더니",
          "original_form": "그냥 블랙했더니",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100033616.1.14",
          "form": "톤다운된거면 괜찮을거같은데",
          "original_form": "톤다운된거면 괜찮을거같은데",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100033616.1.15",
          "form": "회사동생 애플워치 이쁘던데",
          "original_form": "회사동생 애플워치 이쁘던데",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100033616.1.16",
          "form": "사이즈 40미리 44미리 고민했는데",
          "original_form": "사이즈 40미리 44미리 고민했는데",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100033616.1.17",
          "form": "근데 줄은 추가로사서 바꿀수있지않아요?",
          "original_form": "근데 줄은 추가로사서 바꿀수있지않아요?",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100033616.1.18",
          "form": "고민하다 카드할인 선착 끝나서 2만원 추가로\r\n더 냈네요 ㅎ",
          "original_form": "고민하다 카드할인 선착 끝나서 2만원 추가로\r\n더 냈네요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100033616.1.19",
          "form": "네 줄 바꿀수있어요",
          "original_form": "네 줄 바꿀수있어요",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100033616.1.20",
          "form": "44미리 했으면 큰일날뻔했어요",
          "original_form": "44미리 했으면 큰일날뻔했어요",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100033616.1.21",
          "form": "40미리도 손목에 꽉차더라구요",
          "original_form": "40미리도 손목에 꽉차더라구요",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100033616.1.22",
          "form": "여성이 쓰긴 작은게나을듯요",
          "original_form": "여성이 쓰긴 작은게나을듯요",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100033616.1.23",
          "form": "걷기 그걸로하심 되겠어요ㅋ",
          "original_form": "걷기 그걸로하심 되겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100033616.1.24",
          "form": "팔 마구 흔들면 되려나",
          "original_form": "팔 마구 흔들면 되려나",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100033616.1.25",
          "form": "그건 안될듯요",
          "original_form": "그건 안될듯요",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100033616.1.26",
          "form": "걷기인식되는거같던데 안되나요?",
          "original_form": "걷기인식되는거같던데 안되나요?",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100033616.1.27",
          "form": "삼성헬스랑 연동되면 될텐데",
          "original_form": "삼성헬스랑 연동되면 될텐데",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100033616.1.28",
          "form": "토스만보기는 폰을 흔들어야할껄요?",
          "original_form": "토스만보기는 폰을 흔들어야할껄요?",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100033616.1.29",
          "form": "아 토스는그럴듯한데",
          "original_form": "아 토스는그럴듯한데",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100033616.1.30",
          "form": "다른데써먹질못하겠네요",
          "original_form": "다른데써먹질못하겠네요",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100033616.1.31",
          "form": "네 아마도",
          "original_form": "네 아마도",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033616.1.32",
          "form": "그냥 비싼만보기 ㅋ",
          "original_form": "그냥 비싼만보기 ㅋ",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033616.1.33",
          "form": "근데 체지방 측정되는건 신기해요",
          "original_form": "근데 체지방 측정되는건 신기해요",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033616.1.34",
          "form": "ㅋㅋ 문자도보낼수 있어요?",
          "original_form": "ㅋㅋ 문자도보낼수 있어요?",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033616.1.35",
          "form": "오 그런것도돼요?",
          "original_form": "오 그런것도돼요?",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033616.1.36",
          "form": "네 이번신상 새로운기능 이래요",
          "original_form": "네 이번신상 새로운기능 이래요",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033616.1.37",
          "form": "아직 문자는 안보내봤어요 ㅋ",
          "original_form": "아직 문자는 안보내봤어요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033616.1.38",
          "form": "참좋은세상이네요ㅎ",
          "original_form": "참좋은세상이네요ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033616.1.39",
          "form": "당뇨측정도 된다던데",
          "original_form": "당뇨측정도 된다던데",
          "speaker_id": "1",
          "time": "20210825 19:52"
        }
      ]
    }
