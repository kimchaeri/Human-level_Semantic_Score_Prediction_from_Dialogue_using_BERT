      "utterance": [
        {
          "id": "MDRW2100006561.1.1",
          "form": "네 변이가 나왔더라고요",
          "original_form": "네 변이가 나왔더라고요",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100006561.1.2",
          "form": "백신은 맞으셨나요",
          "original_form": "백신은 맞으셨나요",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006561.1.3",
          "form": "네 일차는 맞았어요 맞으셨나요?",
          "original_form": "네 일차는 맞았어요 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006561.1.4",
          "form": "네 저는 얀센이요",
          "original_form": "네 저는 얀센이요",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100006561.1.5",
          "form": "아 얀센 맞으셨군요!",
          "original_form": "아 얀센 맞으셨군요!",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006561.1.6",
          "form": "네.1회로 끝나니.좋긴해요",
          "original_form": "네.1회로 끝나니.좋긴해요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006561.1.7",
          "form": "또 안맞아도 되니까 편하시겠어요",
          "original_form": "또 안맞아도 되니까 편하시겠어요",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100006561.1.8",
          "form": "네.부담이 덜하긴하네여",
          "original_form": "네.부담이 덜하긴하네여",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100006561.1.9",
          "form": "저도 귀찮아서 빨리 맞고 싶은데 추석 이후에 맞네요",
          "original_form": "저도 귀찮아서 빨리 맞고 싶은데 추석 이후에 맞네요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100006561.1.10",
          "form": "화이자인가요?",
          "original_form": "화이자인가요?",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100006561.1.11",
          "form": "네 화이자 맞았어요",
          "original_form": "네 화이자 맞았어요",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100006561.1.12",
          "form": "좀 빨리 맞아야 효과가 있을거 같기도 한데",
          "original_form": "좀 빨리 맞아야 효과가 있을거 같기도 한데",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100006561.1.13",
          "form": "이주 미뤄졌어요",
          "original_form": "이주 미뤄졌어요",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100006561.1.14",
          "form": "아 백신 수급 문제때문에 그럴까요",
          "original_form": "아 백신 수급 문제때문에 그럴까요",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100006561.1.15",
          "form": "네 그랬던거 같아요",
          "original_form": "네 그랬던거 같아요",
          "speaker_id": "1",
          "time": "20210908 02:26"
        }
