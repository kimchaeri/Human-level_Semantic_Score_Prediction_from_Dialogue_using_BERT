      "utterance": [
        {
          "id": "MDRW2100006499.1.1",
          "form": "너 요즘 카페인 안먹는다고? 왜?",
          "original_form": "너 요즘 카페인 안먹는다고? 왜?",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100006499.1.2",
          "form": "건강이 좀 안 좋아진게 느껴져서",
          "original_form": "건강이 좀 안 좋아진게 느껴져서",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100006499.1.3",
          "form": "끊으려고 노력 중이야",
          "original_form": "끊으려고 노력 중이야",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100006499.1.4",
          "form": "건강? 그냥?? 뭐 건강검진에서 뭐 안좋게 나왔어?",
          "original_form": "건강? 그냥?? 뭐 건강검진에서 뭐 안좋게 나왔어?",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100006499.1.5",
          "form": "음 혈압도 높아진 편이고",
          "original_form": "음 혈압도 높아진 편이고",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100006499.1.6",
          "form": "커피 마시면 가슴 쪽에 통증도 가끔 오고 해서",
          "original_form": "커피 마시면 가슴 쪽에 통증도 가끔 오고 해서",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100006499.1.7",
          "form": "끊으려고 노력 중이야",
          "original_form": "끊으려고 노력 중이야",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100006499.1.8",
          "form": "가슴에 통증이 온다구? 원래 그랬어?",
          "original_form": "가슴에 통증이 온다구? 원래 그랬어?",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100006499.1.9",
          "form": "아니 안 그랬지",
          "original_form": "아니 안 그랬지",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100006499.1.10",
          "form": "알잖아 내가 카페인 얼마나 먹어왔었는지 ㅋㅋㅋ",
          "original_form": "알잖아 내가 카페인 얼마나 먹어왔었는지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100006499.1.11",
          "form": "너 카페인 알약도 있다고 그랬어서 살짝 걱정했었어",
          "original_form": "너 카페인 알약도 있다고 그랬어서 살짝 걱정했었어",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006499.1.12",
          "form": "어 내가 말했었나?",
          "original_form": "어 내가 말했었나?",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006499.1.13",
          "form": "근데 뭐 커피 먹는거랑 비슷해",
          "original_form": "근데 뭐 커피 먹는거랑 비슷해",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006499.1.14",
          "form": "응응 말했어",
          "original_form": "응응 말했어",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100006499.1.15",
          "form": "나도 캔커피 같은 것 먹다가 어느순간 확 그래서 조심하라고",
          "original_form": "나도 캔커피 같은 것 먹다가 어느순간 확 그래서 조심하라고",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006499.1.16",
          "form": "스벅보다 카페인 낮을걸?",
          "original_form": "스벅보다 카페인 낮을걸?",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006499.1.17",
          "form": "알약이?? 스벅이 투샷이라서",
          "original_form": "알약이?? 스벅이 투샷이라서",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006499.1.18",
          "form": "캔커피도 그렇지 뭐",
          "original_form": "캔커피도 그렇지 뭐",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006499.1.19",
          "form": "맞아 그래서 나 스벅도 안 가거나 가면 음료마셔",
          "original_form": "맞아 그래서 나 스벅도 안 가거나 가면 음료마셔",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006499.1.20",
          "form": "그래 근데 나도 건강 생각해셔 평소에는 안마셔 카페인",
          "original_form": "그래 근데 나도 건강 생각해셔 평소에는 안마셔 카페인",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006499.1.21",
          "form": "밤샐때 먹는건데ㅠ 앞으로 줄여야겠다 더 ㅠ",
          "original_form": "밤샐때 먹는건데ㅠ 앞으로 줄여야겠다 더 ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100006499.1.22",
          "form": "좋아좋아 말하고싶었어 그냥 조심해서 조절하라고",
