      "utterance": [
        {
          "id": "MDRW2100039523.1.1",
          "form": "오늘 날은 쨩쨍해서 싫었는데",
          "original_form": "오늘 날은 쨩쨍해서 싫었는데",
          "speaker_id": "1",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100039523.1.2",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "1",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100039523.1.3",
          "form": "저도 햇빛이 너무 쎄서 별로였어요",
          "original_form": "저도 햇빛이 너무 쎄서 별로였어요",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039523.1.4",
          "form": "어떤 날을 좋아하시나요?",
          "original_form": "어떤 날을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039523.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039523.1.6",
          "form": "저도 선선한날이요 밤이요",
          "original_form": "저도 선선한날이요 밤이요",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039523.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039523.1.8",
          "form": "저도 선선한 날이요",
          "original_form": "저도 선선한 날이요",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039523.1.9",
          "form": "약간 그늘지고..",
          "original_form": "약간 그늘지고..",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039523.1.10",
          "form": "근데 해 있는 날은 쨍쨍한데 구름은 이뻐서 좋더라구요",
          "original_form": "근데 해 있는 날은 쨍쨍한데 구름은 이뻐서 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039523.1.11",
          "form": "그건 마자요ㅎㅎ",
          "original_form": "그건 마자요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039523.1.12",
          "form": "구름만 보면 좋더라구요",
          "original_form": "구름만 보면 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039523.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100039523.1.14",
          "form": "어제는 자는데 너무 춥더라구요",
          "original_form": "어제는 자는데 너무 춥더라구요",
          "speaker_id": "2",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100039523.1.15",
          "form": "그러셨나요?",
          "original_form": "그러셨나요?",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100039523.1.16",
          "form": "전 아침에도 좀 추웠답니다",
          "original_form": "전 아침에도 좀 추웠답니다",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100039523.1.17",
          "form": "요즘 두꺼운 이불 덮어야 하는 날씨인거같아요",
          "original_form": "요즘 두꺼운 이불 덮어야 하는 날씨인거같아요",
          "speaker_id": "2",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100039523.1.18",
          "form": "아직 얇은 이불 덮으시나요?",
          "original_form": "아직 얇은 이불 덮으시나요?",
          "speaker_id": "2",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100039523.1.19",
          "form": "음 지금 중간의 이불을 덮고 있습니다",
          "original_form": "음 지금 중간의 이불을 덮고 있습니다",
          "speaker_id": "1",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100039523.1.20",
          "form": "마자요 두께가 있어야할것같어요",
          "original_form": "마자요 두께가 있어야할것같어요",
          "speaker_id": "1",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100039523.1.21",
          "form": "춥지만 더운것보단 낫네요",
          "original_form": "춥지만 더운것보단 낫네요",
          "speaker_id": "2",
          "time": "20210904 14:43"
        },
        {
          "id": "MDRW2100039523.1.22",
          "form": "추운거, 더운거 뭐가 더 싫으세요?",
          "original_form": "추운거, 더운거 뭐가 더 싫으세요?",
          "speaker_id": "2",
          "time": "20210904 14:43"
        },
        {
          "id": "MDRW2100039523.1.23",
          "form": "더운게 더 싫습니다...",
          "original_form": "더운게 더 싫습니다...",
          "speaker_id": "1",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100039523.1.24",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100039523.1.25",
          "form": "저는...",
          "original_form": "저는...",
          "speaker_id": "2",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100039523.1.26",
          "form": "지금은 추운게 싫어요ㅎㅎ",
          "original_form": "지금은 추운게 싫어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100039523.1.27",
          "form": "오늘도 해가 쨍쨍하네요",
          "original_form": "오늘도 해가 쨍쨍하네요",
          "speaker_id": "2",
          "time": "20210905 14:12"
        },
        {
          "id": "MDRW2100039523.1.28",
          "form": "네...저는 더운날이...더워서...싫어요....ㅠㅠ",
          "original_form": "네...저는 더운날이...더워서...싫어요....ㅠㅠ",
