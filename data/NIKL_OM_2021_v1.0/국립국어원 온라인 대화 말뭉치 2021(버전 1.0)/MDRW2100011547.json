      "utterance": [
        {
          "id": "MDRW2100011547.1.1",
          "form": "방금 둘째랑 장보고왔다ㅋㅋ",
          "original_form": "방금 둘째랑 장보고왔다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011547.1.2",
          "form": "오늘 저녁은 오리고기,상추ㅋ",
          "original_form": "오늘 저녁은 오리고기,상추ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011547.1.3",
          "form": "오리고기 땡기네 ㅎㅎ",
          "original_form": "오리고기 땡기네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011547.1.4",
          "form": "난 name3 언어센터왔음",
          "original_form": "난 &name3& 언어센터왔음",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011547.1.5",
          "form": "name4은 놀이터 ㅋㅋ",
          "original_form": "&name4&은 놀이터 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011547.1.6",
          "form": "응응 언어센터갔구나",
          "original_form": "응응 언어센터갔구나",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011547.1.7",
          "form": "언니도 바쁘다잉",
          "original_form": "언니도 바쁘다잉",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011547.1.8",
          "form": "나는 지금 가계부작성중ㅋㅋ내일 월급날이다 좋다ㅋㅋ",
          "original_form": "나는 지금 가계부작성중ㅋㅋ내일 월급날이다 좋다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011547.1.9",
          "form": "아하 월급날은 또 써야지",
          "original_form": "아하 월급날은 또 써야지",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011547.1.10",
          "form": "name3 책 팔아야되는데 너무 귀찮네",
          "original_form": "&name3& 책 팔아야되는데 너무 귀찮네",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011547.1.11",
          "form": "응 너무좋네. 월급날 히히",
          "original_form": "응 너무좋네. 월급날 히히",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011547.1.12",
          "form": "당근에 팔아야되는데",
          "original_form": "당근에 팔아야되는데",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011547.1.13",
          "form": "사기만 사고",
          "original_form": "사기만 사고",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.14",
          "form": "팔지는 않고",
          "original_form": "팔지는 않고",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.15",
          "form": "그래ㅜ나도 책 또 정리해야되는데",
          "original_form": "그래ㅜ나도 책 또 정리해야되는데",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.16",
          "form": "귀찮다ㅜㅜ",
          "original_form": "귀찮다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.17",
          "form": "맞어 ㅋㅋㅋㅋ다 비슷하지않겠나",
          "original_form": "맞어 ㅋㅋㅋㅋ다 비슷하지않겠나",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.18",
          "form": "오리 얼마하더노",
          "original_form": "오리 얼마하더노",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.19",
          "form": "6900원인가하더라ㅋ",
          "original_form": "6900원인가하더라ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.20",
          "form": "다른거는 만원대하더라고",
          "original_form": "다른거는 만원대하더라고",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.21",
          "form": "쿠팡에 600그람 11천원하는거 며칠전에 먹었는데",
          "original_form": "쿠팡에 600그람 11천원하는거 며칠전에 먹었는데",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.22",
          "form": "몇그람??",
          "original_form": "몇그람??",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.23",
          "form": "그래서 세일하는거ㅋㅋ",
          "original_form": "그래서 세일하는거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011547.1.24",
          "form": "아하 집에가는길에 마트 들려봐야겠네",
          "original_form": "아하 집에가는길에 마트 들려봐야겠네",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.25",
          "form": "400그람이네ㅋ",
          "original_form": "400그람이네ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.26",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.27",
          "form": "오늘 그거샀다",
          "original_form": "오늘 그거샀다",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.28",
          "form": "뭐샀노?",
          "original_form": "뭐샀노?",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.29",
          "form": "네이버드가니 성유리가 광고하는 유리드 스틱",
          "original_form": "네이버드가니 성유리가 광고하는 유리드 스틱",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.30",
          "form": "앰플스틱 ㅋㅋㅋㄱㅋㄱㄱ",
          "original_form": "앰플스틱 ㅋㅋㅋㄱㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.31",
          "form": "하나사도 무료배송에 팩까지 준다고 해서",
          "original_form": "하나사도 무료배송에 팩까지 준다고 해서",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.32",
          "form": "앰플스틱?ㅋㅋ그건또뭐고ㅋㅋ",
          "original_form": "앰플스틱?ㅋㅋ그건또뭐고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011547.1.33",
          "form": "요새 유행하는거",
          "original_form": "요새 유행하는거",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011547.1.34",
          "form": "밤?",
          "original_form": "밤?",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011547.1.35",
          "form": "쓱쓱 얼굴에 바르면 되는",
          "original_form": "쓱쓱 얼굴에 바르면 되는",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011547.1.36",
          "form": "기미 잡티없애주는건가?",
          "original_form": "기미 잡티없애주는건가?",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011547.1.37",
          "form": "어어 스틱으로 된 밤이지",
          "original_form": "어어 스틱으로 된 밤이지",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011547.1.38",
          "form": "아니 이건 수분?? 광채나고 뭐 이런거 있잖아",
          "original_form": "아니 이건 수분?? 광채나고 뭐 이런거 있잖아",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011547.1.39",
          "form": "그거 바르면 좋은가?ㅋㅋ",
          "original_form": "그거 바르면 좋은가?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:45"
        }
      ]
