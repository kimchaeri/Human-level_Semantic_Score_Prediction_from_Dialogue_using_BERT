      "utterance": [
        {
          "id": "MDRW2100023778.1.1",
          "form": "차이 많이나?",
          "original_form": "차이 많이나?",
          "speaker_id": "2",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100023778.1.2",
          "form": "많이 나지 빨강도 여러 빨강이 있어",
          "original_form": "많이 나지 빨강도 여러 빨강이 있어",
          "speaker_id": "1",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100023778.1.3",
          "form": "톤 땜에 그런가?",
          "original_form": "톤 땜에 그런가?",
          "speaker_id": "2",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100023778.1.4",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100023778.1.5",
          "form": "그럼 톤은 할 때 정해야겠네",
          "original_form": "그럼 톤은 할 때 정해야겠네",
          "speaker_id": "2",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100023778.1.6",
          "form": "인터넷으로 염색 사진을 찾아보다 보면 꽃이는 색깔이 나오더라고",
          "original_form": "인터넷으로 염색 사진을 찾아보다 보면 꽃이는 색깔이 나오더라고",
          "speaker_id": "1",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100023778.1.7",
          "form": "아 그것도 괜찮네",
          "original_form": "아 그것도 괜찮네",
          "speaker_id": "2",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100023778.1.8",
          "form": "그런데 이제 막상 미용실에 가서 하면 같은 색상이 나올지는 모르는",
          "original_form": "그런데 이제 막상 미용실에 가서 하면 같은 색상이 나올지는 모르는",
          "speaker_id": "1",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100023778.1.9",
          "form": "미지수지",
          "original_form": "미지수지",
          "speaker_id": "2",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100023778.1.10",
          "form": "그래도 일단 설명은 해줘야 되니까",
          "original_form": "그래도 일단 설명은 해줘야 되니까",
          "speaker_id": "1",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100023778.1.11",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210830 17:20"
        },
        {
          "id": "MDRW2100023778.1.12",
          "form": "아니면 아예 염색약을 사서 집에서 하는 방법도 있어",
          "original_form": "아니면 아예 염색약을 사서 집에서 하는 방법도 있어",
          "speaker_id": "1",
          "time": "20210830 17:21"
        },
        {
          "id": "MDRW2100023778.1.13",
          "form": "나 집에서만 2번 해봤어 ㅎㅎ",
          "original_form": "나 집에서만 2번 해봤어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 17:21"
        },
        {
          "id": "MDRW2100023778.1.14",
          "form": "그치 한번 그렇게 하다 보면 미용실 가기가 아깝더라",
          "original_form": "그치 한번 그렇게 하다 보면 미용실 가기가 아깝더라",
          "speaker_id": "1",
          "time": "20210830 17:21"
        },
        {
          "id": "MDRW2100023778.1.15",
          "form": "근데 미용실이 더 색이 잘 나오는거 같아",
          "original_form": "근데 미용실이 더 색이 잘 나오는거 같아",
          "speaker_id": "2",
          "time": "20210830 17:22"
        },
        {
          "id": "MDRW2100023778.1.16",
          "form": "염색이 나오기 어려운 색들은 미용실 가야 돼",
          "original_form": "염색이 나오기 어려운 색들은 미용실 가야 돼",
          "speaker_id": "1",
          "time": "20210830 17:22"
        }
      ]
