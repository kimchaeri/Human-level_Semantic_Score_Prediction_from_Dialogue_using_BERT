      "utterance": [
        {
          "id": "MDRW2100010546.1.1",
          "form": "네 대학때ㅋ",
          "original_form": "네 대학때ㅋ",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010546.1.2",
          "form": "저도 수능 끝난 직후부터 열심히 ㅋㅋㅋ",
          "original_form": "저도 수능 끝난 직후부터 열심히 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010546.1.3",
          "form": "다시보기 겁나요ㅋ",
          "original_form": "다시보기 겁나요ㅋ",
          "speaker_id": "2",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010546.1.4",
          "form": "저도요 ㅋㅋ 그게 소중하긴 한데 손발이 오그라들것 같아요",
          "original_form": "저도요 ㅋㅋ 그게 소중하긴 한데 손발이 오그라들것 같아요",
          "speaker_id": "1",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010546.1.5",
          "form": "용기가 필요할듯요ㅎ",
          "original_form": "용기가 필요할듯요ㅎ",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010546.1.6",
          "form": "전 영상도 많이 올려놨거든요. ㄷㄷㄷ",
          "original_form": "전 영상도 많이 올려놨거든요. ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010546.1.7",
          "form": "영상 ㅋ",
          "original_form": "영상 ㅋ",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010546.1.8",
          "form": "넘나 무섭",
          "original_form": "넘나 무섭",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010546.1.9",
          "form": "다이어리는 쓰셨나요?",
          "original_form": "다이어리는 쓰셨나요?",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010546.1.10",
          "form": "네 오그라드네요ㅋ",
          "original_form": "네 오그라드네요ㅋ",
          "speaker_id": "2",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010546.1.11",
          "form": "도토리 좀 모으셨었나요?",
          "original_form": "도토리 좀 모으셨었나요?",
          "speaker_id": "1",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010546.1.12",
          "form": "도토리모아서 노래사고",
          "original_form": "도토리모아서 노래사고",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010546.1.13",
          "form": "추억돋네요",
          "original_form": "추억돋네요",
          "speaker_id": "2",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010546.1.14",
          "form": "그 미니미 꾸미는 것도 샀잖아요",
          "original_form": "그 미니미 꾸미는 것도 샀잖아요",
          "speaker_id": "1",
          "time": "20210823 19:51"
        },
        {
          "id": "MDRW2100010546.1.15",
          "form": "미니미  기억나요ㅎ",
          "original_form": "미니미  기억나요ㅎ",
          "speaker_id": "2",
