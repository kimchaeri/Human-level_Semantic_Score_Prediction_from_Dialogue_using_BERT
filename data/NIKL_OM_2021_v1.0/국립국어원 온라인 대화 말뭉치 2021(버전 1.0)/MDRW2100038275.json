        },
        {
          "id": "MDRW2100038275.1.3",
          "form": "아침 뭐 드셨나요",
          "original_form": "아침 뭐 드셨나요",
          "speaker_id": "1",
          "time": "20210902 07:50"
        },
        {
          "id": "MDRW2100038275.1.4",
          "form": "빈속이에요....출근해서 군것질좀할려구요",
          "original_form": "빈속이에요....출근해서 군것질좀할려구요",
          "speaker_id": "2",
          "time": "20210902 07:50"
        },
        {
          "id": "MDRW2100038275.1.5",
          "form": "배고파요ㅜㅜ",
          "original_form": "배고파요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 07:50"
        },
        {
          "id": "MDRW2100038275.1.6",
          "form": "밥주세요ㅠㅠ",
          "original_form": "밥주세요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 07:51"
        },
        {
          "id": "MDRW2100038275.1.7",
          "form": "ㅠㅠ 빈속으로 출근하셨다니",
          "original_form": "ㅠㅠ 빈속으로 출근하셨다니",
          "speaker_id": "1",
          "time": "20210902 07:52"
        },
        {
          "id": "MDRW2100038275.1.8",
          "form": "군것질 잘 챙겨드세요",
          "original_form": "군것질 잘 챙겨드세요",
          "speaker_id": "1",
          "time": "20210902 07:52"
        },
        {
          "id": "MDRW2100038275.1.9",
          "form": "네ㅜㅜ과자있어서 과자먹을듯해요",
          "original_form": "네ㅜㅜ과자있어서 과자먹을듯해요",
          "speaker_id": "2",
          "time": "20210902 07:54"
        },
        {
          "id": "MDRW2100038275.1.10",
          "form": "퇴근하면맛있는거먹을테야!!!",
          "original_form": "퇴근하면맛있는거먹을테야!!!",
          "speaker_id": "2",
          "time": "20210902 07:54"
        },
        {
          "id": "MDRW2100038275.1.11",
          "form": "ㅜㆍㅜ",
          "original_form": "ㅜㆍㅜ",
          "speaker_id": "2",
          "time": "20210902 07:54"
        },
        {
          "id": "MDRW2100038275.1.12",
          "form": "맞아요 저녁은 꼭 맛있게 드세요",
          "original_form": "맞아요 저녁은 꼭 맛있게 드세요",
          "speaker_id": "1",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100038275.1.13",
          "form": "네 그래야죠 먹어야 사니깐요",
          "original_form": "네 그래야죠 먹어야 사니깐요",
          "speaker_id": "2",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100038275.1.14",
          "form": "먹기위해서 저는 사니깐요",
          "original_form": "먹기위해서 저는 사니깐요",
          "speaker_id": "2",
          "time": "20210902 07:55"
        },
        {
          "id": "MDRW2100038275.1.15",
          "form": "먹기위해서 사는건 모두가 똑같군요ㅋㅋ",
          "original_form": "먹기위해서 사는건 모두가 똑같군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100038275.1.16",
          "form": "당연하죠!!! 먹어야 움직이니깐요",
          "original_form": "당연하죠!!! 먹어야 움직이니깐요",
          "speaker_id": "2",
          "time": "20210902 08:00"
        },
        {
          "id": "MDRW2100038275.1.17",
          "form": "움직이면 또 먹을걸 찾게되고",
          "original_form": "움직이면 또 먹을걸 찾게되고",
          "speaker_id": "1",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100038275.1.18",
          "form": "결국은 하나의 순환이군요",
          "original_form": "결국은 하나의 순환이군요",
          "speaker_id": "1",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100038275.1.19",
          "form": "그니깐 밥먹는배 후식먹는배 따로 있어요",
          "original_form": "그니깐 밥먹는배 후식먹는배 따로 있어요",
