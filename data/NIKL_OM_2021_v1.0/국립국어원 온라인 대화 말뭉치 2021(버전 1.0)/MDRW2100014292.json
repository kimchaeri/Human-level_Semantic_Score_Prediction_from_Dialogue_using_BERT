          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014292.1.3",
          "form": "쟈갸 어디가",
          "original_form": "쟈갸 어디가",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014292.1.4",
          "form": "와따시는 엄마친구 성악 공연보러가",
          "original_form": "와따시는 엄마친구 성악 공연보러가",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014292.1.5",
          "form": "갸아아악",
          "original_form": "갸아아악",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014292.1.6",
          "form": "힝 답장해조",
          "original_form": "힝 답장해조",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014292.1.7",
          "form": "쏴리 나도 나갈것같읭",
          "original_form": "쏴리 나도 나갈것같읭",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014292.1.8",
          "form": "아 믜칀ㄷㄷㄷㄷ",
          "original_form": "아 믜칀ㄷㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014292.1.9",
          "form": "언제시작해 공연",
          "original_form": "언제시작해 공연",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014292.1.10",
          "form": "7시 반!!",
          "original_form": "7시 반!!",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014292.1.11",
          "form": "천서진 ㄷㄷ",
          "original_form": "천서진 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014292.1.12",
          "form": "우왕",
