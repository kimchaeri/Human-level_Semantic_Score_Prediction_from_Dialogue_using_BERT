      "utterance": [
        {
          "id": "MDRW2100037129.1.1",
          "form": "부모님 용돈도 줄까 말까인데",
          "original_form": "부모님 용돈도 줄까 말까인데",
          "speaker_id": "2",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100037129.1.2",
          "form": "아무리 장녀라지만",
          "original_form": "아무리 장녀라지만",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100037129.1.3",
          "form": "지금이 60년대야 뭐야",
          "original_form": "지금이 60년대야 뭐야",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100037129.1.4",
          "form": "심지어 걘 고딩때부터 기숙사 살았어",
          "original_form": "심지어 걘 고딩때부터 기숙사 살았어",
          "speaker_id": "2",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100037129.1.5",
          "form": "나랑 안산지 거의 10년임",
          "original_form": "나랑 안산지 거의 10년임",
          "speaker_id": "2",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100037129.1.6",
          "form": "나도 내가 고등학생때 기숙사 살았음 ㅋㅋㅋㅋㅋ",
          "original_form": "나도 내가 고등학생때 기숙사 살았음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037129.1.7",
          "form": "그리고 걔랑 나랑 데면데면하면",
          "original_form": "그리고 걔랑 나랑 데면데면하면",
          "speaker_id": "2",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037129.1.8",
          "form": "나한테 뭐라해",
          "original_form": "나한테 뭐라해",
          "speaker_id": "2",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037129.1.9",
          "form": "난 솔직히",
          "original_form": "난 솔직히",
          "speaker_id": "2",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037129.1.10",
          "form": "걔랑 데면데면한게",
          "original_form": "걔랑 데면데면한게",
          "speaker_id": "2",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037129.1.11",
          "form": "좋단말야",
          "original_form": "좋단말야",
          "speaker_id": "2",
          "time": "20210831 20:22"
        },
        {
          "id": "MDRW2100037129.1.12",
          "form": "굳이 친한척하거나 뭘 같이 하느니",
          "original_form": "굳이 친한척하거나 뭘 같이 하느니",
          "speaker_id": "2",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100037129.1.13",
          "form": "나도 ㅋㅋㅋㅋㅋ 가족이라고 굳이 친하게 지낼 필요 있나",
          "original_form": "나도 ㅋㅋㅋㅋㅋ 가족이라고 굳이 친하게 지낼 필요 있나",
          "speaker_id": "1",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100037129.1.14",
          "form": "걍 서로 필요한거 딱 말하고",
          "original_form": "걍 서로 필요한거 딱 말하고",
          "speaker_id": "2",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100037129.1.15",
          "form": "방에 들어가서 이어폰 끼고 안나오는게",
          "original_form": "방에 들어가서 이어폰 끼고 안나오는게",
          "speaker_id": "2",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100037129.1.16",
          "form": "딱 좋다고....",
          "original_form": "딱 좋다고....",
          "speaker_id": "2",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100037129.1.17",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100037129.1.18",
          "form": "그리고 솔직히",
          "original_form": "그리고 솔직히",
          "speaker_id": "1",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100037129.1.19",
          "form": "걍 내비두면",
          "original_form": "걍 내비두면",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.20",
          "form": "나랑 동생이 알아서",
          "original_form": "나랑 동생이 알아서",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.21",
          "form": "서로 적절한 합의점을 찾는단말임?",
          "original_form": "서로 적절한 합의점을 찾는단말임?",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.22",
          "form": "ㅇㅇㅎ",
          "original_form": "ㅇㅇㅎ",
          "speaker_id": "2",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.23",
          "form": "우리도 디지게 싸우다가",
          "original_form": "우리도 디지게 싸우다가",
          "speaker_id": "2",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.24",
          "form": "괜히 부모가 개입하면 서로 더 질색하게 됨",
          "original_form": "괜히 부모가 개입하면 서로 더 질색하게 됨",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.25",
          "form": "찾은 합의점이",
          "original_form": "찾은 합의점이",
          "speaker_id": "2",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.26",
          "form": "둘 다 이어폰 끼고",
          "original_form": "둘 다 이어폰 끼고",
          "speaker_id": "2",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.27",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037129.1.28",
          "form": "각자의 방으로 들어가있기란 말임",
          "original_form": "각자의 방으로 들어가있기란 말임",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037129.1.29",
          "form": "굳이 꺼내서",
          "original_form": "굳이 꺼내서",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037129.1.30",
          "form": "붙여두려하면",
          "original_form": "붙여두려하면",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037129.1.31",
          "form": "싸움이 안나겠냐고",
          "original_form": "싸움이 안나겠냐고",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037129.1.32",
          "form": "좀 엄마 머릿속에 꽃밭이 있는것같아",
          "original_form": "좀 엄마 머릿속에 꽃밭이 있는것같아",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037129.1.33",
          "form": "ㅇㅇ ㅎ",
          "original_form": "ㅇㅇ ㅎ",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037129.1.34",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037129.1.35",
          "form": "비록 서로의 사정으로 바쁘지만(ㅋ) 다 같이 모이면 화기애애하게 단란한 식사자리를 갖는",
          "original_form": "비록 서로의 사정으로 바쁘지만(ㅋ) 다 같이 모이면 화기애애하게 단란한 식사자리를 갖는",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037129.1.36",
          "form": "그런 모습?",
          "original_form": "그런 모습?",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037129.1.37",
          "form": "가족 다 모일 일 있으면",
          "original_form": "가족 다 모일 일 있으면",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037129.1.38",
          "form": "꼭 한끼쯤",
          "original_form": "꼭 한끼쯤",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037129.1.39",
          "form": "엄마의 욕심으로 그런 자리가 만들어지는데",
          "original_form": "엄마의 욕심으로 그런 자리가 만들어지는데",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037129.1.40",
          "form": "그게 될리가ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그게 될리가ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037129.1.41",
          "form": "보통 아빠가 성질부리거나",
          "original_form": "보통 아빠가 성질부리거나",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037129.1.42",
          "form": "동생이 자리 박차고 집으로 가버리고",
          "original_form": "동생이 자리 박차고 집으로 가버리고",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037129.1.43",
          "form": "엄마는 나한테 가족들이 너무 매정하다고 울고ㅋ",
          "original_form": "엄마는 나한테 가족들이 너무 매정하다고 울고ㅋ",
          "speaker_id": "2",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037129.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037129.1.45",
          "form": "우리는 그런 로망을",
          "original_form": "우리는 그런 로망을",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037129.1.46",
          "form": "아빠가 갖고있음 ㅎ",
          "original_form": "아빠가 갖고있음 ㅎ",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.47",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.48",
          "form": "글고 엄마는 뭔가",
          "original_form": "글고 엄마는 뭔가",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.49",
          "form": "집에서 가족들에게 따뜻한 밥 한끼를 차려주는 본인에 대한 로망이 있나봄 ㅎ",
          "original_form": "집에서 가족들에게 따뜻한 밥 한끼를 차려주는 본인에 대한 로망이 있나봄 ㅎ",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.50",
          "form": "동생이 군대가기 전에",
          "original_form": "동생이 군대가기 전에",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.51",
          "form": "아ㅎ",
          "original_form": "아ㅎ",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.52",
          "form": "그것도 뭔지",
          "original_form": "그것도 뭔지",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.53",
          "form": "알것같다",
          "original_form": "알것같다",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.54",
          "form": "자기가 무슨 일이 있어도 집밥을 먹여야겠다고",
          "original_form": "자기가 무슨 일이 있어도 집밥을 먹여야겠다고",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.55",
          "form": "이모도 나도 할머니도",
          "original_form": "이모도 나도 할머니도",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.56",
          "form": "대체 왜 굳이...?",
          "original_form": "대체 왜 굳이...?",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100037129.1.57",
          "form": "하면서 아무도 이해 못하는데",
          "original_form": "하면서 아무도 이해 못하는데",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037129.1.58",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그거 보통",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그거 보통",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037129.1.59",
          "form": "혼자 그 로망에 취해서 밥 거나하게 차림",
          "original_form": "혼자 그 로망에 취해서 밥 거나하게 차림",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037129.1.60",
          "form": "엄마가 맞벌이를 하면",
          "original_form": "엄마가 맞벌이를 하면",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037129.1.61",
          "form": "그런 이해 못할 욕망이",
          "original_form": "그런 이해 못할 욕망이",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037129.1.62",
          "form": "생기더라ㅎ",
          "original_form": "생기더라ㅎ",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037129.1.63",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037129.1.64",
          "form": "근데 우리 엄마는 요리를 못해서",
