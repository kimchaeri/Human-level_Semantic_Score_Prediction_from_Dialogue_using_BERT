      "utterance": [
        {
          "id": "MDRW2100042000.1.1",
          "form": "연골다쳐 젊은 나이에 그러면 안된다",
          "original_form": "연골다쳐 젊은 나이에 그러면 안된다",
          "speaker_id": "2",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100042000.1.2",
          "form": "줄넘기 그래도 건강에 좋지 않아요?",
          "original_form": "줄넘기 그래도 건강에 좋지 않아요?",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042000.1.3",
          "form": "저 요즘 혈약순환이 넘 안돼서 운동 하긴 해야할거같아요",
          "original_form": "저 요즘 혈약순환이 넘 안돼서 운동 하긴 해야할거같아요",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042000.1.4",
          "form": "손발이 차? 여름인데",
          "original_form": "손발이 차? 여름인데",
          "speaker_id": "2",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042000.1.5",
          "form": "그게 아니구 막 저릿저릿하더라구요",
          "original_form": "그게 아니구 막 저릿저릿하더라구요",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042000.1.6",
          "form": "쥐나나보다",
          "original_form": "쥐나나보다",
          "speaker_id": "2",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042000.1.7",
          "form": "ㅠㅠ 자세 안좋으면 바로 쥐나서 피안통하고 그래가지고 억울",
          "original_form": "ㅠㅠ 자세 안좋으면 바로 쥐나서 피안통하고 그래가지고 억울",
          "speaker_id": "1",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042000.1.8",
          "form": "아까는 좋은 자세 하고 있다며",
          "original_form": "아까는 좋은 자세 하고 있다며",
          "speaker_id": "2",
          "time": "20210906 18:58"
        },
        {
          "id": "MDRW2100042000.1.9",
          "form": "강제로 바른자세 중잉에요 ㅋㅋㅋ",
          "original_form": "강제로 바른자세 중잉에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:59"
        },
        {
          "id": "MDRW2100042000.1.10",
          "form": "상기의 사유로 ㅠㅠ",
          "original_form": "상기의 사유로 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:00"
        },
        {
          "id": "MDRW2100042000.1.11",
          "form": "맛사지 열심히 해줘야 혈액순환이 잘된다",
          "original_form": "맛사지 열심히 해줘야 혈액순환이 잘된다",
          "speaker_id": "2",
          "time": "20210906 19:00"
        },
        {
          "id": "MDRW2100042000.1.12",
          "form": "다뜻한 물도 많이 마시고 그래",
          "original_form": "다뜻한 물도 많이 마시고 그래",
          "speaker_id": "2",
          "time": "20210906 19:00"
        },
        {
          "id": "MDRW2100042000.1.13",
          "form": "더워서 자꾸 찬물만 마시게 되는데 그래서 더 그런가봐요",
          "original_form": "더워서 자꾸 찬물만 마시게 되는데 그래서 더 그런가봐요",
          "speaker_id": "1",
          "time": "20210906 19:00"
        },
        {
          "id": "MDRW2100042000.1.14",
          "form": "물부터 잘 관리해야 몸에 좋다",
          "original_form": "물부터 잘 관리해야 몸에 좋다",
          "speaker_id": "2",
          "time": "20210906 19:01"
        },
        {
          "id": "MDRW2100042000.1.15",
          "form": "엄마두 ㅠ 뭐 엄마는 대추차 마신댔지만 안좋을수도 있으니까 너무 과복용하진 마세요",
          "original_form": "엄마두 ㅠ 뭐 엄마는 대추차 마신댔지만 안좋을수도 있으니까 너무 과복용하진 마세요",
          "speaker_id": "1",
          "time": "20210906 19:01"
        },
        {
          "id": "MDRW2100042000.1.16",
          "form": "대추차가 뭐가 몸에 안좋겠니",
          "original_form": "대추차가 뭐가 몸에 안좋겠니",
          "speaker_id": "2",
          "time": "20210906 19:01"
        },
        {
          "id": "MDRW2100042000.1.17",
          "form": "물도 많이마시면 몸에 안좋으니까˜",
          "original_form": "물도 많이마시면 몸에 안좋으니까˜",
          "speaker_id": "1",
          "time": "20210906 19:01"
        },
        {
          "id": "MDRW2100042000.1.18",
          "form": "알겠다 운동 잘 하고 이불 잘 덮고 자",
          "original_form": "알겠다 운동 잘 하고 이불 잘 덮고 자",
          "speaker_id": "2",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100042000.1.19",
          "form": "밥은 드셨어요",
          "original_form": "밥은 드셨어요",
          "speaker_id": "1",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100042000.1.20",
          "form": "아빠가 국밥사와서 먹었다",
          "original_form": "아빠가 국밥사와서 먹었다",
          "speaker_id": "2",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100042000.1.21",
          "form": "몸보신 됐겠네요 ㅋㅋㅋ",
          "original_form": "몸보신 됐겠네요 ㅋㅋㅋ",
