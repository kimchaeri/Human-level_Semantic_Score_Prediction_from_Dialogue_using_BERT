      "utterance": [
        {
          "id": "MDRW2100005808.1.1",
          "form": "영양제를 먹거나 운동하는게 있나요",
          "original_form": "영양제를 먹거나 운동하는게 있나요",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100005808.1.2",
          "form": "비타민 유산균 먹어요",
          "original_form": "비타민 유산균 먹어요",
          "speaker_id": "2",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100005808.1.3",
          "form": "비타민 씨가 감기예방에 좋다고하네요",
          "original_form": "비타민 씨가 감기예방에 좋다고하네요",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005808.1.4",
          "form": "아 저 그래서 최근에 청귤청 담았엉ㅅ",
          "original_form": "아 저 그래서 최근에 청귤청 담았엉ㅅ",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100005808.1.5",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100005808.1.6",
          "form": "제주도라 가능하네요",
          "original_form": "제주도라 가능하네요",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100005808.1.7",
          "form": "맛있겠습니다",
          "original_form": "맛있겠습니다",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100005808.1.8",
          "form": "육지로 가는 것도 가격 괜찮던데요",
          "original_form": "육지로 가는 것도 가격 괜찮던데요",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100005808.1.9",
          "form": "비타민씨가 레몬 10배라고 해서 ㅎㅎ",
          "original_form": "비타민씨가 레몬 10배라고 해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100005808.1.10",
          "form": "청귤청",
          "original_form": "청귤청",
          "speaker_id": "1",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100005808.1.11",
          "form": "우리집도 시도해봐야겠네요",
          "original_form": "우리집도 시도해봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005808.1.12",
          "form": "저는 홍삼도 먹고 있습니다",
          "original_form": "저는 홍삼도 먹고 있습니다",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005808.1.13",
          "form": "아 홍삼을 사셨어요?",
          "original_form": "아 홍삼을 사셨어요?",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005808.1.14",
          "form": "기력회복에 좋다하더군요",
          "original_form": "기력회복에 좋다하더군요",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100005808.1.15",
          "form": "정관장꺼 먹습니다",
          "original_form": "정관장꺼 먹습니다",
          "speaker_id": "1",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100005808.1.16",
          "form": "어머니는 오쿠로 홍삼을 만들어서 우려두시던데",
          "original_form": "어머니는 오쿠로 홍삼을 만들어서 우려두시던데",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100005808.1.17",
          "form": "아 정관장 좋쵸",
          "original_form": "아 정관장 좋쵸",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100005808.1.18",
          "form": "홍삼메이커가 있는데",
          "original_form": "홍삼메이커가 있는데",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005808.1.19",
          "form": "정관장이 먹기 편해서 즐깁니다",
          "original_form": "정관장이 먹기 편해서 즐깁니다",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005808.1.20",
          "form": "매일 드시나여",
          "original_form": "매일 드시나여",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100005808.1.21",
          "form": "아침마다 한포씩 먹습니다",
          "original_form": "아침마다 한포씩 먹습니다",
