      "utterance": [
        {
          "id": "MDRW2100030270.1.1",
          "form": "엄빠랑먹었눈데 엄ㅃㅏ는 시래기만머굼",
          "original_form": "엄빠랑먹었눈데 엄ㅃㅏ는 시래기만머굼",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030270.1.2",
          "form": "됼라빠루네여ㅋㅋㅋㅋㅋㅋ",
          "original_form": "됼라빠루네여ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030270.1.3",
          "form": "먹을거만큼재밌는이야기가없쪄",
          "original_form": "먹을거만큼재밌는이야기가없쪄",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030270.1.4",
          "form": "왜 엄빠는 시래기주고",
          "original_form": "왜 엄빠는 시래기주고",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030270.1.5",
          "form": "혼자 고기먹어요",
          "original_form": "혼자 고기먹어요",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030270.1.6",
          "form": "나쁘네",
          "original_form": "나쁘네",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030270.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030270.1.8",
          "form": "몰라여 고기머구라했ㄴ는디 다들시래기맨먹고 고기손도안댐",
          "original_form": "몰라여 고기머구라했ㄴ는디 다들시래기맨먹고 고기손도안댐",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030270.1.9",
          "form": "나배불러서먹기시른디ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나배불러서먹기시른디ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030270.1.10",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030270.1.11",
          "form": "다들 고기를 안좋아해요?",
          "original_form": "다들 고기를 안좋아해요?",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030270.1.12",
          "form": "별로안좋아해여ㅋㅋㅋ 나만좋아하는듯",
          "original_form": "별로안좋아해여ㅋㅋㅋ 나만좋아하는듯",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030270.1.13",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030270.1.14",
          "form": "울엄마도 그러는데",
          "original_form": "울엄마도 그러는데",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030270.1.15",
          "form": "아빠랑 나만 잘먹는ㅋㅋㅋㄱㅋ",
          "original_form": "아빠랑 나만 잘먹는ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030270.1.16",
          "form": "긍께 맛있는디 왜안먹나몰롸",
          "original_form": "긍께 맛있는디 왜안먹나몰롸",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030270.1.17",
          "form": "치킨먹고싶다요",
          "original_form": "치킨먹고싶다요",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030270.1.18",
          "form": "치킨치킨",
          "original_form": "치킨치킨",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030270.1.19",
          "form": "치킨졸맛 저는 얼마전에 맛초킹먹었어염ㅋㅋㅋ",
          "original_form": "치킨졸맛 저는 얼마전에 맛초킹먹었어염ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030270.1.20",
          "form": "졸맛이겠네요",
          "original_form": "졸맛이겠네요",
          "speaker_id": "1",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030270.1.21",
          "form": "오늘 점심 맘에들지않아요",
          "original_form": "오늘 점심 맘에들지않아요",
          "speaker_id": "1",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030270.1.22",
          "form": "ㅋㅋㅋ오늘점심뭐엿어요",
          "original_form": "ㅋㅋㅋ오늘점심뭐엿어요",
          "speaker_id": "2",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030270.1.23",
          "form": "두부찜",
          "original_form": "두부찜",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030270.1.24",
          "form": "그래서 참기름에 밥비벼먹음",
          "original_form": "그래서 참기름에 밥비벼먹음",
          "speaker_id": "1",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100030270.1.25",
          "form": "아... 저는 내일라면이에여",
          "original_form": "아... 저는 내일라면이에여",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100030270.1.26",
          "form": "난라면시러",
          "original_form": "난라면시러",
          "speaker_id": "2",
          "time": "20210904 15:55"
        }
      ]
    }
