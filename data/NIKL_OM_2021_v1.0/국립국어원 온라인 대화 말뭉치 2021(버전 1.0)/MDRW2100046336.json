        },
        {
          "id": "MDRW2100046336.1.4",
          "form": "취미 있으신가요?",
          "original_form": "취미 있으신가요?",
          "speaker_id": "2",
          "time": "20211018 21:51"
        },
        {
          "id": "MDRW2100046336.1.5",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211018 21:51"
        },
        {
          "id": "MDRW2100046336.1.6",
          "form": "네 게임입니다",
          "original_form": "네 게임입니다",
          "speaker_id": "1",
          "time": "20211018 21:51"
        },
        {
          "id": "MDRW2100046336.1.7",
          "form": "와 저도 게임 좋아해요",
          "original_form": "와 저도 게임 좋아해요",
          "speaker_id": "2",
          "time": "20211018 21:51"
        },
        {
          "id": "MDRW2100046336.1.8",
          "form": "뮤슨 게임 주로 하시나요?",
          "original_form": "뮤슨 게임 주로 하시나요?",
          "speaker_id": "2",
          "time": "20211018 21:52"
        },
        {
          "id": "MDRW2100046336.1.9",
          "form": "ㅎㅎ 굿!   오버워치라는 게임입니다",
          "original_form": "ㅎㅎ 굿!   오버워치라는 게임입니다",
          "speaker_id": "1",
          "time": "20211018 21:52"
        },
        {
          "id": "MDRW2100046336.1.10",
          "form": "오 저도 한때 열심히 했어요 ㅎㅎ",
          "original_form": "오 저도 한때 열심히 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211018 21:52"
        },
        {
          "id": "MDRW2100046336.1.11",
          "form": "오버워치 2도 나온다고 하더라구요",
          "original_form": "오버워치 2도 나온다고 하더라구요",
          "speaker_id": "2",
          "time": "20211018 21:52"
        },
        {
          "id": "MDRW2100046336.1.12",
          "form": "흑흑 게임 이용자수가 줄어서",
          "original_form": "흑흑 게임 이용자수가 줄어서",
          "speaker_id": "1",
          "time": "20211018 21:52"
        },
        {
          "id": "MDRW2100046336.1.13",
          "form": "망해가고있어요",
          "original_form": "망해가고있어요",
          "speaker_id": "1",
          "time": "20211018 21:52"
        },
        {
          "id": "MDRW2100046336.1.14",
          "form": "망하면 안되는데 ㅜ",
          "original_form": "망하면 안되는데 ㅜ",
          "speaker_id": "2",
          "time": "20211018 21:53"
        },
        {
          "id": "MDRW2100046336.1.15",
          "form": "가볍게 즐기기 좋아서 좋아요",
          "original_form": "가볍게 즐기기 좋아서 좋아요",
          "speaker_id": "1",
          "time": "20211018 21:53"
        },
        {
          "id": "MDRW2100046336.1.16",
          "form": "처음 나왔을때",
          "original_form": "처음 나왔을때",
          "speaker_id": "2",
          "time": "20211018 21:53"
        },
        {
          "id": "MDRW2100046336.1.17",
          "form": "복귀해보심이 ㅎㅎ",
          "original_form": "복귀해보심이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211018 21:53"
        },
        {
          "id": "MDRW2100046336.1.18",
          "form": "pc방에서 살았어요 ㅋㅋㅋㅋ",
          "original_form": "pc방에서 살았어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211018 21:53"
        },
        {
          "id": "MDRW2100046336.1.19",
          "form": "너무 재밌더라고요",
          "original_form": "너무 재밌더라고요",
          "speaker_id": "2",
          "time": "20211018 21:53"
        },
        {
          "id": "MDRW2100046336.1.20",
          "form": "ㅋㅋ  배틀그라운드로 갈아탔다가   라이트하게 게임하고싶어서",
          "original_form": "ㅋㅋ  배틀그라운드로 갈아탔다가   라이트하게 게임하고싶어서",
          "speaker_id": "1",
          "time": "20211018 21:54"
        },
        {
          "id": "MDRW2100046336.1.21",
          "form": "오버워치 다시 시작했죠 ㅎㅎ",
          "original_form": "오버워치 다시 시작했죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211018 21:54"
        },
        {
          "id": "MDRW2100046336.1.22",
          "form": "저도 여러가지 하다가",
          "original_form": "저도 여러가지 하다가",
          "speaker_id": "2",
          "time": "20211018 21:54"
        },
        {
          "id": "MDRW2100046336.1.23",
          "form": "요즘엔 힐링겜만 하네요",
          "original_form": "요즘엔 힐링겜만 하네요",
          "speaker_id": "2",
          "time": "20211018 21:54"
        },
        {
