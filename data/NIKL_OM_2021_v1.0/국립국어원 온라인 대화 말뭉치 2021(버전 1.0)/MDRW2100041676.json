      "utterance": [
        {
          "id": "MDRW2100041676.1.1",
          "form": "물비빔냉면은 비빔먹다가 물넣어서 드시는건가요??",
          "original_form": "물비빔냉면은 비빔먹다가 물넣어서 드시는건가요??",
          "speaker_id": "2",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100041676.1.2",
          "form": "비빔인데 첨부터 물이들어잇어요ㅋㅋ",
          "original_form": "비빔인데 첨부터 물이들어잇어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100041676.1.3",
          "form": "그렇구나!! ㅎㅎㅎ",
          "original_form": "그렇구나!! ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100041676.1.4",
          "form": "맛있을거 같아요!!!!!",
          "original_form": "맛있을거 같아요!!!!!",
          "speaker_id": "2",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100041676.1.5",
          "form": "뭐든 맛이 있을거 같긴 한데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭐든 맛이 있을거 같긴 한데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100041676.1.6",
          "form": "그후부터 모든 비빔면에 냉면육수를 넣어 먹어요ㅋㅋ",
          "original_form": "그후부터 모든 비빔면에 냉면육수를 넣어 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100041676.1.7",
          "form": "저 풀무원에서 춘천막국수란거 샀는데",
          "original_form": "저 풀무원에서 춘천막국수란거 샀는데",
          "speaker_id": "2",
          "time": "20210906 14:24"
        },
        {
          "id": "MDRW2100041676.1.8",
          "form": "거기도 육수랑 비빔소스 같이 들어있더라고요",
          "original_form": "거기도 육수랑 비빔소스 같이 들어있더라고요",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100041676.1.9",
          "form": "맛잇나요",
          "original_form": "맛잇나요",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100041676.1.10",
          "form": "아직 안먹어봤는데 맛있다고 해서 샀어요 그렇게 먹는게 맛있나보네여",
          "original_form": "아직 안먹어봤는데 맛있다고 해서 샀어요 그렇게 먹는게 맛있나보네여",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100041676.1.11",
          "form": "막국수는 왠지 면삶는게 어려울거같아여",
          "original_form": "막국수는 왠지 면삶는게 어려울거같아여",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041676.1.12",
          "form": "저 풀무원껀 막국수라고 하는데",
          "original_form": "저 풀무원껀 막국수라고 하는데",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041676.1.13",
          "form": "본격 막국수가 아닌거 같긴 했어요",
          "original_form": "본격 막국수가 아닌거 같긴 했어요",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041676.1.14",
          "form": "그냥 국수처럼 하면 되려나요\r\n찾아봐야겠네요",
          "original_form": "그냥 국수처럼 하면 되려나요\r\n찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041676.1.15",
          "form": "그럴거 같아요",
          "original_form": "그럴거 같아요",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041676.1.16",
          "form": "그냥 편하게 나왔을거 같아요",
          "original_form": "그냥 편하게 나왔을거 같아요",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041676.1.17",
          "form": "하긴 어려우면 안살거같긴 해요ㅋㅋ",
          "original_form": "하긴 어려우면 안살거같긴 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041676.1.18",
          "form": "저 그럼 못샀어영ㅋㅋㅋㅋ",
          "original_form": "저 그럼 못샀어영ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041676.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041676.1.20",
          "form": "어쩌다보니 풀무원꺼 많이 먹고 있거든요",
