      "utterance": [
        {
          "id": "MDRW2100043309.1.1",
          "form": "으아아 퇴근하시나요 ㅠ",
          "original_form": "으아아 퇴근하시나요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100043309.1.2",
          "form": "네 퇴근길이예요",
          "original_form": "네 퇴근길이예요",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100043309.1.3",
          "form": "축축..",
          "original_form": "축축..",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100043309.1.4",
          "form": "날씨가 눅눅 하긴해요 ㅠ",
          "original_form": "날씨가 눅눅 하긴해요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100043309.1.5",
          "form": "네 습도가 높아요",
          "original_form": "네 습도가 높아요",
          "speaker_id": "1",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043309.1.6",
          "form": "저녁은 오늘 어떤거 드실 예정이세요?",
          "original_form": "저녁은 오늘 어떤거 드실 예정이세요?",
          "speaker_id": "2",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043309.1.7",
          "form": "오늘은 비도 오고 그러니",
          "original_form": "오늘은 비도 오고 그러니",
          "speaker_id": "1",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043309.1.8",
          "form": "뜨끈한 국물류가 당겨요",
          "original_form": "뜨끈한 국물류가 당겨요",
          "speaker_id": "1",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100043309.1.9",
          "form": "오 좋아요 좋아",
          "original_form": "오 좋아요 좋아",
          "speaker_id": "2",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043309.1.10",
          "form": "몸이 따뜻해지고 최고일것 같아요",
          "original_form": "몸이 따뜻해지고 최고일것 같아요",
          "speaker_id": "2",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043309.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043309.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043309.1.13",
          "form": "저는 오늘은 핫케이크요 ㅎㅎ",
          "original_form": "저는 오늘은 핫케이크요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043309.1.14",
          "form": "와 핫케이크 좋은데요",
          "original_form": "와 핫케이크 좋은데요",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043309.1.15",
          "form": "달달한 향",
          "original_form": "달달한 향",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043309.1.16",
          "form": "바닐라 향이 은은하게 나면서 버터향도 살짝 나고",
          "original_form": "바닐라 향이 은은하게 나면서 버터향도 살짝 나고",
          "speaker_id": "2",
