      "utterance": [
        {
          "id": "MDRW2100031876.1.1",
          "form": "결혼해서 좋은 점이 있나요?",
          "original_form": "결혼해서 좋은 점이 있나요?",
          "speaker_id": "1",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100031876.1.2",
          "form": "나는 인간을 만들었다",
          "original_form": "나는 인간을 만들었다",
          "speaker_id": "2",
          "time": "20210905 07:01"
        },
        {
          "id": "MDRW2100031876.1.3",
          "form": "정정합니다",
          "original_form": "정정합니다",
          "speaker_id": "2",
          "time": "20210905 07:01"
        },
        {
          "id": "MDRW2100031876.1.4",
          "form": "나는 인간을 만들었다 좋은 말이네요",
          "original_form": "나는 인간을 만들었다 좋은 말이네요",
          "speaker_id": "1",
          "time": "20210905 07:01"
        },
        {
          "id": "MDRW2100031876.1.5",
          "form": "나를 인 인간 만들었다",
          "original_form": "나를 인 인간 만들었다",
          "speaker_id": "2",
          "time": "20210905 07:01"
        },
        {
          "id": "MDRW2100031876.1.6",
          "form": "음성인식이라",
          "original_form": "음성인식이라",
          "speaker_id": "2",
          "time": "20210905 07:01"
        },
        {
          "id": "MDRW2100031876.1.7",
          "form": "저를 사람답게 만들엇단 뜻입니다",
          "original_form": "저를 사람답게 만들엇단 뜻입니다",
          "speaker_id": "2",
          "time": "20210905 07:01"
        },
        {
          "id": "MDRW2100031876.1.8",
          "form": "원래 사람아니엇거든요",
          "original_form": "원래 사람아니엇거든요",
          "speaker_id": "2",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031876.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031876.1.10",
          "form": "네 결혼해서 좋은 점은 뭐가 있고 안 좋은 점은 뭐가 있는지 궁금해요",
          "original_form": "네 결혼해서 좋은 점은 뭐가 있고 안 좋은 점은 뭐가 있는지 궁금해요",
          "speaker_id": "1",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031876.1.11",
          "form": "지금 나이가 어떻게 되시죠",
          "original_form": "지금 나이가 어떻게 되시죠",
          "speaker_id": "2",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031876.1.12",
          "form": "30대 후반입니다",
          "original_form": "30대 후반입니다",
          "speaker_id": "1",
          "time": "20210905 07:03"
        },
        {
          "id": "MDRW2100031876.1.13",
          "form": "남자시죠?",
          "original_form": "남자시죠?",
          "speaker_id": "2",
          "time": "20210905 07:03"
        },
        {
          "id": "MDRW2100031876.1.14",
          "form": "형님이시네요",
          "original_form": "형님이시네요",
          "speaker_id": "2",
          "time": "20210905 07:03"
        },
        {
          "id": "MDRW2100031876.1.15",
          "form": "저는 삼칠입니다",
          "original_form": "저는 삼칠입니다",
          "speaker_id": "2",
          "time": "20210905 07:04"
        },
        {
          "id": "MDRW2100031876.1.16",
          "form": "웬만하면 가지 마세요",
          "original_form": "웬만하면 가지 마세요",
          "speaker_id": "2",
          "time": "20210905 07:04"
        },
        {
          "id": "MDRW2100031876.1.17",
          "form": "아니요 저 여자예요",
          "original_form": "아니요 저 여자예요",
          "speaker_id": "1",
          "time": "20210905 07:04"
        },
        {
          "id": "MDRW2100031876.1.18",
          "form": "비추하는 이유를 알고 싶어요",
          "original_form": "비추하는 이유를 알고 싶어요",
          "speaker_id": "1",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100031876.1.19",
          "form": "부인되시는 분이 일 안 하세요?",
          "original_form": "부인되시는 분이 일 안 하세요?",
          "speaker_id": "1",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100031876.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100031876.1.21",
          "form": "내 지금은 안 해요",
          "original_form": "내 지금은 안 해요",
          "speaker_id": "2",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100031876.1.22",
          "form": "다시 시작할 가능성은 없나요?",
          "original_form": "다시 시작할 가능성은 없나요?",
          "speaker_id": "1",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100031876.1.23",
          "form": "결혼하셨나요",
          "original_form": "결혼하셨나요",
          "speaker_id": "2",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100031876.1.24",
          "form": "어 다시 시작할 겁니다",
          "original_form": "어 다시 시작할 겁니다",
          "speaker_id": "2",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100031876.1.25",
          "form": "아직 애기가 어려서",
          "original_form": "아직 애기가 어려서",
          "speaker_id": "2",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100031876.1.26",
          "form": "아니 안 했어요",
          "original_form": "아니 안 했어요",
          "speaker_id": "1",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100031876.1.27",
          "form": "재취업 가능한 직장인가 봐요",
          "original_form": "재취업 가능한 직장인가 봐요",
          "speaker_id": "1",
          "time": "20210905 07:08"
        },
        {
          "id": "MDRW2100031876.1.28",
          "form": "네 그렇습니다",
          "original_form": "네 그렇습니다",
          "speaker_id": "2",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100031876.1.29",
          "form": "여자는 결혼하면 손해라고 합니다",
          "original_form": "여자는 결혼하면 손해라고 합니다",
          "speaker_id": "2",
          "time": "20210905 07:09"
        },
        {
          "id": "MDRW2100031876.1.30",
          "form": "네 잘해봤자 본전 같아요",
          "original_form": "네 잘해봤자 본전 같아요",
          "speaker_id": "1",
          "time": "20210905 07:09"
        }
      ]
    }
  ]
