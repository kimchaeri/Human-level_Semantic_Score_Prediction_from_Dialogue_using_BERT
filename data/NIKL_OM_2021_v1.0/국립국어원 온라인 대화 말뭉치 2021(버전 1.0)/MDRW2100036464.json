      "utterance": [
        {
          "id": "MDRW2100036464.1.1",
          "form": "보리 음료 좋아하세요?",
          "original_form": "보리 음료 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036464.1.2",
          "form": "차가운 보리차는 좋죠",
          "original_form": "차가운 보리차는 좋죠",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036464.1.3",
          "form": "오.. 저는 보리가 들어가면 뭐둔 다 좋아요",
          "original_form": "오.. 저는 보리가 들어가면 뭐둔 다 좋아요",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036464.1.4",
          "form": "보리떡\r\n보리차\r\n보리술",
          "original_form": "보리떡\r\n보리차\r\n보리술",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036464.1.5",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036464.1.6",
          "form": "새싹보리",
          "original_form": "새싹보리",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036464.1.7",
          "form": "그건 싫네여",
          "original_form": "그건 싫네여",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036464.1.8",
          "form": "맛있엇는데.. 배가아파서 그만 .퓨",
          "original_form": "맛있엇는데.. 배가아파서 그만 .퓨",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036464.1.9",
          "form": "가루 싫어요..",
          "original_form": "가루 싫어요..",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036464.1.10",
          "form": "맛있긴 엄청 맛있었어요 완전 취향ㅠ",
          "original_form": "맛있긴 엄청 맛있었어요 완전 취향ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036464.1.11",
          "form": "네.. 앞으로도 열심히 드세여",
          "original_form": "네.. 앞으로도 열심히 드세여",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036464.1.12",
          "form": "집에없네요",
          "original_form": "집에없네요",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036464.1.13",
          "form": "왜죠?왜없죠?",
          "original_form": "왜죠?왜없죠?",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036464.1.14",
          "form": "배탈난다고 버렸어요",
          "original_form": "배탈난다고 버렸어요",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100036464.1.15",
          "form": "아..... 그럼 버려야죠..어쩌겟어요",
