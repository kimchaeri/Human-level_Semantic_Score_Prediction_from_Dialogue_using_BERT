      "utterance": [
        {
          "id": "MDRW2100006569.1.1",
          "form": "육아용품 준비 하셨나요",
          "original_form": "육아용품 준비 하셨나요",
          "speaker_id": "1",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100006569.1.2",
          "form": "아직 다 준비하지 못했어요ㅠㅠ",
          "original_form": "아직 다 준비하지 못했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100006569.1.3",
          "form": "준비할게 많죠 ~",
          "original_form": "준비할게 많죠 ~",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100006569.1.4",
          "form": "생각보다 더 많더라구요.... 비용도 생각보다 많이 들구요ㅠㅠ",
          "original_form": "생각보다 더 많더라구요.... 비용도 생각보다 많이 들구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100006569.1.5",
          "form": "인터넷  찾오보는데 사람들마다 다 말도 다르고 어떻게 준비하는게 맞는지 잘 모르겠네요ㅠㅠ",
          "original_form": "인터넷  찾오보는데 사람들마다 다 말도 다르고 어떻게 준비하는게 맞는지 잘 모르겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100006569.1.6",
          "form": "그렇죠 주로 어디서 사세요",
          "original_form": "그렇죠 주로 어디서 사세요",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006569.1.7",
          "form": "오픈마켓에서 주로 구매하고 있어요",
          "original_form": "오픈마켓에서 주로 구매하고 있어요",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100006569.1.8",
          "form": "인터넷니 저렴한것같아요 귀찮긴해도",
          "original_form": "인터넷니 저렴한것같아요 귀찮긴해도",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006569.1.9",
          "form": "맞아요. 잘 찾으면 저렴하게 살 수 있고 배송도 다 해주니까 편하기도 하구요",
          "original_form": "맞아요. 잘 찾으면 저렴하게 살 수 있고 배송도 다 해주니까 편하기도 하구요",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100006569.1.10",
          "form": "최근 사신건 뭐있나요",
          "original_form": "최근 사신건 뭐있나요",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100006569.1.11",
          "form": "기억나는.선물 있나요",
          "original_form": "기억나는.선물 있나요",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100006569.1.12",
          "form": "가장최근은 원피스가 기억에 남아요",
          "original_form": "가장최근은 원피스가 기억에 남아요",
          "speaker_id": "2",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100006569.1.13",
          "form": "원피스를.선물받으신거예요,?",
          "original_form": "원피스를.선물받으신거예요,?",
          "speaker_id": "1",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100006569.1.14",
          "form": "네. 임부원피스는 별로 사고 싶지 않았는데 어떻게 친구가",
          "original_form": "네. 임부원피스는 별로 사고 싶지 않았는데 어떻게 친구가",
          "speaker_id": "2",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100006569.1.15",
          "form": "아 그러셨군요 잘 입으시나요?",
          "original_form": "아 그러셨군요 잘 입으시나요?",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100006569.1.16",
          "form": "임부원피스가 아니지만 배나와도 입기편한 원피스를 선물로 줬거든요",
          "original_form": "임부원피스가 아니지만 배나와도 입기편한 원피스를 선물로 줬거든요",
          "speaker_id": "2",
          "time": "20210908 05:57"
        }
      ]
