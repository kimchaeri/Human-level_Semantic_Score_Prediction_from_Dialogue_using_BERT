      "utterance": [
        {
          "id": "MDRW2100006345.1.1",
          "form": "저녁은 뭐드셨나요",
          "original_form": "저녁은 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006345.1.2",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006345.1.3",
          "form": "ㅎ 김치볶음밥이요",
          "original_form": "ㅎ 김치볶음밥이요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006345.1.4",
          "form": "전 포케? 샐러드먹었는데 맛나더라구요",
          "original_form": "전 포케? 샐러드먹었는데 맛나더라구요",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006345.1.5",
          "form": "9800원 가격 후덜덜",
          "original_form": "9800원 가격 후덜덜",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006345.1.6",
          "form": "작은 딸램이 첨으로 해줬어요.",
          "original_form": "작은 딸램이 첨으로 해줬어요.",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006345.1.7",
          "form": "포케? 요건 몰까요~~~",
          "original_form": "포케? 요건 몰까요~~~",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.8",
          "form": "가격이 쎄네요",
          "original_form": "가격이 쎄네요",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.9",
          "form": "오ㅎ 직접요?",
          "original_form": "오ㅎ 직접요?",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.10",
          "form": "네.",
          "original_form": "네.",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.11",
          "form": "그냥 이름같아요 ㅎ",
          "original_form": "그냥 이름같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.12",
          "form": "새우랑 병아리콩",
          "original_form": "새우랑 병아리콩",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.13",
          "form": "햄이 손가락 한마디만해요. 고1인데 칼질 첨해볼꺼에요",
          "original_form": "햄이 손가락 한마디만해요. 고1인데 칼질 첨해볼꺼에요",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.14",
          "form": "오~~~  맛있겠어요",
          "original_form": "오~~~  맛있겠어요",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.15",
          "form": "렌틸콩 파인애플 등등 ㅎ 여러가지들어있어서 푸짐하긴해요",
          "original_form": "렌틸콩 파인애플 등등 ㅎ 여러가지들어있어서 푸짐하긴해요",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006345.1.16",
          "form": "새우좋은데",
          "original_form": "새우좋은데",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006345.1.17",
          "form": "샐러드 밥 반반할수도있고ㅎ",
          "original_form": "샐러드 밥 반반할수도있고ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006345.1.18",
          "form": "전요즘 아보카도. 토마토. 양상추넣고 해먹어요",
          "original_form": "전요즘 아보카도. 토마토. 양상추넣고 해먹어요",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006345.1.19",
          "form": "그래도 따님이 직접해줘서 뿌듯하셨을듯ㅎ 맛은어땠나요ㅎ",
          "original_form": "그래도 따님이 직접해줘서 뿌듯하셨을듯ㅎ 맛은어땠나요ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006345.1.20",
          "form": "아보카도 먹다보니 먹을만하더라구요.",
          "original_form": "아보카도 먹다보니 먹을만하더라구요.",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006345.1.21",
          "form": "아 맛이 느껴지나요?ㅎ",
          "original_form": "아 맛이 느껴지나요?ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100006345.1.22",
          "form": "맛은 쫌~~이긴한데 맛있다고 해줬지요^^",
          "original_form": "맛은 쫌~~이긴한데 맛있다고 해줬지요^^",
