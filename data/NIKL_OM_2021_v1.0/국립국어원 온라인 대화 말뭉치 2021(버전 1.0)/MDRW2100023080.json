      "utterance": [
        {
          "id": "MDRW2100023080.1.1",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023080.1.2",
          "form": "응 나는 요즘 와플이 그렇게 먹고 싶더라",
          "original_form": "응 나는 요즘 와플이 그렇게 먹고 싶더라",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023080.1.3",
          "form": "와플 맛있지",
          "original_form": "와플 맛있지",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023080.1.4",
          "form": "벨기에식 어플도 좋고 미국식 어플도 좋고 홍콩식 와플 도 진짜 맛있어",
          "original_form": "벨기에식 어플도 좋고 미국식 어플도 좋고 홍콩식 와플 도 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023080.1.5",
          "form": "그냥 와플 자체가 맛있음",
          "original_form": "그냥 와플 자체가 맛있음",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023080.1.6",
          "form": "근데 은근히 나라별 방식이 많이 달라",
          "original_form": "근데 은근히 나라별 방식이 많이 달라",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023080.1.7",
          "form": "그니까 그런거 같더라고",
          "original_form": "그니까 그런거 같더라고",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023080.1.8",
          "form": "근데 어쩜 그렇게 다 맛있을까 정말",
          "original_form": "근데 어쩜 그렇게 다 맛있을까 정말",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023080.1.9",
          "form": "계속 먹어도 안물려",
          "original_form": "계속 먹어도 안물려",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023080.1.10",
          "form": "계속 먹을려면 약간 담백한 와플을 먹으면 돼",
          "original_form": "계속 먹을려면 약간 담백한 와플을 먹으면 돼",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023080.1.11",
          "form": "근데 와플이 좀 달지 않나?",
          "original_form": "근데 와플이 좀 달지 않나?",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023080.1.12",
          "form": "달 긴 한데 그래도 막 크림인 아이스크림을 올려 먹지 않으면 좀 덜 날지",
          "original_form": "달 긴 한데 그래도 막 크림인 아이스크림을 올려 먹지 않으면 좀 덜 날지",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023080.1.13",
          "form": "갑자기 와플 먹고 싶다",
          "original_form": "갑자기 와플 먹고 싶다",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023080.1.14",
          "form": "난 며칠 전부터 계속 먹고 싶었어",
          "original_form": "난 며칠 전부터 계속 먹고 싶었어",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023080.1.15",
          "form": "먹으면 어떤 와플 먹고 싶어?",
          "original_form": "먹으면 어떤 와플 먹고 싶어?",
          "speaker_id": "1",
          "time": "20210829 21:35"
        }
      ]
