        },
        {
          "id": "MDRW2100026948.1.27",
          "form": "ㅜㅜㅜㅜㅜㅜㅜㅠ",
          "original_form": "ㅜㅜㅜㅜㅜㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026948.1.28",
          "form": "그래도 친구도 맛있댔어요",
          "original_form": "그래도 친구도 맛있댔어요",
          "speaker_id": "2",
          "time": "20210902 01:52"
        },
        {
