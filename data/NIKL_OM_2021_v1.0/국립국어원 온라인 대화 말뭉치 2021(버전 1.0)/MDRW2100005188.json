      "utterance": [
        {
          "id": "MDRW2100005188.1.1",
          "form": "다른 좋아하는 음식은 뭐 있으세여??",
          "original_form": "다른 좋아하는 음식은 뭐 있으세여??",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005188.1.2",
          "form": "전 골고루 잘 먹는 편이에요 ㅎㅎ",
          "original_form": "전 골고루 잘 먹는 편이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005188.1.3",
          "form": "요즘엔 메밀국수가 먹고싶어요",
          "original_form": "요즘엔 메밀국수가 먹고싶어요",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005188.1.4",
          "form": "오우 시원하고 맛있겠네요",
          "original_form": "오우 시원하고 맛있겠네요",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005188.1.5",
          "form": "날이 서늘해지긴 했는데도 먹고 싶네요 ㅎㅎ",
          "original_form": "날이 서늘해지긴 했는데도 먹고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005188.1.6",
          "form": "간짜장 좋아하시나요",
          "original_form": "간짜장 좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005188.1.7",
          "form": "네! 오늘도 먹었어요 ㅎㅎ",
          "original_form": "네! 오늘도 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005188.1.8",
          "form": "전 짜장에 들어간 양파가 좋아요!",
          "original_form": "전 짜장에 들어간 양파가 좋아요!",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005188.1.9",
          "form": "저도요!!!!",
          "original_form": "저도요!!!!",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005188.1.10",
          "form": "간짜장은 특히 양파가 큼직큼직해서 좋더라고요",
          "original_form": "간짜장은 특히 양파가 큼직큼직해서 좋더라고요",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005188.1.11",
          "form": "간짜장은 그맛으로 먹는 거 같아요 큼직한 양파 ㅎㅎ",
          "original_form": "간짜장은 그맛으로 먹는 거 같아요 큼직한 양파 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005188.1.12",
          "form": "맞아요ㅎㅎ 면 다 먹고도 양파 집어서 먹어요",
          "original_form": "맞아요ㅎㅎ 면 다 먹고도 양파 집어서 먹어요",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005188.1.13",
          "form": "간짜장 말고 또 어떤 거 좋아하세요??",
          "original_form": "간짜장 말고 또 어떤 거 좋아하세요??",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005188.1.14",
          "form": "저는 마라탕 좋아해요!",
          "original_form": "저는 마라탕 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005188.1.15",
          "form": "엇 저두요! 매운 걸 잘 못 먹어서 순하게 먹긴 하지만 ㅎㅎ",
          "original_form": "엇 저두요! 매운 걸 잘 못 먹어서 순하게 먹긴 하지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005188.1.16",
          "form": "저도 맵찔이에용 ㅎㅎ",
          "original_form": "저도 맵찔이에용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005188.1.17",
          "form": "전 마라에 면보다 야채랑 고기 넣는 게 더 좋아요 ㅎㅎ",
          "original_form": "전 마라에 면보다 야채랑 고기 넣는 게 더 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005188.1.18",
          "form": "오 어떤 야채 넣으세요?",
          "original_form": "오 어떤 야채 넣으세요?",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005188.1.19",
          "form": "청경채도 넣고 버섯도 넣고 두부랑 이름 모를 야채들...?",
          "original_form": "청경채도 넣고 버섯도 넣고 두부랑 이름 모를 야채들...?",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005188.1.20",
          "form": "옹 그렇군요",
          "original_form": "옹 그렇군요",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005188.1.21",
          "form": "건두부도 꽤 맛있는 것 같아요!",
          "original_form": "건두부도 꽤 맛있는 것 같아요!",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005188.1.22",
          "form": "맞아요 한국 요리에선 자주먹지 않는 재료인데 마라탕 먹을 땐 쇽쇽 넣어먹어요 ㅎㅎ",
          "original_form": "맞아요 한국 요리에선 자주먹지 않는 재료인데 마라탕 먹을 땐 쇽쇽 넣어먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005188.1.23",
          "form": "그렇죠 마라탕 재료들이 대개 그런 것 같아요",
          "original_form": "그렇죠 마라탕 재료들이 대개 그런 것 같아요",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005188.1.24",
          "form": "애초에 중식이라",
          "original_form": "애초에 중식이라",
          "speaker_id": "1",
          "time": "20210821 22:13"
        }
      ]
    }
  ]
