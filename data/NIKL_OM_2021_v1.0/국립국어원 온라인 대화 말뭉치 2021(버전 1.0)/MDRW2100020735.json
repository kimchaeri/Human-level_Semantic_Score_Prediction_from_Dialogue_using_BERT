        },
        {
          "id": "MDRW2100020735.1.23",
          "form": "name1님은 좀 정하셨나요",
          "original_form": "&name1&님은 좀 정하셨나요",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020735.1.24",
