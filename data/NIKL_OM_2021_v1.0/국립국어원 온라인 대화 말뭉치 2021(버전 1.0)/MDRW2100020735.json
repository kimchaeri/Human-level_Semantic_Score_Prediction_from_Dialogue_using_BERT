      "utterance": [
        {
          "id": "MDRW2100020735.1.1",
          "form": "슬슬점심인데",
          "original_form": "슬슬점심인데",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020735.1.2",
          "form": "이제 메뉴를 정하셨나요",
          "original_form": "이제 메뉴를 정하셨나요",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020735.1.3",
          "form": "앗 점심드시러 가셔야죠",
          "original_form": "앗 점심드시러 가셔야죠",
          "speaker_id": "2",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020735.1.4",
          "form": "아니요 8분 남았습니다",
          "original_form": "아니요 8분 남았습니다",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020735.1.5",
          "form": "아 저 좀 더 고민해볼까해요..",
          "original_form": "아 저 좀 더 고민해볼까해요..",
          "speaker_id": "2",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020735.1.6",
          "form": "급 비빔밥이 끌리네요",
          "original_form": "급 비빔밥이 끌리네요",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.7",
          "form": "아 비빔밥",
          "original_form": "아 비빔밥",
          "speaker_id": "1",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.8",
          "form": "뭐넣으세요",
          "original_form": "뭐넣으세요",
          "speaker_id": "1",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.9",
          "form": "확실한 시간이군요",
          "original_form": "확실한 시간이군요",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.10",
          "form": "일단 참기름을 넣고",
          "original_form": "일단 참기름을 넣고",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.11",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.12",
          "form": "참기름 필수",
          "original_form": "참기름 필수",
          "speaker_id": "1",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.13",
          "form": "또요",
          "original_form": "또요",
          "speaker_id": "1",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.14",
          "form": "집에있는 나물들을 넣어보려구요",
          "original_form": "집에있는 나물들을 넣어보려구요",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.15",
          "form": "고사리 넣으시나요",
          "original_form": "고사리 넣으시나요",
          "speaker_id": "1",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.16",
          "form": "아침보다 이성이 돌아왔어요",
          "original_form": "아침보다 이성이 돌아왔어요",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020735.1.17",
          "form": "다행입니다",
          "original_form": "다행입니다",
          "speaker_id": "1",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020735.1.18",
          "form": "고사리 본가가면 많은데 여긴 없게요",
          "original_form": "고사리 본가가면 많은데 여긴 없게요",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020735.1.19",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020735.1.20",
          "form": "아쉽..",
          "original_form": "아쉽..",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020735.1.21",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "1",
          "time": "20210828 11:54"
        },
        {
