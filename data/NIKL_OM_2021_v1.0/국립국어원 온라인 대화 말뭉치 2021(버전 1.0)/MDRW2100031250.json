      "utterance": [
        {
          "id": "MDRW2100031250.1.1",
          "form": "name1님 동네 날씨는 어떠세요?",
          "original_form": "&name1&님 동네 날씨는 어떠세요?",
          "speaker_id": "2",
          "time": "20210904 19:04"
        },
        {
          "id": "MDRW2100031250.1.2",
          "form": "저는 지금 여행중인데 날씨 좋았어요",
          "original_form": "저는 지금 여행중인데 날씨 좋았어요",
          "speaker_id": "1",
          "time": "20210904 19:07"
        },
        {
          "id": "MDRW2100031250.1.3",
          "form": "오! 어디세요?",
          "original_form": "오! 어디세요?",
          "speaker_id": "2",
          "time": "20210904 19:07"
        },
        {
          "id": "MDRW2100031250.1.4",
          "form": "용인이에요, 에버랜드랑 민속촌 들렀어요",
          "original_form": "용인이에요, 에버랜드랑 민속촌 들렀어요",
          "speaker_id": "1",
          "time": "20210904 19:07"
        },
        {
          "id": "MDRW2100031250.1.5",
          "form": "사람들 많아요?",
          "original_form": "사람들 많아요?",
          "speaker_id": "2",
          "time": "20210904 19:08"
        },
        {
          "id": "MDRW2100031250.1.6",
          "form": "오우, 많아요",
          "original_form": "오우, 많아요",
          "speaker_id": "1",
          "time": "20210904 19:08"
        },
        {
          "id": "MDRW2100031250.1.7",
          "form": "코로나 아닌 줄",
          "original_form": "코로나 아닌 줄",
          "speaker_id": "1",
          "time": "20210904 19:08"
        },
        {
          "id": "MDRW2100031250.1.8",
          "form": "ㅋㅋㅋㅋㅋ 다들 시간이 많이 지나다보니까 답답해서 나오게 되나봐요",
          "original_form": "ㅋㅋㅋㅋㅋ 다들 시간이 많이 지나다보니까 답답해서 나오게 되나봐요",
          "speaker_id": "2",
          "time": "20210904 19:08"
        },
        {
          "id": "MDRW2100031250.1.9",
          "form": "재밌으셨나요?",
          "original_form": "재밌으셨나요?",
          "speaker_id": "2",
          "time": "20210904 19:09"
        },
        {
          "id": "MDRW2100031250.1.10",
          "form": "네네, 발바닥 아플 만큼 돌아다녔어요",
          "original_form": "네네, 발바닥 아플 만큼 돌아다녔어요",
          "speaker_id": "1",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100031250.1.11",
          "form": "날씨가 좋아서",
          "original_form": "날씨가 좋아서",
          "speaker_id": "1",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100031250.1.12",
          "form": "다 좋았습니다ㅎㅎ",
          "original_form": "다 좋았습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100031250.1.13",
          "form": "아이고 오늘 푹 쉬셔야겠네요ㅋㅋㅋㅋ 어떤게 제일 좋으셨어요?",
          "original_form": "아이고 오늘 푹 쉬셔야겠네요ㅋㅋㅋㅋ 어떤게 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100031250.1.14",
          "form": "날이 좋아서 사진도 잘 찍히고",
          "original_form": "날이 좋아서 사진도 잘 찍히고",
          "speaker_id": "1",
          "time": "20210904 19:11"
        },
        {
          "id": "MDRW2100031250.1.15",
          "form": "물 맞는 놀이기구 타도 안 춥더라구요",
          "original_form": "물 맞는 놀이기구 타도 안 춥더라구요",
          "speaker_id": "1",
          "time": "20210904 19:11"
        },
        {
          "id": "MDRW2100031250.1.16",
          "form": "저 그 강물 같은데서 타는거 그거 기억나요",
          "original_form": "저 그 강물 같은데서 타는거 그거 기억나요",
          "speaker_id": "2",
          "time": "20210904 19:12"
        },
        {
          "id": "MDRW2100031250.1.17",
          "form": "그거 말씀하신거죠",
          "original_form": "그거 말씀하신거죠",
          "speaker_id": "2",
          "time": "20210904 19:12"
        },
        {
          "id": "MDRW2100031250.1.18",
          "form": "네네, 그런게 두 개 있더라구요",
          "original_form": "네네, 그런게 두 개 있더라구요",
          "speaker_id": "1",
          "time": "20210904 19:35"
        },
        {
          "id": "MDRW2100031250.1.19",
          "form": "날이 진짜 좋았나봐요 생각보다 물이 많이 튀었던걸로 기억하거든요ㅋㅋㅋㅋ",
          "original_form": "날이 진짜 좋았나봐요 생각보다 물이 많이 튀었던걸로 기억하거든요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100031250.1.20",
          "form": "하루에 에버랜드랑 민속촌 둘다 가신거에요?",
          "original_form": "하루에 에버랜드랑 민속촌 둘다 가신거에요?",
          "speaker_id": "2",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100031250.1.21",
          "form": "네네, 두 탕 뛰었습니다",
          "original_form": "네네, 두 탕 뛰었습니다",
          "speaker_id": "1",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100031250.1.22",
          "form": "비 안와서 다행이에요ㅎㅎ",
          "original_form": "비 안와서 다행이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100031250.1.23",
          "form": "가벼운 차림으로 다닐 수 있었어요",
          "original_form": "가벼운 차림으로 다닐 수 있었어요",
          "speaker_id": "1",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100031250.1.24",
          "form": "날이 좋아서 다행이었네요! 둘 다 야외라 더웠으면 금방 지쳤을지도 몰라요",
          "original_form": "날이 좋아서 다행이었네요! 둘 다 야외라 더웠으면 금방 지쳤을지도 몰라요",
          "speaker_id": "2",
          "time": "20210904 19:40"
        }
      ]
    }
