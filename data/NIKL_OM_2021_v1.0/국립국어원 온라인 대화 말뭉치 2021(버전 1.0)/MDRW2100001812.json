      "utterance": [
        {
          "id": "MDRW2100001812.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001812.1.2",
          "form": "넌 코로나 끝나면 가고싶은 여행지 있어?",
          "original_form": "넌 코로나 끝나면 가고싶은 여행지 있어?",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001812.1.3",
          "form": "난 스폐인 가보고 싶어요",
          "original_form": "난 스폐인 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001812.1.4",
          "form": "스페인?",
          "original_form": "스페인?",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001812.1.5",
          "form": "아직 안 봤는데 나도 가고싶어",
          "original_form": "아직 안 봤는데 나도 가고싶어",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001812.1.6",
          "form": "예전에 다른 나라에서 게스트 하우스에서",
          "original_form": "예전에 다른 나라에서 게스트 하우스에서",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001812.1.7",
          "form": "만난 애가 스페인 교환학생이었거든",
          "original_form": "만난 애가 스페인 교환학생이었거든",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001812.1.8",
          "form": "저두 안가봐서 \r\n프랑스갔을때 갔어야했는데 아쉬워요",
          "original_form": "저두 안가봐서 \r\n프랑스갔을때 갔어야했는데 아쉬워요",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001812.1.9",
          "form": "한여름 낮이 더워서 문 닫고 쉬는 시간 있대",
          "original_form": "한여름 낮이 더워서 문 닫고 쉬는 시간 있대",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001812.1.10",
          "form": "프랑스 갔어?",
          "original_form": "프랑스 갔어?",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001812.1.11",
          "form": "난 유럽은 동유럽만 가봤거든",
          "original_form": "난 유럽은 동유럽만 가봤거든",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001812.1.12",
          "form": "한달가량 근데 파업하는 바람에 차질이 생겨서 돈도 날리고",
          "original_form": "한달가량 근데 파업하는 바람에 차질이 생겨서 돈도 날리고",
          "speaker_id": "2",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001812.1.13",
          "form": "대부분 스페인도 많이 가더라",
          "original_form": "대부분 스페인도 많이 가더라",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001812.1.14",
          "form": "볼것도 많고",
          "original_form": "볼것도 많고",
          "speaker_id": "2",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001812.1.15",
          "form": "하긴 유럽애들이 파업 많이 해",
          "original_form": "하긴 유럽애들이 파업 많이 해",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001812.1.16",
          "form": "전에 꽃보다 할배 프로그램에 스페인 봤는데 매력 넘치더라",
          "original_form": "전에 꽃보다 할배 프로그램에 스페인 봤는데 매력 넘치더라",
          "speaker_id": "1",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001812.1.17",
          "form": "맞아요 티비 영향인듯해요",
          "original_form": "맞아요 티비 영향인듯해요",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001812.1.18",
          "form": "스페인 관련 되게 많았어",
          "original_form": "스페인 관련 되게 많았어",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001812.1.19",
          "form": "프랑스 갔었으면 어땠어?",
          "original_form": "프랑스 갔었으면 어땠어?",
          "speaker_id": "1",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001812.1.20",
          "form": "생각보다 지저분하는 느낌",
          "original_form": "생각보다 지저분하는 느낌",
          "speaker_id": "2",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001812.1.21",
          "form": "지하철 더럽지 않아?",
          "original_form": "지하철 더럽지 않아?",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001812.1.22",
          "form": "난 체코랑 헝가리 갔는데",
          "original_form": "난 체코랑 헝가리 갔는데",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001812.1.23",
          "form": "지하철 무서웠어",
          "original_form": "지하철 무서웠어",
          "speaker_id": "1",
          "time": "20210819 21:32"
        }
      ]
