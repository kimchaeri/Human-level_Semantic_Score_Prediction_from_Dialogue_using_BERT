      "utterance": [
        {
          "id": "MDRW2100026827.1.1",
          "form": "점심 뭐드시나용~?",
          "original_form": "점심 뭐드시나용~?",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100026827.1.2",
          "form": "점심은 한솥 먹었어요 치킨마요",
          "original_form": "점심은 한솥 먹었어요 치킨마요",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100026827.1.3",
          "form": "name2은 뭐 드셨나요?",
          "original_form": "&name2&은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100026827.1.4",
          "form": "와 그거 진짜맛임ㅅ는데!!!!!!!!! 한솥먹을때 그것만 먹어요",
          "original_form": "와 그거 진짜맛임ㅅ는데!!!!!!!!! 한솥먹을때 그것만 먹어요",
          "speaker_id": "2",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100026827.1.5",
          "form": "저는 찜닭이랑 콩나물국이요~!",
          "original_form": "저는 찜닭이랑 콩나물국이요~!",
          "speaker_id": "2",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100026827.1.6",
          "form": "맛있게 드셨네요!",
          "original_form": "맛있게 드셨네요!",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100026827.1.7",
          "form": "저녁은 뭐 드셨어요??",
          "original_form": "저녁은 뭐 드셨어요??",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100026827.1.8",
          "form": "저녁은 밥먹고 만두먹었어요",
          "original_form": "저녁은 밥먹고 만두먹었어요",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100026827.1.9",
          "form": "올만에 만두먹으니깐 너무 맛있네요",
          "original_form": "올만에 만두먹으니깐 너무 맛있네요",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100026827.1.10",
          "form": "오 만두 맛있겠어요 무슨 만두 드셨어요? 냉동 아님 근처에서 사드셨나여",
          "original_form": "오 만두 맛있겠어요 무슨 만두 드셨어요? 냉동 아님 근처에서 사드셨나여",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100026827.1.11",
          "form": "저는 김치만두파에요 ㅋㅋ",
          "original_form": "저는 김치만두파에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100026827.1.12",
          "form": "저 비비고 만두요!",
          "original_form": "저 비비고 만두요!",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100026827.1.13",
          "form": "김치만두~ 저도 좋아해요",
          "original_form": "김치만두~ 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100026827.1.14",
          "form": "새우만두도 맛있는거같아여",
          "original_form": "새우만두도 맛있는거같아여",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100026827.1.15",
          "form": "비비고 맛있져 새우도 진짜 맛있고 ㅎㅎ",
          "original_form": "비비고 맛있져 새우도 진짜 맛있고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100026827.1.16",
          "form": "풀무원 얇은피만두도 맛있어요",
          "original_form": "풀무원 얇은피만두도 맛있어요",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100026827.1.17",
          "form": "저는 그건 깻잎만두 좋아해요",
          "original_form": "저는 그건 깻잎만두 좋아해요",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100026827.1.18",
          "form": "오 풀무원은 못먹어봤어요",
          "original_form": "오 풀무원은 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100026827.1.19",
          "form": "담에 풀무원꺼도 먹어봐야겠어요",
          "original_form": "담에 풀무원꺼도 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100026827.1.20",
          "form": "그거 김치는 조금 짜서 고기나 깻잎 그런 종류가 더 낫더라고요",
          "original_form": "그거 김치는 조금 짜서 고기나 깻잎 그런 종류가 더 낫더라고요",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100026827.1.21",
          "form": "나중에 한번 드셔보세요 저도 비비고 질릴때 가끔 그거 사먹어요 ㅎㅎ",
          "original_form": "나중에 한번 드셔보세요 저도 비비고 질릴때 가끔 그거 사먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100026827.1.22",
          "form": "오 이런 좋은 정보를 주시다니 감사해요 ㅎ",
          "original_form": "오 이런 좋은 정보를 주시다니 감사해요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100026827.1.23",
          "form": "요거 다 먹으면 주문해볼게요~",
          "original_form": "요거 다 먹으면 주문해볼게요~",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100026827.1.24",
          "form": "아 근데 지금 뭔가 또 땡기네요 많이 먹었는데",
          "original_form": "아 근데 지금 뭔가 또 땡기네요 많이 먹었는데",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100026827.1.25",
          "form": "저두요 지금 다른거 해먹으려고요",
          "original_form": "저두요 지금 다른거 해먹으려고요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100026827.1.26",
          "form": "그 그릭요거트복숭아 박나래화사가 해먹은거 아세요?",
          "original_form": "그 그릭요거트복숭아 박나래화사가 해먹은거 아세요?",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100026827.1.27",
          "form": "집에 복숭아 있어서 해먹었더니 맛있더라고요",
          "original_form": "집에 복숭아 있어서 해먹었더니 맛있더라고요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100026827.1.28",
          "form": "오 그거 대충 보기만했는데 하기쉽나요?",
          "original_form": "오 그거 대충 보기만했는데 하기쉽나요?",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100026827.1.29",
          "form": "집에 그릭요거트는 있는뎅",
          "original_form": "집에 그릭요거트는 있는뎅",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100026827.1.30",
          "form": "네 저는 모양 안내고 먹었거든요 그래놀라나 씨리얼 밑에 깔아놓고 복숭아 대충 잘라서 놓은 후에 그릭요거트만 얹어먹으면 돼요",
          "original_form": "네 저는 모양 안내고 먹었거든요 그래놀라나 씨리얼 밑에 깔아놓고 복숭아 대충 잘라서 놓은 후에 그릭요거트만 얹어먹으면 돼요",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100026827.1.31",
          "form": "꿀 있으면 넣으시고 근데 복숭아가 달아서 안넣어도 맛있더라고요",
          "original_form": "꿀 있으면 넣으시고 근데 복숭아가 달아서 안넣어도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100026827.1.32",
          "form": "오 낼 당장 먹어보께요!!!!!!",
          "original_form": "오 낼 당장 먹어보께요!!!!!!",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100026827.1.33",
          "form": "지금 당 땡겨서 급 밤만쥬 먹고있다죠",
          "original_form": "지금 당 땡겨서 급 밤만쥬 먹고있다죠",
