      "utterance": [
        {
          "id": "MDRW2100021876.1.1",
          "form": "게임좋아하세요?",
          "original_form": "게임좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021876.1.2",
          "form": "네 몇가지정도만 소소하게 좋아합니다!",
          "original_form": "네 몇가지정도만 소소하게 좋아합니다!",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021876.1.3",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021876.1.4",
          "form": "저 롤이요!",
          "original_form": "저 롤이요!",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021876.1.5",
          "form": "지금도 롤중이에요 ㅎㄹ",
          "original_form": "지금도 롤중이에요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100021876.1.6",
          "form": "오 롤..!!",
          "original_form": "오 롤..!!",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021876.1.7",
          "form": "전 롤 직접 플레이하지는 않지만 어떻게 하다보니 재미붙어서  lck 챙겨보고있어요!",
          "original_form": "전 롤 직접 플레이하지는 않지만 어떻게 하다보니 재미붙어서  lck 챙겨보고있어요!",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021876.1.8",
          "form": "나중에는 언제 함 해보려고 하는데 어렵지않나요?",
          "original_form": "나중에는 언제 함 해보려고 하는데 어렵지않나요?",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021876.1.9",
          "form": "오오!!오늘 lck결승보셨나요?",
          "original_form": "오오!!오늘 lck결승보셨나요?",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021876.1.10",
          "form": "어려유ㅓ요ㅠㅠ",
          "original_form": "어려유ㅓ요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021876.1.11",
          "form": "네네 아까 봤어요!!ㅎㅎ",
          "original_form": "네네 아까 봤어요!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:44"
        },
        {
          "id": "MDRW2100021876.1.12",
          "form": "아앟 그럼 저는 거의 봇전만 하게 되겠네요ㅎㅎ",
          "original_form": "아앟 그럼 저는 거의 봇전만 하게 되겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:45"
        },
        {
          "id": "MDRW2100021876.1.13",
          "form": "그 외 다른 게임 하시는 거 있으신가요??",
          "original_form": "그 외 다른 게임 하시는 거 있으신가요??",
          "speaker_id": "2",
          "time": "20210829 00:45"
        },
        {
          "id": "MDRW2100021876.1.14",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100021876.1.15",
          "form": "저는 티원응원해서 눈물났습니다ㅠㅠ",
          "original_form": "저는 티원응원해서 눈물났습니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100021876.1.16",
          "form": "오버워치좋아했었어요!",
          "original_form": "오버워치좋아했었어요!",
          "speaker_id": "1",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100021876.1.17",
          "form": "아아ㅠ 저도 티원 팬이어서 응원열심히 했는데 아쉽..ㅠ 그래도 롤드컵 가니까 다행입니다...!!ㅠ",
          "original_form": "아아ㅠ 저도 티원 팬이어서 응원열심히 했는데 아쉽..ㅠ 그래도 롤드컵 가니까 다행입니다...!!ㅠ",
          "speaker_id": "2",
          "time": "20210829 10:16"
        },
        {
          "id": "MDRW2100021876.1.18",
          "form": "오 오버워치도 멋있죠! 전 한판하고 어려워서 못했지만ㅎㅎ",
          "original_form": "오 오버워치도 멋있죠! 전 한판하고 어려워서 못했지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 10:17"
        },
        {
          "id": "MDRW2100021876.1.19",
          "form": "티원 누구제일죻아하세요?",
          "original_form": "티원 누구제일죻아하세요?",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100021876.1.20",
          "form": "음...!! 굳이 고르자면 페이커요!!",
          "original_form": "음...!! 굳이 고르자면 페이커요!!",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100021876.1.21",
          "form": "다 좋아하긴합니다ㅎㅎ",
          "original_form": "다 좋아하긴합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100021876.1.22",
          "form": "티원에 어떤 선수 제일 좋아하시나요?",
          "original_form": "티원에 어떤 선수 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100021876.1.23",
          "form": "케리아랑 커즈요 ㅎㅎㅎㅎ",
          "original_form": "케리아랑 커즈요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100021876.1.24",
          "form": "두번째는요?",
          "original_form": "두번째는요?",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100021876.1.25",
          "form": "음..!! 맨 처음 접했던 경기에서 칸나가 활약하는 거 보고 재미붙이기 시작해서 칸나선수...??ㅎㅎ",
          "original_form": "음..!! 맨 처음 접했던 경기에서 칸나가 활약하는 거 보고 재미붙이기 시작해서 칸나선수...??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100021876.1.26",
          "form": "롤드컵 얼른했으면 좋겠네요ㅋㅋ",
          "original_form": "롤드컵 얼른했으면 좋겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100021876.1.27",
          "form": "오오!!",
          "original_form": "오오!!",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100021876.1.28",
          "form": "칸나 그웬도 잘하죠 ㅎㅎ",
          "original_form": "칸나 그웬도 잘하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100021876.1.29",
          "form": "카밀진짜 저는 보고 놀랬어요 첨에",
          "original_form": "카밀진짜 저는 보고 놀랬어요 첨에",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100021876.1.30",
          "form": "내일5신가도 있잖아요 !",
          "original_form": "내일5신가도 있잖아요 !",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100021876.1.31",
          "form": "아 맞아요..!! 헉 롤드컵 나간다는 거에 정신 팔려가지고 까먹고 있었어요..!!",
          "original_form": "아 맞아요..!! 헉 롤드컵 나간다는 거에 정신 팔려가지고 까먹고 있었어요..!!",
          "speaker_id": "2",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100021876.1.32",
          "form": "챙겨봐야겠네요ㅎㅎ",
          "original_form": "챙겨봐야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
