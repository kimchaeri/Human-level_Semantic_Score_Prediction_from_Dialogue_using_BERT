      "utterance": [
        {
          "id": "MDRW2100037888.1.1",
          "form": "name2님 게임 하시나요?!",
          "original_form": "&name2&님 게임 하시나요?!",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037888.1.2",
          "form": "저는 쿠킹덤해요 ㅎㅎ name1님은요??",
          "original_form": "저는 쿠킹덤해요 ㅎㅎ &name1&님은요??",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100037888.1.3",
          "form": "헐헐 저도요!!!!!!",
          "original_form": "헐헐 저도요!!!!!!",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037888.1.4",
          "form": "저 며칠전에 10-31 깨서",
          "original_form": "저 며칠전에 10-31 깨서",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037888.1.5",
          "form": "완전 행복하거든요!",
          "original_form": "완전 행복하거든요!",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037888.1.6",
          "form": "오오 반갑습니다 ㅎㅎ",
          "original_form": "오오 반갑습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037888.1.7",
          "form": "ㅋㅋㅋㅋㅋ 저도 그거 깨느라 죽을뻔했어여..",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 그거 깨느라 죽을뻔했어여..",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037888.1.8",
          "form": "아레나도 열심히 하세요!?",
          "original_form": "아레나도 열심히 하세요!?",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037888.1.9",
          "form": "아레나 열심히할때도 있고 아예 안하는날도 있어요 ㅎㅎ",
          "original_form": "아레나 열심히할때도 있고 아예 안하는날도 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037888.1.10",
          "form": "요즘 소다제도 협력전땜에 선베드에 맨날 누워있고 그래여 ㅎㅎ",
          "original_form": "요즘 소다제도 협력전땜에 선베드에 맨날 누워있고 그래여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037888.1.11",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037888.1.12",
          "form": "선베드 너무 올래 걸려서 짜증나지 않나요",
          "original_form": "선베드 너무 올래 걸려서 짜증나지 않나요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037888.1.13",
          "form": "맞아요 ㅠㅠ 다섯명밖에 못올리고..ㅠ",
          "original_form": "맞아요 ㅠㅠ 다섯명밖에 못올리고..ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037888.1.14",
          "form": "오늘은 케이크도 리셋되가지고 할게 많네여....ㅠㅠㅋ",
          "original_form": "오늘은 케이크도 리셋되가지고 할게 많네여....ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037888.1.15",
          "form": "솔직히 ㅠ 10분안에 말릴 수 있으면 좋겠어요 ㅠ.ㅠ",
          "original_form": "솔직히 ㅠ 10분안에 말릴 수 있으면 좋겠어요 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037888.1.16",
          "form": "마자여 ㅠㅠ 3시간 넘 길어요",
          "original_form": "마자여 ㅠㅠ 3시간 넘 길어요",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037888.1.17",
          "form": "그리고 그 문어?",
          "original_form": "그리고 그 문어?",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037888.1.18",
          "form": "왤케 점점 쎄지나요 ㅠ",
          "original_form": "왤케 점점 쎄지나요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100037888.1.19",
          "form": "예전에는 한 방이면 됐는데 ㅠㅠ",
          "original_form": "예전에는 한 방이면 됐는데 ㅠㅠ",
