      "utterance": [
        {
          "id": "MDRW2100013299.1.1",
          "form": "오늘 신입왔어요",
          "original_form": "오늘 신입왔어요",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013299.1.2",
          "form": "신입이 속 안썩이던가요",
          "original_form": "신입이 속 안썩이던가요",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013299.1.3",
          "form": "생각보다 일을 잘하더라구요",
          "original_form": "생각보다 일을 잘하더라구요",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013299.1.4",
          "form": "아진짜요? 전 신입 연수 진짜 안좋아해요",
          "original_form": "아진짜요? 전 신입 연수 진짜 안좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013299.1.5",
          "form": "저도 연수는 힘든데 해야죠",
          "original_form": "저도 연수는 힘든데 해야죠",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013299.1.6",
          "form": "신입은 잘 이해하던가요?",
          "original_form": "신입은 잘 이해하던가요?",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013299.1.7",
          "form": "네ㅋㅋ 의욕이 엄청나요ㅋㄱㄱ",
          "original_form": "네ㅋㅋ 의욕이 엄청나요ㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013299.1.8",
          "form": "열정신입인가보네요ㅋㅋ",
          "original_form": "열정신입인가보네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013299.1.9",
          "form": "네 그래서 윗분들도 좋아하시더라구요",
          "original_form": "네 그래서 윗분들도 좋아하시더라구요",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013299.1.10",
          "form": "제가 신입일 때가 생각나네요",
          "original_form": "제가 신입일 때가 생각나네요",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013299.1.11",
          "form": "어떠셨어요 신입때?",
          "original_form": "어떠셨어요 신입때?",
          "speaker_id": "1",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013299.1.12",
          "form": "저도 실수 엄청하고 긴장도 많이했어요",
          "original_form": "저도 실수 엄청하고 긴장도 많이했어요",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100013299.1.13",
          "form": "저도 그랬던 거같네요",
          "original_form": "저도 그랬던 거같네요",
