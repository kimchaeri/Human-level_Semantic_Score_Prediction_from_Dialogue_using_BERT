      "utterance": [
        {
          "id": "MDRW2100026888.1.1",
          "form": "다음카페 하시나요",
          "original_form": "다음카페 하시나요",
          "speaker_id": "1",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026888.1.2",
          "form": "다음카페 합니다",
          "original_form": "다음카페 합니다",
          "speaker_id": "2",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026888.1.3",
          "form": "네이버도 합니다",
          "original_form": "네이버도 합니다",
          "speaker_id": "2",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026888.1.4",
          "form": "그러고보니 온라인카페에서 친구를 사귄적 있습니다",
          "original_form": "그러고보니 온라인카페에서 친구를 사귄적 있습니다",
          "speaker_id": "2",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026888.1.5",
          "form": "아 정말이요?",
          "original_form": "아 정말이요?",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026888.1.6",
          "form": "어떻1ㅔ 만나게되셨나요",
          "original_form": "어떻1ㅔ 만나게되셨나요",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026888.1.7",
          "form": "관심사가 같았나요?",
          "original_form": "관심사가 같았나요?",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026888.1.8",
          "form": "친구구함 글올려서 만나서 7년째  친구입니다",
          "original_form": "친구구함 글올려서 만나서 7년째  친구입니다",
          "speaker_id": "2",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026888.1.9",
          "form": "동갑친구가 필요했거든요",
          "original_form": "동갑친구가 필요했거든요",
          "speaker_id": "2",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026888.1.10",
          "form": "친구를 구하는 글을 올리기도 하는군요",
          "original_form": "친구를 구하는 글을 올리기도 하는군요",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026888.1.11",
          "form": "신기한 경험이에요",
          "original_form": "신기한 경험이에요",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026888.1.12",
          "form": "낯선 지역에서 온라인 커뮤니티로 사람을 만날수있어요",
          "original_form": "낯선 지역에서 온라인 커뮤니티로 사람을 만날수있어요",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026888.1.13",
          "form": "오래 만나셨네요",
          "original_form": "오래 만나셨네요",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026888.1.14",
          "form": "네. 용기있는자가 벗을 얻는다",
          "original_form": "네. 용기있는자가 벗을 얻는다",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026888.1.15",
          "form": "맞아요 커뮤니티의 장점같아요",
          "original_form": "맞아요 커뮤니티의 장점같아요",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026888.1.16",
          "form": "친구도 만날수 있고",
          "original_form": "친구도 만날수 있고",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026888.1.17",
          "form": "온라인커뮤니티에서 트랜드도 볼 수있구요",
          "original_form": "온라인커뮤니티에서 트랜드도 볼 수있구요",
          "speaker_id": "2",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026888.1.18",
          "form": "정보도 공유하고",
          "original_form": "정보도 공유하고",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100026888.1.19",
          "form": "시사도 알수있구요",
          "original_form": "시사도 알수있구요",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100026888.1.20",
          "form": "단점보단 장점이 많죠",
          "original_form": "단점보단 장점이 많죠",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100026888.1.21",
          "form": "등업은 힘듭니다",
          "original_form": "등업은 힘듭니다",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100026888.1.22",
          "form": "커뮤니티 정말 좋네요",
          "original_form": "커뮤니티 정말 좋네요",
