      "utterance": [
        {
          "id": "MDRW2100027875.1.1",
          "form": "이슬람얘기 계속 할줄 ㅋㅋㅋㅋ",
          "original_form": "이슬람얘기 계속 할줄 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027875.1.2",
          "form": "요즘 이런거 되게 유행하더라",
          "original_form": "요즘 이런거 되게 유행하더라",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027875.1.3",
          "form": "곰표가 원존가",
          "original_form": "곰표가 원존가",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027875.1.4",
          "form": "맥주랑 팝콘?",
          "original_form": "맥주랑 팝콘?",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027875.1.5",
          "form": "어... 그럴듯 난 말표만 마셔봄",
          "original_form": "어... 그럴듯 난 말표만 마셔봄",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.6",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.7",
          "form": "맛소금 팝콘... 생각나네",
          "original_form": "맛소금 팝콘... 생각나네",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.8",
          "form": "나는 술 별로 안마시니까ㅋㅋㅋㅋ",
          "original_form": "나는 술 별로 안마시니까ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.9",
          "form": "말표가 원래 무슨 회사지",
          "original_form": "말표가 원래 무슨 회사지",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.10",
          "form": "구두?",
          "original_form": "구두?",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.11",
          "form": "시멘트?",
          "original_form": "시멘트?",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.12",
          "form": "근데 곰표가 제일 맛있다며",
          "original_form": "근데 곰표가 제일 맛있다며",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.13",
          "form": "구두약인듯",
          "original_form": "구두약인듯",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.14",
          "form": "구두약맛 맥주",
          "original_form": "구두약맛 맥주",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027875.1.15",
          "form": "나도 곰표 팝콘 먹어볼까하다가 늘 멈춰",
          "original_form": "나도 곰표 팝콘 먹어볼까하다가 늘 멈춰",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027875.1.16",
          "form": "예전에 친구가 일본 자가리코 과자 가방 갖고싶대서",
          "original_form": "예전에 친구가 일본 자가리코 과자 가방 갖고싶대서",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027875.1.17",
          "form": "옥션 엄청 돌아다녔는뎈ㅋㅋㅋ",
          "original_form": "옥션 엄청 돌아다녔는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027875.1.18",
          "form": "과자가방?",
          "original_form": "과자가방?",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027875.1.19",
          "form": "이런게 한국에도 넘어오는구먼",
          "original_form": "이런게 한국에도 넘어오는구먼",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027875.1.20",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027875.1.21",
          "form": "사람 만한 가방",
          "original_form": "사람 만한 가방",
          "speaker_id": "1",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027875.1.22",
          "form": "그래서 내 머릿속에",
          "original_form": "그래서 내 머릿속에",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.23",
          "form": "자가리코라는",
          "original_form": "자가리코라는",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.24",
          "form": "단어가",
          "original_form": "단어가",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.25",
          "form": "머리에",
          "original_form": "머리에",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.26",
          "form": "박",
          "original_form": "박",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.27",
          "form": "힘",
          "original_form": "힘",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.28",
          "form": "힘",
          "original_form": "힘",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.29",
          "form": "나쁜말이야 조심히써",
          "original_form": "나쁜말이야 조심히써",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.30",
          "form": "쟈가라코 맛있어",
          "original_form": "쟈가라코 맛있어",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.31",
          "form": "나쁜말이구나 박...",
          "original_form": "나쁜말이구나 박...",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.32",
          "form": "ㅎ..ㅣ...ㅁ",
          "original_form": "ㅎ..ㅣ...ㅁ",
          "speaker_id": "2",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.33",
          "form": "그치 ",
          "original_form": "그치 ",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027875.1.34",
          "form": "그 사실 곰젤리 비누나",
          "original_form": "그 사실 곰젤리 비누나",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027875.1.35",
          "form": "직사각형인데 위에 딸기잼 사과잼 발린 과자 형태의 비누도",
          "original_form": "직사각형인데 위에 딸기잼 사과잼 발린 과자 형태의 비누도",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027875.1.36",
          "form": "이런 일환 아닌가",
          "original_form": "이런 일환 아닌가",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027875.1.37",
          "form": "근데 이제 이런거 못하게 한다매",
          "original_form": "근데 이제 이런거 못하게 한다매",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027875.1.38",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100027875.1.39",
          "form": "애기들이 먹어서?",
          "original_form": "애기들이 먹어서?",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027875.1.40",
          "form": "애기 뿐만 아니라 소비자가 헷갈려서",
          "original_form": "애기 뿐만 아니라 소비자가 헷갈려서",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027875.1.41",
          "form": "막 배치를 음식 옆에 두고 하니까",
          "original_form": "막 배치를 음식 옆에 두고 하니까",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027875.1.42",
          "form": "오해하게 되나봐",
          "original_form": "오해하게 되나봐",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027875.1.43",
          "form": "젊은 사람이야 괜찮은데",
          "original_form": "젊은 사람이야 괜찮은데",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027875.1.44",
          "form": "중장년층만 가도",
          "original_form": "중장년층만 가도",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027875.1.45",
          "form": "난리니까",
          "original_form": "난리니까",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027875.1.46",
          "form": "재미있긴 한데 먹는 건 막긴해야할듯",
          "original_form": "재미있긴 한데 먹는 건 막긴해야할듯",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027875.1.47",
          "form": "구두약초코인줄알고 퍼먹고 이럼 클나지",
          "original_form": "구두약초코인줄알고 퍼먹고 이럼 클나지",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027875.1.48",
          "form": "아맞아 그렇다는 거 같더라ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아맞아 그렇다는 거 같더라ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027875.1.49",
          "form": "먹는 거 말고 다른 거로 콜라보하면 괜찮을거같은데",
          "original_form": "먹는 거 말고 다른 거로 콜라보하면 괜찮을거같은데",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027875.1.50",
          "form": "그치 진로소주 가방 이런거 엄청귀엽던데",
          "original_form": "그치 진로소주 가방 이런거 엄청귀엽던데",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027875.1.51",
          "form": "아 불닭치약ㅋㅋㅋㅋㅋ 이런거 때문에 에엣... 된다",
          "original_form": "아 불닭치약ㅋㅋㅋㅋㅋ 이런거 때문에 에엣... 된다",
