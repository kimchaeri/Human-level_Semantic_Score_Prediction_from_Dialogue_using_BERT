      "utterance": [
        {
          "id": "MDRW2100042234.1.1",
          "form": "저희 동네는 비가 많이 와요",
          "original_form": "저희 동네는 비가 많이 와요",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042234.1.2",
          "form": "조금씩오다가지금은 멈춘거같아요",
          "original_form": "조금씩오다가지금은 멈춘거같아요",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042234.1.3",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042234.1.4",
          "form": "지역이어디셔요?",
          "original_form": "지역이어디셔요?",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042234.1.5",
          "form": "저흰 점점 빗줄기가 굵어져요ㅜ",
          "original_form": "저흰 점점 빗줄기가 굵어져요ㅜ",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042234.1.6",
          "form": "경기서부요",
          "original_form": "경기서부요",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042234.1.7",
          "form": "여기는 인천이에요",
          "original_form": "여기는 인천이에요",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042234.1.8",
          "form": "비슷한데날씨가다르네요;;;;",
          "original_form": "비슷한데날씨가다르네요;;;;",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042234.1.9",
          "form": "옷 그럼 멀지 않을텐데",
          "original_form": "옷 그럼 멀지 않을텐데",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042234.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042234.1.11",
          "form": "근데 옆동네여도 날씨가 다르기도 하더라고요",
          "original_form": "근데 옆동네여도 날씨가 다르기도 하더라고요",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042234.1.12",
          "form": "맞아요 인천안에서도 날씨가다르더라구요",
          "original_form": "맞아요 인천안에서도 날씨가다르더라구요",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042234.1.13",
          "form": "네너 맞아요",
          "original_form": "네너 맞아요",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042234.1.14",
          "form": "저희도 이젠 비가 좀 잦아드는 것 같아요",
          "original_form": "저희도 이젠 비가 좀 잦아드는 것 같아요",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042234.1.15",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042234.1.16",
          "form": "네 내일 아침엔 그쳤으면 좋겠어요",
          "original_form": "네 내일 아침엔 그쳤으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042234.1.17",
          "form": "아마도그치지않을까요",
          "original_form": "아마도그치지않을까요",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042234.1.18",
          "form": "비가넘자주와요ㅠ",
          "original_form": "비가넘자주와요ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042234.1.19",
          "form": "맞아요 요즘  비가 자주 왔죠ㅜ",
          "original_form": "맞아요 요즘  비가 자주 왔죠ㅜ",
