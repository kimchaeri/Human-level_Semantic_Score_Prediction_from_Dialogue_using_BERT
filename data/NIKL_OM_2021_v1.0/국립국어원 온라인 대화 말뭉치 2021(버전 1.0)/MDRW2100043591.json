      "utterance": [
        {
          "id": "MDRW2100043591.1.1",
          "form": "맞아요 신라면처럼 매운맛 기준이 됐죠 ㅋㅋ",
          "original_form": "맞아요 신라면처럼 매운맛 기준이 됐죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043591.1.2",
          "form": "이제 무뎌져서",
          "original_form": "이제 무뎌져서",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043591.1.3",
          "form": "불닭도 막 맵진않구",
          "original_form": "불닭도 막 맵진않구",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043591.1.4",
          "form": "불닭볶음면 처음나왔을때 너무 매웠었는데....",
          "original_form": "불닭볶음면 처음나왔을때 너무 매웠었는데....",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043591.1.5",
          "form": "그쵸 ㅋㅋㅋㅋ 불닭 첨에는 진짜 배웠었는데 지금은 딱이죠",
          "original_form": "그쵸 ㅋㅋㅋㅋ 불닭 첨에는 진짜 배웠었는데 지금은 딱이죠",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043591.1.6",
          "form": "(위가 고통받는소리)",
          "original_form": "(위가 고통받는소리)",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043591.1.7",
          "form": "주인이 미안해~!",
          "original_form": "주인이 미안해~!",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043591.1.8",
          "form": "으음 .. 맞아요ㅋㅋㅋㅋ 가끔 넘 매운거 먹으면 꾸르륵 ..하더라구요",
          "original_form": "으음 .. 맞아요ㅋㅋㅋㅋ 가끔 넘 매운거 먹으면 꾸르륵 ..하더라구요",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043591.1.9",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043591.1.10",
          "form": "어느 순간 쓰린 느낌을 맞이하고",
          "original_form": "어느 순간 쓰린 느낌을 맞이하고",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043591.1.11",
          "form": "배가아파지기 시작하며...",
          "original_form": "배가아파지기 시작하며...",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043591.1.12",
          "form": "후회하지만 .. 또 매운걸 먹게되는..",
          "original_form": "후회하지만 .. 또 매운걸 먹게되는..",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043591.1.13",
          "form": "약간 반복입니다 ㅋㅋㅋ",
          "original_form": "약간 반복입니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043591.1.14",
          "form": "맞아요ㅋㅋㅋ전 장이 안좋아서 자제해야하는데",
          "original_form": "맞아요ㅋㅋㅋ전 장이 안좋아서 자제해야하는데",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043591.1.15",
          "form": "또 땡겨서 또 먹고ㅋㅋ",
          "original_form": "또 땡겨서 또 먹고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043591.1.16",
          "form": "그니까요 ㅋㅋ 그럼 떡볶이도 좋아하시나요?",
          "original_form": "그니까요 ㅋㅋ 그럼 떡볶이도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043591.1.17",
          "form": "네네 완전요",
          "original_form": "네네 완전요",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043591.1.18",
          "form": "로제떡볶이 넘 맛있어요",
          "original_form": "로제떡볶이 넘 맛있어요",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043591.1.19",
          "form": "맞아여 어디꺼 제일 좋아하세요?",
          "original_form": "맞아여 어디꺼 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043591.1.20",
          "form": "신참이었나? 거기 맛있더라고요",
          "original_form": "신참이었나? 거기 맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043591.1.21",
          "form": "로제떡볶이는 다 맛있는것같아요",
          "original_form": "로제떡볶이는 다 맛있는것같아요",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043591.1.22",
          "form": "신참 오리지널은 먹어봤는데 로제는 안먹어봤네여",
          "original_form": "신참 오리지널은 먹어봤는데 로제는 안먹어봤네여",
          "speaker_id": "2",
          "time": "20210907 20:32"
        }
      ]
