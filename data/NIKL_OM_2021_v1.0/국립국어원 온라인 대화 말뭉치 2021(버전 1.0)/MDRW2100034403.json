      "utterance": [
        {
          "id": "MDRW2100034403.1.1",
          "form": "일하고계신가요?",
          "original_form": "일하고계신가요?",
          "speaker_id": "1",
          "time": "20210828 06:50"
        },
        {
          "id": "MDRW2100034403.1.2",
          "form": "육아휴직중이예요",
          "original_form": "육아휴직중이예요",
          "speaker_id": "2",
          "time": "20210828 07:30"
        },
        {
          "id": "MDRW2100034403.1.3",
          "form": "그러시군요!",
          "original_form": "그러시군요!",
          "speaker_id": "1",
          "time": "20210828 07:34"
        },
        {
          "id": "MDRW2100034403.1.4",
          "form": "무슨일 하시나요?",
          "original_form": "무슨일 하시나요?",
          "speaker_id": "1",
          "time": "20210828 07:35"
        },
        {
          "id": "MDRW2100034403.1.5",
          "form": "중소기업",
          "original_form": "중소기업",
          "speaker_id": "2",
          "time": "20210828 07:36"
        },
        {
          "id": "MDRW2100034403.1.6",
          "form": "다녀요",
          "original_form": "다녀요",
          "speaker_id": "2",
          "time": "20210828 07:36"
        },
        {
          "id": "MDRW2100034403.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 07:37"
        },
        {
          "id": "MDRW2100034403.1.8",
          "form": "저는 취준중이에요",
          "original_form": "저는 취준중이에요",
          "speaker_id": "1",
          "time": "20210828 07:37"
        },
        {
          "id": "MDRW2100034403.1.9",
          "form": "시험준비하시나요?",
          "original_form": "시험준비하시나요?",
          "speaker_id": "2",
          "time": "20210828 07:50"
        },
        {
          "id": "MDRW2100034403.1.10",
          "form": "네 준비중이에요",
          "original_form": "네 준비중이에요",
          "speaker_id": "1",
          "time": "20210828 07:51"
        },
        {
          "id": "MDRW2100034403.1.11",
          "form": "애쓰시겠어요 공부는 어디서 하시나요?",
          "original_form": "애쓰시겠어요 공부는 어디서 하시나요?",
          "speaker_id": "2",
          "time": "20210828 07:54"
        },
        {
          "id": "MDRW2100034403.1.12",
          "form": "스터디카페에서 했는데 코로나때문에 주로 집에서 많이 하고 있어요",
          "original_form": "스터디카페에서 했는데 코로나때문에 주로 집에서 많이 하고 있어요",
          "speaker_id": "1",
          "time": "20210828 07:54"
        },
        {
          "id": "MDRW2100034403.1.13",
          "form": "그러시군요 코로나때문에 애쓰시겠어요 스터디카페에서 취업 준비하시는 분들 많을 것 같아요",
          "original_form": "그러시군요 코로나때문에 애쓰시겠어요 스터디카페에서 취업 준비하시는 분들 많을 것 같아요",
          "speaker_id": "2",
          "time": "20210828 08:19"
        },
        {
          "id": "MDRW2100034403.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100034403.1.15",
          "form": "조금 늦게 가는날에는 자리를 뺏기기도 하더라구요",
          "original_form": "조금 늦게 가는날에는 자리를 뺏기기도 하더라구요",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100034403.1.16",
          "form": "보면서 다들 열심히구나 하는 생각을 하게되요ㅎㅎ",
          "original_form": "보면서 다들 열심히구나 하는 생각을 하게되요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 08:23"
        },
        {
          "id": "MDRW2100034403.1.17",
          "form": "그러게요 좋은 직장 구하시길 응원해봅니다",
          "original_form": "그러게요 좋은 직장 구하시길 응원해봅니다",
          "speaker_id": "2",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100034403.1.18",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
          "speaker_id": "1",
          "time": "20210828 08:29"
        }
      ]
