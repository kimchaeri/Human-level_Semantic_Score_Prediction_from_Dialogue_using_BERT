      "utterance": [
        {
          "id": "MDRW2100043460.1.1",
          "form": "평소에도 채식으로 드세요?",
          "original_form": "평소에도 채식으로 드세요?",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043460.1.2",
          "form": "가급적이여",
          "original_form": "가급적이여",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043460.1.3",
          "form": "건강때문인가요?",
          "original_form": "건강때문인가요?",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043460.1.4",
          "form": "아니여 순수하게 맛이요",
          "original_form": "아니여 순수하게 맛이요",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043460.1.5",
          "form": "맞아요 야채도 맛잇어요",
          "original_form": "맞아요 야채도 맛잇어요",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043460.1.6",
          "form": "어떻게 드시는걸 좋아하세요? 샐러드?",
          "original_form": "어떻게 드시는걸 좋아하세요? 샐러드?",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043460.1.7",
          "form": "나물 샐러드 현미밥 이런거요",
          "original_form": "나물 샐러드 현미밥 이런거요",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043460.1.8",
          "form": "저도 현미밥 좋아해요",
          "original_form": "저도 현미밥 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043460.1.9",
          "form": "현미밥에 강된장 호박잎",
          "original_form": "현미밥에 강된장 호박잎",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043460.1.10",
          "form": "한의사분은 소고기네 쌀밥먹으라시는데",
          "original_form": "한의사분은 소고기네 쌀밥먹으라시는데",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043460.1.11",
          "form": "참 입맛이 안맞아요",
          "original_form": "참 입맛이 안맞아요",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043460.1.12",
          "form": "한의원에서 소고기 드시래요??",
          "original_form": "한의원에서 소고기 드시래요??",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043460.1.13",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043460.1.14",
          "form": "멸치국물 좋아하는데 해물은 몸에 안 맞다고 ㅠ",
          "original_form": "멸치국물 좋아하는데 해물은 몸에 안 맞다고 ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043460.1.15",
          "form": "ㅠㅠ저도",
          "original_form": "ㅠㅠ저도",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043460.1.16",
          "form": "해산물 너무 좋아하는데 저랑 해물 안맞대요 ㅋㅋㅋ....",
          "original_form": "해산물 너무 좋아하는데 저랑 해물 안맞대요 ㅋㅋㅋ....",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043460.1.17",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043460.1.18",
          "form": "소고기에 쌀밥먹으라고 ㅎㅎ",
          "original_form": "소고기에 쌀밥먹으라고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043460.1.19",
          "form": "몸이 많이 찬 편이라서 해물은 안맞다더라구요",
          "original_form": "몸이 많이 찬 편이라서 해물은 안맞다더라구요",
          "speaker_id": "1",
          "time": "20210907 19:29"
        }
      ]
