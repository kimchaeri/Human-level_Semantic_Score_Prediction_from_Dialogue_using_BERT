      "utterance": [
        {
          "id": "MDRW2100015093.1.1",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015093.1.2",
          "form": "약간 한국은 동아리 할 여건이 못되지않나",
          "original_form": "약간 한국은 동아리 할 여건이 못되지않나",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015093.1.3",
          "form": "그러면 넷플은 보시나요?",
          "original_form": "그러면 넷플은 보시나요?",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015093.1.4",
          "form": "넷플 종종 봅니다",
          "original_form": "넷플 종종 봅니다",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015093.1.5",
          "form": "구독은 안하고ㅈ보고싶을때만?",
          "original_form": "구독은 안하고ㅈ보고싶을때만?",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015093.1.6",
          "form": "넷플 보나요",
          "original_form": "넷플 보나요",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015093.1.7",
          "form": "엇 저도요",
          "original_form": "엇 저도요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015093.1.8",
          "form": "약간 넷플 보고싶은거 찾는용도로 쓰는게 국룰인가봐요",
          "original_form": "약간 넷플 보고싶은거 찾는용도로 쓰는게 국룰인가봐요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015093.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015093.1.10",
          "form": "볼 기력은 없다.",
          "original_form": "볼 기력은 없다.",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015093.1.11",
          "form": "뭔가 정기결제는 아까위",
          "original_form": "뭔가 정기결제는 아까위",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015093.1.12",
          "form": "뭐가 재밌다더라~",
          "original_form": "뭐가 재밌다더라~",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015093.1.13",
          "form": "말만듣고 봐야지 메모만 가득해놓는..",
          "original_form": "말만듣고 봐야지 메모만 가득해놓는..",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015093.1.14",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015093.1.15",
          "form": "그냥 제목만 보죠",
          "original_form": "그냥 제목만 보죠",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015093.1.16",
          "form": "사람 사는거 다 똑같다ㅋㅋㅋㄲ",
          "original_form": "사람 사는거 다 똑같다ㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015093.1.17",
          "form": "그럼 최근에 영화 보신거 있으세요?",
          "original_form": "그럼 최근에 영화 보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015093.1.18",
          "form": "그럼 왓챠 티빙 웨이브 이런것도 구독 안하시나",
          "original_form": "그럼 왓챠 티빙 웨이브 이런것도 구독 안하시나",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015093.1.19",
          "form": "왓챠 그 무료권 있어서 하고 티빙은 네플멤버십있어서 봐요!!",
          "original_form": "왓챠 그 무료권 있어서 하고 티빙은 네플멤버십있어서 봐요!!",
          "speaker_id": "1",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015093.1.20",
          "form": "거의 뭐 드라마만 봤었어요",
          "original_form": "거의 뭐 드라마만 봤었어요",
          "speaker_id": "1",
