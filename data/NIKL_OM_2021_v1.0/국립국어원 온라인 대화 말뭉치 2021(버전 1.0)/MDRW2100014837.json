      "utterance": [
        {
          "id": "MDRW2100014837.1.1",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014837.1.2",
          "form": "저 홈트 열심히 하고 있어요 잘은 못하지만 ㅎㅎ",
          "original_form": "저 홈트 열심히 하고 있어요 잘은 못하지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014837.1.3",
          "form": "해야하는데 귀차니즘이란",
          "original_form": "해야하는데 귀차니즘이란",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014837.1.4",
          "form": "유투브로요?",
          "original_form": "유투브로요?",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014837.1.5",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014837.1.6",
          "form": "전부 따라하는건 아니구요",
          "original_form": "전부 따라하는건 아니구요",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014837.1.7",
          "form": "제가 할수있는 동작들만ㅋㅋㅋㅋㅋ",
          "original_form": "제가 할수있는 동작들만ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014837.1.8",
          "form": "전 저스트댄스 구독했는데 작년에는 좀했는데 올해는 두번했나...",
          "original_form": "전 저스트댄스 구독했는데 작년에는 좀했는데 올해는 두번했나...",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014837.1.9",
          "form": "저는 유투버 몇명 영상 보면서 따라하는데",
          "original_form": "저는 유투버 몇명 영상 보면서 따라하는데",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014837.1.10",
          "form": "무릎에 무리가는 동작들은 피하고 있어요 ㅎㅎ",
          "original_form": "무릎에 무리가는 동작들은 피하고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014837.1.11",
          "form": "무릎....",
          "original_form": "무릎....",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014837.1.12",
          "form": "그래서 제가 운동을 못하는 거예요",
          "original_form": "그래서 제가 운동을 못하는 거예요",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014837.1.13",
          "form": "근육통 올까봐. ㅋㅋ",
          "original_form": "근육통 올까봐. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014837.1.14",
          "form": "무릎은 진짜 아껴야해요 ㅠㅠ",
          "original_form": "무릎은 진짜 아껴야해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014837.1.15",
          "form": "한번 상하면....",
          "original_form": "한번 상하면....",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014837.1.16",
          "form": "그쵸? 병원가기에도 어려운 시국에",
          "original_form": "그쵸? 병원가기에도 어려운 시국에",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014837.1.17",
          "form": "맞아요 ㅠㅠ 코로나 때문에",
          "original_form": "맞아요 ㅠㅠ 코로나 때문에",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014837.1.18",
          "form": "어떤거하세요? 홈트로?",
          "original_form": "어떤거하세요? 홈트로?",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014837.1.19",
          "form": "저는 힙으뜸이랑 빅씨스라는 유투브 채널 즐겨봐요",
          "original_form": "저는 힙으뜸이랑 빅씨스라는 유투브 채널 즐겨봐요",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014837.1.20",
          "form": "오 한번 찾아봐야겠어요",
          "original_form": "오 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014837.1.21",
          "form": "그냥 보기만 하겠지만",
          "original_form": "그냥 보기만 하겠지만",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014837.1.22",
          "form": "전 간혹 인스타보다가 간단운동 따라해요",
          "original_form": "전 간혹 인스타보다가 간단운동 따라해요",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014837.1.23",
          "form": "그때 뿐이지만",
          "original_form": "그때 뿐이지만",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014837.1.24",
          "form": "간단운동이라도 하면 좋더라구요",
          "original_form": "간단운동이라도 하면 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014837.1.25",
          "form": "저도 숨쉬기 운동만 하고 살다가",
          "original_form": "저도 숨쉬기 운동만 하고 살다가",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014837.1.26",
          "form": "건강이 너무 안좋아져서ㅠㅠ",
          "original_form": "건강이 너무 안좋아져서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014837.1.27",
          "form": "요즘 누워서 하는 운동이 많더라구요",
          "original_form": "요즘 누워서 하는 운동이 많더라구요",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014837.1.28",
          "form": "맞아요 허리가 안좋은 사람도 많아서",
          "original_form": "맞아요 허리가 안좋은 사람도 많아서",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014837.1.29",
          "form": "데드버그 좋아요",
          "original_form": "데드버그 좋아요",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014837.1.30",
          "form": "누워서 하니까 허리에 무리가 가지 않으면서 복부에 힘이 들어가거든요",
          "original_form": "누워서 하니까 허리에 무리가 가지 않으면서 복부에 힘이 들어가거든요",
          "speaker_id": "2",
          "time": "20210825 21:06"
        }
      ]
    }
