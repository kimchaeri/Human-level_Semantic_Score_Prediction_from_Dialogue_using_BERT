      "utterance": [
        {
          "id": "MDRW2100022851.1.1",
          "form": "불법 촬영물 용 커메라 설치한거?",
          "original_form": "불법 촬영물 용 커메라 설치한거?",
          "speaker_id": "2",
          "time": "20210829 18:31"
        },
        {
          "id": "MDRW2100022851.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022851.1.3",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022851.1.4",
          "form": "그거랑",
          "original_form": "그거랑",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022851.1.5",
          "form": "거기남고애들이",
          "original_form": "거기남고애들이",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022851.1.6",
          "form": "고등학교애들엄청",
          "original_form": "고등학교애들엄청",
          "speaker_id": "1",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022851.1.7",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022851.1.8",
          "form": "그거보니까",
          "original_form": "그거보니까",
          "speaker_id": "1",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022851.1.9",
          "form": "중학교생각남",
          "original_form": "중학교생각남",
          "speaker_id": "1",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022851.1.10",
          "form": "분명걔네들도그랬을거같음",
          "original_form": "분명걔네들도그랬을거같음",
          "speaker_id": "1",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022851.1.11",
          "form": "아 중학교 애들",
          "original_form": "아 중학교 애들",
          "speaker_id": "2",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022851.1.12",
          "form": "체육시간에 나오면",
          "original_form": "체육시간에 나오면",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022851.1.13",
          "form": "남중 애들이 물뿌렸음",
          "original_form": "남중 애들이 물뿌렸음",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022851.1.14",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022851.1.15",
          "form": "나 그거 맨앞에서 맞고 진심 너무 화가 났었는데",
          "original_form": "나 그거 맨앞에서 맞고 진심 너무 화가 났었는데",
          "speaker_id": "2",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022851.1.16",
          "form": "남자는다똑같은듯",
          "original_form": "남자는다똑같은듯",
          "speaker_id": "1",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022851.1.17",
          "form": "일부러",
          "original_form": "일부러",
          "speaker_id": "1",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022851.1.18",
          "form": "관심받으려고",
          "original_form": "관심받으려고",
          "speaker_id": "1",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022851.1.19",
          "form": "그러는듯",
          "original_form": "그러는듯",
          "speaker_id": "1",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022851.1.20",
          "form": "어휴 진짜 너무 화나서 걔네가 놓친 물병이랑 바가지 다시 던졌잖아",
          "original_form": "어휴 진짜 너무 화나서 걔네가 놓친 물병이랑 바가지 다시 던졌잖아",
          "speaker_id": "2",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022851.1.21",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022851.1.22",
          "form": "너무싫어",
          "original_form": "너무싫어",
          "speaker_id": "1",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100022851.1.23",
          "form": "그걸 제지 안하는 선생은",
          "original_form": "그걸 제지 안하는 선생은",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022851.1.24",
          "form": "교권을 왜 잡고 있을까 해야 할 일도 안하면서",
          "original_form": "교권을 왜 잡고 있을까 해야 할 일도 안하면서",
          "speaker_id": "2",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100022851.1.25",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022851.1.26",
          "form": "왜걔를",
          "original_form": "왜걔를",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022851.1.27",
          "form": "나서서",
          "original_form": "나서서",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022851.1.28",
          "form": "쉴드치는지",
          "original_form": "쉴드치는지",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022851.1.29",
          "form": "이해안감",
          "original_form": "이해안감",
          "speaker_id": "1",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022851.1.30",
          "form": "잡을 수 있으면서",
          "original_form": "잡을 수 있으면서",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022851.1.31",
          "form": "아니 선생의 의무가 뭐야",
          "original_form": "아니 선생의 의무가 뭐야",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022851.1.32",
          "form": "교육과 보호잖아",
          "original_form": "교육과 보호잖아",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022851.1.33",
          "form": "그래서절대",
          "original_form": "그래서절대",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022851.1.34",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022851.1.35",
          "form": "담근처로",
          "original_form": "담근처로",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022851.1.36",
          "form": "안갔잖아",
          "original_form": "안갔잖아",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022851.1.37",
          "form": "최대한계단근처에있었음",
          "original_form": "최대한계단근처에있었음",
