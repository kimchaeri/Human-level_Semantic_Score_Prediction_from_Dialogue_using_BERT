      "utterance": [
        {
          "id": "MDRW2100011349.1.1",
          "form": "중고거래 즐겨하시나요?",
          "original_form": "중고거래 즐겨하시나요?",
          "speaker_id": "1",
          "time": "20210824 11:55"
        },
        {
          "id": "MDRW2100011349.1.2",
          "form": "가끔 하는 정도예요",
          "original_form": "가끔 하는 정도예요",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100011349.1.3",
          "form": "불안해서요",
          "original_form": "불안해서요",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100011349.1.4",
          "form": "아 사기위험도가 조금 있죠ㅠㅠ",
          "original_form": "아 사기위험도가 조금 있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 11:58"
        },
        {
          "id": "MDRW2100011349.1.5",
          "form": "네 사람 직접 만나는것도 요즘은 좀그렇잖아요",
          "original_form": "네 사람 직접 만나는것도 요즘은 좀그렇잖아요",
          "speaker_id": "2",
          "time": "20210824 12:01"
        },
        {
          "id": "MDRW2100011349.1.6",
          "form": "그래서 당근 이용하긴 하는데 많이는 안하게 되더라고요",
          "original_form": "그래서 당근 이용하긴 하는데 많이는 안하게 되더라고요",
          "speaker_id": "2",
          "time": "20210824 12:02"
        },
        {
          "id": "MDRW2100011349.1.7",
          "form": "아 맞아요 직거래도 찝찝하긴 하죠",
          "original_form": "아 맞아요 직거래도 찝찝하긴 하죠",
          "speaker_id": "1",
          "time": "20210824 12:03"
        },
        {
          "id": "MDRW2100011349.1.8",
          "form": "요즘은 옛날에 샀던 카세트테이프 올려두긴 했는데",
          "original_form": "요즘은 옛날에 샀던 카세트테이프 올려두긴 했는데",
          "speaker_id": "2",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100011349.1.9",
          "form": "안팔리더라고요ㅋㅋ",
          "original_form": "안팔리더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100011349.1.10",
          "form": "아저도 한 일년째 안팔리는 옷들 있네요ㅠ",
          "original_form": "아저도 한 일년째 안팔리는 옷들 있네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:19"
        },
        {
          "id": "MDRW2100011349.1.11",
          "form": "자리만 차지하고 무료나눔이라도 해야하나봐요",
          "original_form": "자리만 차지하고 무료나눔이라도 해야하나봐요",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011349.1.12",
          "form": "무료나눔하면 바로 오실듯하네요ㅎㅎ",
          "original_form": "무료나눔하면 바로 오실듯하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:24"
        },
        {
          "id": "MDRW2100011349.1.13",
          "form": "그럴까요 워낙 옛날테이프라ㅋㅋ",
          "original_form": "그럴까요 워낙 옛날테이프라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100011349.1.14",
          "form": "한번 올려보시죠ㅎㅎ",
          "original_form": "한번 올려보시죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100011349.1.15",
          "form": "그래야겠네요\r\n끼고 살아봤자 듣지도 않더라고요ㅋㅋ",
          "original_form": "그래야겠네요\r\n끼고 살아봤자 듣지도 않더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100011349.1.16",
          "form": "버리는것보단 나을것같아요",
          "original_form": "버리는것보단 나을것같아요",
          "speaker_id": "1",
