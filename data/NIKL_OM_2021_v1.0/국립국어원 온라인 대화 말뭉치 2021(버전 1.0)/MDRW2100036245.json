        },
        {
          "id": "MDRW2100036245.1.2",
          "form": "내가 팔고싶은건 잘 안팔리는가!",
          "original_form": "내가 팔고싶은건 잘 안팔리는가!",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100036245.1.3",
          "form": "어떤거",
          "original_form": "어떤거",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100036245.1.4",
          "form": "나는 사는편임ㅎ",
          "original_form": "나는 사는편임ㅎ",
          "speaker_id": "2",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100036245.1.5",
          "form": "헌혈 영화관람권",
          "original_form": "헌혈 영화관람권",
          "speaker_id": "1",
          "time": "20210830 19:58"
        },
        {
          "id": "MDRW2100036245.1.6",
          "form": "사람들 안가니께..ㅜ",
          "original_form": "사람들 안가니께..ㅜ",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100036245.1.7",
          "form": "스벅 아아도 맨날 손해보고 팔고ㅜ",
          "original_form": "스벅 아아도 맨날 손해보고 팔고ㅜ",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100036245.1.8",
          "form": "니가 무라..ㅋㅋ 그럴꺼문ㅋㅋ",
          "original_form": "니가 무라..ㅋㅋ 그럴꺼문ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036245.1.9",
          "form": "한푼이라도 어떻게 ㅋㅋ",
          "original_form": "한푼이라도 어떻게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036245.1.10",
          "form": "벌어보겠다고 ㅋㅋㅋ",
          "original_form": "벌어보겠다고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036245.1.11",
          "form": "ㅋㅋㅋ 현금화가 어렵네",
          "original_form": "ㅋㅋㅋ 현금화가 어렵네",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100036245.1.12",
          "form": "또 올려놓고나면 못팔아서 안달임 ㅋㅋㅋ",
          "original_form": "또 올려놓고나면 못팔아서 안달임 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100036245.1.13",
          "form": "하 7500에 누가 사겠다고했다가 연락두절ㅜ",
          "original_form": "하 7500에 누가 사겠다고했다가 연락두절ㅜ",
          "speaker_id": "1",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100036245.1.14",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "2",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100036245.1.15",
          "form": "비와서 영화관가기싫었남",
          "original_form": "비와서 영화관가기싫었남",
          "speaker_id": "2",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100036245.1.16",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100036245.1.17",
          "form": "마음을 널널하게 잡아야지원",
          "original_form": "마음을 널널하게 잡아야지원",
          "speaker_id": "1",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100036245.1.18",
          "form": "당근에서 누가 7천원 제안함",
          "original_form": "당근에서 누가 7천원 제안함",
          "speaker_id": "1",
          "time": "20210831 14:05"
        },
        {
          "id": "MDRW2100036245.1.19",
          "form": "오 뭐 나쁘진않네",
          "original_form": "오 뭐 나쁘진않네",
