      "utterance": [
        {
          "id": "MDRW2100020479.1.1",
          "form": "사이드는 기본만 드시나요",
          "original_form": "사이드는 기본만 드시나요",
          "speaker_id": "1",
          "time": "20210828 08:10"
        },
        {
          "id": "MDRW2100020479.1.2",
          "form": "넵 전 기본 감자튀김을 사랑합니다",
          "original_form": "넵 전 기본 감자튀김을 사랑합니다",
          "speaker_id": "2",
          "time": "20210828 08:11"
        },
        {
          "id": "MDRW2100020479.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 08:11"
        },
        {
          "id": "MDRW2100020479.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210828 08:11"
        },
        {
          "id": "MDRW2100020479.1.5",
          "form": "특히 버거킹 감튀는 도톰해서",
          "original_form": "특히 버거킹 감튀는 도톰해서",
          "speaker_id": "1",
          "time": "20210828 08:11"
        },
        {
          "id": "MDRW2100020479.1.6",
          "form": "더 맛있는거같아요",
          "original_form": "더 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210828 08:11"
        },
        {
          "id": "MDRW2100020479.1.7",
          "form": "그쵸 딴데랑 좀 달라요",
          "original_form": "그쵸 딴데랑 좀 달라요",
          "speaker_id": "2",
          "time": "20210828 08:11"
        },
        {
          "id": "MDRW2100020479.1.8",
          "form": "감튀에 케첩찍어드시나요",
          "original_form": "감튀에 케첩찍어드시나요",
          "speaker_id": "1",
          "time": "20210828 08:11"
        },
        {
          "id": "MDRW2100020479.1.9",
          "form": "네 케첩 필수!",
          "original_form": "네 케첩 필수!",
          "speaker_id": "2",
          "time": "20210828 08:12"
        },
        {
          "id": "MDRW2100020479.1.10",
          "form": "그쵸 뭔가 감튀만 먹기는 아쉬우니까",
          "original_form": "그쵸 뭔가 감튀만 먹기는 아쉬우니까",
          "speaker_id": "1",
          "time": "20210828 08:12"
        },
        {
          "id": "MDRW2100020479.1.11",
          "form": "칠리소스도 맛있을거같아요",
          "original_form": "칠리소스도 맛있을거같아요",
          "speaker_id": "2",
          "time": "20210828 08:12"
        },
        {
          "id": "MDRW2100020479.1.12",
          "form": "그것도 너무좋을거같아요",
          "original_form": "그것도 너무좋을거같아요",
          "speaker_id": "1",
          "time": "20210828 08:12"
        },
        {
          "id": "MDRW2100020479.1.13",
          "form": "감튀에 칠리 완벽한데요",
          "original_form": "감튀에 칠리 완벽한데요",
          "speaker_id": "1",
          "time": "20210828 08:13"
        },
        {
          "id": "MDRW2100020479.1.14",
          "form": "name1님은 어떤 햄버거 브랜드 좋아하세요?",
          "original_form": "&name1&님은 어떤 햄버거 브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 08:13"
        },
        {
          "id": "MDRW2100020479.1.15",
          "form": "음료는 어떤거드시나요",
          "original_form": "음료는 어떤거드시나요",
          "speaker_id": "1",
          "time": "20210828 08:13"
        },
        {
          "id": "MDRW2100020479.1.16",
          "form": "저는 가리는게 없어요",
          "original_form": "저는 가리는게 없어요",
          "speaker_id": "1",
          "time": "20210828 08:13"
        },
        {
          "id": "MDRW2100020479.1.17",
          "form": "브랜드마다 좋아하는게 다 있어서",
          "original_form": "브랜드마다 좋아하는게 다 있어서",
          "speaker_id": "1",
          "time": "20210828 08:13"
        },
        {
          "id": "MDRW2100020479.1.18",
          "form": "제로콜라...?",
          "original_form": "제로콜라...?",
          "speaker_id": "2",
          "time": "20210828 08:13"
        },
        {
          "id": "MDRW2100020479.1.19",
          "form": "오 저도 각 브랜드마다 거의 정해져있어요",
          "original_form": "오 저도 각 브랜드마다 거의 정해져있어요",
          "speaker_id": "2",
          "time": "20210828 08:13"
        },
        {
          "id": "MDRW2100020479.1.20",
          "form": "제로콜라 맛있어요ㅎ",
          "original_form": "제로콜라 맛있어요ㅎ",
          "speaker_id": "1",
          "time": "20210828 08:13"
        }
