          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000342.1.3",
          "form": "무슨일하세요",
          "original_form": "무슨일하세요",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000342.1.4",
          "form": "아마 반대편에서 일하고 있을걸요?",
          "original_form": "아마 반대편에서 일하고 있을걸요?",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000342.1.5",
          "form": "아아 외국에서 근무하시는구나",
          "original_form": "아아 외국에서 근무하시는구나",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000342.1.6",
          "form": "뭐라는거에요..",
          "original_form": "뭐라는거에요..",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000342.1.7",
          "form": "저희 낯선관계인가요?",
          "original_form": "저희 낯선관계인가요?",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000342.1.8",
          "form": "그냥 드립인데요",
          "original_form": "그냥 드립인데요",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000342.1.9",
          "form": "아르바이트",
          "original_form": "아르바이트",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000342.1.10",
          "form": "뭐해보셨어요",
          "original_form": "뭐해보셨어요",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000342.1.11",
          "form": "저 투썸에서 일해봤어요",
          "original_form": "저 투썸에서 일해봤어요",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000342.1.12",
          "form": "오 투썸 어때여",
          "original_form": "오 투썸 어때여",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000342.1.13",
          "form": "진상개많아요",
          "original_form": "진상개많아요",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000342.1.14",
          "form": "ㅋㅋ기억나는거 하나만",
          "original_form": "ㅋㅋ기억나는거 하나만",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000342.1.15",
          "form": "ㄱ",
          "original_form": "ㄱ",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000342.1.16",
          "form": "진상썰이요?",
          "original_form": "진상썰이요?",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000342.1.17",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000342.1.18",
          "form": "ㅋㅋㅋ남자분이 포인트 적립해달라고 \"포인트요\"라고말했는데",
          "original_form": "ㅋㅋㅋ남자분이 포인트 적립해달라고 \"포인트요\"라고말했는데",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000342.1.19",
          "form": "포인트를 쓴다는건지 적립해달라는건지 이해못해서",
          "original_form": "포인트를 쓴다는건지 적립해달라는건지 이해못해서",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000342.1.20",
          "form": "네??라고 한 3번 되물으니까 혼자 화났는지",
          "original_form": "네??라고 한 3번 되물으니까 혼자 화났는지",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000342.1.21",
          "form": "주먹들었음",
          "original_form": "주먹들었음",
          "speaker_id": "1",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000342.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000342.1.23",
          "form": "맞았으면 알바비대신",
          "original_form": "맞았으면 알바비대신",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000342.1.24",
          "form": "깽값으로 한달 채웠을텐데..",
          "original_form": "깽값으로 한달 채웠을텐데..",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000342.1.25",
