      "utterance": [
        {
          "id": "MDRW2100013046.1.1",
          "form": "이제 애들 수업 끝날 때 거의 다 됐네 ㅎㅎ",
          "original_form": "이제 애들 수업 끝날 때 거의 다 됐네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100013046.1.2",
          "form": "ㅇㅇ 12시에 끝나는 거 너무 짧은 것 같아",
          "original_form": "ㅇㅇ 12시에 끝나는 거 너무 짧은 것 같아",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013046.1.3",
          "form": "오후에도 동영상 강의 있고 하니",
          "original_form": "오후에도 동영상 강의 있고 하니",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013046.1.4",
          "form": "일단 줌 끝나면 하기 싫어하더라고",
          "original_form": "일단 줌 끝나면 하기 싫어하더라고",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013046.1.5",
          "form": "거의 과제 중심으로 되어 있다보니",
          "original_form": "거의 과제 중심으로 되어 있다보니",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013046.1.6",
          "form": "ㅇㅇ 오후엔 숙제하다 끝나",
          "original_form": "ㅇㅇ 오후엔 숙제하다 끝나",
          "speaker_id": "1",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100013046.1.7",
          "form": "언제까지 온라인이지?",
          "original_form": "언제까지 온라인이지?",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013046.1.8",
          "form": "9월 첫째주까지",
          "original_form": "9월 첫째주까지",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013046.1.9",
          "form": "그 이후엔 아직 계확 없는거지?",
          "original_form": "그 이후엔 아직 계확 없는거지?",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013046.1.10",
          "form": "ㅇㅇ 4단계에선 계속 온라인일 것 같아 ㅠ",
          "original_form": "ㅇㅇ 4단계에선 계속 온라인일 것 같아 ㅠ",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013046.1.11",
          "form": "소규모학교라 3단계 가야 등교하겠네",
          "original_form": "소규모학교라 3단계 가야 등교하겠네",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013046.1.12",
          "form": "아마 그럴듯.. 3단계도 불안하긴 해",
          "original_form": "아마 그럴듯.. 3단계도 불안하긴 해",
          "speaker_id": "1",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013046.1.13",
          "form": "그애도 밥은 안먹고 오니까",
          "original_form": "그애도 밥은 안먹고 오니까",
          "speaker_id": "2",
          "time": "20210825 11:40"
        },
        {
          "id": "MDRW2100013046.1.14",
          "form": "급식 안먹고 와도 한반에 25명 다닥다닥이라 불안하지 ㅎㅎ",
          "original_form": "급식 안먹고 와도 한반에 25명 다닥다닥이라 불안하지 ㅎㅎ",
