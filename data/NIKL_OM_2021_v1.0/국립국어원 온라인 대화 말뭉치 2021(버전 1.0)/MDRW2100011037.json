      "utterance": [
        {
          "id": "MDRW2100011037.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011037.1.2",
          "form": "하시는 게임이 있으신가용",
          "original_form": "하시는 게임이 있으신가용",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011037.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011037.1.4",
          "form": "그냥 간단히 시간때우기용으로 해요",
          "original_form": "그냥 간단히 시간때우기용으로 해요",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011037.1.5",
          "form": "퍼즐맞추기같은거",
          "original_form": "퍼즐맞추기같은거",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011037.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011037.1.7",
          "form": "아아 주제가 겜이나와가지구 저는 모바일게임만 해욘",
          "original_form": "아아 주제가 겜이나와가지구 저는 모바일게임만 해욘",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100011037.1.8",
          "form": "모바일 게임중에 퍼즐맞추기도 해용ㅎㅎ",
          "original_form": "모바일 게임중에 퍼즐맞추기도 해용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100011037.1.9",
          "form": "예전엔 애니팡 카카오프렌즈 이런거 많이했어요ㅎ",
          "original_form": "예전엔 애니팡 카카오프렌즈 이런거 많이했어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011037.1.10",
          "form": "아아 ㅎㅎ 그럼 혹시 프로젝트 런웨이 해보셨나용",
          "original_form": "아아 ㅎㅎ 그럼 혹시 프로젝트 런웨이 해보셨나용",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011037.1.11",
          "form": "퍼즐게임인데 재밌어용",
          "original_form": "퍼즐게임인데 재밌어용",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011037.1.12",
          "form": "첨들어봐요ㅎ 복잡하진않나요?",
          "original_form": "첨들어봐요ㅎ 복잡하진않나요?",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011037.1.13",
          "form": "전 단순검 좋아해서",
          "original_form": "전 단순검 좋아해서",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011037.1.14",
          "form": "네넵 걍 퍼즐만 하는건데 재밌어용 ㅎㅎ 저두 가끔 심심할때만해용",
          "original_form": "네넵 걍 퍼즐만 하는건데 재밌어용 ㅎㅎ 저두 가끔 심심할때만해용",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011037.1.15",
          "form": "다른 게임은 안하시나용",
          "original_form": "다른 게임은 안하시나용",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011037.1.16",
          "form": "네 그냥 출첵하면 짝맞추기나 그런건 하는데 ㅎ",
          "original_form": "네 그냥 출첵하면 짝맞추기나 그런건 하는데 ㅎ",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011037.1.17",
          "form": "겜은 잘안해요^#",
          "original_form": "겜은 잘안해요^#",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011037.1.18",
          "form": "다들 모바일겜 많이하던데 아이템도 사고 그러세요?",
          "original_form": "다들 모바일겜 많이하던데 아이템도 사고 그러세요?",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011037.1.19",
          "form": "저는 돈은 절대 안써요!!",
          "original_form": "저는 돈은 절대 안써요!!",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011037.1.20",
          "form": "돈은 뭔가 아깝더라구요ㅠㅠㅠ",
          "original_form": "돈은 뭔가 아깝더라구요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011037.1.21",
          "form": "저도요ㅎ 앱테크용 겜이 있었는데",
          "original_form": "저도요ㅎ 앱테크용 겜이 있었는데",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011037.1.22",
          "form": "영업정지먹었더라구요",
          "original_form": "영업정지먹었더라구요",
          "speaker_id": "2",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011037.1.23",
          "form": "앗 어떤 게임인가요 궁금하네용",
          "original_form": "앗 어떤 게임인가요 궁금하네용",
          "speaker_id": "1",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011037.1.24",
          "form": "하루100원씩 줬는데",
          "original_form": "하루100원씩 줬는데",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011037.1.25",
          "form": "그냥 가위바위보 단순겜인데 사행성조장한다고",
          "original_form": "그냥 가위바위보 단순겜인데 사행성조장한다고",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011037.1.26",
          "form": "구글에서 정지시켜가지고",
          "original_form": "구글에서 정지시켜가지고",
          "speaker_id": "2",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011037.1.27",
          "form": "오호 처음들어봤어용 앱테크는 종종 들어봤는데",
          "original_form": "오호 처음들어봤어용 앱테크는 종종 들어봤는데",
          "speaker_id": "1",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011037.1.28",
          "form": "게임으로도 주는게 있었다니 ㅎㅎ",
          "original_form": "게임으로도 주는게 있었다니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011037.1.29",
          "form": "계좌로 바로 들어오나요",
          "original_form": "계좌로 바로 들어오나요",
          "speaker_id": "1",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011037.1.30",
          "form": "원래천원도 주고그랬는데",
          "original_form": "원래천원도 주고그랬는데",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011037.1.31",
          "form": "계좌로 신청함 들어왔어요",
          "original_form": "계좌로 신청함 들어왔어요",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011037.1.32",
          "form": "와 ㅎㅎㅎ꿀팁이네요",
          "original_form": "와 ㅎㅎㅎ꿀팁이네요",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011037.1.33",
          "form": "한달에 몇천원에서 만원이상 벌기도하고 좋았거든요ㅋㅋ",
          "original_form": "한달에 몇천원에서 만원이상 벌기도하고 좋았거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011037.1.34",
          "form": "좋은 얘기 감사합니다!",
          "original_form": "좋은 얘기 감사합니다!",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011037.1.35",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011037.1.36",
          "form": "다음에 저도 찾아봐야겠어요 ㅎㅎ",
          "original_form": "다음에 저도 찾아봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011037.1.37",
          "form": "플레이랑 이라고 퍼즐겜인데 이것도  앱테크용이예요",
          "original_form": "플레이랑 이라고 퍼즐겜인데 이것도  앱테크용이예요",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011037.1.38",
