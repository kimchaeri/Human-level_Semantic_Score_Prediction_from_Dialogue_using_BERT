      "utterance": [
        {
          "id": "MDRW2100022728.1.1",
          "form": "못 따라하거든",
          "original_form": "못 따라하거든",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022728.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022728.1.3",
          "form": "아마 자기가 기대한것 만큼",
          "original_form": "아마 자기가 기대한것 만큼",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022728.1.4",
          "form": "못따라오니까",
          "original_form": "못따라오니까",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022728.1.5",
          "form": "다시 파양한것 같더라",
          "original_form": "다시 파양한것 같더라",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022728.1.6",
          "form": "이래서 품종견 전시가",
          "original_form": "이래서 품종견 전시가",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022728.1.7",
          "form": "진짜 유해함",
          "original_form": "진짜 유해함",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022728.1.8",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.9",
          "form": "어떤견은 착하고 천사견이다",
          "original_form": "어떤견은 착하고 천사견이다",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.10",
          "form": "반려동물을 기르기로 했으면",
          "original_form": "반려동물을 기르기로 했으면",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.11",
          "form": "어떤종은 똑똑하다",
          "original_form": "어떤종은 똑똑하다",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.12",
          "form": "자기가 거기에 맞춰야지",
          "original_form": "자기가 거기에 맞춰야지",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.13",
          "form": "이런거...진짜 그만 나왓으면좋겠어",
          "original_form": "이런거...진짜 그만 나왓으면좋겠어",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.14",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.15",
          "form": "지한테 맞는 애를 찾고 있음",
          "original_form": "지한테 맞는 애를 찾고 있음",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.16",
          "form": "그 펫키지라고",
          "original_form": "그 펫키지라고",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.17",
          "form": "jtbc에서",
          "original_form": "jtbc에서",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.18",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.19",
          "form": "강아지랑 연예인이랑 여행보내는 예능 얼마전에 나왓거든",
          "original_form": "강아지랑 연예인이랑 여행보내는 예능 얼마전에 나왓거든",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.20",
          "form": "근데 거기서 김희철이ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 거기서 김희철이ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022728.1.21",
          "form": "초보 보호자들은",
          "original_form": "초보 보호자들은",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.22",
          "form": "유기견 입양하면안된다고",
          "original_form": "유기견 입양하면안된다고",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.23",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.24",
          "form": "버려진아픔이 잇는 애들이라서",
          "original_form": "버려진아픔이 잇는 애들이라서",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.25",
          "form": "진짜 짜증난다",
          "original_form": "진짜 짜증난다",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.26",
          "form": "ㄱ ㅐ들도 힘들고 주인도 힘들다",
          "original_form": "ㄱ ㅐ들도 힘들고 주인도 힘들다",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.27",
          "form": "이런식으로말해서",
          "original_form": "이런식으로말해서",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.28",
          "form": "지금 난리남",
          "original_form": "지금 난리남",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.29",
          "form": "안그래도 거기나오는 김희철이랑 태연 둘다",
          "original_form": "안그래도 거기나오는 김희철이랑 태연 둘다",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.30",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.31",
          "form": "펫샵에서 개 사온사람들인데",
          "original_form": "펫샵에서 개 사온사람들인데",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.32",
          "form": "진짜 별로다",
          "original_form": "진짜 별로다",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022728.1.33",
          "form": "그 얼마나 된 예능이야",
          "original_form": "그 얼마나 된 예능이야",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022728.1.34",
          "form": "나온지 얼마안됨",
          "original_form": "나온지 얼마안됨",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022728.1.35",
          "form": "2화까진가 나왔을걸?",
          "original_form": "2화까진가 나왔을걸?",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022728.1.36",
          "form": "아니 완전 펫샵 사업을 부추기네",
          "original_form": "아니 완전 펫샵 사업을 부추기네",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022728.1.37",
          "form": "내가 봤을땐",
          "original_form": "내가 봤을땐",
          "speaker_id": "2",
          "time": "20210829 17:30"
        }
      ]
    }
