      "utterance": [
        {
          "id": "MDRW2100012792.1.1",
          "form": "설렁탕에 소주한잔했는데 이야dd",
          "original_form": "설렁탕에 소주한잔했는데 이야dd",
          "speaker_id": "2",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012792.1.2",
          "form": "기가막힙니다ㅎㅎ",
          "original_form": "기가막힙니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012792.1.3",
          "form": "아 설렁탕이면 소주 먹어도 속이 확 풀리죠ㅎ",
          "original_form": "아 설렁탕이면 소주 먹어도 속이 확 풀리죠ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:17"
        },
        {
          "id": "MDRW2100012792.1.4",
          "form": "거기에 수육 추가하면 정말 천국",
          "original_form": "거기에 수육 추가하면 정말 천국",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012792.1.5",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012792.1.6",
          "form": "술로 통하는군요~^^",
          "original_form": "술로 통하는군요~^^",
          "speaker_id": "2",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012792.1.7",
          "form": "주량이?",
          "original_form": "주량이?",
          "speaker_id": "2",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012792.1.8",
          "form": "소주 두병이요",
          "original_form": "소주 두병이요",
          "speaker_id": "1",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100012792.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100012792.1.10",
          "form": "저도 그정도에요 ㅡㅎ",
          "original_form": "저도 그정도에요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100012792.1.11",
          "form": "근데 자주 안마셔요ㅎ",
          "original_form": "근데 자주 안마셔요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100012792.1.12",
          "form": "안주는 보통 뭐드세요?",
          "original_form": "안주는 보통 뭐드세요?",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100012792.1.13",
          "form": "좋아하는 안주는 회!",
          "original_form": "좋아하는 안주는 회!",
          "speaker_id": "1",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100012792.1.14",
          "form": "사람들과 자리하기 만만한 것은 삼겹살이죠ㅎ",
          "original_form": "사람들과 자리하기 만만한 것은 삼겹살이죠ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100012792.1.15",
          "form": "글죠ㅎㅎ",
          "original_form": "글죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100012792.1.16",
          "form": "안주는 삼겹살, 회 만한게 없죠ㅎㅎ",
          "original_form": "안주는 삼겹살, 회 만한게 없죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100012792.1.17",
          "form": "그런데 주변에 회 좋아하는 사람이 생각보다 없네요ㅜ",
          "original_form": "그런데 주변에 회 좋아하는 사람이 생각보다 없네요ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100012792.1.18",
          "form": "흰살생선만 먹는 사람도 많고",
          "original_form": "흰살생선만 먹는 사람도 많고",
          "speaker_id": "1",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100012792.1.19",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100012792.1.20",
          "form": "회는 왠만하면 좋아하는데ㅠ",
          "original_form": "회는 왠만하면 좋아하는데ㅠ",
          "speaker_id": "2",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012792.1.21",
          "form": "근데 날씨가 더워서 그럴꺼에요ㅎ 아다리 잘못나면 진짜 고생해요ㅎ",
          "original_form": "근데 날씨가 더워서 그럴꺼에요ㅎ 아다리 잘못나면 진짜 고생해요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012792.1.22",
          "form": "그러니까요ㅜ 다 나름의 맛이 있는데",
          "original_form": "그러니까요ㅜ 다 나름의 맛이 있는데",
          "speaker_id": "1",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012792.1.23",
          "form": "근데 저도 회 좋아하지만ㅎ",
          "original_form": "근데 저도 회 좋아하지만ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100012792.1.24",
          "form": "솔직히 초장이랑 간장 빼면 못먹지않아요?",
          "original_form": "솔직히 초장이랑 간장 빼면 못먹지않아요?",
          "speaker_id": "2",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100012792.1.25",
          "form": "ㅎㅎ 그냥 먹으면 물리지요",
          "original_form": "ㅎㅎ 그냥 먹으면 물리지요",
          "speaker_id": "1",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100012792.1.26",
          "form": "참치는 예외인데 맛이라는게 꼭 깊은맛이 있어야하는건 아니지만",
          "original_form": "참치는 예외인데 맛이라는게 꼭 깊은맛이 있어야하는건 아니지만",
          "speaker_id": "2",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012792.1.27",
          "form": "없으면 허전하더라구요ㅎ",
          "original_form": "없으면 허전하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012792.1.28",
          "form": "맞아요. 하다못해 소금이나 기름이라도ㅜ",
          "original_form": "맞아요. 하다못해 소금이나 기름이라도ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012792.1.29",
          "form": "가끔 tv보면 회만 음미하면서 드시는데... 뻥같음ㅎㅎㅎㅎ",
          "original_form": "가끔 tv보면 회만 음미하면서 드시는데... 뻥같음ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012792.1.30",
          "form": "안되면 와사비나 겨자라도 있어야죠",
          "original_form": "안되면 와사비나 겨자라도 있어야죠",
          "speaker_id": "1",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012792.1.31",
          "form": "회는 무슨종류 좋아해요?",
          "original_form": "회는 무슨종류 좋아해요?",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100012792.1.32",
          "form": "좋아하는 생선?",
          "original_form": "좋아하는 생선?",
          "speaker_id": "2",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100012792.1.33",
          "form": "정말 다 좋아해서 못 골라요ㅜ",
          "original_form": "정말 다 좋아해서 못 골라요ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100012792.1.34",
          "form": "방어 도다리 놀래미 전어 계절생선은 다 찾아먹어서",
          "original_form": "방어 도다리 놀래미 전어 계절생선은 다 찾아먹어서",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100012792.1.35",
          "form": "그래도 하나 고르면 솔직히",
          "original_form": "그래도 하나 고르면 솔직히",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100012792.1.36",
          "form": "광어 지느러미입니다ㅎㅎ 일반 부위 말고 지느러미만",
          "original_form": "광어 지느러미입니다ㅎㅎ 일반 부위 말고 지느러미만",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100012792.1.37",
          "form": "ㅎㅎ 맞죻 저도 좋아해요ㅎ",
          "original_form": "ㅎㅎ 맞죻 저도 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012792.1.38",
          "form": "아.,  아나고와 투톱이요",
          "original_form": "아.,  아나고와 투톱이요",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012792.1.39",
          "form": "참치는 안드시네요ㅎㅎ",
          "original_form": "참치는 안드시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012792.1.40",
          "form": "참치도 좋아해요ㅎ",
          "original_form": "참치도 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100012792.1.41",
          "form": "근데 많이 먹기 어려운 생선이라서요",
          "original_form": "근데 많이 먹기 어려운 생선이라서요",
          "speaker_id": "1",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100012792.1.42",
          "form": "참치는 그렇죠ㅎ 보통 기본 5만원이니",
          "original_form": "참치는 그렇죠ㅎ 보통 기본 5만원이니",
          "speaker_id": "2",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100012792.1.43",
          "form": "계절 없이 먹으니 이때 꼭 먹어야 한다는 강박증도 안 생기고ㅎ",
          "original_form": "계절 없이 먹으니 이때 꼭 먹어야 한다는 강박증도 안 생기고ㅎ",
