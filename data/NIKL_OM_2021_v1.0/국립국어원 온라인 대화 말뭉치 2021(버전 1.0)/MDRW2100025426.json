      "utterance": [
        {
          "id": "MDRW2100025426.1.1",
          "form": "취업 하셨나요?",
          "original_form": "취업 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025426.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025426.1.3",
          "form": "저는 서비스직 일하고 있어요",
          "original_form": "저는 서비스직 일하고 있어요",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025426.1.4",
          "form": "저듀요!",
          "original_form": "저듀요!",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025426.1.5",
          "form": "무슨일하시나요?",
          "original_form": "무슨일하시나요?",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025426.1.6",
          "form": "저는 카페쪽",
          "original_form": "저는 카페쪽",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025426.1.7",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025426.1.8",
          "form": "거기는 지금 손님 많나요?",
          "original_form": "거기는 지금 손님 많나요?",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025426.1.9",
          "form": "아니요 점심시간이라서",
          "original_form": "아니요 점심시간이라서",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025426.1.10",
          "form": "조용해요",
          "original_form": "조용해요",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025426.1.11",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025426.1.12",
          "form": "여기는 비가 와서 오늘 손님 많을것같아요",
          "original_form": "여기는 비가 와서 오늘 손님 많을것같아요",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025426.1.13",
          "form": "저희도 비가 점심이후가 걱정이네요!!",
          "original_form": "저희도 비가 점심이후가 걱정이네요!!",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025426.1.14",
          "form": "괴롭히는 직장 사람은 없으신가요",
          "original_form": "괴롭히는 직장 사람은 없으신가요",
          "speaker_id": "1",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025426.1.15",
          "form": "네네 근데 조용한 친구가있는데 조끔  대하기 어렵더라구여 ㅠㅠ",
          "original_form": "네네 근데 조용한 친구가있는데 조끔  대하기 어렵더라구여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025426.1.16",
          "form": "그쪽은 있으신가요",
          "original_form": "그쪽은 있으신가요",
          "speaker_id": "2",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025426.1.17",
          "form": "아 쫌 그렇네요 말을 잘 안하나봐요",
          "original_form": "아 쫌 그렇네요 말을 잘 안하나봐요",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025426.1.18",
          "form": "저는 카페 담당자 있는데 말은 쫌 기분나쁘게 하는 사람이 있네요",
          "original_form": "저는 카페 담당자 있는데 말은 쫌 기분나쁘게 하는 사람이 있네요",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025426.1.19",
          "form": "얼른 이직 하고 싶네요",
          "original_form": "얼른 이직 하고 싶네요",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025426.1.20",
          "form": "헐 기분나쁘게 말하는게 젤 싫어요",
          "original_form": "헐 기분나쁘게 말하는게 젤 싫어요",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025426.1.21",
          "form": "상대방 생각을안하능거같아요 ㅠ",
          "original_form": "상대방 생각을안하능거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025426.1.22",
          "form": "저도 이직하고싶네요 ㅠㅠ",
          "original_form": "저도 이직하고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025426.1.23",
          "form": "적게일하고 많이 벌고 싶어요",
          "original_form": "적게일하고 많이 벌고 싶어요",
          "speaker_id": "1",
          "time": "20210901 11:44"
        }
      ]
    }
