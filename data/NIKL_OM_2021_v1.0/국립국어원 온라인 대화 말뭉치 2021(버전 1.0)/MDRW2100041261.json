      "utterance": [
        {
          "id": "MDRW2100041261.1.1",
          "form": "싸운다",
          "original_form": "싸운다",
          "speaker_id": "2",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041261.1.2",
          "form": "ㄱㄴㅇㅋㅋ",
          "original_form": "ㄱㄴㅇㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ얼른 화해시키고 재워요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ얼른 화해시키고 재워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.4",
          "form": "아 웃겨 ㅋㅋ",
          "original_form": "아 웃겨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.5",
          "form": "울신랑은 화장실에서 나올생각을안하노",
          "original_form": "울신랑은 화장실에서 나올생각을안하노",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.6",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.7",
          "form": "애들양치해야되는데",
          "original_form": "애들양치해야되는데",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.8",
          "form": "아하. ㅋㅋ 언니보고 재우라고 ㅋㅋ",
          "original_form": "아하. ㅋㅋ 언니보고 재우라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.9",
          "form": "아하 ㅋㅋㅋㅋ 다른 화장실 있잖아요",
          "original_form": "아하 ㅋㅋㅋㅋ 다른 화장실 있잖아요",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.10",
          "form": "난 화장실이 두개인데 하나인것 처럼ㅋㅇㅋ",
          "original_form": "난 화장실이 두개인데 하나인것 처럼ㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.11",
          "form": "화장실 한군데 또 안써요?",
          "original_form": "화장실 한군데 또 안써요?",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.12",
          "form": "와 대단하다 ㅋㅋㅋ",
          "original_form": "와 대단하다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.13",
          "form": "하나는 거의안씀ㅋㅋ",
          "original_form": "하나는 거의안씀ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041261.1.14",
          "form": "와 ㅋㅋ 어떻게 그럴수있지 ㅋㅋ 안불편해요?",
          "original_form": "와 ㅋㅋ 어떻게 그럴수있지 ㅋㅋ 안불편해요?",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041261.1.15",
          "form": "이렇게 살다보니ㅋ 적응됀듯ㅋㅋ",
          "original_form": "이렇게 살다보니ㅋ 적응됀듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041261.1.16",
          "form": "누가이사와도 당당할수있다  이집 엉망이였는거 안방화장실 보여주면서ㅋㅋ 원래 이상태였다고ㅋㅋ",
          "original_form": "누가이사와도 당당할수있다  이집 엉망이였는거 안방화장실 보여주면서ㅋㅋ 원래 이상태였다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100041261.1.17",
          "form": "청소는한다 한달에한번ㅋㅋ",
          "original_form": "청소는한다 한달에한번ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100041261.1.18",
          "form": "ㅋㄱㅋㅋㅋㅋ와ㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㅋ와ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100041261.1.19",
          "form": "대단한데요ㅋ",
          "original_form": "대단한데요ㅋ",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100041261.1.20",
          "form": "오늘 좀불편하네ㅡㅡ",
          "original_form": "오늘 좀불편하네ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041261.1.21",
          "form": "애들이 못씻어서ㅡㅡ",
          "original_form": "애들이 못씻어서ㅡㅡ",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041261.1.22",
          "form": "아ㅋㅋ 다른데써요ㅋㅋ",
          "original_form": "아ㅋㅋ 다른데써요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100041261.1.23",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100041261.1.24",
          "form": "저는 이제 지안이재우려구요ㅋ",
          "original_form": "저는 이제 지안이재우려구요ㅋ",
          "speaker_id": "1",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100041261.1.25",
          "form": "그래 닌도 육퇴해라",
          "original_form": "그래 닌도 육퇴해라",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100041261.1.26",
          "form": "얼른ㅋ",
          "original_form": "얼른ㅋ",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100041261.1.27",
          "form": "얍얍",
          "original_form": "얍얍",
          "speaker_id": "1",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100041261.1.28",
          "form": "언니도 육퇴해요ㅋ",
          "original_form": "언니도 육퇴해요ㅋ",
          "speaker_id": "1",
          "time": "20210905 21:07"
        }
      ]
    }
