      "utterance": [
        {
          "id": "MDRW2100033749.1.1",
          "form": "나는 그냥 엄마가 사주는 거 써.",
          "original_form": "나는 그냥 엄마가 사주는 거 써.",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.2",
          "form": "마틸라 이불 갖고싶어",
          "original_form": "마틸라 이불 갖고싶어",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.3",
          "form": "좀 예쁘더랴",
          "original_form": "좀 예쁘더랴",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.4",
          "form": "이불 브랜드 이름이야?",
          "original_form": "이불 브랜드 이름이야?",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.5",
          "form": "근데 내가 이불을 사기엔 돈아까워ㅡ 집에 이불이 많거든.",
          "original_form": "근데 내가 이불을 사기엔 돈아까워ㅡ 집에 이불이 많거든.",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.6",
          "form": "응. 한번 구경해보ㅓ",
          "original_form": "응. 한번 구경해보ㅓ",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.7",
          "form": "카카오톡 선물하기에도 있더라",
          "original_form": "카카오톡 선물하기에도 있더라",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.8",
          "form": "그렇구나 좋아",
          "original_form": "그렇구나 좋아",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.9",
          "form": "너 그럼 겨울이불을 사믄거야?",
          "original_form": "너 그럼 겨울이불을 사믄거야?",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.10",
          "form": "근데 이불이 많으면 나 하나",
          "original_form": "근데 이불이 많으면 나 하나",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.11",
          "form": "나눠주는건 어때?",
          "original_form": "나눠주는건 어때?",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.12",
          "form": "와서 사가.",
          "original_form": "와서 사가.",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.13",
          "form": "아니 사계절 차렵이불을 사서 쓸꺼야",
          "original_form": "아니 사계절 차렵이불을 사서 쓸꺼야",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.14",
          "form": "싸게 팔게",
          "original_form": "싸게 팔게",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.15",
          "form": "오만원정도",
          "original_form": "오만원정도",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.16",
          "form": "겨울에 후회할거야.",
          "original_form": "겨울에 후회할거야.",
          "speaker_id": "2",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100033749.1.17",
          "form": "흠 그정도면 당근에서도 안팔릴꺼야",
          "original_form": "흠 그정도면 당근에서도 안팔릴꺼야",
          "speaker_id": "1",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100033749.1.18",
          "form": "자취는 추운법이야.",
          "original_form": "자취는 추운법이야.",
          "speaker_id": "2",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100033749.1.19",
          "form": "겨울에는 전기장판이 있어",
          "original_form": "겨울에는 전기장판이 있어",
          "speaker_id": "1",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100033749.1.20",
          "form": "코시려.",
          "original_form": "코시려.",
          "speaker_id": "2",
          "time": "20210825 00:11"
        }
      ]
    }
