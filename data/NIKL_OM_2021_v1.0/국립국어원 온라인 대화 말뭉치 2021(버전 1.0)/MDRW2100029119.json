      "utterance": [
        {
          "id": "MDRW2100029119.1.1",
          "form": "그래서 친구가 더 개인팬화되는듯",
          "original_form": "그래서 친구가 더 개인팬화되는듯",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029119.1.2",
          "form": "정상팬들이 고통받는다고",
          "original_form": "정상팬들이 고통받는다고",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029119.1.3",
          "form": "음지 싫어하는데 그게 주류가되버려서",
          "original_form": "음지 싫어하는데 그게 주류가되버려서",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029119.1.4",
          "form": "역시 혼덕질이 최고야^_ㅜ",
          "original_form": "역시 혼덕질이 최고야^_ㅜ",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029119.1.5",
          "form": "그런거 쿨스루할수있어",
          "original_form": "그런거 쿨스루할수있어",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029119.1.6",
          "form": "ㅇㅇㅇㅇ ㄹㅇ 혼덕질이 제일행복해",
          "original_form": "ㅇㅇㅇㅇ ㄹㅇ 혼덕질이 제일행복해",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029119.1.7",
          "form": "이상한팬덤분위기",
          "original_form": "이상한팬덤분위기",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029119.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞어 혼덕질이 제일 맘편해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞어 혼덕질이 제일 맘편해",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029119.1.9",
          "form": "정보만 딱 얻고",
          "original_form": "정보만 딱 얻고",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029119.1.10",
          "form": "떡밥소화는 내가 알아서 하는게 최고",
          "original_form": "떡밥소화는 내가 알아서 하는게 최고",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029119.1.11",
          "form": "ㅇㅇㅇㅇㅇ 정말 그게 딱좋은듯",
          "original_form": "ㅇㅇㅇㅇㅇ 정말 그게 딱좋은듯",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029119.1.12",
          "form": "저도그래서요새 마음이 좀편해여",
          "original_form": "저도그래서요새 마음이 좀편해여",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029119.1.13",
          "form": "연예인덕질 아이돌덕질",
          "original_form": "연예인덕질 아이돌덕질",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
