      "utterance": [
        {
          "id": "MDRW2100020010.1.1",
          "form": "이래서 윗대가...ㄹㅣ...바껴야해요ㅋㅋㅋㅋㅋ",
          "original_form": "이래서 윗대가...ㄹㅣ...바껴야해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020010.1.2",
          "form": "귓등으로도 안들은거죠ㅋㅋㅋ전무님도 나쁜게 대리님을 좀 만만히 봤는지 보고할 때 마다 이 bird 키야 너 내가 자꾸 그런거 올리지말랬지?",
          "original_form": "귓등으로도 안들은거죠ㅋㅋㅋ전무님도 나쁜게 대리님을 좀 만만히 봤는지 보고할 때 마다 이 bird 키야 너 내가 자꾸 그런거 올리지말랬지?",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020010.1.3",
          "form": "그게 입사 몇개월차에 일어난일이죠?",
          "original_form": "그게 입사 몇개월차에 일어난일이죠?",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020010.1.4",
          "form": "라는거에요",
          "original_form": "라는거에요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020010.1.5",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020010.1.6",
          "form": "진짜 찐초기요...",
          "original_form": "진짜 찐초기요...",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020010.1.7",
          "form": "전무님인성 무슨일이져",
          "original_form": "전무님인성 무슨일이져",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020010.1.8",
          "form": "입사 이틀만에.. ㅋㅋㅋㅋ",
          "original_form": "입사 이틀만에.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020010.1.9",
          "form": "저 동공지진...",
          "original_form": "저 동공지진...",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020010.1.10",
          "form": "ㅋㅋㅋ쨌든 그러다보니 둘이 그 때쯤 서로 극에 달해있던거였고",
          "original_form": "ㅋㅋㅋ쨌든 그러다보니 둘이 그 때쯤 서로 극에 달해있던거였고",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020010.1.11",
          "form": "이틀",
          "original_form": "이틀",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020010.1.12",
          "form": "ㄷㄷㄷㄷㄷ",
          "original_form": "ㄷㄷㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020010.1.13",
          "form": "대리님 퍽발...!",
          "original_form": "대리님 퍽발...!",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020010.1.14",
          "form": "대박이다",
          "original_form": "대박이다",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020010.1.15",
          "form": "이러니까 회사가 그대로인거야!!! 이러고...",
          "original_form": "이러니까 회사가 그대로인거야!!! 이러고...",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020010.1.16",
          "form": "회사다니는거 재밋을 것 같은데요? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "회사다니는거 재밋을 것 같은데요? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020010.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ전 그 때 내가 잘못들어온건가 했어요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 그 때 내가 잘못들어온건가 했어요...",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020010.1.18",
          "form": "하지만 구경하면서 탈주각 잴것같은 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하지만 구경하면서 탈주각 잴것같은 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020010.1.19",
          "form": "그게 맞았고옄ㅋㅋㅋ",
          "original_form": "그게 맞았고옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020010.1.20",
          "form": "그러더니 대리님이 자기 책상위에 컴퓨터며 보고서들이며",
          "original_form": "그러더니 대리님이 자기 책상위에 컴퓨터며 보고서들이며",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020010.1.21",
          "form": "다 집어던지면서",
          "original_form": "다 집어던지면서",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020010.1.22",
          "form": "다른직원들한테도 니들도 똑같아!!! 이러고",
          "original_form": "다른직원들한테도 니들도 똑같아!!! 이러고",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020010.1.23",
          "form": "전무님도 쌍욕하고 나가라고 막 소리치고...",
          "original_form": "전무님도 쌍욕하고 나가라고 막 소리치고...",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020010.1.24",
          "form": "근데대리님도 아무리 쌓였어도",
          "original_form": "근데대리님도 아무리 쌓였어도",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020010.1.25",
          "form": "엄청 많이쌓이셔서그런가 ㅋㅋㅋㅋㅋ",
          "original_form": "엄청 많이쌓이셔서그런가 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020010.1.26",
          "form": "그런 회사에서 2년이나!",
          "original_form": "그런 회사에서 2년이나!",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020010.1.27",
          "form": "ㅋㅋㅋㅋㅋ그러게요...",
          "original_form": "ㅋㅋㅋㅋㅋ그러게요...",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020010.1.28",
          "form": "대리님이 가만안둔다고 얘기하고 1층에 막 대기타고있었는지 경찰이 왔었어요",
          "original_form": "대리님이 가만안둔다고 얘기하고 1층에 막 대기타고있었는지 경찰이 왔었어요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020010.1.29",
          "form": "그래서 대리님의 마지막은 그렇게 끝...",
          "original_form": "그래서 대리님의 마지막은 그렇게 끝...",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020010.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020010.1.31",
