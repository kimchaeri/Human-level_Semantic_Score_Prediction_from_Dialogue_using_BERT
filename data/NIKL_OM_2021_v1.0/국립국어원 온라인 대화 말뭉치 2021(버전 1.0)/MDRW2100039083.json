      "utterance": [
        {
          "id": "MDRW2100039083.1.1",
          "form": "저 오늘 휴가냈어요",
          "original_form": "저 오늘 휴가냈어요",
          "speaker_id": "2",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100039083.1.2",
          "form": "대박왜요",
          "original_form": "대박왜요",
          "speaker_id": "1",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100039083.1.3",
          "form": "오늘 이사해요!",
          "original_form": "오늘 이사해요!",
          "speaker_id": "2",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100039083.1.4",
          "form": "ㅇ ㅖ ....? 집이요?",
          "original_form": "ㅇ ㅖ ....? 집이요?",
          "speaker_id": "1",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100039083.1.5",
          "form": "네!!! 근데 그냥 같은 아파트 단지 내에서 해요",
          "original_form": "네!!! 근데 그냥 같은 아파트 단지 내에서 해요",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100039083.1.6",
          "form": "111동에서 109동으로",
          "original_form": "111동에서 109동으로",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100039083.1.7",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 왜요???",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 왜요???",
          "speaker_id": "1",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100039083.1.8",
          "form": "전세였거든요..^^ 집주인이 나가라면 나가야지여?",
          "original_form": "전세였거든요..^^ 집주인이 나가라면 나가야지여?",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100039083.1.9",
          "form": "저도 일 열심히해서",
          "original_form": "저도 일 열심히해서",
          "speaker_id": "2",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039083.1.10",
          "form": "아이구 글쿠나",
          "original_form": "아이구 글쿠나",
          "speaker_id": "1",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039083.1.11",
          "form": "돈많이 벌어서",
          "original_form": "돈많이 벌어서",
          "speaker_id": "2",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039083.1.12",
          "form": "집사야겠어여",
          "original_form": "집사야겠어여",
          "speaker_id": "2",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039083.1.13",
          "form": "네네 그럽시다",
          "original_form": "네네 그럽시다",
          "speaker_id": "1",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039083.1.14",
          "form": "이직하는거어때요",
          "original_form": "이직하는거어때요",
          "speaker_id": "1",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039083.1.15",
          "form": "하고싶죠 근데 지금하고있는 분야말고 다른데로 가고싶어요",
          "original_form": "하고싶죠 근데 지금하고있는 분야말고 다른데로 가고싶어요",
          "speaker_id": "2",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100039083.1.16",
          "form": "오예를들면 ?",
          "original_form": "오예를들면 ?",
          "speaker_id": "1",
          "time": "20210903 09:38"
        },
        {
          "id": "MDRW2100039083.1.17",
          "form": "저 재봉 배워보고싶어요",
          "original_form": "저 재봉 배워보고싶어요",
          "speaker_id": "2",
          "time": "20210903 09:38"
        },
        {
          "id": "MDRW2100039083.1.18",
          "form": "미싱하는 사람 어떻게 생각해요?",
          "original_form": "미싱하는 사람 어떻게 생각해요?",
          "speaker_id": "2",
          "time": "20210903 09:39"
        },
        {
          "id": "MDRW2100039083.1.19",
          "form": "예쁠거같아요",
          "original_form": "예쁠거같아요",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039083.1.20",
          "form": "ㅋㅋㅋ 재봉 그거 고되요 ~~",
          "original_form": "ㅋㅋㅋ 재봉 그거 고되요 ~~",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039083.1.21",
          "form": "손기술조으세요?",
          "original_form": "손기술조으세요?",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039083.1.22",
          "form": "저 나름.. 손은 야물딱지답니다^^",
          "original_form": "저 나름.. 손은 야물딱지답니다^^",
          "speaker_id": "2",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100039083.1.23",
          "form": "근데 그거하면 진짜 ㅋㅋㅋ목이 아플것같아요 ㅋㅋㅋ먼지도..목을 숙이는것도..",
          "original_form": "근데 그거하면 진짜 ㅋㅋㅋ목이 아플것같아요 ㅋㅋㅋ먼지도..목을 숙이는것도..",
          "speaker_id": "2",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100039083.1.24",
          "form": "오그래요?",
          "original_form": "오그래요?",
          "speaker_id": "1",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100039083.1.25",
          "form": "마자요. .. 옷공장에 먼지풀풀",
          "original_form": "마자요. .. 옷공장에 먼지풀풀",
