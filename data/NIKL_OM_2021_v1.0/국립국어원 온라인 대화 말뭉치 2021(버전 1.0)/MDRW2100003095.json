      "utterance": [
        {
          "id": "MDRW2100003095.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003095.1.2",
          "form": "안녕하세요 저녁드셨나요",
          "original_form": "안녕하세요 저녁드셨나요",
          "speaker_id": "2",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003095.1.3",
          "form": "아뇨 아직 고민중이에요 드셨나요?",
          "original_form": "아뇨 아직 고민중이에요 드셨나요?",
          "speaker_id": "1",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003095.1.4",
          "form": "곧먹으려구요",
          "original_form": "곧먹으려구요",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003095.1.5",
          "form": "생각한 메뉴있으세요?",
          "original_form": "생각한 메뉴있으세요?",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003095.1.6",
          "form": "아 저는 김밥먹을까 생각중이에요",
          "original_form": "아 저는 김밥먹을까 생각중이에요",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003095.1.7",
          "form": "콩님은 뭐드시나요?",
          "original_form": "콩님은 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003095.1.8",
          "form": "전 전복죽먹으려구요ㅎ",
          "original_form": "전 전복죽먹으려구요ㅎ",
          "speaker_id": "2",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003095.1.9",
          "form": "전복죽맛있죠ㅎㅎ",
          "original_form": "전복죽맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003095.1.10",
          "form": "네ㅎ 지금 뜨거워서 식히고있어요",
          "original_form": "네ㅎ 지금 뜨거워서 식히고있어요",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003095.1.11",
          "form": "물김치랑 같이 드셔야하는데!",
          "original_form": "물김치랑 같이 드셔야하는데!",
          "speaker_id": "1",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003095.1.12",
          "form": "어머 그건없네요ㅠ",
          "original_form": "어머 그건없네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003095.1.13",
          "form": "맛을 아시는 분이시네요",
          "original_form": "맛을 아시는 분이시네요",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003095.1.14",
          "form": "아 아쉽네요ㅜㅜㅎㅎ 사이드도 중요하게 생각합니다ㅋㅋ",
          "original_form": "아 아쉽네요ㅜㅜㅎㅎ 사이드도 중요하게 생각합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003095.1.15",
          "form": "맞아요ㅎ 사실 만두도 준비해놨어요",
          "original_form": "맞아요ㅎ 사실 만두도 준비해놨어요",
          "speaker_id": "2",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003095.1.16",
          "form": "name2님도 맛을 잘아시네요ㅎㅎ 고기인가여?",
          "original_form": "&name2&님도 맛을 잘아시네요ㅎㅎ 고기인가여?",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003095.1.17",
          "form": "모듬만두에요ㅋㅋ고기랑 새우있어요",
          "original_form": "모듬만두에요ㅋㅋ고기랑 새우있어요",
          "speaker_id": "2",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003095.1.18",
          "form": "새우ㅠㅠ 저도 급땡기네여 새우만두 낼 먹어야겠어요",
          "original_form": "새우ㅠㅠ 저도 급땡기네여 새우만두 낼 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003095.1.19",
          "form": "네 ㅎㅎ 새우좋죠",
          "original_form": "네 ㅎㅎ 새우좋죠",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003095.1.20",
