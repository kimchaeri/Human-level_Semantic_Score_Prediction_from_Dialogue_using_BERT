      "utterance": [
        {
          "id": "MDRW2100007354.1.1",
          "form": "난 게임 잘 모르지만 님이 좋아하니까 고름",
          "original_form": "난 게임 잘 모르지만 님이 좋아하니까 고름",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007354.1.2",
          "form": "게임은 안하지만 최근에 게임 원작인 영화는 봤어",
          "original_form": "게임은 안하지만 최근에 게임 원작인 영화는 봤어",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007354.1.3",
          "form": "반교 디텐션 알아?",
          "original_form": "반교 디텐션 알아?",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007354.1.4",
          "form": "아 알지",
          "original_form": "아 알지",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007354.1.5",
          "form": "반교 공포게임",
          "original_form": "반교 공포게임",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007354.1.6",
          "form": "그거 영화로도 나옴?",
          "original_form": "그거 영화로도 나옴?",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007354.1.7",
          "form": "옹 작년에 영화로 나왔어",
          "original_form": "옹 작년에 영화로 나왔어",
          "speaker_id": "1",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007354.1.8",
          "form": "반교 해봤음?",
          "original_form": "반교 해봤음?",
          "speaker_id": "1",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007354.1.9",
          "form": "오 아니 해본적은 없는데 게임방송으로 본 적 있음",
          "original_form": "오 아니 해본적은 없는데 게임방송으로 본 적 있음",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100007354.1.10",
          "form": "영화로 보니까 어땠어?",
          "original_form": "영화로 보니까 어땠어?",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100007354.1.11",
          "form": "게임방송 누구껄로 봤어? 괜찮았으면 나도 볼래",
          "original_form": "게임방송 누구껄로 봤어? 괜찮았으면 나도 볼래",
          "speaker_id": "1",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100007354.1.12",
          "form": "줄거리도 안보고 그냥 본거라 처음에 띠용 했어",
          "original_form": "줄거리도 안보고 그냥 본거라 처음에 띠용 했어",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100007354.1.13",
          "form": "대만 계엄령 있었던 거 알아? 난 첨알았어",
          "original_form": "대만 계엄령 있었던 거 알아? 난 첨알았어",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100007354.1.14",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100007354.1.15",
          "form": "나 예전에 대도서관 볼때 그때 봤던거라",
          "original_form": "나 예전에 대도서관 볼때 그때 봤던거라",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100007354.1.16",
          "form": "내용은 잘 기억 안남",
          "original_form": "내용은 잘 기억 안남",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100007354.1.17",
          "form": "그게 시간개념이 왔다갔다해서 처음엔 혼란스러웠어",
          "original_form": "그게 시간개념이 왔다갔다해서 처음엔 혼란스러웠어",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100007354.1.18",
          "form": "근데 남자선생님이랑 여학생이랑 썸...? 같이 표현해서",
          "original_form": "근데 남자선생님이랑 여학생이랑 썸...? 같이 표현해서",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100007354.1.19",
          "form": "가슴이 싸늘해짐",
          "original_form": "가슴이 싸늘해짐",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100007354.1.20",
          "form": "요즘 게임은 자빱만 봐? 자빱 재밌어",
          "original_form": "요즘 게임은 자빱만 봐? 자빱 재밌어",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100007354.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ개웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ개웃겨",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100007354.1.22",
          "form": "자빱 재밌다고 한 거야 아님 재밌냐고 묻는 거야?",
          "original_form": "자빱 재밌다고 한 거야 아님 재밌냐고 묻는 거야?",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100007354.1.23",
          "form": "사람 기분 더럽게 해 보다가 좀 떨떠름해짐",
          "original_form": "사람 기분 더럽게 해 보다가 좀 떨떠름해짐",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100007354.1.24",
          "form": "자빱 재밌다고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "자빱 재밌다고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100007354.1.25",
          "form": "마인크래프트 잘모르는데 걍 스토리텔링 재밌어서 보게 돼",
          "original_form": "마인크래프트 잘모르는데 걍 스토리텔링 재밌어서 보게 돼",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100007354.1.26",
          "form": "아하 뭐 봤어",
          "original_form": "아하 뭐 봤어",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100007354.1.27",
          "form": "우정리 뿐이야,,",
          "original_form": "우정리 뿐이야,,",
          "speaker_id": "1",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100007354.1.28",
          "form": "하지만 재밌어,, 난 게임방송 진짜 안봐서 이정도면 나한테 거진 기적임",
          "original_form": "하지만 재밌어,, 난 게임방송 진짜 안봐서 이정도면 나한테 거진 기적임",
          "speaker_id": "1",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100007354.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 인정한다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 인정한다",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100007354.1.30",
          "form": "우정리 어디까지 봤어",
          "original_form": "우정리 어디까지 봤어",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100007354.1.31",
          "form": "몰라 너무 오래되서",
          "original_form": "몰라 너무 오래되서",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100007354.1.32",
          "form": "다 끝나면 첨부터 다시 정주행 하고싶어",
          "original_form": "다 끝나면 첨부터 다시 정주행 하고싶어",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100007354.1.33",
          "form": "님도 마인크래프트 함?",
          "original_form": "님도 마인크래프트 함?",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100007354.1.34",
          "form": "하지",
          "original_form": "하지",
          "speaker_id": "2",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100007354.1.35",
          "form": "빱이같은 마크는 아니지만",
          "original_form": "빱이같은 마크는 아니지만",
          "speaker_id": "2",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100007354.1.36",
          "form": "머가.. 다른거야..?",
          "original_form": "머가.. 다른거야..?",
          "speaker_id": "1",
          "time": "20210822 18:33"
        },
        {
          "id": "MDRW2100007354.1.37",
          "form": "빱이는.. 방송용, 스토리컨텐츠용이라 원래 마크 게임에서 모드나 기타 여러가지 기능을 활용해서 만든 무대같은 거고",
          "original_form": "빱이는.. 방송용, 스토리컨텐츠용이라 원래 마크 게임에서 모드나 기타 여러가지 기능을 활용해서 만든 무대같은 거고",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100007354.1.38",
          "form": "나는 마크 게임 날것 자체로 게임함",
          "original_form": "나는 마크 게임 날것 자체로 게임함",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100007354.1.39",
          "form": "오호 걍 본인만 재밌으면 장땡",
          "original_form": "오호 걍 본인만 재밌으면 장땡",
          "speaker_id": "1",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100007354.1.40",
          "form": "심즈도 아직 해?",
          "original_form": "심즈도 아직 해?",
          "speaker_id": "1",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100007354.1.41",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100007354.1.42",
          "form": "심즈 아직 하지",
          "original_form": "심즈 아직 하지",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100007354.1.43",
          "form": "의리로",
          "original_form": "의리로",
          "speaker_id": "2",
          "time": "20210822 18:48"
        },
        {
          "id": "MDRW2100007354.1.44",
          "form": "게임을 어떻게 의리로 하지",
          "original_form": "게임을 어떻게 의리로 하지",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100007354.1.45",
          "form": "겜 안하는 놈은 이해 할 수 없다",
          "original_form": "겜 안하는 놈은 이해 할 수 없다",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100007354.1.46",
          "form": "그냥.. 오래했으니까.. 접긴 그렇고.. 계속 하는 거지",
          "original_form": "그냥.. 오래했으니까.. 접긴 그렇고.. 계속 하는 거지",
          "speaker_id": "2",
          "time": "20210823 15:41"
        },
        {
