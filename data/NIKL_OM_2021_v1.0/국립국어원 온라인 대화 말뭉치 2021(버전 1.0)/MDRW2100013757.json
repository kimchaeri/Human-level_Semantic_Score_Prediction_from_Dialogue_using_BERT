        {
          "id": "MDRW2100013757.1.1",
          "form": "수육 5조각먹었어요 ㅋㅋㅋㅋ 어제",
          "original_form": "수육 5조각먹었어요 ㅋㅋㅋㅋ 어제",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013757.1.2",
          "form": "먹다가 조금 남은거 먹은건지라",
          "original_form": "먹다가 조금 남은거 먹은건지라",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013757.1.3",
          "form": "ㅋㅋㅋ 수육맛있겠네요",
          "original_form": "ㅋㅋㅋ 수육맛있겠네요",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013757.1.4",
          "form": "저녁은 몇시쯤드시나요?",
          "original_form": "저녁은 몇시쯤드시나요?",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013757.1.5",
          "form": "저녁 보통 빠르면 6시 늦어도 7시 30분 안으론 먹어용",
          "original_form": "저녁 보통 빠르면 6시 늦어도 7시 30분 안으론 먹어용",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013757.1.6",
          "form": "저녁은 보통 뭘드시나요?",
          "original_form": "저녁은 보통 뭘드시나요?",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013757.1.7",
          "form": "그날그날마다 다른데",
          "original_form": "그날그날마다 다른데",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013757.1.8",
          "form": "주로 먹는거라면 라면?",
          "original_form": "주로 먹는거라면 라면?",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013757.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013757.1.10",
          "form": "라면어떤거 좋아하세요?",
          "original_form": "라면어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013757.1.11",
          "form": "전 진라면이랑 삼양 엄청 좋아해요!",
          "original_form": "전 진라면이랑 삼양 엄청 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013757.1.12",
          "form": "그런데 다 가리지 않고 먹어요 ㅎㅎ",
          "original_form": "그런데 다 가리지 않고 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013757.1.13",
          "form": "ㅋㅋㅋ저랑 라면취향이",
          "original_form": "ㅋㅋㅋ저랑 라면취향이",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013757.1.14",
          "form": "다르시군요?",
          "original_form": "다르시군요?",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013757.1.15",
          "form": "헉 어느 라면 좋아하시나용",
          "original_form": "헉 어느 라면 좋아하시나용",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013757.1.16",
          "form": "전 열라면 감자면 틈새라면",
          "original_form": "전 열라면 감자면 틈새라면",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013757.1.17",
          "form": "이요ㅋㅋㅋㅋ",
          "original_form": "이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013757.1.18",
          "form": "헐 감자면 아시는구나",
          "original_form": "헐 감자면 아시는구나",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013757.1.19",
          "form": "감자면 은근 모르는분 많던데",
          "original_form": "감자면 은근 모르는분 많던데",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013757.1.20",
          "form": "엄청 맛있어요",
          "original_form": "엄청 맛있어요",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013757.1.21",
          "form": "ㅋㅋㅋ 감자면 메이져라구요!!!!!!!!!!!",
          "original_form": "ㅋㅋㅋ 감자면 메이져라구요!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013757.1.22",
          "form": "우리 감자면 기죽이지마세요",
          "original_form": "우리 감자면 기죽이지마세요",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013757.1.23",
          "form": "근데 진짜 감자면 엄청 맛있어요",
          "original_form": "근데 진짜 감자면 엄청 맛있어요",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013757.1.24",
          "form": "먹은건 최근인데 요근래에 진짜 자주사먹는거같음..",
          "original_form": "먹은건 최근인데 요근래에 진짜 자주사먹는거같음..",
