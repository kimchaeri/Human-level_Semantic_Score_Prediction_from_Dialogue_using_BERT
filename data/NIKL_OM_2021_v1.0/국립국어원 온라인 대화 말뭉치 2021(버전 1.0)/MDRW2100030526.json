      "utterance": [
        {
          "id": "MDRW2100030526.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 03:42"
        },
        {
          "id": "MDRW2100030526.1.2",
          "form": "내일 뭐드세여",
          "original_form": "내일 뭐드세여",
          "speaker_id": "1",
          "time": "20210904 03:43"
        },
        {
          "id": "MDRW2100030526.1.3",
          "form": "아직 모르겠어요",
          "original_form": "아직 모르겠어요",
          "speaker_id": "2",
          "time": "20210904 03:44"
        },
        {
          "id": "MDRW2100030526.1.4",
          "form": "저희집은 중국집에서 시킬것같아요.",
          "original_form": "저희집은 중국집에서 시킬것같아요.",
          "speaker_id": "1",
          "time": "20210904 03:44"
        },
        {
          "id": "MDRW2100030526.1.5",
          "form": "중국집에서 시킬 때 어떤 메뉴로 주문하시나요?",
          "original_form": "중국집에서 시킬 때 어떤 메뉴로 주문하시나요?",
          "speaker_id": "2",
          "time": "20210904 03:45"
        },
        {
          "id": "MDRW2100030526.1.6",
          "form": "저는 볶음밥이요.",
          "original_form": "저는 볶음밥이요.",
          "speaker_id": "1",
          "time": "20210904 03:45"
        },
        {
          "id": "MDRW2100030526.1.7",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "1",
          "time": "20210904 03:45"
        },
        {
          "id": "MDRW2100030526.1.8",
          "form": "저도 최근에는 볶음밥을 골랐어요",
          "original_form": "저도 최근에는 볶음밥을 골랐어요",
          "speaker_id": "2",
          "time": "20210904 03:45"
        },
        {
          "id": "MDRW2100030526.1.9",
          "form": "국물도 줘요?",
          "original_form": "국물도 줘요?",
          "speaker_id": "1",
          "time": "20210904 03:46"
        },
        {
          "id": "MDRW2100030526.1.10",
          "form": "하지만 짜장면 주문한 사람과 조금씩 나누어 먹었죠",
          "original_form": "하지만 짜장면 주문한 사람과 조금씩 나누어 먹었죠",
          "speaker_id": "2",
          "time": "20210904 03:46"
        },
        {
          "id": "MDRW2100030526.1.11",
          "form": "짬뽕국물요?",
          "original_form": "짬뽕국물요?",
          "speaker_id": "2",
          "time": "20210904 03:46"
        },
        {
          "id": "MDRW2100030526.1.12",
          "form": "오... 네",
          "original_form": "오... 네",
          "speaker_id": "1",
          "time": "20210904 03:46"
        },
        {
          "id": "MDRW2100030526.1.13",
          "form": "정확히 기억나지는 않네요\r\n받은듯도 하고 아닌듯도 하고",
          "original_form": "정확히 기억나지는 않네요\r\n받은듯도 하고 아닌듯도 하고",
          "speaker_id": "2",
          "time": "20210904 03:47"
        },
        {
          "id": "MDRW2100030526.1.14",
          "form": "만약에 시키게되면 어떻게 나오지 궁근하네요",
          "original_form": "만약에 시키게되면 어떻게 나오지 궁근하네요",
          "speaker_id": "1",
          "time": "20210904 03:49"
        },
        {
          "id": "MDRW2100030526.1.15",
          "form": "국물 오는거 좋아하시나요",
          "original_form": "국물 오는거 좋아하시나요",
          "speaker_id": "2",
          "time": "20210904 03:50"
        },
        {
          "id": "MDRW2100030526.1.16",
          "form": "네 입가심이요",
          "original_form": "네 입가심이요",
          "speaker_id": "1",
          "time": "20210904 03:50"
        }
      ]
