      "utterance": [
        {
          "id": "MDRW2100016882.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016882.1.2",
          "form": "전 대부분 뭐든 잘먹어요",
          "original_form": "전 대부분 뭐든 잘먹어요",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016882.1.3",
          "form": "그런데 요즘 먹고픈건 훠궈에요",
          "original_form": "그런데 요즘 먹고픈건 훠궈에요",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016882.1.4",
          "form": "훠궈 맛날것같아요",
          "original_form": "훠궈 맛날것같아요",
          "speaker_id": "1",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016882.1.5",
          "form": "맛도 있지만 코로나이후엔 거의 못먹어서 더 머꼬파요",
          "original_form": "맛도 있지만 코로나이후엔 거의 못먹어서 더 머꼬파요",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016882.1.6",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016882.1.7",
          "form": "안타까운 현실",
          "original_form": "안타까운 현실",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016882.1.8",
          "form": "마주보고 한 솥에 끓이니..",
          "original_form": "마주보고 한 솥에 끓이니..",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016882.1.9",
          "form": "ㅠㅠ헐",
          "original_form": "ㅠㅠ헐",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016882.1.10",
          "form": "전 밀가루음식 다 좋아해요",
          "original_form": "전 밀가루음식 다 좋아해요",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016882.1.11",
          "form": "홈베이킹을 하시나요?",
          "original_form": "홈베이킹을 하시나요?",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016882.1.12",
          "form": "아녀 그냥 사먹어요ㅋ",
          "original_form": "아녀 그냥 사먹어요ㅋ",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016882.1.13",
          "form": "전 핫케이크 정도만 해먹어요 ㅋㅋㅋ",
          "original_form": "전 핫케이크 정도만 해먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016882.1.14",
          "form": "전 호떡이여ㅋㅋㅋ",
          "original_form": "전 호떡이여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016882.1.15",
          "form": "호떡도 홈 베이킹이죠!",
          "original_form": "호떡도 홈 베이킹이죠!",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016882.1.16",
          "form": "반죽이 부푼담에 굽자나요",
          "original_form": "반죽이 부푼담에 굽자나요",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016882.1.17",
          "form": "뽀끼도 해요ㅋ",
          "original_form": "뽀끼도 해요ㅋ",
          "speaker_id": "1",
          "time": "20210826 16:08"
        }
      ]
