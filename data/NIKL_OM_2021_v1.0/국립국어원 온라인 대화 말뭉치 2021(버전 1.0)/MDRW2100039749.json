      "utterance": [
        {
          "id": "MDRW2100039749.1.1",
          "form": "최근에 사신거있나요?",
          "original_form": "최근에 사신거있나요?",
          "speaker_id": "1",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039749.1.2",
          "form": "오늘 집 돌아오는 길에 노트북 파우치를 사왔어요!",
          "original_form": "오늘 집 돌아오는 길에 노트북 파우치를 사왔어요!",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039749.1.3",
          "form": "역 근처에 아름다운 가게같이 물건 기증받아서 파는 가게가 있어서",
          "original_form": "역 근처에 아름다운 가게같이 물건 기증받아서 파는 가게가 있어서",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039749.1.4",
          "form": "싸게 잘 사긴했는데 디자인이 정말 애매해요ㅋㅋㅋ",
          "original_form": "싸게 잘 사긴했는데 디자인이 정말 애매해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039749.1.5",
          "form": "와 좋은데서 사셨네요",
          "original_form": "와 좋은데서 사셨네요",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039749.1.6",
          "form": "디자인이 별루예요?",
          "original_form": "디자인이 별루예요?",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039749.1.7",
          "form": "네ㅋㅋㅋ 그림이 별로 안예쁜데",
          "original_form": "네ㅋㅋㅋ 그림이 별로 안예쁜데",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039749.1.8",
          "form": "기능과 가격에 반해서 샀어요!",
          "original_form": "기능과 가격에 반해서 샀어요!",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039749.1.9",
          "form": "얼마전에 노트북 경첩 깨뜨려서",
          "original_form": "얼마전에 노트북 경첩 깨뜨려서",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039749.1.10",
          "form": "ㅎㅎ 그래도 가성비니까 잘 사셨네요",
          "original_form": "ㅎㅎ 그래도 가성비니까 잘 사셨네요",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039749.1.11",
          "form": "헉 수리 맡기셨어요?",
          "original_form": "헉 수리 맡기셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039749.1.12",
          "form": "쓰다가 나중에 예쁜거 있으면 새로사야겠어요ㅋㅋ",
          "original_form": "쓰다가 나중에 예쁜거 있으면 새로사야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039749.1.13",
          "form": "넵ㅋㅋㅋ 경첩 갈지는 않고",
          "original_form": "넵ㅋㅋㅋ 경첩 갈지는 않고",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039749.1.14",
          "form": "기본 점검으로 어긋난데만 맞춰왔어요",
          "original_form": "기본 점검으로 어긋난데만 맞춰왔어요",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039749.1.15",
          "form": "오ᆢㄷㄷㄷ 노트북 비싼데ㅜㅜ",
          "original_form": "오ᆢㄷㄷㄷ 노트북 비싼데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039749.1.16",
          "form": "테이프로 잘 붙여서 쓰고있어요",
          "original_form": "테이프로 잘 붙여서 쓰고있어요",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039749.1.17",
          "form": "안타까워요",
          "original_form": "안타까워요",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039749.1.18",
          "form": "그래도 오래된 기종이라",
          "original_form": "그래도 오래된 기종이라",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039749.1.19",
          "form": "좀만 더 쓰면 얘도 천수를 누린게 될 것 같아요",
          "original_form": "좀만 더 쓰면 얘도 천수를 누린게 될 것 같아요",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039749.1.20",
          "form": "아 새로 사시는건 바둔 모델있으세요?",
          "original_form": "아 새로 사시는건 바둔 모델있으세요?",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039749.1.21",
          "form": "제 주위에선 그램 많이 쓰는데",
          "original_form": "제 주위에선 그램 많이 쓰는데",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039749.1.22",
          "form": "그램 많이쓰죠",
          "original_form": "그램 많이쓰죠",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039749.1.23",
          "form": "저는 좀 더 무겁고 두꺼워도 되니까",
          "original_form": "저는 좀 더 무겁고 두꺼워도 되니까",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039749.1.24",
          "form": "성능이 좋았으면 좋겠어요!",
          "original_form": "성능이 좋았으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039749.1.25",
          "form": "역시 기계는 성능이 최고죠",
          "original_form": "역시 기계는 성능이 최고죠",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039749.1.26",
          "form": "그런데 그걸 만족하는 게이밍 노트북같은건 너무 비싸서ㅠㅠ",
          "original_form": "그런데 그걸 만족하는 게이밍 노트북같은건 너무 비싸서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039749.1.27",
          "form": "게임용은 다 비싸요ㅜㅜ",
          "original_form": "게임용은 다 비싸요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039749.1.28",
          "form": "노트북 찾다가 옆길로 새서 태블릿을 찾아보고 있어요",
          "original_form": "노트북 찾다가 옆길로 새서 태블릿을 찾아보고 있어요",
          "speaker_id": "2",
          "time": "20210903 21:18"
        }
      ]
    }
