      "utterance": [
        {
          "id": "MDRW2100008211.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008211.1.2",
          "form": "쇼핑좋아요 ㅎㅎ",
          "original_form": "쇼핑좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008211.1.3",
          "form": "요새 전 주로 식품만 사는것 같아요",
          "original_form": "요새 전 주로 식품만 사는것 같아요",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008211.1.4",
          "form": "뭐 많이 사세요?",
          "original_form": "뭐 많이 사세요?",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008211.1.5",
          "form": "전 최근에 아쿠아슈즈 샀어요",
          "original_form": "전 최근에 아쿠아슈즈 샀어요",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008211.1.6",
          "form": "근데 좀 실망ㅜ",
          "original_form": "근데 좀 실망ㅜ",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008211.1.7",
          "form": "아 잘못사면 실망이죠",
          "original_form": "아 잘못사면 실망이죠",
          "speaker_id": "2",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008211.1.8",
          "form": "전 사이즈가 넘작게 나와서",
          "original_form": "전 사이즈가 넘작게 나와서",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008211.1.9",
          "form": "저도 실패요",
          "original_form": "저도 실패요",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008211.1.10",
          "form": "디자인이 별로던가요?",
          "original_form": "디자인이 별로던가요?",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008211.1.11",
          "form": "음 발수능력? 좋다더니",
          "original_form": "음 발수능력? 좋다더니",
          "speaker_id": "1",
          "time": "20210822 14:11"
        },
        {
          "id": "MDRW2100008211.1.12",
          "form": "너무 안말라서ㅜㅜ",
          "original_form": "너무 안말라서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 14:11"
        },
        {
          "id": "MDRW2100008211.1.13",
          "form": "아 그럼 그냥 심발이죠",
          "original_form": "아 그럼 그냥 심발이죠",
          "speaker_id": "2",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008211.1.14",
          "form": "신발",
          "original_form": "신발",
          "speaker_id": "2",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008211.1.15",
          "form": "어찌 물에서 신어요",
          "original_form": "어찌 물에서 신어요",
          "speaker_id": "2",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100008211.1.16",
          "form": "진짜 놀러갔다 낭패였겠어요",
          "original_form": "진짜 놀러갔다 낭패였겠어요",
          "speaker_id": "2",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008211.1.17",
          "form": "반품이 신어서 안되죠?",
          "original_form": "반품이 신어서 안되죠?",
          "speaker_id": "2",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008211.1.18",
          "form": "이럴때 난감해요",
          "original_form": "이럴때 난감해요",
          "speaker_id": "2",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008211.1.19",
          "form": "바로 천이 손상? 되서",
          "original_form": "바로 천이 손상? 되서",
          "speaker_id": "1",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008211.1.20",
          "form": "응? 했어요 ㅋㅋㅋ",
          "original_form": "응? 했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008211.1.21",
          "form": "오 다행이예요",
          "original_form": "오 다행이예요",
          "speaker_id": "2",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008211.1.22",
          "form": "전 반품 잘못해요 ㅜㅜ",
          "original_form": "전 반품 잘못해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008211.1.23",
          "form": "전 프린터 샀는데 요샌 가전 제품 하자여도 as가서 확인돼야 새로준데요",
          "original_form": "전 프린터 샀는데 요샌 가전 제품 하자여도 as가서 확인돼야 새로준데요",
          "speaker_id": "2",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008211.1.24",
          "form": "참나 그래서 일주일 기다려서 고쳤네요",
          "original_form": "참나 그래서 일주일 기다려서 고쳤네요",
          "speaker_id": "2",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008211.1.25",
          "form": "그날 학교과제 애가 내야 해서 산건데",
          "original_form": "그날 학교과제 애가 내야 해서 산건데",
          "speaker_id": "2",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008211.1.26",
          "form": "괜히 샀어요",
          "original_form": "괜히 샀어요",
          "speaker_id": "2",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008211.1.27",
          "form": "혹시 머지포인트는 서셨어요?",
          "original_form": "혹시 머지포인트는 서셨어요?",
          "speaker_id": "2",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008211.1.28",
          "form": "사셨어요?",
          "original_form": "사셨어요?",
          "speaker_id": "2",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008211.1.29",
          "form": "요새 난리던데",
          "original_form": "요새 난리던데",
          "speaker_id": "2",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008211.1.30",
          "form": "전 샀었어요 ㅡㅡ",
          "original_form": "전 샀었어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 14:30"
        },
        {
          "id": "MDRW2100008211.1.31",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008211.1.32",
          "form": "머지포인트 어떻게하셨어요?ㅜㅜ",
          "original_form": "머지포인트 어떻게하셨어요?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008211.1.33",
          "form": "저는 지방이라서",
          "original_form": "저는 지방이라서",
          "speaker_id": "1",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008211.1.34",
          "form": "제휴브랜드가 잘없어서 안샀는데ㅜ",
          "original_form": "제휴브랜드가 잘없어서 안샀는데ㅜ",
          "speaker_id": "1",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008211.1.35",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008211.1.36",
          "form": "그냥 조금남아서",
          "original_form": "그냥 조금남아서",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008211.1.37",
          "form": "잊고살아요",
          "original_form": "잊고살아요",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008211.1.38",
          "form": "앱도 싹 지웠어요",
          "original_form": "앱도 싹 지웠어요",
          "speaker_id": "2",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008211.1.39",
          "form": "전 그전에 너무 많이 팔길래 싸해서 많이써서 다행이였어요",
          "original_form": "전 그전에 너무 많이 팔길래 싸해서 많이써서 다행이였어요",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008211.1.40",
          "form": "와 진짜 그래도 다행이네요 천만원씩 산 사람도 있다던데ㅜ",
          "original_form": "와 진짜 그래도 다행이네요 천만원씩 산 사람도 있다던데ㅜ",
          "speaker_id": "1",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008211.1.41",
          "form": "그사람은 진짜 뭔생각으오",
          "original_form": "그사람은 진짜 뭔생각으오",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008211.1.42",
          "form": "프린터기는 삼성이나 엘지 였나요?",
          "original_form": "프린터기는 삼성이나 엘지 였나요?",
          "speaker_id": "1",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008211.1.43",
          "form": "그게 외식빼곤 쓸때도 없단데",
          "original_form": "그게 외식빼곤 쓸때도 없단데",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008211.1.44",
          "form": "아뇨 ㅜㅜ",
          "original_form": "아뇨 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008211.1.45",
          "form": "괜히 앱손을 샀어요",
          "original_form": "괜히 앱손을 샀어요",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008211.1.46",
          "form": "삼성이 as는좋은데",
          "original_form": "삼성이 as는좋은데",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008211.1.47",
          "form": "근데 맘카페에서 입소문 타서 50만원 정도 산 주부들은 좀 있는것같았어요",
          "original_form": "근데 맘카페에서 입소문 타서 50만원 정도 산 주부들은 좀 있는것같았어요",
          "speaker_id": "1",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008211.1.48",
          "form": "금방고장나서 딴거 산건데",
          "original_form": "금방고장나서 딴거 산건데",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008211.1.49",
          "form": "네 저도 좀 샀었어요 ㅜㅜ",
          "original_form": "네 저도 좀 샀었어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008211.1.50",
          "form": "에효 제품운도 좀  있더라고요",
          "original_form": "에효 제품운도 좀  있더라고요",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008211.1.51",
          "form": "그게 20만원 단위로 행사라",
          "original_form": "그게 20만원 단위로 행사라",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008211.1.52",
          "form": "아 좋으시겠어요",
          "original_form": "아 좋으시겠어요",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008211.1.53",
          "form": "전 없어요",
          "original_form": "전 없어요",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008211.1.54",
          "form": "항시 고장나요",
          "original_form": "항시 고장나요",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008211.1.55",
          "form": "할인소식!",
          "original_form": "할인소식!",
          "speaker_id": "1",
          "time": "20210823 18:45"
        }
      ]
