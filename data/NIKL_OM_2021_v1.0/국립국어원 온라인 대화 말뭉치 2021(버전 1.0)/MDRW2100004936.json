        },
        {
          "id": "MDRW2100004936.1.2",
          "form": "빌리엘리어트 예매해둔거 취소돼서",
          "original_form": "빌리엘리어트 예매해둔거 취소돼서",
          "speaker_id": "1",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100004936.1.3",
          "form": "너무슬퍼",
          "original_form": "너무슬퍼",
          "speaker_id": "1",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100004936.1.4",
          "form": "헐 빌리가 취소되다니...",
          "original_form": "헐 빌리가 취소되다니...",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100004936.1.5",
          "form": "너무 슬픈일이다...",
          "original_form": "너무 슬픈일이다...",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100004936.1.6",
          "form": "그거 계속 기대했는데",
          "original_form": "그거 계속 기대했는데",
          "speaker_id": "1",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100004936.1.7",
          "form": "그러니까ㅠ",
          "original_form": "그러니까ㅠ",
          "speaker_id": "1",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100004936.1.8",
          "form": "ㅠㅠ코로나 이자식ㅜㅜㅜ",
          "original_form": "ㅠㅠ코로나 이자식ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100004936.1.9",
          "form": "예매 다시 해봐야지ㅜㅜ",
          "original_form": "예매 다시 해봐야지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100004936.1.10",
          "form": "가만두지않겠어",
          "original_form": "가만두지않겠어",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100004936.1.11",
          "form": "보고말겠어",
          "original_form": "보고말겠어",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100004936.1.12",
          "form": "꼭 볼수있길!!",
          "original_form": "꼭 볼수있길!!",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100004936.1.13",
          "form": "맞아 가만두지않겠어",
          "original_form": "맞아 가만두지않겠어",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100004936.1.14",
          "form": "전시는",
          "original_form": "전시는",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100004936.1.15",
