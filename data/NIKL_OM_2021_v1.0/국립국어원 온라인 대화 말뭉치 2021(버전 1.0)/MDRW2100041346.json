      "utterance": [
        {
          "id": "MDRW2100041346.1.1",
          "form": "먹거리 쇼핑이 어려워요",
          "original_form": "먹거리 쇼핑이 어려워요",
          "speaker_id": "1",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100041346.1.2",
          "form": "전 아무거나 먹지만 강아지에겐 엄격",
          "original_form": "전 아무거나 먹지만 강아지에겐 엄격",
