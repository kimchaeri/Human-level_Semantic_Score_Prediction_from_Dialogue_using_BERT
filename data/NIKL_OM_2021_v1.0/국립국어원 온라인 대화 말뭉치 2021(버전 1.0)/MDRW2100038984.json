      "utterance": [
        {
          "id": "MDRW2100038984.1.1",
          "form": "농담으로 체포?",
          "original_form": "농담으로 체포?",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038984.1.2",
          "form": "이 농담으로 화성연쇄살인 사건 누명을",
          "original_form": "이 농담으로 화성연쇄살인 사건 누명을",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038984.1.3",
          "form": "말을 조심해야한다",
          "original_form": "말을 조심해야한다",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038984.1.4",
          "form": "다방에서 직원에게 농담을 던졌다네",
          "original_form": "다방에서 직원에게 농담을 던졌다네",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100038984.1.5",
          "form": "의심받을 말을 했겠지만",
          "original_form": "의심받을 말을 했겠지만",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100038984.1.6",
          "form": "그러게 이게 살인의 추억 영화",
          "original_form": "그러게 이게 살인의 추억 영화",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100038984.1.7",
          "form": "엉 맞아 송강호 연기 좋았는데",
          "original_form": "엉 맞아 송강호 연기 좋았는데",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100038984.1.8",
          "form": "빨간옷을 입으면 죽는데 이게 정답",
          "original_form": "빨간옷을 입으면 죽는데 이게 정답",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100038984.1.9",
          "form": "이사건 경찰투입이 20만영",
          "original_form": "이사건 경찰투입이 20만영",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100038984.1.10",
          "form": "진짜 많은 사람들이 피해를 입었네",
          "original_form": "진짜 많은 사람들이 피해를 입었네",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100038984.1.11",
          "form": "그러게 여론이 어마어마했음 그당시",
          "original_form": "그러게 여론이 어마어마했음 그당시",
