      "utterance": [
        {
          "id": "MDRW2100019044.1.1",
          "form": "작은 아들 티셔츠 도착했다 ㅎㅎ",
          "original_form": "작은 아들 티셔츠 도착했다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019044.1.2",
          "form": "이번엔 잘 맞아?",
          "original_form": "이번엔 잘 맞아?",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019044.1.3",
          "form": "ㅇㅇ 다행히 잘 맞네 ㅋ",
          "original_form": "ㅇㅇ 다행히 잘 맞네 ㅋ",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019044.1.4",
          "form": "본의 아니게 큰 아들이랑 커플티 됐네",
          "original_form": "본의 아니게 큰 아들이랑 커플티 됐네",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019044.1.5",
          "form": "그러게 말야 ㅎㅎ 저번에 산거 너무 커서 어쩔 수 없이 큰아들이 입네",
          "original_form": "그러게 말야 ㅎㅎ 저번에 산거 너무 커서 어쩔 수 없이 큰아들이 입네",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019044.1.6",
          "form": "둘이 똑같은 옷 입고 좋지 뭐~",
          "original_form": "둘이 똑같은 옷 입고 좋지 뭐~",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019044.1.7",
          "form": "옷장이 하나라 헷갈리겠어",
          "original_form": "옷장이 하나라 헷갈리겠어",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019044.1.8",
          "form": "옷 갈아 입을 때 섞이고 난리나겠지 ㅋㅋ",
          "original_form": "옷 갈아 입을 때 섞이고 난리나겠지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019044.1.9",
          "form": "면 재질이 좋아서 편하게 입을 수 있겠어",
          "original_form": "면 재질이 좋아서 편하게 입을 수 있겠어",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019044.1.10",
          "form": "ㅇㅇ 한 번 세탁하고 입혀요",
          "original_form": "ㅇㅇ 한 번 세탁하고 입혀요",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019044.1.11",
          "form": "당연하지융~ 새 옷이라 냄새가 코를 찔러 ㅎㅎ",
          "original_form": "당연하지융~ 새 옷이라 냄새가 코를 찔러 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019044.1.12",
          "form": "ㅎㅎ 저번에 보니 냄새 진짜 고약하더라",
          "original_form": "ㅎㅎ 저번에 보니 냄새 진짜 고약하더라",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019044.1.13",
          "form": "꼭 세탁해서 입어야 함",
          "original_form": "꼭 세탁해서 입어야 함",
          "speaker_id": "2",
          "time": "20210827 14:54"
