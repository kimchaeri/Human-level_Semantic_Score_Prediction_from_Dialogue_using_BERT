      "utterance": [
        {
          "id": "MDRW2100001706.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001706.1.2",
          "form": "안녕하세요 요즘 운동 하시나요?",
          "original_form": "안녕하세요 요즘 운동 하시나요?",
          "speaker_id": "2",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001706.1.3",
          "form": "요즘 헬스다녀요",
          "original_form": "요즘 헬스다녀요",
          "speaker_id": "1",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001706.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001706.1.5",
          "form": "저는 소도구 필라테스요!",
          "original_form": "저는 소도구 필라테스요!",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001706.1.6",
          "form": "헬스는 혼자하세요??",
          "original_form": "헬스는 혼자하세요??",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001706.1.7",
          "form": "네네 필라테스 생각중인데 필라테스 중 어떤 운동주로 하세요?",
          "original_form": "네네 필라테스 생각중인데 필라테스 중 어떤 운동주로 하세요?",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001706.1.8",
          "form": "저는 작은 볼로 하는 운동이 엄청 효과있더라구요!",
          "original_form": "저는 작은 볼로 하는 운동이 엄청 효과있더라구요!",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001706.1.9",
          "form": "하고나면 다리가 후덜덜해요",
          "original_form": "하고나면 다리가 후덜덜해요",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001706.1.10",
          "form": "그거 생각보다 힘 많이 들어보였어요",
          "original_form": "그거 생각보다 힘 많이 들어보였어요",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001706.1.11",
          "form": "유튜브로 봤을때",
          "original_form": "유튜브로 봤을때",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001706.1.12",
          "form": "근데 또 운동은 하고나면 오는 근육통에",
          "original_form": "근데 또 운동은 하고나면 오는 근육통에",
          "speaker_id": "2",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001706.1.13",
          "form": "뿌듯함을 느끼기도 하니까요 ㅎ",
          "original_form": "뿌듯함을 느끼기도 하니까요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001706.1.14",
          "form": "운동은 다이어트목적으로 하시나요?",
          "original_form": "운동은 다이어트목적으로 하시나요?",
          "speaker_id": "1",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001706.1.15",
          "form": "아니요 그냥 안하면 죽을 것 같아서 조금씩 다녀요 ㅎ",
          "original_form": "아니요 그냥 안하면 죽을 것 같아서 조금씩 다녀요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100001706.1.16",
          "form": "어떤 목적으로 하세요?",
          "original_form": "어떤 목적으로 하세요?",
          "speaker_id": "2",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100001706.1.17",
          "form": "저도 그런 의무적인 느낌이에요",
          "original_form": "저도 그런 의무적인 느낌이에요",
          "speaker_id": "1",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100001706.1.18",
          "form": "그죠 의무적으로 안하면",
          "original_form": "그죠 의무적으로 안하면",
          "speaker_id": "2",
          "time": "20210819 19:27"
        },
        {
          "id": "MDRW2100001706.1.19",
          "form": "정말 움직일일이 없더라구요",
          "original_form": "정말 움직일일이 없더라구요",
          "speaker_id": "2",
          "time": "20210819 19:27"
        },
        {
          "id": "MDRW2100001706.1.20",
          "form": "특히 코로나시국이라",
          "original_form": "특히 코로나시국이라",
          "speaker_id": "1",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100001706.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100001706.1.22",
          "form": "살만 찌고...배달음식을 많이 먹으니까",
          "original_form": "살만 찌고...배달음식을 많이 먹으니까",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100001706.1.23",
          "form": "건강에도 안좋은 것 같아요ㅠ",
          "original_form": "건강에도 안좋은 것 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:19"
        }
      ]
