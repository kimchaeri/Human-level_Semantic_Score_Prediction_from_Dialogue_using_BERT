        {
          "id": "MDRW2100046145.1.2",
          "form": "머먹었댔지",
          "original_form": "머먹었댔지",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046145.1.3",
          "form": "족발",
          "original_form": "족발",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046145.1.4",
          "form": "족발만??",
          "original_form": "족발만??",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046145.1.5",
          "form": "막국수도먹었지",
          "original_form": "막국수도먹었지",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046145.1.6",
          "form": "존맛예상...맛집임?",
          "original_form": "존맛예상...맛집임?",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046145.1.7",
          "form": "ㄴㄴ 저번에 당면준곳이 더 맛집 닫아서 아쉬워",
          "original_form": "ㄴㄴ 저번에 당면준곳이 더 맛집 닫아서 아쉬워",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046145.1.8",
          "form": "아깝다 당면도줘? 우리는 국수줘 비빔국수 들기름국수같은거",
          "original_form": "아깝다 당면도줘? 우리는 국수줘 비빔국수 들기름국수같은거",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046145.1.9",
          "form": "아 그건 물론 다로주고 당면은 족발위에올려줘",
          "original_form": "아 그건 물론 다로주고 당면은 족발위에올려줘",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046145.1.10",
          "form": "완전조아",
          "original_form": "완전조아",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046145.1.11",
          "form": "담에먹으실?",
          "original_form": "담에먹으실?",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046145.1.12",
          "form": "우리는 그 머지 파채? 양파랑 그게유명",
          "original_form": "우리는 그 머지 파채? 양파랑 그게유명",
