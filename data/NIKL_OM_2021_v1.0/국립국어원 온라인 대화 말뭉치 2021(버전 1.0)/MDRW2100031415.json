      "utterance": [
        {
          "id": "MDRW2100031415.1.1",
          "form": "좋아하는 프로그램 있으세요?",
          "original_form": "좋아하는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031415.1.2",
          "form": "저 슬의생이요!",
          "original_form": "저 슬의생이요!",
          "speaker_id": "2",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100031415.1.3",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "2",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100031415.1.4",
          "form": "슬의생 재밌죠",
          "original_form": "슬의생 재밌죠",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031415.1.5",
          "form": "매번 본방 챙겨보세요?",
          "original_form": "매번 본방 챙겨보세요?",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100031415.1.6",
          "form": "아뇨 거의 재방으로 보는 것 같아요ㅋㅋㅋㅋ 거의 매번?",
          "original_form": "아뇨 거의 재방으로 보는 것 같아요ㅋㅋㅋㅋ 거의 매번?",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100031415.1.7",
          "form": "아 재방아니구나",
          "original_form": "아 재방아니구나",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100031415.1.8",
          "form": "넷플릭스로 봐요 ㅋㅋㅋ",
          "original_form": "넷플릭스로 봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100031415.1.9",
          "form": "제시간에 보기 힘들죠",
          "original_form": "제시간에 보기 힘들죠",
          "speaker_id": "1",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100031415.1.10",
          "form": "넷플에도 슬의생 있나봐요?",
          "original_form": "넷플에도 슬의생 있나봐요?",
          "speaker_id": "1",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100031415.1.11",
          "form": "네! 거의 넷플로 봐요 ㅎㅎㅎ name1님은 어떤 프로 좋아하세요?",
          "original_form": "네! 거의 넷플로 봐요 ㅎㅎㅎ &name1&님은 어떤 프로 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100031415.1.12",
          "form": "유퀴즈요 ㅎㅎ",
          "original_form": "유퀴즈요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:52"
        },
        {
          "id": "MDRW2100031415.1.13",
          "form": "요즘 사실 거의 티비를 안보는데",
          "original_form": "요즘 사실 거의 티비를 안보는데",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031415.1.14",
          "form": "이것만 가끔봐요",
          "original_form": "이것만 가끔봐요",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031415.1.15",
          "form": "저도 유퀴즈 좋아요!! 넷플에 없어가지구 ㅠㅠㅠㅠ",
          "original_form": "저도 유퀴즈 좋아요!! 넷플에 없어가지구 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100031415.1.16",
          "form": "어쩌다가 틀면 10분 남아있고 ㅠㅠ",
          "original_form": "어쩌다가 틀면 10분 남아있고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031415.1.17",
          "form": "그래서 제대로 본 적이 별로 없어요",
          "original_form": "그래서 제대로 본 적이 별로 없어요",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031415.1.18",
          "form": "가끔은 그냥 유투브 클립 잘린거 볼때도 있어요",
          "original_form": "가끔은 그냥 유투브 클립 잘린거 볼때도 있어요",
          "speaker_id": "1",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031415.1.19",
          "form": "저도 그래서 그 방법을 애용해요 ㅋㅋㅋ",
          "original_form": "저도 그래서 그 방법을 애용해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:54"
        },
        {
          "id": "MDRW2100031415.1.20",
          "form": "그쵸 요즘 사실 티비보다 유투브를 더봐요",
          "original_form": "그쵸 요즘 사실 티비보다 유투브를 더봐요",
          "speaker_id": "1",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031415.1.21",
          "form": "맞아요 진짜 예전엔 티비만 봤었는데",
          "original_form": "맞아요 진짜 예전엔 티비만 봤었는데",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031415.1.22",
          "form": "스마트폰 있었어도 티비를 더 많이 봤는데",
          "original_form": "스마트폰 있었어도 티비를 더 많이 봤는데",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031415.1.23",
          "form": "이제는 다들 유튜브를 더 많이 보는 것 같아요",
          "original_form": "이제는 다들 유튜브를 더 많이 보는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031415.1.24",
          "form": "영상보는것도 원하는 시간에 보는게 편하죠",
