      "utterance": [
        {
          "id": "MDRW2100032035.1.1",
          "form": "제주도 맛집입니까",
          "original_form": "제주도 맛집입니까",
          "speaker_id": "2",
          "time": "20210905 11:17"
        },
        {
          "id": "MDRW2100032035.1.2",
          "form": "그래도 남편과 매일 같이 데이트를 한다니 좋겠네요",
          "original_form": "그래도 남편과 매일 같이 데이트를 한다니 좋겠네요",
          "speaker_id": "2",
          "time": "20210905 11:17"
        },
        {
          "id": "MDRW2100032035.1.3",
          "form": "누구랑 같이 사시나요",
          "original_form": "누구랑 같이 사시나요",
          "speaker_id": "1",
          "time": "20210905 11:17"
        },
        {
          "id": "MDRW2100032035.1.4",
          "form": "조금 떨어져 있으면 좋겠어요",
          "original_form": "조금 떨어져 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210905 11:17"
        },
        {
          "id": "MDRW2100032035.1.5",
          "form": "남편 분이랑 차이가 많이 좋으신가 봐요",
          "original_form": "남편 분이랑 차이가 많이 좋으신가 봐요",
          "speaker_id": "2",
          "time": "20210905 11:17"
        },
        {
          "id": "MDRW2100032035.1.6",
          "form": "사이가 많이",
          "original_form": "사이가 많이",
          "speaker_id": "2",
          "time": "20210905 11:17"
        },
        {
          "id": "MDRW2100032035.1.7",
          "form": "절친입니다",
          "original_form": "절친입니다",
          "speaker_id": "1",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.8",
          "form": "남편분이 좋아하시죠",
          "original_form": "남편분이 좋아하시죠",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.9",
          "form": "항상 내 편을 줄 수 있는 사람이 곁에 있다는 거는 정말 좋은 일이지요",
          "original_form": "항상 내 편을 줄 수 있는 사람이 곁에 있다는 거는 정말 좋은 일이지요",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.10",
          "form": "함께 하는 걸 좋아해요",
          "original_form": "함께 하는 걸 좋아해요",
          "speaker_id": "1",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.11",
          "form": "있으면 귀찮고 없으면 허전하다고 하지요",
          "original_form": "있으면 귀찮고 없으면 허전하다고 하지요",
          "speaker_id": "1",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.12",
          "form": "강아지가 드시겠어요",
          "original_form": "강아지가 드시겠어요",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.13",
          "form": "한 번씩 놀러 가면 이쁩니다",
          "original_form": "한 번씩 놀러 가면 이쁩니다",
          "speaker_id": "1",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.14",
          "form": "그걸 소리내서 말로 하면 듣고 있지 않나요",
          "original_form": "그걸 소리내서 말로 하면 듣고 있지 않나요",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.15",
          "form": "남편분 표정이 어떻지요",
          "original_form": "남편분 표정이 어떻지요",
          "speaker_id": "2",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.16",
          "form": "괜찮아요 무시 하면 됩니다 제가 늘 이러거든요",
          "original_form": "괜찮아요 무시 하면 됩니다 제가 늘 이러거든요",
          "speaker_id": "1",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100032035.1.17",
          "form": "요즘엔 9분 알아서 어디 놀러 가지도 못 하잖아요 딱 붙어 있으시겠네요",
          "original_form": "요즘엔 9분 알아서 어디 놀러 가지도 못 하잖아요 딱 붙어 있으시겠네요",
          "speaker_id": "2",
          "time": "20210905 11:19"
        },
        {
          "id": "MDRW2100032035.1.18",
          "form": "토익 한다고 리스닝 하나 봐요 다행이죠",
          "original_form": "토익 한다고 리스닝 하나 봐요 다행이죠",
          "speaker_id": "1",
          "time": "20210905 11:19"
        },
        {
          "id": "MDRW2100032035.1.19",
          "form": "정말 천만다행이네요 상처 받을 수도 있잖아요",
          "original_form": "정말 천만다행이네요 상처 받을 수도 있잖아요",
          "speaker_id": "2",
          "time": "20210905 11:19"
        },
        {
          "id": "MDRW2100032035.1.20",
          "form": "어제는 친구랑 드라이브 가더라고요 기뻤지요",
          "original_form": "어제는 친구랑 드라이브 가더라고요 기뻤지요",
          "speaker_id": "1",
          "time": "20210905 11:19"
        },
        {
          "id": "MDRW2100032035.1.21",
          "form": "조금 더 소중하게 대해 주시죠",
          "original_form": "조금 더 소중하게 대해 주시죠",
          "speaker_id": "2",
          "time": "20210905 11:19"
        }
