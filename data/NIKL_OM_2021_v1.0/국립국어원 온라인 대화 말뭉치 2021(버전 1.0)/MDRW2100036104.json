      "utterance": [
        {
          "id": "MDRW2100036104.1.1",
          "form": "보시는 프로그램 있으세요?",
          "original_form": "보시는 프로그램 있으세요?",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036104.1.2",
          "form": "저는 심야괴담회 자주 봐요호",
          "original_form": "저는 심야괴담회 자주 봐요호",
          "speaker_id": "1",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036104.1.3",
          "form": "으스스한거 잘봅니당",
          "original_form": "으스스한거 잘봅니당",
          "speaker_id": "1",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036104.1.4",
          "form": "오 구러면 공포영화도 잘 보세요?",
          "original_form": "오 구러면 공포영화도 잘 보세요?",
          "speaker_id": "2",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100036104.1.5",
          "form": "넹넹ㅋㅋㅋ 근데 심야괴담회는 실화 위주라",
          "original_form": "넹넹ㅋㅋㅋ 근데 심야괴담회는 실화 위주라",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100036104.1.6",
          "form": "작위적인 공포영화는 자주는 안봐용",
          "original_form": "작위적인 공포영화는 자주는 안봐용",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100036104.1.7",
          "form": "실화바탕 좋아하시는구나",
          "original_form": "실화바탕 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036104.1.8",
          "form": "네네 name2님은 뭐 보세요?",
          "original_form": "네네 &name2&님은 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036104.1.9",
          "form": "저는 무서운 거 잘 못 보는데 엄청 궁금해는 해요",
          "original_form": "저는 무서운 거 잘 못 보는데 엄청 궁금해는 해요",
          "speaker_id": "2",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036104.1.10",
          "form": "저는 놀토 꼭 챙겨봐요",
          "original_form": "저는 놀토 꼭 챙겨봐요",
          "speaker_id": "2",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036104.1.11",
          "form": "놀토 멤버들 케미 좋더라구요ㅋㅋㅋㅋ",
          "original_form": "놀토 멤버들 케미 좋더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:31"
        },
        {
          "id": "MDRW2100036104.1.12",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "1",
          "time": "20210830 16:31"
        },
        {
          "id": "MDRW2100036104.1.13",
          "form": "맞아요 멤버들 바뀌었어도 케미는 안 죽어서 넘 좋아요",
          "original_form": "맞아요 멤버들 바뀌었어도 케미는 안 죽어서 넘 좋아요",
          "speaker_id": "2",
          "time": "20210830 16:32"
        },
        {
          "id": "MDRW2100036104.1.14",
          "form": "한해가 은근히 웃기더라구요ㅋㅋㅋ",
          "original_form": "한해가 은근히 웃기더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100036104.1.15",
          "form": "키랑 합이 잘맞아요",
          "original_form": "키랑 합이 잘맞아요",
          "speaker_id": "1",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100036104.1.16",
          "form": "한해 구박 받는 거 넘 웃겨요",
          "original_form": "한해 구박 받는 거 넘 웃겨요",
          "speaker_id": "2",
          "time": "20210830 16:33"
        },
        {
          "id": "MDRW2100036104.1.17",
          "form": "그런 캐릭터 하나쯤은 있어야 프로그램이 사는것같아요ㅋㅋㅋ",
          "original_form": "그런 캐릭터 하나쯤은 있어야 프로그램이 사는것같아요ㅋㅋㅋ",
