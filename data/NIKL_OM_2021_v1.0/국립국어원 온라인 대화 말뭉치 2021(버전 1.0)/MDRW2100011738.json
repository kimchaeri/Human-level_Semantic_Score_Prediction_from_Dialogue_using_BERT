      "utterance": [
        {
          "id": "MDRW2100011738.1.1",
          "form": "점심은 뭐 드셨나요!",
          "original_form": "점심은 뭐 드셨나요!",
          "speaker_id": "1",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011738.1.2",
          "form": "저 참치회덮밥이요!",
          "original_form": "저 참치회덮밥이요!",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011738.1.3",
          "form": "뭐드셨나요",
          "original_form": "뭐드셨나요",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011738.1.4",
          "form": "헐 진짜맛있으셨겟다",
          "original_form": "헐 진짜맛있으셨겟다",
          "speaker_id": "1",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100011738.1.5",
          "form": "냉동이었나요",
          "original_form": "냉동이었나요",
          "speaker_id": "1",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100011738.1.6",
          "form": "생인거 같아요",
          "original_form": "생인거 같아요",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011738.1.7",
          "form": "그래서 그린지 오래걸리더라구욬ㅋㅋ",
          "original_form": "그래서 그린지 오래걸리더라구욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011738.1.8",
          "form": "와 대박 맛잇을거같아요",
          "original_form": "와 대박 맛잇을거같아요",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011738.1.9",
          "form": "저 얼마전에 냉동참치먹엇는데 덜녹아서",
          "original_form": "저 얼마전에 냉동참치먹엇는데 덜녹아서",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011738.1.10",
          "form": "생참치가 너무 맛잇을거같더라구요",
          "original_form": "생참치가 너무 맛잇을거같더라구요",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100011738.1.11",
          "form": "헉 잘 녹았으면 더 맛있었을텐데!!",
          "original_form": "헉 잘 녹았으면 더 맛있었을텐데!!",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011738.1.12",
          "form": "저녁은 뭐 드실건가요",
          "original_form": "저녁은 뭐 드실건가요",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011738.1.13",
          "form": "아 고민되네욬ㅋㅋㅋ 저 점심은",
          "original_form": "아 고민되네욬ㅋㅋㅋ 저 점심은",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011738.1.14",
          "form": "학교에서 일해서 급식먹었는데 오늘 장조림 나왔어요!",
          "original_form": "학교에서 일해서 급식먹었는데 오늘 장조림 나왔어요!",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011738.1.15",
          "form": "오 장조림 맛있었나요?",
          "original_form": "오 장조림 맛있었나요?",
          "speaker_id": "2",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011738.1.16",
          "form": "메추리알 잔뜩 먹고왔답니다",
          "original_form": "메추리알 잔뜩 먹고왔답니다",
          "speaker_id": "1",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100011738.1.17",
          "form": "장조림이랑 김치랑 소금김 나와서 밥도둑이었어요 ㅋㅋㅋㅋ",
          "original_form": "장조림이랑 김치랑 소금김 나와서 밥도둑이었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100011738.1.18",
          "form": "ㅋㅋㅋ메추리알 맛있죠!!",
          "original_form": "ㅋㅋㅋ메추리알 맛있죠!!",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100011738.1.19",
          "form": "전 사실 장조림보다 메추리알이 맛있더라고요",
          "original_form": "전 사실 장조림보다 메추리알이 맛있더라고요",
          "speaker_id": "2",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100011738.1.20",
          "form": "저도 메추리알 파에요",
          "original_form": "저도 메추리알 파에요",
          "speaker_id": "1",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100011738.1.21",
          "form": "근데 메추리알 집에서 장조림해먹으려면 까는게 너무귀찮아서 차라리 달걀로 하게되어요 ㅋㅋㅋ",
          "original_form": "근데 메추리알 집에서 장조림해먹으려면 까는게 너무귀찮아서 차라리 달걀로 하게되어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100011738.1.22",
          "form": "ㅋㅋ그렇네요 달걀까기가 더 쉽더라고요",
          "original_form": "ㅋㅋ그렇네요 달걀까기가 더 쉽더라고요",
          "speaker_id": "2",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100011738.1.23",
          "form": "ㅎㅎㅎ 저녁은 맛있게 드셨나요",
          "original_form": "ㅎㅎㅎ 저녁은 맛있게 드셨나요",
          "speaker_id": "1",
          "time": "20210824 21:44"
        }
      ]
    }
