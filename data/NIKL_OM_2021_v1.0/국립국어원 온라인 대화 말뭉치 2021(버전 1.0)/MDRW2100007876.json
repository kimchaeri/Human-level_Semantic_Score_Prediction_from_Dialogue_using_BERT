      "utterance": [
        {
          "id": "MDRW2100007876.1.1",
          "form": "백신 신청하셨나요?",
          "original_form": "백신 신청하셨나요?",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007876.1.2",
          "form": "네 백신 신청했어요!",
          "original_form": "네 백신 신청했어요!",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007876.1.3",
          "form": "9월 9일날 맞으러가요 ㅎㅎ",
          "original_form": "9월 9일날 맞으러가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007876.1.4",
          "form": "일찍 예약하셨네요",
          "original_form": "일찍 예약하셨네요",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007876.1.5",
          "form": "화이자맞나요?",
          "original_form": "화이자맞나요?",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007876.1.6",
          "form": "화이자 아니면 모더나요!",
          "original_form": "화이자 아니면 모더나요!",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007876.1.7",
          "form": "둘중 하나 렌덤이러다라고요",
          "original_form": "둘중 하나 렌덤이러다라고요",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007876.1.8",
          "form": "전 9월 30일 예약했는데  맞기무섭네요",
          "original_form": "전 9월 30일 예약했는데  맞기무섭네요",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007876.1.9",
          "form": "무섭긴한데 그렇다고 안맞기에도 뭐하긴해서 ㅠㅠ",
          "original_form": "무섭긴한데 그렇다고 안맞기에도 뭐하긴해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007876.1.10",
          "form": "제동생이 고3인데",
          "original_form": "제동생이 고3인데",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007876.1.11",
          "form": "화이자 1차 무증상 2차 살짝 아프고 치워서 좀 안심되기도 하고 그렇네요",
          "original_form": "화이자 1차 무증상 2차 살짝 아프고 치워서 좀 안심되기도 하고 그렇네요",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007876.1.12",
          "form": "고3이면 맞았겠네요 2차까지 다요?",
          "original_form": "고3이면 맞았겠네요 2차까지 다요?",
          "speaker_id": "1",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007876.1.13",
          "form": "네 고3쪽은 예외로",
          "original_form": "네 고3쪽은 예외로",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007876.1.14",
          "form": "4주 무조건 기간 지켜서 맞게해준다고 하더라고요",
          "original_form": "4주 무조건 기간 지켜서 맞게해준다고 하더라고요",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007876.1.15",
          "form": "저희는 이제 최대 6주까지 미뤄질수있어요",
          "original_form": "저희는 이제 최대 6주까지 미뤄질수있어요",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007876.1.16",
          "form": "별다른 부작용 없어서 다행이네요",
          "original_form": "별다른 부작용 없어서 다행이네요",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007876.1.17",
          "form": "모더나가 낫다는 얘기도 있고",
          "original_form": "모더나가 낫다는 얘기도 있고",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007876.1.18",
          "form": "모더나가 남자들한테 부작용이 좀 있다곤 들었던거같아요",
          "original_form": "모더나가 남자들한테 부작용이 좀 있다곤 들었던거같아요",
          "speaker_id": "2",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007876.1.19",
          "form": "그래서 화이자를 차라리..맞고싶네요",
          "original_form": "그래서 화이자를 차라리..맞고싶네요",
          "speaker_id": "2",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007876.1.20",
          "form": "서울은 화이자 지방은 모더나 라는얘기도 있던데",
          "original_form": "서울은 화이자 지방은 모더나 라는얘기도 있던데",
          "speaker_id": "1",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007876.1.21",
          "form": "전 다 무섭ㅎㅎ",
          "original_form": "전 다 무섭ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:00"
        },
        {
          "id": "MDRW2100007876.1.22",
          "form": "모더나가 원래 들어올 양의 50%밖에 못들어와서",
          "original_form": "모더나가 원래 들어올 양의 50%밖에 못들어와서",
          "speaker_id": "2",
          "time": "20210822 12:00"
        },
        {
          "id": "MDRW2100007876.1.23",
          "form": "느낌상 대부분 화이자일거같아요",
          "original_form": "느낌상 대부분 화이자일거같아요",
          "speaker_id": "2",
          "time": "20210822 12:00"
        },
        {
          "id": "MDRW2100007876.1.24",
          "form": "아 그래요? 뉴스보니 보내주기로 했다던데 양이 적군요",
          "original_form": "아 그래요? 뉴스보니 보내주기로 했다던데 양이 적군요",
          "speaker_id": "1",
          "time": "20210822 12:01"
        },
        {
          "id": "MDRW2100007876.1.25",
          "form": "네네 그래서 항의하러",
          "original_form": "네네 그래서 항의하러",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007876.1.26",
          "form": "모더나 본사까지 갔지만",
          "original_form": "모더나 본사까지 갔지만",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007876.1.27",
          "form": "소득없이 돌아왔다는..",
          "original_form": "소득없이 돌아왔다는..",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007876.1.28",
          "form": "부작용 없이 그냥 무사히 지나갔음 좋겠어요",
          "original_form": "부작용 없이 그냥 무사히 지나갔음 좋겠어요",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007876.1.29",
          "form": "뭘 맞든ㅎ",
          "original_form": "뭘 맞든ㅎ",
          "speaker_id": "1",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007876.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007876.1.31",
          "form": "주변엔 부작용은 크게 다들없던데",
          "original_form": "주변엔 부작용은 크게 다들없던데",
          "speaker_id": "2",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007876.1.32",
          "form": "친구 부모님은 아직까지 두통에 시달리세요",
          "original_form": "친구 부모님은 아직까지 두통에 시달리세요",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007876.1.33",
