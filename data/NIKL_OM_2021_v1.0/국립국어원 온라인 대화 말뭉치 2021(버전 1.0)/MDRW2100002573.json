      "utterance": [
        {
          "id": "MDRW2100002573.1.1",
          "form": "최근들어 쇼핑같은거 잘안했는데 넌 뭐 산거나 관심가는거 있어?",
          "original_form": "최근들어 쇼핑같은거 잘안했는데 넌 뭐 산거나 관심가는거 있어?",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002573.1.2",
          "form": "산건 없고 갖고 싶은건 많아",
          "original_form": "산건 없고 갖고 싶은건 많아",
          "speaker_id": "2",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002573.1.3",
          "form": "뭐뭐 가지고 싶은데??",
          "original_form": "뭐뭐 가지고 싶은데??",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002573.1.4",
          "form": "컴퓨터도 사고 싶고 만화책도 사고 싶고",
          "original_form": "컴퓨터도 사고 싶고 만화책도 사고 싶고",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002573.1.5",
          "form": "이외에도 많아",
          "original_form": "이외에도 많아",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002573.1.6",
          "form": "너는 관심가는거 있어?",
          "original_form": "너는 관심가는거 있어?",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002573.1.7",
          "form": "나도 막 전자기기쪽으로 사고 싶어",
          "original_form": "나도 막 전자기기쪽으로 사고 싶어",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002573.1.8",
          "form": "최근엔 호기심때문에",
          "original_form": "최근엔 호기심때문에",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002573.1.9",
          "form": "아이트래커라고",
          "original_form": "아이트래커라고",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002573.1.10",
          "form": "내 눈이 어디로 이동하는지 보여주는 기계가 있거든?",
          "original_form": "내 눈이 어디로 이동하는지 보여주는 기계가 있거든?",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002573.1.11",
          "form": "막 그런거 사고 싶더라",
          "original_form": "막 그런거 사고 싶더라",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002573.1.12",
          "form": "넌 만화책이면 소장용?",
          "original_form": "넌 만화책이면 소장용?",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002573.1.13",
          "form": "와 그런게 있어?",
          "original_form": "와 그런게 있어?",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002573.1.14",
          "form": "처음 들어봐 신기하다",
          "original_form": "처음 들어봐 신기하다",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002573.1.15",
          "form": "나도 처음듣고 와 이런게 있어? 라는 생각했어",
          "original_form": "나도 처음듣고 와 이런게 있어? 라는 생각했어",
          "speaker_id": "1",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002573.1.16",
          "form": "의외로 별난게 많더라고",
          "original_form": "의외로 별난게 많더라고",
          "speaker_id": "1",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002573.1.17",
          "form": "만약 사게 되면 너도 써볼래?",
          "original_form": "만약 사게 되면 너도 써볼래?",
          "speaker_id": "1",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002573.1.18",
          "form": "응 궁금해 어떻게 하는건지",
          "original_form": "응 궁금해 어떻게 하는건지",
          "speaker_id": "2",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002573.1.19",
          "form": "맞다 만약 컴퓨터 사게 되면 개학시즌은 피해서 사",
          "original_form": "맞다 만약 컴퓨터 사게 되면 개학시즌은 피해서 사",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002573.1.20",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002573.1.21",
          "form": "더 비싸?",
          "original_form": "더 비싸?",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002573.1.22",
          "form": "그때가 가격이 더 비싸거든",
          "original_form": "그때가 가격이 더 비싸거든",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002573.1.23",
          "form": "아하 몰랐네",
          "original_form": "아하 몰랐네",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002573.1.24",
          "form": "가을때 사는게 젤 좋아",
          "original_form": "가을때 사는게 젤 좋아",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002573.1.25",
          "form": "그럼 좀 기다려야겠네",
          "original_form": "그럼 좀 기다려야겠네",
          "speaker_id": "2",
          "time": "20210820 13:52"
        }
      ]
    }
