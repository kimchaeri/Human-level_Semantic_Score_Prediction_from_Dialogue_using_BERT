      "utterance": [
        {
          "id": "MDRW2100002674.1.1",
          "form": "우리가 게임에서 만났지 ㅋㅋㅋ",
          "original_form": "우리가 게임에서 만났지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002674.1.2",
          "form": "맞아 ㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002674.1.3",
          "form": "마침 게임 주제라니 좋네",
          "original_form": "마침 게임 주제라니 좋네",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002674.1.4",
          "form": "ㅋㅋㅋㅋ 씰온라인은 아직 돌아가나?",
          "original_form": "ㅋㅋㅋㅋ 씰온라인은 아직 돌아가나?",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002674.1.5",
          "form": "같이 게임하던 사람들 생각난다",
          "original_form": "같이 게임하던 사람들 생각난다",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002674.1.6",
          "form": "ㅇㅇ주변 사는 사람들 자주 만나러 다녔는덲 ㅋㅋㅋ",
          "original_form": "ㅇㅇ주변 사는 사람들 자주 만나러 다녔는덲 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002674.1.7",
          "form": "모바일 배그도 작년초까진 했는데",
          "original_form": "모바일 배그도 작년초까진 했는데",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002674.1.8",
          "form": "컴터 하나 사서 같이 하자니까",
          "original_form": "컴터 하나 사서 같이 하자니까",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002674.1.9",
          "form": "컴배그는 넘 어려워..ㅊ.",
          "original_form": "컴배그는 넘 어려워..ㅊ.",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002674.1.10",
          "form": "답답하긴해",
          "original_form": "답답하긴해",
          "speaker_id": "2",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002674.1.11",
          "form": "흥",
          "original_form": "흥",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002674.1.12",
          "form": "ㅋㅋㅋㅋ새 컴사서 그건 내가 배그하고",
          "original_form": "ㅋㅋㅋㅋ새 컴사서 그건 내가 배그하고",
          "speaker_id": "2",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002674.1.13",
          "form": "나는 사면 노트북사서 할거야",
          "original_form": "나는 사면 노트북사서 할거야",
          "speaker_id": "1",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002674.1.14",
          "form": "노트북 배그 못 돌린다",
          "original_form": "노트북 배그 못 돌린다",
          "speaker_id": "2",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002674.1.15",
          "form": "배그 돌아가는 걸로 살겨",
          "original_form": "배그 돌아가는 걸로 살겨",
          "speaker_id": "1",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002674.1.16",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "2",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002674.1.17",
          "form": "홍진호는 요새 배그하나",
          "original_form": "홍진호는 요새 배그하나",
          "speaker_id": "1",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002674.1.18",
