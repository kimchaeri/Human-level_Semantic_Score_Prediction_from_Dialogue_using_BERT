        {
          "id": "MDRW2100045267.1.1",
          "form": "재난 지원금으로 미용실 많이 간데요",
          "original_form": "재난 지원금으로 미용실 많이 간데요",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045267.1.2",
          "form": "저도 머리나 할까 하고 있는데",
          "original_form": "저도 머리나 할까 하고 있는데",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045267.1.3",
          "form": "어디 하고 싶으세요",
          "original_form": "어디 하고 싶으세요",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045267.1.4",
          "form": "저는 파마하고 싶더라고요",
          "original_form": "저는 파마하고 싶더라고요",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045267.1.5",
          "form": "오 저두요",
          "original_form": "오 저두요",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045267.1.6",
          "form": "근데 잘 풀려서 부탁했다는",
          "original_form": "근데 잘 풀려서 부탁했다는",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045267.1.7",
          "form": "그게 처음 하면 잘 안 된다던데",
          "original_form": "그게 처음 하면 잘 안 된다던데",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045267.1.8",
          "form": "저는 머리도 많이 상해서",
          "original_form": "저는 머리도 많이 상해서",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045267.1.9",
          "form": "저도요 빗질이 잘 안 된다는",
          "original_form": "저도요 빗질이 잘 안 된다는",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045267.1.10",
          "form": "파마도 종류가 있던데",
          "original_form": "파마도 종류가 있던데",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045267.1.11",
          "form": "저는 히피 펌이 요",
          "original_form": "저는 히피 펌이 요",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045267.1.12",
          "form": "그게 좋아 보이긴 하더라고요",
          "original_form": "그게 좋아 보이긴 하더라고요",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045267.1.13",
