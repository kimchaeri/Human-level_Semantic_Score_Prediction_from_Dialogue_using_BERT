      "utterance": [
        {
          "id": "MDRW2100039338.1.1",
          "form": "기억나는 공연이나 전시회 있늬",
          "original_form": "기억나는 공연이나 전시회 있늬",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039338.1.2",
          "form": "티켓팅 광탈 당한 기억뿐이네요....ㅎ....",
          "original_form": "티켓팅 광탈 당한 기억뿐이네요....ㅎ....",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039338.1.3",
          "form": "공연이나 전시회 보러간 적 있나유?",
          "original_form": "공연이나 전시회 보러간 적 있나유?",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039338.1.4",
          "form": "공연이면 20년도에 이용신성우님 달빛천사 콘서트를",
          "original_form": "공연이면 20년도에 이용신성우님 달빛천사 콘서트를",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039338.1.5",
          "form": "갔던 기억이",
          "original_form": "갔던 기억이",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039338.1.6",
          "form": "취케팅 성공해서 갔었쥬",
          "original_form": "취케팅 성공해서 갔었쥬",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039338.1.7",
          "form": "아 맞아 그거! 부럽다 흑흑 난 다 광탈의 기억뿐...ㅠㅠㅠㅠ",
          "original_form": "아 맞아 그거! 부럽다 흑흑 난 다 광탈의 기억뿐...ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039338.1.8",
          "form": "성우콘서트도 광탈당했는데",
          "original_form": "성우콘서트도 광탈당했는데",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039338.1.9",
          "form": "아이유와 방탄 콘서트는..",
          "original_form": "아이유와 방탄 콘서트는..",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039338.1.10",
          "form": "경쟁률이 얼마나 셀까.. 워후",
          "original_form": "경쟁률이 얼마나 셀까.. 워후",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100039338.1.11",
          "form": "일단...들어갈 수가 없어....ㅋㅋㅋㅋㅋㅋ",
          "original_form": "일단...들어갈 수가 없어....ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039338.1.12",
          "form": "서버가 인원을 감당못하고 터져버려유...",
          "original_form": "서버가 인원을 감당못하고 터져버려유...",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100039338.1.13",
          "form": "어우 세상에",
          "original_form": "어우 세상에",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039338.1.14",
          "form": "그걸 뚫고 좋은자리 선정하는 사람들은",
          "original_form": "그걸 뚫고 좋은자리 선정하는 사람들은",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039338.1.15",
          "form": "진짜.. 손가락이랑 기기가 좋나보다",
          "original_form": "진짜.. 손가락이랑 기기가 좋나보다",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100039338.1.16",
          "form": "티켓팅 잘하는 사람들은 자기 갖고있는 모든 기기들 동원하거나 pc방에서 하더라규ㅋㅋㅋㅋ...",
          "original_form": "티켓팅 잘하는 사람들은 자기 갖고있는 모든 기기들 동원하거나 pc방에서 하더라규ㅋㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039338.1.17",
          "form": "+운도 있어야....ㅠㅠㅠ",
          "original_form": "+운도 있어야....ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039338.1.18",
