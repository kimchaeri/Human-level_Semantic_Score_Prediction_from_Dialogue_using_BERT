      "utterance": [
        {
          "id": "MDRW2100019903.1.1",
          "form": "게임 하시나요",
          "original_form": "게임 하시나요",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019903.1.2",
          "form": "사실 전 게임을 안하긴 하는데요",
          "original_form": "사실 전 게임을 안하긴 하는데요",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019903.1.3",
          "form": "심즈게임을 해보고 싶네요 ㅎㅎ",
          "original_form": "심즈게임을 해보고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019903.1.4",
          "form": "심즈... 슬픈 사연이있습니다 물어봐주세요",
          "original_form": "심즈... 슬픈 사연이있습니다 물어봐주세요",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019903.1.5",
          "form": "ㅋㄴㅋㅋㅋ왜요 심즈 하시다가 무슨일이..?",
          "original_form": "ㅋㄴㅋㅋㅋ왜요 심즈 하시다가 무슨일이..?",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019903.1.6",
          "form": "저의 꿈을 대신 달성해주길 바랐습니다",
          "original_form": "저의 꿈을 대신 달성해주길 바랐습니다",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019903.1.7",
          "form": "계속 물어봐주세요",
          "original_form": "계속 물어봐주세요",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019903.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019903.1.9",
          "form": "어떤 꿈이었는데요",
          "original_form": "어떤 꿈이었는데요",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019903.1.10",
          "form": "ㅋㅋㅋ 하면 안된데요... 작가가 되어주길 바라였습니다",
          "original_form": "ㅋㅋㅋ 하면 안된데요... 작가가 되어주길 바라였습니다",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019903.1.11",
          "form": "그런데! 작업에 몰중하다하다",
          "original_form": "그런데! 작업에 몰중하다하다",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019903.1.12",
          "form": "노인이 되었습니다",
          "original_form": "노인이 되었습니다",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019903.1.13",
          "form": "ㅋㅋㅋㅋ네? 와 그럴수도 잇군욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ네? 와 그럴수도 잇군욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019903.1.14",
          "form": "심즈도 늙어요?",
          "original_form": "심즈도 늙어요?",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019903.1.15",
          "form": "그때서야 유명한 작가가 되었지요",
          "original_form": "그때서야 유명한 작가가 되었지요",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019903.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019903.1.17",
          "form": "현타가와서 지웠어요",
          "original_form": "현타가와서 지웠어요",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019903.1.18",
          "form": "꿈을 이루긴 했네요ㅎㅎ",
          "original_form": "꿈을 이루긴 했네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019903.1.19",
          "form": "그렇죠 다 늙어서",
          "original_form": "그렇죠 다 늙어서",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019903.1.20",
          "form": "ㅋㅋㅋㅋ흠... 심즈는 특이한 게임이군요",
          "original_form": "ㅋㅋㅋㅋ흠... 심즈는 특이한 게임이군요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019903.1.21",
          "form": "좀 복잡하더라고요",
          "original_form": "좀 복잡하더라고요",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019903.1.22",
          "form": "가수 되는것도 있었는데 방법을 몰라서 못했어요",
          "original_form": "가수 되는것도 있었는데 방법을 몰라서 못했어요",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019903.1.23",
          "form": "저는 아주 아름다운 심즈를 만들어보고싶은데 똥손이라 힘들것 같아요~",
