      "utterance": [
        {
          "id": "MDRW2100032546.1.1",
          "form": "단거 좋아하세요?",
          "original_form": "단거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100032546.1.2",
          "form": "넵! 젤리빈이나 하리보 같은 젤리도 좋아하고 그래용",
          "original_form": "넵! 젤리빈이나 하리보 같은 젤리도 좋아하고 그래용",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032546.1.3",
          "form": "오오 젤리를 좋아하시는군여",
          "original_form": "오오 젤리를 좋아하시는군여",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032546.1.4",
          "form": "저는 초콜릿 같은걸 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 초콜릿 같은걸 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032546.1.5",
          "form": "젤리는 새콤해서 ㅠㅠ",
          "original_form": "젤리는 새콤해서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100032546.1.6",
          "form": "젤리 달기만 한것도 이써요,,!!",
          "original_form": "젤리 달기만 한것도 이써요,,!!",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032546.1.7",
          "form": "오오 마시멜로맛 비슷한 젤리 본거같아요",
          "original_form": "오오 마시멜로맛 비슷한 젤리 본거같아요",
          "speaker_id": "1",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100032546.1.8",
          "form": "매운거는 잘드세요?",
          "original_form": "매운거는 잘드세요?",
          "speaker_id": "1",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100032546.1.9",
          "form": "아녀ㅠ",
          "original_form": "아녀ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032546.1.10",
          "form": "잘못먹어요ㅠㅠ",
          "original_form": "잘못먹어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032546.1.11",
          "form": "오 저도 못먹어요 ㅠㅠ",
          "original_form": "오 저도 못먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032546.1.12",
          "form": "잘먹는 사람 부러워요 ㅋㅋ",
          "original_form": "잘먹는 사람 부러워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032546.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ맞앙요 불닭 진짜 매워서 못먹어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞앙요 불닭 진짜 매워서 못먹어요",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100032546.1.14",
          "form": "입이 삭제되는 느낌ㅠ",
          "original_form": "입이 삭제되는 느낌ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100032546.1.15",
          "form": "왑 불닭 ㅠㅜ 엽떡도 너무 매워요",
          "original_form": "왑 불닭 ㅠㅜ 엽떡도 너무 매워요",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032546.1.16",
          "form": "속도 뒤집어져요 ㅋㅋㅋ",
          "original_form": "속도 뒤집어져요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032546.1.17",
          "form": "맞아요ㅠ 저도 엽떡 안먹어요ㅋㅋㅋ",
          "original_form": "맞아요ㅠ 저도 엽떡 안먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032546.1.18",
          "form": "매워가지규",
          "original_form": "매워가지규",
          "speaker_id": "2",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032546.1.19",
          "form": "짠거랑 단거는 잘먹어요 ㅋㅋㅋㅋ",
          "original_form": "짠거랑 단거는 잘먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032546.1.20",
          "form": "저도용 그중 최고는 단거에요",
          "original_form": "저도용 그중 최고는 단거에요",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032546.1.21",
          "form": "그쵸!! 단게 최고",
