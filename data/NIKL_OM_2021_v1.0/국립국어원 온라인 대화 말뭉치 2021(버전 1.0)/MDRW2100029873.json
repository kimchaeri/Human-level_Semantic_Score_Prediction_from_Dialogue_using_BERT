          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100029873.1.4",
          "form": "아..네...",
          "original_form": "아..네...",
          "speaker_id": "1",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029873.1.5",
          "form": "연애와 결혼에대해 토킹어바웃 해봅시다",
          "original_form": "연애와 결혼에대해 토킹어바웃 해봅시다",
          "speaker_id": "1",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029873.1.6",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029873.1.7",
          "form": "안했습니다",
          "original_form": "안했습니다",
          "speaker_id": "1",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100029873.1.8",
          "form": "님은여",
          "original_form": "님은여",
          "speaker_id": "1",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100029873.1.9",
          "form": "저도 아직요ㅎ",
          "original_form": "저도 아직요ㅎ",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100029873.1.10",
          "form": "만나는 분은 있나요?",
          "original_form": "만나는 분은 있나요?",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100029873.1.11",
          "form": "네 있어요 몇년 됬습니다",
          "original_form": "네 있어요 몇년 됬습니다",
          "speaker_id": "1",
          "time": "20210903 16:02"
        },
        {
          "id": "MDRW2100029873.1.12",
          "form": "결혼은 잘모르겠네요",
          "original_form": "결혼은 잘모르겠네요",
          "speaker_id": "1",
          "time": "20210903 16:02"
        },
        {
          "id": "MDRW2100029873.1.13",
          "form": "그럼 왜 만나시는지...",
          "original_form": "그럼 왜 만나시는지...",
          "speaker_id": "2",
          "time": "20210903 16:03"
        },
        {
          "id": "MDRW2100029873.1.14",
          "form": "전 미래가 그려지지않으면 안만났거든요",
          "original_form": "전 미래가 그려지지않으면 안만났거든요",
          "speaker_id": "2",
          "time": "20210903 16:03"
        },
        {
          "id": "MDRW2100029873.1.15",
          "form": "그래서그런지 텀이 기네요ㅎ",
          "original_form": "그래서그런지 텀이 기네요ㅎ",
          "speaker_id": "2",
          "time": "20210903 16:03"
        },
        {
          "id": "MDRW2100029873.1.16",
          "form": "만나봐야 미래가 그려지는지 아니까요?",
          "original_form": "만나봐야 미래가 그려지는지 아니까요?",
          "speaker_id": "1",
          "time": "20210903 16:03"
        },
        {
          "id": "MDRW2100029873.1.17",
          "form": "시도를해봐야 결과와 변수를 안다고 생각해서요",
          "original_form": "시도를해봐야 결과와 변수를 안다고 생각해서요",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029873.1.18",
          "form": "그런건 만나기전에 얘기하다보면 어느정도 보이지 않나요??",
          "original_form": "그런건 만나기전에 얘기하다보면 어느정도 보이지 않나요??",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029873.1.19",
          "form": "가치관이라던지",
          "original_form": "가치관이라던지",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029873.1.20",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029873.1.21",
          "form": "그게 쉽게 보이면 연애결혼말고 선을보는게 빠르겠군요",
          "original_form": "그게 쉽게 보이면 연애결혼말고 선을보는게 빠르겠군요",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029873.1.22",
          "form": "연애한지 얼마나 되셨어요",
          "original_form": "연애한지 얼마나 되셨어요",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029873.1.23",
          "form": "저 이전 연애는 5년반 이었네요",
          "original_form": "저 이전 연애는 5년반 이었네요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029873.1.24",
          "form": "지금은 없구요",
          "original_form": "지금은 없구요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029873.1.25",
          "form": "5년동안 자유를 즐기셨군요",
          "original_form": "5년동안 자유를 즐기셨군요",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029873.1.26",
          "form": "혼자일때가 그립습니다",
          "original_form": "혼자일때가 그립습니다",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029873.1.27",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029873.1.28",
          "form": "아뇨 연애 기간이 5년반ㅋㅋㅋ",
          "original_form": "아뇨 연애 기간이 5년반ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029873.1.29",
          "form": "아 오래하셨네요",
          "original_form": "아 오래하셨네요",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029873.1.30",
          "form": "헤어지고는 3년? 4년차 됐네요",
