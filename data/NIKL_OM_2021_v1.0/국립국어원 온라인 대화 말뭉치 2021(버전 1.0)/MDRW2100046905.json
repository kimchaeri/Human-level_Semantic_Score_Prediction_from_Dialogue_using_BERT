      "utterance": [
        {
          "id": "MDRW2100046905.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211005 13:59"
        },
        {
          "id": "MDRW2100046905.1.2",
          "form": "가상인간에 대해 어떻게 생각하시나요",
          "original_form": "가상인간에 대해 어떻게 생각하시나요",
          "speaker_id": "1",
          "time": "20211005 13:59"
        },
        {
          "id": "MDRW2100046905.1.3",
          "form": "글쎄요 좀 무섭기도하고",
          "original_form": "글쎄요 좀 무섭기도하고",
          "speaker_id": "2",
          "time": "20211005 14:09"
        },
        {
          "id": "MDRW2100046905.1.4",
          "form": "잘 상용되면 생활이 편리해지지않을까요?",
          "original_form": "잘 상용되면 생활이 편리해지지않을까요?",
          "speaker_id": "2",
          "time": "20211005 14:09"
        },
        {
          "id": "MDRW2100046905.1.5",
          "form": "동의합니다.",
          "original_form": "동의합니다.",
          "speaker_id": "1",
          "time": "20211005 14:16"
        },
        {
          "id": "MDRW2100046905.1.6",
          "form": "인간이 해결하기 어려운 문제를 왕만하게 해결할 수 있다고 생각합니다.",
          "original_form": "인간이 해결하기 어려운 문제를 왕만하게 해결할 수 있다고 생각합니다.",
          "speaker_id": "1",
          "time": "20211005 14:17"
        },
        {
          "id": "MDRW2100046905.1.7",
          "form": "하지만 부작용으로 일거리나 노동자들의 권리가 박탈되지 않을까요?",
          "original_form": "하지만 부작용으로 일거리나 노동자들의 권리가 박탈되지 않을까요?",
          "speaker_id": "1",
          "time": "20211005 14:17"
        },
        {
          "id": "MDRW2100046905.1.8",
          "form": "어떤 일이든 부작용은 따르니까요",
          "original_form": "어떤 일이든 부작용은 따르니까요",
          "speaker_id": "2",
          "time": "20211005 14:21"
        },
        {
          "id": "MDRW2100046905.1.9",
          "form": "한번에 나아지지는 않더라도 차차 좋은 방향으로 바뀔거라 믿어요",
          "original_form": "한번에 나아지지는 않더라도 차차 좋은 방향으로 바뀔거라 믿어요",
          "speaker_id": "2",
          "time": "20211005 14:21"
        },
        {
          "id": "MDRW2100046905.1.10",
          "form": "맞습니다.",
          "original_form": "맞습니다.",
          "speaker_id": "1",
          "time": "20211005 14:23"
        },
        {
          "id": "MDRW2100046905.1.11",
          "form": "그렇다면 나아가서 가상인간이 가쟈오는 다른 긍정적인 효과는 뭐가있을까요?",
          "original_form": "그렇다면 나아가서 가상인간이 가쟈오는 다른 긍정적인 효과는 뭐가있을까요?",
          "speaker_id": "1",
          "time": "20211005 14:24"
        },
        {
          "id": "MDRW2100046905.1.12",
          "form": "서비스업에서 쓸모있지 않을까요",
          "original_form": "서비스업에서 쓸모있지 않을까요",
          "speaker_id": "2",
          "time": "20211005 14:28"
        },
        {
          "id": "MDRW2100046905.1.13",
          "form": "안내같은 용도 인가요?",
          "original_form": "안내같은 용도 인가요?",
          "speaker_id": "1",
          "time": "20211005 14:51"
        },
        {
          "id": "MDRW2100046905.1.14",
          "form": "네 상담, 안내같은 용도나 단순 업무 같은경우는 대체 가능성이 높은것 같아요",
          "original_form": "네 상담, 안내같은 용도나 단순 업무 같은경우는 대체 가능성이 높은것 같아요",
          "speaker_id": "2",
          "time": "20211005 14:55"
        },
        {
          "id": "MDRW2100046905.1.15",
          "form": "좋내요.",
          "original_form": "좋내요.",
          "speaker_id": "1",
          "time": "20211006 03:47"
        },
        {
          "id": "MDRW2100046905.1.16",
          "form": "어디에나있는 진상손님들도",
          "original_form": "어디에나있는 진상손님들도",
          "speaker_id": "1",
          "time": "20211006 03:47"
        },
        {
          "id": "MDRW2100046905.1.17",
          "form": "줄어들지도 모르겟네요",
          "original_form": "줄어들지도 모르겟네요",
          "speaker_id": "1",
          "time": "20211006 03:47"
        },
        {
          "id": "MDRW2100046905.1.18",
          "form": "가상인간이 진상 고객을 상대하면 정말 편할것 같네요",
          "original_form": "가상인간이 진상 고객을 상대하면 정말 편할것 같네요",
          "speaker_id": "2",
          "time": "20211006 09:38"
        },
        {
          "id": "MDRW2100046905.1.19",
          "form": "그렇지만 가상인간인걸 알면 더 진상부릴지도 몰라요",
          "original_form": "그렇지만 가상인간인걸 알면 더 진상부릴지도 몰라요",
          "speaker_id": "2",
          "time": "20211006 09:39"
        },
        {
          "id": "MDRW2100046905.1.20",
          "form": "자신이 가상인간이라는걸",
          "original_form": "자신이 가상인간이라는걸",
          "speaker_id": "1",
          "time": "20211006 10:21"
        },
        {
          "id": "MDRW2100046905.1.21",
          "form": "자각 한다는 말씀하시는건가요?",
          "original_form": "자각 한다는 말씀하시는건가요?",
          "speaker_id": "1",
          "time": "20211006 10:21"
        },
        {
          "id": "MDRW2100046905.1.22",
          "form": "아 아니요 상대가 가상인간이라는걸 알게되면요",
          "original_form": "아 아니요 상대가 가상인간이라는걸 알게되면요",
          "speaker_id": "2",
          "time": "20211006 13:23"
        },
        {
          "id": "MDRW2100046905.1.23",
          "form": "아 땅콩사건처럼",
          "original_form": "아 땅콩사건처럼",
          "speaker_id": "1",
          "time": "20211007 12:28"
        },
        {
          "id": "MDRW2100046905.1.24",
          "form": "그거보다 더한 일이 생길 수 있다는말씀이시죠",
          "original_form": "그거보다 더한 일이 생길 수 있다는말씀이시죠",
          "speaker_id": "1",
          "time": "20211007 12:28"
        }
      ]
