      "utterance": [
        {
          "id": "MDRW2100020434.1.1",
          "form": "네네 매일 마시죠",
          "original_form": "네네 매일 마시죠",
          "speaker_id": "2",
          "time": "20210828 05:33"
        },
        {
          "id": "MDRW2100020434.1.2",
          "form": "아아 드시나용?",
          "original_form": "아아 드시나용?",
          "speaker_id": "1",
          "time": "20210828 05:33"
        },
        {
          "id": "MDRW2100020434.1.3",
          "form": "아아도 좋고 달달한것도 좋아요",
          "original_form": "아아도 좋고 달달한것도 좋아요",
          "speaker_id": "2",
          "time": "20210828 05:33"
        },
        {
          "id": "MDRW2100020434.1.4",
          "form": "살 안찌고 싶은날엔 아아",
          "original_form": "살 안찌고 싶은날엔 아아",
          "speaker_id": "2",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.5",
          "form": "오 넓은 취향을 가지셨네용",
          "original_form": "오 넓은 취향을 가지셨네용",
          "speaker_id": "1",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.6",
          "form": "저는 커피는 카페인때문에 잘 못마시는데",
          "original_form": "저는 커피는 카페인때문에 잘 못마시는데",
          "speaker_id": "1",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.7",
          "form": "넹넹 보통 음식분야에선 가리는게 없어요",
          "original_form": "넹넹 보통 음식분야에선 가리는게 없어요",
          "speaker_id": "2",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.8",
          "form": "카페가는건 엄청 좋아해요ㅋㅋ",
          "original_form": "카페가는건 엄청 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.9",
          "form": "카페 투어 좋아요",
          "original_form": "카페 투어 좋아요",
          "speaker_id": "2",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.10",
          "form": "흐흐ㅋㅋㅋ 그렇게 먹어야 건강하대요",
          "original_form": "흐흐ㅋㅋㅋ 그렇게 먹어야 건강하대요",
          "speaker_id": "1",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.11",
          "form": "요즘 특색있는 카페도 많고",
          "original_form": "요즘 특색있는 카페도 많고",
          "speaker_id": "1",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.12",
          "form": "저도 한 한달 커피 끊었다가",
          "original_form": "저도 한 한달 커피 끊었다가",
          "speaker_id": "2",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.13",
          "form": "디저트 맛있는곳도 많구용ㅋㅋ",
          "original_form": "디저트 맛있는곳도 많구용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.14",
          "form": "다시 시작했더니 좀 두근두근해요",
          "original_form": "다시 시작했더니 좀 두근두근해요",
          "speaker_id": "2",
          "time": "20210828 05:34"
        },
        {
          "id": "MDRW2100020434.1.15",
          "form": "두근두근 카페인 때문일까요ㅋㅋ",
          "original_form": "두근두근 카페인 때문일까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:35"
        },
        {
          "id": "MDRW2100020434.1.16",
          "form": "그쵸 요즘 너무 다양해서",
          "original_form": "그쵸 요즘 너무 다양해서",
          "speaker_id": "2",
          "time": "20210828 05:35"
        },
        {
          "id": "MDRW2100020434.1.17",
          "form": "네 카페인 ㅋㅋㅋ",
          "original_form": "네 카페인 ㅋㅋㅋ",
          "speaker_id": "2",
