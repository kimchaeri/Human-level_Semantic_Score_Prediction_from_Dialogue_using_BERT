      "utterance": [
        {
          "id": "MDRW2100022710.1.1",
          "form": "꽃게 사고싶어요",
          "original_form": "꽃게 사고싶어요",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022710.1.2",
          "form": "새우도 같이 사고싶어요",
          "original_form": "새우도 같이 사고싶어요",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022710.1.3",
          "form": "새우까는거",
          "original_form": "새우까는거",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022710.1.4",
          "form": "잘하세요?",
          "original_form": "잘하세요?",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022710.1.5",
          "form": "음 새우까는거 사고싶은 정도에요",
          "original_form": "음 새우까는거 사고싶은 정도에요",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022710.1.6",
          "form": "그런것도",
          "original_form": "그런것도",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022710.1.7",
          "form": "그런 도구가 있더라고요",
          "original_form": "그런 도구가 있더라고요",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022710.1.8",
          "form": "팔고있겠죠?",
          "original_form": "팔고있겠죠?",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022710.1.9",
          "form": "비쌀까요?",
          "original_form": "비쌀까요?",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022710.1.10",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022710.1.11",
          "form": "못살것같네요 저는,,",
          "original_form": "못살것같네요 저는,,",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022710.1.12",
          "form": "저렴할 것 같은데요",
          "original_form": "저렴할 것 같은데요",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022710.1.13",
          "form": "손으로까먹을게요,,",
          "original_form": "손으로까먹을게요,,",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022710.1.14",
          "form": "몇 번 쓰다말 제춤 느낌이에요",
          "original_form": "몇 번 쓰다말 제춤 느낌이에요",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022710.1.15",
          "form": "꽃게랑 새우를 어디서 사드세요?",
          "original_form": "꽃게랑 새우를 어디서 사드세요?",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022710.1.16",
          "form": "저는 연안부두에서 사먹어요",
          "original_form": "저는 연안부두에서 사먹어요",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022710.1.17",
          "form": "제대로 드시네요",
          "original_form": "제대로 드시네요",
          "speaker_id": "1",
          "time": "20210829 17:15"
        }
