      "utterance": [
        {
          "id": "MDRW2100027972.1.1",
          "form": "이성당들르는",
          "original_form": "이성당들르는",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027972.1.2",
          "form": "제주도는 달리 소개할게 없어요",
          "original_form": "제주도는 달리 소개할게 없어요",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027972.1.3",
          "form": "1박2일정도면 충분하더라구요",
          "original_form": "1박2일정도면 충분하더라구요",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027972.1.4",
          "form": "관광객들이 더 잘 알고오셔서",
          "original_form": "관광객들이 더 잘 알고오셔서",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027972.1.5",
          "form": "힐링여행지겟어요",
          "original_form": "힐링여행지겟어요",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027972.1.6",
          "form": "이성당 가보고 싶네요",
          "original_form": "이성당 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027972.1.7",
          "form": "이성당이 국내 최초 서양식 빵집이라고 들었어요",
          "original_form": "이성당이 국내 최초 서양식 빵집이라고 들었어요",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027972.1.8",
          "form": "거기 빵집 좋아하시나요",
          "original_form": "거기 빵집 좋아하시나요",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027972.1.9",
          "form": "단팥빵이 맛있는 집이에요",
          "original_form": "단팥빵이 맛있는 집이에요",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027972.1.10",
          "form": "택배로도 파는지 궁금해요",
          "original_form": "택배로도 파는지 궁금해요",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027972.1.11",
          "form": "많이 갔는데 요즘은 좀 질렸어요ㅎㅎ",
          "original_form": "많이 갔는데 요즘은 좀 질렸어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027972.1.12",
          "form": "할머니댁이 좋은곳에 있네요^^",
          "original_form": "할머니댁이 좋은곳에 있네요^^",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027972.1.13",
          "form": "택배도 한다고 알고있어요",
          "original_form": "택배도 한다고 알고있어요",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027972.1.14",
          "form": "ㅎㅎㅎ 감사합니다",
          "original_form": "ㅎㅎㅎ 감사합니다",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027972.1.15",
          "form": "할머니댁 여행삼아 가셔도 좋겠어요",
          "original_form": "할머니댁 여행삼아 가셔도 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027972.1.16",
          "form": "저도 가끔 기분내러 서귀포로 드라이브가요",
          "original_form": "저도 가끔 기분내러 서귀포로 드라이브가요",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027972.1.17",
          "form": "와 멋있어요",
          "original_form": "와 멋있어요",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027972.1.18",
          "form": "저는 저번에 제주도 갔을때 해수욕장 가서",
          "original_form": "저는 저번에 제주도 갔을때 해수욕장 가서",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027972.1.19",
          "form": "바다보기만 했는데",
          "original_form": "바다보기만 했는데",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027972.1.20",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027972.1.21",
