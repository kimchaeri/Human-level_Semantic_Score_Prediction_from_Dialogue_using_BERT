      "utterance": [
        {
          "id": "MDRW2100013930.1.1",
          "form": "아르바이트를 해봤어요?",
          "original_form": "아르바이트를 해봤어요?",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013930.1.2",
          "form": "네네 한 번 해봤어요",
          "original_form": "네네 한 번 해봤어요",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013930.1.3",
          "form": "혹시 알바 해보셨나요??",
          "original_form": "혹시 알바 해보셨나요??",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013930.1.4",
          "form": "네 빵집",
          "original_form": "네 빵집",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013930.1.5",
          "form": "스파게티집이요",
          "original_form": "스파게티집이요",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013930.1.6",
          "form": "아 두 번 해보셨어요?",
          "original_form": "아 두 번 해보셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013930.1.7",
          "form": "저도 식당에서 알바했었어요",
          "original_form": "저도 식당에서 알바했었어요",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013930.1.8",
          "form": "오오 무슨식당이요",
          "original_form": "오오 무슨식당이요",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013930.1.9",
          "form": "그게 좀 특이한 식당이었는데",
          "original_form": "그게 좀 특이한 식당이었는데",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013930.1.10",
          "form": "특이해요?",
          "original_form": "특이해요?",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013930.1.11",
          "form": "네네 편백나무찜 하는 데였어요",
          "original_form": "네네 편백나무찜 하는 데였어요",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013930.1.12",
          "form": "근데 수제맥주도 팔고 그런...",
          "original_form": "근데 수제맥주도 팔고 그런...",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013930.1.13",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013930.1.14",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013930.1.15",
          "form": "서빙이요?",
          "original_form": "서빙이요?",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013930.1.16",
          "form": "네넵 지금은 그만뒀어요ㅋㅋ",
          "original_form": "네넵 지금은 그만뒀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013930.1.17",
          "form": "지금 알바하고계세요??",
          "original_form": "지금 알바하고계세요??",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013930.1.18",
          "form": "아니요 ㅋㅋ",
          "original_form": "아니요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100013930.1.19",
          "form": "나중에 카페 차리고 싶어요",
          "original_form": "나중에 카페 차리고 싶어요",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100013930.1.20",
          "form": "오 멋있네요",
          "original_form": "오 멋있네요",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100013930.1.21",
          "form": "분위기 좋거나 특색 있는 카페 가면 기분전환 되고 참 좋은거 같아요ㅎㅎ",
          "original_form": "분위기 좋거나 특색 있는 카페 가면 기분전환 되고 참 좋은거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100013930.1.22",
          "form": "맞아요 취미에요",
          "original_form": "맞아요 취미에요",
          "speaker_id": "1",
          "time": "20210825 19:45"
        }
