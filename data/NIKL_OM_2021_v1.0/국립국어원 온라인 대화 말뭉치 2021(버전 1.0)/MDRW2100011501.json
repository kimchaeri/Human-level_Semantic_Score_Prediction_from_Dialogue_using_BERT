      "utterance": [
        {
          "id": "MDRW2100011501.1.1",
          "form": "마카롱 좋아하세요?ㅎ",
          "original_form": "마카롱 좋아하세요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100011501.1.2",
          "form": "좋아해요.",
          "original_form": "좋아해요.",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011501.1.3",
          "form": "무슨맛 좋아하세요?",
          "original_form": "무슨맛 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011501.1.4",
          "form": "전 사실 마카롱 잘안먹어서 ㅋ",
          "original_form": "전 사실 마카롱 잘안먹어서 ㅋ",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011501.1.5",
          "form": "다들맛나다는데",
          "original_form": "다들맛나다는데",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011501.1.6",
          "form": "좋아하는 사람들 많죠",
          "original_form": "좋아하는 사람들 많죠",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011501.1.7",
          "form": "빵종류는",
          "original_form": "빵종류는",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011501.1.8",
          "form": "전 크림치즈들어간거좋아해요",
          "original_form": "전 크림치즈들어간거좋아해요",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011501.1.9",
          "form": "밀크크레이프도",
          "original_form": "밀크크레이프도",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011501.1.10",
          "form": "냐옹님은요?",
          "original_form": "냐옹님은요?",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011501.1.11",
          "form": "저도 치즈빵요",
          "original_form": "저도 치즈빵요",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011501.1.12",
          "form": "녹여먹어요",
          "original_form": "녹여먹어요",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011501.1.13",
          "form": "녹여서요?ㅎ",
          "original_form": "녹여서요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011501.1.14",
          "form": "전자렌지요",
          "original_form": "전자렌지요",
          "speaker_id": "2",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011501.1.15",
          "form": "그럼 다 녹아서 이상하지않나요?",
          "original_form": "그럼 다 녹아서 이상하지않나요?",
          "speaker_id": "1",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011501.1.16",
          "form": "살짝해야죠",
          "original_form": "살짝해야죠",
          "speaker_id": "2",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011501.1.17",
          "form": "식사는 하셨어요?",
          "original_form": "식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011501.1.18",
          "form": "네, 간단하게 했어요.",
          "original_form": "네, 간단하게 했어요.",
          "speaker_id": "2",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011501.1.19",
          "form": "전 바지락칼국수 먹고왔어요",
          "original_form": "전 바지락칼국수 먹고왔어요",
          "speaker_id": "1",
          "time": "20210824 13:54"
        }
      ]
