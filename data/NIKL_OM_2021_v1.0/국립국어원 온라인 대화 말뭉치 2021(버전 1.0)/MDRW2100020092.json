      "utterance": [
        {
          "id": "MDRW2100020092.1.1",
          "form": "모임이 앱테크지만 목표는 거의 집이지 싶어요 ㅎㅎ",
          "original_form": "모임이 앱테크지만 목표는 거의 집이지 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020092.1.2",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020092.1.3",
          "form": "저도 궁극적인 목표는 그쪽이라",
          "original_form": "저도 궁극적인 목표는 그쪽이라",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020092.1.4",
          "form": "2차 목표는 뭔데영",
          "original_form": "2차 목표는 뭔데영",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020092.1.5",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020092.1.6",
          "form": "2차목표가 이제 생기게되ㅛ어요ㅎ",
          "original_form": "2차목표가 이제 생기게되ㅛ어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020092.1.7",
          "form": "전에 이야기했잖아요ㅎ 이혼하게됫어요ㅎㅎ",
          "original_form": "전에 이야기했잖아요ㅎ 이혼하게됫어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020092.1.8",
          "form": "어쩌다보니ㅎㅎ 그래서 1차목표가 없어졌어요ㅎ",
          "original_form": "어쩌다보니ㅎㅎ 그래서 1차목표가 없어졌어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020092.1.9",
          "form": "아  그때 어쩌다 들었지요",
          "original_form": "아  그때 어쩌다 들었지요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020092.1.10",
          "form": "이제는ㅎ 2차목표가 딸이죠뭐ㅎ",
          "original_form": "이제는ㅎ 2차목표가 딸이죠뭐ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020092.1.11",
          "form": "아가 어려요?",
          "original_form": "아가 어려요?",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020092.1.12",
          "form": "그땐 이집을 유지하는게 가족을 지키는 목표였다면",
          "original_form": "그땐 이집을 유지하는게 가족을 지키는 목표였다면",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020092.1.13",
          "form": "지금은 내가 재정적으로 건전해지는거",
          "original_form": "지금은 내가 재정적으로 건전해지는거",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020092.1.14",
          "form": "딸이 4살이에요ㅎ",
          "original_form": "딸이 4살이에요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020092.1.15",
          "form": "어멋 귀여울 나이",
          "original_form": "어멋 귀여울 나이",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020092.1.16",
          "form": "목표가 있는거 좋은거 같아요 ㅎㅎ",
          "original_form": "목표가 있는거 좋은거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020092.1.17",
          "form": "말도 안듣죠뭐ㅎㅎ",
          "original_form": "말도 안듣죠뭐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020092.1.18",
          "form": "목표를 정해야 저는 뭐든하더라구요ㅎ",
          "original_form": "목표를 정해야 저는 뭐든하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020092.1.19",
          "form": "딸에게 부끄러운 아빠가 안되는게 제 목표에요ㅎ",
          "original_form": "딸에게 부끄러운 아빠가 안되는게 제 목표에요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020092.1.20",
          "form": "2차목표ㅎ",
          "original_form": "2차목표ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020092.1.21",
          "form": "목표를 잘 이뤄보아용!! 열심히 노력해서 ㅎㅎ",
          "original_form": "목표를 잘 이뤄보아용!! 열심히 노력해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020092.1.22",
          "form": "네네ㅎㅎ 그동안 마이너스생긴거 갚고ㅎ",
          "original_form": "네네ㅎㅎ 그동안 마이너스생긴거 갚고ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020092.1.23",
          "form": "대출도 없애야죠ㅎㅎ",
          "original_form": "대출도 없애야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020092.1.24",
          "form": "그래야 딸이 나중에 대학교가거나 하고싶은거 있을때 다해줄수 있을것같애요ㅎ",
          "original_form": "그래야 딸이 나중에 대학교가거나 하고싶은거 있을때 다해줄수 있을것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020092.1.25",
          "form": "목표 이루게 열심히 돕고!! 앱테크 해나가 보아요 ㅎㅎ",
          "original_form": "목표 이루게 열심히 돕고!! 앱테크 해나가 보아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020092.1.26",
          "form": "ㅎㅎ 잘부탁해요ㅎ",
          "original_form": "ㅎㅎ 잘부탁해요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020092.1.27",
          "form": "네넵 저두여",
          "original_form": "네넵 저두여",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020092.1.28",
          "form": "많이 배우고 있습니다. 앱테크ㅎㅎ",
