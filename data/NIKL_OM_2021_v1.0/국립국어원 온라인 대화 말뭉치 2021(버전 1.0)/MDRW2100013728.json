      "utterance": [
        {
          "id": "MDRW2100013728.1.1",
          "form": "요즘 하시는 운동 있으세요?",
          "original_form": "요즘 하시는 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100013728.1.2",
          "form": "네 저 수영이요!",
          "original_form": "네 저 수영이요!",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100013728.1.3",
          "form": "수영 할 줄 아세요?",
          "original_form": "수영 할 줄 아세요?",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100013728.1.4",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100013728.1.5",
          "form": "근데 배워보고 싶어요",
          "original_form": "근데 배워보고 싶어요",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100013728.1.6",
          "form": "강추입니다!!",
          "original_form": "강추입니다!!",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100013728.1.7",
          "form": "어떤 운동 좋아하세요?",
          "original_form": "어떤 운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100013728.1.8",
          "form": "배드민턴 좋아해요",
          "original_form": "배드민턴 좋아해요",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100013728.1.9",
          "form": "name2 님도 좋아하시나요?",
          "original_form": "&name2& 님도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100013728.1.10",
          "form": "네ㅋㅋ재밌어요",
          "original_form": "네ㅋㅋ재밌어요",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100013728.1.11",
          "form": "ㅋㅋ오른쪽만 근육 생기는거 같긴 하더라고요",
          "original_form": "ㅋㅋ오른쪽만 근육 생기는거 같긴 하더라고요",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100013728.1.12",
          "form": "아 맞아요ㅋㅋㅋ 그래도 재밌어요~",
          "original_form": "아 맞아요ㅋㅋㅋ 그래도 재밌어요~",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100013728.1.13",
          "form": "요즘에는 그냥 공원에서 러닝 정도 하는데 체력때문에 꾸준히 해보려고요 ㅣ",
          "original_form": "요즘에는 그냥 공원에서 러닝 정도 하는데 체력때문에 꾸준히 해보려고요 ㅣ",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100013728.1.14",
          "form": "오 요새 런데이 이런거 있던데",
          "original_form": "오 요새 런데이 이런거 있던데",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100013728.1.15",
          "form": "써보셨나요??",
          "original_form": "써보셨나요??",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100013728.1.16",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100013728.1.17",
          "form": "저도 안써봤어요ㅋㅋㅋㅋ",
          "original_form": "저도 안써봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100013728.1.18",
          "form": "애플워치는요??",
          "original_form": "애플워치는요??",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100013728.1.19",
          "form": "지금 찾아보고 왔는데 러닝할 때 도움되겠네요",
          "original_form": "지금 찾아보고 왔는데 러닝할 때 도움되겠네요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100013728.1.20",
          "form": "워치는 안 샀어요",
          "original_form": "워치는 안 샀어요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100013728.1.21",
          "form": "워치 쓰시나요?",
          "original_form": "워치 쓰시나요?",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100013728.1.22",
          "form": "아니요 저도 없어욬ㅋㅋㅋ",
          "original_form": "아니요 저도 없어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100013728.1.23",
          "form": "앗ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100013728.1.24",
          "form": "수영할때 쓰는분들 계시더라고요",
          "original_form": "수영할때 쓰는분들 계시더라고요",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100013728.1.25",
          "form": "헉 방수도 되나보네요",
          "original_form": "헉 방수도 되나보네요",
