      "utterance": [
        {
          "id": "MDRW2100026146.1.1",
          "form": "여행 갈수있다면 가고싶은곳있어요?",
          "original_form": "여행 갈수있다면 가고싶은곳있어요?",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026146.1.2",
          "form": "아니면 가봤던곳중에 기억남는곳이나",
          "original_form": "아니면 가봤던곳중에 기억남는곳이나",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026146.1.3",
          "form": "저는 일본이",
          "original_form": "저는 일본이",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026146.1.4",
          "form": "너무가고싶어요",
          "original_form": "너무가고싶어요",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026146.1.5",
          "form": "사실 일본이가고싶다기보단",
          "original_form": "사실 일본이가고싶다기보단",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026146.1.6",
          "form": "일본에 교환갓던때로 돌아갈뤠..",
          "original_form": "일본에 교환갓던때로 돌아갈뤠..",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026146.1.7",
          "form": "ㅋㅋㅋㅋ저는 해외여행을 일본밖에 안가봐서 다른곳도 가보고싶어요ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ저는 해외여행을 일본밖에 안가봐서 다른곳도 가보고싶어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026146.1.8",
          "form": "원래 뉴욕 가려다가 못갔는데 코로나 터져서 가지도못하고",
          "original_form": "원래 뉴욕 가려다가 못갔는데 코로나 터져서 가지도못하고",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026146.1.9",
          "form": "넘 가고싶다..",
          "original_form": "넘 가고싶다..",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026146.1.10",
          "form": "뉴욕도 가고싶네",
          "original_form": "뉴욕도 가고싶네",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026146.1.11",
          "form": "저는 올랜도 제일가고싶어요 ㅠㅠㅠ",
          "original_form": "저는 올랜도 제일가고싶어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026146.1.12",
          "form": "유니버셜스튜디오..ㅜ...",
          "original_form": "유니버셜스튜디오..ㅜ...",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026146.1.13",
          "form": "유니버셜 스튜디오 원래는 관심없었는데",
          "original_form": "유니버셜 스튜디오 원래는 관심없었는데",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026146.1.14",
          "form": "해리포터 본 후로 너무 가보고싶어요ㅜㅜ",
          "original_form": "해리포터 본 후로 너무 가보고싶어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026146.1.15",
          "form": "이럴수가......",
          "original_form": "이럴수가......",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026146.1.16",
          "form": "진짜존잼이에여",
          "original_form": "진짜존잼이에여",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026146.1.17",
          "form": "ㅠㅠㅠㅠㅠ저두",
          "original_form": "ㅠㅠㅠㅠㅠ저두",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026146.1.18",
          "form": "올랜도 호그스미드 너무가보고싶ㆍㅇ",
          "original_form": "올랜도 호그스미드 너무가보고싶ㆍㅇ",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026146.1.19",
          "form": "디즈니랜드도 가보고싶어",
          "original_form": "디즈니랜드도 가보고싶어",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026146.1.20",
          "form": "놀이기구는 못타지만..",
          "original_form": "놀이기구는 못타지만..",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026146.1.21",
          "form": "나중에 코로나 사라지면 저랑",
          "original_form": "나중에 코로나 사라지면 저랑",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.22",
          "form": "여행가요",
          "original_form": "여행가요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.23",
          "form": "코로나사라지면...",
          "original_form": "코로나사라지면...",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.24",
          "form": "덕질여행 가자",
          "original_form": "덕질여행 가자",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.25",
          "form": "언..제...?",
          "original_form": "언..제...?",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ조아요 ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ조아요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.27",
          "form": "ㅎㅎ.. ........",
          "original_form": "ㅎㅎ.. ........",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.28",
          "form": "디즈니랜드는",
          "original_form": "디즈니랜드는",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.29",
          "form": "ㅜㅜ 그래도 언젠가는 상황이 좋아지겠지",
          "original_form": "ㅜㅜ 그래도 언젠가는 상황이 좋아지겠지",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.30",
          "form": "타는거보다 볼게많아서",
          "original_form": "타는거보다 볼게많아서",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.31",
          "form": "괜차늘듯",
          "original_form": "괜차늘듯",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.32",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.33",
          "form": "그렇겟죠 ㅠ.ㅠㅠ",
          "original_form": "그렇겟죠 ㅠ.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026146.1.34",
          "form": "디즈니랜드 꼭 가보고싶었어",
          "original_form": "디즈니랜드 꼭 가보고싶었어",
          "speaker_id": "1",
          "time": "20210901 21:03"
        }
      ]
