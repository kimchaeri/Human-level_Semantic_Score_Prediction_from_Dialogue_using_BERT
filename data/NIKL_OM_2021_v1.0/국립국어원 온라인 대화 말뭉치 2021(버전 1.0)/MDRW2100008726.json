      "utterance": [
        {
          "id": "MDRW2100008726.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "2",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008726.1.2",
          "form": "아니요 키우고 싶어요",
          "original_form": "아니요 키우고 싶어요",
          "speaker_id": "1",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008726.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "1",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008726.1.4",
          "form": "저도 생각만",
          "original_form": "저도 생각만",
          "speaker_id": "2",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008726.1.5",
          "form": "키우면 햄스터 키우고 싶어요",
          "original_form": "키우면 햄스터 키우고 싶어요",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008726.1.6",
          "form": "털알러지가 있어서",
          "original_form": "털알러지가 있어서",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008726.1.7",
          "form": "돌돌이 필수죠",
          "original_form": "돌돌이 필수죠",
          "speaker_id": "2",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008726.1.8",
          "form": "고양이 있는 친구 집은 숨만 쉬어도 털이 붙더라고요",
          "original_form": "고양이 있는 친구 집은 숨만 쉬어도 털이 붙더라고요",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008726.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008726.1.10",
          "form": "친구집에 검은바지 입고 갔다가 식겁",
          "original_form": "친구집에 검은바지 입고 갔다가 식겁",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008726.1.11",
          "form": "단모종은 그래도 괜찮던데 장모종 친구들은 정말...",
          "original_form": "단모종은 그래도 괜찮던데 장모종 친구들은 정말...",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008726.1.12",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008726.1.13",
          "form": "강아지가 좋으세요 고양이가 좋으세요",
          "original_form": "강아지가 좋으세요 고양이가 좋으세요",
          "speaker_id": "1",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008726.1.14",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008726.1.15",
          "form": "어떤 종이 좋으세요",
          "original_form": "어떤 종이 좋으세요",
          "speaker_id": "1",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008726.1.16",
          "form": "종은 잘 몰라서",
          "original_form": "종은 잘 몰라서",
          "speaker_id": "2",
          "time": "20210822 19:52"
        }
