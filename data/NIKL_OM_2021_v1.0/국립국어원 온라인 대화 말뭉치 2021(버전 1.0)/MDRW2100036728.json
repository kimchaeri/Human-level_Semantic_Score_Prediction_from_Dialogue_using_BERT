        },
        {
          "id": "MDRW2100036728.1.2",
          "form": "웅웅 ㅋㅋㅋ",
          "original_form": "웅웅 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100036728.1.3",
          "form": "애들 머리쓴대잉",
          "original_form": "애들 머리쓴대잉",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100036728.1.4",
          "form": "할아버지 불러서  몰래 사오더라",
          "original_form": "할아버지 불러서  몰래 사오더라",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100036728.1.5",
          "form": "갖고 싶은 것도 많고",
          "original_form": "갖고 싶은 것도 많고",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100036728.1.6",
          "form": "야 대박이네 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "야 대박이네 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100036728.1.7",
          "form": "많아 갖고싶은거 많지 ㅠㅠ",
          "original_form": "많아 갖고싶은거 많지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100036728.1.8",
          "form": "애들한테 진짜 돈 많이 들어간다.",
          "original_form": "애들한테 진짜 돈 많이 들어간다.",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100036728.1.9",
          "form": "아니 이번에 우미린 언니가 우유 24팩넘게 갖다줬거든??",
          "original_form": "아니 이번에 우미린 언니가 우유 24팩넘게 갖다줬거든??",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100036728.1.10",
          "form": "한번씩 애들와서 막 우유 찾고 이러니깐 언니가 미안했는지 갖다주더라구",
          "original_form": "한번씩 애들와서 막 우유 찾고 이러니깐 언니가 미안했는지 갖다주더라구",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100036728.1.11",
          "form": "근데 그거.........며칠만에 다먹음 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 그거.........며칠만에 다먹음 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100036728.1.12",
          "form": "그래도 우유 니까 잘 먹으니까 좋다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래도 우유 니까 잘 먹으니까 좋다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036728.1.13",
          "form": "우린 맨날 초코우유 다ㅠ",
          "original_form": "우린 맨날 초코우유 다ㅠ",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036728.1.14",
          "form": "그러니깐은 ㅋㅋ 우리도 매일 초코우유 ㅋㅋㅋ",
          "original_form": "그러니깐은 ㅋㅋ 우리도 매일 초코우유 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036728.1.15",
          "form": "아 이제 학교 하고 너희 집가는게 코스네 ㅋㅋㅋ",
          "original_form": "아 이제 학교 하고 너희 집가는게 코스네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036728.1.16",
          "form": "아니면은 후레이크 ㅋㅋ",
          "original_form": "아니면은 후레이크 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036728.1.17",
          "form": "우리집도 갔다가 다른친구들집도 갔다가 ㅋㅋ",
          "original_form": "우리집도 갔다가 다른친구들집도 갔다가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036728.1.18",
          "form": "애들  간식도 진짜 감당 못한다ㅠ",
          "original_form": "애들  간식도 진짜 감당 못한다ㅠ",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036728.1.19",
          "form": "혹시 한샘 핫딜 없나,,??",
          "original_form": "혹시 한샘 핫딜 없나,,??",
          "speaker_id": "2",
          "time": "20210831 10:42"
        },
        {
          "id": "MDRW2100036728.1.20",
          "form": "우린 결국 침대랑 옷장 사야할듯..",
          "original_form": "우린 결국 침대랑 옷장 사야할듯..",
          "speaker_id": "2",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100036728.1.21",
          "form": "아고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ결국은 ㅋㅋㅋ 사나요~~",
          "original_form": "아고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ결국은 ㅋㅋㅋ 사나요~~",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036728.1.22",
          "form": "그래야 할 것 같습니다ㅋㅋㅋㅋ",
          "original_form": "그래야 할 것 같습니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100036728.1.23",
          "form": "지마켓으로 주문했다가",
          "original_form": "지마켓으로 주문했다가",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100036728.1.24",
          "form": "핫딜 잘 안뜨긴하던데 ㅋㅋ 일단은 함 볼게 ㅋㅋ",
          "original_form": "핫딜 잘 안뜨긴하던데 ㅋㅋ 일단은 함 볼게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100036728.1.25",
          "form": "쿠폰 쓰면 더 쌀 것 같아서",
          "original_form": "쿠폰 쓰면 더 쌀 것 같아서",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100036728.1.26",
          "form": "취소하고 다시 결제하려고 했는데",
          "original_form": "취소하고 다시 결제하려고 했는데",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
        {
          "id": "MDRW2100036728.1.27",
          "form": "행사가 어제까지 였네요ㅠ",
          "original_form": "행사가 어제까지 였네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 10:51"
        },
