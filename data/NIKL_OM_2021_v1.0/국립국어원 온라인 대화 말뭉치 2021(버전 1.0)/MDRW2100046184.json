      "utterance": [
        {
          "id": "MDRW2100046184.1.1",
          "form": "스벅메뉴 중에 뭐 좋아하세요?",
          "original_form": "스벅메뉴 중에 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046184.1.2",
          "form": "저 어제 신메뉴마셔봤어요",
          "original_form": "저 어제 신메뉴마셔봤어요",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100046184.1.3",
          "form": "오오 어때요??",
          "original_form": "오오 어때요??",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046184.1.4",
          "form": "녹자 글레이즈? 이런이름이었는데 엄청 달고 맛있더라구오ㅡ",
          "original_form": "녹자 글레이즈? 이런이름이었는데 엄청 달고 맛있더라구오ㅡ",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046184.1.5",
          "form": "오 녹차는 맛없을 수가 없죠! 완전 맛있을 거 같아요",
          "original_form": "오 녹차는 맛없을 수가 없죠! 완전 맛있을 거 같아요",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046184.1.6",
          "form": "신메뉴있을동안 자주먹을것같아요",
          "original_form": "신메뉴있을동안 자주먹을것같아요",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100046184.1.7",
          "form": "신메뉴는 한정이니까 부지런히 먹어야죠!",
          "original_form": "신메뉴는 한정이니까 부지런히 먹어야죠!",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046184.1.8",
          "form": "맞아요 ㅎㅎㅎ커피좋아하세요?",
          "original_form": "맞아요 ㅎㅎㅎ커피좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046184.1.9",
          "form": "네! 커피 없음 좀비예요ㅠㅠ",
          "original_form": "네! 커피 없음 좀비예요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046184.1.10",
          "form": "ㅋㅋㅋㅋ저도오늘 점심에 하나 마셔야할것같아요",
          "original_form": "ㅋㅋㅋㅋ저도오늘 점심에 하나 마셔야할것같아요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046184.1.11",
          "form": "그죠 점심  때 안 먹음 못 버텨요ㅠㅠ",
          "original_form": "그죠 점심  때 안 먹음 못 버텨요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046184.1.12",
          "form": "맞아요 식곤증와요",
          "original_form": "맞아요 식곤증와요",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046184.1.13",
          "form": "오늘은 따뜻한라떼먹을거에요",
          "original_form": "오늘은 따뜻한라떼먹을거에요",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046184.1.14",
          "form": "전 민초라떼가 땡기네요",
          "original_form": "전 민초라떼가 땡기네요",
          "speaker_id": "1",
          "time": "20210908 11:06"
        }
      ]
