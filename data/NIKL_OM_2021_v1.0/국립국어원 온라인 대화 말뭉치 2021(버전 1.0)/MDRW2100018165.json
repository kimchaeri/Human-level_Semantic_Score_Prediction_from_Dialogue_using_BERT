        {
          "id": "MDRW2100018165.1.1",
          "form": "name2은 그럼 법카로 드신 거겠네요",
          "original_form": "&name2&은 그럼 법카로 드신 거겠네요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018165.1.2",
          "form": "야근식사는 법카죠",
          "original_form": "야근식사는 법카죠",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018165.1.3",
          "form": "포괄임금제라 수당있는것도아닌데",
          "original_form": "포괄임금제라 수당있는것도아닌데",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018165.1.4",
          "form": "힁!!",
          "original_form": "힁!!",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018165.1.5",
          "form": "어이없어!",
          "original_form": "어이없어!",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018165.1.6",
          "form": "식대 빵빵하게 줘라!",
          "original_form": "식대 빵빵하게 줘라!",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018165.1.7",
          "form": "참나 참나",
          "original_form": "참나 참나",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018165.1.8",
          "form": "소확횡으로 사무용품 좋은거 주문하구",
          "original_form": "소확횡으로 사무용품 좋은거 주문하구",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018165.1.9",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018165.1.10",
          "form": "휴..",
          "original_form": "휴..",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018165.1.11",
          "form": "소확횡으로 믹스커피",
          "original_form": "소확횡으로 믹스커피",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018165.1.12",
          "form": "맥심 신상주문했어요",
          "original_form": "맥심 신상주문했어요",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018165.1.13",
          "form": "남자들잡지맥심x 커피맥심o",
          "original_form": "남자들잡지맥심x 커피맥심o",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018165.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018165.1.15",
          "form": "신상 나왔어요??",
          "original_form": "신상 나왔어요??",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018165.1.16",
          "form": "두근",
          "original_form": "두근",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018165.1.17",
          "form": "그 뭐냐광고",
          "original_form": "그 뭐냐광고",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018165.1.18",
          "form": "슈프림라떼",
          "original_form": "슈프림라떼",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018165.1.19",
          "form": "신상이라고비싸도 회삿돈으로먹구욧",
          "original_form": "신상이라고비싸도 회삿돈으로먹구욧",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018165.1.20",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018165.1.21",
          "form": "간식은 주로 뭐시키세요 ㅋㅋㅋㅋ",
          "original_form": "간식은 주로 뭐시키세요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018165.1.22",
          "form": "전 브라우니 헤헤",
          "original_form": "전 브라우니 헤헤",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018165.1.23",
          "form": "저희는 간식딱히 사는거없이 커피만사고요",
          "original_form": "저희는 간식딱히 사는거없이 커피만사고요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018165.1.24",
          "form": "브라우니맛있죠",
          "original_form": "브라우니맛있죠",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018165.1.25",
          "form": "아하 그러시구나! 슈프림라떼 저도 귱금해요",
          "original_form": "아하 그러시구나! 슈프림라떼 저도 귱금해요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018165.1.26",
          "form": "아직 안온거죠",
          "original_form": "아직 안온거죠",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018165.1.27",
          "form": "아뇨 마트주문오로먹고있어요",
          "original_form": "아뇨 마트주문오로먹고있어요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018165.1.28",
          "form": "화이트골드와 비교하면 더 카페라떼 모방한 느낌",
          "original_form": "화이트골드와 비교하면 더 카페라떼 모방한 느낌",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018165.1.29",
          "form": "드셔보세요",
          "original_form": "드셔보세요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018165.1.30",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018165.1.31",
          "form": "오 카페라떼...",
          "original_form": "오 카페라떼...",
