        },
        {
          "id": "MDRW2100005250.1.4",
          "form": "ㅎ 저도 졸업한지가",
          "original_form": "ㅎ 저도 졸업한지가",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005250.1.5",
          "form": "ㅋㅋ기억안나네요",
          "original_form": "ㅋㅋ기억안나네요",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005250.1.6",
          "form": "학교다닐땨가 좋았죠 ㅎ",
          "original_form": "학교다닐땨가 좋았죠 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005250.1.7",
          "form": "네",
          "original_form": "네",
