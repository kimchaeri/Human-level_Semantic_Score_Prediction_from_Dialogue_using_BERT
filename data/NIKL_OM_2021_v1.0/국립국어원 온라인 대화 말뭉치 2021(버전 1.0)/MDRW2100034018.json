      "utterance": [
        {
          "id": "MDRW2100034018.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100034018.1.2",
          "form": "안녕하세요. 맛집 자주 가보시나요?",
          "original_form": "안녕하세요. 맛집 자주 가보시나요?",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100034018.1.3",
          "form": "음 좋아하긴해요!",
          "original_form": "음 좋아하긴해요!",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100034018.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100034018.1.5",
          "form": "동네 맛집은 가끔 가요",
          "original_form": "동네 맛집은 가끔 가요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100034018.1.6",
          "form": "멀리도 찾아가시나요?",
          "original_form": "멀리도 찾아가시나요?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100034018.1.7",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100034018.1.8",
          "form": "여행지에 갔다면요ㅋㅋㅋ",
          "original_form": "여행지에 갔다면요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100034018.1.9",
          "form": "그렇죠ㅋㅋ 유명하면 대기도 길더라구요",
          "original_form": "그렇죠ㅋㅋ 유명하면 대기도 길더라구요",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100034018.1.10",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100034018.1.11",
          "form": "연돈이 대표적이죠",
          "original_form": "연돈이 대표적이죠",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100034018.1.12",
          "form": "아 맞아요. 대기 없이는 못먹을거 같아요ㅋㅋ",
          "original_form": "아 맞아요. 대기 없이는 못먹을거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100034018.1.13",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100034018.1.14",
          "form": "맛집이 좋긴해요저도",
          "original_form": "맛집이 좋긴해요저도",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100034018.1.15",
          "form": "덜유명한곳보단",
          "original_form": "덜유명한곳보단",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100034018.1.16",
          "form": "가끔 방송에 근처 맛집 나오면 가보긴 해요",
          "original_form": "가끔 방송에 근처 맛집 나오면 가보긴 해요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100034018.1.17",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100034018.1.18",
          "form": "비쥬얼이 이쁜곳도",
          "original_form": "비쥬얼이 이쁜곳도",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100034018.1.19",
          "form": "좋더라고요ㅋㅋㅋ",
          "original_form": "좋더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100034018.1.20",
          "form": "좋져ㅋㅋㅋ 사진찍기도 좋구요",
          "original_form": "좋져ㅋㅋㅋ 사진찍기도 좋구요",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100034018.1.21",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100034018.1.22",
          "form": "전그렇곳좋아해요",
          "original_form": "전그렇곳좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100034018.1.23",
          "form": "전",
          "original_form": "전",
          "speaker_id": "2",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100034018.1.24",
          "form": "비쥬얼좋은 맛집좋아해요ㅋㅋㅋㅋ",
          "original_form": "비쥬얼좋은 맛집좋아해요ㅋㅋㅋㅋ",
