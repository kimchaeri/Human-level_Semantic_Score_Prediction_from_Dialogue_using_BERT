        },
        {
          "id": "MDRW2100029696.1.2",
          "form": "아이즈원 노래 좋아요",
          "original_form": "아이즈원 노래 좋아요",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029696.1.3",
          "form": "어느 장르 좋아하세요",
          "original_form": "어느 장르 좋아하세요",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029696.1.4",
          "form": "전 아이돌 노래 좋아해요 여자아이돌 ㅎㅎ",
          "original_form": "전 아이돌 노래 좋아해요 여자아이돌 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100029696.1.5",
          "form": "저도요 특히 아이즈원",
          "original_form": "저도요 특히 아이즈원",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029696.1.6",
          "form": "피에스타 제일 좋아해요",
          "original_form": "피에스타 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029696.1.7",
          "form": "ㅋㅋ전 러블리즈도 좋어요 노래",
