      "utterance": [
        {
          "id": "MDRW2100040231.1.1",
          "form": "오랜만이에요 ! 점심 드셨나요",
          "original_form": "오랜만이에요 ! 점심 드셨나요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040231.1.2",
          "form": "네 점심 먹었어요ㅎㅎ",
          "original_form": "네 점심 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040231.1.3",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040231.1.4",
          "form": "저는 아침 대충 먹고 낮잠잤어요ㅋㅋㅋ",
          "original_form": "저는 아침 대충 먹고 낮잠잤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040231.1.5",
          "form": "이제 쿠키 먹으면서 티타임이나 가져볼까 합니다 후후후",
          "original_form": "이제 쿠키 먹으면서 티타임이나 가져볼까 합니다 후후후",
          "speaker_id": "2",
          "time": "20210904 15:36"
        },
        {
          "id": "MDRW2100040231.1.6",
          "form": "이야 멋진 주말을 보내고 계시네요ㅋㅋㅋ 무슨 쿠키 드세요?",
          "original_form": "이야 멋진 주말을 보내고 계시네요ㅋㅋㅋ 무슨 쿠키 드세요?",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100040231.1.7",
          "form": "쿠키세트를 선물받아가지고ㅋㅋㅋㅋ 녹차쿠키랑 얼그레이 쿠키인 것 같아요",
          "original_form": "쿠키세트를 선물받아가지고ㅋㅋㅋㅋ 녹차쿠키랑 얼그레이 쿠키인 것 같아요",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040231.1.8",
          "form": "음하하",
          "original_form": "음하하",
          "speaker_id": "2",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040231.1.9",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040231.1.10",
          "form": "진짜 맛있어보여요 으악",
          "original_form": "진짜 맛있어보여요 으악",
          "speaker_id": "1",
          "time": "20210904 15:38"
        },
        {
          "id": "MDRW2100040231.1.11",
          "form": "티는 뭐 드세요",
          "original_form": "티는 뭐 드세요",
          "speaker_id": "1",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100040231.1.12",
          "form": "티는 제가 좋아하는 홍차로ㅎㅎㅎ 근데 좀 덥네여",
          "original_form": "티는 제가 좋아하는 홍차로ㅎㅎㅎ 근데 좀 덥네여",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040231.1.13",
          "form": "ㅋㅋㅋㅋㅋ아직 날이 뜨뜻한데 따뜻한 음료 드셔서 그런가봐요",
          "original_form": "ㅋㅋㅋㅋㅋ아직 날이 뜨뜻한데 따뜻한 음료 드셔서 그런가봐요",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040231.1.14",
          "form": "저도 뭐 마시는 중인데 뭐 먹는지 맞춰보실래요?",
          "original_form": "저도 뭐 마시는 중인데 뭐 먹는지 맞춰보실래요?",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040231.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앗 완전좋아요~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앗 완전좋아요~",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040231.1.16",
          "form": "따뜻한 음료인가요?",
          "original_form": "따뜻한 음료인가요?",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040231.1.17",
          "form": "아니요~ 차가운 음료예요",
          "original_form": "아니요~ 차가운 음료예요",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040231.1.18",
          "form": "얼음을 띄웠나요?",
          "original_form": "얼음을 띄웠나요?",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040231.1.19",
          "form": "아니요! 얼음이 없습니다 ㅎㅎㅎ",
          "original_form": "아니요! 얼음이 없습니다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040231.1.20",
          "form": "색이 투명한가요 탁한가요?",
          "original_form": "색이 투명한가요 탁한가요?",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040231.1.21",
          "form": "탁합니다 예리한 질문이예요",
          "original_form": "탁합니다 예리한 질문이예요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        }
