        {
          "id": "MDRW2100017012.1.1",
          "form": "여행 좋아하세요",
          "original_form": "여행 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100017012.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017012.1.3",
          "form": "가장 최근에 다녀오신곳이 어디예요?",
          "original_form": "가장 최근에 다녀오신곳이 어디예요?",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017012.1.4",
          "form": "속초 다녀왔습니다",
          "original_form": "속초 다녀왔습니다",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017012.1.5",
          "form": "속초 맛있는 음식이 뭔가요?",
          "original_form": "속초 맛있는 음식이 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017012.1.6",
          "form": "회가 맛있죠",
          "original_form": "회가 맛있죠",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017012.1.7",
          "form": "무슨 회가 맛있나요?",
          "original_form": "무슨 회가 맛있나요?",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017012.1.8",
          "form": "광어회가 제일 맛있더라구요",
          "original_form": "광어회가 제일 맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100017012.1.9",
          "form": "해외여행 갔다온적 있어요?",
          "original_form": "해외여행 갔다온적 있어요?",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100017012.1.10",
          "form": "아직 없어요",
          "original_form": "아직 없어요",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100017012.1.11",
          "form": "어디 가보고 싶으세요?",
          "original_form": "어디 가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100017012.1.12",
          "form": "괌이나 하와이 가보고싶어요",
          "original_form": "괌이나 하와이 가보고싶어요",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100017012.1.13",
          "form": "이유가 있나요?",
          "original_form": "이유가 있나요?",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100017012.1.14",
          "form": "휴양하기 좋다고 들었어요",
          "original_form": "휴양하기 좋다고 들었어요",
          "speaker_id": "2",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100017012.1.15",
          "form": "관광파보다 휴양파인가요?",
          "original_form": "관광파보다 휴양파인가요?",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100017012.1.16",
          "form": "네 휴양파예요",
          "original_form": "네 휴양파예요",
          "speaker_id": "2",
