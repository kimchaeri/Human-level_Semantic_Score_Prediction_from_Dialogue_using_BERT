      "utterance": [
        {
          "id": "MDRW2100028035.1.1",
          "form": "잉 쓴거 봤니?",
          "original_form": "잉 쓴거 봤니?",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028035.1.2",
          "form": "보임",
          "original_form": "보임",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028035.1.3",
          "form": "왕 난 사라져서",
          "original_form": "왕 난 사라져서",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100028035.1.4",
          "form": "원래 이런거 한번 거하게 ㅁㅏㅇ해봐야 안하는데",
          "original_form": "원래 이런거 한번 거하게 ㅁㅏㅇ해봐야 안하는데",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100028035.1.5",
          "form": "그래도 전보다는 일러레 편을 들어주는 사람이 줄어든 것 같긴 한데... 모르지 나도 편향적인 사이트에 다녀서",
          "original_form": "그래도 전보다는 일러레 편을 들어주는 사람이 줄어든 것 같긴 한데... 모르지 나도 편향적인 사이트에 다녀서",
          "speaker_id": "1",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028035.1.6",
          "form": "이런건 욕해야되는디...ㅋㅋㅋ",
          "original_form": "이런건 욕해야되는디...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028035.1.7",
          "form": "일러레 편 들어주는 아가들은 어디 사는 사람들인지",
          "original_form": "일러레 편 들어주는 아가들은 어디 사는 사람들인지",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028035.1.8",
          "form": "꼭 만나보고 싶군요",
          "original_form": "꼭 만나보고 싶군요",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028035.1.9",
          "form": "근데 너무 신기하다 섭남파업 정도면 그렇게 인지도 낮은 것도 아닌데....",
          "original_form": "근데 너무 신기하다 섭남파업 정도면 그렇게 인지도 낮은 것도 아닌데....",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100028035.1.10",
          "form": "조치를 안한다는건",
          "original_form": "조치를 안한다는건",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028035.1.11",
          "form": "계약금도 안돌려받는건가?",
          "original_form": "계약금도 안돌려받는건가?",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028035.1.12",
          "form": "그럼 그럴 수 이ㅆ지",
          "original_form": "그럼 그럴 수 이ㅆ지",
          "speaker_id": "2",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028035.1.13",
          "form": "순 범죄자들이네",
          "original_form": "순 범죄자들이네",
          "speaker_id": "2",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028035.1.14",
          "form": "ㅇ... 매몰비용이라던데....",
          "original_form": "ㅇ... 매몰비용이라던데....",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028035.1.15",
          "form": "전체적으로 웹소설 시장이 제대로 굴러가는것 같진 않은ㅋㅋㅋ",
          "original_form": "전체적으로 웹소설 시장이 제대로 굴러가는것 같진 않은ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028035.1.16",
          "form": "과도기라 그런건가...",
          "original_form": "과도기라 그런건가...",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028035.1.17",
          "form": "아님 동인 마인드가 너무 팽배한가",
          "original_form": "아님 동인 마인드가 너무 팽배한가",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028035.1.18",
          "form": "이거 하면서 만화도 추천받음",
          "original_form": "이거 하면서 만화도 추천받음",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028035.1.19",
          "form": "네이버 웹툰이었는데",
          "original_form": "네이버 웹툰이었는데",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028035.1.20",
          "form": "강아지 나오는거",
          "original_form": "강아지 나오는거",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028035.1.21",
          "form": "그게 먼뎈ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그게 먼뎈ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028035.1.22",
          "form": "두개 추천받았는데 하나는 개를 낳았다 였구",
          "original_form": "두개 추천받았는데 하나는 개를 낳았다 였구",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028035.1.23",
          "form": "하나는 기억안남 네글자였는데ㅋㅋㅋㅋ",
          "original_form": "하나는 기억안남 네글자였는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100028035.1.24",
          "form": "나는 아는 거",
          "original_form": "나는 아는 거",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028035.1.25",
          "form": "오늘도 사랑스럽개 밖에 없는데",
          "original_form": "오늘도 사랑스럽개 밖에 없는데",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028035.1.26",
          "form": "네글자.... 롤랑롤랑?",
          "original_form": "네글자.... 롤랑롤랑?",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028035.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028035.1.28",
          "form": "강아지 일상툰이랬는데",
          "original_form": "강아지 일상툰이랬는데",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100028035.1.29",
          "form": "나의 어린 고양이와 늙은 개....",
          "original_form": "나의 어린 고양이와 늙은 개....",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028035.1.30",
          "form": "네 글자 맞출 의지조차 없움",
          "original_form": "네 글자 맞출 의지조차 없움",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028035.1.31",
          "form": "하지만 기억안나니깐 어쩔수없다",
          "original_form": "하지만 기억안나니깐 어쩔수없다",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028035.1.32",
          "form": "그냥 강아지만 나오면 되는거 아닌가?",
          "original_form": "그냥 강아지만 나오면 되는거 아닌가?",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028035.1.33",
          "form": "네글자면 극한견주?",
          "original_form": "네글자면 극한견주?",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028035.1.34",
          "form": "근데 이거 다음인데",
          "original_form": "근데 이거 다음인데",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028035.1.35",
          "form": "뭐 하나 맞지 않네ㅋㅋㅋ",
          "original_form": "뭐 하나 맞지 않네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:44"
        }
      ]
