      "utterance": [
        {
          "id": "MDRW2100026769.1.1",
          "form": "저는 자주갈땐 지명했는데",
          "original_form": "저는 자주갈땐 지명했는데",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026769.1.2",
          "form": "너무 친해지니까 팁도 부담스럽고",
          "original_form": "너무 친해지니까 팁도 부담스럽고",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026769.1.3",
          "form": "그래서 랜덤 배정 받는데 사바사라 더 잘해주십사 하고 걍 줘요",
          "original_form": "그래서 랜덤 배정 받는데 사바사라 더 잘해주십사 하고 걍 줘요",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026769.1.4",
          "form": "마사지 기기 써보셨나요?",
          "original_form": "마사지 기기 써보셨나요?",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026769.1.5",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026769.1.6",
          "form": "네 친구집에 잇어서 ㅋㅋㅋ",
          "original_form": "네 친구집에 잇어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026769.1.7",
          "form": "그거도 갠찬던데요 ㅋㅋㅋ",
          "original_form": "그거도 갠찬던데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026769.1.8",
          "form": "근데 집에 놓기너무큼",
          "original_form": "근데 집에 놓기너무큼",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026769.1.9",
          "form": "바디프렌즈 ㅋㅋㅋㅋ",
          "original_form": "바디프렌즈 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026769.1.10",
          "form": "그쵸 저도 없어요ㅎ",
          "original_form": "그쵸 저도 없어요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026769.1.11",
          "form": "걍 ㅜㅜ 마사지볼 이런거나 씁니다",
          "original_form": "걍 ㅜㅜ 마사지볼 이런거나 씁니다",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026769.1.12",
          "form": "세븐라이너 있긴한데",
          "original_form": "세븐라이너 있긴한데",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026769.1.13",
          "form": "땅콩볼 이런거요?",
          "original_form": "땅콩볼 이런거요?",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026769.1.14",
          "form": "폼롤러가 더 시원한듯",
          "original_form": "폼롤러가 더 시원한듯",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026769.1.15",
          "form": "네 땅콩! ㅋㅋ",
          "original_form": "네 땅콩! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026769.1.16",
          "form": "오오오오오",
          "original_form": "오오오오오",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026769.1.17",
          "form": "저도 폼롤로 써요",
          "original_form": "저도 폼롤로 써요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026769.1.18",
          "form": "셀룰라이트 없어지길 기원하면서",
          "original_form": "셀룰라이트 없어지길 기원하면서",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026769.1.19",
          "form": "ㅋㅋㅋㅋ안없어지네요 그건",
          "original_form": "ㅋㅋㅋㅋ안없어지네요 그건",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026769.1.20",
          "form": "그지만 시원해요 ㅋㅋㅋㅋ",
          "original_form": "그지만 시원해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026769.1.21",
          "form": "근데 잘안하게 되요ㅋㅋㅋ",
          "original_form": "근데 잘안하게 되요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026769.1.22",
          "form": "스트레칭 좋아하시나요?",
          "original_form": "스트레칭 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026769.1.23",
          "form": "넹... 사실 귀차는데 .. 하면 조으니까",
          "original_form": "넹... 사실 귀차는데 .. 하면 조으니까",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026769.1.24",
          "form": "그 하기가 어렵더라구여ㅋ",
          "original_form": "그 하기가 어렵더라구여ㅋ",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026769.1.25",
          "form": "맛난거 더 먹을라면 조금이라도 하는게 좋은데",
          "original_form": "맛난거 더 먹을라면 조금이라도 하는게 좋은데",
          "speaker_id": "2",
          "time": "20210902 00:08"
        },
        {
          "id": "MDRW2100026769.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026769.1.27",
          "form": "진짜 시간 내서 제대로하면 좋긴한데",
          "original_form": "진짜 시간 내서 제대로하면 좋긴한데",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026769.1.28",
          "form": "저도대충만합니다",
          "original_form": "저도대충만합니다",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026769.1.29",
          "form": "건강한 돼지가 되는것도 쉽지가 않아요",
          "original_form": "건강한 돼지가 되는것도 쉽지가 않아요",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026769.1.30",
          "form": "ㅋㅋㅋㅋㅋ 그조 따로 다이어트 하시나요",
          "original_form": "ㅋㅋㅋㅋㅋ 그조 따로 다이어트 하시나요",
          "speaker_id": "1",
          "time": "20210902 00:10"
        }
      ]
    }
