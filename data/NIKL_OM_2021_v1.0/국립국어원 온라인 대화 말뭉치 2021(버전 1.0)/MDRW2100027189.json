      "utterance": [
        {
          "id": "MDRW2100027189.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027189.1.2",
          "form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "original_form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100027189.1.3",
          "form": "안녕하세요!!! 네 강아지 키워요",
          "original_form": "안녕하세요!!! 네 강아지 키워요",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100027189.1.4",
          "form": "name1은 반려동물 있으세요?",
          "original_form": "&name1&은 반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100027189.1.5",
          "form": "어머 어떤아이인가요?",
          "original_form": "어머 어떤아이인가요?",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027189.1.6",
          "form": "푸들이에여!!! 4kg 좀 안돼요.",
          "original_form": "푸들이에여!!! 4kg 좀 안돼요.",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027189.1.7",
          "form": "저는 훌륭한 랜선집사요 ㅋㅋ",
          "original_form": "저는 훌륭한 랜선집사요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027189.1.8",
          "form": "와 푸들푸들",
          "original_form": "와 푸들푸들",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027189.1.9",
          "form": "저도 유투브에서 막 영상 찾아보고 그래요ㅋㅋ",
          "original_form": "저도 유투브에서 막 영상 찾아보고 그래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027189.1.10",
          "form": "초코 크림 블랙 실버  어떤아이에요",
          "original_form": "초코 크림 블랙 실버  어떤아이에요",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027189.1.11",
          "form": "갈색 푸들이에요 ㅋㅋ",
          "original_form": "갈색 푸들이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027189.1.12",
          "form": "저는 키울상황이 못돼서 누가 자랑하는거 듣는거도 좋아해요 ㅋㅋ",
          "original_form": "저는 키울상황이 못돼서 누가 자랑하는거 듣는거도 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027189.1.13",
          "form": "다들 양념치킨 같다고 하는",
          "original_form": "다들 양념치킨 같다고 하는",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027189.1.14",
          "form": "솔직히 저희 집 애라서 하는 말이 아니라 진짜 귀여워요",
          "original_form": "솔직히 저희 집 애라서 하는 말이 아니라 진짜 귀여워요",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027189.1.15",
          "form": "이름은 뭔가요?",
          "original_form": "이름은 뭔가요?",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027189.1.16",
          "form": "푸들 귀엽죠ㅜㅜㅜ",
          "original_form": "푸들 귀엽죠ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027189.1.17",
          "form": "몇살이에용",
          "original_form": "몇살이에용",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027189.1.18",
          "form": "여섯살이에여!",
          "original_form": "여섯살이에여!",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027189.1.19",
          "form": "사실 올해 초에 좀 아팠거든요. 막 mri 찍고 그랬는데",
          "original_form": "사실 올해 초에 좀 아팠거든요. 막 mri 찍고 그랬는데",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027189.1.20",
          "form": "완치되는 병은 아니지만 이제 많이 나았어요 ㅎㅎ",
          "original_form": "완치되는 병은 아니지만 이제 많이 나았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027189.1.21",
          "form": "에고 속상하셨겠어요ㅜㅜ",
          "original_form": "에고 속상하셨겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027189.1.22",
          "form": "첨엔 어떻게 키우게되셨어요?",
          "original_form": "첨엔 어떻게 키우게되셨어요?",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027189.1.23",
          "form": "사실 강아지를 진짜 키우고 싶었는데 상황 때문에 많이 고민했어요.",
          "original_form": "사실 강아지를 진짜 키우고 싶었는데 상황 때문에 많이 고민했어요.",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027189.1.24",
          "form": "끝까지 책임져야 한다고 생각하니까 쉽지 않더라구요.",
          "original_form": "끝까지 책임져야 한다고 생각하니까 쉽지 않더라구요.",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027189.1.25",
          "form": "근데 친한 친구가 고양이를 데려오고 너무 행복해하는 걸 보다보니... 넘어갔습니다 ㅠ",
          "original_form": "근데 친한 친구가 고양이를 데려오고 너무 행복해하는 걸 보다보니... 넘어갔습니다 ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027189.1.26",
          "form": "지금 너무 행복해요",
          "original_form": "지금 너무 행복해요",
