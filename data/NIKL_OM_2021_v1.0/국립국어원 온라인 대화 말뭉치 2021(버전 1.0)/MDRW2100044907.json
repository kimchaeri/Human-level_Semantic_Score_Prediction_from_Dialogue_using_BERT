      "utterance": [
        {
          "id": "MDRW2100044907.1.1",
          "form": "제목은 이쁘네ㅋ",
          "original_form": "제목은 이쁘네ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044907.1.2",
          "form": "한국제목은 사랑은연재중이래",
          "original_form": "한국제목은 사랑은연재중이래",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044907.1.3",
          "form": "남주가 하는말 당신처럼 염치없는 여자는 처음 봅니다",
          "original_form": "남주가 하는말 당신처럼 염치없는 여자는 처음 봅니다",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044907.1.4",
          "form": "나 이런거 좋아하네ㅋㅋ",
          "original_form": "나 이런거 좋아하네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044907.1.5",
          "form": "서로싸우는거ㅋㅋ",
          "original_form": "서로싸우는거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044907.1.6",
          "form": "ㅋㅋㅋ취향 개척했넉",
          "original_form": "ㅋㅋㅋ취향 개척했넉",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044907.1.7",
          "form": "여주 도른듯 ㅋㅋㅋ",
          "original_form": "여주 도른듯 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044907.1.8",
          "form": "개웃기다 ㅋㅋ",
          "original_form": "개웃기다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044907.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044907.1.10",
          "form": "그게 웃낀 포인트인든",
          "original_form": "그게 웃낀 포인트인든",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044907.1.11",
          "form": "ㅇㅇ개당당하게이상한짓하고",
          "original_form": "ㅇㅇ개당당하게이상한짓하고",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044907.1.12",
          "form": "아까 말해준 말만봐도 나 웃겼음ㅋ",
          "original_form": "아까 말해준 말만봐도 나 웃겼음ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044907.1.13",
          "form": "남주랑 말싸움해서 이김",
          "original_form": "남주랑 말싸움해서 이김",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044907.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044907.1.15",
          "form": "출판사에 일했어서 말빨 안질거같어ㅋㅋ",
          "original_form": "출판사에 일했어서 말빨 안질거같어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044907.1.16",
          "form": "그러네!!!",
          "original_form": "그러네!!!",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044907.1.17",
          "form": "그래서 말빨 좋나뵈",
          "original_form": "그래서 말빨 좋나뵈",
          "speaker_id": "2",
          "time": "20210908 00:12"
