        {
          "id": "MDRW2100023207.1.2",
          "form": "브랜드는없고",
          "original_form": "브랜드는없고",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023207.1.3",
          "form": "전 부리형 싫어해요",
          "original_form": "전 부리형 싫어해요",
