        },
        {
          "id": "MDRW2100023207.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023207.1.10",
          "form": "아아 맞아요맞아요",
          "original_form": "아아 맞아요맞아요",
