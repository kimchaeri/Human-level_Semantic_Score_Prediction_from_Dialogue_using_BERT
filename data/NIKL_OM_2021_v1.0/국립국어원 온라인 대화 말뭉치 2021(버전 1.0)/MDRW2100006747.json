      "utterance": [
        {
          "id": "MDRW2100006747.1.1",
          "form": "여행지서 기억에 남는 음식 .있나요",
          "original_form": "여행지서 기억에 남는 음식 .있나요",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100006747.1.2",
          "form": "국내는 저는 전라도음식들이..",
          "original_form": "국내는 저는 전라도음식들이..",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006747.1.3",
          "form": "다 맛나더라구요",
          "original_form": "다 맛나더라구요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006747.1.4",
          "form": "제주도에서는 작은 전복 ..,,머라하죠? 무슨탕인데 ㅎ",
          "original_form": "제주도에서는 작은 전복 ..,,머라하죠? 무슨탕인데 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006747.1.5",
          "form": "아 전라도 음식의.고장이죠",
          "original_form": "아 전라도 음식의.고장이죠",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006747.1.6",
          "form": "기사식당만가도 상다리부러지게 나오더라구요",
          "original_form": "기사식당만가도 상다리부러지게 나오더라구요",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100006747.1.7",
          "form": "깜짝놀랐어요",
          "original_form": "깜짝놀랐어요",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100006747.1.8",
          "form": "기억에남는 음식있으세요?",
          "original_form": "기억에남는 음식있으세요?",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100006747.1.9",
          "form": "아..제주도 음식..보말인가요?",
          "original_form": "아..제주도 음식..보말인가요?",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100006747.1.10",
          "form": "아 보말해물탕",
          "original_form": "아 보말해물탕",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006747.1.11",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006747.1.12",
          "form": "그게젤 맛나게 먹은거같아요",
          "original_form": "그게젤 맛나게 먹은거같아요",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006747.1.13",
          "form": "저도 보말칼국수 맛있게 먹었어요",
          "original_form": "저도 보말칼국수 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100006747.1.14",
          "form": "여행지 기억에 남는곳있으세요?",
          "original_form": "여행지 기억에 남는곳있으세요?",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006747.1.15",
          "form": "네 제주도 좋았구요.. 부산 경주 여수 거제도 좋았어요",
          "original_form": "네 제주도 좋았구요.. 부산 경주 여수 거제도 좋았어요",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006747.1.16",
          "form": "다 좋은곳이죠 ㅎ",
          "original_form": "다 좋은곳이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006747.1.17",
          "form": "저는 일본가서 캐리어 잃어버릴뻔해서",
          "original_form": "저는 일본가서 캐리어 잃어버릴뻔해서",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006747.1.18",
          "form": "진짜 기억에 남아요",
          "original_form": "진짜 기억에 남아요",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006747.1.19",
          "form": "헉 어쩌다가요?",
          "original_form": "헉 어쩌다가요?",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006747.1.20",
          "form": "공항에서 도쿄시내로가는데",
          "original_form": "공항에서 도쿄시내로가는데",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006747.1.21",
          "form": "기차같은거더라구요",
          "original_form": "기차같은거더라구요",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006747.1.22",
          "form": "그래서 캐리어는 다른칸에두고",
          "original_form": "그래서 캐리어는 다른칸에두고",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006747.1.23",
          "form": "자리에 앉아있다가",
          "original_form": "자리에 앉아있다가",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006747.1.24",
          "form": "내릴때 캐리어찾으러가니",
          "original_form": "내릴때 캐리어찾으러가니",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006747.1.25",
          "form": "뒤에 열차가 없더라구요\n어디선가 분리되서",
          "original_form": "뒤에 열차가 없더라구요\n어디선가 분리되서",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006747.1.26",
          "form": "다른곳으로 갔더라구요",
          "original_form": "다른곳으로 갔더라구요",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100006747.1.27",
          "form": "헉...그래서 찾는덴 얼마나 걸리셨나요",
          "original_form": "헉...그래서 찾는덴 얼마나 걸리셨나요",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006747.1.28",
          "form": "역무원이 내가방은 어디로 가고있다 이래가 그역에",
          "original_form": "역무원이 내가방은 어디로 가고있다 이래가 그역에",
          "speaker_id": "2",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006747.1.29",
          "form": "연락해서 있는지 확인하고 내려놓고저는 찾으러갔다오고 ㅜㅠ",
          "original_form": "연락해서 있는지 확인하고 내려놓고저는 찾으러갔다오고 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100006747.1.30",
          "form": "오후체크인이 저녁이 되어버렸죠",
          "original_form": "오후체크인이 저녁이 되어버렸죠",
          "speaker_id": "2",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100006747.1.31",
          "form": "의사소통은 원활하셨나요",
          "original_form": "의사소통은 원활하셨나요",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006747.1.32",
          "form": "저 일본어 할줄몰라요",
          "original_form": "저 일본어 할줄몰라요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006747.1.33",
          "form": "영어도 잘못하고",
          "original_form": "영어도 잘못하고",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006747.1.34",
          "form": "손짓발짓 콩글리쉬 ㅋ",
          "original_form": "손짓발짓 콩글리쉬 ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:03"
        }
      ]
