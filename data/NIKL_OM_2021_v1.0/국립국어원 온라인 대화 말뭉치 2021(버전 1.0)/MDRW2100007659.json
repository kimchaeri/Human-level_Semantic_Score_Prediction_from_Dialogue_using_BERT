      "utterance": [
        {
          "id": "MDRW2100007659.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007659.1.2",
          "form": "게임 잘 안하지만 카카오게임처럼 간단한건 좋아해요ㅋ",
          "original_form": "게임 잘 안하지만 카카오게임처럼 간단한건 좋아해요ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007659.1.3",
          "form": "게임하세요?",
          "original_form": "게임하세요?",
          "speaker_id": "2",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007659.1.4",
          "form": "브롤이요",
          "original_form": "브롤이요",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007659.1.5",
          "form": "폰게임",
          "original_form": "폰게임",
          "speaker_id": "1",
          "time": "20210822 09:26"
        },
        {
          "id": "MDRW2100007659.1.6",
          "form": "브롤 좋아하시는구나ㅋ",
          "original_form": "브롤 좋아하시는구나ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007659.1.7",
          "form": "얼마나 하신거예요?",
          "original_form": "얼마나 하신거예요?",
          "speaker_id": "2",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007659.1.8",
          "form": "1년정도",
          "original_form": "1년정도",
          "speaker_id": "1",
          "time": "20210822 09:36"
        },
        {
          "id": "MDRW2100007659.1.9",
          "form": "오래하셨네요.ㅋ",
          "original_form": "오래하셨네요.ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:37"
        },
        {
          "id": "MDRW2100007659.1.10",
          "form": "매일하세요?",
          "original_form": "매일하세요?",
          "speaker_id": "2",
          "time": "20210822 09:37"
        },
        {
          "id": "MDRW2100007659.1.11",
          "form": "네 출첵 이벤도 있어서",
          "original_form": "네 출첵 이벤도 있어서",
          "speaker_id": "1",
          "time": "20210822 09:38"
        },
        {
          "id": "MDRW2100007659.1.12",
          "form": "아 출첵 이벤트하면 뭐받는거있나요?",
          "original_form": "아 출첵 이벤트하면 뭐받는거있나요?",
          "speaker_id": "2",
          "time": "20210822 09:38"
        },
        {
          "id": "MDRW2100007659.1.13",
          "form": "코인요 모으면 캐릭사요",
          "original_form": "코인요 모으면 캐릭사요",
          "speaker_id": "1",
          "time": "20210822 09:39"
        },
        {
          "id": "MDRW2100007659.1.14",
          "form": "아 ㅋㅋ 캐릭사고 꾸미고도 재미있죠.ㅋㅋ",
          "original_form": "아 ㅋㅋ 캐릭사고 꾸미고도 재미있죠.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007659.1.15",
          "form": "업그레이드되면요ㅋ",
          "original_form": "업그레이드되면요ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007659.1.16",
          "form": "파워도 좋고",
          "original_form": "파워도 좋고",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007659.1.17",
          "form": "그러니깐요ㅋㅋ 브롤만 하시나요? 다른건 안하세요?ㅋ",
          "original_form": "그러니깐요ㅋㅋ 브롤만 하시나요? 다른건 안하세요?ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007659.1.18",
          "form": "브롤만도 벅차네여",
          "original_form": "브롤만도 벅차네여",
          "speaker_id": "1",
          "time": "20210822 09:44"
        }
      ]
