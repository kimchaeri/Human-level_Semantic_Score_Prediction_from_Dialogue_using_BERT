      "utterance": [
        {
          "id": "MDRW2100044231.1.1",
          "form": "엄마가 나물무침 여러개 해주셔서",
          "original_form": "엄마가 나물무침 여러개 해주셔서",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044231.1.2",
          "form": "맛나게 비벼먹었어요~~ㅎㅎ",
          "original_form": "맛나게 비벼먹었어요~~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044231.1.3",
          "form": "크 맛있었겠다 ㅋㅋ  난 카레 먹었엉!",
          "original_form": "크 맛있었겠다 ㅋㅋ  난 카레 먹었엉!",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044231.1.4",
          "form": "오~ 카레카레~~",
          "original_form": "오~ 카레카레~~",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044231.1.5",
          "form": "만들어서 먹은거얌??",
          "original_form": "만들어서 먹은거얌??",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044231.1.6",
          "form": "엄마가 해주셨오!ㅋㅋ",
          "original_form": "엄마가 해주셨오!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044231.1.7",
          "form": "카레~ 맛낫겠군요~//",
          "original_form": "카레~ 맛낫겠군요~//",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044231.1.8",
          "form": "카레에 김치나 깍두기 딱 올려놓고~",
          "original_form": "카레에 김치나 깍두기 딱 올려놓고~",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044231.1.9",
          "form": "웅웅~",
          "original_form": "웅웅~",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044231.1.10",
          "form": "한그릇 뚝딱~",
          "original_form": "한그릇 뚝딱~",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044231.1.11",
          "form": "점심저녁 다 카레??",
          "original_form": "점심저녁 다 카레??",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044231.1.12",
          "form": "저녁만!ㅋㅋ점심엔 그냥 밥ㅋㅋ",
          "original_form": "저녁만!ㅋㅋ점심엔 그냥 밥ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044231.1.13",
          "form": "난 점심저녁 둘다 비빔밥ㅋㅋㅋㅋ",
          "original_form": "난 점심저녁 둘다 비빔밥ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044231.1.14",
          "form": "오 짱인데 ㅋㅋㅋ",
          "original_form": "오 짱인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044231.1.15",
          "form": "양푼에 한껏 담아서 휘적휘적 비벼다가 후루룩짭쫩~//",
          "original_form": "양푼에 한껏 담아서 휘적휘적 비벼다가 후루룩짭쫩~//",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044231.1.16",
          "form": "크 넘 맛있었겠다",
          "original_form": "크 넘 맛있었겠다",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044231.1.17",
          "form": "웅우~ 비빔밥은 안 질리는.거 같아~",
          "original_form": "웅우~ 비빔밥은 안 질리는.거 같아~",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044231.1.18",
          "form": "연속으로 먹어도 맛나~ㅎㅎ",
          "original_form": "연속으로 먹어도 맛나~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044231.1.19",
          "form": "카레는 오뚜기? 바몬드?",
          "original_form": "카레는 오뚜기? 바몬드?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044231.1.20",
