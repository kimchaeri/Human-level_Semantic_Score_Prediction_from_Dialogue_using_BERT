      "utterance": [
        {
          "id": "MDRW2100034458.1.1",
          "form": "랩 댄스 작사 작고 모든걸 다 했네 이현도가",
          "original_form": "랩 댄스 작사 작고 모든걸 다 했네 이현도가",
          "speaker_id": "2",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100034458.1.2",
          "form": "듀오가 아니고 듀스 ㅋㅋ",
          "original_form": "듀오가 아니고 듀스 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100034458.1.3",
          "form": "만능이네ㅋ 울동생왈 군대 안갔다고 하던데ㅋ",
          "original_form": "만능이네ㅋ 울동생왈 군대 안갔다고 하던데ㅋ",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100034458.1.4",
          "form": "룰라 3 4 도 이현도가",
          "original_form": "룰라 3 4 도 이현도가",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034458.1.5",
          "form": "작곡 많이 햏네",
          "original_form": "작곡 많이 햏네",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034458.1.6",
          "form": "엄정화 노래랑 구본승 노래도 만들었다",
          "original_form": "엄정화 노래랑 구본승 노래도 만들었다",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034458.1.7",
          "form": "돈 많이 벌었겠군",
          "original_form": "돈 많이 벌었겠군",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034458.1.8",
          "form": "4위는 신승훈이다",
          "original_form": "4위는 신승훈이다",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100034458.1.9",
          "form": "역시 우리 예상대로",
          "original_form": "역시 우리 예상대로",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100034458.1.10",
          "form": "ㅋㅋ그러게 내가 유일하게 신승훈 노래 좀 안다",
          "original_form": "ㅋㅋ그러게 내가 유일하게 신승훈 노래 좀 안다",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100034458.1.11",
          "form": "신승훈 노래 좋았지",
          "original_form": "신승훈 노래 좋았지",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100034458.1.12",
          "form": "그후로 오랫동안  좋아 했는데",
          "original_form": "그후로 오랫동안  좋아 했는데",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100034458.1.13",
          "form": "히트곡이 많아. 근데 계속 혼자 사네",
          "original_form": "히트곡이 많아. 근데 계속 혼자 사네",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100034458.1.14",
          "form": "발라드라서 많이 안꾸며 지금봐도 안촌스럽다",
          "original_form": "발라드라서 많이 안꾸며 지금봐도 안촌스럽다",
