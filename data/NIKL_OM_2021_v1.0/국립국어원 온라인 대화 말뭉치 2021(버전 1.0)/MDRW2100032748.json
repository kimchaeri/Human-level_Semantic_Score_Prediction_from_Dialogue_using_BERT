      "utterance": [
        {
          "id": "MDRW2100032748.1.1",
          "form": "코로나 전에 국내 vs 해외 어디 선호하셨어요?",
          "original_form": "코로나 전에 국내 vs 해외 어디 선호하셨어요?",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100032748.1.2",
          "form": "전 국내요!",
          "original_form": "전 국내요!",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100032748.1.3",
          "form": "국내 최근어디가보셨어요?",
          "original_form": "국내 최근어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100032748.1.4",
          "form": "음.... 저는 아산쪽에 온천갔던거요!!",
          "original_form": "음.... 저는 아산쪽에 온천갔던거요!!",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100032748.1.5",
          "form": "오오 요즘 비와서 온천가고싶어요 따땃하게ㅋㅋ",
          "original_form": "오오 요즘 비와서 온천가고싶어요 따땃하게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100032748.1.6",
          "form": "전 경주갔는데 의외로 가볼곳이많더라구요",
          "original_form": "전 경주갔는데 의외로 가볼곳이많더라구요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100032748.1.7",
          "form": "와 경주",
          "original_form": "와 경주",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100032748.1.8",
          "form": "경주에서 어디어디 가셨어요?",
          "original_form": "경주에서 어디어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100032748.1.9",
          "form": "저녁야경보러 첨성대갓었고",
          "original_form": "저녁야경보러 첨성대갓었고",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100032748.1.10",
          "form": "대릉원인가 한바퀴걸었어요ㅎㅎ",
          "original_form": "대릉원인가 한바퀴걸었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100032748.1.11",
          "form": "경주빵 드셔보셨어요?ㅋㅋ",
          "original_form": "경주빵 드셔보셨어요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100032748.1.12",
          "form": "경주빵 먹어봤죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "경주빵 먹어봤죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100032748.1.13",
          "form": "저도 코로나 좀 풀리면",
          "original_form": "저도 코로나 좀 풀리면",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100032748.1.14",
          "form": "경주 여행가고 싶네요 ㅠㅠ",
          "original_form": "경주 여행가고 싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100032748.1.15",
          "form": "호텔같은곳보다 은근 조용하게 팬션같은곳도 운치잇더라구요ㅋ",
          "original_form": "호텔같은곳보다 은근 조용하게 팬션같은곳도 운치잇더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100032748.1.16",
          "form": "전 서울가서 막 이리저리 골목다니고파여!",
          "original_form": "전 서울가서 막 이리저리 골목다니고파여!",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100032748.1.17",
          "form": "제대로된맛집 광고말고용ㅋ 찾아다니며 ㅎㅎ",
          "original_form": "제대로된맛집 광고말고용ㅋ 찾아다니며 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100032748.1.18",
          "form": "앜ㅋㅋㅋㅋㅋ맞아요 ㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋㅋ맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100032748.1.19",
          "form": "서울이 아무래도 이것저것 다 있다보니까",
          "original_form": "서울이 아무래도 이것저것 다 있다보니까",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100032748.1.20",
          "form": "맛집도 많잖아요 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "맛집도 많잖아요 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100032748.1.21",
          "form": "네네 막 그냥들어가도 오래되서 맛잇는곳 이런곳ㅋㅋㅋ",
          "original_form": "네네 막 그냥들어가도 오래되서 맛잇는곳 이런곳ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100032748.1.22",
          "form": "여름휴가로 아산다녀오신거에요?",
          "original_form": "여름휴가로 아산다녀오신거에요?",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100032748.1.23",
          "form": "아산은 겨울에 갔었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아산은 겨울에 갔었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100032748.1.24",
          "form": "코시국 이전에... ㅠㅠ",
          "original_form": "코시국 이전에... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100032748.1.25",
          "form": "아아 이런 진짜ㅜㅜ",
          "original_form": "아아 이런 진짜ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100032748.1.26",
          "form": "코시국 올해는 꼭 끝나길... 여행갑시다!",
          "original_form": "코시국 올해는 꼭 끝나길... 여행갑시다!",
          "speaker_id": "1",
          "time": "20210908 00:26"
        }
      ]
    }
