      "utterance": [
        {
          "id": "MDRW2100014712.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014712.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014712.1.3",
          "form": "가장좋아하는계절이 뭐예요?",
          "original_form": "가장좋아하는계절이 뭐예요?",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014712.1.4",
          "form": "저는 겨울이요ㅎㅎ",
          "original_form": "저는 겨울이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014712.1.5",
          "form": "name2좋아하세요?",
          "original_form": "&name2&좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014712.1.6",
          "form": "닉넴은 name2이지만 ㅎㅎ가을을 제일좋아해요",
          "original_form": "닉넴은 &name2&이지만 ㅎㅎ가을을 제일좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014712.1.7",
          "form": "겨울좋아하는이유는 무엇인가요",
          "original_form": "겨울좋아하는이유는 무엇인가요",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014712.1.8",
          "form": "가을도 좋죠!!",
          "original_form": "가을도 좋죠!!",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014712.1.9",
          "form": "눈 오는걸 좋아해욬ㅋㅋㅋ",
          "original_form": "눈 오는걸 좋아해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014712.1.10",
          "form": "저도 눈오는거좋아해요 대신 따뜻한집에서 창문에 내리는 눈을 바라볼때만 ㅎㅎ",
          "original_form": "저도 눈오는거좋아해요 대신 따뜻한집에서 창문에 내리는 눈을 바라볼때만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014712.1.11",
          "form": "맞아요 ㅋㅋㅋㅋ 저도 눈오는건 좋지만 눈올때 돌아다니는건 불편하긴해요ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ 저도 눈오는건 좋지만 눈올때 돌아다니는건 불편하긴해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014712.1.12",
          "form": "벌써 여름이 다 지나간거같아요",
          "original_form": "벌써 여름이 다 지나간거같아요",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014712.1.13",
          "form": "네ㅜ 아직습하긴한데 장마끝나면 가을올것같아요",
          "original_form": "네ㅜ 아직습하긴한데 장마끝나면 가을올것같아요",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014712.1.14",
          "form": "벌써 아침저녁으로는 쌀쌀하더라고요",
          "original_form": "벌써 아침저녁으로는 쌀쌀하더라고요",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014712.1.15",
          "form": "애매한날씨긴해여 또 긴팔입기엔 더워서",
          "original_form": "애매한날씨긴해여 또 긴팔입기엔 더워서",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014712.1.16",
          "form": "아직 낮에는 긴팔입으면 덥죠 ㅠㅠ",
          "original_form": "아직 낮에는 긴팔입으면 덥죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014712.1.17",
          "form": "그래도 일기예보상으로는 내일은 비가 안온다고 해서 다행이에요",
          "original_form": "그래도 일기예보상으로는 내일은 비가 안온다고 해서 다행이에요",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014712.1.18",
          "form": "네 날도 좀 맑았으면 좋겠어요ㅜ",
          "original_form": "네 날도 좀 맑았으면 좋겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014712.1.19",
          "form": "진짜 며칠째 비 그리고 흐림ㅠㅠㅠ",
          "original_form": "진짜 며칠째 비 그리고 흐림ㅠㅠㅠ",
