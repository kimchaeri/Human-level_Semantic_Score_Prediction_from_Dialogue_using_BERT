      "utterance": [
        {
          "id": "MDRW2100011481.1.1",
          "form": "아그래? 지니어스는?",
          "original_form": "아그래? 지니어스는?",
          "speaker_id": "2",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011481.1.2",
          "form": "지니어스도 봤지!",
          "original_form": "지니어스도 봤지!",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011481.1.3",
          "form": "중간에 못본것도있긴하겠지만",
          "original_form": "중간에 못본것도있긴하겠지만",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011481.1.4",
          "form": "크라임씬은 정종연 아니지?",
          "original_form": "크라임씬은 정종연 아니지?",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011481.1.5",
          "form": "응 아니얌",
          "original_form": "응 아니얌",
          "speaker_id": "2",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011481.1.6",
          "form": "어제 대탈출 폐병원 봤어?",
          "original_form": "어제 대탈출 폐병원 봤어?",
          "speaker_id": "2",
          "time": "20210825 08:48"
        },
        {
          "id": "MDRW2100011481.1.7",
          "form": "응 어제봤어 \r\n좀비 한번 보고나서 보니까 안 무섭더라ㅋㅋ",
          "original_form": "응 어제봤어 \r\n좀비 한번 보고나서 보니까 안 무섭더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:55"
        },
        {
          "id": "MDRW2100011481.1.8",
          "form": "그치ㅋㅋ 김호들 원래 본명 뭐였지?ㅋㅋㅋㅋㅋ",
          "original_form": "그치ㅋㅋ 김호들 원래 본명 뭐였지?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100011481.1.9",
          "form": "도망가는거 짱웃김ㅋㅋ",
          "original_form": "도망가는거 짱웃김ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100011481.1.10",
          "form": "김동현!ㅋㅋㅋ그래서 김호들된거였구나\r\n찰떡같다ㅋㅋ",
          "original_form": "김동현!ㅋㅋㅋ그래서 김호들된거였구나\r\n찰떡같다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100011481.1.11",
          "form": "그치ㅋㅋ 순발력좋은 겁보라 웃김ㅠㅠㅋㅋ",
          "original_form": "그치ㅋㅋ 순발력좋은 겁보라 웃김ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100011481.1.12",
          "form": "아니 거기 좀비감옥들어가는 입구에서 뒤로 넘어진거 너무 웃겨ㅋㅋ",
          "original_form": "아니 거기 좀비감옥들어가는 입구에서 뒤로 넘어진거 너무 웃겨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100011481.1.13",
          "form": "뒤로도 넘어졌었나? 아니 사족보행 짱 잘해 네발로 뛰어도되겠음ㅋㅋ",
          "original_form": "뒤로도 넘어졌었나? 아니 사족보행 짱 잘해 네발로 뛰어도되겠음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100011481.1.14",
          "form": "그니까ㅋㅋㅋ그 문열어서 시체나왔을때",
          "original_form": "그니까ㅋㅋㅋ그 문열어서 시체나왔을때",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100011481.1.15",
          "form": "장난아니더라ㅋㅋ",
          "original_form": "장난아니더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100011481.1.16",
          "form": "너 이제 진짜 잘보는구나ㅋㅋㅋ",
          "original_form": "너 이제 진짜 잘보는구나ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100011481.1.17",
          "form": "귀신도 도전가능하겠어?",
          "original_form": "귀신도 도전가능하겠어?",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100011481.1.18",
          "form": "아니야 아직 눈가리고봐ㅋㅋㅋ",
          "original_form": "아니야 아직 눈가리고봐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100011481.1.19",
          "form": "그 경고문구 나올때 눈가릴 준비한다구ㅋㅋ",
          "original_form": "그 경고문구 나올때 눈가릴 준비한다구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100011481.1.20",
          "form": "귀신이 분장 더 무섭니?ㅠ",
          "original_form": "귀신이 분장 더 무섭니?ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100011481.1.21",
          "form": "ㅋㅋㅋ 아맞아 경고문구나와서 좀 나아ㅋㅋ",
          "original_form": "ㅋㅋㅋ 아맞아 경고문구나와서 좀 나아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100011481.1.22",
          "form": "근데 이건 귀신도 갑툭튀 이런건 아니라 난 괜찮더라구?",
          "original_form": "근데 이건 귀신도 갑툭튀 이런건 아니라 난 괜찮더라구?",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100011481.1.23",
          "form": "갑툭튀아니면 괜찮을거같긴도하고ㅋㅋ",
          "original_form": "갑툭튀아니면 괜찮을거같긴도하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100011481.1.24",
          "form": "분장 너무 세지만 않으면 되는데",
          "original_form": "분장 너무 세지만 않으면 되는데",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100011481.1.25",
          "form": "귀신은 주말 대낮에 봐야겠군ㅋㅋ",
          "original_form": "귀신은 주말 대낮에 봐야겠군ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100011481.1.26",
          "form": "ㅋㅋ 그래 또 낮에 보면 괜찮지\r\n밤에 보면 무서우ㅗ",
          "original_form": "ㅋㅋ 그래 또 낮에 보면 괜찮지\r\n밤에 보면 무서우ㅗ",
          "speaker_id": "2",
          "time": "20210825 18:14"
        }
      ]
    }
