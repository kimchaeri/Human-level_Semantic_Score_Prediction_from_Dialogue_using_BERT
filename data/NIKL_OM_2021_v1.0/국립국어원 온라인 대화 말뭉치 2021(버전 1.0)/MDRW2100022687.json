      "utterance": [
        {
          "id": "MDRW2100022687.1.1",
          "form": "도서앱 괜찮더라",
          "original_form": "도서앱 괜찮더라",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022687.1.2",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022687.1.3",
          "form": "니덕에 엄마라 아빠가 집에서 책을 볼 수 있구나",
          "original_form": "니덕에 엄마라 아빠가 집에서 책을 볼 수 있구나",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022687.1.4",
          "form": "ㅎㅎ 엄빠가 좋으시다면 저도 좋아요",
          "original_form": "ㅎㅎ 엄빠가 좋으시다면 저도 좋아요",
          "speaker_id": "1",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022687.1.5",
          "form": "요즘에 그걸로 이것저것 보고 있어",
          "original_form": "요즘에 그걸로 이것저것 보고 있어",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022687.1.6",
          "form": "오.. 잘 활용하시는군요",
          "original_form": "오.. 잘 활용하시는군요",
          "speaker_id": "1",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100022687.1.7",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "2",
          "time": "20210830 06:08"
        },
        {
          "id": "MDRW2100022687.1.8",
          "form": "전자책이 어색하진 않았나요?",
          "original_form": "전자책이 어색하진 않았나요?",
          "speaker_id": "1",
          "time": "20210830 06:41"
        },
        {
          "id": "MDRW2100022687.1.9",
          "form": "금방 익숙해졌나요?",
          "original_form": "금방 익숙해졌나요?",
          "speaker_id": "1",
          "time": "20210831 06:22"
        },
        {
          "id": "MDRW2100022687.1.10",
          "form": "어떠서요?",
          "original_form": "어떠서요?",
          "speaker_id": "1",
          "time": "20210902 05:38"
        },
        {
          "id": "MDRW2100022687.1.11",
          "form": "아빠가 오셔야 직접 확인할텐데요",
          "original_form": "아빠가 오셔야 직접 확인할텐데요",
          "speaker_id": "1",
          "time": "20210902 07:19"
        },
        {
          "id": "MDRW2100022687.1.12",
          "form": "그래.. 잘 된다",
          "original_form": "그래.. 잘 된다",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100022687.1.13",
          "form": "오늘 퇴원해서 보니 구피 잘 돌봤더구나",
          "original_form": "오늘 퇴원해서 보니 구피 잘 돌봤더구나",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100022687.1.14",
          "form": "그럼요 아빠 부탁이잖아요",
          "original_form": "그럼요 아빠 부탁이잖아요",
          "speaker_id": "1",
          "time": "20210903 14:56"
        },
        {
          "id": "MDRW2100022687.1.15",
          "form": "울딸이 엄마 아빠를 위해 이것저것을 잘 하는구나",
          "original_form": "울딸이 엄마 아빠를 위해 이것저것을 잘 하는구나",
          "speaker_id": "2",
          "time": "20210903 14:56"
        },
        {
          "id": "MDRW2100022687.1.16",
          "form": "덕분에 도서관을 안가도 집에서 편하게 책을 볼 수 있어서 좋다",
          "original_form": "덕분에 도서관을 안가도 집에서 편하게 책을 볼 수 있어서 좋다",
          "speaker_id": "2",
          "time": "20210903 14:56"
        },
        {
          "id": "MDRW2100022687.1.17",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100022687.1.18",
          "form": "그래.. 앞으로도 유용한 앱 있으면 알려주렴",
          "original_form": "그래.. 앞으로도 유용한 앱 있으면 알려주렴",
          "speaker_id": "2",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100022687.1.19",
          "form": "네 좋은 책 많이 보세요",
          "original_form": "네 좋은 책 많이 보세요",
          "speaker_id": "1",
          "time": "20210903 14:57"
        }
      ]
