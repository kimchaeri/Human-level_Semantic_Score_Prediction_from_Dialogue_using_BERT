        {
          "id": "MDRW2100009703.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210823 07:19"
        },
        {
          "id": "MDRW2100009703.1.2",
          "form": "아직이요 ㅎㅎ님은?",
          "original_form": "아직이요 ㅎㅎ님은?",
          "speaker_id": "2",
          "time": "20210823 09:19"
        },
        {
          "id": "MDRW2100009703.1.3",
          "form": "저는 걸러요",
          "original_form": "저는 걸러요",
          "speaker_id": "1",
          "time": "20210823 09:52"
        },
        {
          "id": "MDRW2100009703.1.4",
          "form": "저도 ㅎㅎ 점심이나 먹게요",
          "original_form": "저도 ㅎㅎ 점심이나 먹게요",
          "speaker_id": "2",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009703.1.5",
          "form": "속이 더부룩해서",
          "original_form": "속이 더부룩해서",
          "speaker_id": "1",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100009703.1.6",
          "form": "어제 많이 드셨나봐요",
          "original_form": "어제 많이 드셨나봐요",
          "speaker_id": "2",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100009703.1.7",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210823 09:58"
        },
        {
          "id": "MDRW2100009703.1.8",
          "form": "오 무슨 칰인가요",
          "original_form": "오 무슨 칰인가요",
          "speaker_id": "2",
          "time": "20210823 09:59"
        },
        {
          "id": "MDRW2100009703.1.9",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210823 10:00"
        },
        {
          "id": "MDRW2100009703.1.10",
          "form": "그럼요 당연하죠 네네치킨",
          "original_form": "그럼요 당연하죠 네네치킨",
          "speaker_id": "2",
          "time": "20210823 10:00"
        },
        {
          "id": "MDRW2100009703.1.11",
          "form": "후라이드요 진리죠",
          "original_form": "후라이드요 진리죠",
          "speaker_id": "1",
          "time": "20210823 10:01"
        },
        {
          "id": "MDRW2100009703.1.12",
          "form": "어 양념은요",
          "original_form": "어 양념은요",
          "speaker_id": "2",
          "time": "20210823 10:06"
        },
        {
          "id": "MDRW2100009703.1.13",
          "form": "것도 좋지만",
