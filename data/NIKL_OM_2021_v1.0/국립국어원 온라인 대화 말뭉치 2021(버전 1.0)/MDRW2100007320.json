      "utterance": [
        {
          "id": "MDRW2100007320.1.1",
          "form": "인터넷쇼핑은 자주하세요?",
          "original_form": "인터넷쇼핑은 자주하세요?",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007320.1.2",
          "form": "네 인쇼가 편해서요ㅋㅋ",
          "original_form": "네 인쇼가 편해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007320.1.3",
          "form": "어느쪽 선호하세요? 인쇼랑 오프 중에",
          "original_form": "어느쪽 선호하세요? 인쇼랑 오프 중에",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007320.1.4",
          "form": "전원래 인쇼 90프로였는데",
          "original_form": "전원래 인쇼 90프로였는데",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007320.1.5",
          "form": "요즘 동네마트랑 재래시장이 엄청싸서 반반인거같아요",
          "original_form": "요즘 동네마트랑 재래시장이 엄청싸서 반반인거같아요",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007320.1.6",
          "form": "아아 채소같은 건 오프가 낫죠ㅋㅋ",
          "original_form": "아아 채소같은 건 오프가 낫죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007320.1.7",
          "form": "재래시장이 카드만 되면 정말 좋겠어요",
          "original_form": "재래시장이 카드만 되면 정말 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007320.1.8",
          "form": "그리고 들고다니기가 힘들어요ㅋㅋ조금 사면 팔아파요",
          "original_form": "그리고 들고다니기가 힘들어요ㅋㅋ조금 사면 팔아파요",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007320.1.9",
          "form": "그건 그래욬ㅋㅋㅋㅋ",
          "original_form": "그건 그래욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007320.1.10",
          "form": "그래서 하나씩만 사옴ㅋㅋㅋ",
          "original_form": "그래서 하나씩만 사옴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007320.1.11",
          "form": "할머니들 구루마 끌고다니는거 이해가요",
          "original_form": "할머니들 구루마 끌고다니는거 이해가요",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007320.1.12",
          "form": "ㄹㅇ 배낭도요ㅋㅋㅋㅋ",
          "original_form": "ㄹㅇ 배낭도요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007320.1.13",
          "form": "지금 제 인쇼 장바구니엔 여름옷과 겨울옷이 같이 있어요ㅋㅋㅋ",
          "original_form": "지금 제 인쇼 장바구니엔 여름옷과 겨울옷이 같이 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007320.1.14",
          "form": "장도 이마트몰 같은데서 보세요?",
          "original_form": "장도 이마트몰 같은데서 보세요?",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007320.1.15",
          "form": "동네네 대형마트가 없어서 걍 동네마트요",
          "original_form": "동네네 대형마트가 없어서 걍 동네마트요",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007320.1.16",
          "form": "이마트몰은 가아끔 공산품같은 거 살때 배송시키구요",
          "original_form": "이마트몰은 가아끔 공산품같은 거 살때 배송시키구요",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007320.1.17",
          "form": "아 그럼 인쇼는 주로 옷같은거 사시겠네요?",
          "original_form": "아 그럼 인쇼는 주로 옷같은거 사시겠네요?",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007320.1.18",
          "form": "네 인쇼는 옷 화장품 이런 거요",
          "original_form": "네 인쇼는 옷 화장품 이런 거요",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007320.1.19",
          "form": "옷은 실패도 많지만 오프는 부담돼서ㅋㅋㅋ",
          "original_form": "옷은 실패도 많지만 오프는 부담돼서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007320.1.20",
          "form": "저도 옷 실패 엄청해서ㅋㅋㅋ근데 온라인이랑 가격차이 많이 나더라구요",
          "original_form": "저도 옷 실패 엄청해서ㅋㅋㅋ근데 온라인이랑 가격차이 많이 나더라구요",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007320.1.21",
          "form": "마자요 그래서 더 인쇼 이용하는 거 같아요ㅋㅋ",
          "original_form": "마자요 그래서 더 인쇼 이용하는 거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:31"
        }
      ]
    }
  ]
