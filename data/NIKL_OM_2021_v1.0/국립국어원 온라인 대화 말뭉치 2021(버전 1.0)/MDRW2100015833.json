      "utterance": [
        {
          "id": "MDRW2100015833.1.1",
          "form": "커피좋아하세요?",
          "original_form": "커피좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015833.1.2",
          "form": "커피 좋아해여?",
          "original_form": "커피 좋아해여?",
          "speaker_id": "1",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015833.1.3",
          "form": "커피 좋죠 ㅋㅋㅋ",
          "original_form": "커피 좋죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015833.1.4",
          "form": "예전엔 아아를 무슨 맛으로 먹나 했는데",
          "original_form": "예전엔 아아를 무슨 맛으로 먹나 했는데",
          "speaker_id": "2",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015833.1.5",
          "form": "요즘은 아아가 깔끔하다는 말을 이해할 수 있더라구요",
          "original_form": "요즘은 아아가 깔끔하다는 말을 이해할 수 있더라구요",
          "speaker_id": "2",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015833.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 처음에는 적응하기 힘들죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 처음에는 적응하기 힘들죠",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015833.1.7",
          "form": "달달한 빵이랑 커피 조합 최고입니다",
          "original_form": "달달한 빵이랑 커피 조합 최고입니다",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015833.1.8",
          "form": "ㅋㅋㅋ당보충을 빵으로~",
          "original_form": "ㅋㅋㅋ당보충을 빵으로~",
          "speaker_id": "2",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100015833.1.9",
          "form": "요즘 파리바게뜨 행사 진짜 많이해요 요기요에서",
          "original_form": "요즘 파리바게뜨 행사 진짜 많이해요 요기요에서",
          "speaker_id": "2",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015833.1.10",
          "form": "추천드립니다",
          "original_form": "추천드립니다",
          "speaker_id": "2",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015833.1.11",
          "form": "빵순이에게 좋습니다",
          "original_form": "빵순이에게 좋습니다",
          "speaker_id": "2",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015833.1.12",
          "form": "ㅋㅋㅋㅋ네 카페가면 주로 아아만 드세요?",
          "original_form": "ㅋㅋㅋㅋ네 카페가면 주로 아아만 드세요?",
          "speaker_id": "1",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015833.1.13",
          "form": "아 안그래도 오늘 파바다녀왓어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 안그래도 오늘 파바다녀왓어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015833.1.14",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ동질감",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ동질감",
          "speaker_id": "2",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015833.1.15",
          "form": "음 원래는 아아를 안마셨는데 요즘은요! ㅎㅎ",
          "original_form": "음 원래는 아아를 안마셨는데 요즘은요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015833.1.16",
          "form": "저 버터되게 좋아해서 얼마전에 사놔서 식빵사러 갔다왓어요 ㅋㅋㅋㅋ",
          "original_form": "저 버터되게 좋아해서 얼마전에 사놔서 식빵사러 갔다왓어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015833.1.17",
          "form": "덤으로 도넛류 사구요 ㅋㅋㅋㅋ",
          "original_form": "덤으로 도넛류 사구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015833.1.18",
          "form": "오마이갓",
          "original_form": "오마이갓",
          "speaker_id": "2",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015833.1.19",
          "form": "저도 도넛 쟁였습니다",
          "original_form": "저도 도넛 쟁였습니다",
          "speaker_id": "2",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015833.1.20",
          "form": "왕 슈크림 도넛",
          "original_form": "왕 슈크림 도넛",
          "speaker_id": "2",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100015833.1.21",
          "form": "진짜 맛있어요",
          "original_form": "진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100015833.1.22",
          "form": "아 저도 그거 삿어요 ㅋㅋㅋㅋㅋ맛잇더라구요",
          "original_form": "아 저도 그거 삿어요 ㅋㅋㅋㅋㅋ맛잇더라구요",
          "speaker_id": "1",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100015833.1.23",
          "form": "그거랑 그 동글동글항거? 쫀득한",
          "original_form": "그거랑 그 동글동글항거? 쫀득한",
          "speaker_id": "1",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100015833.1.24",
          "form": "초코도넛이엇는데 쫄깃 ㅜㅜ",
          "original_form": "초코도넛이엇는데 쫄깃 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100015833.1.25",
          "form": "그 행사기간동안 계속 샀더니",
          "original_form": "그 행사기간동안 계속 샀더니",
          "speaker_id": "2",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100015833.1.26",
          "form": "산 빵 진열하면 빵집 될 것 같아요",
          "original_form": "산 빵 진열하면 빵집 될 것 같아요",
          "speaker_id": "2",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015833.1.27",
          "form": "동네 파바 다 털었어요",
          "original_form": "동네 파바 다 털었어요",
          "speaker_id": "2",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015833.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015833.1.29",
          "form": "듣고보니 너무 왕슈크림도넛으로 쟁였네요 초코 도넛도 좀 보고 그랬어야하는데",
          "original_form": "듣고보니 너무 왕슈크림도넛으로 쟁였네요 초코 도넛도 좀 보고 그랬어야하는데",
          "speaker_id": "2",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015833.1.30",
          "form": "또 머 추천해주실거 있으세요?",
          "original_form": "또 머 추천해주실거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015833.1.31",
          "form": "저도 내일 또 사러가려구욬ㅋㅋㅋ",
          "original_form": "저도 내일 또 사러가려구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015833.1.32",
          "form": "음 저는 일단! 깨찰빵을 굉장히 좋아해서",
          "original_form": "음 저는 일단! 깨찰빵을 굉장히 좋아해서",
          "speaker_id": "2",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015833.1.33",
          "form": "추천드려봅니다",
          "original_form": "추천드려봅니다",
          "speaker_id": "2",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015833.1.34",
          "form": "저는 가리지 않고 다 좋아하는데",
          "original_form": "저는 가리지 않고 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015833.1.35",
          "form": "깨찰빵 중에서 좀 크고 단단한 놈 골라서",
          "original_form": "깨찰빵 중에서 좀 크고 단단한 놈 골라서",
          "speaker_id": "2",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015833.1.36",
          "form": "오 맞아뇨 그거 맛있는디",
          "original_form": "오 맞아뇨 그거 맛있는디",
          "speaker_id": "1",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015833.1.37",
          "form": "오래 오래 먹으면서 씹는 걸 좋아해욬ㅋㅋ",
          "original_form": "오래 오래 먹으면서 씹는 걸 좋아해욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015833.1.38",
          "form": "메뉴에서 못본거같은데 ㅠ",
          "original_form": "메뉴에서 못본거같은데 ㅠ",
          "speaker_id": "1",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015833.1.39",
          "form": "아앗..!",
          "original_form": "아앗..!",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015833.1.40",
          "form": "낼 아침에 사야겟네욬ㅋㅋ",
          "original_form": "낼 아침에 사야겟네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015833.1.41",
          "form": "지점마다 좀 다른가봐요",
          "original_form": "지점마다 좀 다른가봐요",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015833.1.42",
          "form": "ㅋㅌㅋㅋㅋㅋㅋ넵 파이팅!",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋ넵 파이팅!",
          "speaker_id": "2",
