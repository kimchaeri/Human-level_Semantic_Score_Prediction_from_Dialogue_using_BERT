        },
        {
          "id": "MDRW2100022188.1.13",
          "form": "더위타시는구나",
          "original_form": "더위타시는구나",
          "speaker_id": "1",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022188.1.14",
          "form": "사실 전 추운것도 더운것도 싫답니다..ㅋㅋㅋ",
          "original_form": "사실 전 추운것도 더운것도 싫답니다..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022188.1.15",
          "form": "겨울싫어하시게써요",
          "original_form": "겨울싫어하시게써요",
