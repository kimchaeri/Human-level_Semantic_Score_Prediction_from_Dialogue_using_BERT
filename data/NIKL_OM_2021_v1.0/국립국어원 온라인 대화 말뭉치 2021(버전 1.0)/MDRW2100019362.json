      "utterance": [
        {
          "id": "MDRW2100019362.1.1",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019362.1.2",
          "form": "함 해볼까요",
          "original_form": "함 해볼까요",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019362.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019362.1.4",
          "form": "전 이제까지 50개 했는데",
          "original_form": "전 이제까지 50개 했는데",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019362.1.5",
          "form": "좀더 빨리 하면 더 빨리 칠수 있을것 가터요",
          "original_form": "좀더 빨리 하면 더 빨리 칠수 있을것 가터요",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019362.1.6",
          "form": "언제부터 하셧어요?",
          "original_form": "언제부터 하셧어요?",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019362.1.7",
          "form": "그래서 참가일은 한 삼사일?",
          "original_form": "그래서 참가일은 한 삼사일?",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019362.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019362.1.9",
          "form": "전 어제부터 시작해서 ㅋㅋㅋㅋㅋ",
          "original_form": "전 어제부터 시작해서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019362.1.10",
          "form": "전 시작한지 며칠 안돼서",
          "original_form": "전 시작한지 며칠 안돼서",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019362.1.11",
          "form": "이제 30개정도 했어요",
          "original_form": "이제 30개정도 했어요",
          "speaker_id": "2",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019362.1.12",
          "form": "와 그럼 마니하셨네여 저보다 잘하시는듯 ㅋㅋ",
          "original_form": "와 그럼 마니하셨네여 저보다 잘하시는듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019362.1.13",
          "form": "네 함 해봐요 ㅋㅋㅋ",
          "original_form": "네 함 해봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100019362.1.14",
          "form": "제가 엑셀작업이랑 같이 해서 완전 풀접은 안되는데 ㅋㅋ 그래도 금토일은 계속 답은 드릴수 있을것 가타여 ! 몇시간 제가 자릴 비우더라도 오면 답할테니 계속 말걸어주세용",
          "original_form": "제가 엑셀작업이랑 같이 해서 완전 풀접은 안되는데 ㅋㅋ 그래도 금토일은 계속 답은 드릴수 있을것 가타여 ! 몇시간 제가 자릴 비우더라도 오면 답할테니 계속 말걸어주세용",
