      "utterance": [
        {
          "id": "MDRW2100004120.1.1",
          "form": "아니 너무 글루미해서",
          "original_form": "아니 너무 글루미해서",
          "speaker_id": "1",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004120.1.2",
          "form": "나가고싶은데",
          "original_form": "나가고싶은데",
          "speaker_id": "1",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004120.1.3",
          "form": "나갈 원동력이 모지래",
          "original_form": "나갈 원동력이 모지래",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.4",
          "form": "모질라",
          "original_form": "모질라",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.5",
          "form": "모자라",
          "original_form": "모자라",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.6",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.7",
          "form": "모자라인듯",
          "original_form": "모자라인듯",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.8",
          "form": "아 뭕 알지",
          "original_form": "아 뭕 알지",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.9",
          "form": "그리고 지금 비와서",
          "original_form": "그리고 지금 비와서",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.10",
          "form": "더그렇다",
          "original_form": "더그렇다",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.11",
          "form": "지금 딱",
          "original_form": "지금 딱",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.12",
          "form": "라멘먹으러 언제가지",
          "original_form": "라멘먹으러 언제가지",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.13",
          "form": "헬멜옐 가서",
          "original_form": "헬멜옐 가서",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.14",
          "form": "테라스 비오는거 보면서",
          "original_form": "테라스 비오는거 보면서",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.15",
          "form": "멍때리고싶다",
          "original_form": "멍때리고싶다",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004120.1.16",
          "form": "라멘",
          "original_form": "라멘",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004120.1.17",
          "form": "비 계속 온대가지구",
          "original_form": "비 계속 온대가지구",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004120.1.18",
          "form": "잘모르겠네ㅜㅜ",
          "original_form": "잘모르겠네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004120.1.19",
          "form": "그쟈",
          "original_form": "그쟈",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004120.1.20",
          "form": "토욜까지비온대서",
          "original_form": "토욜까지비온대서",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004120.1.21",
          "form": "나 오늘 약속 금욜로 미룸",
          "original_form": "나 오늘 약속 금욜로 미룸",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004120.1.22",
          "form": "홀리 미나",
          "original_form": "홀리 미나",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004120.1.23",
          "form": "수플레 팬케이크 먹고싶다",
          "original_form": "수플레 팬케이크 먹고싶다",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004120.1.24",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "1",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004120.1.25",
          "form": "크레페...",
          "original_form": "크레페...",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.26",
          "form": "달조리법 크레페",
          "original_form": "달조리법 크레페",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.27",
          "form": "맛있지ㅜㅜㅜㅜ",
          "original_form": "맛있지ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.28",
          "form": "ㅏ...",
          "original_form": "ㅏ...",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.29",
          "form": "거긴 리얼이야",
          "original_form": "거긴 리얼이야",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.30",
          "form": "나는 크레페를별로 좋아하지않아",
          "original_form": "나는 크레페를별로 좋아하지않아",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.31",
          "form": "하지만 달조리법은 예외임 리얼",
          "original_form": "하지만 달조리법은 예외임 리얼",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.32",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.33",
          "form": "맛집인정",
          "original_form": "맛집인정",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.34",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.35",
          "form": "크레페.안 눅눅하고",
          "original_form": "크레페.안 눅눅하고",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.36",
          "form": "바삭한것도 좋고",
          "original_form": "바삭한것도 좋고",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.37",
          "form": "나올때도 넘 이쁘게 나옴 ㅠ",
          "original_form": "나올때도 넘 이쁘게 나옴 ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004120.1.38",
          "form": "아 최근에는",
          "original_form": "아 최근에는",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004120.1.39",
          "form": "꽃은 안붙여주더라",
          "original_form": "꽃은 안붙여주더라",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004120.1.40",
          "form": "그래도 이쁘긴해",
          "original_form": "그래도 이쁘긴해",
          "speaker_id": "1",
          "time": "20210821 15:07"
        }
