      "utterance": [
        {
          "id": "MDRW2100009586.1.1",
          "form": "아침은드셨나요?ㅅㄴ",
          "original_form": "아침은드셨나요?ㅅㄴ",
          "speaker_id": "1",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009586.1.2",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009586.1.3",
          "form": "아침부터 어제남은 엽떡을 먹었더니 속이 따갑네요",
          "original_form": "아침부터 어제남은 엽떡을 먹었더니 속이 따갑네요",
          "speaker_id": "1",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009586.1.4",
          "form": "다른거 좀더 드셨어요? 우유나 계란찜같은거요",
          "original_form": "다른거 좀더 드셨어요? 우유나 계란찜같은거요",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009586.1.5",
          "form": "점심에 국수 해먹었어욧",
          "original_form": "점심에 국수 해먹었어욧",
          "speaker_id": "1",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009586.1.6",
          "form": "시원하니 맛있었겠어요",
          "original_form": "시원하니 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210823 13:06"
        },
        {
          "id": "MDRW2100009586.1.7",
          "form": "네~ 점심은 회사에서 드시는건가요?",
          "original_form": "네~ 점심은 회사에서 드시는건가요?",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100009586.1.8",
          "form": "아뇨 저는 일안해서요 집에서 먹었어요",
          "original_form": "아뇨 저는 일안해서요 집에서 먹었어요",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100009586.1.9",
          "form": "저도 집에서ㅋㅋ먹었어요~ ㅋㅋ 애기자면 커피한잔 하고픈데 안자네요ㅠ",
          "original_form": "저도 집에서ㅋㅋ먹었어요~ ㅋㅋ 애기자면 커피한잔 하고픈데 안자네요ㅠ",
          "speaker_id": "1",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100009586.1.10",
          "form": "애기야 얼른 자라! 텔레파시 쐈습니다",
          "original_form": "애기야 얼른 자라! 텔레파시 쐈습니다",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100009586.1.11",
          "form": "고생 많으세요 얼른 자면 좋겠네요",
          "original_form": "고생 많으세요 얼른 자면 좋겠네요",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100009586.1.12",
          "form": "안자네요ㅡㅋㅋㅋ이런",
          "original_form": "안자네요ㅡㅋㅋㅋ이런",
          "speaker_id": "1",
          "time": "20210823 14:10"
        },
        {
          "id": "MDRW2100009586.1.13",
          "form": "제가졸았어요ㅋㅋ  커피좋아하세요?",
          "original_form": "제가졸았어요ㅋㅋ  커피좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100009586.1.14",
          "form": "엄청 좋아하진 않고 남들 마시면 마시는 편이에요",
          "original_form": "엄청 좋아하진 않고 남들 마시면 마시는 편이에요",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100009586.1.15",
          "form": "아ㅎㅎ 좋아하는 음식은요?",
          "original_form": "아ㅎㅎ 좋아하는 음식은요?",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100009586.1.16",
          "form": "갈비찜이랑 초밥 좋아해요 ㅋㅋ",
          "original_form": "갈비찜이랑 초밥 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:43"
        }
