          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210819 09:26"
        },
        {
          "id": "MDRW2100000337.1.15",
          "form": "춘천다녀왔어요",
          "original_form": "춘천다녀왔어요",
          "speaker_id": "2",
          "time": "20210819 09:26"
        },
        {
          "id": "MDRW2100000337.1.16",
          "form": "가서 뭐했어요",
          "original_form": "가서 뭐했어요",
          "speaker_id": "1",
          "time": "20210819 09:26"
        },
        {
          "id": "MDRW2100000337.1.17",
          "form": "양봤어요",
          "original_form": "양봤어요",
          "speaker_id": "2",
          "time": "20210819 09:27"
