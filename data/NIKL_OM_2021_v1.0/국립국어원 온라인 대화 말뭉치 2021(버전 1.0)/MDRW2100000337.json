          "speaker_id": "2",
          "time": "20210819 09:24"
        },
        {
          "id": "MDRW2100000337.1.3",
          "form": "여행다녀오셨어요?",
          "original_form": "여행다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210819 09:24"
        },
        {
          "id": "MDRW2100000337.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210819 09:24"
        },
        {
          "id": "MDRW2100000337.1.5",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210819 09:24"
        },
        {
          "id": "MDRW2100000337.1.6",
          "form": "이시국에 어딜",
          "original_form": "이시국에 어딜",
          "speaker_id": "1",
          "time": "20210819 09:24"
        },
        {
          "id": "MDRW2100000337.1.7",
          "form": "다들 가던데요",
          "original_form": "다들 가던데요",
          "speaker_id": "2",
          "time": "20210819 09:25"
        },
        {
          "id": "MDRW2100000337.1.8",
          "form": "어딜 가던가요",
          "original_form": "어딜 가던가요",
          "speaker_id": "1",
          "time": "20210819 09:25"
        },
        {
          "id": "MDRW2100000337.1.9",
          "form": "제 친구는",
          "original_form": "제 친구는",
          "speaker_id": "2",
          "time": "20210819 09:26"
        },
        {
          "id": "MDRW2100000337.1.10",
          "form": "박씨는 어디가셨죠?",
          "original_form": "박씨는 어디가셨죠?",
          "speaker_id": "1",
          "time": "20210819 09:26"
        },
        {
          "id": "MDRW2100000337.1.11",
          "form": "이번년도에만 제주도 3번감;",
          "original_form": "이번년도에만 제주도 3번감;",
          "speaker_id": "2",
          "time": "20210819 09:26"
        },
        {
          "id": "MDRW2100000337.1.12",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210819 09:26"
        },
        {
          "id": "MDRW2100000337.1.13",
          "form": "전 두달전에",
          "original_form": "전 두달전에",
          "speaker_id": "2",
          "time": "20210819 09:26"
        },
        {
