      "utterance": [
        {
          "id": "MDRW2100038851.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100038851.1.2",
          "form": "넵!! 저는 주로 옷을 많이 사는거같아요",
          "original_form": "넵!! 저는 주로 옷을 많이 사는거같아요",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038851.1.3",
          "form": "전 옷을 잘 입을 줄 몰라서",
          "original_form": "전 옷을 잘 입을 줄 몰라서",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038851.1.4",
          "form": "항상 세트로 만 삽니다ㅋㅋㅋ",
          "original_form": "항상 세트로 만 삽니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038851.1.5",
          "form": "저두요!! 가끔 마네킹 채로 살때도 있어요 ㅋㅋ",
          "original_form": "저두요!! 가끔 마네킹 채로 살때도 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038851.1.6",
          "form": "아 실패 없는 옷 쇼핑 법물어볼랬는데ㅋㅋㅋㅋ",
          "original_form": "아 실패 없는 옷 쇼핑 법물어볼랬는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038851.1.7",
          "form": "동지었어요ㅋㅋㅋ",
          "original_form": "동지었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038851.1.8",
          "form": "ㅋㅋㅋㅋ 전 쇼핑몰에서 사면 항상 사이즈가.. 안맞드라구요",
          "original_form": "ㅋㅋㅋㅋ 전 쇼핑몰에서 사면 항상 사이즈가.. 안맞드라구요",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038851.1.9",
          "form": "쇼핑몰 모델은 왜 이렇게 마른 거예요",
          "original_form": "쇼핑몰 모델은 왜 이렇게 마른 거예요",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038851.1.10",
          "form": "표준화되어야 된다고 봅니다ㅋㅋ",
          "original_form": "표준화되어야 된다고 봅니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038851.1.11",
          "form": "맞아욬ㅋㅋㅋ 그래서 아에 직접 눈으로 보고 살려해요 ㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ 그래서 아에 직접 눈으로 보고 살려해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038851.1.12",
          "form": "전 인터넷으로 사서 거의 당근으로 다 팝니다ㅋㅋㅋ",
          "original_form": "전 인터넷으로 사서 거의 당근으로 다 팝니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038851.1.13",
          "form": "앜ㅋㅋ 당근으로 팔면 잘 팔리나요??",
          "original_form": "앜ㅋㅋ 당근으로 팔면 잘 팔리나요??",
          "speaker_id": "2",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100038851.1.14",
          "form": "엄청 싸게 팔아야 돼요ㅋㅋㅋ",
          "original_form": "엄청 싸게 팔아야 돼요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038851.1.15",
          "form": "반값 올려도 잘 안 사더라구요ㅋㅋㅋ",
          "original_form": "반값 올려도 잘 안 사더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038851.1.16",
