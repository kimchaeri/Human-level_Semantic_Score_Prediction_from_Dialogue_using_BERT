      "utterance": [
        {
          "id": "MDRW2100004311.1.1",
          "form": "문과가 참 전공살리기가 쉽지않죠",
          "original_form": "문과가 참 전공살리기가 쉽지않죠",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004311.1.2",
          "form": "그죠 ㅠㅠㅠ",
          "original_form": "그죠 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004311.1.3",
          "form": "사실상...전공이 거의 의미 없는 느낌....",
          "original_form": "사실상...전공이 거의 의미 없는 느낌....",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004311.1.4",
          "form": "회계사나 세무사 자격증 들으니까 솔깃하긴 하는데 다들 열심히 하시는 분들인데",
          "original_form": "회계사나 세무사 자격증 들으니까 솔깃하긴 하는데 다들 열심히 하시는 분들인데",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004311.1.5",
          "form": "거기껴서 저는 시간만 보낼거같고..",
          "original_form": "거기껴서 저는 시간만 보낼거같고..",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004311.1.6",
          "form": "뭘 해야할지 아직 고민중이예요",
          "original_form": "뭘 해야할지 아직 고민중이예요",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004311.1.7",
          "form": "언제까지 알바만 할 수는 없으니가 ㅠㅠ",
          "original_form": "언제까지 알바만 할 수는 없으니가 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004311.1.8",
          "form": "ㅠㅠㅠㅠㅠ고민 치열하게 하는 것도 소중한 시간일거에요!!!",
          "original_form": "ㅠㅠㅠㅠㅠ고민 치열하게 하는 것도 소중한 시간일거에요!!!",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004311.1.9",
          "form": "으악 감사해요 ㅠㅠㅠㅠㅠ",
          "original_form": "으악 감사해요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004311.1.10",
          "form": "저도 진입하기 전에 엄청 고민 많이했거든요!",
          "original_form": "저도 진입하기 전에 엄청 고민 많이했거든요!",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004311.1.11",
          "form": "원래 하고 싶었던 일이 있었는데",
          "original_form": "원래 하고 싶었던 일이 있었는데",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004311.1.12",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004311.1.13",
          "form": "좋은 기회로 그 회사 가볼수있었는데",
          "original_form": "좋은 기회로 그 회사 가볼수있었는데",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004311.1.14",
          "form": "신기하게도 업무환경보고 오히려 마음이 접혔어요ㅋㅋㅋ",
          "original_form": "신기하게도 업무환경보고 오히려 마음이 접혔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004311.1.15",
          "form": "아아.. 뭔지 알거같아요",
          "original_form": "아아.. 뭔지 알거같아요",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004311.1.16",
          "form": "저는 올해 공시 치려고 했었는데",
          "original_form": "저는 올해 공시 치려고 했었는데",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.17",
          "form": "그래서 차라리 자격증을 따자! 이렇게 된..",
          "original_form": "그래서 차라리 자격증을 따자! 이렇게 된..",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.18",
          "form": "제가 지망하는 쪽 인원이 확 줄어서",
          "original_form": "제가 지망하는 쪽 인원이 확 줄어서",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.19",
          "form": "1명 뽑더라구요",
          "original_form": "1명 뽑더라구요",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.20",
          "form": "ㅋㅋ..ㅋㅋ",
          "original_form": "ㅋㅋ..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.21",
          "form": "아고....1명이라니 ㅠㅠㅠ",
          "original_form": "아고....1명이라니 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.22",
          "form": "1명은.. 좀.. 너무 하지 않나요 ㅠㅠ",
          "original_form": "1명은.. 좀.. 너무 하지 않나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.23",
          "form": "진짜 너무해요..",
          "original_form": "진짜 너무해요..",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.24",
          "form": "1명은 도전하라는거야 말라는거야",
          "original_form": "1명은 도전하라는거야 말라는거야",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.25",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.26",
          "form": "그죠 ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "그죠 ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004311.1.27",
          "form": "맘고생이 많겠네요",
          "original_form": "맘고생이 많겠네요",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004311.1.28",
          "form": "그래도 아무것도 안하는 것 보단 나으니까",
          "original_form": "그래도 아무것도 안하는 것 보단 나으니까",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004311.1.29",
          "form": "내년 목표로 준비해볼까 하고 있어요",
          "original_form": "내년 목표로 준비해볼까 하고 있어요",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004311.1.30",
          "form": "와 멋있어요",
          "original_form": "와 멋있어요",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004311.1.31",
          "form": "될지 모르겠어서 살짝 걱정이긴해요 ㅠㅠ",
          "original_form": "될지 모르겠어서 살짝 걱정이긴해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004311.1.32",
          "form": "저라면.. 1명..? 이러고 이미 포기했을텐데",
          "original_form": "저라면.. 1명..? 이러고 이미 포기했을텐데",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004311.1.33",
          "form": "앟ㅎㅎ..",
          "original_form": "앟ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004311.1.34",
          "form": "딱 제가 바라던 일이라서 ㅠㅠ",
          "original_form": "딱 제가 바라던 일이라서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004311.1.35",
          "form": "걱정을 가지고도 도전할수있다는게",
          "original_form": "걱정을 가지고도 도전할수있다는게",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004311.1.36",
          "form": "더 용기있는 일이잖아요",
          "original_form": "더 용기있는 일이잖아요",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004311.1.37",
          "form": "바라던 일이라니",
          "original_form": "바라던 일이라니",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004311.1.38",
          "form": "멋짐 두배..",
          "original_form": "멋짐 두배..",
