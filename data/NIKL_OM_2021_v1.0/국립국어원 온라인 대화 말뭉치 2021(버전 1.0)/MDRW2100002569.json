      "utterance": [
        {
          "id": "MDRW2100002569.1.1",
          "form": "여행은 좋아하시나요?",
          "original_form": "여행은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002569.1.2",
          "form": "좋죠ㅋㅋㅋ",
          "original_form": "좋죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002569.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002569.1.4",
          "form": "여행 좋아하죠 ㅋㅋ 코로나 덕분에 요즘 여행을 못가서 참 아쉽네요^",
          "original_form": "여행 좋아하죠 ㅋㅋ 코로나 덕분에 요즘 여행을 못가서 참 아쉽네요^",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002569.1.5",
          "form": "좋았던 여행지 있으신가요?",
          "original_form": "좋았던 여행지 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002569.1.6",
          "form": "제말이요 해외좀가고팠는데 코로나가ㅠ",
          "original_form": "제말이요 해외좀가고팠는데 코로나가ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002569.1.7",
          "form": "전 많이는못가보고2번가봤네요ㅠㅠ",
          "original_form": "전 많이는못가보고2번가봤네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002569.1.8",
          "form": "대만 너무좋았어요!",
          "original_form": "대만 너무좋았어요!",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002569.1.9",
          "form": "우와 대만 좋았겠어요~ 대만은 뭐가 유명해요??",
          "original_form": "우와 대만 좋았겠어요~ 대만은 뭐가 유명해요??",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002569.1.10",
          "form": "대만에 먹을게 한국인입맛에 진짜잘맞는거같아요ㅋㅋㅋ",
          "original_form": "대만에 먹을게 한국인입맛에 진짜잘맞는거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002569.1.11",
          "form": "그리고 센과치히로?에 나온 관광명소도 야경진짜이뻐요",
          "original_form": "그리고 센과치히로?에 나온 관광명소도 야경진짜이뻐요",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002569.1.12",
          "form": "오 어떤맛들일지 궁금하네요ㅋㅋ역시 여행은 보고 먹으러가는거죠!",
          "original_form": "오 어떤맛들일지 궁금하네요ㅋㅋ역시 여행은 보고 먹으러가는거죠!",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002569.1.13",
          "form": "코로나 풀리면 대만 가보고싶네요! 저는 예전에 첫해외여행으로 유럽다녀왔는데 완전좋습니다 ㅎㅎ",
          "original_form": "코로나 풀리면 대만 가보고싶네요! 저는 예전에 첫해외여행으로 유럽다녀왔는데 완전좋습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002569.1.14",
          "form": "오 유렵",
          "original_form": "오 유렵",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100002569.1.15",
          "form": "넘부러워요 저유럽가고싶어요",
          "original_form": "넘부러워요 저유럽가고싶어요",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100002569.1.16",
          "form": "유럽좋습니다 ㅎㅎ 꼭 가보세요",
          "original_form": "유럽좋습니다 ㅎㅎ 꼭 가보세요",
          "speaker_id": "1",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100002569.1.17",
          "form": "작년초에도 코로나 터지기 바로전에 러시아 다녀왔는데 참 운이 좋았네요 ㅋ",
          "original_form": "작년초에도 코로나 터지기 바로전에 러시아 다녀왔는데 참 운이 좋았네요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:11"
        },
        {
          "id": "MDRW2100002569.1.18",
          "form": "와우 러시아좋죠",
          "original_form": "와우 러시아좋죠",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100002569.1.19",
          "form": "러시아 근데 여행지 많나요?",
          "original_form": "러시아 근데 여행지 많나요?",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100002569.1.20",
          "form": "뭐 관광간건아니고 스노보드타러 간거라 ㅎㅎ",
          "original_form": "뭐 관광간건아니고 스노보드타러 간거라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100002569.1.21",
          "form": "보드타고 대게 열심히 먹고왔죠 ㅋ",
          "original_form": "보드타고 대게 열심히 먹고왔죠 ㅋ",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100002569.1.22",
          "form": "러시아에",
          "original_form": "러시아에",
          "speaker_id": "2",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100002569.1.23",
          "form": "보드타는곳이 있나보네요ㅋㅋㅋ",
          "original_form": "보드타는곳이 있나보네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100002569.1.24",
          "form": "보드카 마시면서 탈거같은기분",
          "original_form": "보드카 마시면서 탈거같은기분",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100002569.1.25",
          "form": "ㅋㅋㅋㅋ국내보다 러시아가 훨씬 재밌더라고요",
          "original_form": "ㅋㅋㅋㅋ국내보다 러시아가 훨씬 재밌더라고요",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100002569.1.26",
          "form": "보드카는 다 타고 마셨네요 ㅋㅋ",
          "original_form": "보드카는 다 타고 마셨네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100002569.1.27",
          "form": "전 보드안타봐서ㅋㅋㅋ",
          "original_form": "전 보드안타봐서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100002569.1.28",
          "form": "술을 잘드시나봐요",
          "original_form": "술을 잘드시나봐요",
          "speaker_id": "2",
          "time": "20210824 23:24"
        }
      ]
    }
