      "utterance": [
        {
          "id": "MDRW2100042568.1.1",
          "form": "진짜여 ㅋㅋㅋㅋ",
          "original_form": "진짜여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042568.1.2",
          "form": "쌀국수도 캬",
          "original_form": "쌀국수도 캬",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042568.1.3",
          "form": "날씨에 진짜 딱이지않나요?",
          "original_form": "날씨에 진짜 딱이지않나요?",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042568.1.4",
          "form": "그러니까여",
          "original_form": "그러니까여",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042568.1.5",
          "form": "하 아무튼 국물이 너무너무 필요한 날씨입니다",
          "original_form": "하 아무튼 국물이 너무너무 필요한 날씨입니다",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042568.1.6",
          "form": "저 샤브샤브도 좀 땡겨여",
          "original_form": "저 샤브샤브도 좀 땡겨여",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042568.1.7",
          "form": "맞아여 국물!@",
          "original_form": "맞아여 국물!@",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042568.1.8",
          "form": "저두여.. 샤브샤브 말하니까 땡기네여",
          "original_form": "저두여.. 샤브샤브 말하니까 땡기네여",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042568.1.9",
          "form": "샤브샤브 마지막에 죽드세요 볶음밥 드세요",
          "original_form": "샤브샤브 마지막에 죽드세요 볶음밥 드세요",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042568.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042568.1.11",
          "form": "전 죽파",
          "original_form": "전 죽파",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.12",
          "form": "계란 죽",
          "original_form": "계란 죽",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.13",
          "form": "저도요 ㅋㅋㅋㅋ 저 완전 죽파",
          "original_form": "저도요 ㅋㅋㅋㅋ 저 완전 죽파",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.14",
          "form": "좋아여!!!",
          "original_form": "좋아여!!!",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.15",
          "form": "아 갑자기 너무 먹고싶어요..",
          "original_form": "아 갑자기 너무 먹고싶어요..",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.16",
          "form": "ㅋㅋㅋ 그쵸 죽이 맛있어",
          "original_form": "ㅋㅋㅋ 그쵸 죽이 맛있어",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.17",
          "form": "그 짭짤한 국물에 죽 해먹는거",
          "original_form": "그 짭짤한 국물에 죽 해먹는거",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.18",
          "form": "진짜 너무 최고입나댜",
          "original_form": "진짜 너무 최고입나댜",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.19",
          "form": "그러니까여",
          "original_form": "그러니까여",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042568.1.20",
          "form": "그렇게 따끈한거 먹고",
          "original_form": "그렇게 따끈한거 먹고",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042568.1.21",
          "form": "아이스크림 먹는게 취미입니다!!!",
          "original_form": "아이스크림 먹는게 취미입니다!!!",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042568.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042568.1.23",
          "form": "홀..최상의 조합..",
          "original_form": "홀..최상의 조합..",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042568.1.24",
          "form": "아니 저 지금 진짜 샤브샤브 너무 묵고싶어요",
          "original_form": "아니 저 지금 진짜 샤브샤브 너무 묵고싶어요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042568.1.25",
          "form": "1위로 떠올랐어요..",
          "original_form": "1위로 떠올랐어요..",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042568.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 저는 어묵",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 저는 어묵",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042568.1.27",
          "form": "그리구 내일 현실은 닭가슴살 먹고있는거 아닌지 모르겠네요..",
          "original_form": "그리구 내일 현실은 닭가슴살 먹고있는거 아닌지 모르겠네요..",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042568.1.28",
          "form": "흑흑 ㅁ국물 못먹으면 컵누들이라도 먹어야게ㅛ어요",
          "original_form": "흑흑 ㅁ국물 못먹으면 컵누들이라도 먹어야게ㅛ어요",
