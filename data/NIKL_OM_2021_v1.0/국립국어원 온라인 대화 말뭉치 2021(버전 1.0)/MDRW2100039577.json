        {
          "id": "MDRW2100039577.1.1",
          "form": "오 어디꺼?",
          "original_form": "오 어디꺼?",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039577.1.2",
          "form": "삼성이요",
          "original_form": "삼성이요",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039577.1.3",
          "form": "무선이요?",
          "original_form": "무선이요?",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039577.1.4",
          "form": "아뇽 유선 ㅜ",
          "original_form": "아뇽 유선 ㅜ",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039577.1.5",
          "form": "저도 유선써요 ㅎㅎ",
          "original_form": "저도 유선써요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039577.1.6",
          "form": "유선이 좋죠 ㅋ",
          "original_form": "유선이 좋죠 ㅋ",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039577.1.7",
          "form": "네 힘도 세고 ㅎㅎ",
          "original_form": "네 힘도 세고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039577.1.8",
          "form": "무선 너무 비싸요",
          "original_form": "무선 너무 비싸요",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039577.1.9",
          "form": "네 배터리도 금방 닳고요",
          "original_form": "네 배터리도 금방 닳고요",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039577.1.10",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039577.1.11",
          "form": "유선청소기가 속편하죠",
          "original_form": "유선청소기가 속편하죠",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039577.1.12",
          "form": "네 손목도 안 아프고",
          "original_form": "네 손목도 안 아프고",
          "speaker_id": "2",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039577.1.13",
          "form": "무선은 좀 무겁죠",
          "original_form": "무선은 좀 무겁죠",
          "speaker_id": "1",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039577.1.14",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100039577.1.15",
          "form": "손아파요",
          "original_form": "손아파요",
