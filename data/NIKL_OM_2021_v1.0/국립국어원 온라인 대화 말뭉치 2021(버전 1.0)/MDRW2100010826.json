      "utterance": [
        {
          "id": "MDRW2100010826.1.1",
          "form": ".네 전 무교지만",
          "original_form": ".네 전 무교지만",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010826.1.2",
          "form": "전 사실 종교에 별생각없지먀",
          "original_form": "전 사실 종교에 별생각없지먀",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010826.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010826.1.4",
          "form": "없지만",
          "original_form": "없지만",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010826.1.5",
          "form": "저도요 별 생각 없어요",
          "original_form": "저도요 별 생각 없어요",
          "speaker_id": "1",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010826.1.6",
          "form": "그말은 웃겼어요",
          "original_form": "그말은 웃겼어요",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010826.1.7",
          "form": "사람이 죽으면 하느님은 분리수거를 하시고 부처님은 재활용을 하신다",
          "original_form": "사람이 죽으면 하느님은 분리수거를 하시고 부처님은 재활용을 하신다",
          "speaker_id": "2",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010826.1.8",
          "form": "아 맞아요 ㅋㅋ윤회",
          "original_form": "아 맞아요 ㅋㅋ윤회",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010826.1.9",
          "form": "못살아요 다들 말잘해요",
          "original_form": "못살아요 다들 말잘해요",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010826.1.10",
          "form": "그죠 ㅋㅋ",
          "original_form": "그죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010826.1.11",
          "form": "이거 말해줄때마다",
          "original_form": "이거 말해줄때마다",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010826.1.12",
          "form": "사람들 반응이 좋아서 ㅋㅋ",
          "original_form": "사람들 반응이 좋아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010826.1.13",
          "form": "저의 유일한 종교조크",
          "original_form": "저의 유일한 종교조크",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010826.1.14",
          "form": "바로 터짐니다  오 대박 한건 하셨네요",
          "original_form": "바로 터짐니다  오 대박 한건 하셨네요",
          "speaker_id": "1",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010826.1.15",
          "form": "음 저는 제가 무교나 다름없지만 그래도 무교는 아니라고생각하거든요 ㅋㅋ",
          "original_form": "음 저는 제가 무교나 다름없지만 그래도 무교는 아니라고생각하거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010826.1.16",
          "form": "왜 그런지 물어봐 주세용 ㅋㅋ",
          "original_form": "왜 그런지 물어봐 주세용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010826.1.17",
          "form": "왜요 왜그런가요?ㅋㅋ",
          "original_form": "왜요 왜그런가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010826.1.18",
          "form": "어서 알려주세요",
          "original_form": "어서 알려주세요",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010826.1.19",
          "form": "숨넘어가요 ㅋㅋ",
          "original_form": "숨넘어가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010826.1.20",
          "form": "새로운곳에 이사갔을때나",
          "original_form": "새로운곳에 이사갔을때나",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010826.1.21",
          "form": "유학갔을때",
          "original_form": "유학갔을때",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010826.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010826.1.23",
          "form": "본능적으로 성당이 어딨는지 확인해요 ㅋㅋ",
          "original_form": "본능적으로 성당이 어딨는지 확인해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010826.1.24",
          "form": "오 그럼 인정합니다",
          "original_form": "오 그럼 인정합니다",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010826.1.25",
          "form": "그리고 일단 첫미사는감ㅋㅋ",
          "original_form": "그리고 일단 첫미사는감ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010826.1.26",
          "form": "무교 아니네요",
          "original_form": "무교 아니네요",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010826.1.27",
          "form": "이후는 내맘대로ㅋㅋ",
          "original_form": "이후는 내맘대로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010826.1.28",
          "form": "맘속에 의지가 있으시네요",
          "original_form": "맘속에 의지가 있으시네요",
          "speaker_id": "1",
          "time": "20210823 22:24"
        }
      ]
    }
  ]
