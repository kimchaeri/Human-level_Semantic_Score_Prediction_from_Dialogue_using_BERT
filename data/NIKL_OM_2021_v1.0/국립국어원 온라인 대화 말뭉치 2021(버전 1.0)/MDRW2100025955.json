      "utterance": [
        {
          "id": "MDRW2100025955.1.1",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100025955.1.2",
          "form": "낼 오겠네 그럼",
          "original_form": "낼 오겠네 그럼",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100025955.1.3",
          "form": "ㅇㅇ엄청 많이 시켰다",
          "original_form": "ㅇㅇ엄청 많이 시켰다",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025955.1.4",
          "form": "어디건데 근데 보고 계속함?",
          "original_form": "어디건데 근데 보고 계속함?",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025955.1.5",
          "form": "아저씨가 하더라",
          "original_form": "아저씨가 하더라",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025955.1.6",
          "form": "오리온인가?",
          "original_form": "오리온인가?",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025955.1.7",
          "form": "오 오리온 좋음",
          "original_form": "오 오리온 좋음",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025955.1.8",
          "form": "아니다 롯데다",
          "original_form": "아니다 롯데다",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025955.1.9",
          "form": "아 롯데는 별론데",
          "original_form": "아 롯데는 별론데",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025955.1.10",
          "form": "골고루 시켰어 오면 골라라",
          "original_form": "골고루 시켰어 오면 골라라",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025955.1.11",
          "form": "아싸 젤 먼저 골라야지",
          "original_form": "아싸 젤 먼저 골라야지",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025955.1.12",
          "form": "야 욕심부리지 말고 적당히 골라 안먹고 쟁임 똥된다",
          "original_form": "야 욕심부리지 말고 적당히 골라 안먹고 쟁임 똥된다",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025955.1.13",
          "form": "아 먹거든?",
          "original_form": "아 먹거든?",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025955.1.14",
          "form": "내 방에 있는게 하나씩 먹고있다규",
          "original_form": "내 방에 있는게 하나씩 먹고있다규",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025955.1.15",
          "form": "안 줄던데 먼지 쌓여",
          "original_form": "안 줄던데 먼지 쌓여",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025955.1.16",
          "form": "내 속도대로 먹고 이쯤",
          "original_form": "내 속도대로 먹고 이쯤",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025955.1.17",
          "form": "카톡에 머냐",
          "original_form": "카톡에 머냐",
