      "utterance": [
        {
          "id": "MDRW2100013701.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013701.1.2",
          "form": "코시국 때문에 못가지만",
          "original_form": "코시국 때문에 못가지만",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013701.1.3",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013701.1.4",
          "form": "혹시 좋아하는 여행지 있으신가요?",
          "original_form": "혹시 좋아하는 여행지 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013701.1.5",
          "form": "휴양지 좋아했는데 언제쯤 갈수있을까요",
          "original_form": "휴양지 좋아했는데 언제쯤 갈수있을까요",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013701.1.6",
          "form": "백신 다맞고",
          "original_form": "백신 다맞고",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013701.1.7",
          "form": "내년에는 갈수있지 않을까요?",
          "original_form": "내년에는 갈수있지 않을까요?",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013701.1.8",
          "form": "혹시 코시국 끝나면 가고싶은 곳 있으세요?",
          "original_form": "혹시 코시국 끝나면 가고싶은 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013701.1.9",
          "form": "필리핀이요",
          "original_form": "필리핀이요",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013701.1.10",
          "form": "님은 어디좋아하셔요.",
          "original_form": "님은 어디좋아하셔요.",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013701.1.11",
          "form": "저는 해외는 태국밖에 안가봤는데",
          "original_form": "저는 해외는 태국밖에 안가봤는데",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013701.1.12",
          "form": "괜찮았어요",
          "original_form": "괜찮았어요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013701.1.13",
          "form": "돈모으면 미국으로 여행가고싶어요",
          "original_form": "돈모으면 미국으로 여행가고싶어요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013701.1.14",
          "form": "미국도 좋지요",
          "original_form": "미국도 좋지요",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013701.1.15",
          "form": "혹시 추천해주실 여행지 있으신가요",
          "original_form": "혹시 추천해주실 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013701.1.16",
          "form": "제가 가본곳이 별로 없어서.ㅠ",
          "original_form": "제가 가본곳이 별로 없어서.ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013701.1.17",
          "form": "꼭 가보고싶은곳은 하와이, 산토리니 등이예요",
          "original_form": "꼭 가보고싶은곳은 하와이, 산토리니 등이예요",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013701.1.18",
          "form": "산토리니 너무 멋있을것같아요",
          "original_form": "산토리니 너무 멋있을것같아요",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013701.1.19",
          "form": "예전에 여행 유투버가 산토리니 가는거 봤는데",
          "original_form": "예전에 여행 유투버가 산토리니 가는거 봤는데",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013701.1.20",
          "form": "하늘색이랑 바다색이랑 너무 예쁘더라구요",
          "original_form": "하늘색이랑 바다색이랑 너무 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013701.1.21",
          "form": "제카톡프사도  그리스산토리니",
          "original_form": "제카톡프사도  그리스산토리니",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100013701.1.22",
          "form": "요즘 더워서 산토리니 배경으로 하면 시원할꺼같아요",
          "original_form": "요즘 더워서 산토리니 배경으로 하면 시원할꺼같아요",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100013701.1.23",
          "form": "네 산토리니 좋은곤이죠 코로나끝나면 꼭ㅇ가고싶은곳",
          "original_form": "네 산토리니 좋은곤이죠 코로나끝나면 꼭ㅇ가고싶은곳",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100013701.1.24",
          "form": "어떤여행지 추천하세요?",
          "original_form": "어떤여행지 추천하세요?",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100013701.1.25",
          "form": "저는 태국 방콕 괜찮았어요",
          "original_form": "저는 태국 방콕 괜찮았어요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100013701.1.26",
          "form": "물가도 싸서 호캉스 하기에도 좋고 전철이 잘되있어요",
          "original_form": "물가도 싸서 호캉스 하기에도 좋고 전철이 잘되있어요",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100013701.1.27",
          "form": "아 좋다고 들었어요",
          "original_form": "아 좋다고 들었어요",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100013701.1.28",
          "form": "거기도 코로나 심각하죠?",
          "original_form": "거기도 코로나 심각하죠?",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100013701.1.29",
          "form": "여행으로 먹고사는 나라들은 코로나때문에 타격이 엄청날듯요",
          "original_form": "여행으로 먹고사는 나라들은 코로나때문에 타격이 엄청날듯요",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100013701.1.30",
          "form": "그런곳은 타격이 정말 클것같아요",
          "original_form": "그런곳은 타격이 정말 클것같아요",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100013701.1.31",
          "form": "그리스나 섬나라도",
          "original_form": "그리스나 섬나라도",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100013701.1.32",
          "form": "여행산업이 커서  더힘들것같아요",
          "original_form": "여행산업이 커서  더힘들것같아요",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100013701.1.33",
          "form": "그동안 어떻게 버텼을까요 이미 1년도 넘었는데",
          "original_form": "그동안 어떻게 버텼을까요 이미 1년도 넘었는데",
          "speaker_id": "1",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100013701.1.34",
          "form": "특히 동남아 후진국들은 더 걱정이 되네요.",
          "original_form": "특히 동남아 후진국들은 더 걱정이 되네요.",
          "speaker_id": "1",
          "time": "20210826 08:26"
        }
      ]
