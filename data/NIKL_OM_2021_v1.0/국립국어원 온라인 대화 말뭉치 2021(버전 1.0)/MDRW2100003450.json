      "utterance": [
        {
          "id": "MDRW2100003450.1.1",
          "form": "나혼자산다 이제 할건가봐요",
          "original_form": "나혼자산다 이제 할건가봐요",
          "speaker_id": "1",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100003450.1.2",
          "form": "보고싶은데 딸램이 코난보네요ㅠ",
          "original_form": "보고싶은데 딸램이 코난보네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003450.1.3",
          "form": "극장판이라고",
          "original_form": "극장판이라고",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003450.1.4",
          "form": "에고 오늘 오상욱선수 나온다는거 같던데",
          "original_form": "에고 오늘 오상욱선수 나온다는거 같던데",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003450.1.5",
          "form": "그거볼려고 기다렸네요ㅎㅎ",
          "original_form": "그거볼려고 기다렸네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003450.1.6",
          "form": "오~~~ 오상욱 잘생겼는데^^",
          "original_form": "오~~~ 오상욱 잘생겼는데^^",
          "speaker_id": "2",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003450.1.7",
          "form": "보고싶네요~~",
          "original_form": "보고싶네요~~",
          "speaker_id": "2",
          "time": "20210820 23:19"
        },
        {
          "id": "MDRW2100003450.1.8",
          "form": "폰으로라도 보셔요^^  코난은 언제끝난대요?",
          "original_form": "폰으로라도 보셔요^^  코난은 언제끝난대요?",
          "speaker_id": "1",
          "time": "20210820 23:20"
        },
        {
          "id": "MDRW2100003450.1.9",
          "form": "45~50분쯤 끝날꺼같아요.",
          "original_form": "45~50분쯤 끝날꺼같아요.",
          "speaker_id": "2",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003450.1.10",
          "form": "그럼 보실수 있으실듯 ㅎ 바로 오상욱선수 나올거같진않아요",
          "original_form": "그럼 보실수 있으실듯 ㅎ 바로 오상욱선수 나올거같진않아요",
          "speaker_id": "1",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003450.1.11",
          "form": "유희열의 스케치북에 안산나온다고 왔다갔다하고있어요",
          "original_form": "유희열의 스케치북에 안산나온다고 왔다갔다하고있어요",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003450.1.12",
          "form": "방에들어가래도 안들어가고 귀찮네요 ^^",
          "original_form": "방에들어가래도 안들어가고 귀찮네요 ^^",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003450.1.13",
          "form": "거긴 또 안산선수나와요? 올림픽선수들 바쁘네요",
          "original_form": "거긴 또 안산선수나와요? 올림픽선수들 바쁘네요",
          "speaker_id": "1",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003450.1.14",
          "form": "ㅎㅎ 누가그랬는데 한철이라 바쁘게 다녀야한다고요",
          "original_form": "ㅎㅎ 누가그랬는데 한철이라 바쁘게 다녀야한다고요",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003450.1.15",
          "form": "아는형님에 구본길이 얘기한거같아요",
          "original_form": "아는형님에 구본길이 얘기한거같아요",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003450.1.16",
          "form": "김준호였나ㅋ",
          "original_form": "김준호였나ㅋ",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003450.1.17",
          "form": "그래서 그런지 펜싱4인방 엄청나오던데요",
          "original_form": "그래서 그런지 펜싱4인방 엄청나오던데요",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003450.1.18",
          "form": "ㅋ 김준호는 은근히 흘리더니 소속사계약했더라구요",
          "original_form": "ㅋ 김준호는 은근히 흘리더니 소속사계약했더라구요",
          "speaker_id": "2",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003450.1.19",
          "form": "오상욱선수 바로나오네요ㅋㅋ",
          "original_form": "오상욱선수 바로나오네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003450.1.20",
          "form": "오~~ 나중에 재방송봐야겠네요",
          "original_form": "오~~ 나중에 재방송봐야겠네요",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003450.1.21",
          "form": "방송활동 병행할건가보네요  김준호선수는",
          "original_form": "방송활동 병행할건가보네요  김준호선수는",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003450.1.22",
          "form": "잘생겼네요ㅎㅎ",
          "original_form": "잘생겼네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003450.1.23",
          "form": "그럴꺼같아요.",
          "original_form": "그럴꺼같아요.",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003450.1.24",
          "form": "ㅎ 나이드니 남자던 여자던 이쁜사람이 좋더라구요",
          "original_form": "ㅎ 나이드니 남자던 여자던 이쁜사람이 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003450.1.25",
          "form": "사람보는 눈은 비슷하니ㅋ 티비좀 보고올게요^^",
          "original_form": "사람보는 눈은 비슷하니ㅋ 티비좀 보고올게요^^",
          "speaker_id": "1",
