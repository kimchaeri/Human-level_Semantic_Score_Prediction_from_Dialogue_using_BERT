        {
          "id": "MDRW2100045970.1.1",
          "form": "우대갈비?파는집 알아?",
          "original_form": "우대갈비?파는집 알아?",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045970.1.2",
          "form": "그게 몽탄이잖아",
          "original_form": "그게 몽탄이잖아",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045970.1.3",
          "form": "아 그거 추천이많길래",
          "original_form": "아 그거 추천이많길래",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045970.1.4",
          "form": "우대갈비 유명하지",
          "original_form": "우대갈비 유명하지",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045970.1.5",
          "form": "뭐가다르지?",
          "original_form": "뭐가다르지?",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045970.1.6",
          "form": "소고기가 미국산이던데",
          "original_form": "소고기가 미국산이던데",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045970.1.7",
          "form": "양념같더라",
          "original_form": "양념같더라",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045970.1.8",
          "form": "갈비부분이 원래맛있지",
          "original_form": "갈비부분이 원래맛있지",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045970.1.9",
          "form": "몽탄?거기는 얼마나기다려야해?",
          "original_form": "몽탄?거기는 얼마나기다려야해?",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045970.1.10",
          "form": "보통 한 3시간?",
          "original_form": "보통 한 3시간?",
          "speaker_id": "1",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045970.1.11",
          "form": "카톡으로 알려주나?",
          "original_form": "카톡으로 알려주나?",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045970.1.12",
          "form": "응 다른거하고있다가 가면 돼",
          "original_form": "응 다른거하고있다가 가면 돼",
          "speaker_id": "1",
          "time": "20210908 09:12"
        },
        {
