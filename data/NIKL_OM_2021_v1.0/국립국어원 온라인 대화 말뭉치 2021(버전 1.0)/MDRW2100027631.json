      "utterance": [
        {
          "id": "MDRW2100027631.1.1",
          "form": "나는 사실 학업 스트레스는 없었던 듯 ",
          "original_form": "나는 사실 학업 스트레스는 없었던 듯 ",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027631.1.2",
          "form": "공부가 아닌 학습은 좋아했는데",
          "original_form": "공부가 아닌 학습은 좋아했는데",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027631.1.3",
          "form": "공부가 되면 재미를 못느껴서",
          "original_form": "공부가 되면 재미를 못느껴서",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027631.1.4",
          "form": "지금 수학공부하라면 잘할듯",
          "original_form": "지금 수학공부하라면 잘할듯",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027631.1.5",
          "form": "아마도 아닐수도 있음",
          "original_form": "아마도 아닐수도 있음",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027631.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나도 지금 하면 열심히할둣",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나도 지금 하면 열심히할둣",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027631.1.7",
          "form": "학생때는 다 싫었었는데",
          "original_form": "학생때는 다 싫었었는데",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027631.1.8",
          "form": "지금은 열심히할거같아",
          "original_form": "지금은 열심히할거같아",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027631.1.9",
          "form": "좀 더 배울 기회가 많으니까",
          "original_form": "좀 더 배울 기회가 많으니까",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027631.1.10",
          "form": "이게 학업이 아니니까",
          "original_form": "이게 학업이 아니니까",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027631.1.11",
          "form": "부담도 없고",
          "original_form": "부담도 없고",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027631.1.12",
          "form": "긍데",
          "original_form": "긍데",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027631.1.13",
          "form": "강제력이 없어서",
          "original_form": "강제력이 없어서",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027631.1.14",
          "form": "잘할 수 있을지는...",
          "original_form": "잘할 수 있을지는...",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027631.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ구렇지....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ구렇지....",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027631.1.16",
          "form": "name2이는 학교 생활 어땠어?",
          "original_form": "&name2&이는 학교 생활 어땠어?",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027631.1.17",
          "form": "나는 학교......에서......... 열심히 덕질했지 ㅋㅋㅋㅋㅋ 친구들이.다 아이돌팬이어서",
          "original_form": "나는 학교......에서......... 열심히 덕질했지 ㅋㅋㅋㅋㅋ 친구들이.다 아이돌팬이어서",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027631.1.18",
          "form": "공부보다는 맨날 아이돌얘기하고 ㅋㅋㅋㅋㅋ",
          "original_form": "공부보다는 맨날 아이돌얘기하고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027631.1.19",
          "form": "학교다닐때 제일 재미%던건 역시",
          "original_form": "학교다닐때 제일 재미%던건 역시",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027631.1.20",
          "form": "친구들이랑 약속안잡아도",
          "original_form": "친구들이랑 약속안잡아도",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027631.1.21",
          "form": "매일 만날수있던어같아",
          "original_form": "매일 만날수있던어같아",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027631.1.22",
          "form": "아 맞아 요즘은 잡으려면 다들 날짜를 잡아야하니까ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아 요즘은 잡으려면 다들 날짜를 잡아야하니까ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027631.1.23",
          "form": "다양한 애들 만날 수 있는 것도 좋고",
          "original_form": "다양한 애들 만날 수 있는 것도 좋고",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100027631.1.24",
          "form": "숙제 베끼기 이런 것도 재밌었지",
          "original_form": "숙제 베끼기 이런 것도 재밌었지",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027631.1.25",
          "form": "하나가지고 대여섯명이 돌려봄",
          "original_form": "하나가지고 대여섯명이 돌려봄",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027631.1.26",
          "form": "난 숙제는 내가 한 거 같다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "난 숙제는 내가 한 거 같다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027631.1.27",
          "form": "부지런하네 ㅋㅋㅋㅋ 난 숙제 학교에서 아침에 마구마구하는 타입..",
          "original_form": "부지런하네 ㅋㅋㅋㅋ 난 숙제 학교에서 아침에 마구마구하는 타입..",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027631.1.28",
          "form": "숙제가 별로 없었나? 그건 아닌 거 같은데 ",
          "original_form": "숙제가 별로 없었나? 그건 아닌 거 같은데 ",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027631.1.29",
          "form": "별로 없었던 거 같기도 함..",
          "original_form": "별로 없었던 거 같기도 함..",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027631.1.30",
          "form": "나는 그런 거 많았어 빈칸채우기",
          "original_form": "나는 그런 거 많았어 빈칸채우기",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027631.1.31",
          "form": "끽해야 깜지쓰기 정도?¿",
          "original_form": "끽해야 깜지쓰기 정도?¿",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027631.1.32",
          "form": "한국사랑 세계사때 특히 ㅋㅋㅋㅋ",
          "original_form": "한국사랑 세계사때 특히 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027631.1.33",
          "form": "다른 숙제는 기억도 안남",
          "original_form": "다른 숙제는 기억도 안남",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027631.1.34",
          "form": "아 그건 중학교때 학원에서 한 거 같다",
          "original_form": "아 그건 중학교때 학원에서 한 거 같다",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027631.1.35",
          "form": "안했나..",
          "original_form": "안했나..",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027631.1.36",
          "form": "헉 난 고딩때했어 ㅋㅋㅋㅋ",
          "original_form": "헉 난 고딩때했어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027631.1.37",
          "form": "그래서 선생님이 틀리면 손목을 때렸어 검지랑 중지로",
          "original_form": "그래서 선생님이 틀리면 손목을 때렸어 검지랑 중지로",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027631.1.38",
          "form": "나 그래서 중고딩때 한국사세계사 잘했어",
          "original_form": "나 그래서 중고딩때 한국사세계사 잘했어",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027631.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
