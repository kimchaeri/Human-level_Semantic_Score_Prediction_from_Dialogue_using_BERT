      "utterance": [
        {
          "id": "MDRW2100007941.1.1",
          "form": "맞아ㅠㅠ",
          "original_form": "맞아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007941.1.2",
          "form": "난 코 고치려고 하면 엄마가 역정내신다",
          "original_form": "난 코 고치려고 하면 엄마가 역정내신다",
          "speaker_id": "2",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007941.1.3",
          "form": "내 코에 손도 못대게 하실걸",
          "original_form": "내 코에 손도 못대게 하실걸",
          "speaker_id": "2",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007941.1.4",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007941.1.5",
          "form": "근데 코는 워낙 부작용 얘기가 많으니깐ㅠ",
          "original_form": "근데 코는 워낙 부작용 얘기가 많으니깐ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007941.1.6",
          "form": "그래서 의사랑 상담하고 결정할려고",
          "original_form": "그래서 의사랑 상담하고 결정할려고",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007941.1.7",
          "form": "의술이 그때쯤 더 나아지지않았을까?",
          "original_form": "의술이 그때쯤 더 나아지지않았을까?",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007941.1.8",
          "form": "응응, 병원도 잘 알아보고 고르고",
          "original_form": "응응, 병원도 잘 알아보고 고르고",
          "speaker_id": "2",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007941.1.9",
          "form": "아마 부산가서 할 듯",
          "original_form": "아마 부산가서 할 듯",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007941.1.10",
          "form": "엄마랑 가야지ㅎ",
          "original_form": "엄마랑 가야지ㅎ",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007941.1.11",
          "form": "어 그래? 그렇군ㅋㅋㅋ",
          "original_form": "어 그래? 그렇군ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007941.1.12",
          "form": "보통 2~3군데 돌면서 견적받는대",
          "original_form": "보통 2~3군데 돌면서 견적받는대",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007941.1.13",
          "form": "아하 하긴 신중해야하니까",
          "original_form": "아하 하긴 신중해야하니까",
          "speaker_id": "2",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007941.1.14",
          "form": "얼굴 손 대는거니깐 신중히 해야지ㅎㅎ",
          "original_form": "얼굴 손 대는거니깐 신중히 해야지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007941.1.15",
          "form": "맞아맞아 그리고 비용도 비용이니까",
          "original_form": "맞아맞아 그리고 비용도 비용이니까",
          "speaker_id": "2",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007941.1.16",
          "form": "배고프다ㅠ",
          "original_form": "배고프다ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007941.1.17",
          "form": "근데 이제 진짜 식단관리해야해서",
          "original_form": "근데 이제 진짜 식단관리해야해서",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007941.1.18",
          "form": "참고있음ㅠ",
          "original_form": "참고있음ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007941.1.19",
          "form": "너무 살쪘어ㅠ",
          "original_form": "너무 살쪘어ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007941.1.20",
          "form": "이제 저녁 조깅만으로 몸무게를 관리할 수 없어서",
          "original_form": "이제 저녁 조깅만으로 몸무게를 관리할 수 없어서",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007941.1.21",
          "form": "식단을 좀 줄여야해ㅠ",
          "original_form": "식단을 좀 줄여야해ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007941.1.22",
          "form": "슬픔ㅠ 예전처럼 먹으면 안된다는 사실이ㅠ",
          "original_form": "슬픔ㅠ 예전처럼 먹으면 안된다는 사실이ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007941.1.23",
          "form": "ㅠㅠ식단 관리는 어떻게 하려구? 양 줄이는 방법으로?",
          "original_form": "ㅠㅠ식단 관리는 어떻게 하려구? 양 줄이는 방법으로?",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007941.1.24",
          "form": "일단은 그렇지ㅠ",
          "original_form": "일단은 그렇지ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007941.1.25",
          "form": "하고 단 음료 안마시고",
          "original_form": "하고 단 음료 안마시고",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007941.1.26",
          "form": "당뇨 걱정해야해",
          "original_form": "당뇨 걱정해야해",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007941.1.27",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007941.1.28",
          "form": "단 음료는 진짜 끊어야한다더라",
          "original_form": "단 음료는 진짜 끊어야한다더라",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007941.1.29",
          "form": "의사쌤이",
          "original_form": "의사쌤이",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007941.1.30",
          "form": "집안 내력은 없긴한데",
          "original_form": "집안 내력은 없긴한데",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007941.1.31",
          "form": "너무 단걸 좋아해서ㅠ",
          "original_form": "너무 단걸 좋아해서ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007941.1.32",
          "form": "ㅠㅠ끊으려면 힘들겠다",
          "original_form": "ㅠㅠ끊으려면 힘들겠다",
          "speaker_id": "2",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007941.1.33",
          "form": "평소에 좋아했으면",
          "original_form": "평소에 좋아했으면",
          "speaker_id": "2",
          "time": "20210822 12:16"
        }
      ]
