      "utterance": [
        {
          "id": "MDRW2100011806.1.1",
          "form": "뮤지컬보시나요",
          "original_form": "뮤지컬보시나요",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100011806.1.2",
          "form": "뮤지컬 한번도 못봤어요",
          "original_form": "뮤지컬 한번도 못봤어요",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011806.1.3",
          "form": "제가 사람 노래하는거보다 악기를 좋아해서요",
          "original_form": "제가 사람 노래하는거보다 악기를 좋아해서요",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011806.1.4",
          "form": "비싸기도 하고 ㅜㅜ",
          "original_form": "비싸기도 하고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011806.1.5",
          "form": "비싸요ㅜㅜ",
          "original_form": "비싸요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011806.1.6",
          "form": "아하 악기연주듣는거 좋아하시는군요",
          "original_form": "아하 악기연주듣는거 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011806.1.7",
          "form": "네 뮤지컬 관심은 있는데 잘 안가졌어여",
          "original_form": "네 뮤지컬 관심은 있는데 잘 안가졌어여",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011806.1.8",
          "form": "클래식 보기에만해도 시간과 돈이 딸려서요",
          "original_form": "클래식 보기에만해도 시간과 돈이 딸려서요",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011806.1.9",
          "form": "오페라의 유령 그거는 한번 봤어요",
          "original_form": "오페라의 유령 그거는 한번 봤어요",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011806.1.10",
          "form": "유명한거네요",
          "original_form": "유명한거네요",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011806.1.11",
          "form": "뮤지컬 좋아하시나봐요",
          "original_form": "뮤지컬 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011806.1.12",
          "form": "올해초에 국내순회공연했던거같아요 오페라유령",
          "original_form": "올해초에 국내순회공연했던거같아요 오페라유령",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011806.1.13",
          "form": "그냥 종종봤어요",
          "original_form": "그냥 종종봤어요",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011806.1.14",
          "form": "그럼 좋아하는 뮤지컬 배우랑 스타일 있으시겠네요",
          "original_form": "그럼 좋아하는 뮤지컬 배우랑 스타일 있으시겠네요",
          "speaker_id": "1",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100011806.1.15",
          "form": "친구랑같이 다녔어요 박혜나님하고 전동석님 잘하시더라구요",
          "original_form": "친구랑같이 다녔어요 박혜나님하고 전동석님 잘하시더라구요",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011806.1.16",
          "form": "오 그렇군요..",
          "original_form": "오 그렇군요..",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011806.1.17",
          "form": "수미님은 어느 아티스트를 좋아하시나요",
          "original_form": "수미님은 어느 아티스트를 좋아하시나요",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100011806.1.18",
          "form": "기억해둬야겠어요",
          "original_form": "기억해둬야겠어요",
          "speaker_id": "1",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100011806.1.19",
          "form": "용재오닐?",
          "original_form": "용재오닐?",
          "speaker_id": "2",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100011806.1.20",
          "form": "저 예전에 손열음 좋아하다가",
          "original_form": "저 예전에 손열음 좋아하다가",
          "speaker_id": "1",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100011806.1.21",
          "form": "요즘은 문지영 엄청 좋아해요 문지영 아세요?",
          "original_form": "요즘은 문지영 엄청 좋아해요 문지영 아세요?",
          "speaker_id": "1",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100011806.1.22",
          "form": "오 손열음만 알아요",
          "original_form": "오 손열음만 알아요",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011806.1.23",
          "form": "시간되실때 유튭에서 함보세요",
          "original_form": "시간되실때 유튭에서 함보세요",
          "speaker_id": "1",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011806.1.24",
          "form": "이자람님 다음공연이었어서 얼굴을봤었어요",
          "original_form": "이자람님 다음공연이었어서 얼굴을봤었어요",
          "speaker_id": "2",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011806.1.25",
          "form": "유명한 콩쿨 상도받고 잘치는데 조성진이 워낙 유명해서 밀리는 느낌이에요",
          "original_form": "유명한 콩쿨 상도받고 잘치는데 조성진이 워낙 유명해서 밀리는 느낌이에요",
          "speaker_id": "1",
          "time": "20210824 16:20"
        },
        {
          "id": "MDRW2100011806.1.26",
          "form": "아 그러시군요...",
          "original_form": "아 그러시군요...",
          "speaker_id": "1",
          "time": "20210824 16:20"
        }
      ]
    }
