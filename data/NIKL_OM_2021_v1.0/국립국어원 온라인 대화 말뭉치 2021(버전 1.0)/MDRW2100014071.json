      "utterance": [
        {
          "id": "MDRW2100014071.1.1",
          "form": "오 책 읽는거 좋아하시나요 !",
          "original_form": "오 책 읽는거 좋아하시나요 !",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014071.1.2",
          "form": "아뇨 읽어야하는데 시간이 안나네요 핑계같지만ㅎㅎ 책 좋아하세요?",
          "original_form": "아뇨 읽어야하는데 시간이 안나네요 핑계같지만ㅎㅎ 책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014071.1.3",
          "form": "저도 책 안좋아해요 .. ㅎㅎ 저 진짜 고등학생 이후로",
          "original_form": "저도 책 안좋아해요 .. ㅎㅎ 저 진짜 고등학생 이후로",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014071.1.4",
          "form": "책을 안읽은거같은뎈ㅋㅋ",
          "original_form": "책을 안읽은거같은뎈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014071.1.5",
          "form": "아 그래도 예전에는 도서관 가서 책 읽는거 엄청 좋아했어요 !",
          "original_form": "아 그래도 예전에는 도서관 가서 책 읽는거 엄청 좋아했어요 !",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014071.1.6",
          "form": "저도 고딩때 많이읽구 서점 좋아하는데 요즘엔 못가서ㅜㅜ",
          "original_form": "저도 고딩때 많이읽구 서점 좋아하는데 요즘엔 못가서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014071.1.7",
          "form": "맞죠 ㅠㅠ 그 서점만의 느낌이 좋아요 ㅎㅎ",
          "original_form": "맞죠 ㅠㅠ 그 서점만의 느낌이 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014071.1.8",
          "form": "막 서점가면 잡지 같은것도 그냥 슬쩍 보고 ㅋㅋ 와요",
          "original_form": "막 서점가면 잡지 같은것도 그냥 슬쩍 보고 ㅋㅋ 와요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014071.1.9",
          "form": "맞아요ㅠㅠ카페랑은 또 다른 느낌 ...",
          "original_form": "맞아요ㅠㅠ카페랑은 또 다른 느낌 ...",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014071.1.10",
          "form": "근데 진짜 시간이 없어서 못보는것도 맞는거같아요",
          "original_form": "근데 진짜 시간이 없어서 못보는것도 맞는거같아요",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014071.1.11",
          "form": "일단 책 한권이 엄청 두꺼우니까 ..",
          "original_form": "일단 책 한권이 엄청 두꺼우니까 ..",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014071.1.12",
          "form": "맞아요 각잡고 앉아서 책만 볼 시간...그런 여유가 없어요 ㅠ",
          "original_form": "맞아요 각잡고 앉아서 책만 볼 시간...그런 여유가 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014071.1.13",
          "form": "네 저도 한번에 읽는게 좋구ㅠㅠ",
          "original_form": "네 저도 한번에 읽는게 좋구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014071.1.14",
          "form": "맞아요 ㅠㅠ 한번에 읽는게 젤 좋아요 !",
          "original_form": "맞아요 ㅠㅠ 한번에 읽는게 젤 좋아요 !",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014071.1.15",
          "form": "나눠읽으면 헷갈리더라구요",
          "original_form": "나눠읽으면 헷갈리더라구요",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014071.1.16",
          "form": "저 항상 나눠읽으면 앞페이지부터 갔다가 와요",
          "original_form": "저 항상 나눠읽으면 앞페이지부터 갔다가 와요",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014071.1.17",
          "form": "무슨 내용인지 까먹어섴ㅋㅋ",
          "original_form": "무슨 내용인지 까먹어섴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014071.1.18",
          "form": "ㅋㅋㅋ저두요 진도가 그래서 안나가요ㅋㅋ",
          "original_form": "ㅋㅋㅋ저두요 진도가 그래서 안나가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014071.1.19",
          "form": "ㅋㅋㅋㅋㅋ 그리고 베스트셀러라고 해서 읽었는데",
          "original_form": "ㅋㅋㅋㅋㅋ 그리고 베스트셀러라고 해서 읽었는데",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014071.1.20",
          "form": "재미없는 경우도",
          "original_form": "재미없는 경우도",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014071.1.21",
          "form": "많지않나요 ㅎㅎ",
          "original_form": "많지않나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014071.1.22",
          "form": "네 맞아요ㅋㅋ안맞는 책 엄청 많죠...ㅋㅋ",
          "original_form": "네 맞아요ㅋㅋ안맞는 책 엄청 많죠...ㅋㅋ",
