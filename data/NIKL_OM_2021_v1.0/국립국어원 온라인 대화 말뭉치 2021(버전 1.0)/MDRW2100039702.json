        },
        {
          "id": "MDRW2100039702.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039702.1.3",
          "form": "대학생이세요?",
          "original_form": "대학생이세요?",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039702.1.4",
          "form": "아뇨~",
          "original_form": "아뇨~",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039702.1.5",
          "form": "ㅎㅎ 저는 대학생이라서,,",
          "original_form": "ㅎㅎ 저는 대학생이라서,,",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039702.1.6",
          "form": "뭐.전공하세여?",
          "original_form": "뭐.전공하세여?",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039702.1.7",
          "form": "저는 물리치료 전공해요!",
          "original_form": "저는 물리치료 전공해요!",
          "speaker_id": "1",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039702.1.8",
          "form": "어떤거 전공하셨어요?",
          "original_form": "어떤거 전공하셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100039702.1.9",
          "form": "와~멋지시네요 전오래되었지만 경영학과다녔어요",
          "original_form": "와~멋지시네요 전오래되었지만 경영학과다녔어요",
          "speaker_id": "2",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039702.1.10",
          "form": "우와 경영 무슨 트랙 하셨어요?",
          "original_form": "우와 경영 무슨 트랙 하셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100039702.1.11",
          "form": "그냥 경영학과였는데 제가 전공을 살려서 일한게 아니라 ㅎㅎ",
          "original_form": "그냥 경영학과였는데 제가 전공을 살려서 일한게 아니라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039702.1.12",
          "form": "그냥 무늬만 대학생이엿네요 ㅋㅋ",
          "original_form": "그냥 무늬만 대학생이엿네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039702.1.13",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039702.1.14",
          "form": "지금은 어느분여에서 일하세요?",
          "original_form": "지금은 어느분여에서 일하세요?",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039702.1.15",
          "form": "물리치료 졸업하시면",
          "original_form": "물리치료 졸업하시면",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039702.1.16",
          "form": "지금은 그냥 알바만해요 ㅎ 거의무직이죠ㅠ",
          "original_form": "지금은 그냥 알바만해요 ㅎ 거의무직이죠ㅠ",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039702.1.17",
          "form": "병원가시겠네요~",
          "original_form": "병원가시겠네요~",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039702.1.18",
          "form": "요즘 뉴스에서 엄청 많이 봤어요 ㅠㅠ",
          "original_form": "요즘 뉴스에서 엄청 많이 봤어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039702.1.19",
          "form": "네 아마 그럴 것 같아요..!",
          "original_form": "네 아마 그럴 것 같아요..!",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039702.1.20",
          "form": "탄탄하시네요~~전에 허리랑 목안좋아서 정형외과오래다녔는데",
          "original_form": "탄탄하시네요~~전에 허리랑 목안좋아서 정형외과오래다녔는데",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039702.1.21",
          "form": "도수치료받으면 정말시원하더라구요!",
          "original_form": "도수치료받으면 정말시원하더라구요!",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039702.1.22",
          "form": "오오 정말요?",
          "original_form": "오오 정말요?",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039702.1.23",
          "form": "저는 물리치료학과 지만 한번도 정형외과를 가본적이 없어요..ㅎ",
          "original_form": "저는 물리치료학과 지만 한번도 정형외과를 가본적이 없어요..ㅎ",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039702.1.24",
          "form": "정말요? 전 몇주전에도 어깨가 아파서갔었는데ㅠ",
          "original_form": "정말요? 전 몇주전에도 어깨가 아파서갔었는데ㅠ",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039702.1.25",
          "form": "헐 ㅠㅠ 어디가 많이 안좋으세요??ㅠㅠ",
          "original_form": "헐 ㅠㅠ 어디가 많이 안좋으세요??ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039702.1.26",
          "form": "거북목이라고 하더라구요",
          "original_form": "거북목이라고 하더라구요",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039702.1.27",
          "form": "전 어깨가아파서 갔는데 목이안좋다고하더라구요ㅠ",
          "original_form": "전 어깨가아파서 갔는데 목이안좋다고하더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210903 20:56"
        }
      ]
