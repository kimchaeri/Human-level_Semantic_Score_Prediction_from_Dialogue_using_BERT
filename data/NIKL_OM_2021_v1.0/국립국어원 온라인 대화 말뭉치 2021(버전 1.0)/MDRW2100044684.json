      "utterance": [
        {
          "id": "MDRW2100044684.1.1",
          "form": "그래서 브이앱 이런거 다 거기서하거든 소속가수들....?",
          "original_form": "그래서 브이앱 이런거 다 거기서하거든 소속가수들....?",
          "speaker_id": "1",
