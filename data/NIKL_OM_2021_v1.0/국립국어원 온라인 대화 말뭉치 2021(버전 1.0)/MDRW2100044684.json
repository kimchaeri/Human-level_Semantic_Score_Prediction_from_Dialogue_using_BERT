        },
        {
          "id": "MDRW2100044684.1.2",
          "form": ".......",
          "original_form": ".......",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.3",
          "form": "저번에 민현이가 브이앱하는 도중에 누가(돌로 보이는 누군가)가 들어와서 자기가 여기 예약했다고 그래서 그사람이 자기가 착각했ㄷ고",
          "original_form": "저번에 민현이가 브이앱하는 도중에 누가(돌로 보이는 누군가)가 들어와서 자기가 여기 예약했다고 그래서 그사람이 자기가 착각했ㄷ고",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.4",
          "form": "두렵다",
          "original_form": "두렵다",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.5",
          "form": "나갔었어",
          "original_form": "나갔었어",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.6",
          "form": "근데 그거 누가들어갔는지 어케알아",
          "original_form": "근데 그거 누가들어갔는지 어케알아",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.7",
          "form": "환기도",
          "original_form": "환기도",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.8",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.9",
          "form": "제대로 안시킬거같은데",
          "original_form": "제대로 안시킬거같은데",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.10",
          "form": "민희진의 작품이",
          "original_form": "민희진의 작품이",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ아니 소독을 주기적으로해도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니 소독을 주기적으로해도",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.12",
          "form": "늘하지는 않을거아니야",
          "original_form": "늘하지는 않을거아니야",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.13",
          "form": "직전에 누가썼는지 알게뭐야",
          "original_form": "직전에 누가썼는지 알게뭐야",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.14",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.15",
          "form": "비말입자들 날아다닐텐데",
          "original_form": "비말입자들 날아다닐텐데",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.16",
          "form": "그리고 델타는",
          "original_form": "그리고 델타는",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044684.1.17",
          "form": "접촉감염도 많잖아",
          "original_form": "접촉감염도 많잖아",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044684.1.18",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044684.1.19",
          "form": "ㅡㅡ 아니그래서지금",
          "original_form": "ㅡㅡ 아니그래서지금",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044684.1.20",
          "form": "애들 걸렸을까봐 걱정",
          "original_form": "애들 걸렸을까봐 걱정",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044684.1.21",
          "form": "검사는 다 받아야겠다",
          "original_form": "검사는 다 받아야겠다",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
