      "utterance": [
        {
          "id": "MDRW2100004153.1.1",
          "form": "연애하시고 계시나요?",
          "original_form": "연애하시고 계시나요?",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004153.1.2",
          "form": "아뇨.. 안하고있어요ㅋㅋ",
          "original_form": "아뇨.. 안하고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004153.1.3",
          "form": "먹고사는거만 생각하느라 연애생각도 안나네여",
          "original_form": "먹고사는거만 생각하느라 연애생각도 안나네여",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004153.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004153.1.5",
          "form": "약간 부질 없다고 느끼구요",
          "original_form": "약간 부질 없다고 느끼구요",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004153.1.6",
          "form": "그렇긴하죠 부질없어ㅠ",
          "original_form": "그렇긴하죠 부질없어ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004153.1.7",
          "form": "근데 또 연애를 쉬지않고 하는 사람들도 있고 약간",
          "original_form": "근데 또 연애를 쉬지않고 하는 사람들도 있고 약간",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004153.1.8",
          "form": "사람마다 엄청 다르네요",
          "original_form": "사람마다 엄청 다르네요",
          "speaker_id": "1",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004153.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004153.1.10",
          "form": "결혼은 어떻게 생각하세요?",
          "original_form": "결혼은 어떻게 생각하세요?",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004153.1.11",
          "form": "결혼은 뭔가..하고싶긴해요",
          "original_form": "결혼은 뭔가..하고싶긴해요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004153.1.12",
          "form": "걍 나중에 혼자 살 자신이없어서요",
          "original_form": "걍 나중에 혼자 살 자신이없어서요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004153.1.13",
          "form": "그렇지만 굳이 급해서 할필요는 없는거같아요",
          "original_form": "그렇지만 굳이 급해서 할필요는 없는거같아요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004153.1.14",
          "form": "오 그럴 수 있죠",
          "original_form": "오 그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004153.1.15",
          "form": "저는 결혼 할 생각이 없어요 ㅎ",
          "original_form": "저는 결혼 할 생각이 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004153.1.16",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004153.1.17",
          "form": "저도 굳이 결혼아니고 걍",
          "original_form": "저도 굳이 결혼아니고 걍",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004153.1.18",
          "form": "같이살사람이 있으면 좋겠다고 생각해요",
          "original_form": "같이살사람이 있으면 좋겠다고 생각해요",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004153.1.19",
          "form": "뭐 사랑하는사람아니고",
          "original_form": "뭐 사랑하는사람아니고",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004153.1.20",
          "form": "걍 친구라도 같이살사람있으면",
          "original_form": "걍 친구라도 같이살사람있으면",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004153.1.21",
          "form": "저도 결혼 안할거같아요",
          "original_form": "저도 결혼 안할거같아요",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004153.1.22",
          "form": "맞아요 전 괜찮은 친구랑",
          "original_form": "맞아요 전 괜찮은 친구랑",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004153.1.23",
          "form": "오래 살고싶어요",
          "original_form": "오래 살고싶어요",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004153.1.24",
          "form": "하..그쳐",
          "original_form": "하..그쳐",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004153.1.25",
          "form": "농담처럼 친구랑 나중에 늙으면 같이살자이러는데",
          "original_form": "농담처럼 친구랑 나중에 늙으면 같이살자이러는데",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004153.1.26",
          "form": "사실 농담아니고 걍 같이살아달라고 바짓가랑이잡고싶어요",
          "original_form": "사실 농담아니고 걍 같이살아달라고 바짓가랑이잡고싶어요",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004153.1.27",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004153.1.28",
          "form": "ㅋㅋ ㅋㅋㅋ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 뭔가 이해되네요",
          "original_form": "ㅋㅋ ㅋㅋㅋ ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 뭔가 이해되네요",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004153.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ하핫",
          "original_form": "ㅋㅋㅋㅋㅋㅋ하핫",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004153.1.30",
          "form": "아니면 맘맞는 형제있으면 형제랑 살면 좋을거같아요",
          "original_form": "아니면 맘맞는 형제있으면 형제랑 살면 좋을거같아요",
          "speaker_id": "1",
