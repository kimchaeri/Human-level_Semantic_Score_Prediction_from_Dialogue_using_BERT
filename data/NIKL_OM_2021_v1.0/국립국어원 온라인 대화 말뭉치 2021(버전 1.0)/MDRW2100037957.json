      "utterance": [
        {
          "id": "MDRW2100037957.1.1",
          "form": "요즘 춘식이 쿠션이 갖고싶어졌어요",
          "original_form": "요즘 춘식이 쿠션이 갖고싶어졌어요",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100037957.1.2",
          "form": "춘식이 너무 귀엽죠 ㅋㅋㅋㅋㅋ",
          "original_form": "춘식이 너무 귀엽죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100037957.1.3",
          "form": "춘식이 키링을 샀는데 왜 배송이 안올까요 ㅎㅎ",
          "original_form": "춘식이 키링을 샀는데 왜 배송이 안올까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037957.1.4",
          "form": "아 진짜 ",
          "original_form": "아 진짜 ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037957.1.5",
          "form": "진짜 너무귀여운",
          "original_form": "진짜 너무귀여운",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037957.1.6",
          "form": "와 키링!!!!",
          "original_form": "와 키링!!!!",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037957.1.7",
          "form": "귀엽겠다ㅠ",
          "original_form": "귀엽겠다ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100037957.1.8",
          "form": "저는 춘식이의 그 똥배라인이 너무 귀여워서 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 춘식이의 그 똥배라인이 너무 귀여워서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100037957.1.9",
          "form": "똥배 때문에 샀어요 ㅎㅎ",
          "original_form": "똥배 때문에 샀어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037957.1.10",
          "form": "쿠션 꼭 사셔야해요! ㅋㅋㅋㅋ",
          "original_form": "쿠션 꼭 사셔야해요! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037957.1.11",
          "form": "아 진짜 똥배라인 ㅋㅋㅋ존귀탱이에요",
          "original_form": "아 진짜 똥배라인 ㅋㅋㅋ존귀탱이에요",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037957.1.12",
          "form": "쿠션...",
          "original_form": "쿠션...",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037957.1.13",
          "form": "일단 요즘 차에 돈들어갈때가 생겨서",
          "original_form": "일단 요즘 차에 돈들어갈때가 생겨서",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037957.1.14",
          "form": "미뤄뒀어요ㅠ",
          "original_form": "미뤄뒀어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100037957.1.15",
          "form": "춘식이 인기 많아서 품절은 안될거에요! ㅎㅎ",
          "original_form": "춘식이 인기 많아서 품절은 안될거에요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100037957.1.16",
          "form": "그렇겠죠!?",
          "original_form": "그렇겠죠!?",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100037957.1.17",
          "form": "그 얼굴모양 쿠션 너무 갖고싶어요",
          "original_form": "그 얼굴모양 쿠션 너무 갖고싶어요",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100037957.1.18",
          "form": "그럼요!",
          "original_form": "그럼요!",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037957.1.19",
          "form": "표정 더 다양하게 나올 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "표정 더 다양하게 나올 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037957.1.20",
          "form": "오 좋네요ㅋㅋㅋ표정 너무 귀여워요 프로아이돌",
          "original_form": "오 좋네요ㅋㅋㅋ표정 너무 귀여워요 프로아이돌",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100037957.1.21",
          "form": "아이돌답게 굿즈가 가득해서 좋네요 ㅎㅎ",
          "original_form": "아이돌답게 굿즈가 가득해서 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100037957.1.22",
          "form": "춘식이 실사크기 인형도 내주면 좋겠어요 ㅎㅎ",
          "original_form": "춘식이 실사크기 인형도 내주면 좋겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100037957.1.23",
          "form": "헉 대박 그것도 너무 좋은데요",
          "original_form": "헉 대박 그것도 너무 좋은데요",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100037957.1.24",
          "form": "ㅠㅠ 진짜 춤추는거 하루에 몇번씩 본다죠",
          "original_form": "ㅠㅠ 진짜 춤추는거 하루에 몇번씩 본다죠",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100037957.1.25",
          "form": "진짜ㅋㅋㅋ 그 퀸덤이랑 넥스트레벨 춘거 넘 귀여워요",
          "original_form": "진짜ㅋㅋㅋ 그 퀸덤이랑 넥스트레벨 춘거 넘 귀여워요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100037957.1.26",
          "form": "안무영상이 진짜 한몫 하는 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "안무영상이 진짜 한몫 하는 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100037957.1.27",
          "form": "안무동작 렌티큘러로 팔면 잘 팔릴 것 같아요 ㅋㅋㅋ",
          "original_form": "안무동작 렌티큘러로 팔면 잘 팔릴 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100037957.1.28",
          "form": "맞아요 그게 큰거같아요 ㅎ",
          "original_form": "맞아요 그게 큰거같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100037957.1.29",
          "form": "저도 그전까진. 귀엽네 정도였는데",
          "original_form": "저도 그전까진. 귀엽네 정도였는데",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100037957.1.30",
          "form": "그거보고 홀릭 ㅎ",
          "original_form": "그거보고 홀릭 ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100037957.1.31",
          "form": "안무동작 ㅋㅋ그것도 잘팔릴듯요",
          "original_form": "안무동작 ㅋㅋ그것도 잘팔릴듯요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        }
      ]
