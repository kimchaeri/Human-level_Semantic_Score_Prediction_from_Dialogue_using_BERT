        },
        {
          "id": "MDRW2100047655.1.2",
          "form": "닭발은 드세요?",
          "original_form": "닭발은 드세요?",
          "speaker_id": "1",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100047655.1.3",
          "form": "아니요 못먹어요",
          "original_form": "아니요 못먹어요",
          "speaker_id": "2",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100047655.1.4",
          "form": "드시나요?",
          "original_form": "드시나요?",
          "speaker_id": "2",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100047655.1.5",
          "form": "네! 해산물은 좋아하세요?",
          "original_form": "네! 해산물은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100047655.1.6",
          "form": "네! 너무 좋아해요",
          "original_form": "네! 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100047655.1.7",
          "form": "간장게장은요?",
          "original_form": "간장게장은요?",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100047655.1.8",
          "form": "너무 좋죠 없어서 못먹어요",
          "original_form": "너무 좋죠 없어서 못먹어요",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
