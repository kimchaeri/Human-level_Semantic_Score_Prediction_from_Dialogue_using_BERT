      "utterance": [
        {
          "id": "MDRW2100007388.1.1",
          "form": "오늘 큐킹덤 출첵하러 가야하는데",
          "original_form": "오늘 큐킹덤 출첵하러 가야하는데",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007388.1.2",
          "form": "나는지금하고있다",
          "original_form": "나는지금하고있다",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007388.1.3",
          "form": "나 새로운덱 배우기가",
          "original_form": "나 새로운덱 배우기가",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.4",
          "form": "너무귀찮다",
          "original_form": "너무귀찮다",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.5",
          "form": "좋겠다 아이패드 있어서",
          "original_form": "좋겠다 아이패드 있어서",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.6",
          "form": "대화하면서 게임하고!",
          "original_form": "대화하면서 게임하고!",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.7",
          "form": "새로운덱 뭐?",
          "original_form": "새로운덱 뭐?",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.8",
          "form": "샤벳덱?",
          "original_form": "샤벳덱?",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.9",
          "form": "샤벳있는거",
          "original_form": "샤벳있는거",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.10",
          "form": "연습모드도생겼는데",
          "original_form": "연습모드도생겼는데",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.11",
          "form": "왜그리귀찮은지...",
          "original_form": "왜그리귀찮은지...",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ나도연습모드 거의 안써",
          "original_form": "ㅋㅋㅋㅋㅋㅋ나도연습모드 거의 안써",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.13",
          "form": "야 글고 단계 올라가니까",
          "original_form": "야 글고 단계 올라가니까",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007388.1.14",
          "form": "샤벳이 2번 못때리고 죽어서",
          "original_form": "샤벳이 2번 못때리고 죽어서",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007388.1.15",
          "form": "호밀이랑 별 다를거 없이 뎀나와",
          "original_form": "호밀이랑 별 다를거 없이 뎀나와",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007388.1.16",
          "form": "나 방금 토벌전 돌렸는데",
          "original_form": "나 방금 토벌전 돌렸는데",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007388.1.17",
          "form": "410나왔어",
          "original_form": "410나왔어",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007388.1.18",
          "form": "안바꾸는게 나을지도",
          "original_form": "안바꾸는게 나을지도",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007388.1.19",
          "form": "헐호밀로?",
          "original_form": "헐호밀로?",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007388.1.20",
          "form": "나 해보러간다",
          "original_form": "나 해보러간다",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.21",
          "form": "너근데 호밀 기본 뎀이",
          "original_form": "너근데 호밀 기본 뎀이",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.22",
          "form": "나보다 더 센거같아",
          "original_form": "나보다 더 센거같아",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.23",
          "form": "토핑작 열싲히햇어?",
          "original_form": "토핑작 열싲히햇어?",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.24",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.25",
          "form": "5성이니까",
          "original_form": "5성이니까",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.26",
          "form": "토핑작은호밀은 대충했어",
          "original_form": "토핑작은호밀은 대충했어",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.27",
          "form": "나도 5성임 ㅠ",
          "original_form": "나도 5성임 ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.28",
          "form": "쿠킹덤특성상",
          "original_form": "쿠킹덤특성상",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.29",
          "form": "잉...",
          "original_form": "잉...",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.30",
          "form": "지금볼게 호밀 토핑",
          "original_form": "지금볼게 호밀 토핑",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007388.1.31",
          "form": "공격력이 47.7퍼야",
          "original_form": "공격력이 47.7퍼야",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007388.1.32",
          "form": "피감 12.6있고 치명타피감 7.6있고",
          "original_form": "피감 12.6있고 치명타피감 7.6있고",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007388.1.33",
          "form": "나머지는 별거없네",
          "original_form": "나머지는 별거없네",
          "speaker_id": "1",
          "time": "20210822 01:47"
        }
      ]
    }
