      "utterance": [
        {
          "id": "MDRW2100046751.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 11:59"
        },
        {
          "id": "MDRW2100046751.1.2",
          "form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "original_form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "speaker_id": "2",
          "time": "20211105 00:01"
        },
        {
          "id": "MDRW2100046751.1.3",
          "form": "맛점하세요...",
          "original_form": "맛점하세요...",
          "speaker_id": "1",
          "time": "20211105 00:01"
        },
        {
          "id": "MDRW2100046751.1.4",
          "form": "단풍놀이 계획 있으신가요?",
          "original_form": "단풍놀이 계획 있으신가요?",
          "speaker_id": "2",
          "time": "20211104 13:18"
        },
        {
          "id": "MDRW2100046751.1.5",
          "form": "코로나 때문에 아직이요",
          "original_form": "코로나 때문에 아직이요",
          "speaker_id": "1",
          "time": "20211104 13:46"
        },
        {
          "id": "MDRW2100046751.1.6",
          "form": "이번주가 절정이래요!",
          "original_form": "이번주가 절정이래요!",
          "speaker_id": "2",
          "time": "20211104 14:01"
        },
        {
          "id": "MDRW2100046751.1.7",
          "form": "그래도 아직 코로나때문에 여행가기는 조심스럽죠",
          "original_form": "그래도 아직 코로나때문에 여행가기는 조심스럽죠",
          "speaker_id": "2",
          "time": "20211104 14:01"
        },
        {
          "id": "MDRW2100046751.1.8",
          "form": "이번 주말에는 뭐하시나요~?",
          "original_form": "이번 주말에는 뭐하시나요~?",
          "speaker_id": "2",
          "time": "20211104 14:01"
        },
        {
          "id": "MDRW2100046751.1.9",
          "form": "주말에는 집 대청소 할 예정이에요",
          "original_form": "주말에는 집 대청소 할 예정이에요",
          "speaker_id": "1",
          "time": "20211104 15:11"
        },
        {
          "id": "MDRW2100046751.1.10",
          "form": "저희집도 주말에 몰아서 청소해요 ㅎㅎ",
          "original_form": "저희집도 주말에 몰아서 청소해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 15:12"
        },
        {
          "id": "MDRW2100046751.1.11",
          "form": "청소하는거 좋아하시나요?",
          "original_form": "청소하는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211104 15:12"
        },
        {
          "id": "MDRW2100046751.1.12",
          "form": "좋아하진 않지만 날잡아서 종종 청소해",
          "original_form": "좋아하진 않지만 날잡아서 종종 청소해",
          "speaker_id": "1",
          "time": "20211104 15:12"
        },
        {
          "id": "MDRW2100046751.1.13",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20211104 15:12"
        },
        {
          "id": "MDRW2100046751.1.14",
          "form": "청소는 꼭 해야하는 일이죠 ㅠ",
          "original_form": "청소는 꼭 해야하는 일이죠 ㅠ",
          "speaker_id": "2",
          "time": "20211104 15:13"
        },
        {
          "id": "MDRW2100046751.1.15",
          "form": "그중에서 가장 싫어하는 청소는 뭐예요?",
          "original_form": "그중에서 가장 싫어하는 청소는 뭐예요?",
          "speaker_id": "2",
          "time": "20211104 15:13"
        },
        {
          "id": "MDRW2100046751.1.16",
          "form": "고양이 한마리 키우는데",
          "original_form": "고양이 한마리 키우는데",
          "speaker_id": "1",
          "time": "20211104 15:14"
        },
        {
          "id": "MDRW2100046751.1.17",
          "form": "고양이 화장실 통채로 들고가서 청소하거든요",
          "original_form": "고양이 화장실 통채로 들고가서 청소하거든요",
          "speaker_id": "1",
          "time": "20211104 15:14"
        },
        {
          "id": "MDRW2100046751.1.18",
          "form": "부피가 커서 좀 부담이에요",
          "original_form": "부피가 커서 좀 부담이에요",
          "speaker_id": "1",
          "time": "20211104 15:14"
        },
        {
          "id": "MDRW2100046751.1.19",
          "form": "고양이 키우시는군요!",
          "original_form": "고양이 키우시는군요!",
          "speaker_id": "2",
          "time": "20211104 15:14"
        },
        {
          "id": "MDRW2100046751.1.20",
          "form": "아무래도 반려동물이 있으면 청소에 더 신경쓰게 되는 것 같아요",
          "original_form": "아무래도 반려동물이 있으면 청소에 더 신경쓰게 되는 것 같아요",
          "speaker_id": "2",
          "time": "20211104 15:15"
        },
        {
          "id": "MDRW2100046751.1.21",
          "form": "저도 죽기 전에 고양이 키워보는게 소원이에요",
          "original_form": "저도 죽기 전에 고양이 키워보는게 소원이에요",
          "speaker_id": "2",
          "time": "20211104 15:15"
        },
        {
          "id": "MDRW2100046751.1.22",
          "form": "털 포기하신다면 키워보셔도 좋다고 생각해요 ^^;",
          "original_form": "털 포기하신다면 키워보셔도 좋다고 생각해요 ^^;",
          "speaker_id": "1",
          "time": "20211104 15:15"
        },
        {
          "id": "MDRW2100046751.1.23",
          "form": "털이 공중에 떠다녀서..ㅎㅎ",
          "original_form": "털이 공중에 떠다녀서..ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 15:16"
        },
        {
          "id": "MDRW2100046751.1.24",
          "form": "ㅋㅋㅋ 고양이 키우시는 분들은 다 그렇게 말씀하시더라구요",
          "original_form": "ㅋㅋㅋ 고양이 키우시는 분들은 다 그렇게 말씀하시더라구요",
          "speaker_id": "2",
          "time": "20211104 15:16"
        },
        {
          "id": "MDRW2100046751.1.25",
          "form": "지금 또 한창 털 찔 시기지요?",
          "original_form": "지금 또 한창 털 찔 시기지요?",
          "speaker_id": "2",
          "time": "20211104 15:16"
        },
        {
          "id": "MDRW2100046751.1.26",
          "form": "일년내내였던거 같아요 ^^;",
          "original_form": "일년내내였던거 같아요 ^^;",
          "speaker_id": "1",
          "time": "20211104 15:16"
        },
        {
          "id": "MDRW2100046751.1.27",
          "form": "ㅋㅋㅋㅋㅋ 그정도라니 ㅠㅠ ",
          "original_form": "ㅋㅋㅋㅋㅋ 그정도라니 ㅠㅠ ",
          "speaker_id": "2",
          "time": "20211104 15:17"
        },
        {
          "id": "MDRW2100046751.1.28",
          "form": "털 청소는 그럼 따로 못하시겠네요.. 일년내내면요",
          "original_form": "털 청소는 그럼 따로 못하시겠네요.. 일년내내면요",
          "speaker_id": "2",
          "time": "20211104 15:17"
        },
        {
          "id": "MDRW2100046751.1.29",
          "form": "돌돌이로 이불 밀어주고 잠자리도 해주고 하죵 ㅎㅎ",
          "original_form": "돌돌이로 이불 밀어주고 잠자리도 해주고 하죵 ㅎㅎ",
