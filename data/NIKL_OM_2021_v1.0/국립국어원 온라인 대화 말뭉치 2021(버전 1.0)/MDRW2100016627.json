      "utterance": [
        {
          "id": "MDRW2100016627.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016627.1.2",
          "form": "중고거래 평소에 하시나요?",
          "original_form": "중고거래 평소에 하시나요?",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016627.1.3",
          "form": "자주해요!",
          "original_form": "자주해요!",
          "speaker_id": "1",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016627.1.4",
          "form": "어디서 주로 하세요?",
          "original_form": "어디서 주로 하세요?",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016627.1.5",
          "form": "당근이랑 중고나라요",
          "original_form": "당근이랑 중고나라요",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016627.1.6",
          "form": "사기당해보신적은 없으세요?",
          "original_form": "사기당해보신적은 없으세요?",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016627.1.7",
          "form": "네 한번도요 님은요?",
          "original_form": "네 한번도요 님은요?",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016627.1.8",
          "form": "저도 아직 운이 좋았는지 사기당한적은 없어요",
          "original_form": "저도 아직 운이 좋았는지 사기당한적은 없어요",
          "speaker_id": "2",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016627.1.9",
          "form": "쇼핑은 주로 어디서 하세요?",
          "original_form": "쇼핑은 주로 어디서 하세요?",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016627.1.10",
          "form": "뭐 팔아보셨나요?",
          "original_form": "뭐 팔아보셨나요?",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016627.1.11",
          "form": "네이버요",
          "original_form": "네이버요",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016627.1.12",
          "form": "저는 샀는데 안쓰는건 좀 지나면 다 팔아오",
          "original_form": "저는 샀는데 안쓰는건 좀 지나면 다 팔아오",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016627.1.13",
          "form": "저는 최근에 버즈 팔았어요",
          "original_form": "저는 최근에 버즈 팔았어요",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016627.1.14",
          "form": "아 저는 중고로 버즈 사본적 있는데",
          "original_form": "아 저는 중고로 버즈 사본적 있는데",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
