      "utterance": [
        {
          "id": "MDRW2100017984.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100017984.1.2",
          "form": "저는 휴학중이에여 ㅠㅠ",
          "original_form": "저는 휴학중이에여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100017984.1.3",
          "form": "휴학중이시구나ㅠ",
          "original_form": "휴학중이시구나ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100017984.1.4",
          "form": "무슨과세요?",
          "original_form": "무슨과세요?",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100017984.1.5",
          "form": "저는 경영과에요",
          "original_form": "저는 경영과에요",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100017984.1.6",
          "form": "우와 경영과는 과제 많나요?",
          "original_form": "우와 경영과는 과제 많나요?",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017984.1.7",
          "form": "음 저는 다른 과 얘기를 못 들어봐서 모르겠는데 적진 않은거같아요 ㅜㅜ",
          "original_form": "음 저는 다른 과 얘기를 못 들어봐서 모르겠는데 적진 않은거같아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017984.1.8",
          "form": "발표가 많아서 아 잘못왔다 싶은...ㅋㅋㅋ",
          "original_form": "발표가 많아서 아 잘못왔다 싶은...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017984.1.9",
          "form": "아ㅋㅋ 발표가 많다니.. 저는 발표가 힘들더라구요",
          "original_form": "아ㅋㅋ 발표가 많다니.. 저는 발표가 힘들더라구요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100017984.1.10",
          "form": "익숙해지지 않고  늘 떨려요",
          "original_form": "익숙해지지 않고  늘 떨려요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100017984.1.11",
          "form": "맞아요 ㅠㅠ 저는 발표하는 날에는 끝나기 전까지 머리속 지배당해요 하기싫어서 ㅠ",
          "original_form": "맞아요 ㅠㅠ 저는 발표하는 날에는 끝나기 전까지 머리속 지배당해요 하기싫어서 ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100017984.1.12",
          "form": "아ㅜ 생각만으로도 떨려요",
          "original_form": "아ㅜ 생각만으로도 떨려요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100017984.1.13",
          "form": "준비를해도 왜 말을 잘 못전하는지ㅠ",
          "original_form": "준비를해도 왜 말을 잘 못전하는지ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100017984.1.14",
          "form": "맞아요 ㅠㅠ 대본을 써도 힘들어요",
          "original_form": "맞아요 ㅠㅠ 대본을 써도 힘들어요",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100017984.1.15",
          "form": "알죠알죠ㅠ",
          "original_form": "알죠알죠ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100017984.1.16",
          "form": "차라리 자료조사가 전 백번 낫더라구요",
          "original_form": "차라리 자료조사가 전 백번 낫더라구요",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100017984.1.17",
          "form": "저도요! 글로 써서 내는게 더 편해요",
          "original_form": "저도요! 글로 써서 내는게 더 편해요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100017984.1.18",
          "form": "더 잘 전달할 수 있고요",
          "original_form": "더 잘 전달할 수 있고요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        }
      ]
