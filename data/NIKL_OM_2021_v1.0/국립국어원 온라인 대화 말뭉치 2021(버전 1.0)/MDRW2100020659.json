      "utterance": [
        {
          "id": "MDRW2100020659.1.1",
          "form": "꼬깔콘 중에 새콤달콤한건",
          "original_form": "꼬깔콘 중에 새콤달콤한건",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020659.1.2",
          "form": "좀 뒷순위에요",
          "original_form": "좀 뒷순위에요",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020659.1.3",
          "form": "요즘 대세는 매콤달콤한맛..",
          "original_form": "요즘 대세는 매콤달콤한맛..",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020659.1.4",
          "form": "새콤달콤한맛이 있나요",
          "original_form": "새콤달콤한맛이 있나요",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020659.1.5",
          "form": "맞아요 새콤은 아니라구",
          "original_form": "맞아요 새콤은 아니라구",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020659.1.6",
          "form": "근데 저 맵달맛 안좋아해요",
          "original_form": "근데 저 맵달맛 안좋아해요",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020659.1.7",
          "form": "약간 새콤한 애가 있어요",
          "original_form": "약간 새콤한 애가 있어요",
          "speaker_id": "2",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020659.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.9",
          "form": "헉 맵달의",
          "original_form": "헉 맵달의",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.10",
          "form": "최고봉은",
          "original_form": "최고봉은",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.11",
          "form": "신당동 떡볶이 과자인데",
          "original_form": "신당동 떡볶이 과자인데",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.13",
          "form": "불호신가요",
          "original_form": "불호신가요",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.14",
          "form": "그거 완전맛있어",
          "original_form": "그거 완전맛있어",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.15",
          "form": "머죠",
          "original_form": "머죠",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.16",
          "form": "그거 한번먹으면 끝장 봐야해요",
          "original_form": "그거 한번먹으면 끝장 봐야해요",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.17",
          "form": "감탄사입니다",
          "original_form": "감탄사입니다",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.19",
          "form": "이 두얼굴...",
          "original_form": "이 두얼굴...",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.20",
          "form": "신당동 떡볶이",
          "original_form": "신당동 떡볶이",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020659.1.21",
          "form": "맵달맛의 최고봉인데 좋아하시다니..!",
          "original_form": "맵달맛의 최고봉인데 좋아하시다니..!",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020659.1.22",
          "form": "과자봉지 쪼매나서 이게뭐당가 하는데",
          "original_form": "과자봉지 쪼매나서 이게뭐당가 하는데",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020659.1.23",
          "form": "과자가 묵직해서",
          "original_form": "과자가 묵직해서",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020659.1.24",
          "form": "맞아요 먹고나면 배부르죠",
          "original_form": "맞아요 먹고나면 배부르죠",
          "speaker_id": "2",
          "time": "20210828 10:55"
        }
      ]
