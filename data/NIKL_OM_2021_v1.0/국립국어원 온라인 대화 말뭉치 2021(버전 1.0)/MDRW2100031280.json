      "utterance": [
        {
          "id": "MDRW2100031280.1.1",
          "form": "전자전기컴퓨터공학",
          "original_form": "전자전기컴퓨터공학",
          "speaker_id": "1",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031280.1.2",
          "form": "전자전기컴퓨터공학 전공했어요",
          "original_form": "전자전기컴퓨터공학 전공했어요",
          "speaker_id": "1",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031280.1.3",
          "form": "오 공학 전공이시구나",
          "original_form": "오 공학 전공이시구나",
          "speaker_id": "2",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031280.1.4",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "2",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031280.1.5",
          "form": "저는 완전 문과 스타일이라...",
          "original_form": "저는 완전 문과 스타일이라...",
          "speaker_id": "2",
          "time": "20210904 20:02"
        },
        {
          "id": "MDRW2100031280.1.6",
          "form": "이번에 수리 문과 이과 통합 시험이죠?",
          "original_form": "이번에 수리 문과 이과 통합 시험이죠?",
          "speaker_id": "1",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100031280.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100031280.1.8",
          "form": "수1 수2는 같이 치고",
          "original_form": "수1 수2는 같이 치고",
          "speaker_id": "2",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100031280.1.9",
          "form": "선택 과목은 확통/미적/기하 나눠져 있어요",
          "original_form": "선택 과목은 확통/미적/기하 나눠져 있어요",
          "speaker_id": "2",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100031280.1.10",
          "form": "문과는 대부분 확통 선택하긴 하는데 필수는 아니에요",
          "original_form": "문과는 대부분 확통 선택하긴 하는데 필수는 아니에요",
          "speaker_id": "2",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100031280.1.11",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210904 20:07"
        },
        {
          "id": "MDRW2100031280.1.12",
          "form": "경제학과 가면 미적도 도움 많이될텐데",
          "original_form": "경제학과 가면 미적도 도움 많이될텐데",
          "speaker_id": "1",
          "time": "20210904 20:07"
        },
        {
          "id": "MDRW2100031280.1.13",
          "form": "이과생들이랑 경쟁이 어려워서",
          "original_form": "이과생들이랑 경쟁이 어려워서",
          "speaker_id": "1",
          "time": "20210904 20:07"
        },
        {
          "id": "MDRW2100031280.1.14",
          "form": "수2에서 다항함수 미적분까지 배우고",
          "original_form": "수2에서 다항함수 미적분까지 배우고",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031280.1.15",
          "form": "문과생들이 확실히 저 수능칠때보다 어려워졌네요",
          "original_form": "문과생들이 확실히 저 수능칠때보다 어려워졌네요",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031280.1.16",
          "form": "미적에선 초월함수 배우는 걸로 알고 있어요",
          "original_form": "미적에선 초월함수 배우는 걸로 알고 있어요",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031280.1.17",
          "form": "그렇죠... 아무래도 이과보다 많이 밀리죠",
          "original_form": "그렇죠... 아무래도 이과보다 많이 밀리죠",
          "speaker_id": "2",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031280.1.18",
          "form": "어느 전공으로 진학 생각중인가요?",
          "original_form": "어느 전공으로 진학 생각중인가요?",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031280.1.19",
          "form": "인문계열로 생각중이라",
          "original_form": "인문계열로 생각중이라",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031280.1.20",
          "form": "철학과 생각했다가 종교학과란 곳이 있다는 거 알고 관심 가지게 됐어요",
          "original_form": "철학과 생각했다가 종교학과란 곳이 있다는 거 알고 관심 가지게 됐어요",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031280.1.21",
          "form": "그렇구나 잘 생각해보고 결정하세요",
          "original_form": "그렇구나 잘 생각해보고 결정하세요",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100031280.1.22",
          "form": "좋은 결과 있기를 바랍니다",
          "original_form": "좋은 결과 있기를 바랍니다",
          "speaker_id": "1",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100031280.1.23",
          "form": "앗 감사합니다!",
          "original_form": "앗 감사합니다!",
          "speaker_id": "2",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100031280.1.24",
          "form": "남은 주말 잘 보내시고 힘내세요",
          "original_form": "남은 주말 잘 보내시고 힘내세요",
          "speaker_id": "1",
          "time": "20210904 21:04"
        },
