      "utterance": [
        {
          "id": "MDRW2100024604.1.1",
          "form": "빵이면 다 처먹이어서...ㅋㅋㅋㅋ",
          "original_form": "빵이면 다 처먹이어서...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100024604.1.2",
          "form": "옼ㅋㅋㅋㅋ 저는 사워도우류 좋아해요! 특히치아바타!!",
          "original_form": "옼ㅋㅋㅋㅋ 저는 사워도우류 좋아해요! 특히치아바타!!",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100024604.1.3",
          "form": "아아앙 저는 요즘 크로플에빠져서",
          "original_form": "아아앙 저는 요즘 크로플에빠져서",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024604.1.4",
          "form": "크로플!! 저두 생지사서 만들어먹곤해요~ㅋㅋㅋ",
          "original_form": "크로플!! 저두 생지사서 만들어먹곤해요~ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100024604.1.5",
          "form": "우아   만들어서!!!",
          "original_form": "우아   만들어서!!!",
          "speaker_id": "2",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100024604.1.6",
          "form": "마쟈여 ㅋㅋㅋㅋㅋ 만들어먹는게 훨 맛있다는~",
          "original_form": "마쟈여 ㅋㅋㅋㅋㅋ 만들어먹는게 훨 맛있다는~",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100024604.1.7",
          "form": "멋져여 ...우아..",
          "original_form": "멋져여 ...우아..",
          "speaker_id": "2",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100024604.1.8",
          "form": "브라운치즈 갈갈해서 뿌려먹으면 맛집안부러워요ㅋㅋㅋ헷",
          "original_form": "브라운치즈 갈갈해서 뿌려먹으면 맛집안부러워요ㅋㅋㅋ헷",
          "speaker_id": "1",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100024604.1.9",
          "form": "헐....와",
          "original_form": "헐....와",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100024604.1.10",
          "form": "맛있겠다 진짜",
          "original_form": "맛있겠다 진짜",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100024604.1.11",
          "form": "맛나요 증말! 해드셔보셔유 ~ㅋㅋㅋ",
          "original_form": "맛나요 증말! 해드셔보셔유 ~ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100024604.1.12",
          "form": "하...어찌하나여! ㅋㅋㅋㅋ",
          "original_form": "하...어찌하나여! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:38"
        },
        {
