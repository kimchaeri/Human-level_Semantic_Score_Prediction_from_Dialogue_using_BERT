        },
        {
          "id": "MDRW2100020094.1.24",
          "form": "??????!",
          "original_form": "??????!",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020094.1.25",
          "form": "바나나빵 어떤거?",
          "original_form": "바나나빵 어떤거?",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020094.1.26",
          "form": "그걸 바나나빵이라해야하나",
          "original_form": "그걸 바나나빵이라해야하나",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020094.1.27",
          "form": "봉지에든거 마트에 빵코너에 ㅋㅋㅋ",
          "original_form": "봉지에든거 마트에 빵코너에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020094.1.28",
          "form": "바나나모양으로 설탕묻어있는???",
          "original_form": "바나나모양으로 설탕묻어있는???",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020094.1.29",
          "form": "아 천재인가? 개떡같이 말해도 찰떡같이 알아듣네",
          "original_form": "아 천재인가? 개떡같이 말해도 찰떡같이 알아듣네",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020094.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ나그거 좋아했는데",
