      "utterance": [
        {
          "id": "MDRW2100020094.1.1",
          "form": "나 내일 간식 배송온다",
          "original_form": "나 내일 간식 배송온다",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020094.1.2",
          "form": "앗 어제 샀다고 한거?",
          "original_form": "앗 어제 샀다고 한거?",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020094.1.3",
          "form": "난 오늘 로아커 왓음",
          "original_form": "난 오늘 로아커 왓음",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020094.1.4",
          "form": "최고최고 맛있는데",
          "original_form": "최고최고 맛있는데",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020094.1.5",
          "form": "엄마는 모래같대",
          "original_form": "엄마는 모래같대",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020094.1.6",
          "form": "엥 왜때문에 ㅋㅋㅋ",
          "original_form": "엥 왜때문에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020094.1.7",
          "form": "어디가모래여",
          "original_form": "어디가모래여",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020094.1.8",
          "form": "씹었을때 입안이 모래담은것같다는ㅋㅋㅋ",
          "original_form": "씹었을때 입안이 모래담은것같다는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020094.1.9",
          "form": "우리엄마는 하늘색 좋아하더라",
          "original_form": "우리엄마는 하늘색 좋아하더라",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020094.1.10",
          "form": "음음 난 봉다리는 아빠한테 뺏김",
          "original_form": "음음 난 봉다리는 아빠한테 뺏김",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020094.1.11",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020094.1.12",
          "form": "헐 그럼 자네에게 남은건 뭐란말인가",
          "original_form": "헐 그럼 자네에게 남은건 뭐란말인가",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020094.1.13",
          "form": "넙따리 두개",
          "original_form": "넙따리 두개",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020094.1.14",
          "form": "아빠방에서 몰래 삐먹어야지",
          "original_form": "아빠방에서 몰래 삐먹어야지",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020094.1.15",
          "form": "헐 어찌그런...",
          "original_form": "헐 어찌그런...",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020094.1.16",
          "form": "우리아빠방 완전 과자마트ㅋㅋㅋ",
          "original_form": "우리아빠방 완전 과자마트ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020094.1.17",
          "form": "부친께서 다과를 즐기시오?",
          "original_form": "부친께서 다과를 즐기시오?",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020094.1.18",
          "form": "내가 좋아하는 종류는 별로 없지만..",
          "original_form": "내가 좋아하는 종류는 별로 없지만..",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020094.1.19",
          "form": "예 그렇사옵니다",
          "original_form": "예 그렇사옵니다",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020094.1.20",
          "form": "요즘 사극봤잖아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "요즘 사극봤잖아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020094.1.21",
          "form": "ㅋㅋ사극뭐봄",
          "original_form": "ㅋㅋ사극뭐봄",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020094.1.22",
          "form": "너그거알아?",
          "original_form": "너그거알아?",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020094.1.23",
