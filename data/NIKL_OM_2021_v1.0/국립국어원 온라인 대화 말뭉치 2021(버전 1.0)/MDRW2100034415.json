      "utterance": [
        {
          "id": "MDRW2100034415.1.1",
          "form": "저는 결심했슴다",
          "original_form": "저는 결심했슴다",
          "speaker_id": "1",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100034415.1.2",
          "form": "내년 초에 조개구이를 먹으러 갈 것이에요...",
          "original_form": "내년 초에 조개구이를 먹으러 갈 것이에요...",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100034415.1.3",
          "form": "바다를 보면서....",
          "original_form": "바다를 보면서....",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100034415.1.4",
          "form": "저는 부산에 사는데",
          "original_form": "저는 부산에 사는데",
          "speaker_id": "2",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100034415.1.5",
          "form": "조개구이를 날을 잡아서 먹어야한다는 사실이 충격입니다",
          "original_form": "조개구이를 날을 잡아서 먹어야한다는 사실이 충격입니다",
          "speaker_id": "2",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100034415.1.6",
          "form": "너무 편협하게 살았나봐요",
          "original_form": "너무 편협하게 살았나봐요",
          "speaker_id": "2",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100034415.1.7",
          "form": "이럴수가 부산분이시군요",
          "original_form": "이럴수가 부산분이시군요",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100034415.1.8",
          "form": "저는 서울인지라....",
          "original_form": "저는 서울인지라....",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100034415.1.9",
          "form": "조개구이=여행 입니다....",
          "original_form": "조개구이=여행 입니다....",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100034415.1.10",
          "form": "약간 뮤지컬=여행 같은 기분이네요 ㅋㅋㅋㅋ",
          "original_form": "약간 뮤지컬=여행 같은 기분이네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100034415.1.11",
          "form": "조개구이 먹으러 인천이나 강릉 가시나요?",
          "original_form": "조개구이 먹으러 인천이나 강릉 가시나요?",
          "speaker_id": "2",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100034415.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 앜ㅋㅋㅋ 맞아요 딱 그거네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 앜ㅋㅋㅋ 맞아요 딱 그거네요",
          "speaker_id": "1",
          "time": "20210828 20:08"
        },
        {
          "id": "MDRW2100034415.1.13",
          "form": "헐 정답",
          "original_form": "헐 정답",
          "speaker_id": "1",
          "time": "20210828 20:08"
        },
        {
          "id": "MDRW2100034415.1.14",
          "form": "당신 정말 씨씨티비를 단거야...?",
          "original_form": "당신 정말 씨씨티비를 단거야...?",
          "speaker_id": "1",
          "time": "20210828 20:08"
        },
        {
          "id": "MDRW2100034415.1.15",
          "form": "프로 사찰러를 꿈꾸며 노력 중입니다? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "프로 사찰러를 꿈꾸며 노력 중입니다? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:16"
        },
        {
          "id": "MDRW2100034415.1.16",
          "form": "바다 보러 가면 뭐하시나유?",
          "original_form": "바다 보러 가면 뭐하시나유?",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034415.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 멋져욧",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 멋져욧",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034415.1.18",
          "form": "저는 보통 밤에 이어폰꼽고 한 서너시간 멍때려요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 보통 밤에 이어폰꼽고 한 서너시간 멍때려요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034415.1.19",
          "form": "그래서 너무 겨울에가면 얼어죽습니다",
          "original_form": "그래서 너무 겨울에가면 얼어죽습니다",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034415.1.20",
          "form": "name2님은 뮤지컬여행을 가시는건가유",
          "original_form": "&name2&님은 뮤지컬여행을 가시는건가유",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034415.1.21",
          "form": "밤바다 참 운치있고 낭만 가득하지만",
          "original_form": "밤바다 참 운치있고 낭만 가득하지만",
          "speaker_id": "2",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034415.1.22",
          "form": "한순간에 큰 일 납니다ㅠ",
          "original_form": "한순간에 큰 일 납니다ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034415.1.23",
          "form": "항상 조심하세유ㅠㅠ",
          "original_form": "항상 조심하세유ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034415.1.24",
          "form": "부산에도 가끔 뮤지컬 하러 내려 오시던데",
          "original_form": "부산에도 가끔 뮤지컬 하러 내려 오시던데",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034415.1.25",
          "form": "진짜 보고싶은 배우분들은 잘 안내려오셔서",
          "original_form": "진짜 보고싶은 배우분들은 잘 안내려오셔서",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034415.1.26",
          "form": "그럴 때는 친구들이랑 날 잡아서 올라가요 ㅋㅋㅋㅋ",
          "original_form": "그럴 때는 친구들이랑 날 잡아서 올라가요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034415.1.27",
          "form": "맞아요 따흐흑 위험할때도 있지요",
          "original_form": "맞아요 따흐흑 위험할때도 있지요",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100034415.1.28",
          "form": "뮤지컬 여행 너무좋네요 ㅋㅋㅋㅋ 목적확실",
          "original_form": "뮤지컬 여행 너무좋네요 ㅋㅋㅋㅋ 목적확실",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100034415.1.29",
          "form": "그 여행 못 간지 2년 더 넘었네요ㅠㅠ ㅋㅋㅋ",
          "original_form": "그 여행 못 간지 2년 더 넘었네요ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100034415.1.30",
          "form": "조개구이여행도 귀여워요 ㅋㅋㅋㅋㅋ",
          "original_form": "조개구이여행도 귀여워요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100034415.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저두요ㅜㅜ 여행안간지 어언 2년... 이제 좀이 쑤십니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저두요ㅜㅜ 여행안간지 어언 2년... 이제 좀이 쑤십니다",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034415.1.32",
          "form": "전 여행가면 대하구워먹을거에요 흑흑",
          "original_form": "전 여행가면 대하구워먹을거에요 흑흑",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034415.1.33",
          "form": "대하소금구이 진짜 맛있죠ㅠㅠ",
          "original_form": "대하소금구이 진짜 맛있죠ㅠㅠ",
