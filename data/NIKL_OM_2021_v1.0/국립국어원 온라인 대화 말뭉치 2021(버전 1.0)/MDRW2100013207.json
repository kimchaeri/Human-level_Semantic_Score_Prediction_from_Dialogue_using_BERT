      "utterance": [
        {
          "id": "MDRW2100013207.1.1",
          "form": "네 집밥이오ㅎㅎ",
          "original_form": "네 집밥이오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013207.1.2",
          "form": "뭐드셨어요",
          "original_form": "뭐드셨어요",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013207.1.3",
          "form": "오 재택중이신가요?ㅋㅋ",
          "original_form": "오 재택중이신가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013207.1.4",
          "form": "저도 집밥이요ㅋㅋ",
          "original_form": "저도 집밥이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013207.1.5",
          "form": "아니요 오늘 휴무라서ㅋㅋ",
          "original_form": "아니요 오늘 휴무라서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013207.1.6",
          "form": "뭐 해드셨나요",
          "original_form": "뭐 해드셨나요",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013207.1.7",
          "form": "아ㅋㅋ 휴무셨군요",
          "original_form": "아ㅋㅋ 휴무셨군요",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013207.1.8",
          "form": "어묵국에 밑반찬이랑 먹었어요",
          "original_form": "어묵국에 밑반찬이랑 먹었어요",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013207.1.9",
          "form": "전 미역국과 밑반찬 ㅎㅎ",
          "original_form": "전 미역국과 밑반찬 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013207.1.10",
          "form": "오늘 생일이세요?",
          "original_form": "오늘 생일이세요?",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013207.1.11",
          "form": "아뇨 몇일 전에 와이프가 ㅎㅎ",
          "original_form": "아뇨 몇일 전에 와이프가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013207.1.12",
          "form": "아항 결혼후 첫 생일\r\n잘 챙겨드렸나요?",
          "original_form": "아항 결혼후 첫 생일\r\n잘 챙겨드렸나요?",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013207.1.13",
          "form": "ㅋㅋ현금으로 승부했어요 이번에는",
          "original_form": "ㅋㅋ현금으로 승부했어요 이번에는",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013207.1.14",
          "form": "돈이 짱이죠 ㅋㅋㅋㅋㅋ",
          "original_form": "돈이 짱이죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013207.1.15",
          "form": "작고 반짝이는것도 좋구요ㅋㅋ",
          "original_form": "작고 반짝이는것도 좋구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013207.1.16",
          "form": "저녁은 뭐드시나요",
          "original_form": "저녁은 뭐드시나요",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013207.1.17",
          "form": "오늘 저녁도 집밥인데\r\n뭘 해야될지 고민중요",
          "original_form": "오늘 저녁도 집밥인데\r\n뭘 해야될지 고민중요",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013207.1.18",
          "form": "name2 요리도 하시나요?",
          "original_form": "&name2& 요리도 하시나요?",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013207.1.19",
          "form": "저 계란말이 계란찜?",
          "original_form": "저 계란말이 계란찜?",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013207.1.20",
          "form": "오! 그 정도면 훌륭하시네요",
          "original_form": "오! 그 정도면 훌륭하시네요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013207.1.21",
          "form": "ㅋㅋ name1은요?",
          "original_form": "ㅋㅋ &name1&은요?",
