      "utterance": [
        {
          "id": "MDRW2100020545.1.1",
          "form": "분식 사랑하죠",
          "original_form": "분식 사랑하죠",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020545.1.2",
          "form": "뭘 가장 좋아하시나요",
          "original_form": "뭘 가장 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020545.1.3",
          "form": "김밥튀김",
          "original_form": "김밥튀김",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020545.1.4",
          "form": "짱맛있는데 드물어요",
          "original_form": "짱맛있는데 드물어요",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020545.1.5",
          "form": "순대튀김도",
          "original_form": "순대튀김도",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020545.1.6",
          "form": "아 그걸아시다니",
          "original_form": "아 그걸아시다니",
          "speaker_id": "1",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020545.1.7",
          "form": "맛있죠",
          "original_form": "맛있죠",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020545.1.8",
          "form": "김밥이랑 순대 튀기는 곳 잘 없잖아요",
          "original_form": "김밥이랑 순대 튀기는 곳 잘 없잖아요",
          "speaker_id": "1",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020545.1.9",
          "form": "가끔 튀기는 곳 가면 진짜 너무 좋아서",
          "original_form": "가끔 튀기는 곳 가면 진짜 너무 좋아서",
          "speaker_id": "1",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.10",
          "form": "맞아요 순대튀김은 그래서 집에서 한답니다 이제",
          "original_form": "맞아요 순대튀김은 그래서 집에서 한답니다 이제",
          "speaker_id": "2",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.11",
          "form": "여러개 사와요",
          "original_form": "여러개 사와요",
          "speaker_id": "1",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.12",
          "form": "집에서요?",
          "original_form": "집에서요?",
          "speaker_id": "1",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.13",
          "form": "대박적",
          "original_form": "대박적",
          "speaker_id": "1",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.14",
          "form": "네 순대 사서 걍 튀겨버려요",
          "original_form": "네 순대 사서 걍 튀겨버려요",
          "speaker_id": "2",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.15",
          "form": "주변에 없으니 이게 자급자족이 되더라구요",
          "original_form": "주변에 없으니 이게 자급자족이 되더라구요",
          "speaker_id": "2",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.16",
          "form": "왜 난 그 생각을 한번도 하지 못했는가",
          "original_form": "왜 난 그 생각을 한번도 하지 못했는가",
          "speaker_id": "1",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.17",
          "form": "후후 name1님은 어떤걸 가장 좋아하시나요",
          "original_form": "후후 &name1&님은 어떤걸 가장 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.18",
          "form": "전 김밥 좋아해요",
          "original_form": "전 김밥 좋아해요",
          "speaker_id": "1",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020545.1.19",
          "form": "아 저두요 ㅠㅠ",
          "original_form": "아 저두요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.20",
          "form": "제가 유일하게 먹는 해물",
          "original_form": "제가 유일하게 먹는 해물",
          "speaker_id": "2",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.21",
          "form": "김은 못잃어요",
          "original_form": "김은 못잃어요",
          "speaker_id": "2",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.22",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.23",
          "form": "김밥에 참기름을 꼭 발라야합니다",
          "original_form": "김밥에 참기름을 꼭 발라야합니다",
          "speaker_id": "2",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.25",
          "form": "차르르",
          "original_form": "차르르",
          "speaker_id": "2",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.26",
          "form": "그래야 꼬수워",
          "original_form": "그래야 꼬수워",
          "speaker_id": "1",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.27",
          "form": "혹시 깨 드시나요",
          "original_form": "혹시 깨 드시나요",
          "speaker_id": "2",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.28",
          "form": "혹시 김은 어디꺼 좋아하세요",
          "original_form": "혹시 김은 어디꺼 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.29",
          "form": "네 깨 먹어요",
          "original_form": "네 깨 먹어요",
          "speaker_id": "1",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100020545.1.30",
