      "utterance": [
        {
          "id": "MDRW2100036194.1.1",
          "form": "최근에 산거 뭐 있으세여?",
          "original_form": "최근에 산거 뭐 있으세여?",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036194.1.2",
          "form": "저는 전공책이요 name1님은요?",
          "original_form": "저는 전공책이요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036194.1.3",
          "form": "저는 가방이요!",
          "original_form": "저는 가방이요!",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100036194.1.4",
          "form": "어떤 가방이요?",
          "original_form": "어떤 가방이요?",
          "speaker_id": "2",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036194.1.5",
          "form": "회사다닐때 들고다닐 가방 하나 샀어요!!!",
          "original_form": "회사다닐때 들고다닐 가방 하나 샀어요!!!",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100036194.1.6",
          "form": "가죽인가요 아니면 에코백인가요???",
          "original_form": "가죽인가요 아니면 에코백인가요???",
          "speaker_id": "2",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036194.1.7",
          "form": "가죽이라고 해야하나? 가볍게 메고다닐 가방으로 샀어요ㅎㅎ",
          "original_form": "가죽이라고 해야하나? 가볍게 메고다닐 가방으로 샀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100036194.1.8",
          "form": "전공책 어떤거 사셨어요?",
          "original_form": "전공책 어떤거 사셨어요?",
          "speaker_id": "1",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100036194.1.9",
          "form": "저는 병리학이요",
          "original_form": "저는 병리학이요",
          "speaker_id": "2",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100036194.1.10",
          "form": "name1님은 사고싶으신 물건이 있으신가요?",
          "original_form": "&name1&님은 사고싶으신 물건이 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100036194.1.11",
          "form": "저 이쁜 악세사리 사고싶어요",
          "original_form": "저 이쁜 악세사리 사고싶어요",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100036194.1.12",
          "form": "여즘 반지에 빠져서ㅎㅎ",
          "original_form": "여즘 반지에 빠져서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100036194.1.13",
          "form": "얼마짜리 반지인가요?",
          "original_form": "얼마짜리 반지인가요?",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100036194.1.14",
          "form": "브랜드 반지라 그런지 비싸네요ㅠㅠ",
          "original_form": "브랜드 반지라 그런지 비싸네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100036194.1.15",
          "form": "어떤 브랜드껀지 물어봐도 될까요?",
          "original_form": "어떤 브랜드껀지 물어봐도 될까요?",
          "speaker_id": "2",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100036194.1.16",
          "form": "구찌요!",
          "original_form": "구찌요!",
          "speaker_id": "1",
          "time": "20210901 08:51"
        },
        {
          "id": "MDRW2100036194.1.17",
          "form": "구찌면 많이 비싼가요??",
          "original_form": "구찌면 많이 비싼가요??",
