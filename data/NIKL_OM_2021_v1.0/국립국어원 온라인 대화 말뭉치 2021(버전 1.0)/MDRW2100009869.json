      "utterance": [
        {
          "id": "MDRW2100009869.1.1",
          "form": "직장인 이신가요",
          "original_form": "직장인 이신가요",
          "speaker_id": "2",
          "time": "20210823 11:07"
        },
        {
          "id": "MDRW2100009869.1.2",
          "form": "네 저는 회사다녀요",
          "original_form": "네 저는 회사다녀요",
          "speaker_id": "1",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009869.1.3",
          "form": "직장 다니세요?",
          "original_form": "직장 다니세요?",
          "speaker_id": "1",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100009869.1.4",
          "form": "저는 쉬고있어요",
          "original_form": "저는 쉬고있어요",
          "speaker_id": "2",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009869.1.5",
          "form": "직장인 부럽네요 ㅋㅋ",
          "original_form": "직장인 부럽네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009869.1.6",
          "form": "코로나 이후 계약직 일이 없어져서요",
          "original_form": "코로나 이후 계약직 일이 없어져서요",
          "speaker_id": "2",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009869.1.7",
          "form": "요새 새로운 분야를 공부해야하나",
          "original_form": "요새 새로운 분야를 공부해야하나",
          "speaker_id": "2",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100009869.1.8",
          "form": "고민이 많아요 ㅎㅎ",
          "original_form": "고민이 많아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009869.1.9",
          "form": "아무래도 요즘은 채용폭이 좁아져서",
          "original_form": "아무래도 요즘은 채용폭이 좁아져서",
          "speaker_id": "1",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009869.1.10",
          "form": "어떤공부해보려고 생각중이신데요?",
          "original_form": "어떤공부해보려고 생각중이신데요?",
          "speaker_id": "1",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009869.1.11",
          "form": "현재 실업급여 받고",
          "original_form": "현재 실업급여 받고",
          "speaker_id": "2",
          "time": "20210823 11:17"
        },
        {
          "id": "MDRW2100009869.1.12",
          "form": "고용센터 지원으로 훈련과정들으려고요",
          "original_form": "고용센터 지원으로 훈련과정들으려고요",
          "speaker_id": "2",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009869.1.13",
          "form": "웹디자인과 회계전산 중 고민이에요",
          "original_form": "웹디자인과 회계전산 중 고민이에요",
          "speaker_id": "2",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009869.1.14",
          "form": "기존 전공이랑 전혀 관련없나요?",
          "original_form": "기존 전공이랑 전혀 관련없나요?",
          "speaker_id": "1",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009869.1.15",
          "form": "네 전혀 상관없어요",
          "original_form": "네 전혀 상관없어요",
          "speaker_id": "2",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009869.1.16",
          "form": "새로 시작하려면 어렵겠네요",
          "original_form": "새로 시작하려면 어렵겠네요",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009869.1.17",
          "form": "회계전산은 뭐에요?",
          "original_form": "회계전산은 뭐에요?",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009869.1.18",
          "form": "엑셀인가?",
          "original_form": "엑셀인가?",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009869.1.19",
          "form": "음 경리가 하는 일을 배우는거요",
          "original_form": "음 경리가 하는 일을 배우는거요",
          "speaker_id": "2",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009869.1.20",
          "form": "재무?랄까 세금계산하고",
          "original_form": "재무?랄까 세금계산하고",
          "speaker_id": "2",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009869.1.21",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009869.1.22",
          "form": "잘 되실거에요",
          "original_form": "잘 되실거에요",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009869.1.23",
          "form": "힘 내세요",
          "original_form": "힘 내세요",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009869.1.24",
          "form": "제 와이프도 구직중인데",
          "original_form": "제 와이프도 구직중인데",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009869.1.25",
          "form": "다들 원하는 직장 들어가면 좋겠네요",
          "original_form": "다들 원하는 직장 들어가면 좋겠네요",
          "speaker_id": "1",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009869.1.26",
          "form": "아니면 간호조무사 공부를 해볼까 생각도하고있어요",
          "original_form": "아니면 간호조무사 공부를 해볼까 생각도하고있어요",
          "speaker_id": "2",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009869.1.27",
          "form": "아 이것저것 많이 생각하고 계시는구나",
          "original_form": "아 이것저것 많이 생각하고 계시는구나",
          "speaker_id": "1",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009869.1.28",
          "form": "훈련과정중에서 어떤걸골라야할까 참 고민이 ㅋㅋ",
          "original_form": "훈련과정중에서 어떤걸골라야할까 참 고민이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:31"
        }
      ]
    }
