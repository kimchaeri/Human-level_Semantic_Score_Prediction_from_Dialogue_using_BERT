      "utterance": [
        {
          "id": "MDRW2100044865.1.1",
          "form": "없어서 못먹지..",
          "original_form": "없어서 못먹지..",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044865.1.2",
          "form": "아 나도 딱복? 물복?",
          "original_form": "아 나도 딱복? 물복?",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044865.1.3",
          "form": "모복..",
          "original_form": "모복..",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044865.1.4",
          "form": "(모든 복숭아라는 뜻)",
          "original_form": "(모든 복숭아라는 뜻)",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044865.1.5",
          "form": "나도 그렇긴 한데 그래도 딱복이 좋아 ㅋㅋ",
          "original_form": "나도 그렇긴 한데 그래도 딱복이 좋아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044865.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나는 진짜 둘 다 고를 수 없게 다 좋아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나는 진짜 둘 다 고를 수 없게 다 좋아",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044865.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ나는 아삭한게 뭔가 좋아 물복도 좋긴한데 손이 엉망되어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ나는 아삭한게 뭔가 좋아 물복도 좋긴한데 손이 엉망되어서",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044865.1.8",
          "form": "아 그렇긴하지ㅋㅋㅋ 손 끈적거리지",
          "original_form": "아 그렇긴하지ㅋㅋㅋ 손 끈적거리지",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044865.1.9",
          "form": "맞아맞아 딱복은 안그래서 좋아 ㅋㅋ",
          "original_form": "맞아맞아 딱복은 안그래서 좋아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044865.1.10",
          "form": "근데 물복은 물복만의 매력이 또 있는거같어ㅋㅋㅋ 부드러워서",
          "original_form": "근데 물복은 물복만의 매력이 또 있는거같어ㅋㅋㅋ 부드러워서",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044865.1.11",
          "form": "손으로 껍질 밀면 밀리는거 알지 ㅋㅋ",
          "original_form": "손으로 껍질 밀면 밀리는거 알지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044865.1.12",
          "form": "알지알지ㅋㅋㅋ",
          "original_form": "알지알지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044865.1.13",
          "form": "아 복숭아 먹고싶다 올해는 안사먹었어ㅠ",
          "original_form": "아 복숭아 먹고싶다 올해는 안사먹었어ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044865.1.14",
          "form": "나도ㅠㅠ 올해는 너무 비싸더라ㅠㅜㅠ",
          "original_form": "나도ㅠㅠ 올해는 너무 비싸더라ㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044865.1.15",
          "form": "올해는 수박도 엄청 비쌌어",
          "original_form": "올해는 수박도 엄청 비쌌어",
          "speaker_id": "1",
          "time": "20210908 00:07"
        }
      ]
