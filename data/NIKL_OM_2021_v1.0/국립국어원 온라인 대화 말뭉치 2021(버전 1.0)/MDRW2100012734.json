      "utterance": [
        {
          "id": "MDRW2100012734.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 00:24"
        },
        {
          "id": "MDRW2100012734.1.2",
          "form": "중식 좋아하세요?",
          "original_form": "중식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012734.1.3",
          "form": "양장피 좋아행ᆢ",
          "original_form": "양장피 좋아행ᆢ",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012734.1.4",
          "form": "좋아해요 톡쏘는 맛 좋아해서",
          "original_form": "좋아해요 톡쏘는 맛 좋아해서",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012734.1.5",
          "form": "name1은 뭘 좋아하세요?",
          "original_form": "&name1&은 뭘 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012734.1.6",
          "form": "양장피 맛있나요?",
          "original_form": "양장피 맛있나요?",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012734.1.7",
          "form": "저 한번도 안먹어봤어요ㅋㅋㅋ",
          "original_form": "저 한번도 안먹어봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012734.1.8",
          "form": "냉채족발은 드셔보셨어요?",
          "original_form": "냉채족발은 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012734.1.9",
          "form": "톡 쏘는 겨자맛이에요. 호불호 갈릴수도ㅎ",
          "original_form": "톡 쏘는 겨자맛이에요. 호불호 갈릴수도ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012734.1.10",
          "form": "냉채족발도 안먹어봤네요ㄷㄷ",
          "original_form": "냉채족발도 안먹어봤네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012734.1.11",
          "form": "어떤맛일진 알거같아요ㅎㅎㅎ",
          "original_form": "어떤맛일진 알거같아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012734.1.12",
          "form": "그럼 중식중 뭘 좋아하세요?",
          "original_form": "그럼 중식중 뭘 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012734.1.13",
          "form": "전 짬뽕이요ㅋㅋㅋㅋ",
          "original_form": "전 짬뽕이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012734.1.14",
          "form": "님은 짜장면 vs 짬뽕 어떠신가요?",
          "original_form": "님은 짜장면 vs 짬뽕 어떠신가요?",
          "speaker_id": "1",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012734.1.15",
          "form": "짬뽕! 그중에서도 짬뽕밥이요",
          "original_form": "짬뽕! 그중에서도 짬뽕밥이요",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012734.1.16",
          "form": "근데 혼자 있으면 주로 탕수육 소자 시켜요ㅎ",
          "original_form": "근데 혼자 있으면 주로 탕수육 소자 시켜요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012734.1.17",
          "form": "짬뽕밥에는",
          "original_form": "짬뽕밥에는",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012734.1.18",
          "form": "면이 없지않나요?!",
          "original_form": "면이 없지않나요?!",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012734.1.19",
          "form": "네 저는 면보다 밥파라서요ㅎ",
          "original_form": "네 저는 면보다 밥파라서요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012734.1.20",
          "form": "교동짬뽕은 연두부 같이 넣어주는데 그게 너무 좋아요",
          "original_form": "교동짬뽕은 연두부 같이 넣어주는데 그게 너무 좋아요",
          "speaker_id": "2",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012734.1.21",
          "form": "와우 저랑다르네요 저는 면파인데",
          "original_form": "와우 저랑다르네요 저는 면파인데",
          "speaker_id": "1",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100012734.1.22",
          "form": "연두부를 같이넣어주나요.",
          "original_form": "연두부를 같이넣어주나요.",
          "speaker_id": "1",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100012734.1.23",
          "form": "네 제가 가는 지점만 그런지는 모르겠는데",
          "original_form": "네 제가 가는 지점만 그런지는 모르겠는데",
          "speaker_id": "2",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100012734.1.24",
          "form": "정말 잘 어울려요",
          "original_form": "정말 잘 어울려요",
          "speaker_id": "2",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100012734.1.25",
          "form": "생각해보니",
          "original_form": "생각해보니",
          "speaker_id": "1",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012734.1.26",
          "form": "저희동네 교동짬뽕은",
          "original_form": "저희동네 교동짬뽕은",
          "speaker_id": "1",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012734.1.27",
          "form": "안넣어주는것같네요ㅠ",
          "original_form": "안넣어주는것같네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012734.1.28",
          "form": "짬뽕 말고 짬뽕밥도요?",
          "original_form": "짬뽕 말고 짬뽕밥도요?",
          "speaker_id": "2",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100012734.1.29",
          "form": "여기도 면요리엔 안넣어줘요",
          "original_form": "여기도 면요리엔 안넣어줘요",
          "speaker_id": "2",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100012734.1.30",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 01:37"
        },
        {
          "id": "MDRW2100012734.1.31",
          "form": "제가 짬뽕밥은",
          "original_form": "제가 짬뽕밥은",
          "speaker_id": "1",
          "time": "20210825 01:37"
        },
        {
          "id": "MDRW2100012734.1.32",
          "form": "안먹어봤군요ㅋㅋㅋㅋ",
          "original_form": "안먹어봤군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
