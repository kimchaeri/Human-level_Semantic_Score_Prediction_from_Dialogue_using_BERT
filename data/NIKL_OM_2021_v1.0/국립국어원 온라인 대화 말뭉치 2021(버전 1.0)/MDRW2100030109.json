      "utterance": [
        {
          "id": "MDRW2100030109.1.1",
          "form": "종교있으신가요...?",
          "original_form": "종교있으신가요...?",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030109.1.2",
          "form": "아뇨 전 없어요!",
          "original_form": "아뇨 전 없어요!",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030109.1.3",
          "form": "이 주제 여러 번 나왔는데",
          "original_form": "이 주제 여러 번 나왔는데",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030109.1.4",
          "form": "지금까지 종교 있는 분을 한 분도 못 만났어요ㅋㅋㅋㅋ",
          "original_form": "지금까지 종교 있는 분을 한 분도 못 만났어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030109.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜요?",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030109.1.6",
          "form": "넘 웃긴 우연이네욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "넘 웃긴 우연이네욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030109.1.7",
          "form": "저도 없어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 없어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030109.1.8",
          "form": "그래서 종교관련 음식 얘기를 많이 했죠ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 종교관련 음식 얘기를 많이 했죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.9",
          "form": "앗 종교음식!",
          "original_form": "앗 종교음식!",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.10",
          "form": "복음자리 잼이랑 이삭토스트가 맛있죠",
          "original_form": "복음자리 잼이랑 이삭토스트가 맛있죠",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.11",
          "form": "템플스테이 밥 잘 준다더라 성당음식이 맛있다더라 이런 얘기요ㅋㅋㅋ",
          "original_form": "템플스테이 밥 잘 준다더라 성당음식이 맛있다더라 이런 얘기요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.12",
          "form": "앗...아앗",
          "original_form": "앗...아앗",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.13",
          "form": "아 그러고보니 이삭토스트도!",
          "original_form": "아 그러고보니 이삭토스트도!",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.14",
          "form": "전혀 핀트가 엇나간 얘길 했네요",
          "original_form": "전혀 핀트가 엇나간 얘길 했네요",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.16",
          "form": "네 이삭토스트도 원래 기독교분들이 많이 하셨잖아요",
          "original_form": "네 이삭토스트도 원래 기독교분들이 많이 하셨잖아요",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.17",
          "form": "ㅋㅋㅋㅋㅋ이것도 인정되겠죠?",
          "original_form": "ㅋㅋㅋㅋㅋ이것도 인정되겠죠?",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.18",
          "form": "전 이렇게 맛있는 음식 먹여주면",
          "original_form": "전 이렇게 맛있는 음식 먹여주면",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030109.1.19",
          "form": "신앙심이 샘솟을 것 같은데",
          "original_form": "신앙심이 샘솟을 것 같은데",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100030109.1.20",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100030109.1.21",
          "form": "어릴때 교회앞에서 컵떡볶이 나눠주셨는데",
          "original_form": "어릴때 교회앞에서 컵떡볶이 나눠주셨는데",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100030109.1.22",
          "form": "하나님의 은총 짱이라구 생각했지만",
          "original_form": "하나님의 은총 짱이라구 생각했지만",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100030109.1.23",
          "form": "안갔죠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "안갔죠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100030109.1.24",
          "form": "아 근데 템플스테이 밥 잘준대요??",
          "original_form": "아 근데 템플스테이 밥 잘준대요??",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100030109.1.25",
          "form": "약간 힐링코스랑 수양코스? 나눠져있는데가 있나봐요",
          "original_form": "약간 힐링코스랑 수양코스? 나눠져있는데가 있나봐요",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100030109.1.26",
          "form": "힐링코스로 가면 계속 떡 주고 귤주고",
          "original_form": "힐링코스로 가면 계속 떡 주고 귤주고",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100030109.1.27",
          "form": "열심히 먹여주신다고 하네요ㅋㅋㅋㅋ",
          "original_form": "열심히 먹여주신다고 하네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100030109.1.28",
          "form": "우와 대박....",
          "original_form": "우와 대박....",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100030109.1.29",
          "form": "안그래도 친구가 템플스테이 가고 싶다고 하도 그래서",
          "original_form": "안그래도 친구가 템플스테이 가고 싶다고 하도 그래서",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100030109.1.30",
          "form": "따라갈까 했거든요... 코로나땜에 힘들겠지만",
          "original_form": "따라갈까 했거든요... 코로나땜에 힘들겠지만",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100030109.1.31",
          "form": "근데 웃긴게 그 친구 기독교예요",
          "original_form": "근데 웃긴게 그 친구 기독교예요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100030109.1.32",
          "form": "채식이라 조금 소박하게 주시지 않을까싶은 편견을 확 깨준다고 하더라구요ㅋㅋㅋㅋ",
          "original_form": "채식이라 조금 소박하게 주시지 않을까싶은 편견을 확 깨준다고 하더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100030109.1.33",
          "form": "종교화합 좋죠!! 오예",
          "original_form": "종교화합 좋죠!! 오예",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100030109.1.34",
          "form": "우왕.... 너무너무 끌리네여",
          "original_form": "우왕.... 너무너무 끌리네여",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100030109.1.35",
          "form": "그쵸ㅋㅋㅋㅋㅋㅋㅋㅋ종교화합 짱!",
          "original_form": "그쵸ㅋㅋㅋㅋㅋㅋㅋㅋ종교화합 짱!",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100030109.1.36",
          "form": "아마 템플스테이 검색하면 유명한 리뷰글들이 좀 나올거에요ㅋㅋㅋㅋ",
          "original_form": "아마 템플스테이 검색하면 유명한 리뷰글들이 좀 나올거에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100030109.1.37",
          "form": "절밥도 뷔페식으로 잘 먹여준단 얘기도 있고",
          "original_form": "절밥도 뷔페식으로 잘 먹여준단 얘기도 있고",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100030109.1.38",
          "form": "어딜가도 힐링하고 오실 것 같아요ㅋㅋㅋ",
          "original_form": "어딜가도 힐링하고 오실 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100030109.1.39",
          "form": "앗 대박ㅋㅋㅋㅋㅋㅋㅋ리뷰도 있구나",
          "original_form": "앗 대박ㅋㅋㅋㅋㅋㅋㅋ리뷰도 있구나",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100030109.1.40",
          "form": "완전 짱이에요... 꼭 가야지",
          "original_form": "완전 짱이에요... 꼭 가야지",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100030109.1.41",
          "form": "사실 학생때 사찰같은 데 갈 일이 많았어서",
          "original_form": "사실 학생때 사찰같은 데 갈 일이 많았어서",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100030109.1.42",
          "form": "좀 로망있어요...",
          "original_form": "좀 로망있어요...",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100030109.1.43",
          "form": "동해 삼화사, 속리산 법주사, 해남 대흥사, 북한산 진관사, 양평 용문사 나덬이 불자고 템플스테이 자주 가는 편인데 경치와 힐링으로 갈만한 곳 추천 +치악산 구룡사 + 범어사 + 월정사",
          "original_form": "동해 삼화사, 속리산 법주사, 해남 대흥사, 북한산 진관사, 양평 용문사 나덬이 불자고 템플스테이 자주 가는 편인데 경치와 힐링으로 갈만한 곳 추천 +치악산 구룡사 + 범어사 + 월정사",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100030109.1.44",
          "form": "추천글에 이렇게 적혀있던데",
          "original_form": "추천글에 이렇게 적혀있던데",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100030109.1.45",
          "form": "종교 얘기의 범위겠죠ㅋㅋㅋㅋ",
          "original_form": "종교 얘기의 범위겠죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100030109.1.46",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100030109.1.47",
          "form": "경치좋은 절을 추천하더라구요! 여기랑 낙산사 정도",
          "original_form": "경치좋은 절을 추천하더라구요! 여기랑 낙산사 정도",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100030109.1.48",
          "form": "저도 코로나 좀 진정되면 꼭 가보고 싶어요!",
          "original_form": "저도 코로나 좀 진정되면 꼭 가보고 싶어요!",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100030109.1.49",
          "form": "저두요!! 가서 마주칠 수도 잇겠어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ재밌겠다",
          "original_form": "저두요!! 가서 마주칠 수도 잇겠어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ재밌겠다",
          "speaker_id": "2",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100030109.1.50",
          "form": "근데 정말 다 경치가 좋을 거 같은 곳에 있네요",
          "original_form": "근데 정말 다 경치가 좋을 거 같은 곳에 있네요",
          "speaker_id": "2",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100030109.1.51",
          "form": "짱이당... 템플스테이맛집도 있구나",
          "original_form": "짱이당... 템플스테이맛집도 있구나",
          "speaker_id": "2",
          "time": "20210903 21:55"
        },
        {
