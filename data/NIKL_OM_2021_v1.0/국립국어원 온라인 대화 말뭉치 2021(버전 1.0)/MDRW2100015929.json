        },
        {
          "id": "MDRW2100015929.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100015929.1.3",
          "form": "종교 있으신가요",
          "original_form": "종교 있으신가요",
          "speaker_id": "2",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100015929.1.4",
          "form": "저는 종교가 없어요 ㅎㅎ 종교있으세요?",
          "original_form": "저는 종교가 없어요 ㅎㅎ 종교있으세요?",
          "speaker_id": "1",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100015929.1.5",
          "form": "무교에용",
          "original_form": "무교에용",
          "speaker_id": "2",
          "time": "20210826 03:09"
        },
        {
          "id": "MDRW2100015929.1.6",
          "form": "그럼 주변에 종교있는 분 많으세요? ㅋㅋ",
          "original_form": "그럼 주변에 종교있는 분 많으세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100015929.1.7",
          "form": "아니요 절가본적있으세요?",
          "original_form": "아니요 절가본적있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100015929.1.8",
          "form": "절은 많이 가봤어요 ㅋㅋㅋ",
          "original_form": "절은 많이 가봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100015929.1.9",
          "form": "어릴때부터 많이 가봐서 친숙해요",
          "original_form": "어릴때부터 많이 가봐서 친숙해요",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100015929.1.10",
          "form": "절이름이 뭔가요?",
          "original_form": "절이름이 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100015929.1.11",
          "form": "아 특정 절에 많이 갔다는게 아니구요 ㅋㅋㅋㅋ",
          "original_form": "아 특정 절에 많이 갔다는게 아니구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100015929.1.12",
          "form": "다양한 여러 절에 많이 가봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "다양한 여러 절에 많이 가봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100015929.1.13",
          "form": "부모님이 산을 좋아하셔서 산에 많이 갔었는데",
          "original_form": "부모님이 산을 좋아하셔서 산에 많이 갔었는데",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100015929.1.14",
          "form": "산마다 보통 절이 있잖아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "산마다 보통 절이 있잖아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100015929.1.15",
          "form": "저는 봉은사 가봤어용",
          "original_form": "저는 봉은사 가봤어용",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100015929.1.16",
          "form": "관광으로 가보셨나요?",
          "original_form": "관광으로 가보셨나요?",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100015929.1.17",
          "form": "가까워서 가보게되었어요",
          "original_form": "가까워서 가보게되었어요",
          "speaker_id": "2",
