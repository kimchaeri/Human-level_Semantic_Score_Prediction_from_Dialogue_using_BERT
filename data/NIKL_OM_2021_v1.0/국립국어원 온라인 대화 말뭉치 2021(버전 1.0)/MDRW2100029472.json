      "utterance": [
        {
          "id": "MDRW2100029472.1.1",
          "form": "아침 드셨어요? 전 아직이네요",
          "original_form": "아침 드셨어요? 전 아직이네요",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100029472.1.2",
          "form": "저는 아침 안 먹으려고요",
          "original_form": "저는 아침 안 먹으려고요",
          "speaker_id": "1",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100029472.1.3",
          "form": "헉 왜요? 원래 잘 안 드세요?",
          "original_form": "헉 왜요? 원래 잘 안 드세요?",
          "speaker_id": "2",
          "time": "20210903 09:27"
        },
        {
          "id": "MDRW2100029472.1.4",
          "form": "어제 자기 전에 야식을 너무 많이 먹어서요ㅠㅠㅠ",
          "original_form": "어제 자기 전에 야식을 너무 많이 먹어서요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100029472.1.5",
          "form": "아직 더부룩하고 배고프지가 않네요",
          "original_form": "아직 더부룩하고 배고프지가 않네요",
          "speaker_id": "1",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100029472.1.6",
          "form": "야식 부럽다 뭐 드셨어요??",
          "original_form": "야식 부럽다 뭐 드셨어요??",
          "speaker_id": "2",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100029472.1.7",
          "form": "저 소고기 구워먹었어요ㅋㅋㅋ",
          "original_form": "저 소고기 구워먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100029472.1.8",
          "form": "와.. 소고기 대박이네요ㅠㅠㅠ 맛있었겠어요",
          "original_form": "와.. 소고기 대박이네요ㅠㅠㅠ 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100029472.1.9",
          "form": "네 진짜 맛나더라구요 오랜만에 먹는 야식이라 그런가봐요",
          "original_form": "네 진짜 맛나더라구요 오랜만에 먹는 야식이라 그런가봐요",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100029472.1.10",
          "form": "아침 뭐 드셨어용?",
          "original_form": "아침 뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100029472.1.11",
          "form": "저는 도넛이용",
          "original_form": "저는 도넛이용",
          "speaker_id": "2",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100029472.1.12",
          "form": "던킨도너츠 도넛 좋아해서요",
          "original_form": "던킨도너츠 도넛 좋아해서요",
          "speaker_id": "2",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100029472.1.13",
          "form": "도넛 맛있죠 던킨은 매달 여러 프로모션해서 좋은거 같아요",
          "original_form": "도넛 맛있죠 던킨은 매달 여러 프로모션해서 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100029472.1.14",
          "form": "네네 메뉴도 다양하고 가격도 괜찮더라구요",
          "original_form": "네네 메뉴도 다양하고 가격도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100029472.1.15",
          "form": "요즘은 배달앱에서 배달도 되는게 신기했어요",
          "original_form": "요즘은 배달앱에서 배달도 되는게 신기했어요",
          "speaker_id": "1",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100029472.1.16",
          "form": "맞아여ㅋㅋㅋㅋㅋ 세상 참 좋아졌어요",
          "original_form": "맞아여ㅋㅋㅋㅋㅋ 세상 참 좋아졌어요",
          "speaker_id": "2",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100029472.1.17",
          "form": "달달한 도넛 먹으니까 좋더라구요",
          "original_form": "달달한 도넛 먹으니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100029472.1.18",
          "form": "인정합니다ㅎㅎ",
          "original_form": "인정합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100029472.1.19",
          "form": "저는 점심에 죽이나 먹으려구요",
          "original_form": "저는 점심에 죽이나 먹으려구요",
          "speaker_id": "1",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100029472.1.20",
          "form": "죽도 좋죠 장조림이랑 같이 먹으면 짱",
          "original_form": "죽도 좋죠 장조림이랑 같이 먹으면 짱",
