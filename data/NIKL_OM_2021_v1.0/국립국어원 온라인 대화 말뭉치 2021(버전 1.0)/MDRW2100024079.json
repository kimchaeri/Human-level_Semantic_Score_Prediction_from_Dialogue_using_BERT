      "utterance": [
        {
          "id": "MDRW2100024079.1.1",
          "form": "오늘 점심에 튀김먹었어",
          "original_form": "오늘 점심에 튀김먹었어",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024079.1.2",
          "form": "무슨 튀김 드셨는데요?",
          "original_form": "무슨 튀김 드셨는데요?",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024079.1.3",
          "form": "김말이 먹었어",
          "original_form": "김말이 먹었어",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024079.1.4",
          "form": "맛있는거 드셨네 다른 반찬은 뭐 드셨어요?",
          "original_form": "맛있는거 드셨네 다른 반찬은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024079.1.5",
          "form": "다른건 오이지 사라다 먹었어",
          "original_form": "다른건 오이지 사라다 먹었어",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024079.1.6",
          "form": "김치 김치국",
          "original_form": "김치 김치국",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024079.1.7",
          "form": "그냥 그랬구나 반찬",
          "original_form": "그냥 그랬구나 반찬",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024079.1.8",
          "form": "넌 뭐 먹었어",
          "original_form": "넌 뭐 먹었어",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024079.1.9",
          "form": "된장찌개에 밥 비벼서 먹었어요",
          "original_form": "된장찌개에 밥 비벼서 먹었어요",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024079.1.10",
          "form": "나도 아침엔 된장먹었어",
          "original_form": "나도 아침엔 된장먹었어",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024079.1.11",
          "form": "짜진 않으셨어요?",
          "original_form": "짜진 않으셨어요?",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024079.1.12",
          "form": "데펴서 드셨고?",
          "original_form": "데펴서 드셨고?",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024079.1.13",
          "form": "짰어 생각보다 짜서 밥한숟가락 더 먹었어",
          "original_form": "짰어 생각보다 짜서 밥한숟가락 더 먹었어",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024079.1.14",
          "form": "아니 잘 드셨나 해서 여쭤봤지요",
          "original_form": "아니 잘 드셨나 해서 여쭤봤지요",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
