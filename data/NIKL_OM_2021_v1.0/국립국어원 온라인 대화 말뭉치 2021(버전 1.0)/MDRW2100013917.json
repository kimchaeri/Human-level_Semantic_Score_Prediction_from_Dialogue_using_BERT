      "utterance": [
        {
          "id": "MDRW2100013917.1.1",
          "form": "강아지 키우고 싶당",
          "original_form": "강아지 키우고 싶당",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100013917.1.2",
          "form": "나도 반려동물은 강아지 좋아",
          "original_form": "나도 반려동물은 강아지 좋아",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100013917.1.3",
          "form": "키우고 싶은 종이 있어?",
          "original_form": "키우고 싶은 종이 있어?",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100013917.1.4",
          "form": "난 키우고싶은 종이라기보다 털 안빠지는 종?",
          "original_form": "난 키우고싶은 종이라기보다 털 안빠지는 종?",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100013917.1.5",
          "form": "아토피 때메",
          "original_form": "아토피 때메",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100013917.1.6",
          "form": "아ㅜㅜ털 안빠지는거 없을텐데 사람도 빠지자나\r\n있으려나ㅜㅠ",
          "original_form": "아ㅜㅜ털 안빠지는거 없을텐데 사람도 빠지자나\r\n있으려나ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100013917.1.7",
          "form": "강아지 푸들은 사람보다 안빠진대 ㅋㅋ",
          "original_form": "강아지 푸들은 사람보다 안빠진대 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100013917.1.8",
          "form": "말티랑 비숑도 안빠진대",
          "original_form": "말티랑 비숑도 안빠진대",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100013917.1.9",
          "form": "엉 빠진다던데\r\n회사언니가 비숑키우거덩",
          "original_form": "엉 빠진다던데\r\n회사언니가 비숑키우거덩",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100013917.1.10",
          "form": "근데 비숑 품종은 진짜 비싸더라",
          "original_form": "근데 비숑 품종은 진짜 비싸더라",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100013917.1.11",
          "form": "디기 이쁨\r\n애기 때도 커서도 미적으로는 디기 이쁘더라",
          "original_form": "디기 이쁨\r\n애기 때도 커서도 미적으로는 디기 이쁘더라",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100013917.1.12",
          "form": "ㅇㅇ안빠지는건 아니고 덜 빠진다는 뜻",
          "original_form": "ㅇㅇ안빠지는건 아니고 덜 빠진다는 뜻",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100013917.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100013917.1.14",
          "form": "응 덜빠지는 종은 있는가보더라\r\n비숑이 그래? 비숑 이쁜데 비숑 키움 좋겠네",
          "original_form": "응 덜빠지는 종은 있는가보더라\r\n비숑이 그래? 비숑 이쁜데 비숑 키움 좋겠네",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100013917.1.15",
          "form": "비숑도 좋지 ㅠㅜ 키우고싶당 ㅋㅋ",
          "original_form": "비숑도 좋지 ㅠㅜ 키우고싶당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100013917.1.16",
          "form": "지금은 키울수있는 환경아니니까\r\n좀더 있다 키우겠네\r\n\r\n난 좋은데 나 한명도 버거운데 어캐 책임지나싶어 키우는건 거의 포기ㅎㅎ",
          "original_form": "지금은 키울수있는 환경아니니까\r\n좀더 있다 키우겠네\r\n\r\n난 좋은데 나 한명도 버거운데 어캐 책임지나싶어 키우는건 거의 포기ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 06:11"
        },
        {
          "id": "MDRW2100013917.1.17",
          "form": "그건그래 ㅋㅋ",
          "original_form": "그건그래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100013917.1.18",
          "form": "그래도 혼자 살게 되면 키우게 될 것 같아 꼭 강아지 아니더라도 ㅎㅎ",
          "original_form": "그래도 혼자 살게 되면 키우게 될 것 같아 꼭 강아지 아니더라도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100013917.1.19",
          "form": "그러게 반려동물 있음 좋긴해\r\n나 지금 회사에서 구피 키우는데\r\n좋아 힐링ㅎ",
          "original_form": "그러게 반려동물 있음 좋긴해\r\n나 지금 회사에서 구피 키우는데\r\n좋아 힐링ㅎ",
          "speaker_id": "2",
          "time": "20210827 09:09"
        },
        {
          "id": "MDRW2100013917.1.20",
          "form": "오 저번에 숙사에서 키우지 않았어?",
          "original_form": "오 저번에 숙사에서 키우지 않았어?",
          "speaker_id": "1",
          "time": "20210827 09:11"
        },
        {
          "id": "MDRW2100013917.1.21",
          "form": "기숙사에서 쫒겨났어ㅋ",
          "original_form": "기숙사에서 쫒겨났어ㅋ",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100013917.1.22",
          "form": "글서 회사 책상에서 키움",
          "original_form": "글서 회사 책상에서 키움",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100013917.1.23",
          "form": "오 그래도 안 죽고 계속 키우고있네 대단하다",
          "original_form": "오 그래도 안 죽고 계속 키우고있네 대단하다",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100013917.1.24",
          "form": "아냐 잘 키우다 급으로 죽더라\r\n글서 다시 얻어서 키우는중ㅎ",
          "original_form": "아냐 잘 키우다 급으로 죽더라\r\n글서 다시 얻어서 키우는중ㅎ",
          "speaker_id": "2",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100013917.1.25",
          "form": "아 그렇구나ㅜ 그거 원래 잘 죽는다더라ㅠ",
          "original_form": "아 그렇구나ㅜ 그거 원래 잘 죽는다더라ㅠ",
          "speaker_id": "1",
          "time": "20210827 09:29"
        }
      ]
