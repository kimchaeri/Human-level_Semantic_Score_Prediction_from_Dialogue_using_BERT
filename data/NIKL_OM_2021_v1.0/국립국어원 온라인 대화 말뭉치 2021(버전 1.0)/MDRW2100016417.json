      "utterance": [
        {
          "id": "MDRW2100016417.1.1",
          "form": "배달음식자주드세요?",
          "original_form": "배달음식자주드세요?",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016417.1.2",
          "form": "가족이 자주 시켜서 저는 덩달아 먹어요ㅋㅋ",
          "original_form": "가족이 자주 시켜서 저는 덩달아 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100016417.1.3",
          "form": "치킨좋아하세요?",
          "original_form": "치킨좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016417.1.4",
          "form": "순살 치킨 좋아해요!",
          "original_form": "순살 치킨 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016417.1.5",
          "form": "노랑통닭 굽네 요즘은 지코바 먹어요",
          "original_form": "노랑통닭 굽네 요즘은 지코바 먹어요",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016417.1.6",
          "form": "치킨 어디꺼 드세요?",
          "original_form": "치킨 어디꺼 드세요?",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016417.1.7",
          "form": "bhc주로먹는데 그닥 맛있진않은것같아요",
          "original_form": "bhc주로먹는데 그닥 맛있진않은것같아요",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016417.1.8",
          "form": "요즘 맛있는치킨이 없어요ㅠㅠ",
          "original_form": "요즘 맛있는치킨이 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016417.1.9",
          "form": "치킨 권태기 같은 건가요? ㅋㅋ",
          "original_form": "치킨 권태기 같은 건가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016417.1.10",
          "form": "종목을 바꿔보세요 배달음식 또 뭐 시켜먹어요?",
          "original_form": "종목을 바꿔보세요 배달음식 또 뭐 시켜먹어요?",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016417.1.11",
          "form": "좀 신선한 닭을 깨끗한 기름에 잘~튀기고 기름 탈탈 털어주는 가게가없어요",
          "original_form": "좀 신선한 닭을 깨끗한 기름에 잘~튀기고 기름 탈탈 털어주는 가게가없어요",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016417.1.12",
          "form": "아 기본인데 그거 어렵네요",
          "original_form": "아 기본인데 그거 어렵네요",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016417.1.13",
          "form": "맞아요!  그게어렵나",
          "original_form": "맞아요!  그게어렵나",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016417.1.14",
          "form": "깐부치킨 누가 맛있다던데요",
          "original_form": "깐부치킨 누가 맛있다던데요",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016417.1.15",
          "form": "바삭하다는 거 보면 기름은 잘 털어주는 것 같아요ㅋㅋ",
          "original_form": "바삭하다는 거 보면 기름은 잘 털어주는 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016417.1.16",
          "form": "그래요? ㅎㅎ 도전해볼게요",
          "original_form": "그래요? ㅎㅎ 도전해볼게요",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016417.1.17",
          "form": "ㅋㅋ 저도 시켜먹을 곳 하나 권해주세요",
          "original_form": "ㅋㅋ 저도 시켜먹을 곳 하나 권해주세요",
          "speaker_id": "1",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016417.1.18",
          "form": "음. . .  다 별로여가지고ㅋㅋ",
          "original_form": "음. . .  다 별로여가지고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016417.1.19",
          "form": "혹시광주시면.  . 뽀뽀통닭이라고 있는데",
          "original_form": "혹시광주시면.  . 뽀뽀통닭이라고 있는데",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016417.1.20",
          "form": "거기맛있어요ㅋㅋ 체인점아니구 개인이하는곳이예요",
          "original_form": "거기맛있어요ㅋㅋ 체인점아니구 개인이하는곳이예요",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016417.1.21",
          "form": "닭 살이  엄청 촉촉해서 백숙같이 겹겹이 벗겨지는데 겉은 바삭해요!",
          "original_form": "닭 살이  엄청 촉촉해서 백숙같이 겹겹이 벗겨지는데 겉은 바삭해요!",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016417.1.22",
          "form": "신기한 맛집이예요",
          "original_form": "신기한 맛집이예요",
