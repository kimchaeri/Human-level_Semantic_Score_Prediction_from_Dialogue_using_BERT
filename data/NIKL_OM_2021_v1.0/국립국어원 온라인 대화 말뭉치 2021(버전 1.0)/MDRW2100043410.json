      "utterance": [
        {
          "id": "MDRW2100043410.1.1",
          "form": "형제자매 있으세요?",
          "original_form": "형제자매 있으세요?",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043410.1.2",
          "form": "네 남동생 1명이요!",
          "original_form": "네 남동생 1명이요!",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043410.1.3",
          "form": "아하 name2는 여성분이죠?",
          "original_form": "아하 &name2&는 여성분이죠?",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043410.1.4",
          "form": "name2님",
          "original_form": "&name2&님",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043410.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043410.1.6",
          "form": "name1님도 여자맞으시죠??",
          "original_form": "&name1&님도 여자맞으시죠??",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043410.1.7",
          "form": "네네 ㅋㅋㅋ 저는 여동생만 있어서 남자형제 있는 집 궁금하더라구요",
          "original_form": "네네 ㅋㅋㅋ 저는 여동생만 있어서 남자형제 있는 집 궁금하더라구요",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043410.1.8",
          "form": "우선 말을 별로 안해요ㅋㅋㅋ",
          "original_form": "우선 말을 별로 안해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043410.1.9",
          "form": "앜ㅋㅋㅋㅋㅋ 나이차는 얼마 안나요?",
          "original_form": "앜ㅋㅋㅋㅋㅋ 나이차는 얼마 안나요?",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043410.1.10",
          "form": "네 2살이요!",
          "original_form": "네 2살이요!",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043410.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043410.1.12",
          "form": "저는 연년생이에요 ㅋㅋㅋㅋ",
          "original_form": "저는 연년생이에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043410.1.13",
          "form": "엄청 싸우면서 컸는데 다 크고나니까 동생이 최고네요ㅠㅠ",
          "original_form": "엄청 싸우면서 컸는데 다 크고나니까 동생이 최고네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043410.1.14",
          "form": "동생분이랑 잘 안싸우셨어요?ㅋㅋㅋ",
          "original_form": "동생분이랑 잘 안싸우셨어요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043410.1.15",
          "form": "성인되니까 말 잘 안해요",
          "original_form": "성인되니까 말 잘 안해요",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043410.1.16",
          "form": "어릴땐 많이 싸웠어요",
          "original_form": "어릴땐 많이 싸웠어요",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043410.1.17",
          "form": "어릴땐 어쩔수 없나 봐요 ㅋㅋ",
          "original_form": "어릴땐 어쩔수 없나 봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043410.1.18",
          "form": "맞아요 정말 치고박고 싸웠거든요ㅋㅋ",
          "original_form": "맞아요 정말 치고박고 싸웠거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043410.1.19",
          "form": "첫째라서 서러울때도 있지않나요 흑",
          "original_form": "첫째라서 서러울때도 있지않나요 흑",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043410.1.20",
          "form": "맞아요 어릴땐 누나가 동생 잘못한거도 몰랐다고 덤으로혼나고ㅠ",
          "original_form": "맞아요 어릴땐 누나가 동생 잘못한거도 몰랐다고 덤으로혼나고ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043410.1.21",
          "form": "어릴땐 누나가돼서 동생 잘못한것도 몰랐다고덤으로  혼나고ㅠㅜ",
          "original_form": "어릴땐 누나가돼서 동생 잘못한것도 몰랐다고덤으로  혼나고ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 20:20"
        }
      ]
