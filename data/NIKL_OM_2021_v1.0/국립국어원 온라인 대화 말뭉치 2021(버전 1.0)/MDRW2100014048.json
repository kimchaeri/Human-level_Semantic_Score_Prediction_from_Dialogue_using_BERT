      "utterance": [
        {
          "id": "MDRW2100014048.1.1",
          "form": "스포츠 보는거 좋아하세요???",
          "original_form": "스포츠 보는거 좋아하세요???",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014048.1.2",
          "form": "원래는 즐기지 않았는데 이번 올림픽은 봤어여",
          "original_form": "원래는 즐기지 않았는데 이번 올림픽은 봤어여",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014048.1.3",
          "form": "오오 어떤게 젤 재밌으셨나요 ㅎㅎ",
          "original_form": "오오 어떤게 젤 재밌으셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014048.1.4",
          "form": "저는 배구랑 도마 제밌게 봤어요",
          "original_form": "저는 배구랑 도마 제밌게 봤어요",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014048.1.5",
          "form": "오 저두 배구요 ㅎㅎ",
          "original_form": "오 저두 배구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014048.1.6",
          "form": "한번 직관 가고싶더라구요!",
          "original_form": "한번 직관 가고싶더라구요!",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014048.1.7",
          "form": "4단계 풀리면 가보고싶네요",
          "original_form": "4단계 풀리면 가보고싶네요",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014048.1.8",
          "form": "양궁도 보셨었어요???",
          "original_form": "양궁도 보셨었어요???",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014048.1.9",
          "form": "양궁 실시간으로 보느라 심장이 아팠어요ㅋㅋ",
          "original_form": "양궁 실시간으로 보느라 심장이 아팠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014048.1.10",
          "form": "아 저두욬ㅋㅋ 진짜 슛오프 갈때마다 심장 아프더라구요 !",
          "original_form": "아 저두욬ㅋㅋ 진짜 슛오프 갈때마다 심장 아프더라구요 !",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014048.1.11",
          "form": "정말 한 발 한 발이 저한테 오는 것 같았어요ㅋㅋ",
          "original_form": "정말 한 발 한 발이 저한테 오는 것 같았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014048.1.12",
          "form": "진짜욬ㅋㅋ 근데 금메달 따서 진짜",
          "original_form": "진짜욬ㅋㅋ 근데 금메달 따서 진짜",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014048.1.13",
          "form": "대박인거같아요 ㅠㅠ",
          "original_form": "대박인거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014048.1.14",
          "form": "메달 5개 중에 4개를 우리나라가 땄으니까요ㅠㅠ",
          "original_form": "메달 5개 중에 4개를 우리나라가 땄으니까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014048.1.15",
          "form": "진찌ㅣ요 ㅠㅠ 양궁도 꼭 직관해보고싶어요 !",
