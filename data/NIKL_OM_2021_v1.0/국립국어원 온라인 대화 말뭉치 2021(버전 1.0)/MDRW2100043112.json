      "utterance": [
        {
          "id": "MDRW2100043112.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043112.1.2",
          "form": "커피 좋아하시나요?",
          "original_form": "커피 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043112.1.3",
          "form": "안녕하세요! 잠깰때만 먹어요!",
          "original_form": "안녕하세요! 잠깰때만 먹어요!",
          "speaker_id": "2",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043112.1.4",
          "form": "저는 카페인이 안받아서 ㅜㅜ",
          "original_form": "저는 카페인이 안받아서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043112.1.5",
          "form": "아하 그럼 카페 가시면 뭘 젤 잘 드시나요",
          "original_form": "아하 그럼 카페 가시면 뭘 젤 잘 드시나요",
          "speaker_id": "1",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043112.1.6",
          "form": "저 허브티 위주로 많이 먹고 이디야는 커피메뉴빼고 거의 다먹어봣어요 ㅋㅋㅋㅋ",
          "original_form": "저 허브티 위주로 많이 먹고 이디야는 커피메뉴빼고 거의 다먹어봣어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043112.1.7",
          "form": "아하 이디야도 근데 맛있는거 많잖아요 ㅋㅋ 토피넛라떼 맛나던데",
          "original_form": "아하 이디야도 근데 맛있는거 많잖아요 ㅋㅋ 토피넛라떼 맛나던데",
          "speaker_id": "1",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100043112.1.8",
          "form": "오 제가 간 지점은 그거 진짜 맛없더라구요 ㅜ",
          "original_form": "오 제가 간 지점은 그거 진짜 맛없더라구요 ㅜ",
          "speaker_id": "2",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100043112.1.9",
          "form": "맛잇다하시니 다른지점에서 도전해봐야겟어요",
          "original_form": "맛잇다하시니 다른지점에서 도전해봐야겟어요",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043112.1.10",
          "form": "앗 정말요 ㅠ 지점차가 있나봐요 ㅋㅋ",
          "original_form": "앗 정말요 ㅠ 지점차가 있나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043112.1.11",
          "form": "그럼 디저트류는 뭘 젤 좋아하세요",
          "original_form": "그럼 디저트류는 뭘 젤 좋아하세요",
          "speaker_id": "1",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043112.1.12",
          "form": "행복한 고민이네요",
          "original_form": "행복한 고민이네요",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043112.1.13",
          "form": "저 지금은  슈크림이 먹고싶네요",
          "original_form": "저 지금은  슈크림이 먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043112.1.14",
          "form": "그 아시나요 동글동글한거",
          "original_form": "그 아시나요 동글동글한거",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043112.1.15",
          "form": "오 알죠알죠",
          "original_form": "오 알죠알죠",
          "speaker_id": "1",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043112.1.16",
          "form": "차갑게 먹으면 진짜 맛있잖아요",
          "original_form": "차갑게 먹으면 진짜 맛있잖아요",
          "speaker_id": "1",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043112.1.17",
          "form": "맞아요 맞아요 그거 먹으면 달고 진짜 행복해요",
          "original_form": "맞아요 맞아요 그거 먹으면 달고 진짜 행복해요",
          "speaker_id": "2",
          "time": "20210907 14:36"
        },
        {
          "id": "MDRW2100043112.1.18",
          "form": "좋아하는 디저트 잇으세요?",
          "original_form": "좋아하는 디저트 잇으세요?",
          "speaker_id": "2",
          "time": "20210907 14:36"
        },
        {
          "id": "MDRW2100043112.1.19",
          "form": "흠 저는 치즈케익 좋아해요 진한거 ㅋㅋ",
          "original_form": "흠 저는 치즈케익 좋아해요 진한거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100043112.1.20",
          "form": "캬 그거랑 약간씁쓸한 커피나 차 마시면 기가막혀요",
          "original_form": "캬 그거랑 약간씁쓸한 커피나 차 마시면 기가막혀요",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043112.1.21",
          "form": "그쵸 그 쌉싸름한 맛이랑 잘 맞는거 같아요 ㅎㅎ",
          "original_form": "그쵸 그 쌉싸름한 맛이랑 잘 맞는거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:23"
        }
      ]
    }
