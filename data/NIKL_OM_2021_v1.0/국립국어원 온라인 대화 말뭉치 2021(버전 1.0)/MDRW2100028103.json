      "utterance": [
        {
          "id": "MDRW2100028103.1.1",
          "form": "작년 지방직 때 초시에 붙었어요",
          "original_form": "작년 지방직 때 초시에 붙었어요",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028103.1.2",
          "form": "대박 몇년준비했너요",
          "original_form": "대박 몇년준비했너요",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028103.1.3",
          "form": "지방직이라니 ㅎㅎ",
          "original_form": "지방직이라니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028103.1.4",
          "form": "무슨직종인가요",
          "original_form": "무슨직종인가요",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100028103.1.5",
          "form": "일행이여요",
          "original_form": "일행이여요",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028103.1.6",
          "form": "9개월정도 공부했어요",
          "original_form": "9개월정도 공부했어요",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028103.1.7",
          "form": "대박입니다",
          "original_form": "대박입니다",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028103.1.8",
          "form": "어려서 머리가 팔팔했어요",
          "original_form": "어려서 머리가 팔팔했어요",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028103.1.9",
          "form": "공무원이 쉽지 암ㅎ은데",
          "original_form": "공무원이 쉽지 암ㅎ은데",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028103.1.10",
          "form": "어떻게 그렇게 잘하나요 몇살이뵤",
          "original_form": "어떻게 그렇게 잘하나요 몇살이뵤",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028103.1.11",
          "form": "무슨과로 갔나요?",
          "original_form": "무슨과로 갔나요?",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028103.1.12",
          "form": "25살이요",
          "original_form": "25살이요",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028103.1.13",
          "form": "우와 진짜 어린나이에 벌써 취뽀라니...",
          "original_form": "우와 진짜 어린나이에 벌써 취뽀라니...",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028103.1.14",
          "form": "언제출근했나뇨",
          "original_form": "언제출근했나뇨",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028103.1.15",
          "form": "민원팀이요",
          "original_form": "민원팀이요",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028103.1.16",
          "form": "오늘은 좀 바빴네요 일이",
          "original_form": "오늘은 좀 바빴네요 일이",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028103.1.17",
          "form": "3월에 시작했어요",
          "original_form": "3월에 시작했어요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028103.1.18",
          "form": "동생 아직 수습이에요",
          "original_form": "동생 아직 수습이에요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100028103.1.19",
          "form": "우와 생각보다 많이 됐네요",
          "original_form": "우와 생각보다 많이 됐네요",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028103.1.20",
          "form": "네 그래요",
          "original_form": "네 그래요",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028103.1.21",
          "form": "내보다 일 더 잘하는듯요 ㅋㅋ",
          "original_form": "내보다 일 더 잘하는듯요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028103.1.22",
          "form": "ㅋㅋㅋ 동생들이 그렇게 일을 잘해서 뿌듯하게썽요",
          "original_form": "ㅋㅋㅋ 동생들이 그렇게 일을 잘해서 뿌듯하게썽요",
          "speaker_id": "1",
          "time": "20210903 01:23"
        },
        {
          "id": "MDRW2100028103.1.23",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 08:57"
        },
        {
          "id": "MDRW2100028103.1.24",
          "form": "출근정말싫네요",
          "original_form": "출근정말싫네요",
