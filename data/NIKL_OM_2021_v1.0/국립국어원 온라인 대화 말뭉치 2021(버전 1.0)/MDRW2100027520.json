      "utterance": [
        {
          "id": "MDRW2100027520.1.1",
          "form": "비가와서 그런가 꿉꿉해요",
          "original_form": "비가와서 그런가 꿉꿉해요",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027520.1.2",
          "form": "에어컨 틀어져있으신가요",
          "original_form": "에어컨 틀어져있으신가요",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027520.1.3",
          "form": "네 틀죠",
          "original_form": "네 틀죠",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027520.1.4",
          "form": "그럼 쾌적하시겠어요 여기도 카페라 시원해요",
          "original_form": "그럼 쾌적하시겠어요 여기도 카페라 시원해요",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027520.1.5",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027520.1.6",
          "form": "내일도 하루종일 비온데요",
          "original_form": "내일도 하루종일 비온데요",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027520.1.7",
          "form": "거긴 어때요",
          "original_form": "거긴 어때요",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027520.1.8",
          "form": "흐려요",
          "original_form": "흐려요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027520.1.9",
          "form": "비가 너무 자주와요",
          "original_form": "비가 너무 자주와요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027520.1.10",
          "form": "지겹네요",
          "original_form": "지겹네요",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027520.1.11",
          "form": "이제 가을이라 좋네요 그래도",
          "original_form": "이제 가을이라 좋네요 그래도",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027520.1.12",
          "form": "저는 가을 전어 좋아해요",
          "original_form": "저는 가을 전어 좋아해요",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027520.1.13",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100027520.1.14",
          "form": "그래서 가을되면 살찌는가봐요 ㅋㅋ",
          "original_form": "그래서 가을되면 살찌는가봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027520.1.15",
          "form": "그른가요",
          "original_form": "그른가요",
          "speaker_id": "1",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027520.1.16",
          "form": "지금도. 비오네요",
          "original_form": "지금도. 비오네요",
