      "utterance": [
        {
          "id": "MDRW2100041908.1.1",
          "form": "매일 name3 혼자 있어야되기 그래서 보낸다면서",
          "original_form": "매일 &name3& 혼자 있어야되기 그래서 보낸다면서",
          "speaker_id": "2",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041908.1.2",
          "form": "원래 그렇게 화 안 되는 스타일이 들이",
          "original_form": "원래 그렇게 화 안 되는 스타일이 들이",
          "speaker_id": "1",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041908.1.3",
          "form": "더 무서워요 무슨 생각을 하는지 모름",
          "original_form": "더 무서워요 무슨 생각을 하는지 모름",
          "speaker_id": "1",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041908.1.4",
          "form": "그래 화안내고 참다참다",
          "original_form": "그래 화안내고 참다참다",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041908.1.5",
          "form": "한방에 터뜨리고 끝내네 ㅋㅋㅋㄱㄱㄱㅋ",
          "original_form": "한방에 터뜨리고 끝내네 ㅋㅋㅋㄱㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041908.1.6",
          "form": "영어학원도 소개시켜주고",
          "original_form": "영어학원도 소개시켜주고",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041908.1.7",
          "form": "태권도도 가고",
          "original_form": "태권도도 가고",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041908.1.8",
          "form": "유종의 미를 거두기 힘드네",
          "original_form": "유종의 미를 거두기 힘드네",
          "speaker_id": "1",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041908.1.9",
          "form": "캠핑 갈수있게 만들어줬더니",
          "original_form": "캠핑 갈수있게 만들어줬더니",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041908.1.10",
          "form": "우리신랑이 먹고 살게 해줬디",
          "original_form": "우리신랑이 먹고 살게 해줬디",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041908.1.11",
          "form": "거기는 그렇게 생각 안 할 걸요ㅋㅋㅋㅋ",
          "original_form": "거기는 그렇게 생각 안 할 걸요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041908.1.12",
          "form": "단물만 빼먹고 간다고 ㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "단물만 빼먹고 간다고 ㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041908.1.13",
          "form": "잘 됐어요 언니~",
          "original_form": "잘 됐어요 언니~",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041908.1.14",
          "form": "우리가 name3 아빠 진짜 사람 만들어놨는데\r\nㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우리가 &name3& 아빠 진짜 사람 만들어놨는데\r\nㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041908.1.15",
          "form": "나중에 진짜 더 깊어지고 난 뒤에 이래버리면",
          "original_form": "나중에 진짜 더 깊어지고 난 뒤에 이래버리면",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041908.1.16",
          "form": "그땐 정말ㅠ",
          "original_form": "그땐 정말ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041908.1.17",
          "form": "그래 일단은 올해는 같이 캠핑가지말자고 해놨는데",
          "original_form": "그래 일단은 올해는 같이 캠핑가지말자고 해놨는데",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041908.1.18",
          "form": "그래도 연락안하는건 아니고",
          "original_form": "그래도 연락안하는건 아니고",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041908.1.19",
          "form": "계속 하긴하는데",
          "original_form": "계속 하긴하는데",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041908.1.20",
          "form": "이제 서로 불편하지 뭐;;;",
          "original_form": "이제 서로 불편하지 뭐;;;",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041908.1.21",
          "form": "그게 더 뻘쭘한 거 아니에요ㅋㅋㅋㅋ",
          "original_form": "그게 더 뻘쭘한 거 아니에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041908.1.22",
          "form": "그러니까요 이제 진짜 서로도 플 편함",
          "original_form": "그러니까요 이제 진짜 서로도 플 편함",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041908.1.23",
          "form": "어 이게 서로 불편하지",
          "original_form": "어 이게 서로 불편하지",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100041908.1.24",
          "form": "그래도 애들은 뭐 그냥 또 논다",
          "original_form": "그래도 애들은 뭐 그냥 또 논다",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100041908.1.25",
          "form": "우리 눈에만 안 보일 뿐",
          "original_form": "우리 눈에만 안 보일 뿐",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100041908.1.26",
          "form": "오늘도 여전히 학교 마치고 학원 가면서 놀고",
          "original_form": "오늘도 여전히 학교 마치고 학원 가면서 놀고",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041908.1.27",
          "form": "태권도 가면서도 놀고",
          "original_form": "태권도 가면서도 놀고",
