      "utterance": [
        {
          "id": "MDRW2100044950.1.1",
          "form": "님 이불 삿나요",
          "original_form": "님 이불 삿나요",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044950.1.2",
          "form": "아니요ㅜㅜ 지금 이불을 못샀는데",
          "original_form": "아니요ㅜㅜ 지금 이불을 못샀는데",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044950.1.3",
          "form": "오늘 거울이랑 조명 샀어요",
          "original_form": "오늘 거울이랑 조명 샀어요",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044950.1.4",
          "form": "전신이랑 반신 2개다 샀어요",
          "original_form": "전신이랑 반신 2개다 샀어요",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100044950.1.5",
          "form": "대박 님 플렉스",
          "original_form": "대박 님 플렉스",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100044950.1.6",
          "form": "안혼나요?",
          "original_form": "안혼나요?",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100044950.1.7",
          "form": "제돈으로 산건데요 뭐",
          "original_form": "제돈으로 산건데요 뭐",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100044950.1.8",
          "form": "물론 할부로 ㅎㅎ",
          "original_form": "물론 할부로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100044950.1.9",
          "form": "ㅌㅋㅋㅋ 몇개월",
          "original_form": "ㅌㅋㅋㅋ 몇개월",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100044950.1.10",
          "form": "5개월이요 조명이 13만원정도고",
          "original_form": "5개월이요 조명이 13만원정도고",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100044950.1.11",
          "form": "거울은 2개합쳐서 10만원 조금 넘고용",
          "original_form": "거울은 2개합쳐서 10만원 조금 넘고용",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100044950.1.12",
          "form": "와조명 먼데비싸요",
          "original_form": "와조명 먼데비싸요",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100044950.1.13",
          "form": "밑둥이 우드고 조명 갓부분이 캔버스인데",
          "original_form": "밑둥이 우드고 조명 갓부분이 캔버스인데",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100044950.1.14",
          "form": "예쁘게 주름잡혀있어여!!",
          "original_form": "예쁘게 주름잡혀있어여!!",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100044950.1.15",
          "form": "자랑해주시죠",
          "original_form": "자랑해주시죠",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100044950.1.16",
          "form": "캔버스가 수제라고 하더라구요~~ 오면 자랑할게요!",
          "original_form": "캔버스가 수제라고 하더라구요~~ 오면 자랑할게요!",
