      "utterance": [
        {
          "id": "MDRW2100007542.1.1",
          "form": "배그 좋아해?",
          "original_form": "배그 좋아해?",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007542.1.2",
          "form": "아니... 잘 못해",
          "original_form": "아니... 잘 못해",
          "speaker_id": "2",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007542.1.3",
          "form": "하다가 욕 엄청먹음",
          "original_form": "하다가 욕 엄청먹음",
          "speaker_id": "2",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007542.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007542.1.5",
          "form": "누가 욕해",
          "original_form": "누가 욕해",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007542.1.6",
          "form": "캐리할 생각을 해야지",
          "original_form": "캐리할 생각을 해야지",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007542.1.7",
          "form": "아니 나 오버워치만 하는데",
          "original_form": "아니 나 오버워치만 하는데",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007542.1.8",
          "form": "욕먹을만했음ㅋㅋ배그에선",
          "original_form": "욕먹을만했음ㅋㅋ배그에선",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007542.1.9",
          "form": "낙하산을 이상한데로 폈어",
          "original_form": "낙하산을 이상한데로 폈어",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007542.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007542.1.11",
          "form": "아 그건 좀ㅠㅠ 쉴드 불가",
          "original_form": "아 그건 좀ㅠㅠ 쉴드 불가",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007542.1.12",
          "form": "오버워치도 해?!",
          "original_form": "오버워치도 해?!",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007542.1.13",
          "form": "최애 픽 누구",
          "original_form": "최애 픽 누구",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007542.1.14",
          "form": "ㅋㅋㅋㅋㅋ바로 쉴드불가쳐버리네!!!!",
          "original_form": "ㅋㅋㅋㅋㅋ바로 쉴드불가쳐버리네!!!!",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007542.1.15",
          "form": "나ㅎ.ㅎ로드호그",
          "original_form": "나ㅎ.ㅎ로드호그",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007542.1.16",
          "form": "일리오스였나? 거기 갈고리 꿀잼",
          "original_form": "일리오스였나? 거기 갈고리 꿀잼",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007542.1.17",
          "form": "뚱땡이 할아버지 맞나?",
          "original_form": "뚱땡이 할아버지 맞나?",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007542.1.18",
          "form": "맨날 갈고리 끌려감... ㄱㅡ",
          "original_form": "맨날 갈고리 끌려감... ㄱㅡ",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007542.1.19",
          "form": "나는 애쉬랑 마법사 걔",
          "original_form": "나는 애쉬랑 마법사 걔",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007542.1.20",
          "form": "ㅋㅋㅋㅋㅋ뚱땡이할아버지래",
          "original_form": "ㅋㅋㅋㅋㅋ뚱땡이할아버지래",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007542.1.21",
          "form": "마법사가누구지",
          "original_form": "마법사가누구지",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007542.1.22",
          "form": "메르시?",
          "original_form": "메르시?",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
