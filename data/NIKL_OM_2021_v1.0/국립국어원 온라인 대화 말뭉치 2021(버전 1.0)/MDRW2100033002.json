      "utterance": [
        {
          "id": "MDRW2100033002.1.1",
          "form": "혹시 근처 가면 꼭 드세요!",
          "original_form": "혹시 근처 가면 꼭 드세요!",
          "speaker_id": "2",
          "time": "20210906 12:47"
        },
        {
          "id": "MDRW2100033002.1.2",
          "form": "제가 여의도를 갈일이...",
          "original_form": "제가 여의도를 갈일이...",
          "speaker_id": "1",
          "time": "20210906 12:47"
        },
        {
          "id": "MDRW2100033002.1.3",
          "form": "ㅎ 저도 회사 이사하고 못 갔습니다ㅜㅜㅜ",
          "original_form": "ㅎ 저도 회사 이사하고 못 갔습니다ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100033002.1.4",
          "form": "여기 콩나물국밥 맛집과 동태탕 맛집 파스타맛집 있는데",
          "original_form": "여기 콩나물국밥 맛집과 동태탕 맛집 파스타맛집 있는데",
          "speaker_id": "2",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100033002.1.5",
          "form": "세 음식 다 쏘쏘인 제가 가끔 그리울 정도니ㅜ",
          "original_form": "세 음식 다 쏘쏘인 제가 가끔 그리울 정도니ㅜ",
          "speaker_id": "2",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100033002.1.6",
          "form": "근데 회사가 왜 이사 갔어요",
          "original_form": "근데 회사가 왜 이사 갔어요",
          "speaker_id": "1",
          "time": "20210906 12:50"
        },
        {
          "id": "MDRW2100033002.1.7",
          "form": "임대기간 끝나고?",
          "original_form": "임대기간 끝나고?",
          "speaker_id": "2",
          "time": "20210906 12:51"
        },
        {
          "id": "MDRW2100033002.1.8",
          "form": "값을 많이 올렸나부네요",
          "original_form": "값을 많이 올렸나부네요",
          "speaker_id": "1",
          "time": "20210906 12:51"
        },
        {
          "id": "MDRW2100033002.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 12:51"
        },
        {
          "id": "MDRW2100033002.1.10",
          "form": "지금은 사옥이쥬",
          "original_form": "지금은 사옥이쥬",
          "speaker_id": "2",
          "time": "20210906 12:52"
        },
        {
          "id": "MDRW2100033002.1.11",
          "form": "ㅋㅋ새로간 지역도",
          "original_form": "ㅋㅋ새로간 지역도",
          "speaker_id": "1",
          "time": "20210906 12:52"
        },
        {
          "id": "MDRW2100033002.1.12",
          "form": "맛집이 있을텐데요",
          "original_form": "맛집이 있을텐데요",
          "speaker_id": "1",
          "time": "20210906 12:52"
        },
        {
          "id": "MDRW2100033002.1.13",
          "form": "사장놈이 임대장사 하느라 구내식당도 안 주는ㅜ",
          "original_form": "사장놈이 임대장사 하느라 구내식당도 안 주는ㅜ",
          "speaker_id": "2",
          "time": "20210906 12:52"
        },
        {
          "id": "MDRW2100033002.1.14",
          "form": "두번째는 명동..",
          "original_form": "두번째는 명동..",
          "speaker_id": "2",
          "time": "20210906 12:53"
        },
        {
          "id": "MDRW2100033002.1.15",
          "form": "관광객과 출퇴근했네요ㅎㅎ",
          "original_form": "관광객과 출퇴근했네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:53"
        },
        {
          "id": "MDRW2100033002.1.16",
          "form": "가게는 여의도보다 훨 많은데",
          "original_form": "가게는 여의도보다 훨 많은데",
          "speaker_id": "2",
          "time": "20210906 12:54"
        },
        {
          "id": "MDRW2100033002.1.17",
          "form": "아 맛집도 더 많아요",
          "original_form": "아 맛집도 더 많아요",
          "speaker_id": "2",
          "time": "20210906 12:55"
        },
        {
          "id": "MDRW2100033002.1.18",
          "form": "근데 순대국 파스타 최애집은 여의도ㅜ",
          "original_form": "근데 순대국 파스타 최애집은 여의도ㅜ",
          "speaker_id": "2",
          "time": "20210906 12:55"
        },
        {
          "id": "MDRW2100033002.1.19",
          "form": "ㅋㅋ명동은 요새 관광객",
          "original_form": "ㅋㅋ명동은 요새 관광객",
          "speaker_id": "1",
          "time": "20210906 12:56"
        },
        {
          "id": "MDRW2100033002.1.20",
          "form": "다 박살났다고 합니다",
          "original_form": "다 박살났다고 합니다",
          "speaker_id": "1",
          "time": "20210906 12:56"
        },
        {
          "id": "MDRW2100033002.1.21",
          "form": "ㅎㅎ 지금 직장도 명동서 많이 안 먼데",
          "original_form": "ㅎㅎ 지금 직장도 명동서 많이 안 먼데",
          "speaker_id": "2",
          "time": "20210906 12:57"
        },
        {
          "id": "MDRW2100033002.1.22",
          "form": "가보면 일찍 문닫는 가게들이 늘었더라고요",
          "original_form": "가보면 일찍 문닫는 가게들이 늘었더라고요",
          "speaker_id": "2",
          "time": "20210906 12:58"
        },
        {
          "id": "MDRW2100033002.1.23",
          "form": "중심가에 계시는군요",
          "original_form": "중심가에 계시는군요",
          "speaker_id": "1",
          "time": "20210906 14:16"
        },
        {
          "id": "MDRW2100033002.1.24",
          "form": "역장님",
          "original_form": "역장님",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100033002.1.25",
          "form": "ㅎㅎ 역장님 아닙니다 비천한 노비입니다ㅜㅇㅜ",
          "original_form": "ㅎㅎ 역장님 아닙니다 비천한 노비입니다ㅜㅇㅜ",
          "speaker_id": "2",
          "time": "20210907 20:56"
        }
      ]
    }
