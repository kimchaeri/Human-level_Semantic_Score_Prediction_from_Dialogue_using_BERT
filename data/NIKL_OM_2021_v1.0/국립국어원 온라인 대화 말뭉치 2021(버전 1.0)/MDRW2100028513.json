          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028513.1.7",
          "form": "행복한쿼카",
          "original_form": "행복한쿼카",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028513.1.8",
          "form": "거기시리즈짱귀야",
          "original_form": "거기시리즈짱귀야",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028513.1.9",
          "form": "나 놋북 파우치사고시퍼",
          "original_form": "나 놋북 파우치사고시퍼",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028513.1.10",
          "form": "아수스는 꽁으로 줬는데",
          "original_form": "아수스는 꽁으로 줬는데",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028513.1.11",
          "form": "얘는 없어",
          "original_form": "얘는 없어",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028513.1.12",
          "form": "나도\n나도나도",
          "original_form": "나도\n나도나도",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028513.1.13",
          "form": "정말 없 어",
          "original_form": "정말 없 어",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028513.1.14",
          "form": "놋북가방",
          "original_form": "놋북가방",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028513.1.15",
          "form": "근데 맥북가방은 이쁜거많는데",
          "original_form": "근데 맥북가방은 이쁜거많는데",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028513.1.16",
          "form": "놋북가방은 엄써",
          "original_form": "놋북가방은 엄써",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028513.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028513.1.18",
          "form": "그냥가방은",
          "original_form": "그냥가방은",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028513.1.19",
          "form": "사이즈다른가?",
          "original_form": "사이즈다른가?",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028513.1.20",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028513.1.21",
          "form": "맥북이 좀 얇은듯 내꺼는 익스트라 라지사이즈야",
          "original_form": "맥북이 좀 얇은듯 내꺼는 익스트라 라지사이즈야",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028513.1.22",
          "form": "아수스는?",
          "original_form": "아수스는?",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028513.1.23",
          "form": "아수스도 내꺼에 비하면 말라깽이지",
          "original_form": "아수스도 내꺼에 비하면 말라깽이지",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028513.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아수스 맥북이랑비교하면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아수스 맥북이랑비교하면",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028513.1.25",
          "form": "큰가?",
          "original_form": "큰가?",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028513.1.26",
          "form": "맥북도 사이즈별로있긴한데",
          "original_form": "맥북도 사이즈별로있긴한데",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100028513.1.27",
          "form": "저건 에어랑 비슷할걸",
          "original_form": "저건 에어랑 비슷할걸",
          "speaker_id": "1",
