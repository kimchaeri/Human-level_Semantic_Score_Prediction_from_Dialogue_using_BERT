      "utterance": [
        {
          "id": "MDRW2100002837.1.1",
          "form": "요즘 보신 스포츠경기 있으신가요?",
          "original_form": "요즘 보신 스포츠경기 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002837.1.2",
          "form": "최근에 올림픽 정말 재미있게 봤어요.",
          "original_form": "최근에 올림픽 정말 재미있게 봤어요.",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002837.1.3",
          "form": "양궁이랑 배구 보셨어요?!",
          "original_form": "양궁이랑 배구 보셨어요?!",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002837.1.4",
          "form": "네 저두 봤어요",
          "original_form": "네 저두 봤어요",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002837.1.5",
          "form": "정말 짜릿해요",
          "original_form": "정말 짜릿해요",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002837.1.6",
          "form": "여자배구 한일전 최고였어요.",
          "original_form": "여자배구 한일전 최고였어요.",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002837.1.7",
          "form": "마음 졸이며 봤는데 정말 보기를 잘했어요",
          "original_form": "마음 졸이며 봤는데 정말 보기를 잘했어요",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002837.1.8",
          "form": "맞아요 모든 사람들이",
          "original_form": "맞아요 모든 사람들이",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002837.1.9",
          "form": "다 좋아했잖아요",
          "original_form": "다 좋아했잖아요",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002837.1.10",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002837.1.11",
          "form": "김연경선수 마지막 올림픽이라니 더 아쉬워요",
          "original_form": "김연경선수 마지막 올림픽이라니 더 아쉬워요",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002837.1.12",
          "form": "그러니까요 다음에 또 나와줬으면 정말 좋겠어요 ㅠ",
          "original_form": "그러니까요 다음에 또 나와줬으면 정말 좋겠어요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002837.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002837.1.14",
          "form": "야구나 축구는 좀 실망스러웠어요ㅠ",
          "original_form": "야구나 축구는 좀 실망스러웠어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002837.1.15",
          "form": "그니까유.. 배구랑 양궁이 다 잘되서 다행이에여",
          "original_form": "그니까유.. 배구랑 양궁이 다 잘되서 다행이에여",
          "speaker_id": "1",
          "time": "20210820 18:04"
        },
        {
          "id": "MDRW2100002837.1.16",
          "form": "배구 직관 해보신 적 있으세요?",
          "original_form": "배구 직관 해보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100002837.1.17",
          "form": "직관은 안해봤어요",
          "original_form": "직관은 안해봤어요",
          "speaker_id": "1",
          "time": "20210820 20:16"
        },
        {
          "id": "MDRW2100002837.1.18",
          "form": "혹시 해보셨어요?",
          "original_form": "혹시 해보셨어요?",
          "speaker_id": "1",
          "time": "20210820 20:16"
        },
        {
          "id": "MDRW2100002837.1.19",
          "form": "저도 배구 직관은 안해봤어요...",
          "original_form": "저도 배구 직관은 안해봤어요...",
          "speaker_id": "2",
          "time": "20210820 20:19"
        },
        {
          "id": "MDRW2100002837.1.20",
          "form": "배구는 올해 제일 재밌게 봐서 이제 갓 관심이 생긴 정도예요 ㅋㅋㅋㅋ",
          "original_form": "배구는 올해 제일 재밌게 봐서 이제 갓 관심이 생긴 정도예요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:19"
        },
        {
          "id": "MDRW2100002837.1.21",
          "form": "나중에 한번 보고싶어요",
          "original_form": "나중에 한번 보고싶어요",
          "speaker_id": "1",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100002837.1.22",
          "form": "실제로보면 더 재밌을 거 같아요 ㅎ",
          "original_form": "실제로보면 더 재밌을 거 같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100002837.1.23",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "2",
          "time": "20210820 20:25"
        }
      ]
