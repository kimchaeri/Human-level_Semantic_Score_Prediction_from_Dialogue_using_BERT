      "utterance": [
        {
          "id": "MDRW2100015336.1.1",
          "form": "오늘 날씨 어떠셨어요?",
          "original_form": "오늘 날씨 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015336.1.2",
          "form": "계속 비가 오락가락하네요",
          "original_form": "계속 비가 오락가락하네요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015336.1.3",
          "form": "name1님 지역은요?",
          "original_form": "&name1&님 지역은요?",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015336.1.4",
          "form": "저희 지역은 오늘 맑았어요",
          "original_form": "저희 지역은 오늘 맑았어요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015336.1.5",
          "form": "약간 덥더라구요",
          "original_form": "약간 덥더라구요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015336.1.6",
          "form": "비오셨으면 추우셨어요?",
          "original_form": "비오셨으면 추우셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015336.1.7",
          "form": "아뇨 적당히 선선해서 좋았어요",
          "original_form": "아뇨 적당히 선선해서 좋았어요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015336.1.8",
          "form": "아직은 반팔 날씨네요",
          "original_form": "아직은 반팔 날씨네요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015336.1.9",
          "form": "그럼 오늘 아예 비가 안 왔어요?",
          "original_form": "그럼 오늘 아예 비가 안 왔어요?",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015336.1.10",
          "form": "네 아예 안왔어요",
          "original_form": "네 아예 안왔어요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015336.1.11",
          "form": "요즘 날씨가 오락가락해서 옷입기 애매한거 같아요",
          "original_form": "요즘 날씨가 오락가락해서 옷입기 애매한거 같아요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015336.1.12",
          "form": "긴팔 가디건 가방에 챙겨다녀요",
          "original_form": "긴팔 가디건 가방에 챙겨다녀요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015336.1.13",
          "form": "그런데 밖보다 대중교통 때 걸치는 것같아요",
          "original_form": "그런데 밖보다 대중교통 때 걸치는 것같아요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015336.1.14",
          "form": "그쵸 에어컨틀어져있는데가 애매해요",
          "original_form": "그쵸 에어컨틀어져있는데가 애매해요",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015336.1.15",
          "form": "회사는 춥지않으세요?",
          "original_form": "회사는 춥지않으세요?",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015336.1.16",
          "form": "그나마 요새는 조금 나은데 예전에는 너무 빵빵하게 틀었죠",
          "original_form": "그나마 요새는 조금 나은데 예전에는 너무 빵빵하게 틀었죠",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015336.1.17",
          "form": "회사도 사실 좀 추워요ㅜ",
          "original_form": "회사도 사실 좀 추워요ㅜ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015336.1.18",
          "form": "저는 에어컨 직빵인 자리라 추워요ㅜ",
          "original_form": "저는 에어컨 직빵인 자리라 추워요ㅜ",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015336.1.19",
          "form": "그나마 요즘에는 껐다켰다하네요",
          "original_form": "그나마 요즘에는 껐다켰다하네요",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015336.1.20",
          "form": "자리 이사할 때 에어컨자리 피하느라 동료끼리 눈치싸움했네요",
          "original_form": "자리 이사할 때 에어컨자리 피하느라 동료끼리 눈치싸움했네요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015336.1.21",
          "form": "전력대란이다 뭐다하며 아끼라 할 때 솔직히 좀 반가웠어요ㅡ",
          "original_form": "전력대란이다 뭐다하며 아끼라 할 때 솔직히 좀 반가웠어요ㅡ",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015336.1.22",
          "form": "ㅎㅎㅎ 그럼 요즘은 에어컨 안트세요?",
          "original_form": "ㅎㅎㅎ 그럼 요즘은 에어컨 안트세요?",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015336.1.23",
          "form": "집에서는 안 틀어요ㅎㅎ",
          "original_form": "집에서는 안 틀어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015336.1.24",
          "form": "name1님은 트시나요?",
          "original_form": "&name1&님은 트시나요?",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015336.1.25",
          "form": "저도 집에서는 안틀어요 선풍기만 써요",
          "original_form": "저도 집에서는 안틀어요 선풍기만 써요",
