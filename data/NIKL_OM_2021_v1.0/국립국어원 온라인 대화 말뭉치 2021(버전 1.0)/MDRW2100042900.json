      "utterance": [
        {
          "id": "MDRW2100042900.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100042900.1.2",
          "form": "닉네임이 마음에 들어서 말 걸어봤어요",
          "original_form": "닉네임이 마음에 들어서 말 걸어봤어요",
          "speaker_id": "1",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100042900.1.3",
          "form": "최고시네요",
          "original_form": "최고시네요",
          "speaker_id": "1",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100042900.1.4",
          "form": "아앟 안녕하세요ㅋㅋㅋ",
          "original_form": "아앟 안녕하세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042900.1.5",
          "form": "평소에 운동 하시나요?",
          "original_form": "평소에 운동 하시나요?",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042900.1.6",
          "form": "아니요ㅋㅋ그래도걷는건 많이걸어요",
          "original_form": "아니요ㅋㅋ그래도걷는건 많이걸어요",
          "speaker_id": "1",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042900.1.7",
          "form": "최근에 하루당 만오천보에서 이만보정도 걸은것같아요",
          "original_form": "최근에 하루당 만오천보에서 이만보정도 걸은것같아요",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042900.1.8",
          "form": "헉 엄청 많이 걸으시네요 따로 운동 안하셔도 되시겠는데요ㅎㅎ",
          "original_form": "헉 엄청 많이 걸으시네요 따로 운동 안하셔도 되시겠는데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100042900.1.9",
          "form": "요즘 조금씩 날이 선선해지고 있어서 저도 슬슬 걸어야겠네요",
          "original_form": "요즘 조금씩 날이 선선해지고 있어서 저도 슬슬 걸어야겠네요",
          "speaker_id": "2",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100042900.1.10",
          "form": "많이걷고 많이 먹습니당!!",
          "original_form": "많이걷고 많이 먹습니당!!",
          "speaker_id": "1",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100042900.1.11",
          "form": "name2님은",
          "original_form": "&name2&님은",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100042900.1.12",
          "form": "운동하시는것 있나요?",
          "original_form": "운동하시는것 있나요?",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100042900.1.13",
          "form": "전 요즘은 백신맞고 쉰다 어쩐다 하다가 아무것도 안하고 있긴한데 그 전까지는 가~끔씩 홈트.. 조금 했어요!!",
          "original_form": "전 요즘은 백신맞고 쉰다 어쩐다 하다가 아무것도 안하고 있긴한데 그 전까지는 가~끔씩 홈트.. 조금 했어요!!",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100042900.1.14",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100042900.1.15",
          "form": "그래도건강관리 하시다니 멋있네여",
          "original_form": "그래도건강관리 하시다니 멋있네여",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100042900.1.16",
          "form": "저도 어제 백신맞았어요!!",
          "original_form": "저도 어제 백신맞았어요!!",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100042900.1.17",
          "form": "자주는 안해서 효과는 거의 없었을거에요ㅎㅎㅎ",
          "original_form": "자주는 안해서 효과는 거의 없었을거에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100042900.1.18",
          "form": "아아 어제 맞으셨군요..!! 팔 안아프신가요?? 전 이틀째 꽤나 아파서 타이레놀 먹었었는데..!!",
          "original_form": "아아 어제 맞으셨군요..!! 팔 안아프신가요?? 전 이틀째 꽤나 아파서 타이레놀 먹었었는데..!!",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100042900.1.19",
          "form": "다행히 맞은부분만 아파서",
          "original_form": "다행히 맞은부분만 아파서",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100042900.1.20",
          "form": "일상생활에 지장은 없었어요!!!!",
          "original_form": "일상생활에 지장은 없었어요!!!!",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100042900.1.21",
          "form": "쌩쌩해서 오히려 주변인들이 당황스러워했답니다",
          "original_form": "쌩쌩해서 오히려 주변인들이 당황스러워했답니다",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100042900.1.22",
          "form": "헉 다행이네요!! 웬만한 사람들이 근육통 난다고 해서 나름 괜찮으셨다니 저도 신기하긴하네요ㅎㅎㅎ",
          "original_form": "헉 다행이네요!! 웬만한 사람들이 근육통 난다고 해서 나름 괜찮으셨다니 저도 신기하긴하네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100042900.1.23",
          "form": "주사맞은쪽은 멍든거마냥 아프더라고요",
          "original_form": "주사맞은쪽은 멍든거마냥 아프더라고요",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100042900.1.24",
          "form": "지금은 거의 다 괜찮ㅇ아진것같아요!!",
          "original_form": "지금은 거의 다 괜찮ㅇ아진것같아요!!",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100042900.1.25",
          "form": "금방 괜찮아 지셔서 다행이에요!!",
          "original_form": "금방 괜찮아 지셔서 다행이에요!!",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100042900.1.26",
          "form": "종류는 어떤거 맞으셨나요??",
          "original_form": "종류는 어떤거 맞으셨나요??",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100042900.1.27",
          "form": "전 화이자 맞았어요!!",
          "original_form": "전 화이자 맞았어요!!",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100042900.1.28",
          "form": "저두요!!!",
          "original_form": "저두요!!!",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100042900.1.29",
          "form": "보통 2차가 더  아프대서",
          "original_form": "보통 2차가 더  아프대서",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100042900.1.30",
          "form": "안아프길 바라는중이에요!",
          "original_form": "안아프길 바라는중이에요!",
          "speaker_id": "1",
