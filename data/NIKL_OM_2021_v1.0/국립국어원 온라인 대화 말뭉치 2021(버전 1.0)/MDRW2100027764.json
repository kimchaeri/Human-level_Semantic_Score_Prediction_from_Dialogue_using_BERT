      "utterance": [
        {
          "id": "MDRW2100027764.1.1",
          "form": "아까 밥먹는다고 했잖아 뭐먹었니",
          "original_form": "아까 밥먹는다고 했잖아 뭐먹었니",
          "speaker_id": "1",
          "time": "20210902 14:49"
        },
        {
          "id": "MDRW2100027764.1.2",
          "form": "냉면ㅋㅋㅋㅋㅋ",
          "original_form": "냉면ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027764.1.3",
          "form": "걍 그 육수랑 면사서 먹었는데",
          "original_form": "걍 그 육수랑 면사서 먹었는데",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027764.1.4",
          "form": "이건 진짜 맛없는듯",
          "original_form": "이건 진짜 맛없는듯",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027764.1.5",
          "form": "집에서 먹은거야?",
          "original_form": "집에서 먹은거야?",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027764.1.6",
          "form": "해먹은거?",
          "original_form": "해먹은거?",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027764.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027764.1.8",
          "form": "이열..",
          "original_form": "이열..",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027764.1.9",
          "form": "근데 cj이나 이런 곳에서 파는 거 먹어도 맛없기는 매한가지",
          "original_form": "근데 cj이나 이런 곳에서 파는 거 먹어도 맛없기는 매한가지",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027764.1.10",
          "form": "냉면 면은 잘못삶으면 넘 퍼지지않아?",
          "original_form": "냉면 면은 잘못삶으면 넘 퍼지지않아?",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027764.1.11",
          "form": "1분 30초 삶으면 되니까",
          "original_form": "1분 30초 삶으면 되니까",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027764.1.12",
          "form": "잘삶나보네",
          "original_form": "잘삶나보네",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027764.1.13",
          "form": "그냥 맛이 없어",
          "original_form": "그냥 맛이 없어",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027764.1.14",
          "form": "사먹어야겟어",
          "original_form": "사먹어야겟어",
          "speaker_id": "2",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027764.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ어느쪽이 맛없는거야?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ어느쪽이 맛없는거야?",
          "speaker_id": "1",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100027764.1.16",
          "form": "면? 육수?",
          "original_form": "면? 육수?",
          "speaker_id": "1",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100027764.1.17",
          "form": "둘다!",
          "original_form": "둘다!",
          "speaker_id": "2",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100027764.1.18",
          "form": "이런...",
          "original_form": "이런...",
          "speaker_id": "1",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100027764.1.19",
          "form": "갈비도 짜서 ㅠㅠㅠ",
          "original_form": "갈비도 짜서 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100027764.1.20",
          "form": "넌 뭐먹었어",
          "original_form": "넌 뭐먹었어",
          "speaker_id": "2",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100027764.1.21",
          "form": "잔 치 국 수",
          "original_form": "잔 치 국 수",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027764.1.22",
          "form": "오랜만에 먹어줬다 ㅎ",
          "original_form": "오랜만에 먹어줬다 ㅎ",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027764.1.23",
          "form": "잔치국수 맛있겠다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잔치국수 맛있겠다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027764.1.24",
          "form": "우리집에 소면도 많앗는데 이제 없어 왜업지",
          "original_form": "우리집에 소면도 많앗는데 이제 없어 왜업지",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027764.1.25",
          "form": "다 흡입하신겨",
          "original_form": "다 흡입하신겨",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027764.1.26",
          "form": "우리집 소면 많이 안묵는데",
          "original_form": "우리집 소면 많이 안묵는데",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027764.1.27",
          "form": "너무 슬프다",
          "original_form": "너무 슬프다",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027764.1.28",
