      "utterance": [
        {
          "id": "MDRW2100007352.1.1",
          "form": "케바케일까요",
          "original_form": "케바케일까요",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007352.1.2",
          "form": "무기계약직차별은...",
          "original_form": "무기계약직차별은...",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007352.1.3",
          "form": "아무래도 공채가 좀 더 안전하긴한데",
          "original_form": "아무래도 공채가 좀 더 안전하긴한데",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007352.1.4",
          "form": "요새는 공채 뽑는거 자체가 티오가 적어서ㅠㅠ",
          "original_form": "요새는 공채 뽑는거 자체가 티오가 적어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007352.1.5",
          "form": "사실 제가 학부때 세무사공부했는데 미련이 남아서",
          "original_form": "사실 제가 학부때 세무사공부했는데 미련이 남아서",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007352.1.6",
          "form": "무기로 들어가서 남는시간에 공부하면 어떨까하고ㅠㅠ",
          "original_form": "무기로 들어가서 남는시간에 공부하면 어떨까하고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007352.1.7",
          "form": "실례지만 나이",
          "original_form": "실례지만 나이",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007352.1.8",
          "form": "여쭤봐도 될까요?",
          "original_form": "여쭤봐도 될까요?",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007352.1.9",
          "form": "저 26이요 ㅋㅋ졸업을 늦게했어요 휴학때문에",
          "original_form": "저 26이요 ㅋㅋ졸업을 늦게했어요 휴학때문에",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007352.1.10",
          "form": "되게 애매해요 공기업인턴도하고 한국사도땄는데 세무사공부만 2년하고",
          "original_form": "되게 애매해요 공기업인턴도하고 한국사도땄는데 세무사공부만 2년하고",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007352.1.11",
          "form": "26이면 아직",
          "original_form": "26이면 아직",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007352.1.12",
          "form": "충분히 도전하셔도",
          "original_form": "충분히 도전하셔도",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007352.1.13",
          "form": "될 나이입니다ㅎㅎ",
          "original_form": "될 나이입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007352.1.14",
          "form": "저 여기 들어올때 30이었습니다!",
          "original_form": "저 여기 들어올때 30이었습니다!",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007352.1.15",
          "form": "헉...! 멋있어요진짜",
          "original_form": "헉...! 멋있어요진짜",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007352.1.16",
          "form": "저 학부때 인턴을 공기업에서 2달 했는데",
          "original_form": "저 학부때 인턴을 공기업에서 2달 했는데",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007352.1.17",
          "form": "그때 언니들 다 25살 26살 이래서",
          "original_form": "그때 언니들 다 25살 26살 이래서",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007352.1.18",
          "form": "제가 지금 그나이되니까 되게 쫄리네요ㅠ무섭고",
          "original_form": "제가 지금 그나이되니까 되게 쫄리네요ㅠ무섭고",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007352.1.19",
          "form": "음 제기준으로는",
          "original_form": "음 제기준으로는",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007352.1.20",
          "form": "그때 입사할때 33살 여자도 봤어요",
          "original_form": "그때 입사할때 33살 여자도 봤어요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007352.1.21",
          "form": "아직 충분히 어리세요^^",
          "original_form": "아직 충분히 어리세요^^",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007352.1.22",
          "form": "오... 근데 무기로 들어가서 이직하거나 공부하는 건 별로일까요?ㅠ",
          "original_form": "오... 근데 무기로 들어가서 이직하거나 공부하는 건 별로일까요?ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007352.1.23",
          "form": "도전하는게 맞는건가요...",
          "original_form": "도전하는게 맞는건가요...",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007352.1.24",
          "form": "여기서 이렇게 좋은분만나고 너무감사해요",
          "original_form": "여기서 이렇게 좋은분만나고 너무감사해요",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007352.1.25",
          "form": "저도 말씀 나누고 좋네요",
          "original_form": "저도 말씀 나누고 좋네요",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007352.1.26",
          "form": "음 정답이 없긴한데",
          "original_form": "음 정답이 없긴한데",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007352.1.27",
          "form": "저는 일단 들어가서 준비하는게",
          "original_form": "저는 일단 들어가서 준비하는게",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007352.1.28",
          "form": "맞다고 봅니다",
          "original_form": "맞다고 봅니다",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007352.1.29",
          "form": "ㅋㅋ사실 들어가서 준비하고싶은마음이 더커요!! 직장인 세시생들도 많구...",
          "original_form": "ㅋㅋ사실 들어가서 준비하고싶은마음이 더커요!! 직장인 세시생들도 많구...",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007352.1.30",
          "form": "다만 무기계약직이라는 신분이",
          "original_form": "다만 무기계약직이라는 신분이",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007352.1.31",
          "form": "친구들앞에서 너무부끄러울까봐...",
          "original_form": "친구들앞에서 너무부끄러울까봐...",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007352.1.32",
          "form": "네 그렇긴한데",
          "original_form": "네 그렇긴한데",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007352.1.33",
          "form": "에이 신분이 중요한게 아니고",
          "original_form": "에이 신분이 중요한게 아니고",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007352.1.34",
          "form": "직장인은 월급이 중요합니다",
          "original_form": "직장인은 월급이 중요합니다",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007352.1.35",
          "form": "ㅋㅋㅋㅋ그렇죠!!!",
          "original_form": "ㅋㅋㅋㅋ그렇죠!!!",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007352.1.36",
          "form": "뭔가 힘이 생겼어요",
          "original_form": "뭔가 힘이 생겼어요",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007352.1.37",
          "form": "무기계약직 들어가서 세무사되기!!!",
          "original_form": "무기계약직 들어가서 세무사되기!!!",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007352.1.38",
          "form": "너무너무감사해요",
          "original_form": "너무너무감사해요",
          "speaker_id": "1",
          "time": "20210822 01:42"
        }
      ]
