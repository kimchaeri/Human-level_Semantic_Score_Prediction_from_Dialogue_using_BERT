        },
        {
          "id": "MDRW2100044848.1.2",
          "form": "돈을 못벌기를 해 뭘하기를 해",
          "original_form": "돈을 못벌기를 해 뭘하기를 해",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.3",
          "form": "가오떨어죠",
          "original_form": "가오떨어죠",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.4",
          "form": "져",
          "original_form": "져",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.5",
          "form": "다 잘하면서",
          "original_form": "다 잘하면서",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.6",
          "form": "한국에서도 날아가",
          "original_form": "한국에서도 날아가",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.7",
          "form": "날갯짓읋 해줘",
          "original_form": "날갯짓읋 해줘",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.8",
          "form": "집둘기여 날갯짓을 ㅎ주시오",
          "original_form": "집둘기여 날갯짓을 ㅎ주시오",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.9",
          "form": "돌아가 제발",
          "original_form": "돌아가 제발",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.10",
          "form": "하긴 근데 돌아가도",
          "original_form": "하긴 근데 돌아가도",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ한국에 왜와?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ한국에 왜와?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.12",
          "form": "다시돌아갈거면",
          "original_form": "다시돌아갈거면",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044848.1.13",
          "form": "이제 방송도 못나올테고",
          "original_form": "이제 방송도 못나올테고",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044848.1.14",
          "form": "그래놓고 한국 까는거 개맘에안들어",
          "original_form": "그래놓고 한국 까는거 개맘에안들어",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044848.1.15",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044848.1.16",
          "form": "에스엠정신차려",
          "original_form": "에스엠정신차려",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044848.1.17",
          "form": "중국몽 할때가 아니갸",
          "original_form": "중국몽 할때가 아니갸",
          "speaker_id": "1",
