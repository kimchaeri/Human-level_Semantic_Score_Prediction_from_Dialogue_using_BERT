      "utterance": [
        {
          "id": "MDRW2100043882.1.1",
          "form": "건강하시죠.",
          "original_form": "건강하시죠.",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043882.1.2",
          "form": "아직은 그런데",
          "original_form": "아직은 그런데",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043882.1.3",
          "form": "잘 모르겠어요",
          "original_form": "잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043882.1.4",
          "form": "요즘은 나이랑 건강이 상관 없는거같아요",
          "original_form": "요즘은 나이랑 건강이 상관 없는거같아요",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043882.1.5",
          "form": "네. 젊으신 분들도 많이 아프시더라구요.\n관리하고 살아야지요.",
          "original_form": "네. 젊으신 분들도 많이 아프시더라구요.\n관리하고 살아야지요.",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043882.1.6",
          "form": "그런거같아요 ㅠ",
          "original_form": "그런거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043882.1.7",
          "form": "영양제 드시나요?",
          "original_form": "영양제 드시나요?",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043882.1.8",
          "form": "안먹었는데\n챙겨먹어야겠더라구요",
          "original_form": "안먹었는데\n챙겨먹어야겠더라구요",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043882.1.9",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043882.1.10",
          "form": "요즘 D랑 칼슘 마그네슘은 다들 먹는 분위기더라구요",
          "original_form": "요즘 D랑 칼슘 마그네슘은 다들 먹는 분위기더라구요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043882.1.11",
          "form": "유산균도 먹으라고 하더라구요.\n귀걸이 좋아하세요?",
          "original_form": "유산균도 먹으라고 하더라구요.\n귀걸이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043882.1.12",
          "form": "아 맞아요 유산균도 좋다는데 종류가 너무 많아서 아직 모르겠어요",
          "original_form": "아 맞아요 유산균도 좋다는데 종류가 너무 많아서 아직 모르겠어요",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043882.1.13",
          "form": "영양제 고르는 것도 힘들지요.",
          "original_form": "영양제 고르는 것도 힘들지요.",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043882.1.14",
          "form": "귀걸이 좋아는 하는데",
          "original_form": "귀걸이 좋아는 하는데",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043882.1.15",
          "form": "머리카락에 자꾸 걸려서",
          "original_form": "머리카락에 자꾸 걸려서",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043882.1.16",
          "form": "그냥 젤 작은 링만 해요",
          "original_form": "그냥 젤 작은 링만 해요",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043882.1.17",
          "form": "저도 문신처럼 작은 링만 하네요.ㅎ",
          "original_form": "저도 문신처럼 작은 링만 하네요.ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043882.1.18",
          "form": "그쵸 가끔은 까먹고 끼고 자기도 하고 ㅋㅋㅋ",
          "original_form": "그쵸 가끔은 까먹고 끼고 자기도 하고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043882.1.19",
          "form": "편한게 제일인거갘아요",
          "original_form": "편한게 제일인거갘아요",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043882.1.20",
          "form": "네. 반지는 끼고 다니세요?",
          "original_form": "네. 반지는 끼고 다니세요?",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043882.1.21",
          "form": "반지 있는데 잘 끼진 않아요",
          "original_form": "반지 있는데 잘 끼진 않아요",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043882.1.22",
          "form": "끼고다니다가 하나 잊어버려서 ㅠ",
          "original_form": "끼고다니다가 하나 잊어버려서 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:59"
        }
      ]
