      "utterance": [
        {
          "id": "MDRW2100041591.1.1",
          "form": "게장이요?",
          "original_form": "게장이요?",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100041591.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100041591.1.3",
          "form": "음 간장게장은 별로요",
          "original_form": "음 간장게장은 별로요",
          "speaker_id": "2",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041591.1.4",
          "form": "아 왜요?",
          "original_form": "아 왜요?",
          "speaker_id": "1",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041591.1.5",
          "form": "짜기만 하고 맛을 잘 모르겠던데..",
          "original_form": "짜기만 하고 맛을 잘 모르겠던데..",
          "speaker_id": "2",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041591.1.6",
          "form": "그럼 양념 게장은 좋아하세요?",
          "original_form": "그럼 양념 게장은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041591.1.7",
          "form": "양념게장은 양념맛이 맛있어서 좋아해요 ㅎㅎ",
          "original_form": "양념게장은 양념맛이 맛있어서 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041591.1.8",
          "form": "저는 간장게장도 좋아하는데 맛있는집을 찾기가 어렵긴 해요",
          "original_form": "저는 간장게장도 좋아하는데 맛있는집을 찾기가 어렵긴 해요",
          "speaker_id": "1",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041591.1.9",
          "form": "저도 맛있는곳에서 안먹어봐서 그런걸지도",
          "original_form": "저도 맛있는곳에서 안먹어봐서 그런걸지도",
          "speaker_id": "2",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041591.1.10",
          "form": "맞아요! 양념게장은 양념맛이 거기서 거기라 ㅋㅋ",
          "original_form": "맞아요! 양념게장은 양념맛이 거기서 거기라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:00"
        },
        {
          "id": "MDRW2100041591.1.11",
          "form": "그쵸 매콤하고 달콤하고 ㅋㅋㅋ근데 손을 너무 많이 배려서",
          "original_form": "그쵸 매콤하고 달콤하고 ㅋㅋㅋ근데 손을 너무 많이 배려서",
          "speaker_id": "2",
          "time": "20210906 12:00"
        },
        {
          "id": "MDRW2100041591.1.12",
          "form": "요즘은 그래서 순살양념게장같은거도 팔던데요!",
          "original_form": "요즘은 그래서 순살양념게장같은거도 팔던데요!",
          "speaker_id": "1",
          "time": "20210906 12:00"
        },
        {
          "id": "MDRW2100041591.1.13",
          "form": "오 그래요? 신기하네요",
          "original_form": "오 그래요? 신기하네요",
          "speaker_id": "2",
          "time": "20210906 12:00"
