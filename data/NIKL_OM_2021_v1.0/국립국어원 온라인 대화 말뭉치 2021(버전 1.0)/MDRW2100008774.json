      "utterance": [
        {
          "id": "MDRW2100008774.1.1",
          "form": "저녁 메뉴는 뭐에요?",
          "original_form": "저녁 메뉴는 뭐에요?",
          "speaker_id": "1",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100008774.1.2",
          "form": "저는 안 먹으려고요",
          "original_form": "저는 안 먹으려고요",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008774.1.3",
          "form": "진짜 하루종일 뭘 먹어서 ㅋㅋ",
          "original_form": "진짜 하루종일 뭘 먹어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008774.1.4",
          "form": "헉 저녁을 안드신다구요?",
          "original_form": "헉 저녁을 안드신다구요?",
          "speaker_id": "1",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008774.1.5",
          "form": "저녁 안먹으면 잠이 안오지 않나용?",
          "original_form": "저녁 안먹으면 잠이 안오지 않나용?",
          "speaker_id": "1",
          "time": "20210822 18:33"
        },
        {
          "id": "MDRW2100008774.1.6",
          "form": "직전까지 뭘 먹었거든요",
          "original_form": "직전까지 뭘 먹었거든요",
          "speaker_id": "2",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100008774.1.7",
          "form": "너무 많이 먹어서",
          "original_form": "너무 많이 먹어서",
          "speaker_id": "2",
          "time": "20210822 18:39"
        },
        {
          "id": "MDRW2100008774.1.8",
          "form": "지금 배터질거같아요 ㅋㅋ",
          "original_form": "지금 배터질거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:39"
        },
        {
          "id": "MDRW2100008774.1.9",
          "form": "아항ㅠ 그럼 저녁은 패스하는걸로",
          "original_form": "아항ㅠ 그럼 저녁은 패스하는걸로",
          "speaker_id": "1",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008774.1.10",
          "form": "저는 골비빔면 하는 중이에요 ㅋㅋ",
          "original_form": "저는 골비빔면 하는 중이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008774.1.11",
          "form": "오 맛있겟어요 전 운동중이에요",
          "original_form": "오 맛있겟어요 전 운동중이에요",
          "speaker_id": "2",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008774.1.12",
          "form": "오 운동 하시면은",
          "original_form": "오 운동 하시면은",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008774.1.13",
          "form": "다이어트 식단도 병행하세요?",
          "original_form": "다이어트 식단도 병행하세요?",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008774.1.14",
          "form": "말만 다이어트 다이어트하고",
          "original_form": "말만 다이어트 다이어트하고",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008774.1.15",
          "form": "어제는 마카롱 오늘은 아이스크림 먹었어요ㅜ",
          "original_form": "어제는 마카롱 오늘은 아이스크림 먹었어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008774.1.16",
          "form": "앜ㅋㅋㅋ 마카롱이면 칼로리가ㄷㄷ",
          "original_form": "앜ㅋㅋㅋ 마카롱이면 칼로리가ㄷㄷ",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008774.1.17",
          "form": "근데 달달구리는 끊기가 힘들죵",
          "original_form": "근데 달달구리는 끊기가 힘들죵",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008774.1.18",
          "form": "아니 마카롱이 한개에 천원이래서 ㅜ",
          "original_form": "아니 마카롱이 한개에 천원이래서 ㅜ",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008774.1.19",
          "form": "뚱카롱은 2천원익ᆢ",
          "original_form": "뚱카롱은 2천원익ᆢ",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008774.1.20",
          "form": "이고",
          "original_form": "이고",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008774.1.21",
          "form": "가격 무엇ㅋㅋㅋㅋ",
          "original_form": "가격 무엇ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008774.1.22",
          "form": "쟁여템각인데요",
          "original_form": "쟁여템각인데요",
          "speaker_id": "1",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008774.1.23",
          "form": "그래서 만원어치샀는데",
          "original_form": "그래서 만원어치샀는데",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008774.1.24",
          "form": "아이스팩이 오백원이라는거에요",
          "original_form": "아이스팩이 오백원이라는거에요",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008774.1.25",
          "form": "딱만원짜리 한장들고갔는데",
          "original_form": "딱만원짜리 한장들고갔는데",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008774.1.26",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008774.1.27",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008774.1.28",
          "form": "헉... 어떻게 하셨어요? ㅋㅋ",
          "original_form": "헉... 어떻게 하셨어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008774.1.29",
          "form": "그냥 달라 그랬죠ㅋㅋ",
          "original_form": "그냥 달라 그랬죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100008774.1.30",
          "form": "근데 그게어제였잖아요",
          "original_form": "근데 그게어제였잖아요",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100008774.1.31",
          "form": "갑자기 비가 미친듯이 쏟아짐ㅜㅜ",
          "original_form": "갑자기 비가 미친듯이 쏟아짐ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100008774.1.32",
          "form": "ㅠㅠ 아이고 ㅠㅠ",
          "original_form": "ㅠㅠ 아이고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100008774.1.33",
          "form": "맙소사네요 진짜 ㅋㅋ",
          "original_form": "맙소사네요 진짜 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:04"
        },
        {
          "id": "MDRW2100008774.1.34",
          "form": "순간 너무 어이없을정도로 많이오는거에요",
          "original_form": "순간 너무 어이없을정도로 많이오는거에요",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008774.1.35",
          "form": "우산있어도 의미없을정도로? ㅋㅋ",
          "original_form": "우산있어도 의미없을정도로? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008774.1.36",
          "form": "느낌 아시겠죵? ㅋㅋ",
          "original_form": "느낌 아시겠죵? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008774.1.37",
          "form": "네ㅠㅠ 어제 유경험자로\r\n저도 그 느낌 잘 압니다ㅋㅋㅋㅋ",
          "original_form": "네ㅠㅠ 어제 유경험자로\r\n저도 그 느낌 잘 압니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100008774.1.38",
          "form": "그래도 마카롱은 무사했던거죠? ㅋㅋ",
          "original_form": "그래도 마카롱은 무사했던거죠? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100008774.1.39",
          "form": "주인도 안쓰러웠는지 큐알찍고 앉아있다가라더라고요",
          "original_form": "주인도 안쓰러웠는지 큐알찍고 앉아있다가라더라고요",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008774.1.40",
          "form": "아메리카노 하나주면서",
          "original_form": "아메리카노 하나주면서",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008774.1.41",
          "form": "원래 테이크아웃 전문점인데",
          "original_form": "원래 테이크아웃 전문점인데",
          "speaker_id": "2",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100008774.1.42",
          "form": "그래도 사장님이 친절하시네요^^",
          "original_form": "그래도 사장님이 친절하시네요^^",
          "speaker_id": "1",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100008774.1.43",
          "form": "그래서 커피와 함께 먹기시작했는데 다먹었어요 그자리에서ㅜㅜ",
          "original_form": "그래서 커피와 함께 먹기시작했는데 다먹었어요 그자리에서ㅜㅜ",
          "speaker_id": "2",
