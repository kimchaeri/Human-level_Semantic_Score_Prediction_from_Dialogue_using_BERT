      "utterance": [
        {
          "id": "MDRW2100019123.1.1",
          "form": "그래서 사이즈 안맞으면",
          "original_form": "그래서 사이즈 안맞으면",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019123.1.2",
          "form": "엄청 불편할듯",
          "original_form": "엄청 불편할듯",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019123.1.3",
          "form": "난 내가 귀가 작아서 아픈줄알았는데",
          "original_form": "난 내가 귀가 작아서 아픈줄알았는데",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019123.1.4",
          "form": "나보다 귀큰 아빠도 오래끼면 아프다더라구",
          "original_form": "나보다 귀큰 아빠도 오래끼면 아프다더라구",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019123.1.5",
          "form": "ㅇㅇ 나도 오래 끼면 귀아파",
          "original_form": "ㅇㅇ 나도 오래 끼면 귀아파",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019123.1.6",
          "form": "그래서 사실 충전기 잃어버렸는데",
          "original_form": "그래서 사실 충전기 잃어버렸는데",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019123.1.7",
          "form": "이어폰 디톡스 할 겸 구매 아직 안하고 있음",
          "original_form": "이어폰 디톡스 할 겸 구매 아직 안하고 있음",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019123.1.8",
          "form": "당근마켓에서 실시간 시세만 좀 알아보고 ㅋㅋㅋㅋ",
          "original_form": "당근마켓에서 실시간 시세만 좀 알아보고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019123.1.9",
          "form": "아 근데 블투이어폰 ㄹㅇ불편해",
          "original_form": "아 근데 블투이어폰 ㄹㅇ불편해",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019123.1.10",
          "form": "왜 내가 이어폰까지 충전시켜가면서 써야하는거야",
          "original_form": "왜 내가 이어폰까지 충전시켜가면서 써야하는거야",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019123.1.11",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019123.1.12",
          "form": "그리고 옆으로 누워서 끼고있으면",
          "original_form": "그리고 옆으로 누워서 끼고있으면",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019123.1.13",
          "form": "아 눌리는 느낌 나지 ㅋㅋㅋㅋ",
          "original_form": "아 눌리는 느낌 나지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.14",
          "form": "눌려서 다음으로 넘어가거나 빠져",
          "original_form": "눌려서 다음으로 넘어가거나 빠져",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.15",
          "form": "그거 웨어러블 앱어서",
          "original_form": "그거 웨어러블 앱어서",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.16",
          "form": "잠금할 수 있음",
          "original_form": "잠금할 수 있음",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.17",
          "form": "글킨한데",
          "original_form": "글킨한데",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.18",
          "form": "그거하는 재미로 쓰는건데",
          "original_form": "그거하는 재미로 쓰는건데",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.19",
          "form": "안하면 뭔가",
          "original_form": "안하면 뭔가",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.20",
          "form": "빠진 늑김이야...",
          "original_form": "빠진 늑김이야...",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.22",
          "form": "누워있을 때만 잠깐 잠가두면 되잖아",
          "original_form": "누워있을 때만 잠깐 잠가두면 되잖아",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019123.1.23",
          "form": "난 ㄹㅇ",
          "original_form": "난 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019123.1.24",
          "form": "100퍼센트 도로 켜는거 잊어버릴 자신 있음",
          "original_form": "100퍼센트 도로 켜는거 잊어버릴 자신 있음",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019123.1.25",
          "form": "ㅇㅏ 졸린데? 하고 걍 빼놓고 잘듯",
          "original_form": "ㅇㅏ 졸린데? 하고 걍 빼놓고 잘듯",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019123.1.26",
          "form": "나의 기억력에 그쪽으로는 크게 신뢰가 있어",
          "original_form": "나의 기억력에 그쪽으로는 크게 신뢰가 있어",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019123.1.27",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019123.1.28",
          "form": "그럴 수 있긴 하지 ㅋㅋㅋㅋㅋ",
          "original_form": "그럴 수 있긴 하지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019123.1.29",
          "form": "그리고 좀 잘 빠지기도 해서",
          "original_form": "그리고 좀 잘 빠지기도 해서",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019123.1.30",
          "form": "걍 누워서는 잘 안써",
          "original_form": "걍 누워서는 잘 안써",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019123.1.31",
          "form": "나도 집에서는 이어폰 안씀",
          "original_form": "나도 집에서는 이어폰 안씀",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019123.1.32",
          "form": "난 새벽에 좀 써",
          "original_form": "난 새벽에 좀 써",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019123.1.33",
          "form": "아하 새벽에",
          "original_form": "아하 새벽에",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019123.1.34",
          "form": "집안일을 할때는",
          "original_form": "집안일을 할때는",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019123.1.35",
          "form": "로오동용 노래가 필요한데",
          "original_form": "로오동용 노래가 필요한데",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019123.1.36",
          "form": "엄마가 시끄럽대서",
          "original_form": "엄마가 시끄럽대서",
          "speaker_id": "2",
          "time": "20210827 15:33"
        }
      ]
    }
