      "utterance": [
        {
          "id": "MDRW2100019924.1.1",
          "form": "맥주?",
          "original_form": "맥주?",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019924.1.2",
          "form": "맥주는 요즘 많이안먹음",
          "original_form": "맥주는 요즘 많이안먹음",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.3",
          "form": "왜냐?",
          "original_form": "왜냐?",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.4",
          "form": "맥주는 담날 숙취있어서",
          "original_form": "맥주는 담날 숙취있어서",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.5",
          "form": "맨날먹던데",
          "original_form": "맨날먹던데",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.6",
          "form": "알딸딸하게 먹으려면 4병마셔야하는데",
          "original_form": "알딸딸하게 먹으려면 4병마셔야하는데",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.7",
          "form": "무슨소리",
          "original_form": "무슨소리",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.8",
          "form": "4병마시면 배너무부르고",
          "original_form": "4병마시면 배너무부르고",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.9",
          "form": "숙취있다고? 알콜도수 안높지 않나",
          "original_form": "숙취있다고? 알콜도수 안높지 않나",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.10",
          "form": "담날 머리좀 아픔",
          "original_form": "담날 머리좀 아픔",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.11",
          "form": "그럼 의미없음",
          "original_form": "그럼 의미없음",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.12",
          "form": "오키?",
          "original_form": "오키?",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.13",
          "form": "그래 왜 4병씩 마심",
          "original_form": "그래 왜 4병씩 마심",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.14",
          "form": "소주를 먹어야지 소주1병만 빨리마시면",
          "original_form": "소주를 먹어야지 소주1병만 빨리마시면",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.15",
          "form": "알딸딸함",
          "original_form": "알딸딸함",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.16",
          "form": "ㅇ?",
          "original_form": "ㅇ?",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.17",
          "form": "ㅇㅋ?",
          "original_form": "ㅇㅋ?",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.18",
          "form": "한캔씩만 먹으면되지",
          "original_form": "한캔씩만 먹으면되지",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019924.1.19",
          "form": "옌날엔 그래 묵읏는데",
          "original_form": "옌날엔 그래 묵읏는데",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019924.1.20",
          "form": "소주 많이 마시지마라",
          "original_form": "소주 많이 마시지마라",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019924.1.21",
          "form": "알콜중독자 된다",
          "original_form": "알콜중독자 된다",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019924.1.22",
          "form": "맥주 안먹은지 오래됫음",
          "original_form": "맥주 안먹은지 오래됫음",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019924.1.23",
          "form": "치킨 우라빠 시켜라고?",
          "original_form": "치킨 우라빠 시켜라고?",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019924.1.24",
          "form": "배송비 넘아깝다..",
          "original_form": "배송비 넘아깝다..",
          "speaker_id": "1",
          "time": "20210827 22:55"
