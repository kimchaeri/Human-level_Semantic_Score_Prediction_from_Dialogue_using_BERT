      "utterance": [
        {
          "id": "MDRW2100039885.1.1",
          "form": "주단태가 너무 욕심을 부렸어",
          "original_form": "주단태가 너무 욕심을 부렸어",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039885.1.2",
          "form": "건물이 터지는데 쟤네는 멀쩡하네",
          "original_form": "건물이 터지는데 쟤네는 멀쩡하네",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039885.1.3",
          "form": "스케일이 크다ㅋㅋㅋㅋ 무슨 재난영화인줄",
          "original_form": "스케일이 크다ㅋㅋㅋㅋ 무슨 재난영화인줄",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039885.1.4",
          "form": "아니 로건은 얼마나 부자인거야",
          "original_form": "아니 로건은 얼마나 부자인거야",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039885.1.5",
          "form": "10조 뭐야 미국재벌 재산이 1조라는데ㅋㅋㅋㅋ",
          "original_form": "10조 뭐야 미국재벌 재산이 1조라는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039885.1.6",
          "form": "이건희야???",
          "original_form": "이건희야???",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039885.1.7",
          "form": "아니 오윤희 천서진이 죽였어??!!",
          "original_form": "아니 오윤희 천서진이 죽였어??!!",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039885.1.8",
          "form": "안본사이에 무슨일이",
          "original_form": "안본사이에 무슨일이",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039885.1.9",
          "form": "천서진이 죽였어",
          "original_form": "천서진이 죽였어",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039885.1.10",
          "form": "은별이가 고생 많이하네",
          "original_form": "은별이가 고생 많이하네",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039885.1.11",
          "form": "주단태 죽고 끝날줄 알았더니",
          "original_form": "주단태 죽고 끝날줄 알았더니",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039885.1.12",
          "form": "다음주까지 하다니",
          "original_form": "다음주까지 하다니",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039885.1.13",
          "form": "더 나올얘기가 있나봐",
          "original_form": "더 나올얘기가 있나봐",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039885.1.14",
          "form": "다음주에 천서진한테 복수하고 끝나려나",
          "original_form": "다음주에 천서진한테 복수하고 끝나려나",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039885.1.15",
          "form": "천서진은 나쁜짓을 너무 많이 했어",
          "original_form": "천서진은 나쁜짓을 너무 많이 했어",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039885.1.16",
          "form": "망해야돼",
          "original_form": "망해야돼",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039885.1.17",
          "form": "이제 이거 끝나면 뭐 보나",
          "original_form": "이제 이거 끝나면 뭐 보나",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039885.1.18",
          "form": "그러니까 요즘 티비에서 재밌는걸  안해",
          "original_form": "그러니까 요즘 티비에서 재밌는걸  안해",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039885.1.19",
          "form": "드라마도 재미없고",
          "original_form": "드라마도 재미없고",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039885.1.20",
          "form": "저 비서는 뭔데 저렇게 충성하는거야ㅋㅋㅋㅋ",
          "original_form": "저 비서는 뭔데 저렇게 충성하는거야ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039885.1.21",
          "form": "돈을 많이 받나",
          "original_form": "돈을 많이 받나",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039885.1.22",
          "form": "그러니까 돈을 얼마나 받길래 배신을 안하나",
          "original_form": "그러니까 돈을 얼마나 받길래 배신을 안하나",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039885.1.23",
          "form": "둘이 사랑하나봐",
          "original_form": "둘이 사랑하나봐",
          "speaker_id": "1",
          "time": "20210903 22:47"
        }
      ]
    }
