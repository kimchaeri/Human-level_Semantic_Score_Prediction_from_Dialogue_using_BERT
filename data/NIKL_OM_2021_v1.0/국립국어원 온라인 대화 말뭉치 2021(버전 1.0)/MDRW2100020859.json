      "utterance": [
        {
          "id": "MDRW2100020859.1.1",
          "form": "좋아하는 식사메뉴있나요?",
          "original_form": "좋아하는 식사메뉴있나요?",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020859.1.2",
          "form": "저는 돈까스류 좋아해요",
          "original_form": "저는 돈까스류 좋아해요",
          "speaker_id": "1",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100020859.1.3",
          "form": "좋아하시는거 있으세요?",
          "original_form": "좋아하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100020859.1.4",
          "form": "어제 나혼자산다에 강민경? 김민경인가가",
          "original_form": "어제 나혼자산다에 강민경? 김민경인가가",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020859.1.5",
          "form": "납작만두먹는거 나오던데요",
          "original_form": "납작만두먹는거 나오던데요",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020859.1.6",
          "form": "오늘 바로 오픈마켓에 납작만두가 메인에 떠있더라구요 ㅎ",
          "original_form": "오늘 바로 오픈마켓에 납작만두가 메인에 떠있더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020859.1.7",
          "form": "납작만두  맛있겠어요.",
          "original_form": "납작만두  맛있겠어요.",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020859.1.8",
          "form": "전  곱창좋아해요.곱창넣은 순대볶음도 좋아요",
          "original_form": "전  곱창좋아해요.곱창넣은 순대볶음도 좋아요",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020859.1.9",
          "form": "아 저두 막창곱창대창 다좋아해요",
          "original_form": "아 저두 막창곱창대창 다좋아해요",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020859.1.10",
          "form": "며칠전에 백순대에 곱창들어가있는거 주문했어요 ㅎ",
          "original_form": "며칠전에 백순대에 곱창들어가있는거 주문했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020859.1.11",
          "form": "저두요~~왜이리 맛있을 까요",
          "original_form": "저두요~~왜이리 맛있을 까요",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020859.1.12",
          "form": "글쵸? 그리고 순대국도 같이 시켰어요",
          "original_form": "글쵸? 그리고 순대국도 같이 시켰어요",
          "speaker_id": "1",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100020859.1.13",
          "form": "무봉리순대국 아세요?",
          "original_form": "무봉리순대국 아세요?",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020859.1.14",
          "form": "네. 근처에는 없지만 예전 동네에서 가봤어요",
          "original_form": "네. 근처에는 없지만 예전 동네에서 가봤어요",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020859.1.15",
          "form": "집근처엔 신의주",
          "original_form": "집근처엔 신의주",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020859.1.16",
          "form": "저는 거기꺼가 맛나더라구요",
          "original_form": "저는 거기꺼가 맛나더라구요",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020859.1.17",
          "form": "무봉리요?",
          "original_form": "무봉리요?",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020859.1.18",
          "form": "네 무봉리순대국",
          "original_form": "네 무봉리순대국",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020859.1.19",
          "form": "맛있죠. 요즘 초밥이 땡겨요",
          "original_form": "맛있죠. 요즘 초밥이 땡겨요",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020859.1.20",
          "form": "저는 날것을 안먹어서 초밥을 안먹어요",
          "original_form": "저는 날것을 안먹어서 초밥을 안먹어요",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020859.1.21",
          "form": "맛나나요?",
          "original_form": "맛나나요?",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020859.1.22",
          "form": "가끔 대형마트  덴리 코너 초밥 사서 먹는데 맛있어요",
          "original_form": "가끔 대형마트  덴리 코너 초밥 사서 먹는데 맛있어요",
          "speaker_id": "2",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020859.1.23",
          "form": "델리",
          "original_form": "델리",
          "speaker_id": "2",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020859.1.24",
          "form": "아 많이들 사시더라구요",
          "original_form": "아 많이들 사시더라구요",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020859.1.25",
          "form": "와사비맛?ㅎ",
          "original_form": "와사비맛?ㅎ",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020859.1.26",
          "form": "초밥은 고추냉이맛이군요. ㅋㅋ 전에는 초밥이 맛있는줄 몰랐는데 먹다보니 맛있어요",
          "original_form": "초밥은 고추냉이맛이군요. ㅋㅋ 전에는 초밥이 맛있는줄 몰랐는데 먹다보니 맛있어요",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020859.1.27",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100020859.1.28",
          "form": "그게 먹다보면 먹는 밥양보다 많이 먹게된다고하더라구요",
          "original_form": "그게 먹다보면 먹는 밥양보다 많이 먹게된다고하더라구요",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020859.1.29",
          "form": "김밥처럼  탄수화물을 많이먹게되죠",
          "original_form": "김밥처럼  탄수화물을 많이먹게되죠",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020859.1.30",
          "form": "탄수화뮨 ㅜㅠ",
          "original_form": "탄수화뮨 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020859.1.31",
          "form": "저 며칠전에 탄수화물줄이겠다고",
          "original_form": "저 며칠전에 탄수화물줄이겠다고",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020859.1.32",
          "form": "밥을 덜펐더니",
          "original_form": "밥을 덜펐더니",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020859.1.33",
          "form": "점심먹은지 몇시간후..",
          "original_form": "점심먹은지 몇시간후..",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020859.1.34",
          "form": "배가 급격하게 고파지더라구요 ㅜㅠ",
          "original_form": "배가 급격하게 고파지더라구요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 14:01"
        }
      ]
    }
