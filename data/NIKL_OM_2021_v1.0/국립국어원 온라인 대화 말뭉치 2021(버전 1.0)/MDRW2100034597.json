        },
        {
          "id": "MDRW2100034597.1.2",
          "form": "오 고단도 댄스",
          "original_form": "오 고단도 댄스",
          "speaker_id": "2",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034597.1.3",
          "form": "얘는 누군지 알겠다ㅋㅋ",
          "original_form": "얘는 누군지 알겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034597.1.4",
          "form": "누군데",
          "original_form": "누군데",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034597.1.5",
          "form": "그룹에서 솔로 데뷔가",
          "original_form": "그룹에서 솔로 데뷔가",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034597.1.6",
          "form": "아이즈원 멤버 아까 그 민주랑 같은 멤버였음",
          "original_form": "아이즈원 멤버 아까 그 민주랑 같은 멤버였음",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034597.1.7",
          "form": "아~솔로데뷔한거네",
          "original_form": "아~솔로데뷔한거네",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034597.1.8",
          "form": "그러네 얘는 솔로 데뷔 시켜 줬네",
          "original_form": "그러네 얘는 솔로 데뷔 시켜 줬네",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034597.1.9",
          "form": "그래서 엄청 길게 잡아주네 카메라가",
          "original_form": "그래서 엄청 길게 잡아주네 카메라가",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034597.1.10",
          "form": "10위 안에는 아는 가수 좀 있네",
          "original_form": "10위 안에는 아는 가수 좀 있네",
          "speaker_id": "1",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034597.1.11",
          "form": "오10cm 무한도전 때문에 안다",
          "original_form": "오10cm 무한도전 때문에 안다",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034597.1.12",
          "form": "무도가 키워졌지",
          "original_form": "무도가 키워졌지",
          "speaker_id": "1",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034597.1.13",
          "form": "엉 근데 말을 못하니 더 클수있었는데",
          "original_form": "엉 근데 말을 못하니 더 클수있었는데",
          "speaker_id": "2",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034597.1.14",
          "form": "노래도 아메 밖에 없다",
          "original_form": "노래도 아메 밖에 없다",
          "speaker_id": "1",
          "time": "20210828 16:05"
