      "utterance": [
        {
          "id": "MDRW2100030382.1.1",
          "form": "학교는 어때 요즘?",
          "original_form": "학교는 어때 요즘?",
          "speaker_id": "1",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030382.1.2",
          "form": "전면등교래",
          "original_form": "전면등교래",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030382.1.3",
          "form": "가기싫어잉.",
          "original_form": "가기싫어잉.",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030382.1.4",
          "form": "헐 우리는 그래도",
          "original_form": "헐 우리는 그래도",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030382.1.5",
          "form": "이번 달은 원격이랑 등교 번갈아서 할거 같은..아닌가?",
          "original_form": "이번 달은 원격이랑 등교 번갈아서 할거 같은..아닌가?",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030382.1.6",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030382.1.7",
          "form": "우리는 수듄떨어진다고",
          "original_form": "우리는 수듄떨어진다고",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030382.1.8",
          "form": "전면한대",
          "original_form": "전면한대",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030382.1.9",
          "form": "와우...너무 힘둘겠다...",
          "original_form": "와우...너무 힘둘겠다...",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030382.1.10",
          "form": "프로그래밍인가 그건 아직도 그러니",
          "original_form": "프로그래밍인가 그건 아직도 그러니",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030382.1.11",
          "form": "프로그래밍?",
          "original_form": "프로그래밍?",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030382.1.12",
          "form": "아 컴그?",
          "original_form": "아 컴그?",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030382.1.13",
          "form": "여전하지 바뀌겠냐",
          "original_form": "여전하지 바뀌겠냐",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030382.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 컴그의 늪에서 빠져나오지 못하...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 컴그의 늪에서 빠져나오지 못하...",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030382.1.15",
          "form": "적어도 내년까지는 그럴듯.........",
          "original_form": "적어도 내년까지는 그럴듯.........",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030382.1.16",
          "form": "으....",
          "original_form": "으....",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030382.1.17",
          "form": "포토샵 이론을 모든 컴퓨터에 포토샵이 깔려있는 학교에서 수업할때가 아니라 서른명중 한명 컴퓨터에 있을까 말까한 온라인 수업때 한다니깐",
          "original_form": "포토샵 이론을 모든 컴퓨터에 포토샵이 깔려있는 학교에서 수업할때가 아니라 서른명중 한명 컴퓨터에 있을까 말까한 온라인 수업때 한다니깐",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030382.1.18",
          "form": "왜 등교때 안함...? 놀라운걸..",
          "original_form": "왜 등교때 안함...? 놀라운걸..",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030382.1.19",
          "form": "내말이,,,",
          "original_form": "내말이,,,",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030382.1.20",
          "form": "내년까지면 당신 졸업이잖아",
          "original_form": "내년까지면 당신 졸업이잖아",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030382.1.21",
          "form": "등교땐 다짜고짜 포토샵 기출문제 풀라그럼",
          "original_form": "등교땐 다짜고짜 포토샵 기출문제 풀라그럼",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030382.1.22",
          "form": "3학년땐 안하지 않을까 하는 작은 희망........",
          "original_form": "3학년땐 안하지 않을까 하는 작은 희망........",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030382.1.23",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030382.1.24",
          "form": "안했음 좋겠다",
          "original_form": "안했음 좋겠다",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030382.1.25",
          "form": "포토샵 기출문제 처음 들어본다...",
          "original_form": "포토샵 기출문제 처음 들어본다...",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030382.1.26",
          "form": "GTQ라고 자격증 시험인데",
          "original_form": "GTQ라고 자격증 시험인데",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030382.1.27",
          "form": "그게 90분인가? 안에 주어진 사진을 써서 이미지 만드는걸 네개를 해야해",
          "original_form": "그게 90분인가? 안에 주어진 사진을 써서 이미지 만드는걸 네개를 해야해",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030382.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030382.1.29",
          "form": "어렵네ㅜㅜㅜ",
          "original_form": "어렵네ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030382.1.30",
          "form": "하나도 아니고 4개...",
          "original_form": "하나도 아니고 4개...",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030382.1.31",
          "form": "근데 등교때는 포토샵 알려주지도 않아놓고 갑자기 하라그러질 않나",
          "original_form": "근데 등교때는 포토샵 알려주지도 않아놓고 갑자기 하라그러질 않나",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030382.1.32",
          "form": "온라인때나 이론수업 하고 앉았고",
          "original_form": "온라인때나 이론수업 하고 앉았고",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030382.1.33",
          "form": "시간절약이 최우선인데 한참을 삥 돌아가는 방식으로 설명하는 주제에",
          "original_form": "시간절약이 최우선인데 한참을 삥 돌아가는 방식으로 설명하는 주제에",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030382.1.34",
          "form": "1학년하고는 엄청 비교해댐",
          "original_form": "1학년하고는 엄청 비교해댐",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030382.1.35",
          "form": "헐...그게 학교니. ..",
          "original_form": "헐...그게 학교니. ..",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030382.1.36",
          "form": "그냥 자습하는게 더 더움이 될듯",
          "original_form": "그냥 자습하는게 더 더움이 될듯",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030382.1.37",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "2",
          "time": "20210904 01:00"
        }
      ]
    }
