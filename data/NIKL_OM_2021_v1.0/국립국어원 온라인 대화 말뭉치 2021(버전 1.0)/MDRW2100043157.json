      "utterance": [
        {
          "id": "MDRW2100043157.1.1",
          "form": "최근에 구매하신 물건이 있나용?",
          "original_form": "최근에 구매하신 물건이 있나용?",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043157.1.2",
          "form": "아 ㅋㅋ 그러게요ㅠㅠㅍ",
          "original_form": "아 ㅋㅋ 그러게요ㅠㅠㅍ",
          "speaker_id": "2",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100043157.1.3",
          "form": "저는 가을을 맞아..",
          "original_form": "저는 가을을 맞아..",
          "speaker_id": "2",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100043157.1.4",
          "form": "바지랑 아노락을 샀습니다 ㅎㅎㅎ",
          "original_form": "바지랑 아노락을 샀습니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043157.1.5",
          "form": "혹시 최근에 뭐 사신게 있나요?",
          "original_form": "혹시 최근에 뭐 사신게 있나요?",
          "speaker_id": "2",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043157.1.6",
          "form": "ㅠㅜㅜㅠ",
          "original_form": "ㅠㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043157.1.7",
          "form": "아노락이 뭔가용?",
          "original_form": "아노락이 뭔가용?",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043157.1.8",
          "form": "저는 트레이닝 세트 사려고 생각중입니다",
          "original_form": "저는 트레이닝 세트 사려고 생각중입니다",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043157.1.9",
          "form": "오 트레이닝 세트 좋네요",
          "original_form": "오 트레이닝 세트 좋네요",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100043157.1.10",
          "form": "염두해두신 브랜드가 있으신가요?",
          "original_form": "염두해두신 브랜드가 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100043157.1.11",
          "form": "히프나틱이요!",
          "original_form": "히프나틱이요!",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043157.1.12",
          "form": "골반이 잘 잡히더라구요 ㄹㄹ",
          "original_form": "골반이 잘 잡히더라구요 ㄹㄹ",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043157.1.13",
          "form": "오 ㅎㅎ 방금 서치해봤는데 예쁘네요",
          "original_form": "오 ㅎㅎ 방금 서치해봤는데 예쁘네요",
          "speaker_id": "2",
          "time": "20210907 17:59"
        },
        {
          "id": "MDRW2100043157.1.14",
          "form": "트레이닝복을 사야하나...",
          "original_form": "트레이닝복을 사야하나...",
          "speaker_id": "2",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100043157.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100043157.1.16",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043157.1.17",
          "form": "여자분이신거조??",
          "original_form": "여자분이신거조??",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043157.1.18",
          "form": "넵넵 맞습니다 ㅎㅎ",
          "original_form": "넵넵 맞습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043157.1.19",
          "form": "혹시 샐러드 같은 식품을 구독해보신적 있으신가요? ㅎㅎ",
          "original_form": "혹시 샐러드 같은 식품을 구독해보신적 있으신가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043157.1.20",
          "form": "헉 구독해서 먹고싶은데 하고계신가요?",
          "original_form": "헉 구독해서 먹고싶은데 하고계신가요?",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043157.1.21",
          "form": "아 저 이번에 처음으로 2주분만 신청해봤어요!",
          "original_form": "아 저 이번에 처음으로 2주분만 신청해봤어요!",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043157.1.22",
          "form": "오오 어디서 구매하셨나요?",
          "original_form": "오오 어디서 구매하셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043157.1.23",
          "form": "음 야구르트에서 하는 프레딧에서 했습니다 ㅎㅎ 하루에 하나씩 가져다주시더라구요",
          "original_form": "음 야구르트에서 하는 프레딧에서 했습니다 ㅎㅎ 하루에 하나씩 가져다주시더라구요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043157.1.24",
          "form": "샐러드를 그렇게 좋아하지는 않는데..안쌓이게 하려고 챙겨먹게되는것같아요 ㅎㅎ",
          "original_form": "샐러드를 그렇게 좋아하지는 않는데..안쌓이게 하려고 챙겨먹게되는것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043157.1.25",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ 맞아요 저도 그래요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ 맞아요 저도 그래요",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043157.1.26",
          "form": "하나에 얼마정도에요?",
          "original_form": "하나에 얼마정도에요?",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043157.1.27",
          "form": "ㅋㅋ음 쿠폰먹여서 사서 한 4000-5000원 정도 낸것같아요",
          "original_form": "ㅋㅋ음 쿠폰먹여서 사서 한 4000-5000원 정도 낸것같아요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043157.1.28",
          "form": "ㅋㅋㅋ안먹으면 부채감이 생기는..",
          "original_form": "ㅋㅋㅋ안먹으면 부채감이 생기는..",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043157.1.29",
          "form": "요새 편의점에서도 5000원정도에 샐러드 파는 것 같던데",
          "original_form": "요새 편의점에서도 5000원정도에 샐러드 파는 것 같던데",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043157.1.30",
          "form": "그거랑 비교하면 퀄리티는 좋은 것 같습니당",
          "original_form": "그거랑 비교하면 퀄리티는 좋은 것 같습니당",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043157.1.31",
          "form": "오오 그정도면 좋죠!",
          "original_form": "오오 그정도면 좋죠!",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043157.1.32",
          "form": "동네에서 포장해와도 6000원 넘어서요 요즘 ㅠㅠ",
          "original_form": "동네에서 포장해와도 6000원 넘어서요 요즘 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043157.1.33",
          "form": "아 맞아요 아무래도 비싸죠ㅠㅍㅍ",
          "original_form": "아 맞아요 아무래도 비싸죠ㅠㅍㅍ",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043157.1.34",
          "form": "사실 재료 소분해서 만들어 먹는게 제일 좋을텐데 ㅎㅎ저는 그러면 항상 남기더라구요..",
          "original_form": "사실 재료 소분해서 만들어 먹는게 제일 좋을텐데 ㅎㅎ저는 그러면 항상 남기더라구요..",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043157.1.35",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
