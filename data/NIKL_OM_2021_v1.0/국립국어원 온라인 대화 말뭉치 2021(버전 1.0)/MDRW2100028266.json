        },
        {
          "id": "MDRW2100028266.1.3",
          "form": "어렵다",
          "original_form": "어렵다",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028266.1.4",
          "form": "콜라보한 마케팅",
          "original_form": "콜라보한 마케팅",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028266.1.5",
          "form": "제품들 산적있오?",
          "original_form": "제품들 산적있오?",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028266.1.6",
          "form": "흠... 산적은 없는데",
          "original_form": "흠... 산적은 없는데",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028266.1.7",
          "form": "본적은 잇당",
          "original_form": "본적은 잇당",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028266.1.8",
          "form": "냐낭시는 어떠신가여",
          "original_form": "냐낭시는 어떠신가여",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028266.1.9",
          "form": "나두 사지는 않았지만 편의점에서",
          "original_form": "나두 사지는 않았지만 편의점에서",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028266.1.10",
          "form": "한번씩 봤다ㅋㅋㅋㅋ",
          "original_form": "한번씩 봤다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028266.1.11",
          "form": "곰표 맥주 완젼 인기 많자넝",
          "original_form": "곰표 맥주 완젼 인기 많자넝",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028266.1.12",
          "form": "그치 만이들먹던데",
          "original_form": "그치 만이들먹던데",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028266.1.13",
          "form": "난아직안먹어봣어",
          "original_form": "난아직안먹어봣어",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028266.1.14",
          "form": "담에같이먹어볼까",
          "original_form": "담에같이먹어볼까",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028266.1.15",
          "form": "핫 한 캔 다 마실지는 모르겠지만 좋아",
          "original_form": "핫 한 캔 다 마실지는 모르겠지만 좋아",
          "speaker_id": "1",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100028266.1.16",
          "form": "난 종이컵에 맛볼테니 내 몫도 마셔줘ㅎㅎ",
          "original_form": "난 종이컵에 맛볼테니 내 몫도 마셔줘ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100028266.1.17",
          "form": "고양이키우는 친구가 맛있다고 했던거 같아",
          "original_form": "고양이키우는 친구가 맛있다고 했던거 같아",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100028266.1.18",
          "form": "진로 두꺼비하니깐 울집에",
          "original_form": "진로 두꺼비하니깐 울집에",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100028266.1.19",
          "form": "진로 소주잔 있더라",
          "original_form": "진로 소주잔 있더라",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100028266.1.20",
          "form": "ㅋㅋㅋㅋㅋ그래",
          "original_form": "ㅋㅋㅋㅋㅋ그래",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028266.1.21",
          "form": "name1이네 사들고 가서 그걸루 마셔야하남",
          "original_form": "&name1&이네 사들고 가서 그걸루 마셔야하남",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028266.1.22",
          "form": "곰표맥주랑",
          "original_form": "곰표맥주랑",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028266.1.23",
          "form": "진로 떡볶이 조합 어때",
          "original_form": "진로 떡볶이 조합 어때",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028266.1.24",
          "form": "콜라보 마게팅 제품이 신박하고 잼있긴하지만",
          "original_form": "콜라보 마게팅 제품이 신박하고 잼있긴하지만",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028266.1.25",
          "form": "원래 제품이랑 달라서 아이들에게 위험하다는 뉴스를 본 적이 있어",
          "original_form": "원래 제품이랑 달라서 아이들에게 위험하다는 뉴스를 본 적이 있어",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028266.1.26",
          "form": "서울우유에서 바디워시를 냈다던가",
          "original_form": "서울우유에서 바디워시를 냈다던가",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028266.1.27",
          "form": "맞어맞어",
          "original_form": "맞어맞어",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028266.1.28",
          "form": "그 뭐더라 펜 잉크같은 음료 이런것두",
          "original_form": "그 뭐더라 펜 잉크같은 음료 이런것두",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028266.1.29",
          "form": "위험하다구하드라",
          "original_form": "위험하다구하드라",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028266.1.30",
          "form": "나도 그건좀..싶긴햇다",
          "original_form": "나도 그건좀..싶긴햇다",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028266.1.31",
          "form": "헉 잉크음료?",
          "original_form": "헉 잉크음료?",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028266.1.32",
          "form": "어린애들한테 위험해..",
          "original_form": "어린애들한테 위험해..",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028266.1.33",
          "form": "어른들도 언듯 보면 헷갈리는데 애들은 구별하기 힘들잖아",
          "original_form": "어른들도 언듯 보면 헷갈리는데 애들은 구별하기 힘들잖아",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028266.1.34",
          "form": "콜라보 마케팅이 기본제품과 다르게 색다르고 재밌지만 적절한 선에서 만들어줬음 좋겠다고생각해!",
          "original_form": "콜라보 마케팅이 기본제품과 다르게 색다르고 재밌지만 적절한 선에서 만들어줬음 좋겠다고생각해!",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028266.1.35",
          "form": "마자 먹을건 먹을거로만",
          "original_form": "마자 먹을건 먹을거로만",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028266.1.36",
          "form": "아님 적어두 모양이라도 먹을거처럼ㅜ",
          "original_form": "아님 적어두 모양이라도 먹을거처럼ㅜ",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028266.1.37",
          "form": "해야한다구봄",
          "original_form": "해야한다구봄",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028266.1.38",
          "form": "맞아맞앙ㅜ",
          "original_form": "맞아맞앙ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028266.1.39",
          "form": "아니면 앞에 주의를 적어야된다구 생각해",
          "original_form": "아니면 앞에 주의를 적어야된다구 생각해",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028266.1.40",
          "form": "앞으로도 재밌는 콜라보 마케팅이 많이 나오길-!",
          "original_form": "앞으로도 재밌는 콜라보 마케팅이 많이 나오길-!",
