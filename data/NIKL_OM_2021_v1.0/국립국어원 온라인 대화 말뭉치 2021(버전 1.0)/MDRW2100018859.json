      "utterance": [
        {
          "id": "MDRW2100018859.1.1",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018859.1.2",
          "form": "특히 가지",
          "original_form": "특히 가지",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018859.1.3",
          "form": "가지는 생으로 먹는건 저도 좋아하는데",
          "original_form": "가지는 생으로 먹는건 저도 좋아하는데",
          "speaker_id": "1",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018859.1.4",
          "form": "가지는 꼭 양념맛이 잘 베이게 조리된 것만 먹어요",
          "original_form": "가지는 꼭 양념맛이 잘 베이게 조리된 것만 먹어요",
          "speaker_id": "2",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018859.1.5",
          "form": "조리된거 드시는구나",
          "original_form": "조리된거 드시는구나",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018859.1.6",
          "form": "가지요리 어떤거 드세요",
          "original_form": "가지요리 어떤거 드세요",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018859.1.7",
          "form": "가지무침..?",
          "original_form": "가지무침..?",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018859.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018859.1.9",
          "form": "가지무침에 밥 먹는 거 제일 좋아해요",
          "original_form": "가지무침에 밥 먹는 거 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018859.1.10",
          "form": "참기름 꼭 넣어서...",
          "original_form": "참기름 꼭 넣어서...",
          "speaker_id": "2",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018859.1.11",
          "form": "맛을 아시는 분이시군요",
          "original_form": "맛을 아시는 분이시군요",
          "speaker_id": "1",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018859.1.12",
          "form": "그럼 좋아하는 반찬은 뭔가요",
          "original_form": "그럼 좋아하는 반찬은 뭔가요",
          "speaker_id": "1",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018859.1.13",
          "form": "저어는...",
          "original_form": "저어는...",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018859.1.14",
          "form": "생선구이를 제일 좋아해요",
          "original_form": "생선구이를 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018859.1.15",
          "form": "바다쪽에 살아서",
          "original_form": "바다쪽에 살아서",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018859.1.16",
          "form": "아~ 고등어 갈치 전어 이런거요?",
          "original_form": "아~ 고등어 갈치 전어 이런거요?",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018859.1.17",
          "form": "아빠가 낚시로 생선 잡아다가 구워주세요",
          "original_form": "아빠가 낚시로 생선 잡아다가 구워주세요",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018859.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018859.1.19",
          "form": "감성돔...",
          "original_form": "감성돔...",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.20",
          "form": "대박 완전 신선하겠다",
          "original_form": "대박 완전 신선하겠다",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.21",
          "form": "진짜 맛있어요",
          "original_form": "진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.22",
          "form": "돔을",
          "original_form": "돔을",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.23",
          "form": "구워서..?",
          "original_form": "구워서..?",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.24",
          "form": "돔수저시군요",
          "original_form": "돔수저시군요",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.25",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.26",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.27",
          "form": "그러네여",
          "original_form": "그러네여",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018859.1.28",
          "form": "저는 돔은 비싸서 못먹어봤거든요ㅋㅋ 어떤맛이던가요",
          "original_form": "저는 돔은 비싸서 못먹어봤거든요ㅋㅋ 어떤맛이던가요",
          "speaker_id": "1",
          "time": "20210827 13:05"
        }
      ]
