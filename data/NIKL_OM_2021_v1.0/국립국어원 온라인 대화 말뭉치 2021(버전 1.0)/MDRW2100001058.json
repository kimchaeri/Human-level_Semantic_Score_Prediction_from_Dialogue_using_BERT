      "utterance": [
        {
          "id": "MDRW2100001058.1.1",
          "form": "봉사나 이런쪽은 관심이 있나요?",
          "original_form": "봉사나 이런쪽은 관심이 있나요?",
          "speaker_id": "1",
          "time": "20210722 21:40"
        },
        {
          "id": "MDRW2100001058.1.2",
          "form": "가끔 후원같은거 보고는있어요",
          "original_form": "가끔 후원같은거 보고는있어요",
          "speaker_id": "2",
          "time": "20210722 21:41"
        },
        {
          "id": "MDRW2100001058.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210722 21:41"
        },
        {
          "id": "MDRW2100001058.1.4",
          "form": "어디쪽꺼요?",
          "original_form": "어디쪽꺼요?",
          "speaker_id": "1",
          "time": "20210722 21:42"
        },
        {
          "id": "MDRW2100001058.1.5",
          "form": "제가 개인으로는 못하고 아이돌팬들끼리 기념일날 모아서 하는걸 찾아보고 있어요",
          "original_form": "제가 개인으로는 못하고 아이돌팬들끼리 기념일날 모아서 하는걸 찾아보고 있어요",
          "speaker_id": "2",
          "time": "20210722 21:42"
        },
        {
          "id": "MDRW2100001058.1.6",
          "form": "아하 아이돌한테 후원그런거구나!",
          "original_form": "아하 아이돌한테 후원그런거구나!",
          "speaker_id": "1",
          "time": "20210722 21:42"
        },
        {
          "id": "MDRW2100001058.1.7",
          "form": "저는 사회복지쪽이어서",
          "original_form": "저는 사회복지쪽이어서",
          "speaker_id": "1",
          "time": "20210722 21:42"
        },
        {
          "id": "MDRW2100001058.1.8",
          "form": "봉사나 이런거 되게 다양하게 하거든요 ㅎㅎ",
          "original_form": "봉사나 이런거 되게 다양하게 하거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210722 21:42"
        },
        {
          "id": "MDRW2100001058.1.9",
          "form": "아이돌이름으로 단체에기부하는거 보고있어요",
          "original_form": "아이돌이름으로 단체에기부하는거 보고있어요",
          "speaker_id": "2",
          "time": "20210722 21:42"
        },
        {
          "id": "MDRW2100001058.1.10",
          "form": "예전에 부산강서구에서 봉사에대한 강이? 같은거 봤었어요",
          "original_form": "예전에 부산강서구에서 봉사에대한 강이? 같은거 봤었어요",
          "speaker_id": "2",
          "time": "20210722 21:43"
        },
        {
          "id": "MDRW2100001058.1.11",
          "form": "근데 그거 중1때 봉사시간때문에간거라 ㅋㅋ",
          "original_form": "근데 그거 중1때 봉사시간때문에간거라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 21:43"
        },
        {
          "id": "MDRW2100001058.1.12",
          "form": "아 그거 들으면 봉사시간같은거 주나봐요",
          "original_form": "아 그거 들으면 봉사시간같은거 주나봐요",
          "speaker_id": "1",
          "time": "20210722 21:43"
        },
        {
          "id": "MDRW2100001058.1.13",
          "form": "5시간인가 10시간인가 주셨어요",
          "original_form": "5시간인가 10시간인가 주셨어요",
          "speaker_id": "2",
          "time": "20210722 21:44"
        },
        {
          "id": "MDRW2100001058.1.14",
          "form": "그때 3일거서 하는거라가지고",
          "original_form": "그때 3일거서 하는거라가지고",
          "speaker_id": "2",
          "time": "20210722 21:44"
        },
        {
          "id": "MDRW2100001058.1.15",
          "form": "허걱 완전많이 주네옼",
          "original_form": "허걱 완전많이 주네옼",
          "speaker_id": "1",
          "time": "20210722 21:44"
        },
        {
          "id": "MDRW2100001058.1.16",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 21:44"
        },
        {
          "id": "MDRW2100001058.1.17",
          "form": "그런 봉사는 꿀봉사죠><",
          "original_form": "그런 봉사는 꿀봉사죠><",
          "speaker_id": "1",
          "time": "20210722 21:44"
        },
        {
          "id": "MDRW2100001058.1.18",
          "form": "중학교도 봉사시간모으나요?",
          "original_form": "중학교도 봉사시간모으나요?",
          "speaker_id": "1",
          "time": "20210722 21:45"
        },
        {
          "id": "MDRW2100001058.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210722 21:45"
        },
        {
          "id": "MDRW2100001058.1.20",
          "form": "기본 개인으로 채워야하는 시간이 있고 학교에서 채워주는 시간이 있어요",
          "original_form": "기본 개인으로 채워야하는 시간이 있고 학교에서 채워주는 시간이 있어요",
          "speaker_id": "2",
          "time": "20210722 21:45"
        },
        {
          "id": "MDRW2100001058.1.21",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210722 21:45"
        },
        {
          "id": "MDRW2100001058.1.22",
          "form": "근데 개인이 해야하는건 코로나때문에 안하고있어요",
          "original_form": "근데 개인이 해야하는건 코로나때문에 안하고있어요",
          "speaker_id": "2",
          "time": "20210722 21:45"
        },
        {
          "id": "MDRW2100001058.1.23",
          "form": "저는 중학교때 60시간 딱 채우고",
          "original_form": "저는 중학교때 60시간 딱 채우고",
          "speaker_id": "1",
          "time": "20210722 21:46"
        },
        {
          "id": "MDRW2100001058.1.24",
          "form": "고등학교때 218시간모으고",
          "original_form": "고등학교때 218시간모으고",
          "speaker_id": "1",
          "time": "20210722 21:46"
        },
        {
          "id": "MDRW2100001058.1.25",
          "form": "이랬죠",
          "original_form": "이랬죠",
          "speaker_id": "1",
          "time": "20210722 21:46"
        },
        {
          "id": "MDRW2100001058.1.26",
          "form": "저는 1학년때 학교랑 개인시간 합져서 20시간 채웠어야해서",
          "original_form": "저는 1학년때 학교랑 개인시간 합져서 20시간 채웠어야해서",
          "speaker_id": "2",
          "time": "20210722 21:46"
        },
        {
          "id": "MDRW2100001058.1.27",
          "form": "좀 힘들었는데",
          "original_form": "좀 힘들었는데",
          "speaker_id": "2",
          "time": "20210722 21:46"
        },
        {
          "id": "MDRW2100001058.1.28",
          "form": "잘만구하면 금방해요",
          "original_form": "잘만구하면 금방해요",
          "speaker_id": "1",
          "time": "20210722 21:47"
        }
      ]
    }
