      "utterance": [
        {
          "id": "MDRW2100026504.1.1",
          "form": "어제는 피곤해서 일찍 잠들었네요",
          "original_form": "어제는 피곤해서 일찍 잠들었네요",
          "speaker_id": "2",
          "time": "20210902 06:47"
        },
        {
          "id": "MDRW2100026504.1.2",
          "form": "일찍 일어나셨네요:)",
          "original_form": "일찍 일어나셨네요:)",
          "speaker_id": "1",
          "time": "20210902 07:29"
        },
        {
          "id": "MDRW2100026504.1.3",
          "form": "오늘 아침에 사무실에 일이 있어서 평소보다 일찍일어나서 출근했어요 ㅎ",
          "original_form": "오늘 아침에 사무실에 일이 있어서 평소보다 일찍일어나서 출근했어요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100026504.1.4",
          "form": "8시도 안되었는데\n출근하셨군용!",
          "original_form": "8시도 안되었는데\n출근하셨군용!",
          "speaker_id": "1",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100026504.1.5",
          "form": "피곤하시겠어요ㅠ",
          "original_form": "피곤하시겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100026504.1.6",
          "form": "월급 받아 살기가 쉽지않네요 참 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "월급 받아 살기가 쉽지않네요 참 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:02"
        },
        {
          "id": "MDRW2100026504.1.7",
          "form": "그츄:)\n남의 돈 벌기가 쉽지않죠",
          "original_form": "그츄:)\n남의 돈 벌기가 쉽지않죠",
          "speaker_id": "1",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100026504.1.8",
          "form": "님은 연애중이신가요?",
          "original_form": "님은 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100026504.1.9",
          "form": "아니요.....ㅜ",
          "original_form": "아니요.....ㅜ",
          "speaker_id": "2",
          "time": "20210902 08:09"
        },
        {
          "id": "MDRW2100026504.1.10",
          "form": "그럼 빛이나는 솔로?",
          "original_form": "그럼 빛이나는 솔로?",
          "speaker_id": "1",
          "time": "20210902 08:23"
        },
        {
          "id": "MDRW2100026504.1.11",
          "form": "빛이 나지는 않지만...솔로입니다 ㅎㅎ",
          "original_form": "빛이 나지는 않지만...솔로입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:24"
        },
        {
          "id": "MDRW2100026504.1.12",
          "form": "제일 부럽네요:)",
          "original_form": "제일 부럽네요:)",
          "speaker_id": "1",
          "time": "20210902 08:26"
        },
        {
          "id": "MDRW2100026504.1.13",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210902 08:26"
        },
        {
          "id": "MDRW2100026504.1.14",
          "form": "나이가 있어서 그런지.. 같이 있을때 편한사람이 좋더라도요 ㅎ",
          "original_form": "나이가 있어서 그런지.. 같이 있을때 편한사람이 좋더라도요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 08:29"
        },
        {
          "id": "MDRW2100026504.1.15",
          "form": "근데 편해지려면 불편한시간을 함께 보내야 하는데 그건또 싫고",
          "original_form": "근데 편해지려면 불편한시간을 함께 보내야 하는데 그건또 싫고",
          "speaker_id": "2",
          "time": "20210902 08:30"
        },
        {
          "id": "MDRW2100026504.1.16",
          "form": "그쵸ㅠ",
          "original_form": "그쵸ㅠ",
          "speaker_id": "1",
          "time": "20210902 08:31"
        },
        {
          "id": "MDRW2100026504.1.17",
          "form": "그런 에너지를 쏘ㄷ.기가 싫죠",
          "original_form": "그런 에너지를 쏘ㄷ.기가 싫죠",
          "speaker_id": "1",
          "time": "20210902 08:31"
        },
        {
          "id": "MDRW2100026504.1.18",
          "form": "그래서 나이먹을수록 연애하기가 힘든가봐요",
          "original_form": "그래서 나이먹을수록 연애하기가 힘든가봐요",
          "speaker_id": "2",
          "time": "20210902 08:40"
        },
        {
          "id": "MDRW2100026504.1.19",
          "form": "맞는거 같아요:)",
          "original_form": "맞는거 같아요:)",
          "speaker_id": "1",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100026504.1.20",
          "form": "님은 결혼하셨나요?",
          "original_form": "님은 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100026504.1.21",
          "form": "그리고 재는것도\n많아 지게 되는거 같구요",
          "original_form": "그리고 재는것도\n많아 지게 되는거 같구요",
          "speaker_id": "1",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100026504.1.22",
          "form": "네:) 저는 7년차입니당",
          "original_form": "네:) 저는 7년차입니당",
          "speaker_id": "1",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100026504.1.23",
          "form": "아하  몇살때하셨나요",
          "original_form": "아하  몇살때하셨나요",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100026504.1.24",
          "form": "저는 29살에 했어요^^",
          "original_form": "저는 29살에 했어요^^",
          "speaker_id": "1",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100026504.1.25",
          "form": "오  좋을때 하셨네요 ㅎㅎ",
          "original_form": "오  좋을때 하셨네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100026504.1.26",
          "form": "좀 더 늦게 할걸 그랬어요 ㅋㅋ",
          "original_form": "좀 더 늦게 할걸 그랬어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100026504.1.27",
