      "utterance": [
        {
          "id": "MDRW2100022401.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022401.1.2",
          "form": "안녕하세요 점심은 드셨나요?",
          "original_form": "안녕하세요 점심은 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022401.1.3",
          "form": "네 국밥 먹었습니다!",
          "original_form": "네 국밥 먹었습니다!",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022401.1.4",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022401.1.5",
          "form": "저는 아직이에요. 뭘 먹을지 고민중이랍니다",
          "original_form": "저는 아직이에요. 뭘 먹을지 고민중이랍니다",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022401.1.6",
          "form": "매끼 먹을 때마다 고민되죠 ㅎㅎ",
          "original_form": "매끼 먹을 때마다 고민되죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022401.1.7",
          "form": "ㅋㅋㅋ그렇죠 전 한번 주문하려면 메뉴 고르는데에 시간 엄청 걸리는데",
          "original_form": "ㅋㅋㅋ그렇죠 전 한번 주문하려면 메뉴 고르는데에 시간 엄청 걸리는데",
          "speaker_id": "2",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100022401.1.8",
          "form": "저도 좀 오래 걸려요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 좀 오래 걸려요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022401.1.9",
          "form": "그래서 가끔은 그냥 제비뽑기하고 그래요 ㅋㅋㅋ",
          "original_form": "그래서 가끔은 그냥 제비뽑기하고 그래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022401.1.10",
          "form": "ㅋㅋㅋ제비뽑기 그런거에 선택권을 주는 것도 나쁘지는 않죠",
          "original_form": "ㅋㅋㅋ제비뽑기 그런거에 선택권을 주는 것도 나쁘지는 않죠",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022401.1.11",
          "form": "너무 이것저것 다 먹고싶어해서요 ㅋㅋㅋ",
          "original_form": "너무 이것저것 다 먹고싶어해서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022401.1.12",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022401.1.13",
          "form": "저는 치킨이요",
          "original_form": "저는 치킨이요",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022401.1.14",
          "form": "치킨 자주 드시나요?",
          "original_form": "치킨 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022401.1.15",
          "form": "네네 치킨 자주 먹어요 ㅎ",
          "original_form": "네네 치킨 자주 먹어요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022401.1.16",
          "form": "신상 치킨 나오면 다 드셔보시나요 ㅎ",
          "original_form": "신상 치킨 나오면 다 드셔보시나요 ㅎ",
