      "utterance": [
        {
          "id": "MDRW2100033653.1.1",
          "form": "사고싶은 물건 있으세요?",
          "original_form": "사고싶은 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100033653.1.2",
          "form": "소파도 사고싶어요",
          "original_form": "소파도 사고싶어요",
          "speaker_id": "1",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100033653.1.3",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100033653.1.4",
          "form": "어떤 쇼파요?",
          "original_form": "어떤 쇼파요?",
          "speaker_id": "2",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100033653.1.5",
          "form": "2~3인용으로 나오는 작은 패브릭소파 사고싶어요",
          "original_form": "2~3인용으로 나오는 작은 패브릭소파 사고싶어요",
          "speaker_id": "1",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100033653.1.6",
          "form": "거실이 좁아서 큰건 못둬서요",
          "original_form": "거실이 좁아서 큰건 못둬서요",
          "speaker_id": "1",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100033653.1.7",
          "form": "쇼파 있으면 좋을 거 같아요",
          "original_form": "쇼파 있으면 좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100033653.1.8",
          "form": "좌식생활이",
          "original_form": "좌식생활이",
          "speaker_id": "2",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100033653.1.9",
          "form": "무릎에 안좋거둔요",
          "original_form": "무릎에 안좋거둔요",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100033653.1.10",
          "form": "그죠 저도 그래서 고민중이예요",
          "original_form": "그죠 저도 그래서 고민중이예요",
          "speaker_id": "1",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100033653.1.11",
          "form": "하나 장만하는 거 추천해요!",
          "original_form": "하나 장만하는 거 추천해요!",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100033653.1.12",
          "form": "손님왔을때 앉힐곳도 애매하구요ㅠ",
          "original_form": "손님왔을때 앉힐곳도 애매하구요ㅠ",
          "speaker_id": "1",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100033653.1.13",
          "form": "이사할때 짐이될까봐",
          "original_form": "이사할때 짐이될까봐",
          "speaker_id": "1",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100033653.1.14",
          "form": "룸메랑 고민중이예요",
          "original_form": "룸메랑 고민중이예요",
          "speaker_id": "1",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100033653.1.15",
          "form": "추천감사드려요",
          "original_form": "추천감사드려요",
          "speaker_id": "1",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100033653.1.16",
          "form": "name2님은 사고싶은거 있으신가요",
          "original_form": "&name2&님은 사고싶은거 있으신가요",
          "speaker_id": "1",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100033653.1.17",
          "form": "저도 가구 사고싶네요",
          "original_form": "저도 가구 사고싶네요",
          "speaker_id": "2",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100033653.1.18",
          "form": "마음 같아서는 침대랑 바꾸고 싶어요",
          "original_form": "마음 같아서는 침대랑 바꾸고 싶어요",
          "speaker_id": "2",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100033653.1.19",
          "form": "침대 왜 바꾸고 싶으세요?",
          "original_form": "침대 왜 바꾸고 싶으세요?",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100033653.1.20",
          "form": "그냥요 뭔가 인테리어 바꿀겸",
          "original_form": "그냥요 뭔가 인테리어 바꿀겸",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100033653.1.21",
          "form": "바꾸고싶더라구요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "바꾸고싶더라구요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100033653.1.22",
          "form": "아 그러시구나! 어떤 디자인으로 사고 싶으셔요?",
          "original_form": "아 그러시구나! 어떤 디자인으로 사고 싶으셔요?",
          "speaker_id": "1",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100033653.1.23",
          "form": "전 수납 많은 거요 ㅎㅎ ㅎㅎ",
          "original_form": "전 수납 많은 거요 ㅎㅎ ㅎㅎ",
