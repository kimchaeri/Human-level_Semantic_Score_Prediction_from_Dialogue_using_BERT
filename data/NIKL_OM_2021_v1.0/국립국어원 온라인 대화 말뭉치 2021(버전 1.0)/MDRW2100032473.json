      "utterance": [
        {
          "id": "MDRW2100032473.1.1",
          "form": "쇼핑하는거 좋아하세요?",
          "original_form": "쇼핑하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100032473.1.2",
          "form": "넵넵ㅋㅋ 간식 쟁이는거 습관이에요ㅜ",
          "original_form": "넵넵ㅋㅋ 간식 쟁이는거 습관이에요ㅜ",
          "speaker_id": "2",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100032473.1.3",
          "form": "ㅋㅋㅋㅋㅋ 어디다 쟁여요?",
          "original_form": "ㅋㅋㅋㅋㅋ 어디다 쟁여요?",
          "speaker_id": "1",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100032473.1.4",
          "form": "아 회사에 돌리고요ㅋㅋ 식구들한테 나눠주고 서랍에 쟁여요ㅋㅋ",
          "original_form": "아 회사에 돌리고요ㅋㅋ 식구들한테 나눠주고 서랍에 쟁여요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100032473.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 젤리같은거요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 젤리같은거요?",
          "speaker_id": "1",
          "time": "20210905 18:58"
        },
        {
          "id": "MDRW2100032473.1.6",
          "form": "초컬릿이랑 쿠키ㅋㅋ 이런거요ㅋㅋ",
          "original_form": "초컬릿이랑 쿠키ㅋㅋ 이런거요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:00"
        },
        {
          "id": "MDRW2100032473.1.7",
          "form": "당 떨어질때를 대비한",
          "original_form": "당 떨어질때를 대비한",
          "speaker_id": "1",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100032473.1.8",
          "form": "간식들이군요 ㅎㅎ",
          "original_form": "간식들이군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100032473.1.9",
          "form": "넵ㅋㅋ 회사에서 없음 안돼요ㅋㅋ",
          "original_form": "넵ㅋㅋ 회사에서 없음 안돼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:19"
        },
        {
          "id": "MDRW2100032473.1.10",
          "form": "ㅋㅋㅋㅋㅋ  전그냥 커피만 마시는데",
          "original_form": "ㅋㅋㅋㅋㅋ  전그냥 커피만 마시는데",
          "speaker_id": "1",
          "time": "20210905 19:21"
        },
        {
          "id": "MDRW2100032473.1.11",
          "form": "아.... ㅋㅋ",
          "original_form": "아.... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100032473.1.12",
          "form": "커피는 간식을 먹기위한 보조...",
          "original_form": "커피는 간식을 먹기위한 보조...",
          "speaker_id": "2",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100032473.1.13",
          "form": "ㅋㅋㅋㅋㅋㄱ 단거 먹기위한?",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 단거 먹기위한?",
          "speaker_id": "1",
          "time": "20210905 19:37"
        },
        {
          "id": "MDRW2100032473.1.14",
          "form": "그렇죠! 아메와 함께하는 브라우니!",
          "original_form": "그렇죠! 아메와 함께하는 브라우니!",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032473.1.15",
          "form": "간식 계속 먹다보면 달고살거 같아서 커피로만 때워요 ㅎㅎ",
          "original_form": "간식 계속 먹다보면 달고살거 같아서 커피로만 때워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032473.1.16",
          "form": "ㅜㅠ 근데 간식 습관이에요ㅜㅠ",
          "original_form": "ㅜㅠ 근데 간식 습관이에요ㅜㅠ",
