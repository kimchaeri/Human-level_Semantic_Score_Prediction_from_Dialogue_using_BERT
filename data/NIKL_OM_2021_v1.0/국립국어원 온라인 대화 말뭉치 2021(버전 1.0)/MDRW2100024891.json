        {
          "id": "MDRW2100024891.1.2",
          "form": "재택근무한지 얼마나 됐지??",
          "original_form": "재택근무한지 얼마나 됐지??",
          "speaker_id": "2",
          "time": "20210831 20:18"
        },
        {
          "id": "MDRW2100024891.1.3",
          "form": "한달은 지났고,,",
          "original_form": "한달은 지났고,,",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.4",
          "form": "다음주까지하면 두달째~~",
          "original_form": "다음주까지하면 두달째~~",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.5",
          "form": "벌써??",
          "original_form": "벌써??",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.6",
          "form": "오래됐네~",
          "original_form": "오래됐네~",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.7",
          "form": "하다보니 그렇게 됐네~ㅋㅋㅋ",
          "original_form": "하다보니 그렇게 됐네~ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.8",
          "form": "재택하니까 확실히 편하지??",
          "original_form": "재택하니까 확실히 편하지??",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.9",
          "form": "그렇지요~~",
          "original_form": "그렇지요~~",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.10",
          "form": "출퇴근 안하는 것만으로도 살거 같아요~~",
          "original_form": "출퇴근 안하는 것만으로도 살거 같아요~~",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.11",
          "form": "교통비도 안 나가고",
          "original_form": "교통비도 안 나가고",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024891.1.12",
          "form": "돈도 굳고 스트레스도 덜 받고~",
          "original_form": "돈도 굳고 스트레스도 덜 받고~",
          "speaker_id": "1",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024891.1.13",
          "form": "언제까지 하는거야??",
          "original_form": "언제까지 하는거야??",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024891.1.14",
          "form": "그건 모르겟어요~",
          "original_form": "그건 모르겟어요~",
          "speaker_id": "1",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024891.1.15",
          "form": "대표님이 여타부타 말씀이 없으셔서~",
          "original_form": "대표님이 여타부타 말씀이 없으셔서~",
          "speaker_id": "1",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100024891.1.16",
          "form": "코로나가 가라앉을 때까지 하는건가?",
          "original_form": "코로나가 가라앉을 때까지 하는건가?",
