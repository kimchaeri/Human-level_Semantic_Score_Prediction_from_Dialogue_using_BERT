      "utterance": [
        {
          "id": "MDRW2100041801.1.1",
          "form": "로댕과 피카소가 울고 갈거라며 말이에요",
          "original_form": "로댕과 피카소가 울고 갈거라며 말이에요",
          "speaker_id": "1",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100041801.1.2",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100041801.1.3",
          "form": "근데 의외로 요리실력과 상관업이 과일을 못깍는 사람들이 있어요",
          "original_form": "근데 의외로 요리실력과 상관업이 과일을 못깍는 사람들이 있어요",
          "speaker_id": "2",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100041801.1.4",
          "form": "요리는 불조절을 잘하고 간만 잘맞추면 끝이에요",
          "original_form": "요리는 불조절을 잘하고 간만 잘맞추면 끝이에요",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100041801.1.5",
          "form": "그... 분명 사람 먹을 음식이긴한데",
          "original_form": "그... 분명 사람 먹을 음식이긴한데",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100041801.1.6",
          "form": "이상하게 맛이 없어요",
          "original_form": "이상하게 맛이 없어요",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100041801.1.7",
          "form": "제가 그 유명한 요리 똥손인가봐요",
          "original_form": "제가 그 유명한 요리 똥손인가봐요",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100041801.1.8",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100041801.1.9",
          "form": "맛있는 요리를 하는 곳을 잘 알아내서 먹는 걸로 합시다",
          "original_form": "맛있는 요리를 하는 곳을 잘 알아내서 먹는 걸로 합시다",
          "speaker_id": "2",
          "time": "20210906 19:34"
        },
        {
          "id": "MDRW2100041801.1.10",
          "form": "name2님께선 어떤 음식을 제일 잘 만드시나요??",
          "original_form": "&name2&님께선 어떤 음식을 제일 잘 만드시나요??",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100041801.1.11",
          "form": "다양하게 해보고 있지만",
          "original_form": "다양하게 해보고 있지만",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100041801.1.12",
          "form": "한식을 잘해요",
          "original_form": "한식을 잘해요",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100041801.1.13",
          "form": "요리대회 나가서 대상탄 적도 있어요",
          "original_form": "요리대회 나가서 대상탄 적도 있어요",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100041801.1.14",
          "form": "세상에!! 정말 잘하시는 거잖아요!!",
          "original_form": "세상에!! 정말 잘하시는 거잖아요!!",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100041801.1.15",
          "form": "알고보니 한식요리 대가 신 건 아니죠?",
          "original_form": "알고보니 한식요리 대가 신 건 아니죠?",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100041801.1.16",
          "form": "어떤 요리로 대상 받으셨어요??",
          "original_form": "어떤 요리로 대상 받으셨어요??",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100041801.1.17",
          "form": "닭요리 대회였어요",
          "original_form": "닭요리 대회였어요",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100041801.1.18",
          "form": "대상받는장면 방송에도 잠깐 나왔어요",
          "original_form": "대상받는장면 방송에도 잠깐 나왔어요",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100041801.1.19",
          "form": "앗 정말요?!",
          "original_form": "앗 정말요?!",
          "speaker_id": "1",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100041801.1.20",
          "form": "저... 선생님.. 저의 요리 실력을 구제할 방법이 없을까요 엉엉",
          "original_form": "저... 선생님.. 저의 요리 실력을 구제할 방법이 없을까요 엉엉",
          "speaker_id": "1",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100041801.1.21",
          "form": "한식 닭요리라면 어떤 음식이었어요?? 새로운 래시피로 만드셨나요??",
          "original_form": "한식 닭요리라면 어떤 음식이었어요?? 새로운 래시피로 만드셨나요??",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041801.1.22",
          "form": "일단 요리를 하려는 마음이 있으면 절반은 된거에요",
          "original_form": "일단 요리를 하려는 마음이 있으면 절반은 된거에요",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041801.1.23",
          "form": "의지 조차 없으면 개선이 안되죠",
          "original_form": "의지 조차 없으면 개선이 안되죠",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041801.1.24",
          "form": "레시피를 언니가 정해줘서 기억도 안나요",
          "original_form": "레시피를 언니가 정해줘서 기억도 안나요",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041801.1.25",
          "form": "닭과 인삼을 어쩌고 저쩌고",
          "original_form": "닭과 인삼을 어쩌고 저쩌고",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041801.1.26",
          "form": "저희 언니가 요리사거든요",
          "original_form": "저희 언니가 요리사거든요",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100041801.1.27",
          "form": "우와!!",
          "original_form": "우와!!",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100041801.1.28",
          "form": "자매 두분다 요리실력이 좋으시군요!",
          "original_form": "자매 두분다 요리실력이 좋으시군요!",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100041801.1.29",
          "form": "그치만 레시피대로 해서 완벽한 결과물이 나왔던 거잖아요!",
          "original_form": "그치만 레시피대로 해서 완벽한 결과물이 나왔던 거잖아요!",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100041801.1.30",
          "form": "레시피도 훌륭했을 거고 역시 name2님이 실력이 굉장히 좋아서 상을 받으실 수 있으셨던 걸거에요!",
          "original_form": "레시피도 훌륭했을 거고 역시 &name2&님이 실력이 굉장히 좋아서 상을 받으실 수 있으셨던 걸거에요!",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100041801.1.31",
          "form": "정말 멋있어요!",
          "original_form": "정말 멋있어요!",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100041801.1.32",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100041801.1.33",
          "form": "저희 친정이 요리를 잘 하는 유전자가 있어서 저도 도움을 좀 받은 듯해요",
          "original_form": "저희 친정이 요리를 잘 하는 유전자가 있어서 저도 도움을 좀 받은 듯해요",
          "speaker_id": "2",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100041801.1.34",
          "form": "대대로 요리를 잘 하시는군요!!",
          "original_form": "대대로 요리를 잘 하시는군요!!",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100041801.1.35",
          "form": "저는 제가 돌연변이에요 ㅋㅋ XD",
          "original_form": "저는 제가 돌연변이에요 ㅋㅋ XD",
          "speaker_id": "1",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100041801.1.36",
          "form": "저런.. 아쉽겠네요",
          "original_form": "저런.. 아쉽겠네요",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100041801.1.37",
          "form": "좋은 유전자는 다 가지고 와야하는데",
          "original_form": "좋은 유전자는 다 가지고 와야하는데",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100041801.1.38",
          "form": "그래도 나중에 따라잡을 수 있을거에요",
          "original_form": "그래도 나중에 따라잡을 수 있을거에요",
          "speaker_id": "2",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100041801.1.39",
          "form": "저도 언닌들 따라가려면 멀었거든요 ㅎㅎ",
          "original_form": "저도 언닌들 따라가려면 멀었거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100041801.1.40",
          "form": "꼭.. 정말 꼭.. 따라잡고 싶어요!",
          "original_form": "꼭.. 정말 꼭.. 따라잡고 싶어요!",
