        },
        {
          "id": "MDRW2100039630.1.2",
          "form": "조미료의 힘?",
          "original_form": "조미료의 힘?",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039630.1.3",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039630.1.4",
          "form": "요리하는거조아하시나욤?",
          "original_form": "요리하는거조아하시나욤?",
          "speaker_id": "2",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039630.1.5",
          "form": "네 좋아하는데 결과물을 늘 가족과 먹어서 맛은 보장을 못해요",
          "original_form": "네 좋아하는데 결과물을 늘 가족과 먹어서 맛은 보장을 못해요",
          "speaker_id": "1",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039630.1.6",
          "form": "ㅋㅋ가족들이 좋아하겟어욤",
          "original_form": "ㅋㅋ가족들이 좋아하겟어욤",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039630.1.7",
          "form": "남친이 금손이라서\r\n저는 맛잇게먹는담당입니다 ㅎ",
          "original_form": "남친이 금손이라서\r\n저는 맛잇게먹는담당입니다 ㅎ",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039630.1.8",
          "form": "ㅋㅋ엄마가 70프로를 하셔서",
          "original_form": "ㅋㅋ엄마가 70프로를 하셔서",
          "speaker_id": "1",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039630.1.9",
          "form": "이런 사랑꾼!",
          "original_form": "이런 사랑꾼!",
          "speaker_id": "1",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039630.1.10",
          "form": "ㅎㅎ맞아욤",
          "original_form": "ㅎㅎ맞아욤",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039630.1.11",
          "form": "이제 내일 주말인데",
          "original_form": "이제 내일 주말인데",
          "speaker_id": "2",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039630.1.12",
          "form": "맛난거드시러가시나용",
          "original_form": "맛난거드시러가시나용",
          "speaker_id": "2",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039630.1.13",
          "form": "코로나 이후로 배달만ㅋㅋ",
          "original_form": "코로나 이후로 배달만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039630.1.14",
          "form": "낼은 아마 치킨 먹을거같아요",
          "original_form": "낼은 아마 치킨 먹을거같아요",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039630.1.15",
          "form": "ㄷㅔ이트때 뭐드세요?",
          "original_form": "ㄷㅔ이트때 뭐드세요?",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039630.1.16",
          "form": "저희도 장금이가 집에서ㅜㅜ",
          "original_form": "저희도 장금이가 집에서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039630.1.17",
          "form": "저번주말엔 수육해먹엇는데",
          "original_form": "저번주말엔 수육해먹엇는데",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039630.1.18",
          "form": "ㅋㅋ 아 진짜 장금이 분양 안되나요??",
          "original_form": "ㅋㅋ 아 진짜 장금이 분양 안되나요??",
          "speaker_id": "1",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039630.1.19",
          "form": "입에서녹앗어요",
          "original_form": "입에서녹앗어요",
          "speaker_id": "2",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039630.1.20",
          "form": "수육이요?? 대박 완전 능력자군요!",
          "original_form": "수육이요?? 대박 완전 능력자군요!",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039630.1.21",
          "form": "쌍화탕 넣더라구요",
          "original_form": "쌍화탕 넣더라구요",
          "speaker_id": "2",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039630.1.22",
          "form": "ㅊㅣ트키인가바욤",
          "original_form": "ㅊㅣ트키인가바욤",
