      "utterance": [
        {
          "id": "MDRW2100033724.1.1",
          "form": "코로나 시국에 집에서 아기 돌보려면 힘드시겠어요ㅜ",
          "original_form": "코로나 시국에 집에서 아기 돌보려면 힘드시겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033724.1.2",
          "form": "네ㅠㅠ외출도 자유롭지못하니ㅜ아무래도ㅠ",
          "original_form": "네ㅠㅠ외출도 자유롭지못하니ㅜ아무래도ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033724.1.3",
          "form": "애도불쌍해여",
          "original_form": "애도불쌍해여",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033724.1.4",
          "form": "그러게요 아가도 나가고 싶을텐데",
          "original_form": "그러게요 아가도 나가고 싶을텐데",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033724.1.5",
          "form": "육아하는 name2님도 힘드실테고",
          "original_form": "육아하는 &name2&님도 힘드실테고",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100033724.1.6",
          "form": "지금은 육퇴하셨나요?",
          "original_form": "지금은 육퇴하셨나요?",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033724.1.7",
          "form": "안자요ㅠ둘째가",
          "original_form": "안자요ㅠ둘째가",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033724.1.8",
          "form": "오늘 낮잠이 늦어져서",
          "original_form": "오늘 낮잠이 늦어져서",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033724.1.9",
          "form": "늦게일어났더니 늦게요 취침이",
          "original_form": "늦게일어났더니 늦게요 취침이",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033724.1.10",
          "form": "에고 아가가 늦게까지 안자네요ㅜ",
          "original_form": "에고 아가가 늦게까지 안자네요ㅜ",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033724.1.11",
          "form": "제 친구도 아가가 늦게까지 안자서 요즘 넘 힘들다던데",
          "original_form": "제 친구도 아가가 늦게까지 안자서 요즘 넘 힘들다던데",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033724.1.12",
          "form": "원래도 잠이 없는것같아요ㅠ",
          "original_form": "원래도 잠이 없는것같아요ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033724.1.13",
          "form": "낮잠도 시간 끌다끌다자니 늦고. 악순환ㅠ",
          "original_form": "낮잠도 시간 끌다끌다자니 늦고. 악순환ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033724.1.14",
          "form": "에고 육아할 때 잠 잘 자는 것도 복이라는데ㅠ",
          "original_form": "에고 육아할 때 잠 잘 자는 것도 복이라는데ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033724.1.15",
          "form": "집안일은 남편분께서 많이 하시나요?",
          "original_form": "집안일은 남편분께서 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033724.1.16",
          "form": "맞아요 잘자고 잘먹고만ㅜ",
          "original_form": "맞아요 잘자고 잘먹고만ㅜ",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033724.1.17",
          "form": "해줘도",
          "original_form": "해줘도",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033724.1.18",
          "form": "네 아가는 잘먹고 잘자는 게 젤 효도죠",
          "original_form": "네 아가는 잘먹고 잘자는 게 젤 효도죠",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100033724.1.19",
          "form": "우리애는 그런면에서는 불효녀네요",
          "original_form": "우리애는 그런면에서는 불효녀네요",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033724.1.20",
          "form": "흑",
          "original_form": "흑",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033724.1.21",
          "form": "ㅠㅠ육아난이도가 있는 아가인가봐요",
          "original_form": "ㅠㅠ육아난이도가 있는 아가인가봐요",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100033724.1.22",
          "form": "아가가 얼른 자야 name2님도 쉴텐데",
          "original_form": "아가가 얼른 자야 &name2&님도 쉴텐데",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100033724.1.23",
          "form": "아가가 얼른 잠들길 기원할게요~",
          "original_form": "아가가 얼른 잠들길 기원할게요~",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033724.1.24",
          "form": "같이 기원해봅니다ㅋㅋ",
          "original_form": "같이 기원해봅니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033724.1.25",
          "form": "육퇴를 기원합니다!!",
          "original_form": "육퇴를 기원합니다!!",
          "speaker_id": "1",
