      "utterance": [
        {
          "id": "MDRW2100028062.1.1",
          "form": "긴머리세요?",
          "original_form": "긴머리세요?",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028062.1.2",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028062.1.3",
          "form": "단발은 관리가 귀찮아요!",
          "original_form": "단발은 관리가 귀찮아요!",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028062.1.4",
          "form": "잘랐을 때는 편한데",
          "original_form": "잘랐을 때는 편한데",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028062.1.5",
          "form": "잡초처럼 자라는 머리 감당이ㅜ",
          "original_form": "잡초처럼 자라는 머리 감당이ㅜ",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028062.1.6",
          "form": "단발 거기 존이 있다더라구요",
          "original_form": "단발 거기 존이 있다더라구요",
          "speaker_id": "2",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100028062.1.7",
          "form": "못견딘다든데",
          "original_form": "못견딘다든데",
          "speaker_id": "2",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100028062.1.8",
          "form": "맞아요 거기ㅜㅜ",
          "original_form": "맞아요 거기ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028062.1.9",
          "form": "전 심지어 머리도 빨리 자라는데",
          "original_form": "전 심지어 머리도 빨리 자라는데",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028062.1.10",
          "form": "그 구간을 못 견뎌요",
          "original_form": "그 구간을 못 견뎌요",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028062.1.11",
          "form": "다시 확 자르거나",
          "original_form": "다시 확 자르거나",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028062.1.12",
          "form": "평생 단발존을 넘는 기장을 고수하거나",
          "original_form": "평생 단발존을 넘는 기장을 고수하거나",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100028062.1.13",
          "form": "전 후자를 골랐쥬ㅎㅎ",
          "original_form": "전 후자를 골랐쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028062.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028062.1.15",
          "form": "어디까지 오나요?",
          "original_form": "어디까지 오나요?",
          "speaker_id": "2",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028062.1.16",
          "form": "등 중간?",
          "original_form": "등 중간?",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028062.1.17",
          "form": "허리 넘어요ㅎ",
          "original_form": "허리 넘어요ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028062.1.18",
          "form": "코로나 핑계로 미용실을 안 갔더니ㅜ",
          "original_form": "코로나 핑계로 미용실을 안 갔더니ㅜ",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028062.1.19",
          "form": "이제 그 길이로 자르러 가야죠",
          "original_form": "이제 그 길이로 자르러 가야죠",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028062.1.20",
          "form": "오아",
          "original_form": "오아",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028062.1.21",
          "form": "김태원 아저씨만큼 기네요",
          "original_form": "김태원 아저씨만큼 기네요",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028062.1.22",
          "form": "머리카락은 왜 자라는 걸까요",
          "original_form": "머리카락은 왜 자라는 걸까요",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028062.1.23",
          "form": "ㅋㅋ 나란히 있으면 형제 같쥬?ㅎㅎ",
          "original_form": "ㅋㅋ 나란히 있으면 형제 같쥬?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028062.1.24",
          "form": "ㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028062.1.25",
          "form": "머리카락은 세상에 나오고 싶어합니다",
          "original_form": "머리카락은 세상에 나오고 싶어합니다",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028062.1.26",
          "form": "그나마 허리 넘으면 잘 안자라긴 해요ㅋㅋ",
          "original_form": "그나마 허리 넘으면 잘 안자라긴 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028062.1.27",
          "form": "이 세상에 뭐 볼거 있다구ㅜㅜㅜ",
          "original_form": "이 세상에 뭐 볼거 있다구ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028062.1.28",
          "form": "김태원아저씨 형제가 있자나여",
          "original_form": "김태원아저씨 형제가 있자나여",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028062.1.29",
          "form": "그게 저입니다",
          "original_form": "그게 저입니다",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028062.1.30",
          "form": "아름답습니다",
          "original_form": "아름답습니다",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028062.1.31",
          "form": "민간사찰 하셨나유",
          "original_form": "민간사찰 하셨나유",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028062.1.32",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028062.1.33",
          "form": "제 전문입니다",
          "original_form": "제 전문입니다",
          "speaker_id": "2",
          "time": "20210902 17:35"
        }
      ]
    }
