        {
          "id": "MDRW2100009410.1.1",
          "form": "좋아하는스포츠 있나요",
          "original_form": "좋아하는스포츠 있나요",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009410.1.2",
          "form": "축구 보는거요",
          "original_form": "축구 보는거요",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009410.1.3",
          "form": "저는 골프보는것 재미 있더라고요",
          "original_form": "저는 골프보는것 재미 있더라고요",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009410.1.4",
          "form": "넘 길지않나요?",
          "original_form": "넘 길지않나요?",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009410.1.5",
          "form": "그래도 저는 재미 있어요",
          "original_form": "그래도 저는 재미 있어요",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009410.1.6",
          "form": "룰을 잘 몰라서",
          "original_form": "룰을 잘 몰라서",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009410.1.7",
          "form": "알면 재미 있어요",
          "original_form": "알면 재미 있어요",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009410.1.8",
          "form": "그쳐 알면",
          "original_form": "그쳐 알면",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009410.1.9",
          "form": "저도 이번 올림픽때 더 자세하게 알게 되었지요",
          "original_form": "저도 이번 올림픽때 더 자세하게 알게 되었지요",
          "speaker_id": "1",
          "time": "20210823 06:03"
        },
        {
          "id": "MDRW2100009410.1.10",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "2",
          "time": "20210823 07:20"
        },
        {
          "id": "MDRW2100009410.1.11",
          "form": "보면볼수록 재미 있고 흥미로운 스포츠네요",
          "original_form": "보면볼수록 재미 있고 흥미로운 스포츠네요",
          "speaker_id": "1",
          "time": "20210823 07:39"
        },
        {
          "id": "MDRW2100009410.1.12",
          "form": "직접 하시는거?",
          "original_form": "직접 하시는거?",
          "speaker_id": "2",
          "time": "20210823 07:40"
        },
        {
