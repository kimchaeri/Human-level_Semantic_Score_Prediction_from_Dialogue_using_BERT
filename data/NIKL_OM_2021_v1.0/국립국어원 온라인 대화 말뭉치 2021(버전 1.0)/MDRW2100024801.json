      "utterance": [
        {
          "id": "MDRW2100024801.1.1",
          "form": "사겼던 건가요?",
          "original_form": "사겼던 건가요?",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024801.1.2",
          "form": "아니요 사귄거는아니고 서로 좋아하는사이였는데 제가 걔를 더좋아했는데 걔가 되게 잘알아요",
          "original_form": "아니요 사귄거는아니고 서로 좋아하는사이였는데 제가 걔를 더좋아했는데 걔가 되게 잘알아요",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024801.1.3",
          "form": "제가 표현을 많이 했거든요",
          "original_form": "제가 표현을 많이 했거든요",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024801.1.4",
          "form": "가까운데 살면 만나서 해결하세요",
          "original_form": "가까운데 살면 만나서 해결하세요",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024801.1.5",
          "form": "걔가 멀리살아요ㅠㅠ",
          "original_form": "걔가 멀리살아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024801.1.6",
          "form": "그래서 연락만주고받고있었어요",
          "original_form": "그래서 연락만주고받고있었어요",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024801.1.7",
          "form": "만난적없어요?",
          "original_form": "만난적없어요?",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024801.1.8",
          "form": "네 만난적은없어요",
          "original_form": "네 만난적은없어요",
          "speaker_id": "2",
          "time": "20210901 05:58"
        },
        {
          "id": "MDRW2100024801.1.9",
          "form": "ㅜㅜ 그럼 또 좋은 인연 만날 거에요!",
          "original_form": "ㅜㅜ 그럼 또 좋은 인연 만날 거에요!",
          "speaker_id": "1",
          "time": "20210901 08:00"
        },
        {
          "id": "MDRW2100024801.1.10",
          "form": "어제저녁까지만해도 이별상태였는데",
          "original_form": "어제저녁까지만해도 이별상태였는데",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100024801.1.11",
          "form": "화해해서 다시 분위기 괜찮아졌어요",
          "original_form": "화해해서 다시 분위기 괜찮아졌어요",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100024801.1.12",
          "form": "오호 축하드려요",
          "original_form": "오호 축하드려요",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100024801.1.13",
          "form": "나이차가 좀 많이 나거든요",
          "original_form": "나이차가 좀 많이 나거든요",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100024801.1.14",
          "form": "연애는 어렵죠",
          "original_form": "연애는 어렵죠",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100024801.1.15",
          "form": "ㅜㆍㅜ안붙잡을꺼라고해놓고서는",
          "original_form": "ㅜㆍㅜ안붙잡을꺼라고해놓고서는",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100024801.1.16",
          "form": "다시 붙잡았네요",
          "original_form": "다시 붙잡았네요",
          "speaker_id": "2",
          "time": "20210901 17:12"
        }
      ]
