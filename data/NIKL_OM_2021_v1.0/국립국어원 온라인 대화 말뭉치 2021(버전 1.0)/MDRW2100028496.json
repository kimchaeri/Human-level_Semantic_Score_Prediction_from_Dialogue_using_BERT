      "utterance": [
        {
          "id": "MDRW2100028496.1.1",
          "form": "최근 잘 산템 있나요",
          "original_form": "최근 잘 산템 있나요",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028496.1.2",
          "form": "흠 제가 룸스프레이 모으는게 취민데",
          "original_form": "흠 제가 룸스프레이 모으는게 취민데",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028496.1.3",
          "form": "서퍼라는 브랜드에 쳇베이커라는 향이있거든요?",
          "original_form": "서퍼라는 브랜드에 쳇베이커라는 향이있거든요?",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028496.1.4",
          "form": "근데 정말 향이 좋습니다...지속력은 길진않지만ㅠㅠ",
          "original_form": "근데 정말 향이 좋습니다...지속력은 길진않지만ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028496.1.5",
          "form": "우와 궁금해요 서퍼 쳇베이커",
          "original_form": "우와 궁금해요 서퍼 쳇베이커",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028496.1.6",
          "form": "러쉬는 안좋아하세요?",
          "original_form": "러쉬는 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028496.1.7",
          "form": "러쉬는 선물받은 거만써용",
          "original_form": "러쉬는 선물받은 거만써용",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028496.1.8",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028496.1.9",
          "form": "저는 양키캔들 라벤더 스프레이있는데",
          "original_form": "저는 양키캔들 라벤더 스프레이있는데",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028496.1.10",
          "form": "이거엄청 강력하더라고요",
          "original_form": "이거엄청 강력하더라고요",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028496.1.11",
          "form": "강력한게 좋기는한데",
          "original_form": "강력한게 좋기는한데",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028496.1.12",
          "form": "향이 은은하게 오래가는 것들은 비싸요ㅠㅠ",
          "original_form": "향이 은은하게 오래가는 것들은 비싸요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028496.1.13",
          "form": "아 은은한게 ㄷㅓ비싸군요",
          "original_form": "아 은은한게 ㄷㅓ비싸군요",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028496.1.14",
          "form": "또 추천해주세요",
          "original_form": "또 추천해주세요",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028496.1.15",
          "form": "그 서울 종로쪽에 그랑핸드라고 있는데",
          "original_form": "그 서울 종로쪽에 그랑핸드라고 있는데",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028496.1.16",
          "form": "거기 오드퍼퓸인가? 그래서 지속력꽤긴데",
          "original_form": "거기 오드퍼퓸인가? 그래서 지속력꽤긴데",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028496.1.17",
          "form": "향도 좋은거 꽤있어요!",
          "original_form": "향도 좋은거 꽤있어요!",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028496.1.18",
          "form": "와 특별한 브랜드 많이아시네요",
          "original_form": "와 특별한 브랜드 많이아시네요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028496.1.19",
          "form": "저는 요즘 아로마 오일 좋아요",
          "original_form": "저는 요즘 아로마 오일 좋아요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028496.1.20",
          "form": "아로마오일은",
          "original_form": "아로마오일은",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028496.1.21",
          "form": "한번도 안써봤는데",
          "original_form": "한번도 안써봤는데",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028496.1.22",
          "form": "그 밑에 캔들놓고 사용하는 그건가요???",
          "original_form": "그 밑에 캔들놓고 사용하는 그건가요???",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028496.1.23",
          "form": "아 그냥 따뜩한 물에 한방울하거나",
          "original_form": "아 그냥 따뜩한 물에 한방울하거나",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028496.1.24",
          "form": "어디 떨어뜨리면 향이나요",
          "original_form": "어디 떨어뜨리면 향이나요",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028496.1.25",
          "form": "그렇군여 한번 찾아봐야겠어요",
          "original_form": "그렇군여 한번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028496.1.26",
          "form": "아로마티카 좋아요",
          "original_form": "아로마티카 좋아요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        }
      ]
    }
