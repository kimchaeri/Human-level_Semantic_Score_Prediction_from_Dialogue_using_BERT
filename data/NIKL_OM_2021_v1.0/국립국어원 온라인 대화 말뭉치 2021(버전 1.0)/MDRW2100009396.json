      "utterance": [
        {
          "id": "MDRW2100009396.1.1",
          "form": "애들 내복도 다 새로 사야는데~",
          "original_form": "애들 내복도 다 새로 사야는데~",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009396.1.2",
          "form": "아가방 스판끼있는 바지가",
          "original_form": "아가방 스판끼있는 바지가",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009396.1.3",
          "form": "눈에 아룬거리나?ㅋㅋㅋ",
          "original_form": "눈에 아룬거리나?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009396.1.4",
          "form": "응응 그러니깐ㅋ",
          "original_form": "응응 그러니깐ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009396.1.5",
          "form": "비싸서 고민됐는데",
          "original_form": "비싸서 고민됐는데",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009396.1.6",
          "form": "내 기준엔 비싸서 히히",
          "original_form": "내 기준엔 비싸서 히히",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009396.1.7",
          "form": "그거 쫌 얇아서 ㅋㅋㅋㅋ얼마 못입음 ㅋㅋㅋㅋ",
          "original_form": "그거 쫌 얇아서 ㅋㅋㅋㅋ얼마 못입음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009396.1.8",
          "form": "아아 그래? ㅋㅋ",
          "original_form": "아아 그래? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009396.1.9",
          "form": "겨울옷이 가격이 좀 내가봐도 글타~",
          "original_form": "겨울옷이 가격이 좀 내가봐도 글타~",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009396.1.10",
          "form": "세일도 1도 안해주니..",
          "original_form": "세일도 1도 안해주니..",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009396.1.11",
          "form": "응 그러니깐ㅋㅋ",
          "original_form": "응 그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009396.1.12",
          "form": "신상이라 그런가봐",
          "original_form": "신상이라 그런가봐",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009396.1.13",
          "form": "내복도 다 작아졌나?",
          "original_form": "내복도 다 작아졌나?",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009396.1.14",
          "form": "ㅋㅋㅋㅋ여긴 자기들만에 가격책정 기준이 있음 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ여긴 자기들만에 가격책정 기준이 있음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009396.1.15",
          "form": "웅~첫찌꺼 다 새로 사야함..",
          "original_form": "웅~첫찌꺼 다 새로 사야함..",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009396.1.16",
          "form": "아 많이 또 컸구나ㅋ",
          "original_form": "아 많이 또 컸구나ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009396.1.17",
          "form": "둘찌는..뭐..새거 사달라할꺼 뻔하고..",
          "original_form": "둘찌는..뭐..새거 사달라할꺼 뻔하고..",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009396.1.18",
          "form": "옷값도 장난아니다ㅋ",
          "original_form": "옷값도 장난아니다ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009396.1.19",
          "form": "와 둘째껏도 같이 사려면ㅋㅋ",
          "original_form": "와 둘째껏도 같이 사려면ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009396.1.20",
          "form": "늠 말라가 일부러 옷을 맞게 샀디..안카면 허리가 늠 커ㅠ",
          "original_form": "늠 말라가 일부러 옷을 맞게 샀디..안카면 허리가 늠 커ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009396.1.21",
          "form": "그렇지ㅜ울 첫째도 허리가 크더라ㅋ",
          "original_form": "그렇지ㅜ울 첫째도 허리가 크더라ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:13"
        }
      ]
