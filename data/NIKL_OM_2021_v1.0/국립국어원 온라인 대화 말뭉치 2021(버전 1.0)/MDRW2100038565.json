          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038565.1.4",
          "form": "뮤튼 ㅋㅋㅋㅋ",
          "original_form": "뮤튼 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038565.1.5",
          "form": "ㅋㅋㅋ에이포도 많이 샀규먼",
          "original_form": "ㅋㅋㅋ에이포도 많이 샀규먼",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038565.1.6",
          "form": "사람들 타공기로 구멍뚫어서 셀프제본할때",
          "original_form": "사람들 타공기로 구멍뚫어서 셀프제본할때",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038565.1.7",
          "form": "아 언니 프린트기도 있지 ㅋㅋ",
          "original_form": "아 언니 프린트기도 있지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038565.1.8",
          "form": "링끼우던데 ㅋㅋㅋ",
          "original_form": "링끼우던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038565.1.9",
          "form": "코팅기까지사면",
          "original_form": "코팅기까지사면",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038565.1.10",
          "form": "학습소여",
          "original_form": "학습소여",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038565.1.11",
          "form": "ㅋㅋㅋ아주 본격적이야",
          "original_form": "ㅋㅋㅋ아주 본격적이야",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038565.1.12",
          "form": "ㅋㅋㅋㅋ인쇄소임",
          "original_form": "ㅋㅋㅋㅋ인쇄소임",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038565.1.13",
          "form": "계속 쥰비만",
          "original_form": "계속 쥰비만",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038565.1.14",
          "form": "몇달하능가",
          "original_form": "몇달하능가",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038565.1.15",
          "form": "ㅋㅋㅋ하면서 사들이는거쥬",
          "original_form": "ㅋㅋㅋ하면서 사들이는거쥬",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038565.1.16",
          "form": "ㅠㅡㅠ....ㅋㅋㅋㅋㅋ",
          "original_form": "ㅠㅡㅠ....ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038565.1.17",
          "form": "ㅋㅋㅋ내 필기노트? 들고 방문할게여",
          "original_form": "ㅋㅋㅋ내 필기노트? 들고 방문할게여",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.18",
          "form": "신세계 실수직원은",
          "original_form": "신세계 실수직원은",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.19",
          "form": "그럼 짤리나",
          "original_form": "그럼 짤리나",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.20",
          "form": "그거 제본해주세염",
          "original_form": "그거 제본해주세염",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.21",
          "form": "안되",
          "original_form": "안되",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.22",
          "form": "그러게 ㅠㅠ최소 시말서다",
          "original_form": "그러게 ㅠㅠ최소 시말서다",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.23",
          "form": "그거랑 낱장으로 가져오면",
          "original_form": "그거랑 낱장으로 가져오면",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.24",
          "form": "뚫어서해줄순잇오..",
          "original_form": "뚫어서해줄순잇오..",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.26",
          "form": "ㅋㅋㅋㅋ그럼 망쳐도 상관없눈 책부터 해보자",
          "original_form": "ㅋㅋㅋㅋ그럼 망쳐도 상관없눈 책부터 해보자",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038565.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ칼질대서 쮹쭉하는거잇더만",
          "original_form": "ㅋㅋㅋㅋㅋㅋ칼질대서 쮹쭉하는거잇더만",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100038565.1.28",
          "form": "그거잇으면 할만하겟지!?",
          "original_form": "그거잇으면 할만하겟지!?",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100038565.1.29",
          "form": "난 구냥 버릴책자대고 ㅋㅋㅋㅋ자도 없이 그냥 그어버림",
          "original_form": "난 구냥 버릴책자대고 ㅋㅋㅋㅋ자도 없이 그냥 그어버림",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100038565.1.30",
          "form": "참 무식혀 ㅋㅋㅋ",
          "original_form": "참 무식혀 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100038565.1.31",
          "form": "ㅋㅋㅋ어차피 나 보기 편하기만 하면 되지!",
          "original_form": "ㅋㅋㅋ어차피 나 보기 편하기만 하면 되지!",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100038565.1.32",
          "form": "과연? ㅋㅋㅋㅋ",
          "original_form": "과연? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100038565.1.33",
          "form": "집에 자뎅이랑 심우철 구문왓던데",
          "original_form": "집에 자뎅이랑 심우철 구문왓던데",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100038565.1.34",
          "form": "구문해야봐야지",
          "original_form": "구문해야봐야지",
