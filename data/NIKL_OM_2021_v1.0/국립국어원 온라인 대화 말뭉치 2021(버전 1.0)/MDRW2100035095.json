        {
          "id": "MDRW2100035095.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035095.1.2",
          "form": "나 어제 소개팅했다고 한거 기억나지",
          "original_form": "나 어제 소개팅했다고 한거 기억나지",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035095.1.3",
          "form": "어 어땠어",
          "original_form": "어 어땠어",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035095.1.4",
          "form": "첫 인상은 정말 취향이었어",
          "original_form": "첫 인상은 정말 취향이었어",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035095.1.5",
          "form": "외모가 좋은 편이었구나",
          "original_form": "외모가 좋은 편이었구나",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035095.1.6",
          "form": "맞아 일단 키가 커",
          "original_form": "맞아 일단 키가 커",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035095.1.7",
          "form": "대화는 잘 통했니",
          "original_form": "대화는 잘 통했니",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035095.1.8",
          "form": "그게 아쉬운 점이야",
          "original_form": "그게 아쉬운 점이야",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035095.1.9",
          "form": "아 가장 중요한 점인데",
          "original_form": "아 가장 중요한 점인데",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035095.1.10",
          "form": "자꾸 자기 이야기만해",
          "original_form": "자꾸 자기 이야기만해",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035095.1.11",
          "form": "듣지 못하는 사람은 최악이다",
          "original_form": "듣지 못하는 사람은 최악이다",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035095.1.12",
          "form": "맞아 배려심이 부족해보여",
          "original_form": "맞아 배려심이 부족해보여",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035095.1.13",
          "form": "우 머 아쉽다",
          "original_form": "우 머 아쉽다",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035095.1.14",
          "form": "너무",
          "original_form": "너무",
          "speaker_id": "2",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035095.1.15",
          "form": "그래도 일단 한 번더 만나보려고",
          "original_form": "그래도 일단 한 번더 만나보려고",
