      "utterance": [
        {
          "id": "MDRW2100026767.1.1",
          "form": "배부르대...",
          "original_form": "배부르대...",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026767.1.2",
          "form": "0.5인분이야 애들아....",
          "original_form": "0.5인분이야 애들아....",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026767.1.3",
          "form": ".....친구들아... 그거 먹고 어떻게 춤추고 노래하니...",
          "original_form": ".....친구들아... 그거 먹고 어떻게 춤추고 노래하니...",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026767.1.4",
          "form": "젊어서 그런거니....",
          "original_form": "젊어서 그런거니....",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026767.1.5",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ 아니 남자애들인데",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ 아니 남자애들인데",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026767.1.6",
          "form": "1인 1치킨 못한대",
          "original_form": "1인 1치킨 못한대",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026767.1.7",
          "form": "부이앱에서 먹방이라도 하면",
          "original_form": "부이앱에서 먹방이라도 하면",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026767.1.8",
          "form": "아직 음식이가득한데...",
          "original_form": "아직 음식이가득한데...",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026767.1.9",
          "form": "눼....? 1인 1치킨을 못한다니....\n20대 한창 남자들이!!!!",
          "original_form": "눼....? 1인 1치킨을 못한다니....\n20대 한창 남자들이!!!!",
          "speaker_id": "2",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026767.1.10",
          "form": "3인1치킨하나봄",
          "original_form": "3인1치킨하나봄",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026767.1.11",
          "form": "두세조각먹고",
          "original_form": "두세조각먹고",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026767.1.12",
          "form": "안먹는다는데...",
          "original_form": "안먹는다는데...",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026767.1.13",
          "form": "?????\n여자들도 2인 1치킨 아니라요...",
          "original_form": "?????\n여자들도 2인 1치킨 아니라요...",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026767.1.14",
          "form": "아닌가요",
          "original_form": "아닌가요",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026767.1.15",
          "form": "3인 1치킨 무선 129...",
          "original_form": "3인 1치킨 무선 129...",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026767.1.16",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㄱ그니까요",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㄱ그니까요",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026767.1.17",
          "form": "애들다먹으면 두마리시켜서먹더라구요",
          "original_form": "애들다먹으면 두마리시켜서먹더라구요",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026767.1.18",
          "form": "아모리 아이돌이래도",
          "original_form": "아모리 아이돌이래도",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026767.1.19",
          "form": "아니근데잘은 먹어",
          "original_form": "아니근데잘은 먹어",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026767.1.20",
          "form": "적게짧게먹어서글치",
          "original_form": "적게짧게먹어서글치",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026767.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ3인 1치킨이 너무 충격이었습니다..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ3인 1치킨이 너무 충격이었습니다..",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026767.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ 그 부이앱 음식차려놓고 먹는거보면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그 부이앱 음식차려놓고 먹는거보면",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026767.1.23",
          "form": "맘속으로",
          "original_form": "맘속으로",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026767.1.24",
          "form": "음식이아까웠엉...",
          "original_form": "음식이아까웠엉...",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026767.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ우리라면 더 맛깔나게 많은 메뉴를 뿌셨을텐데...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ우리라면 더 맛깔나게 많은 메뉴를 뿌셨을텐데...",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026767.1.26",
          "form": "그래두 못먹는거 아니고",
          "original_form": "그래두 못먹는거 아니고",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026767.1.27",
          "form": "잘먹고다니니깐  괜찮앙",
          "original_form": "잘먹고다니니깐  괜찮앙",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026767.1.28",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026767.1.29",
          "form": "ㅋㅋㄱㄱㅋㄱ잘은먹구다녀",
          "original_form": "ㅋㅋㄱㄱㅋㄱ잘은먹구다녀",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026767.1.30",
          "form": "다들 맛난거먹고다니라궁",
          "original_form": "다들 맛난거먹고다니라궁",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026767.1.31",
          "form": "먹는게젤중요",
          "original_form": "먹는게젤중요",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026767.1.32",
          "form": "개말라인간보단",
          "original_form": "개말라인간보단",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026767.1.33",
          "form": "건강한게최고다",
          "original_form": "건강한게최고다",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026767.1.34",
          "form": "오 엔시티는 잘 먹는친구들이군여",
          "original_form": "오 엔시티는 잘 먹는친구들이군여",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026767.1.35",
          "form": "먹방도 보는맛있겠군",
          "original_form": "먹방도 보는맛있겠군",
          "speaker_id": "1",
          "time": "20210902 00:21"
        }
      ]
