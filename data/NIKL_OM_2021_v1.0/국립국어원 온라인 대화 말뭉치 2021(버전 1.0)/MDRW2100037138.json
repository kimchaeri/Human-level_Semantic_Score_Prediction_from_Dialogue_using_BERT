      "utterance": [
        {
          "id": "MDRW2100037138.1.1",
          "form": "아이유 그 딩고채널에 라이브보셨어요?",
          "original_form": "아이유 그 딩고채널에 라이브보셨어요?",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100037138.1.2",
          "form": "노래 부르응거여?",
          "original_form": "노래 부르응거여?",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100037138.1.3",
          "form": "완전.. 능력자..",
          "original_form": "완전.. 능력자..",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100037138.1.4",
          "form": "네네네 그 음향되게좋은 ㅠㅠ",
          "original_form": "네네네 그 음향되게좋은 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100037138.1.5",
          "form": "아이유때문에 첨알앗는데 진짜 미쳣더라구요",
          "original_form": "아이유때문에 첨알앗는데 진짜 미쳣더라구요",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037138.1.6",
          "form": "성시경도 추천드림당..",
          "original_form": "성시경도 추천드림당..",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037138.1.7",
          "form": "들었어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "들었어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100037138.1.8",
          "form": "성시경한테 별 관심 없었는데",
          "original_form": "성시경한테 별 관심 없었는데",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100037138.1.9",
          "form": "목소리가 정말 좋더라구요",
          "original_form": "목소리가 정말 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100037138.1.10",
          "form": "그쵸 ㅠㅠㅠ지금 비도오는데 감성돋게 간만에 봐야겠어요 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅠㅠㅠ지금 비도오는데 감성돋게 간만에 봐야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100037138.1.11",
          "form": "ㅋㅋㅋㅋㅋ 어떤 놀9",
          "original_form": "ㅋㅋㅋㅋㅋ 어떤 놀9",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100037138.1.12",
          "form": "제일 좋아하시나요",
          "original_form": "제일 좋아하시나요",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100037138.1.13",
          "form": "저 다양하게 들어요 ㅋㅋㅋㅋ 발라드도좋아하구 댄스도좋아하구요 ㅋㅋㅋㅋ",
          "original_form": "저 다양하게 들어요 ㅋㅋㅋㅋ 발라드도좋아하구 댄스도좋아하구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037138.1.14",
          "form": "아 성시경노래 말씀하세여?",
          "original_form": "아 성시경노래 말씀하세여?",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037138.1.15",
          "form": "네네ㅋㅋㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037138.1.16",
          "form": "성시경 내게오는길이랑 넌감동이었어 희재 좋으해요!!",
          "original_form": "성시경 내게오는길이랑 넌감동이었어 희재 좋으해요!!",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037138.1.17",
          "form": "하 감성 돋네요 ㅋㅋㅋㅋㅋ",
          "original_form": "하 감성 돋네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037138.1.18",
          "form": "아 !!! 희재 좋아요",
          "original_form": "아 !!! 희재 좋아요",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037138.1.19",
          "form": "전 거리에서 이 노래도요",
          "original_form": "전 거리에서 이 노래도요",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037138.1.20",
          "form": "이 노래 들으면 왜이리 아련해지는지..",
          "original_form": "이 노래 들으면 왜이리 아련해지는지..",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037138.1.21",
          "form": "그쵸 ㅠㅠ비오는날 감성에도 딱입니다 ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅠㅠ비오는날 감성에도 딱입니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037138.1.22",
          "form": "엔시티노래 중에서는 어느곡 젤 좋아하세여?",
          "original_form": "엔시티노래 중에서는 어느곡 젤 좋아하세여?",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100037138.1.23",
          "form": "저 백야 좋아해요",
          "original_form": "저 백야 좋아해요",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037138.1.24",
          "form": "딱 하나를 고르긴 어려워요ㅋㅋㅠ",
          "original_form": "딱 하나를 고르긴 어려워요ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037138.1.25",
          "form": "오 그렇군요 저는 보스 들어봤어요 ㅋㅋㅋㅋ",
          "original_form": "오 그렇군요 저는 보스 들어봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037138.1.26",
          "form": "운동할때가끔들어요",
          "original_form": "운동할때가끔들어요",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037138.1.27",
          "form": "보스 좋죠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "보스 좋죠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037138.1.28",
          "form": "운동 할 때 듣는다니",
          "original_form": "운동 할 때 듣는다니",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037138.1.29",
          "form": "신선하네요",
          "original_form": "신선하네요",
          "speaker_id": "2",
