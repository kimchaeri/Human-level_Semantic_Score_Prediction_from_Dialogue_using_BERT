        },
        {
          "id": "MDRW2100023058.1.2",
          "form": "전 계속 없다가 요즘 근육운동에 관심있어요",
          "original_form": "전 계속 없다가 요즘 근육운동에 관심있어요",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023058.1.3",
          "form": "오 어떤거요?",
          "original_form": "오 어떤거요?",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023058.1.4",
          "form": "탄력있는 몸이 갖고싶더라구요",
          "original_form": "탄력있는 몸이 갖고싶더라구요",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023058.1.5",
          "form": "롤모델있으세요?",
          "original_form": "롤모델있으세요?",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023058.1.6",
          "form": "로제요",
          "original_form": "로제요",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023058.1.7",
          "form": "아 로제는 넘 마르지않았나요ㅠㅠ",
          "original_form": "아 로제는 넘 마르지않았나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023058.1.8",
          "form": "공연 영상을 봤는데 격한춤을 춰도 떨릴 살이없더라구요",
          "original_form": "공연 영상을 봤는데 격한춤을 춰도 떨릴 살이없더라구요",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023058.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023058.1.10",
          "form": "살이 근육에 착 붙어있는 느낌?",
          "original_form": "살이 근육에 착 붙어있는 느낌?",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023058.1.11",
          "form": "운동도 엄청할테니 탄력도 있겠죠",
          "original_form": "운동도 엄청할테니 탄력도 있겠죠",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023058.1.12",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023058.1.13",
          "form": "사복패션도 엄청 예쁘잖아요",
          "original_form": "사복패션도 엄청 예쁘잖아요",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023058.1.14",
          "form": "얼굴도 얼굴인데 핏은 진짜 ...",
          "original_form": "얼굴도 얼굴인데 핏은 진짜 ...",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100023058.1.15",
          "form": "갑자기 운동하고싶네요",
          "original_form": "갑자기 운동하고싶네요",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100023058.1.16",
          "form": ".마른편이세요?",
          "original_form": ".마른편이세요?",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100023058.1.17",
          "form": "마른편이긴한데 근육없이지방이 살짝 있는 느낌?",
          "original_form": "마른편이긴한데 근육없이지방이 살짝 있는 느낌?",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100023058.1.18",
          "form": "이제 선선해졌으니 아침운동이나 조깅좀 해볼까봐요",
          "original_form": "이제 선선해졌으니 아침운동이나 조깅좀 해볼까봐요",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100023058.1.19",
          "form": "올해 가기전까지 ?",
          "original_form": "올해 가기전까지 ?",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100023058.1.20",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100023058.1.21",
          "form": "그래서 운동계획이 어떻게 돼요?",
          "original_form": "그래서 운동계획이 어떻게 돼요?",
          "speaker_id": "1",
          "time": "20210829 21:19"
        }
