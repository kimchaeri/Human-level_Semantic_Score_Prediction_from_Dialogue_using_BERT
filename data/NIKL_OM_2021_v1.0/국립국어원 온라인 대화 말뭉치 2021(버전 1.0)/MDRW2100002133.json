      "utterance": [
        {
          "id": "MDRW2100002133.1.1",
          "form": "싸이월드 부활한대 들었어?",
          "original_form": "싸이월드 부활한대 들었어?",
          "speaker_id": "1",
          "time": "20210819 23:51"
        },
        {
          "id": "MDRW2100002133.1.2",
          "form": "ㅇㅇ 너 싸이 했었어?",
          "original_form": "ㅇㅇ 너 싸이 했었어?",
          "speaker_id": "2",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002133.1.3",
          "form": "어릴때 안해본 사람 있겠어?ㅋㅋㅋ",
          "original_form": "어릴때 안해본 사람 있겠어?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:53"
        },
        {
          "id": "MDRW2100002133.1.4",
          "form": "너도 도토리질 좀 했구나 ㅋㅋㅋ",
          "original_form": "너도 도토리질 좀 했구나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002133.1.5",
          "form": "우리또래는 싸이월드 특유의 감성이 있지..",
          "original_form": "우리또래는 싸이월드 특유의 감성이 있지..",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100002133.1.6",
          "form": "또래 ㅋㅋㅋ 몇살인데",
          "original_form": "또래 ㅋㅋㅋ 몇살인데",
          "speaker_id": "1",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002133.1.7",
          "form": "8090세대는 싸이월드 모를수가 없다 진짜",
          "original_form": "8090세대는 싸이월드 모를수가 없다 진짜",
          "speaker_id": "2",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002133.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002133.1.9",
          "form": "부활 기대돼",
          "original_form": "부활 기대돼",
          "speaker_id": "2",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002133.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ부활한다고 사람들이 할까?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ부활한다고 사람들이 할까?",
          "speaker_id": "1",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002133.1.11",
          "form": "다시 활성화되긴 힘들어도",
          "original_form": "다시 활성화되긴 힘들어도",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002133.1.12",
          "form": "추억이있으니 다들 한번씩 들르겠지?",
          "original_form": "추억이있으니 다들 한번씩 들르겠지?",
          "speaker_id": "2",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002133.1.13",
          "form": "맞아 옛날사진 한번씩보구 ㅋㅋㅋ",
          "original_form": "맞아 옛날사진 한번씩보구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002133.1.14",
          "form": "그때 감성 그립군ㅋㅋㅋㅋ",
          "original_form": "그때 감성 그립군ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002133.1.15",
          "form": "웅ㅋㅋㅋ 요즘애들은 모르겠지...",
          "original_form": "웅ㅋㅋㅋ 요즘애들은 모르겠지...",
          "speaker_id": "1",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002133.1.16",
          "form": "그땐 연예인들도 많이해서",
          "original_form": "그땐 연예인들도 많이해서",
          "speaker_id": "2",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002133.1.17",
          "form": "채연 눈물짤같은것두 다 싸이에서 나왔잖아ㅋㅋㅋ",
          "original_form": "채연 눈물짤같은것두 다 싸이에서 나왔잖아ㅋㅋㅋ",
          "speaker_id": "2",
