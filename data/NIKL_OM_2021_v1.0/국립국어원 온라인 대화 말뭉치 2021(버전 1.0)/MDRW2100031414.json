        {
          "id": "MDRW2100031414.1.1",
          "form": "티비 프로그램 보나요",
          "original_form": "티비 프로그램 보나요",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031414.1.2",
          "form": "다\u0000운받아서봅니당",
          "original_form": "다\u0000운받아서봅니당",
          "speaker_id": "2",
          "time": "20210904 21:55"
        },
        {
          "id": "MDRW2100031414.1.3",
          "form": "재밌는거있나요",
          "original_form": "재밌는거있나요",
          "speaker_id": "1",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100031414.1.4",
          "form": "글쎄요.. 그것이알고싶다?",
          "original_form": "글쎄요.. 그것이알고싶다?",
          "speaker_id": "2",
          "time": "20210906 07:35"
        },
        {
          "id": "MDRW2100031414.1.5",
          "form": "전 예능봐요",
          "original_form": "전 예능봐요",
          "speaker_id": "1",
          "time": "20210906 07:41"
        },
        {
          "id": "MDRW2100031414.1.6",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210906 07:48"
        },
        {
          "id": "MDRW2100031414.1.7",
          "form": "골목식당, 나혼자산다, 놀면뭐하니 등이요",
          "original_form": "골목식당, 나혼자산다, 놀면뭐하니 등이요",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100031414.1.8",
          "form": "많이보시네여",
          "original_form": "많이보시네여",
          "speaker_id": "2",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100031414.1.9",
          "form": "유튜브 삼프로티비 이효석아카데미 골드러시 각도기티비 등을 추천합니다",
          "original_form": "유튜브 삼프로티비 이효석아카데미 골드러시 각도기티비 등을 추천합니다",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100031414.1.10",
          "form": "유튜브 많이 보시네요",
          "original_form": "유튜브 많이 보시네요",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100031414.1.11",
          "form": "유튜브는 사랑입니다",
          "original_form": "유튜브는 사랑입니다",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100031414.1.12",
          "form": "유튜브 재밌는게 많은거 같아요",
          "original_form": "유튜브 재밌는게 많은거 같아요",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100031414.1.13",
          "form": "네 유튜브가 그래서 수익이 어마어마하죠",
          "original_form": "네 유튜브가 그래서 수익이 어마어마하죠",
          "speaker_id": "2",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100031414.1.14",
          "form": "그래서 연예인들이 유튜브 많이 하나봐요",
          "original_form": "그래서 연예인들이 유튜브 많이 하나봐요",
          "speaker_id": "1",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100031414.1.15",
          "form": "아 그거 돈 엄청되죠 유튜브갑부",
