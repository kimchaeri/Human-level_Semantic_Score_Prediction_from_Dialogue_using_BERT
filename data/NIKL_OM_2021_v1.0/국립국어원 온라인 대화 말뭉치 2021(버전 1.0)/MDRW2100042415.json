      "utterance": [
        {
          "id": "MDRW2100042415.1.1",
          "form": "유튜브가 나을거 같아요",
          "original_form": "유튜브가 나을거 같아요",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.2",
          "form": "만든다말인가요????",
          "original_form": "만든다말인가요????",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.3",
          "form": "그런거 배우는건 ㅋㅋㅋㅋ",
          "original_form": "그런거 배우는건 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.4",
          "form": "허허허",
          "original_form": "허허허",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.5",
          "form": "그러게여",
          "original_form": "그러게여",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.6",
          "form": "저 유튜브 잘안봤는데",
          "original_form": "저 유튜브 잘안봤는데",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.7",
          "form": "제빵하려면 무조건 봐야되겠더라고요",
          "original_form": "제빵하려면 무조건 봐야되겠더라고요",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.8",
          "form": "가끔... 밖에 나가고싶을때 좋은거같아욬ㅋㅋ체험",
          "original_form": "가끔... 밖에 나가고싶을때 좋은거같아욬ㅋㅋ체험",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 마실겸",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 마실겸",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042415.1.10",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042415.1.11",
          "form": "그냥 사람이가끔 그리울때",
          "original_form": "그냥 사람이가끔 그리울때",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042415.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042415.1.13",
          "form": "ㅋㅋㅋㅋ 아 그럴때!!",
          "original_form": "ㅋㅋㅋㅋ 아 그럴때!!",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042415.1.14",
          "form": "체험 좋은거 같아요",
          "original_form": "체험 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042415.1.15",
          "form": "가죽공예도 좀 궁금해요",
          "original_form": "가죽공예도 좀 궁금해요",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042415.1.16",
          "form": "식사 메뉴를 말해야겠지만",
          "original_form": "식사 메뉴를 말해야겠지만",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042415.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042415.1.18",
          "form": "가죽공예도 맞아요 한번쯤하고싶어요",
          "original_form": "가죽공예도 맞아요 한번쯤하고싶어요",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042415.1.19",
          "form": "하여튼 체험 재미있어보이긴 한데 커리큘럼 잘보고 가야돼요 ㅋㅋㅋ",
          "original_form": "하여튼 체험 재미있어보이긴 한데 커리큘럼 잘보고 가야돼요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042415.1.20",
          "form": "가죽공예 가서도 가죽 잘 자르는게 궁금한데 엉뚱한거만 하고 올수도 있어서 ㅋㅋㅋㅋ",
          "original_form": "가죽공예 가서도 가죽 잘 자르는게 궁금한데 엉뚱한거만 하고 올수도 있어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042415.1.21",
          "form": "하여튼 떡 만들기 이런것도 재미있어 보였어요",
          "original_form": "하여튼 떡 만들기 이런것도 재미있어 보였어요",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042415.1.22",
          "form": "우와 떡만들기도 있엉ㅅ???어요???",
          "original_form": "우와 떡만들기도 있엉ㅅ???어요???",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042415.1.23",
          "form": "아니 무슨",
          "original_form": "아니 무슨",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042415.1.24",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042415.1.25",
          "form": "다 만드네욬ㅋㅋㅋㅋㅋ",
          "original_form": "다 만드네욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042415.1.26",
          "form": "제가모르는사잌ㅋㅋㅋㅋㅋ",
          "original_form": "제가모르는사잌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042415.1.27",
          "form": "그런거 많더라고요!!",
          "original_form": "그런거 많더라고요!!",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042415.1.28",
          "form": "오오.... 뭔가 빵부터만들어보고싶긴해욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오오.... 뭔가 빵부터만들어보고싶긴해욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042415.1.29",
          "form": "떡보다",
          "original_form": "떡보다",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
