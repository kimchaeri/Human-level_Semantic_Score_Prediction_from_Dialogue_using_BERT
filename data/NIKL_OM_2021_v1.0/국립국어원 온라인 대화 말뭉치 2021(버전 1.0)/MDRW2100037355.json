      "utterance": [
        {
          "id": "MDRW2100037355.1.1",
          "form": "너 충전기 정품 써?",
          "original_form": "너 충전기 정품 써?",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037355.1.2",
          "form": "충전기 선을 어디서 사야 잘샀다고 소문이ㅠ날까?",
          "original_form": "충전기 선을 어디서 사야 잘샀다고 소문이ㅠ날까?",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037355.1.3",
          "form": "정품선은 너무 비싼 거 같아.",
          "original_form": "정품선은 너무 비싼 거 같아.",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037355.1.4",
          "form": "나 집에 커피스틱을 사둘까?",
          "original_form": "나 집에 커피스틱을 사둘까?",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.5",
          "form": "쟈뎅 커피 먹어봤어?",
          "original_form": "쟈뎅 커피 먹어봤어?",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.6",
          "form": "완전 추천해",
          "original_form": "완전 추천해",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.7",
          "form": "사두면 많이 먹으려나??",
          "original_form": "사두면 많이 먹으려나??",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.8",
          "form": "근데 집에 커피스틱이 있어도 카페커피를 마시고싳더라",
          "original_form": "근데 집에 커피스틱이 있어도 카페커피를 마시고싳더라",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.9",
          "form": "너무 피곤하면 마시긴해",
          "original_form": "너무 피곤하면 마시긴해",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.10",
          "form": "사실 집에 많아서 이번에 집에가면 좀 들고오려구",
          "original_form": "사실 집에 많아서 이번에 집에가면 좀 들고오려구",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.11",
          "form": "나가기귀찮은 날 있잖아",
          "original_form": "나가기귀찮은 날 있잖아",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.12",
          "form": "쟈뎅커피는 맛있어.",
          "original_form": "쟈뎅커피는 맛있어.",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.13",
          "form": "집에는 뭘 사둔거야?",
          "original_form": "집에는 뭘 사둔거야?",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037355.1.14",
          "form": "맞아 나 쟈뎅커피있어",
          "original_form": "맞아 나 쟈뎅커피있어",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037355.1.15",
          "form": "이디야커피스틱은 별로였어",
          "original_form": "이디야커피스틱은 별로였어",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037355.1.16",
          "form": "너 완전 귀신이구나",
          "original_form": "너 완전 귀신이구나",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037355.1.17",
          "form": "좋은 선택을 했구나",
          "original_form": "좋은 선택을 했구나",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037355.1.18",
          "form": "나 너희집 단골이잖아",
          "original_form": "나 너희집 단골이잖아",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037355.1.19",
          "form": "그리고 루카스인가 그거도 있어",
          "original_form": "그리고 루카스인가 그거도 있어",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037355.1.20",
          "form": "루카스는 맛있어?",
          "original_form": "루카스는 맛있어?",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037355.1.21",
          "form": "이건 산거는 아니고 어디서 받았어",
          "original_form": "이건 산거는 아니고 어디서 받았어",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037355.1.22",
          "form": "응 그냥 그저그래",
          "original_form": "응 그냥 그저그래",
          "speaker_id": "2",
          "time": "20210831 22:52"
        }
      ]
