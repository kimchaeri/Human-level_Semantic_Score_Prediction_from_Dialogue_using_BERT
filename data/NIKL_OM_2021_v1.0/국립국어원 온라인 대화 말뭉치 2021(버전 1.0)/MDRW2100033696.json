      "utterance": [
        {
          "id": "MDRW2100033696.1.1",
          "form": "name2님은 어떤 음식을 좋아하시나요?",
          "original_form": "&name2&님은 어떤 음식을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100033696.1.2",
          "form": "전 고기 1순위요ㅎ",
          "original_form": "전 고기 1순위요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033696.1.3",
          "form": "아아 고기 좋아하시는구나",
          "original_form": "아아 고기 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033696.1.4",
          "form": "달고 짜고 맵고 주로 자극적인거 좋아해요",
          "original_form": "달고 짜고 맵고 주로 자극적인거 좋아해요",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033696.1.5",
          "form": "어떤 고기를 특히 좋아하셔요?ㅎㅎ",
          "original_form": "어떤 고기를 특히 좋아하셔요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033696.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033696.1.7",
          "form": "소고기 돼지고기요ㅎ",
          "original_form": "소고기 돼지고기요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033696.1.8",
          "form": "아 그럼 제육볶음 이런 걸 좋아하시나요?",
          "original_form": "아 그럼 제육볶음 이런 걸 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033696.1.9",
          "form": "맵고 달고 고기",
          "original_form": "맵고 달고 고기",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033696.1.10",
          "form": "고기는 구워먹는거좋아해요",
          "original_form": "고기는 구워먹는거좋아해요",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033696.1.11",
          "form": "갈비양념 좋아하고",
          "original_form": "갈비양념 좋아하고",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033696.1.12",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033696.1.13",
          "form": "저도 고기 구워먹는 것 좋아해요",
          "original_form": "저도 고기 구워먹는 것 좋아해요",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033696.1.14",
          "form": "지난 주말에도 삼겹살 구워서 비빔면이랑 먹었어요ㅎㅎ",
          "original_form": "지난 주말에도 삼겹살 구워서 비빔면이랑 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033696.1.15",
          "form": "오 맛있었겠어요ㅎ",
          "original_form": "오 맛있었겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033696.1.16",
          "form": "전주로 양파볶고 김치구워서 소스랑같이",
          "original_form": "전주로 양파볶고 김치구워서 소스랑같이",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100033696.1.17",
          "form": "오 제대로 즐기시는군요ㅎㅎ",
          "original_form": "오 제대로 즐기시는군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100033696.1.18",
          "form": "고기 구울 때 양파 마늘 김치 이런 것 같이 구우면 꿀맛이죠ㅎㅎ",
          "original_form": "고기 구울 때 양파 마늘 김치 이런 것 같이 구우면 꿀맛이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100033696.1.19",
          "form": "맞아요 요즘 회식도없어서 집에서라도 먹어요",
          "original_form": "맞아요 요즘 회식도없어서 집에서라도 먹어요",
          "speaker_id": "2",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100033696.1.20",
          "form": "그쵸 아무래도 회식이 없으면 밖에서 고기 구워먹을 일은 거의 없어서...",
          "original_form": "그쵸 아무래도 회식이 없으면 밖에서 고기 구워먹을 일은 거의 없어서...",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033696.1.21",
          "form": "그래도 집에서 name2님 스타일대로 조리해 먹을 수 있으니까",
          "original_form": "그래도 집에서 &name2&님 스타일대로 조리해 먹을 수 있으니까",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033696.1.22",
          "form": "구우면서 먹어야되는데 집에서 후라이팬에 먹어서",
          "original_form": "구우면서 먹어야되는데 집에서 후라이팬에 먹어서",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033696.1.23",
          "form": "확실히 외식이~~^^",
          "original_form": "확실히 외식이~~^^",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033696.1.24",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033696.1.25",
          "form": "요즘은 집에서 고기 구워먹을 수 있는 도구들도 많이 나오던데",
          "original_form": "요즘은 집에서 고기 구워먹을 수 있는 도구들도 많이 나오던데",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100033696.1.26",
          "form": "미니 화로도 나오긴하더라구요ㅎ",
