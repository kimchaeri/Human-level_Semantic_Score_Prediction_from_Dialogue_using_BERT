      "utterance": [
        {
          "id": "MDRW2100035230.1.1",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100035230.1.2",
          "form": "저 참기름, 들기름 싫어하는데 여긴 오케이에요",
          "original_form": "저 참기름, 들기름 싫어하는데 여긴 오케이에요",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100035230.1.3",
          "form": "잘어울리더라구요!",
          "original_form": "잘어울리더라구요!",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100035230.1.4",
          "form": "오오 !!!",
          "original_form": "오오 !!!",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100035230.1.5",
          "form": "다음주에 바로 먹어볼께요 ㅋㅋ",
          "original_form": "다음주에 바로 먹어볼께요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100035230.1.6",
          "form": "국수에는 들기름이죠 ㅎㅎㅎ",
          "original_form": "국수에는 들기름이죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100035230.1.7",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100035230.1.8",
          "form": "아 그게 들기름이였군요ㅋㅋ",
          "original_form": "아 그게 들기름이였군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100035230.1.9",
          "form": "둘 중 뭔지 몰랐어요ㅋㅋ",
          "original_form": "둘 중 뭔지 몰랐어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100035230.1.10",
          "form": "약간 참기름은 비빔밥용이고",
          "original_form": "약간 참기름은 비빔밥용이고",
          "speaker_id": "1",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100035230.1.11",
          "form": "들기름은 국수 느낌이에요 ㅎㅎ",
          "original_form": "들기름은 국수 느낌이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100035230.1.12",
          "form": "둘 다 느끼해서 안좋아하시나요?!",
          "original_form": "둘 다 느끼해서 안좋아하시나요?!",
          "speaker_id": "1",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100035230.1.13",
          "form": "아항ㅋㅋ",
          "original_form": "아항ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100035230.1.14",
          "form": "제가 요리하면 안넣어욥",
          "original_form": "제가 요리하면 안넣어욥",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100035230.1.15",
          "form": "향이 강하게 느껴지더라구요ㅜ",
          "original_form": "향이 강하게 느껴지더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100035230.1.16",
          "form": "아아 맞죠 ㅜㅜ 향이 세긴해요",
          "original_form": "아아 맞죠 ㅜㅜ 향이 세긴해요",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100035230.1.17",
          "form": "들기름 막국수도 맛있는데 안좋아하실거같네요 ㅠㅠ",
          "original_form": "들기름 막국수도 맛있는데 안좋아하실거같네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100035230.1.18",
          "form": "앗...들기름만 들어간건가요?",
          "original_form": "앗...들기름만 들어간건가요?",
          "speaker_id": "2",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100035230.1.19",
          "form": "들기름 + 김가루인데 모밀소바랑 비슷한 느낌이에요 !",
          "original_form": "들기름 + 김가루인데 모밀소바랑 비슷한 느낌이에요 !",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100035230.1.20",
          "form": "오.. 궁금하네요!",
          "original_form": "오.. 궁금하네요!",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100035230.1.21",
          "form": "사진보고왔는데 너무 맛있어보이는걸요?!",
          "original_form": "사진보고왔는데 너무 맛있어보이는걸요?!",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100035230.1.22",
          "form": "네 저도 기름이여서 느끼할줄 알았는데 맛있었어요 !",
          "original_form": "네 저도 기름이여서 느끼할줄 알았는데 맛있었어요 !",
          "speaker_id": "1",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035230.1.23",
          "form": "들기름 안좋아하시니까 ㅠㅠ 혹시나 가족분들이 드시면 한입 드셔보셔요 ㅎㅎ",
          "original_form": "들기름 안좋아하시니까 ㅠㅠ 혹시나 가족분들이 드시면 한입 드셔보셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100035230.1.24",
          "form": "오.. 그래야겠어욥",
          "original_form": "오.. 그래야겠어욥",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035230.1.25",
          "form": "부모님은 좋아하실거 같아오!",
          "original_form": "부모님은 좋아하실거 같아오!",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035230.1.26",
          "form": "아 맞아요 저희 부모님도 좋아하셨어요 !",
          "original_form": "아 맞아요 저희 부모님도 좋아하셨어요 !",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100035230.1.27",
          "form": "가끔 티몬에 1900원으로 떠요 ㅎㅎ",
          "original_form": "가끔 티몬에 1900원으로 떠요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100035230.1.28",
          "form": "오옹 대박.. 티몬부터 깔아야겠네요ㅋㅋㅋ",
          "original_form": "오옹 대박.. 티몬부터 깔아야겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:06"
        }
      ]
    }
