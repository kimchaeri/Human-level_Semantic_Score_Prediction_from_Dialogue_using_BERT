      "utterance": [
        {
          "id": "MDRW2100011014.1.1",
          "form": "배달 많이시키시나여?",
          "original_form": "배달 많이시키시나여?",
          "speaker_id": "1",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011014.1.2",
          "form": "네 오늘도 전시켜서 포도주 먹었어요",
          "original_form": "네 오늘도 전시켜서 포도주 먹었어요",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011014.1.3",
          "form": "배달 많이 드세요?",
          "original_form": "배달 많이 드세요?",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011014.1.4",
          "form": "포도주도 배달되요? 전 3년ㅇ 한전 시킬까 말까해요",
          "original_form": "포도주도 배달되요? 전 3년ㅇ 한전 시킬까 말까해요",
          "speaker_id": "1",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011014.1.5",
          "form": "아뇨 전만 배달하고",
          "original_form": "아뇨 전만 배달하고",
          "speaker_id": "2",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011014.1.6",
          "form": "포도주는 있었어요 ㅋㅋ",
          "original_form": "포도주는 있었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011014.1.7",
          "form": "뭐랑 마신거에요? 안주시킨거에요?",
          "original_form": "뭐랑 마신거에요? 안주시킨거에요?",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100011014.1.8",
          "form": "전이 안주 였구요",
          "original_form": "전이 안주 였구요",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011014.1.9",
          "form": "막서리대신 포도주요",
          "original_form": "막서리대신 포도주요",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011014.1.10",
          "form": "모듬전이 비와서 그런가 맛났러요",
          "original_form": "모듬전이 비와서 그런가 맛났러요",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011014.1.11",
          "form": "아..'나'를 뜻하는 '전'인줄.. ㅎㅎㅎ",
          "original_form": "아..'나'를 뜻하는 '전'인줄.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011014.1.12",
          "form": "전도 배달이 돼요?",
          "original_form": "전도 배달이 돼요?",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011014.1.13",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011014.1.14",
          "form": "배달이 되더라구요",
          "original_form": "배달이 되더라구요",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011014.1.15",
          "form": "싱기하다.. 배달 잘 안먹어서 몰랏어요..",
          "original_form": "싱기하다.. 배달 잘 안먹어서 몰랏어요..",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011014.1.16",
          "form": "근데 늦게 시작해서 기다려서 시켰어요",
          "original_form": "근데 늦게 시작해서 기다려서 시켰어요",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011014.1.17",
          "form": "가격은 적당한가여",
          "original_form": "가격은 적당한가여",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011014.1.18",
          "form": "네 신장개업이라",
          "original_form": "네 신장개업이라",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011014.1.19",
          "form": "진짜 배터지게 먹었는데",
          "original_form": "진짜 배터지게 먹었는데",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011014.1.20",
          "form": "23000원요",
          "original_form": "23000원요",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011014.1.21",
          "form": "최저가 같아요",
          "original_form": "최저가 같아요",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011014.1.22",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011014.1.23",
          "form": "저는 저녘은 항상 샐러드요.. ㅎㅎㅎ",
          "original_form": "저는 저녘은 항상 샐러드요.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011014.1.24",
          "form": "어머 건강하게 사시네요",
          "original_form": "어머 건강하게 사시네요",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011014.1.25",
          "form": "식탐이 없어서.. 배만 부름 되요..",
          "original_form": "식탐이 없어서.. 배만 부름 되요..",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011014.1.26",
          "form": "와 최고입니다",
          "original_form": "와 최고입니다",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011014.1.27",
          "form": "부러워요 ㅜㅜ",
          "original_form": "부러워요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011014.1.28",
          "form": "이상하게 술안마심 식욕이 없어요..",
          "original_form": "이상하게 술안마심 식욕이 없어요..",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011014.1.29",
          "form": "근데 술을 매일 마심.. ㅋㅋㅋ",
          "original_form": "근데 술을 매일 마심.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011014.1.30",
          "form": "ㅋㅋ 전 조금 마셔요 약해요",
          "original_form": "ㅋㅋ 전 조금 마셔요 약해요",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011014.1.31",
          "form": "좋아하긴해요 ㅋㅋ",
          "original_form": "좋아하긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011014.1.32",
          "form": "샐러드만 드시고 버티시고 저도 식욕좀 없음좋겠네요 ㅎㅎ",
          "original_form": "샐러드만 드시고 버티시고 저도 식욕좀 없음좋겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:05"
        }
      ]
