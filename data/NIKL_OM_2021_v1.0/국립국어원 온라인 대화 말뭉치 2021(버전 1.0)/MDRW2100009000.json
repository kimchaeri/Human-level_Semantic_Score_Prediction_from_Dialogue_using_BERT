      "utterance": [
        {
          "id": "MDRW2100009000.1.1",
          "form": "오 연애중이신가요~?",
          "original_form": "오 연애중이신가요~?",
          "speaker_id": "2",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100009000.1.2",
          "form": "연애중이긴 해요ㅎㅎ 연애하시나요?",
          "original_form": "연애중이긴 해요ㅎㅎ 연애하시나요?",
          "speaker_id": "1",
          "time": "20210822 20:21"
        },
        {
          "id": "MDRW2100009000.1.3",
          "form": "아뇨~ 6개월째 쉬고있어요",
          "original_form": "아뇨~ 6개월째 쉬고있어요",
          "speaker_id": "2",
          "time": "20210822 20:22"
        },
        {
          "id": "MDRW2100009000.1.4",
          "form": "아직은 솔로의 자유에 만족하고있어요ㅎㅎ",
          "original_form": "아직은 솔로의 자유에 만족하고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 20:22"
        },
        {
          "id": "MDRW2100009000.1.5",
          "form": "연애할땐 또 솔로일때가 좋았던거같기도 하구 그래요",
          "original_form": "연애할땐 또 솔로일때가 좋았던거같기도 하구 그래요",
          "speaker_id": "1",
          "time": "20210822 20:23"
        },
        {
          "id": "MDRW2100009000.1.6",
          "form": "그래도 연애하면 좋죠~~",
          "original_form": "그래도 연애하면 좋죠~~",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100009000.1.7",
          "form": "같이 있어도 외로울때가 있는거같아요ㅎㅎ",
          "original_form": "같이 있어도 외로울때가 있는거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100009000.1.8",
          "form": "그맘 알죠 이게 외로움이 온전히 채워지기는 쉽지 않은거 같아요",
          "original_form": "그맘 알죠 이게 외로움이 온전히 채워지기는 쉽지 않은거 같아요",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009000.1.9",
          "form": "맞아요 그걸 기대하면 안되는데 맘같지가 않아요ㅎㅎ",
          "original_form": "맞아요 그걸 기대하면 안되는데 맘같지가 않아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100009000.1.10",
          "form": "기대하다가 맘상하고 또 외로워지는 악순환이 되는데 그럴땐 잠~깐 다른 일에 집중을 해보시는것도 좋다고생각해요",
          "original_form": "기대하다가 맘상하고 또 외로워지는 악순환이 되는데 그럴땐 잠~깐 다른 일에 집중을 해보시는것도 좋다고생각해요",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100009000.1.11",
          "form": "맞아요 그래야겠어요ㅎㅎ 멀어질거같아서ㅠㅠ",
          "original_form": "맞아요 그래야겠어요ㅎㅎ 멀어질거같아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009000.1.12",
          "form": "또 조금 거리두고 있다보면 자연스럽게 가까워질거라고 생각해요",
          "original_form": "또 조금 거리두고 있다보면 자연스럽게 가까워질거라고 생각해요",
          "speaker_id": "2",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100009000.1.13",
          "form": "네 맞아요 아휴 맘같지가 않아요 정말",
          "original_form": "네 맞아요 아휴 맘같지가 않아요 정말",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100009000.1.14",
          "form": "사람맘이 어떻게 그렇게 맘대로되겠어요ㅎㅎ",
          "original_form": "사람맘이 어떻게 그렇게 맘대로되겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100009000.1.15",
          "form": "그러게요...ㅎㅎ",
          "original_form": "그러게요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100009000.1.16",
          "form": "그냥 내맘 내맘대로 안되는것처럼 상대방 마음도 그렇겠지...하고 받아들이는게 그나마 편하죠ㅜ",
          "original_form": "그냥 내맘 내맘대로 안되는것처럼 상대방 마음도 그렇겠지...하고 받아들이는게 그나마 편하죠ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100009000.1.17",
          "form": "맘이 한결 편하네요ㅎㅎ",
          "original_form": "맘이 한결 편하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:33"
        }
