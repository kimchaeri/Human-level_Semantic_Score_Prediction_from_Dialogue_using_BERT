        },
        {
          "id": "MDRW2100016655.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016655.1.3",
          "form": "연애중이신가용?",
          "original_form": "연애중이신가용?",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016655.1.4",
          "form": "아니요ㅋㅋ",
          "original_form": "아니요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016655.1.5",
          "form": "이상형이 어떻게되세요?",
          "original_form": "이상형이 어떻게되세요?",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016655.1.6",
          "form": "혼자서도 잘지내는 사람이요",
          "original_form": "혼자서도 잘지내는 사람이요",
          "speaker_id": "2",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016655.1.7",
          "form": "최고죠ㅋㅋ",
          "original_form": "최고죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016655.1.8",
          "form": "못견뎌하는 사람이 더러 있더라구요",
          "original_form": "못견뎌하는 사람이 더러 있더라구요",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016655.1.9",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016655.1.10",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016655.1.11",
          "form": "저는 선하고 현실적인 사람이요ㅋㅋ",
          "original_form": "저는 선하고 현실적인 사람이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016655.1.12",
          "form": "허황된 꿈보다 주식 한 주 더 쥐고있는게 더 좋아요",
          "original_form": "허황된 꿈보다 주식 한 주 더 쥐고있는게 더 좋아요",
          "speaker_id": "1",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016655.1.13",
          "form": "오 경제관념도 맞아야하죠ㅋㅋ",
          "original_form": "오 경제관념도 맞아야하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016655.1.14",
          "form": "결혼할 생각있으세요?",
          "original_form": "결혼할 생각있으세요?",
          "speaker_id": "2",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016655.1.15",
          "form": "시기가 맞으면 결혼할 거같아요ㅎㅎ",
          "original_form": "시기가 맞으면 결혼할 거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016655.1.16",
          "form": "저는 비혼주의에요",
          "original_form": "저는 비혼주의에요",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016655.1.17",
          "form": "좋죠 혼자서 지내는 게 젤 편하고 젤 재있죠",
          "original_form": "좋죠 혼자서 지내는 게 젤 편하고 젤 재있죠",
          "speaker_id": "1",
          "time": "20210826 14:53"
        }
