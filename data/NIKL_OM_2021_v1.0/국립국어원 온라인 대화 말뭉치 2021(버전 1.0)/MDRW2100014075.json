      "utterance": [
        {
          "id": "MDRW2100014075.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014075.1.2",
          "form": "종교가 있으신가요",
          "original_form": "종교가 있으신가요",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014075.1.3",
          "form": "무교입니다!",
          "original_form": "무교입니다!",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014075.1.4",
          "form": "오 저는 기독교 입니다",
          "original_form": "오 저는 기독교 입니다",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014075.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014075.1.6",
          "form": "이유가 있으신가요?",
          "original_form": "이유가 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014075.1.7",
          "form": "모태신앙이고 종교에 의지하는 부분이 있는 것 같아요",
          "original_form": "모태신앙이고 종교에 의지하는 부분이 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014075.1.8",
          "form": "name2은 종교에 관심은 있으신가용",
          "original_form": "&name2&은 종교에 관심은 있으신가용",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014075.1.9",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014075.1.10",
          "form": "저는 무교인데 사실",
          "original_form": "저는 무교인데 사실",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014075.1.11",
          "form": "큰 관심은없답니다ㅠ진짜솔직히",
          "original_form": "큰 관심은없답니다ㅠ진짜솔직히",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014075.1.12",
          "form": "저도 가끔 의문이 들 때는 많아요 ㅋㅋ",
          "original_form": "저도 가끔 의문이 들 때는 많아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014075.1.13",
          "form": "진짠가 싶어서",
          "original_form": "진짠가 싶어서",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014075.1.14",
          "form": "ㅠㅠ 그래요??",
          "original_form": "ㅠㅠ 그래요??",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014075.1.15",
          "form": "요즘은 코로나때문에",
          "original_form": "요즘은 코로나때문에",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014075.1.16",
          "form": "교회를더잘못가겠어요",
          "original_form": "교회를더잘못가겠어요",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014075.1.17",
          "form": "맞아요 일부 교회들이",
          "original_form": "맞아요 일부 교회들이",
          "speaker_id": "1",
          "time": "20210826 09:02"
        },
        {
          "id": "MDRW2100014075.1.18",
          "form": "방역지침어겨서",
          "original_form": "방역지침어겨서",
          "speaker_id": "1",
          "time": "20210826 09:02"
        },
        {
          "id": "MDRW2100014075.1.19",
          "form": "문제도 많더라구요  ㅠ",
          "original_form": "문제도 많더라구요  ㅠ",
          "speaker_id": "1",
          "time": "20210826 09:03"
        },
        {
          "id": "MDRW2100014075.1.20",
          "form": "ㅠ 참 코로나시대엔",
          "original_form": "ㅠ 참 코로나시대엔",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100014075.1.21",
          "form": "종교도 쉽지않겠어요",
          "original_form": "종교도 쉽지않겠어요",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100014075.1.22",
          "form": "네 ㅠ 여러사람들에게 피해주는 행위가 참된 종교인지는 모르겠네요",
          "original_form": "네 ㅠ 여러사람들에게 피해주는 행위가 참된 종교인지는 모르겠네요",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100014075.1.23",
          "form": "ㅠㅠ 참 어려운부분같네요",
          "original_form": "ㅠㅠ 참 어려운부분같네요",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100014075.1.24",
          "form": "자제해야하는 부분은 맞는것같아요",
          "original_form": "자제해야하는 부분은 맞는것같아요",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100014075.1.25",
          "form": "그쵸 ㅠ",
          "original_form": "그쵸 ㅠ",
          "speaker_id": "1",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100014075.1.26",
          "form": "어릴 때 친구따라 종교행사 가본적 있으신가용",
          "original_form": "어릴 때 친구따라 종교행사 가본적 있으신가용",
