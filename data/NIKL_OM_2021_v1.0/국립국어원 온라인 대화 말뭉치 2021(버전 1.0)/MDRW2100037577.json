      "utterance": [
        {
          "id": "MDRW2100037577.1.1",
          "form": "진미김밥 어디고?",
          "original_form": "진미김밥 어디고?",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100037577.1.2",
          "form": "진미김밥ㅋㅋㅋ저희집에 진미있어서",
          "original_form": "진미김밥ㅋㅋㅋ저희집에 진미있어서",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037577.1.3",
          "form": "그거넣고 만들었쥬ㅋㅋ",
          "original_form": "그거넣고 만들었쥬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037577.1.4",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037577.1.5",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037577.1.6",
          "form": "난  떡볶이 신랑이사왔네",
          "original_form": "난  떡볶이 신랑이사왔네",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037577.1.7",
          "form": "오늘주사맞고",
          "original_form": "오늘주사맞고",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037577.1.8",
          "form": "ㅋㅋㅋㅋ오 좋네유 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ오 좋네유 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037577.1.9",
          "form": "같이먹고있다",
          "original_form": "같이먹고있다",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037577.1.10",
          "form": "오 좋네요 언니 ㅋㅋ",
          "original_form": "오 좋네요 언니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100037577.1.11",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100037577.1.12",
          "form": "이렇게 해결 ㅋㅋ",
          "original_form": "이렇게 해결 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100037577.1.13",
          "form": "ㅋㅋ 저녁은 또뭐하농",
          "original_form": "ㅋㅋ 저녁은 또뭐하농",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100037577.1.14",
          "form": "맨날걱정",
          "original_form": "맨날걱정",
          "speaker_id": "2",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100037577.1.15",
          "form": "그러니깐요 ㅋㅋ 진짜 매일 걱정",
          "original_form": "그러니깐요 ㅋㅋ 진짜 매일 걱정",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100037577.1.16",
          "form": "미역국을 해야되나 싶기도 하고 ㅋㅋ",
          "original_form": "미역국을 해야되나 싶기도 하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100037577.1.17",
          "form": "미역국 나도생각했다",
          "original_form": "미역국 나도생각했다",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100037577.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ만만한게 미역국이니깐요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ만만한게 미역국이니깐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100037577.1.19",
          "form": "소고기 국거리도 샀겠다 히히",
          "original_form": "소고기 국거리도 샀겠다 히히",
          "speaker_id": "1",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100037577.1.20",
          "form": "나도 냉동실에 좀있어서 소고기ㅋ",
          "original_form": "나도 냉동실에 좀있어서 소고기ㅋ",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100037577.1.21",
          "form": "오늘은 그렇게하자",
          "original_form": "오늘은 그렇게하자",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100037577.1.22",
          "form": "얍얍 ㅋ 반찬이 문제죠 뭐 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "얍얍 ㅋ 반찬이 문제죠 뭐 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100037577.1.23",
          "form": "반찬ㅜㅜ 그래그게문제네",
          "original_form": "반찬ㅜㅜ 그래그게문제네",
          "speaker_id": "2",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100037577.1.24",
          "form": "그러니깐요 ㅋㅋ",
          "original_form": "그러니깐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:43"
