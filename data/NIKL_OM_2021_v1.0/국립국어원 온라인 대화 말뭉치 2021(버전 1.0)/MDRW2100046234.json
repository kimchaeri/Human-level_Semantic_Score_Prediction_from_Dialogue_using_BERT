          "speaker_id": "1",
          "time": "20211016 22:21"
        },
        {
          "id": "MDRW2100046234.1.4",
          "form": "반갑네요",
          "original_form": "반갑네요",
          "speaker_id": "2",
          "time": "20211016 22:21"
        },
        {
          "id": "MDRW2100046234.1.5",
          "form": "저도 민초단입니당",
          "original_form": "저도 민초단입니당",
          "speaker_id": "2",
          "time": "20211016 22:21"
        },
        {
          "id": "MDRW2100046234.1.6",
          "form": "네 ㅎㅎㅎ",
          "original_form": "네 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211016 22:21"
        },
        {
          "id": "MDRW2100046234.1.7",
          "form": "그럼 혹시 파인애플 피자는 어떻게 생각하시나요",
          "original_form": "그럼 혹시 파인애플 피자는 어떻게 생각하시나요",
          "speaker_id": "2",
          "time": "20211016 22:21"
        },
        {
          "id": "MDRW2100046234.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 22:21"
        },
        {
          "id": "MDRW2100046234.1.9",
          "form": "없어서 못먹죠",
          "original_form": "없어서 못먹죠",
          "speaker_id": "1",
          "time": "20211016 22:21"
        },
        {
          "id": "MDRW2100046234.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 22:21"
        },
        {
          "id": "MDRW2100046234.1.11",
          "form": "오 ㅋㅋㅋㅋㅋㅋㅋ 사실 전 파인애플 피자까지는..",
          "original_form": "오 ㅋㅋㅋㅋㅋㅋㅋ 사실 전 파인애플 피자까지는..",
          "speaker_id": "2",
          "time": "20211016 22:22"
        },
        {
          "id": "MDRW2100046234.1.12",
          "form": "품지못합니다...ㅎ",
          "original_form": "품지못합니다...ㅎ",
          "speaker_id": "2",
          "time": "20211016 22:22"
        },
        {
          "id": "MDRW2100046234.1.13",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211016 22:22"
        },
        {
          "id": "MDRW2100046234.1.14",
          "form": "파인애플피자에 민초까지 품으시는데",
          "original_form": "파인애플피자에 민초까지 품으시는데",
          "speaker_id": "2",
          "time": "20211016 22:23"
        },
        {
          "id": "MDRW2100046234.1.15",
          "form": "이것만은 못참겠다하시는게 있으실까요 ㅋㅋㅋㅋㅋ",
          "original_form": "이것만은 못참겠다하시는게 있으실까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 22:23"
        },
        {
          "id": "MDRW2100046234.1.16",
          "form": "조던이요 저는 신발 좋아해서요",
          "original_form": "조던이요 저는 신발 좋아해서요",
          "speaker_id": "1",
          "time": "20211016 22:23"
        },
        {
