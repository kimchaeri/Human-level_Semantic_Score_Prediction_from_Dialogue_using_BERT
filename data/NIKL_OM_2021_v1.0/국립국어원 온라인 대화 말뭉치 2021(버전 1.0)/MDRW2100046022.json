      "utterance": [
        {
          "id": "MDRW2100046022.1.1",
          "form": "양배추 장아찌 아닙니다 하사",
          "original_form": "양배추 장아찌 아닙니다 하사",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046022.1.2",
          "form": "저는 양배추 , 기름 이어서 양배추들어간 부침개 아니먼 토스트 생각햄ㅅ는데",
          "original_form": "저는 양배추 , 기름 이어서 양배추들어간 부침개 아니먼 토스트 생각햄ㅅ는데",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046022.1.3",
          "form": "계란두 안드가고 ㅠㅠ!!",
          "original_form": "계란두 안드가고 ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046022.1.4",
          "form": "다른 재료도 물어보세요 히히",
          "original_form": "다른 재료도 물어보세요 히히",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046022.1.5",
          "form": "방울토마초양배추볶음...",
          "original_form": "방울토마초양배추볶음...",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046022.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋz돗자리 신공을 시도하시나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋz돗자리 신공을 시도하시나요",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046022.1.7",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㄱㄱㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㅋㄱㄱㅋㄱㄱㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046022.1.8",
          "form": "아쉽슴다 아닙니다!",
          "original_form": "아쉽슴다 아닙니다!",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046022.1.9",
          "form": "근데 실패인거같아요",
          "original_form": "근데 실패인거같아요",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046022.1.10",
          "form": "ㅋㅋㅠ엳기",
          "original_form": "ㅋㅋㅠ엳기",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046022.1.11",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046022.1.12",
          "form": "돼지고기들어가나요",
          "original_form": "돼지고기들어가나요",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100046022.1.13",
          "form": "아니요! 고기가 없어도 맛에 ㅁ문제가 없어요!",
          "original_form": "아니요! 고기가 없어도 맛에 ㅁ문제가 없어요!",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046022.1.14",
          "form": "돼지고기양배추볶음도 아니었네요..",
          "original_form": "돼지고기양배추볶음도 아니었네요..",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046022.1.15",
          "form": "볶기는 볶아요",
          "original_form": "볶기는 볶아요",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046022.1.16",
          "form": "이 소스가 들어갑니다",
          "original_form": "이 소스가 들어갑니다",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046022.1.17",
          "form": "굴소스",
          "original_form": "굴소스",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.18",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.19",
          "form": "두부굴소스볶음",
          "original_form": "두부굴소스볶음",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.20",
          "form": "두부 안 넣어요ㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "두부 안 넣어요ㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.21",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.22",
          "form": "어디서 튀어나온 두부야",
          "original_form": "어디서 튀어나온 두부야",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.23",
          "form": "ㅋㄱㅋㅋㄱㅋㄱㅋㅋㄱㅋ",
          "original_form": "ㅋㄱㅋㅋㄱㅋㄱㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.24",
          "form": "ㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.25",
          "form": "양배추덮밥!!!!!",
          "original_form": "양배추덮밥!!!!!",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.26",
          "form": "딩동댕!!!!!!!!!",
          "original_form": "딩동댕!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046022.1.27",
          "form": "두부 ㅋㅋㅋㅋㅋㅌㅌㅋ 네이버가 알려줬어요ㅠㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "두부 ㅋㅋㅋㅋㅋㅌㅌㅋ 네이버가 알려줬어요ㅠㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100046022.1.28",
          "form": "양배추 굴소스덮밥이었습니다",
          "original_form": "양배추 굴소스덮밥이었습니다",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100046022.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100046022.1.30",
          "form": "그거말곤 하나도 안들어가능거에여...?",
          "original_form": "그거말곤 하나도 안들어가능거에여...?",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100046022.1.31",
          "form": "네이버짜식이 잘못했네요!",
          "original_form": "네이버짜식이 잘못했네요!",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100046022.1.32",
          "form": "돼지고기 넣어주세요..",
          "original_form": "돼지고기 넣어주세요..",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100046022.1.33",
          "form": "양파도 들어가는디",
          "original_form": "양파도 들어가는디",
