      "utterance": [
        {
          "id": "MDRW2100024972.1.1",
          "form": "요새 유투브 머봐",
          "original_form": "요새 유투브 머봐",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024972.1.2",
          "form": "나 완전 취향타는디",
          "original_form": "나 완전 취향타는디",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024972.1.3",
          "form": "유투브 요새 뭐버",
          "original_form": "유투브 요새 뭐버",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024972.1.4",
          "form": "나는 무조건 동물나오는거",
          "original_form": "나는 무조건 동물나오는거",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024972.1.5",
          "form": "동물이안나오면 안봄",
          "original_form": "동물이안나오면 안봄",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024972.1.6",
          "form": "그뭐여 모티비라고 브랜딩관련된거",
          "original_form": "그뭐여 모티비라고 브랜딩관련된거",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024972.1.7",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024972.1.8",
          "form": "라디오처럼 틀어놓고 딴짓하기 좋은걸로",
          "original_form": "라디오처럼 틀어놓고 딴짓하기 좋은걸로",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024972.1.9",
          "form": "트는편",
          "original_form": "트는편",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024972.1.10",
          "form": "오 어떤브랜딩...",
          "original_form": "오 어떤브랜딩...",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024972.1.11",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024972.1.12",
          "form": "예를 들면 오늘 갔던 프릳츠 창업자의 창업스토리 같은 거....?",
          "original_form": "예를 들면 오늘 갔던 프릳츠 창업자의 창업스토리 같은 거....?",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100024972.1.13",
          "form": "골목 카페나 의류 브랜드 사장님 등등 여러 사람들의 인터뷰 같은 거??",
          "original_form": "골목 카페나 의류 브랜드 사장님 등등 여러 사람들의 인터뷰 같은 거??",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024972.1.14",
          "form": "오 프릳츠도 있어?",
          "original_form": "오 프릳츠도 있어?",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024972.1.15",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024972.1.16",
          "form": "난 요새 그냥 정보의바다가 힘들어서",
          "original_form": "난 요새 그냥 정보의바다가 힘들어서",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024972.1.17",
          "form": "동물나오느게 최고좋음...",
          "original_form": "동물나오느게 최고좋음...",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024972.1.18",
          "form": "고양이 최고야",
          "original_form": "고양이 최고야",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024972.1.19",
          "form": "하하 하같은거 보나",
          "original_form": "하하 하같은거 보나",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024972.1.20",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024972.1.21",
          "form": "하하하랑 매탈남",
          "original_form": "하하하랑 매탈남",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024972.1.22",
          "form": "아니면 퐁키",
          "original_form": "아니면 퐁키",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024972.1.23",
          "form": "요새 뭉땡이네 안올려서",
          "original_form": "요새 뭉땡이네 안올려서",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024972.1.24",
          "form": "최애채널인데 슬픔",
          "original_form": "최애채널인데 슬픔",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024972.1.25",
          "form": "ㅠㅠ 뭉이 땡이 나도 보고싶당",
          "original_form": "ㅠㅠ 뭉이 땡이 나도 보고싶당",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024972.1.26",
          "form": "난 요새 왓챠로 옛날 만화많이봄",
          "original_form": "난 요새 왓챠로 옛날 만화많이봄",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024972.1.27",
          "form": "짱재밋",
          "original_form": "짱재밋",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024972.1.28",
          "form": "ㅋㅋㄱㄱ뭐보는데 요새",
          "original_form": "ㅋㅋㄱㄱ뭐보는데 요새",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024972.1.29",
          "form": "슬레이어즈라고 있는디 추억팔이 쩌러",
          "original_form": "슬레이어즈라고 있는디 추억팔이 쩌러",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024972.1.30",
          "form": "그땐 몰랐는데 아이걸 먼치킨이라고 하는구나했슴",
          "original_form": "그땐 몰랐는데 아이걸 먼치킨이라고 하는구나했슴",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024972.1.31",
          "form": "아 슬레이어즈",
          "original_form": "아 슬레이어즈",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024972.1.32",
          "form": "추억의이름이군",
          "original_form": "추억의이름이군",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024972.1.33",
          "form": "라떼는말이야",
          "original_form": "라떼는말이야",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024972.1.34",
          "form": "마법소녀 리나였어",
          "original_form": "마법소녀 리나였어",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024972.1.35",
          "form": "더빙이 왜 나쁜말이지",
          "original_form": "더빙이 왜 나쁜말이지",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100024972.1.36",
          "form": "하여간에 일본어 적응이 안됨",
          "original_form": "하여간에 일본어 적응이 안됨",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100024972.1.37",
          "form": "덥판이 차라리나",
          "original_form": "덥판이 차라리나",
          "speaker_id": "1",
          "time": "20210831 22:37"
        }
      ]
    }
