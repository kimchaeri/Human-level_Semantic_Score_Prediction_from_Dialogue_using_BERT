      "utterance": [
        {
          "id": "MDRW2100032574.1.1",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100032574.1.2",
          "form": "요새운동하시나요?",
          "original_form": "요새운동하시나요?",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032574.1.3",
          "form": "음 요즘 산책이랑 댄스레슨 받아요",
          "original_form": "음 요즘 산책이랑 댄스레슨 받아요",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032574.1.4",
          "form": "오와 댄스요?",
          "original_form": "오와 댄스요?",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032574.1.5",
          "form": "방송댄스인가요?",
          "original_form": "방송댄스인가요?",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032574.1.6",
          "form": "네네 마자요 ㅋㅋ",
          "original_form": "네네 마자요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100032574.1.7",
          "form": "우와앙",
          "original_form": "우와앙",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032574.1.8",
          "form": "아스트로 배웠어요",
          "original_form": "아스트로 배웠어요",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032574.1.9",
          "form": "요새배우는곡은무엇인가용",
          "original_form": "요새배우는곡은무엇인가용",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032574.1.10",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032574.1.11",
          "form": "애프터 미드나잇 입니다",
          "original_form": "애프터 미드나잇 입니다",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032574.1.12",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032574.1.13",
          "form": "저도한번가봤는데",
          "original_form": "저도한번가봤는데",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032574.1.14",
          "form": "안되겠더라고요ㅎㅎㅎ",
          "original_form": "안되겠더라고요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032574.1.15",
          "form": "못외우는머리와 말듣지않는몸의콜라보..",
          "original_form": "못외우는머리와 말듣지않는몸의콜라보..",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032574.1.16",
          "form": "앜ㅋㅋㅋ귀여우시다",
          "original_form": "앜ㅋㅋㅋ귀여우시다",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032574.1.17",
          "form": "저는 계속 하고있어요",
          "original_form": "저는 계속 하고있어요",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032574.1.18",
          "form": "멋있으세요",
          "original_form": "멋있으세요",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100032574.1.19",
          "form": "저도산책좋아해요",
          "original_form": "저도산책좋아해요",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100032574.1.20",
          "form": "요새날씨도걷기좋은날씨에요",
          "original_form": "요새날씨도걷기좋은날씨에요",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100032574.1.21",
          "form": "그쵸 좋아ㅇㅅ",
          "original_form": "그쵸 좋아ㅇㅅ",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032574.1.22",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032574.1.23",
          "form": "저도오늘걸었는데",
          "original_form": "저도오늘걸었는데",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032574.1.24",
          "form": "길을잘못들어서ㅎㅎㅎ생각보다더걸었지뭐에요",
          "original_form": "길을잘못들어서ㅎㅎㅎ생각보다더걸었지뭐에요",
