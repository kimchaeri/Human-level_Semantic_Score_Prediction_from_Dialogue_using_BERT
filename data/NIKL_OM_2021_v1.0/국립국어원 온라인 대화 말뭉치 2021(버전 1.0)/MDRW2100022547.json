      "utterance": [
        {
          "id": "MDRW2100022547.1.1",
          "form": "네네 전혀 마켓컬리가 싸지는 않은거같아요",
          "original_form": "네네 전혀 마켓컬리가 싸지는 않은거같아요",
          "speaker_id": "2",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100022547.1.2",
          "form": "그쵸 ㅜㅜ 마켓컬리 가격대가 전체적으로 좀 높아요",
          "original_form": "그쵸 ㅜㅜ 마켓컬리 가격대가 전체적으로 좀 높아요",
          "speaker_id": "1",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100022547.1.3",
          "form": "근데 잘 찾아보면 괜찮은 가격도 많더라구요",
          "original_form": "근데 잘 찾아보면 괜찮은 가격도 많더라구요",
          "speaker_id": "1",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100022547.1.4",
          "form": "아까 제가 말한 템포 휴지 그것도 마켓컬리에도 팔걸요?",
          "original_form": "아까 제가 말한 템포 휴지 그것도 마켓컬리에도 팔걸요?",
          "speaker_id": "1",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100022547.1.5",
          "form": "네네 차라리 이마트몰이나 다른 쇼핑몰이 나아요",
          "original_form": "네네 차라리 이마트몰이나 다른 쇼핑몰이 나아요",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.6",
          "form": "요즘은 지마켓이 열일 하더라구요",
          "original_form": "요즘은 지마켓이 열일 하더라구요",
          "speaker_id": "1",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.7",
          "form": "오 그래요? 장바구니담아놔야겟어요",
          "original_form": "오 그래요? 장바구니담아놔야겟어요",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.8",
          "form": "원래 쿠폰 안뿌렸던 거 같은데 최근에 엄청 뿌려요",
          "original_form": "원래 쿠폰 안뿌렸던 거 같은데 최근에 엄청 뿌려요",
          "speaker_id": "1",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.9",
          "form": "저는 11번가 애용해요 ㅋㅋㅋㅋ",
          "original_form": "저는 11번가 애용해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.10",
          "form": "아 저도 11번가 예전에 많이 이용했어요 ㅋㅋ",
          "original_form": "아 저도 11번가 예전에 많이 이용했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.11",
          "form": "오 그렇군여",
          "original_form": "오 그렇군여",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.12",
          "form": "거기가 딜 같은거 많이 올라와서 ㅎㅎ",
          "original_form": "거기가 딜 같은거 많이 올라와서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.13",
          "form": "ㅋㅋㅋㅋ네맞아요 티멤버십 할인되서 좋아요",
          "original_form": "ㅋㅋㅋㅋ네맞아요 티멤버십 할인되서 좋아요",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022547.1.14",
          "form": "근데 11번가 티멤버십 할인이 11일 밖에 안돼죠?",
          "original_form": "근데 11번가 티멤버십 할인이 11일 밖에 안돼죠?",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.15",
          "form": "아 다른날에도 되요! 근데 상품마다달라서 제한적이에요",
          "original_form": "아 다른날에도 되요! 근데 상품마다달라서 제한적이에요",
          "speaker_id": "2",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.16",
          "form": "헐 전혀 몰랐어요 ㅋㅋㅋ",
          "original_form": "헐 전혀 몰랐어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.17",
          "form": "십일절엔 무조건 사여 ㅋㅋㅋㅋ",
          "original_form": "십일절엔 무조건 사여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.18",
          "form": "매번 하려고 할 때마다 티멤버십 안되가지고...",
          "original_form": "매번 하려고 할 때마다 티멤버십 안되가지고...",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.19",
          "form": "그때 기프티콘도 싸요",
          "original_form": "그때 기프티콘도 싸요",
          "speaker_id": "2",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.20",
          "form": "십일절엔 필수죠 그때가 대축제에요 완전",
          "original_form": "십일절엔 필수죠 그때가 대축제에요 완전",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.21",
          "form": "ㅋㅋㅋㅋㅋ맞아요 대축제",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 대축제",
          "speaker_id": "2",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.22",
          "form": "기프티콘 뭐 사면 좋아요?",
          "original_form": "기프티콘 뭐 사면 좋아요?",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022547.1.23",
          "form": "선물용으로 좀 쟁여놓게요",
          "original_form": "선물용으로 좀 쟁여놓게요",
          "speaker_id": "1",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022547.1.24",
          "form": "저는 베라 많이샀어요 ㅋㅋㅋㅋ",
          "original_form": "저는 베라 많이샀어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022547.1.25",
          "form": "쿼터 만원정도해요!",
          "original_form": "쿼터 만원정도해요!",
