      "utterance": [
        {
          "id": "MDRW2100035392.1.1",
          "form": "다이어트!",
          "original_form": "다이어트!",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100035392.1.2",
          "form": "해야하는데 안합니다",
          "original_form": "해야하는데 안합니다",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100035392.1.3",
          "form": "쉽지않죠",
          "original_form": "쉽지않죠",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035392.1.4",
          "form": "저는 물러설 곳이 없어 시작했습니닷",
          "original_form": "저는 물러설 곳이 없어 시작했습니닷",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035392.1.5",
          "form": "게다가 어제 발목을 다쳐서",
          "original_form": "게다가 어제 발목을 다쳐서",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035392.1.6",
          "form": "당분간못할것같아요,,",
          "original_form": "당분간못할것같아요,,",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035392.1.7",
          "form": "운동 어떤거하세요?",
          "original_form": "운동 어떤거하세요?",
          "speaker_id": "2",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100035392.1.8",
          "form": "아.. 충분한 휴식 취하셔야조ㅡ!",
          "original_form": "아.. 충분한 휴식 취하셔야조ㅡ!",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100035392.1.9",
          "form": "걷기랑 스쿼트하고 있어요!",
          "original_form": "걷기랑 스쿼트하고 있어요!",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100035392.1.10",
          "form": "스쿼트!",
          "original_form": "스쿼트!",
          "speaker_id": "2",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100035392.1.11",
          "form": "최고죠",
          "original_form": "최고죠",
          "speaker_id": "2",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100035392.1.12",
          "form": "오오 역시 그런가요?",
          "original_form": "오오 역시 그런가요?",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035392.1.13",
          "form": "이상하게 다이어트만 하면 조급해져요ㅋㅋ",
          "original_form": "이상하게 다이어트만 하면 조급해져요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035392.1.14",
          "form": "김신영스쿼트가 살엄청빠진대요",
          "original_form": "김신영스쿼트가 살엄청빠진대요",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035392.1.15",
          "form": "오..? 그냥 스쿼트랑 다른가보네요",
          "original_form": "오..? 그냥 스쿼트랑 다른가보네요",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100035392.1.16",
          "form": "벽에등대고하는거라던데",
          "original_form": "벽에등대고하는거라던데",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035392.1.17",
          "form": "그만큼 힘들대요",
          "original_form": "그만큼 힘들대요",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035392.1.18",
          "form": "허억.. 찾아봐야겠어요",
          "original_form": "허억.. 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100035392.1.19",
          "form": "등에 대서 버티는 것도 힘들던데..",
          "original_form": "등에 대서 버티는 것도 힘들던데..",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100035392.1.20",
          "form": "정말 말할수록 더하기싫어지는게 운동같아요",
          "original_form": "정말 말할수록 더하기싫어지는게 운동같아요",
          "speaker_id": "2",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100035392.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100035392.1.22",
          "form": "왜그런걸까요ㅋㅋㅋ",
          "original_form": "왜그런걸까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100035392.1.23",
          "form": "해야되는건 맞는데 참,,,ㅋㅋㄱㅋ",
          "original_form": "해야되는건 맞는데 참,,,ㅋㅋㄱㅋ",
