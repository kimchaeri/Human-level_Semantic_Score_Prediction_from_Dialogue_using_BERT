      "utterance": [
        {
          "id": "MDRW2100035480.1.1",
          "form": "놀면뭐하니가 진짜 내용 잘뽑는거 같아요",
          "original_form": "놀면뭐하니가 진짜 내용 잘뽑는거 같아요",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100035480.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100035480.1.3",
          "form": "이번에 그 김현정씨 면접보는거",
          "original_form": "이번에 그 김현정씨 면접보는거",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100035480.1.4",
          "form": "너무웃기더라고옄ㅋㅋㅋㅋㅋ",
          "original_form": "너무웃기더라고옄ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100035480.1.5",
          "form": "아 이번에 못봤는데!!",
          "original_form": "아 이번에 못봤는데!!",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035480.1.6",
          "form": "늘 조합이 좋아요ㅋㅋ",
          "original_form": "늘 조합이 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035480.1.7",
          "form": "케미가 좋은건가?",
          "original_form": "케미가 좋은건가?",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035480.1.8",
          "form": "재밌는 예능인들을 잘 찾아오는거같아료 ㅎㅎ",
          "original_form": "재밌는 예능인들을 잘 찾아오는거같아료 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100035480.1.9",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035480.1.10",
          "form": "비 나왔을때 그리 웃길 줄 몰랐어요",
          "original_form": "비 나왔을때 그리 웃길 줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035480.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100035480.1.12",
          "form": "비 말씀하시이까 그",
          "original_form": "비 말씀하시이까 그",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100035480.1.13",
          "form": "전현무가 진행하던거 생각나요",
          "original_form": "전현무가 진행하던거 생각나요",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100035480.1.14",
          "form": "성대모사하는거",
          "original_form": "성대모사하는거",
          "speaker_id": "2",
          "time": "20210829 18:11"
        },
        {
          "id": "MDRW2100035480.1.15",
          "form": "ㅋㅋㅋㅋㄲㅋ어디서요?",
          "original_form": "ㅋㅋㅋㅋㄲㅋ어디서요?",
          "speaker_id": "1",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100035480.1.16",
          "form": "그 아",
          "original_form": "그 아",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100035480.1.17",
          "form": "노래 부르늗건데ㅠㅠ",
          "original_form": "노래 부르늗건데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100035480.1.18",
          "form": "히든싱어!!",
          "original_form": "히든싱어!!",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100035480.1.19",
          "form": "오오ㅋㅋㅋㅋ 거기 비도 나왔었군요",
          "original_form": "오오ㅋㅋㅋㅋ 거기 비도 나왔었군요",
          "speaker_id": "1",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100035480.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100035480.1.21",
          "form": "객석에서 나오던 장면 잊을슈가없어요 ㅋ",
          "original_form": "객석에서 나오던 장면 잊을슈가없어요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100035480.1.22",
          "form": "멋있었나보네요!?",
          "original_form": "멋있었나보네요!?",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100035480.1.23",
          "form": "그 히든싱어가 실제 가수 찾아내는거잖아요!",
          "original_form": "그 히든싱어가 실제 가수 찾아내는거잖아요!",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100035480.1.24",
          "form": "다들 신나서 찾았는데",
          "original_form": "다들 신나서 찾았는데",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100035480.1.25",
          "form": "알고보니 박스 안에 비가 없었고",
          "original_form": "알고보니 박스 안에 비가 없었고",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100035480.1.26",
          "form": "객석에 숨어있다가 나와서",
          "original_form": "객석에 숨어있다가 나와서",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100035480.1.27",
          "form": "패널들이랑 시청자들이랑 다 멘붕왔었거든요 ㅎㅎ",
          "original_form": "패널들이랑 시청자들이랑 다 멘붕왔었거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100035480.1.28",
          "form": "대박ㅋㅋㅋ 완전 그랬겠네요",
          "original_form": "대박ㅋㅋㅋ 완전 그랬겠네요",
          "speaker_id": "1",
          "time": "20210829 20:23"
        }
      ]
