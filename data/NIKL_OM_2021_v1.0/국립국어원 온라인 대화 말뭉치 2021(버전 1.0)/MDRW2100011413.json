      "utterance": [
        {
          "id": "MDRW2100011413.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100011413.1.2",
          "form": "네 안녕하세요~ 여행좋아하시나요?",
          "original_form": "네 안녕하세요~ 여행좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100011413.1.3",
          "form": "좋아하는 편은 아니예요 님은요?",
          "original_form": "좋아하는 편은 아니예요 님은요?",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100011413.1.4",
          "form": "전 좋아하는데 코로나때문에 못가고있어요",
          "original_form": "전 좋아하는데 코로나때문에 못가고있어요",
          "speaker_id": "2",
          "time": "20210824 23:33"
        },
        {
          "id": "MDRW2100011413.1.5",
          "form": "해외 어디가고싶으세요!",
          "original_form": "해외 어디가고싶으세요!",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100011413.1.6",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100011413.1.7",
          "form": "전 유럽쪽도 가고싶고 미국도 가보고싶어요~~ㅎㅎ",
          "original_form": "전 유럽쪽도 가고싶고 미국도 가보고싶어요~~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100011413.1.8",
          "form": "언제갈수있을지 모르겠네요",
          "original_form": "언제갈수있을지 모르겠네요",
          "speaker_id": "2",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100011413.1.9",
          "form": "저는 멕시코 가고싶어요",
          "original_form": "저는 멕시코 가고싶어요",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100011413.1.10",
          "form": "와 멕시코! 왜가고싶으세요?",
          "original_form": "와 멕시코! 왜가고싶으세요?",
          "speaker_id": "2",
          "time": "20210825 00:56"
        },
        {
          "id": "MDRW2100011413.1.11",
          "form": "타코 먹으러요!",
          "original_form": "타코 먹으러요!",
          "speaker_id": "1",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100011413.1.12",
          "form": "앗 그이유라면 저도 가고싶네요ㅎㅎ타코 넘 좋아해서 오리지널의 맛이 궁금해요",
          "original_form": "앗 그이유라면 저도 가고싶네요ㅎㅎ타코 넘 좋아해서 오리지널의 맛이 궁금해요",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100011413.1.13",
          "form": "유럽이나 미국에 가고싶은 이유가 있을까요?",
          "original_form": "유럽이나 미국에 가고싶은 이유가 있을까요?",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100011413.1.14",
          "form": "일본 중국은 몇번 가봣는데 그쪽은 안가봐서요ㅎㅎ",
          "original_form": "일본 중국은 몇번 가봣는데 그쪽은 안가봐서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100011413.1.15",
          "form": "음식도 궁금하고요",
          "original_form": "음식도 궁금하고요",
          "speaker_id": "2",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100011413.1.16",
          "form": "저도 가보고 싶네요",
          "original_form": "저도 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210825 01:14"
        }
      ]
