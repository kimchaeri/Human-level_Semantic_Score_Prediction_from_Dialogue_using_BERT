      "utterance": [
        {
          "id": "MDRW2100018064.1.1",
          "form": "좋아하시는 맛집 있으세요??",
          "original_form": "좋아하시는 맛집 있으세요??",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018064.1.2",
          "form": "흠 ... 이태원에 버거 파는데",
          "original_form": "흠 ... 이태원에 버거 파는데",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018064.1.3",
          "form": "이름뭐지ㅠㅠㅠ 엄청 유명한덴데",
          "original_form": "이름뭐지ㅠㅠㅠ 엄청 유명한덴데",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018064.1.4",
          "form": "아 다운타우너 아보카도 버거 좋아해용",
          "original_form": "아 다운타우너 아보카도 버거 좋아해용",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018064.1.5",
          "form": "세상에 저 거기 최애햄버거집이에요",
          "original_form": "세상에 저 거기 최애햄버거집이에요",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100018064.1.6",
          "form": "헉헉 ㅋㅋ 통햇당",
          "original_form": "헉헉 ㅋㅋ 통햇당",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018064.1.7",
          "form": "또 어디조아하세용 ㅋㅋ",
          "original_form": "또 어디조아하세용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018064.1.8",
          "form": "음 니뽕내뽕 아세요?? 거기로뽕이 진짜 맛있어요",
          "original_form": "음 니뽕내뽕 아세요?? 거기로뽕이 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018064.1.9",
          "form": "아 ㅋㅋㅋ 다른거 먹었는데 로뽕 안먹어봑어요",
          "original_form": "아 ㅋㅋㅋ 다른거 먹었는데 로뽕 안먹어봑어요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018064.1.10",
          "form": "다른거 뭐드셨었어요??",
          "original_form": "다른거 뭐드셨었어요??",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018064.1.11",
          "form": "그냥 일반 짬뽕 ? ㅋㅋㅋㅋ",
          "original_form": "그냥 일반 짬뽕 ? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018064.1.12",
          "form": "로제짬뽕이에요?",
          "original_form": "로제짬뽕이에요?",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018064.1.13",
          "form": "네 로뽕은 로제에요",
          "original_form": "네 로뽕은 로제에요",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018064.1.14",
          "form": "위에 치즈가득이라 맛있어요",
          "original_form": "위에 치즈가득이라 맛있어요",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018064.1.15",
          "form": "신기하다 ㅋㅋㅋ",
          "original_form": "신기하다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018064.1.16",
          "form": "로제 떡볶이보다 맛있나용 ㅋㅋㅋ",
          "original_form": "로제 떡볶이보다 맛있나용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018064.1.17",
          "form": "헛 생각지 못 한..음 막상막하에요",
          "original_form": "헛 생각지 못 한..음 막상막하에요",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018064.1.18",
          "form": "로뽕이 덜 질리는 거 같아요",
          "original_form": "로뽕이 덜 질리는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018064.1.19",
          "form": "오호...한번 가보겟습니다 ..로뽕!!",
          "original_form": "오호...한번 가보겟습니다 ..로뽕!!",
