      "utterance": [
        {
          "id": "MDRW2100022628.1.1",
          "form": "음 어렸을때 해본 적은 있어요",
          "original_form": "음 어렸을때 해본 적은 있어요",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022628.1.2",
          "form": "근데 운동을 열심히 한 게 아니라",
          "original_form": "근데 운동을 열심히 한 게 아니라",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022628.1.3",
          "form": "밥 줄이고 굶고 그렇게 뺐어서",
          "original_form": "밥 줄이고 굶고 그렇게 뺐어서",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022628.1.4",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022628.1.5",
          "form": "살과 함께 키가 빠진 것 같아요",
          "original_form": "살과 함께 키가 빠진 것 같아요",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022628.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022628.1.7",
          "form": "지금이라면 안 하겠지만",
          "original_form": "지금이라면 안 하겠지만",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022628.1.8",
          "form": "그 때는 티비에 나오는 연예인들이 마른 게",
          "original_form": "그 때는 티비에 나오는 연예인들이 마른 게",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022628.1.9",
          "form": "어찌나 부러웠는지",
          "original_form": "어찌나 부러웠는지",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022628.1.10",
          "form": "맞아요 매체에서 여성의 마름을 당연시하게끔 만들잖아요",
          "original_form": "맞아요 매체에서 여성의 마름을 당연시하게끔 만들잖아요",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022628.1.11",
          "form": "약간 미디어가 심어준 미 따라하려고",
          "original_form": "약간 미디어가 심어준 미 따라하려고",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022628.1.12",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022628.1.13",
          "form": "굶고 다닐 때는 앉아있다가 빨리 일어나면",
          "original_form": "굶고 다닐 때는 앉아있다가 빨리 일어나면",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022628.1.14",
          "form": "어지럽고 그랬는데",
          "original_form": "어지럽고 그랬는데",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022628.1.15",
          "form": "어지러운느낌 ㅠ",
          "original_form": "어지러운느낌 ㅠ",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022628.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022628.1.17",
          "form": "지금은 건강을 최우선으로 두고",
          "original_form": "지금은 건강을 최우선으로 두고",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022628.1.18",
          "form": "열심히 건강하게 먹고 있어요",
          "original_form": "열심히 건강하게 먹고 있어요",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022628.1.19",
          "form": "저는 학생때까지 마른편이었거든요",
          "original_form": "저는 학생때까지 마른편이었거든요",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022628.1.20",
          "form": "뷔페에서 8~9접시를 먹는데도 50키로를 안넘었어요",
          "original_form": "뷔페에서 8~9접시를 먹는데도 50키로를 안넘었어요",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022628.1.21",
          "form": "근데 ㅋㅋㅋ ㅋ직장인이 되면서 학생때보다 군것질이 늘고 스트레스도 많이 받고",
          "original_form": "근데 ㅋㅋㅋ ㅋ직장인이 되면서 학생때보다 군것질이 늘고 스트레스도 많이 받고",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022628.1.22",
          "form": "학생때는 체육시간이라도 있어서 했던 운동조차도 이제 안해버리니까",
          "original_form": "학생때는 체육시간이라도 있어서 했던 운동조차도 이제 안해버리니까",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022628.1.23",
          "form": "살은 계속 찌고 체력은 바닥도 없이 떨어지더라고요",
          "original_form": "살은 계속 찌고 체력은 바닥도 없이 떨어지더라고요",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022628.1.24",
          "form": "맞아요 체력이 훅훅 깎이는 게 느껴지더라구요",
          "original_form": "맞아요 체력이 훅훅 깎이는 게 느껴지더라구요",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022628.1.25",
          "form": "이젠 살기 위해 운동을 해야 할 수준이예요ㅋㅋㅋ",
          "original_form": "이젠 살기 위해 운동을 해야 할 수준이예요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022628.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022628.1.27",
          "form": "그리고 아침 안먹는것도 큰거같아요",
          "original_form": "그리고 아침 안먹는것도 큰거같아요",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022628.1.28",
          "form": "헉 찔린다",
          "original_form": "헉 찔린다",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022628.1.29",
          "form": "학생땐 부모님이 억지로 끌어앉혀서 아침을 먹였는데",
          "original_form": "학생땐 부모님이 억지로 끌어앉혀서 아침을 먹였는데",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022628.1.30",
          "form": "이젠 아침을 안먹고.. 저녁은 왕창먹고.. ㅋㅋㅋ",
          "original_form": "이젠 아침을 안먹고.. 저녁은 왕창먹고.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022628.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜 공감 백퍼예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜 공감 백퍼예요",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022628.1.32",
          "form": "학생 신분 벗어난 이후로",
          "original_form": "학생 신분 벗어난 이후로",
          "speaker_id": "2",
