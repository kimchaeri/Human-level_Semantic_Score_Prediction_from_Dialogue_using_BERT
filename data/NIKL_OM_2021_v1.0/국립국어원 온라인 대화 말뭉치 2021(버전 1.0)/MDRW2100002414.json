      "utterance": [
        {
          "id": "MDRW2100002414.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002414.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100002414.1.3",
          "form": "좋아하시는 프로그램있으신가요?",
          "original_form": "좋아하시는 프로그램있으신가요?",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100002414.1.4",
          "form": "예능프로 좋아해서요",
          "original_form": "예능프로 좋아해서요",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002414.1.5",
          "form": "이거저거 보는편이에요",
          "original_form": "이거저거 보는편이에요",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002414.1.6",
          "form": "요즘은 놀토를 열심히 보네요",
          "original_form": "요즘은 놀토를 열심히 보네요",
          "speaker_id": "2",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002414.1.7",
          "form": "놀토너무재밌죠ㅎㅎ! 저는 해방타운이라는프로를보는데",
          "original_form": "놀토너무재밌죠ㅎㅎ! 저는 해방타운이라는프로를보는데",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002414.1.8",
          "form": "혹시아시나요?",
          "original_form": "혹시아시나요?",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002414.1.9",
          "form": "제목만 들엉봤어용",
          "original_form": "제목만 들엉봤어용",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002414.1.10",
          "form": "어떤 내용인가요",
          "original_form": "어떤 내용인가요",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002414.1.11",
          "form": "아 장윤정 등등이 나와서 집밖에서 하루자는 프로그램입니다ㅎㅎ",
          "original_form": "아 장윤정 등등이 나와서 집밖에서 하루자는 프로그램입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002414.1.12",
          "form": "요즘은 백지영도나와요!",
          "original_form": "요즘은 백지영도나와요!",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002414.1.13",
          "form": "한번 봐봐야 겠네요",
          "original_form": "한번 봐봐야 겠네요",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002414.1.14",
          "form": "근데 아무생각없이 볼수있는건 역시 맛녀석이에요",
          "original_form": "근데 아무생각없이 볼수있는건 역시 맛녀석이에요",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002414.1.15",
          "form": "아 맛녀석좋죠ㅋㅋㅋ",
          "original_form": "아 맛녀석좋죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002414.1.16",
          "form": "저그거애청자입니다",
          "original_form": "저그거애청자입니다",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002414.1.17",
          "form": "본거 또봐도 재미있어서 리모컨 돌려가면서 그것만 볼때도 잇어여",
          "original_form": "본거 또봐도 재미있어서 리모컨 돌려가면서 그것만 볼때도 잇어여",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002414.1.18",
          "form": "이제 김준현 빠진다고 해서 너무 아쉬워요",
          "original_form": "이제 김준현 빠진다고 해서 너무 아쉬워요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002414.1.19",
          "form": "헐 맛녀석",
          "original_form": "헐 맛녀석",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002414.1.20",
          "form": "맛표현 제일 잘했었는데",
          "original_form": "맛표현 제일 잘했었는데",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002414.1.21",
          "form": "김준현이 빠진대요???",
          "original_form": "김준현이 빠진대요???",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002414.1.22",
          "form": "이번주가 마지막이에요",
          "original_form": "이번주가 마지막이에요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002414.1.23",
          "form": "이제 셋이 한대요",
          "original_form": "이제 셋이 한대요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002414.1.24",
          "form": "헐 대박이네요 건강이 많이안좋아보이긴하던데",
          "original_form": "헐 대박이네요 건강이 많이안좋아보이긴하던데",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002414.1.25",
          "form": "배가 남산같더라구요ㅠㅠ",
          "original_form": "배가 남산같더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002414.1.26",
          "form": "저도 건강때문인가햇는데",
          "original_form": "저도 건강때문인가햇는데",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002414.1.27",
          "form": "그건 아니래요",
          "original_form": "그건 아니래요",
          "speaker_id": "2",
          "time": "20210820 12:43"
        }
      ]
    }
  ]
