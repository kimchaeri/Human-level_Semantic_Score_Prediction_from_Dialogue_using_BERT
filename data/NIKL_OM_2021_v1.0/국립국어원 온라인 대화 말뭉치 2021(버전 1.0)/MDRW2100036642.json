      "utterance": [
        {
          "id": "MDRW2100036642.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 03:41"
        },
        {
          "id": "MDRW2100036642.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 03:43"
        },
        {
          "id": "MDRW2100036642.1.3",
          "form": "거기날씨어떤가요",
          "original_form": "거기날씨어떤가요",
          "speaker_id": "1",
          "time": "20210831 03:46"
        },
        {
          "id": "MDRW2100036642.1.4",
          "form": "날씨 맑았었어요~",
          "original_form": "날씨 맑았었어요~",
          "speaker_id": "2",
          "time": "20210831 03:49"
        },
        {
          "id": "MDRW2100036642.1.5",
          "form": "오락가락 가을옷언제꺼내야되나 고민중이에요",
          "original_form": "오락가락 가을옷언제꺼내야되나 고민중이에요",
          "speaker_id": "1",
          "time": "20210831 03:50"
        },
        {
          "id": "MDRW2100036642.1.6",
          "form": "가을옷꺼내셨어요?",
          "original_form": "가을옷꺼내셨어요?",
          "speaker_id": "1",
          "time": "20210831 03:50"
        },
        {
          "id": "MDRW2100036642.1.7",
          "form": "네~  꺼냈어요",
          "original_form": "네~  꺼냈어요",
          "speaker_id": "2",
          "time": "20210831 03:50"
        },
        {
          "id": "MDRW2100036642.1.8",
          "form": "오 빠르게 정리하셨네요",
          "original_form": "오 빠르게 정리하셨네요",
          "speaker_id": "1",
          "time": "20210831 03:51"
        },
        {
          "id": "MDRW2100036642.1.9",
          "form": "전 지금 고민만하다  겨울옷꺼내게될판이에여ㅋㅋㅋ",
          "original_form": "전 지금 고민만하다  겨울옷꺼내게될판이에여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 03:52"
        },
        {
          "id": "MDRW2100036642.1.10",
          "form": "계절중에 어떤계절좋아하세요",
          "original_form": "계절중에 어떤계절좋아하세요",
          "speaker_id": "1",
          "time": "20210831 03:52"
        },
        {
          "id": "MDRW2100036642.1.11",
          "form": "아직 여름 옷을 집어넣지 않아 여름&가을 옷 함께 있는 상태에요",
          "original_form": "아직 여름 옷을 집어넣지 않아 여름&가을 옷 함께 있는 상태에요",
          "speaker_id": "2",
          "time": "20210831 03:53"
        },
        {
          "id": "MDRW2100036642.1.12",
          "form": "전 가을을 좋아해요",
          "original_form": "전 가을을 좋아해요",
          "speaker_id": "2",
          "time": "20210831 03:53"
        },
        {
          "id": "MDRW2100036642.1.13",
          "form": "그쳐 아직은더우니까요",
          "original_form": "그쳐 아직은더우니까요",
          "speaker_id": "1",
          "time": "20210831 03:53"
        },
        {
          "id": "MDRW2100036642.1.14",
          "form": "저도 가을을 젤좋아해요",
          "original_form": "저도 가을을 젤좋아해요",
          "speaker_id": "1",
          "time": "20210831 03:54"
        },
        {
          "id": "MDRW2100036642.1.15",
          "form": "어떤이유에서 좋아하세요",
          "original_form": "어떤이유에서 좋아하세요",
          "speaker_id": "1",
          "time": "20210831 03:54"
        },
        {
          "id": "MDRW2100036642.1.16",
          "form": "덥지도 않고 춥지도 않고 생활하기 좋은 계절이라서요\n그리고 선선한 느낌 시원한 바람이 좋아요",
          "original_form": "덥지도 않고 춥지도 않고 생활하기 좋은 계절이라서요\n그리고 선선한 느낌 시원한 바람이 좋아요",
          "speaker_id": "2",
          "time": "20210831 03:55"
        },
        {
          "id": "MDRW2100036642.1.17",
          "form": "그쵸 저도 딱 선선하고 적당한온도랑 습하지않아서 좋아요",
          "original_form": "그쵸 저도 딱 선선하고 적당한온도랑 습하지않아서 좋아요",
          "speaker_id": "1",
          "time": "20210831 04:02"
        },
        {
          "id": "MDRW2100036642.1.18",
          "form": "하늘도 넘이쁘구요",
          "original_form": "하늘도 넘이쁘구요",
          "speaker_id": "1",
          "time": "20210831 04:02"
        },
        {
          "id": "MDRW2100036642.1.19",
          "form": "맞아요 하늘도 높고 푸르고 예쁘죠",
          "original_form": "맞아요 하늘도 높고 푸르고 예쁘죠",
          "speaker_id": "2",
          "time": "20210831 04:04"
        },
        {
          "id": "MDRW2100036642.1.20",
          "form": "가을에 즐겨입는 옷은어떤거에요",
          "original_form": "가을에 즐겨입는 옷은어떤거에요",
          "speaker_id": "1",
          "time": "20210831 04:11"
        },
        {
          "id": "MDRW2100036642.1.21",
          "form": "음 얇은 옷이지만 긴팔요",
          "original_form": "음 얇은 옷이지만 긴팔요",
          "speaker_id": "2",
          "time": "20210831 04:13"
        },
        {
          "id": "MDRW2100036642.1.22",
          "form": "가디건이나 트렌치 코트 잘 걸치구요",
          "original_form": "가디건이나 트렌치 코트 잘 걸치구요",
          "speaker_id": "2",
          "time": "20210831 04:13"
        }
      ]
    }
