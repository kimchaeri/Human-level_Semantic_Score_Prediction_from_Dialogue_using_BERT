      "utterance": [
        {
          "id": "MDRW2100001720.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001720.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001720.1.3",
          "form": "요즘 날씨 선선해졌죠?",
          "original_form": "요즘 날씨 선선해졌죠?",
          "speaker_id": "1",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001720.1.4",
          "form": "네 많이 시원해졌어요.\n방금  비가 잠깐 내려서 후덥지근해졌는데\n창문여니 괜찮아졌어요",
          "original_form": "네 많이 시원해졌어요.\n방금  비가 잠깐 내려서 후덥지근해졌는데\n창문여니 괜찮아졌어요",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001720.1.5",
          "form": "거긴 비왔나요? 여긴 날씨 좋아요",
          "original_form": "거긴 비왔나요? 여긴 날씨 좋아요",
          "speaker_id": "1",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001720.1.6",
          "form": "잠깐 지나가는 소나기였는지 금방 그치더라구요",
          "original_form": "잠깐 지나가는 소나기였는지 금방 그치더라구요",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001720.1.7",
          "form": "어젠 아침저녁 엄청 시원했는데 오늘은 살짝덥네요",
          "original_form": "어젠 아침저녁 엄청 시원했는데 오늘은 살짝덥네요",
          "speaker_id": "1",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001720.1.8",
          "form": "날씨때문에 \n환절기  감기에 걸릴까봐서요",
          "original_form": "날씨때문에 \n환절기  감기에 걸릴까봐서요",
          "speaker_id": "2",
          "time": "20210819 18:29"
        },
        {
          "id": "MDRW2100001720.1.9",
          "form": "맞아요 갑자기 추워지면 감기걸리지쉽죠",
          "original_form": "맞아요 갑자기 추워지면 감기걸리지쉽죠",
          "speaker_id": "1",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001720.1.10",
          "form": "저희 아이 며칠전에 더워서 에어컨 밤새 틀더니  고열나서 며칠고생했어요",
          "original_form": "저희 아이 며칠전에 더워서 에어컨 밤새 틀더니  고열나서 며칠고생했어요",
          "speaker_id": "2",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001720.1.11",
          "form": "에고 벌써 감기걸렸군요.",
          "original_form": "에고 벌써 감기걸렸군요.",
          "speaker_id": "1",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001720.1.12",
          "form": "열이나서 5일간 고생했어요  코로나검사는 음성판성이구요",
          "original_form": "열이나서 5일간 고생했어요  코로나검사는 음성판성이구요",
          "speaker_id": "2",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001720.1.13",
          "form": "다행이네요 요즘 고열나도 무서워요 코로나땜에",
          "original_form": "다행이네요 요즘 고열나도 무서워요 코로나땜에",
          "speaker_id": "1",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001720.1.14",
          "form": "좀지남 더추워질테니 언능 약먹고 낫길 바랄거요",
          "original_form": "좀지남 더추워질테니 언능 약먹고 낫길 바랄거요",
          "speaker_id": "1",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001720.1.15",
          "form": "담주까진 비예보도 있더라구요",
          "original_form": "담주까진 비예보도 있더라구요",
          "speaker_id": "1",
          "time": "20210819 18:40"
        },
        {
          "id": "MDRW2100001720.1.16",
          "form": "그쳐 담주까지  비소식 제가 사는곳은 경남이라",
          "original_form": "그쳐 담주까지  비소식 제가 사는곳은 경남이라",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001720.1.17",
          "form": "제주도랑 가까워서 비가 오락가락해요",
          "original_form": "제주도랑 가까워서 비가 오락가락해요",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001720.1.18",
          "form": "여기도 주말에 비온다고 하더라구요",
          "original_form": "여기도 주말에 비온다고 하더라구요",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100001720.1.19",
          "form": "감기 더심해지지않게 조심하셔요",
          "original_form": "감기 더심해지지않게 조심하셔요",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001720.1.20",
          "form": "다행히 열감기였는지 이제 열이 다 내려서 정상체온이예요\n걱정 감사합니다",
          "original_form": "다행히 열감기였는지 이제 열이 다 내려서 정상체온이예요\n걱정 감사합니다",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001720.1.21",
          "form": "저녁먹는다고 이제봤네요. 맛난저녁드세요",
          "original_form": "저녁먹는다고 이제봤네요. 맛난저녁드세요",
          "speaker_id": "1",
          "time": "20210819 19:21"
        }
      ]
    }
