      "utterance": [
        {
          "id": "MDRW2100016366.1.1",
          "form": "게임좋아하시나요?",
          "original_form": "게임좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016366.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016366.1.3",
          "form": "어떤게임 좋아하시나요",
          "original_form": "어떤게임 좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016366.1.4",
          "form": "피파온라인이요!",
          "original_form": "피파온라인이요!",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016366.1.5",
          "form": "피파! 현질 많은게임으로 알고있는데 어때요?",
          "original_form": "피파! 현질 많은게임으로 알고있는데 어때요?",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016366.1.6",
          "form": "현질은 안하고있습니닿ㅎ",
          "original_form": "현질은 안하고있습니닿ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016366.1.7",
          "form": "현질안해도 할만한가요?",
          "original_form": "현질안해도 할만한가요?",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016366.1.8",
          "form": "넵 할만합니다",
          "original_form": "넵 할만합니다",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016366.1.9",
          "form": "어떤점이 재밌나요?",
          "original_form": "어떤점이 재밌나요?",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100016366.1.10",
          "form": "축구 좋아하면 다 좋아하는것같아요",
          "original_form": "축구 좋아하면 다 좋아하는것같아요",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016366.1.11",
          "form": "직접 선수 컨트롤하는 게임인가요?",
          "original_form": "직접 선수 컨트롤하는 게임인가요?",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016366.1.12",
          "form": "네 맞습니다ㅎㅎ",
          "original_form": "네 맞습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016366.1.13",
          "form": "컨트롤 중요하겠네요",
          "original_form": "컨트롤 중요하겠네요",
