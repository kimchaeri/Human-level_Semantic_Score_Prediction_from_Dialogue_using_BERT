        {
          "id": "MDRW2100015341.1.1",
          "form": "name1님은 어떤 거 키워보고 싶으세요?",
          "original_form": "&name1&님은 어떤 거 키워보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015341.1.2",
          "form": "강아지 털은 많이 날리나요?",
          "original_form": "강아지 털은 많이 날리나요?",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015341.1.3",
          "form": "털 날리는 시기가 있는 것 같아요",
          "original_form": "털 날리는 시기가 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015341.1.4",
          "form": "저는 고양이 강아지 다 좋아하는데",
          "original_form": "저는 고양이 강아지 다 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015341.1.5",
          "form": "사알짝 알러지가 있어서 ㅋㅋㅠㅠ",
          "original_form": "사알짝 알러지가 있어서 ㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015341.1.6",
          "form": "털 알러지요?",
          "original_form": "털 알러지요?",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015341.1.7",
          "form": "네 먼지알러지도 있고 ㅋㅋ",
          "original_form": "네 먼지알러지도 있고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015341.1.8",
          "form": "그럼 포메라이언 같은 건 포기하셔야겠네요 ㅜㅜ",
          "original_form": "그럼 포메라이언 같은 건 포기하셔야겠네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015341.1.9",
          "form": "막 친구강아지들 만지면 얼굴에 뭐가 나요 ㅋㅋㅋㅋㅋ",
          "original_form": "막 친구강아지들 만지면 얼굴에 뭐가 나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015341.1.10",
          "form": "그래도 키우고싶세요",
          "original_form": "그래도 키우고싶세요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015341.1.11",
          "form": "공원에 잠깐 구경하는 걸로 만끽하셔야겠는데요?ㅠ",
          "original_form": "공원에 잠깐 구경하는 걸로 만끽하셔야겠는데요?ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015341.1.12",
          "form": "푸들류는 털 거의 안빠진다고 하더라고요",
          "original_form": "푸들류는 털 거의 안빠진다고 하더라고요",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015341.1.13",
          "form": "아 푸들~ 좋죠 갈색푸들 진짜 귀엽지 않아요?",
          "original_form": "아 푸들~ 좋죠 갈색푸들 진짜 귀엽지 않아요?",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015341.1.14",
          "form": "그래도 강아지 키우니 행복하지않으세요? ㅋㅋㅋㅋ좀 으르렁해도",
          "original_form": "그래도 강아지 키우니 행복하지않으세요? ㅋㅋㅋㅋ좀 으르렁해도",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015341.1.15",
          "form": "이미 저에겐 남의집 개에요..",
          "original_form": "이미 저에겐 남의집 개에요..",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015341.1.16",
          "form": "맞아요 !! 갈색푸들 키우고싶어요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 !! 갈색푸들 키우고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015341.1.17",
          "form": "자주 못봐서 그런거 아닐까요??",
          "original_form": "자주 못봐서 그런거 아닐까요??",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015341.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015341.1.19",
          "form": "맞아요 그래도 이상하게 가끔 보고싶고 생각나요",
          "original_form": "맞아요 그래도 이상하게 가끔 보고싶고 생각나요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015341.1.20",
          "form": "애증의 관계..",
          "original_form": "애증의 관계..",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015341.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그렇죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그렇죠",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015341.1.22",
          "form": "강아지 나이가 어떻게 돼요??",
          "original_form": "강아지 나이가 어떻게 돼요??",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015341.1.23",
          "form": "거의 10살이요!",
          "original_form": "거의 10살이요!",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015341.1.24",
          "form": "지금 살쪄서 여름만 되면 헥헥거려요",
          "original_form": "지금 살쪄서 여름만 되면 헥헥거려요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015341.1.25",
          "form": "와!!! ㅋㅋㅋ진짜 귀여울듯",
          "original_form": "와!!! ㅋㅋㅋ진짜 귀여울듯",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015341.1.26",
          "form": "사람나이로 중년인가요",
          "original_form": "사람나이로 중년인가요",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015341.1.27",
          "form": "무슨색이에요??",
          "original_form": "무슨색이에요??",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015341.1.28",
          "form": "네 ㅎㅎ 하얀색인데 약간 아이보리 같기도 해요",
          "original_form": "네 ㅎㅎ 하얀색인데 약간 아이보리 같기도 해요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015341.1.29",
          "form": "와 완전 솜사탕같을거같아요",
          "original_form": "와 완전 솜사탕같을거같아요",
