      "utterance": [
        {
          "id": "MDRW2100020081.1.1",
          "form": "밀떡!",
          "original_form": "밀떡!",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020081.1.2",
          "form": "전 어묵좋아합니다",
          "original_form": "전 어묵좋아합니다",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020081.1.3",
          "form": "그리고 떡볶이는 밀떡이라고 생각합니다",
          "original_form": "그리고 떡볶이는 밀떡이라고 생각합니다",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020081.1.4",
          "form": "떡 좋아하시나요",
          "original_form": "떡 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020081.1.5",
          "form": "아뇨 저도 어묵이요",
          "original_form": "아뇨 저도 어묵이요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020081.1.6",
          "form": "저는 볶이파라",
          "original_form": "저는 볶이파라",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020081.1.7",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020081.1.8",
          "form": "떡 잘 안먹어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "떡 잘 안먹어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020081.1.9",
          "form": "맛잘알의 세계란",
          "original_form": "맛잘알의 세계란",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020081.1.10",
          "form": "완벽하네요",
          "original_form": "완벽하네요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020081.1.11",
          "form": "약간 떡말고 어묵 좀 많이 줬음 좋겠는데",
          "original_form": "약간 떡말고 어묵 좀 많이 줬음 좋겠는데",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020081.1.12",
          "form": "맨날 떡만 한바가지와서 슬퍼요",
          "original_form": "맨날 떡만 한바가지와서 슬퍼요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020081.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020081.1.14",
          "form": "떡볶이는 어떤 떡볶이 좋아하세요",
          "original_form": "떡볶이는 어떤 떡볶이 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020081.1.15",
          "form": "떡만 남는다구요ㅠ",
          "original_form": "떡만 남는다구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020081.1.16",
          "form": "떡볶이는 가리지 않습니다 ㅋㅋㅋㅋ-ㅋㅋㅋㅋ",
          "original_form": "떡볶이는 가리지 않습니다 ㅋㅋㅋㅋ-ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020081.1.17",
          "form": "그니까요 떡 그거 무서운게 다 먹었다고 생각했는데 저 아래 또있어",
          "original_form": "그니까요 떡 그거 무서운게 다 먹었다고 생각했는데 저 아래 또있어",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020081.1.18",
          "form": "끝나지않는 떡 지옥",
          "original_form": "끝나지않는 떡 지옥",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020081.1.19",
          "form": "가리시지 않으시구나",
          "original_form": "가리시지 않으시구나",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020081.1.20",
          "form": "그럼 고추장 vs 로제",
          "original_form": "그럼 고추장 vs 로제",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020081.1.21",
          "form": "맞아요 가라앉아잇는거 소름",
          "original_form": "맞아요 가라앉아잇는거 소름",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020081.1.22",
          "form": "고추장",
          "original_form": "고추장",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020081.1.23",
          "form": "아무래도 오리지널이 맛있죠",
          "original_form": "아무래도 오리지널이 맛있죠",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020081.1.24",
          "form": "맞아요 로제듀 맛잇긴 한데",
          "original_form": "맞아요 로제듀 맛잇긴 한데",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020081.1.25",
          "form": "그럼 계란 쪼갠다 통으로 먹는다",
          "original_form": "그럼 계란 쪼갠다 통으로 먹는다",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020081.1.26",
          "form": "돌고돌아 클래식",
          "original_form": "돌고돌아 클래식",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020081.1.27",
          "form": "쪼갠다",
          "original_form": "쪼갠다",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020081.1.28",
          "form": "약간 쪼개서 먹는게 맛있죠",
          "original_form": "약간 쪼개서 먹는게 맛있죠",
          "speaker_id": "1",
