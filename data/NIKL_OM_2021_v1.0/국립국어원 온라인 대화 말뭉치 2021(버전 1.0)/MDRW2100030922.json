      "utterance": [
        {
          "id": "MDRW2100030922.1.1",
          "form": "연애중이싱가요",
          "original_form": "연애중이싱가요",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030922.1.2",
          "form": "네넹. 거의 만 5년되었어요",
          "original_form": "네넹. 거의 만 5년되었어요",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100030922.1.3",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100030922.1.4",
          "form": "우와 없어요",
          "original_form": "우와 없어요",
          "speaker_id": "1",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100030922.1.5",
          "form": "장기연애비결은 ?",
          "original_form": "장기연애비결은 ?",
          "speaker_id": "1",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100030922.1.6",
          "form": "음.........",
          "original_form": "음.........",
          "speaker_id": "2",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030922.1.7",
          "form": "미워하지 않기",
          "original_form": "미워하지 않기",
          "speaker_id": "2",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030922.1.8",
          "form": "이해해주기",
          "original_form": "이해해주기",
          "speaker_id": "2",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030922.1.9",
          "form": "항상 사랑한다구 해주기",
          "original_form": "항상 사랑한다구 해주기",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100030922.1.10",
          "form": "성격이 조으시네요",
          "original_form": "성격이 조으시네요",
          "speaker_id": "1",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100030922.1.11",
          "form": "그럴려구 노력하구 있어요 ㅋ ㅋㅋㅋㅋ",
          "original_form": "그럴려구 노력하구 있어요 ㅋ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100030922.1.12",
          "form": "mbti 아시나요",
          "original_form": "mbti 아시나요",
          "speaker_id": "1",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100030922.1.13",
          "form": "뭔지 알기는 알아요! 근데 제꺼가 뭔지 까먹었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "뭔지 알기는 알아요! 근데 제꺼가 뭔지 까먹었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100030922.1.14",
          "form": "아궁금하네요",
          "original_form": "아궁금하네요",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100030922.1.15",
          "form": "E 어쩌고 였던것 같아요",
          "original_form": "E 어쩌고 였던것 같아요",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100030922.1.16",
          "form": "ENFP???",
          "original_form": "ENFP???",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100030922.1.17",
          "form": "ㅋㅋㅋ 어울령요",
          "original_form": "ㅋㅋㅋ 어울령요",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030922.1.18",
          "form": "name1님은 mbti 잘 아셔욤?",
          "original_form": "&name1&님은 mbti 잘 아셔욤?",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030922.1.19",
          "form": "재밋어해요",
          "original_form": "재밋어해요",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030922.1.20",
          "form": "어느정도 맞는 편인가요? ㅋ ㅋㅋㅋ 다른 주변분들을 봤을때...",
          "original_form": "어느정도 맞는 편인가요? ㅋ ㅋㅋㅋ 다른 주변분들을 봤을때...",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030922.1.21",
          "form": "네 걍 자기가 고른거니까요 ㅋㅋㅋ 재믹어요그냥",
          "original_form": "네 걍 자기가 고른거니까요 ㅋㅋㅋ 재믹어요그냥",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100030922.1.22",
          "form": "여자친구도 성격 조으신가요",
          "original_form": "여자친구도 성격 조으신가요",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100030922.1.23",
          "form": "넹! 성격 좋구 똑똑하구, 사랑스러워요 ㅋㅋㅋ",
          "original_form": "넹! 성격 좋구 똑똑하구, 사랑스러워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100030922.1.24",
          "form": "뭔가 연애할때...",
          "original_form": "뭔가 연애할때...",
          "speaker_id": "2",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100030922.1.25",
          "form": "우와... 조켓다 결혼언제요",
          "original_form": "우와... 조켓다 결혼언제요",
          "speaker_id": "1",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100030922.1.26",
          "form": "ㅋ ㅋㅋㅋㅋ 서로 상호 협의를 해야겠지요",
          "original_form": "ㅋ ㅋㅋㅋㅋ 서로 상호 협의를 해야겠지요",
          "speaker_id": "2",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100030922.1.27",
          "form": "아직 자세하게 얘기는 해본적은 없어요~",
          "original_form": "아직 자세하게 얘기는 해본적은 없어요~",
          "speaker_id": "2",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100030922.1.28",
          "form": "서로 대화가 잘 통한다는게 연애할때 젤 중요한것 같아욤",
          "original_form": "서로 대화가 잘 통한다는게 연애할때 젤 중요한것 같아욤",
          "speaker_id": "2",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030922.1.29",
          "form": "연애 많이 해보셨나요!",
          "original_form": "연애 많이 해보셨나요!",
          "speaker_id": "2",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030922.1.30",
          "form": "소개팅햇어요?",
          "original_form": "소개팅햇어요?",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030922.1.31",
          "form": "아뇨 ㅎㅎㅎ",
          "original_form": "아뇨 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030922.1.32",
          "form": "친구 소개로 만났어요 ㅋㅋㅋ!",
          "original_form": "친구 소개로 만났어요 ㅋㅋㅋ!",
          "speaker_id": "2",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030922.1.33",
          "form": "정작 그 친구랑은 지금 연이 끊긴 ㅠ",
          "original_form": "정작 그 친구랑은 지금 연이 끊긴 ㅠ",
          "speaker_id": "2",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030922.1.34",
          "form": "앗 왜요",
          "original_form": "앗 왜요",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100030922.1.35",
          "form": "그냥 연락이 뜸하다보니까 자연스럽게 끊긴것 같아요!",
          "original_form": "그냥 연락이 뜸하다보니까 자연스럽게 끊긴것 같아요!",
          "speaker_id": "2",
          "time": "20210904 14:40"
        },
        {
          "id": "MDRW2100030922.1.36",
          "form": "아항 ~ 조은인연남기고 떠낫군요 ㅋㅋ",
          "original_form": "아항 ~ 조은인연남기고 떠낫군요 ㅋㅋ",
