      "utterance": [
        {
          "id": "MDRW2100015915.1.1",
          "form": "요즘 영화 자주 보세요?",
          "original_form": "요즘 영화 자주 보세요?",
          "speaker_id": "2",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015915.1.2",
          "form": "저는 최근에는 집에서 영화 봐오.",
          "original_form": "저는 최근에는 집에서 영화 봐오.",
          "speaker_id": "1",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015915.1.3",
          "form": "저도 코로나때문에 영화관은 피하게 되더라구요",
          "original_form": "저도 코로나때문에 영화관은 피하게 되더라구요",
          "speaker_id": "2",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015915.1.4",
          "form": "가장 최근에 영화관간 게 언제인지도 모르겠네요",
          "original_form": "가장 최근에 영화관간 게 언제인지도 모르겠네요",
          "speaker_id": "1",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015915.1.5",
          "form": "그래도 저는 대작 나오면 꾸준히 갔었어요ㅎㅎ",
          "original_form": "그래도 저는 대작 나오면 꾸준히 갔었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015915.1.6",
          "form": "어떤 거 보셨어요?",
          "original_form": "어떤 거 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015915.1.7",
          "form": "음...가장 최근에는 더수어사이드스쿼드요",
          "original_form": "음...가장 최근에는 더수어사이드스쿼드요",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015915.1.8",
          "form": "그거 잔인하다던데",
          "original_form": "그거 잔인하다던데",
          "speaker_id": "1",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015915.1.9",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "1",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015915.1.10",
          "form": "솔직히 좀 잔인하긴 했어요",
          "original_form": "솔직히 좀 잔인하긴 했어요",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015915.1.11",
          "form": "영화 자체도 청불이고",
          "original_form": "영화 자체도 청불이고",
          "speaker_id": "2",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015915.1.12",
          "form": "그래도 재미있었어요",
          "original_form": "그래도 재미있었어요",
          "speaker_id": "2",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015915.1.13",
          "form": "히어로 영화 좋아하시나봐요?",
          "original_form": "히어로 영화 좋아하시나봐요?",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015915.1.14",
          "form": "네. 좋아해요!",
          "original_form": "네. 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015915.1.15",
          "form": "마블 이런거요?",
          "original_form": "마블 이런거요?",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015915.1.16",
          "form": "마블도 좋아하고 디씨도 좋아해요",
          "original_form": "마블도 좋아하고 디씨도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015915.1.17",
          "form": "블랙위도우 보셨나요?",
          "original_form": "블랙위도우 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015915.1.18",
          "form": "아니오",
          "original_form": "아니오",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015915.1.19",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015915.1.20",
          "form": "저는 되게 재밌게 봐서",
          "original_form": "저는 되게 재밌게 봐서",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015915.1.21",
          "form": "꼭 추천드리고 싶어요.",
          "original_form": "꼭 추천드리고 싶어요.",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015915.1.22",
          "form": "잔인한 장면도 거의 안 나오더라구요",
          "original_form": "잔인한 장면도 거의 안 나오더라구요",
          "speaker_id": "2",
          "time": "20210826 02:53"
        }
      ]
