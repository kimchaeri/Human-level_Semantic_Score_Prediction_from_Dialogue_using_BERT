      "utterance": [
        {
          "id": "MDRW2100013890.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013890.1.2",
          "form": "안녕하세여~",
          "original_form": "안녕하세여~",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013890.1.3",
          "form": "좋아하는 프로그램 있으세여?",
          "original_form": "좋아하는 프로그램 있으세여?",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013890.1.4",
          "form": "저는 여즘 대탈출 보고 잇어요",
          "original_form": "저는 여즘 대탈출 보고 잇어요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013890.1.5",
          "form": "헐 저도 대탈출 봐요",
          "original_form": "헐 저도 대탈출 봐요",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013890.1.6",
          "form": "대탈출 존잼이져",
          "original_form": "대탈출 존잼이져",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013890.1.7",
          "form": "저 좀비편을 하필 새벽에 다시보기로 봐서..\n정말 무서웠어요",
          "original_form": "저 좀비편을 하필 새벽에 다시보기로 봐서..\n정말 무서웠어요",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013890.1.8",
          "form": "그거 새벽에 보면 무섭죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그거 새벽에 보면 무섭죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013890.1.9",
          "form": "저는 천해명 그런거",
          "original_form": "저는 천해명 그런거",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013890.1.10",
          "form": "새벽에 보면 무섭더라고요 ㅋㅋㅋ",
          "original_form": "새벽에 보면 무섭더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013890.1.11",
          "form": "아 그건 진짜 못봐요. 같이 보는 사람 꼭 있러야 봐요",
          "original_form": "아 그건 진짜 못봐요. 같이 보는 사람 꼭 있러야 봐요",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013890.1.12",
          "form": "마자요 ㅠㅠㅜㅠㅠㅠㅠ",
          "original_form": "마자요 ㅠㅠㅜㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013890.1.13",
          "form": "시즌 4 보셧어여?",
          "original_form": "시즌 4 보셧어여?",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013890.1.14",
          "form": "넹넹 저번주가 정말 대박이였어요. 어떻게 구성을 그렇게 짰는지 보면서 감탄핬어요.",
          "original_form": "넹넹 저번주가 정말 대박이였어요. 어떻게 구성을 그렇게 짰는지 보면서 감탄핬어요.",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013890.1.15",
          "form": "그니까여 진짜 피디랑 작가 대박이에여...",
          "original_form": "그니까여 진짜 피디랑 작가 대박이에여...",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013890.1.16",
          "form": "그거랑 전 요즘 노는 언니도 자주 봐요",
          "original_form": "그거랑 전 요즘 노는 언니도 자주 봐요",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013890.1.17",
          "form": "노는언니 재밌져",
          "original_form": "노는언니 재밌져",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013890.1.18",
          "form": "전엔 자주 보가가 요즘은 좀 안봣는ㄷ0",
          "original_form": "전엔 자주 보가가 요즘은 좀 안봣는ㄷ0",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013890.1.19",
          "form": "최근에 재밌는 편 잇낭7",
          "original_form": "최근에 재밌는 편 잇낭7",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013890.1.20",
          "form": "최근에 곽민정님이랑 김온아님 남현희님이 안나오셔서 ㅜㅜ",
          "original_form": "최근에 곽민정님이랑 김온아님 남현희님이 안나오셔서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013890.1.21",
          "form": "아마 그 1주년 언리픽도 재밌었어요",
          "original_form": "아마 그 1주년 언리픽도 재밌었어요",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013890.1.22",
          "form": "아 남현희님은 올림픽 때문에 안나오신건가?..",
          "original_form": "아 남현희님은 올림픽 때문에 안나오신건가?..",
          "speaker_id": "1",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013890.1.23",
          "form": "그 골때리는 이라는 프로그램에서 부상때문에 못나오시는 걸로 알고있어요",
          "original_form": "그 골때리는 이라는 프로그램에서 부상때문에 못나오시는 걸로 알고있어요",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013890.1.24",
          "form": "헐 그렇구나...",
          "original_form": "헐 그렇구나...",
          "speaker_id": "1",
          "time": "20210825 16:29"
        }
      ]
    }
  ]
