      "utterance": [
        {
          "id": "MDRW2100034483.1.1",
          "form": "네 방금 파스타를 먹었어요ㅎㅎ",
          "original_form": "네 방금 파스타를 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034483.1.2",
          "form": "빵 잘 만들고 계신가요",
          "original_form": "빵 잘 만들고 계신가요",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034483.1.3",
          "form": "아 빵 만들고 집에 왔어요ㅎㅎ",
          "original_form": "아 빵 만들고 집에 왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034483.1.4",
          "form": "파운드 케이크 베이킹 성공적으로 잘 끝냈습니다ㅋㅋ",
          "original_form": "파운드 케이크 베이킹 성공적으로 잘 끝냈습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034483.1.5",
          "form": "파스타 맛있게 드셨어요?",
          "original_form": "파스타 맛있게 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034483.1.6",
          "form": "오 금방 만드시네요!",
          "original_form": "오 금방 만드시네요!",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034483.1.7",
          "form": "파스타 맛있어요 로컬맛집!!",
          "original_form": "파스타 맛있어요 로컬맛집!!",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034483.1.8",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100034483.1.9",
          "form": "베이킹은 10시부터 해서",
          "original_form": "베이킹은 10시부터 해서",
          "speaker_id": "1",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100034483.1.10",
          "form": "기본인가요? 아님 뭔가 더 재료가 들어갔나요",
          "original_form": "기본인가요? 아님 뭔가 더 재료가 들어갔나요",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100034483.1.11",
          "form": "레몬이랑 초코파운드 케이크요",
          "original_form": "레몬이랑 초코파운드 케이크요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034483.1.12",
          "form": "오 진짜 맛있겠다! 드시나요",
          "original_form": "오 진짜 맛있겠다! 드시나요",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034483.1.13",
          "form": "이따 저녁 쯤 가족들이랑 먹으려고요",
          "original_form": "이따 저녁 쯤 가족들이랑 먹으려고요",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034483.1.14",
          "form": "무슨 파스타 드셨어요?ㅎㅎ",
          "original_form": "무슨 파스타 드셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034483.1.15",
          "form": "저는 오이스터 파스타랑 새우 로제 파스타 먹었어요",
          "original_form": "저는 오이스터 파스타랑 새우 로제 파스타 먹었어요",
          "speaker_id": "2",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100034483.1.16",
          "form": "해물류를 좋아하시나봐요",
          "original_form": "해물류를 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100034483.1.17",
          "form": "맛있으셨겠네요",
          "original_form": "맛있으셨겠네요",
          "speaker_id": "1",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100034483.1.18",
          "form": "그러게요ㅋㅋㅋ 하다보니 해물류",
          "original_form": "그러게요ㅋㅋㅋ 하다보니 해물류",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100034483.1.19",
          "form": "로컬맛집이었다니",
          "original_form": "로컬맛집이었다니",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034483.1.20",
          "form": "더 맛있었겠어요",
          "original_form": "더 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100034483.1.21",
          "form": "네네 시내에 있는 맛집인데 저희동네에 분점을 냈더라구요ㅋㅋ",
          "original_form": "네네 시내에 있는 맛집인데 저희동네에 분점을 냈더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100034483.1.22",
          "form": "오오 그랬군요",
          "original_form": "오오 그랬군요",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034483.1.23",
          "form": "주말에 맛있는 음식 드셔서 기분 좋으시겠어요",
          "original_form": "주말에 맛있는 음식 드셔서 기분 좋으시겠어요",
          "speaker_id": "1",
