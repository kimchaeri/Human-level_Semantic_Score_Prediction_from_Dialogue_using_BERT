      "utterance": [
        {
          "id": "MDRW2100046026.1.1",
          "form": "집에서하는거 귀찮다는 사람도있던데",
          "original_form": "집에서하는거 귀찮다는 사람도있던데",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.2",
          "form": "저도 집순이라서 집이 젤 좋아요",
          "original_form": "저도 집순이라서 집이 젤 좋아요",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.3",
          "form": "저는 진짜잘했어영",
          "original_form": "저는 진짜잘했어영",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.4",
          "form": "저도요 ㅋㅋㅋ나가는게더귀찬..",
          "original_form": "저도요 ㅋㅋㅋ나가는게더귀찬..",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.5",
          "form": "완전 꿀구매네요",
          "original_form": "완전 꿀구매네요",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.6",
          "form": "전 식품만 쟁일 생각했지",
          "original_form": "전 식품만 쟁일 생각했지",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.7",
          "form": "제모기는 생각 못했어요ㅋㅋㅋ",
          "original_form": "제모기는 생각 못했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.8",
          "form": "진짜조아요",
          "original_form": "진짜조아요",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.9",
          "form": "지금스마일데이",
          "original_form": "지금스마일데이",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.10",
          "form": "딜없니모르겟네요",
          "original_form": "딜없니모르겟네요",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046026.1.11",
          "form": "호시탐탐 노리다가",
          "original_form": "호시탐탐 노리다가",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.12",
          "form": "저는 딱 바로전 스마일데이때샀어요",
          "original_form": "저는 딱 바로전 스마일데이때샀어요",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.13",
          "form": "나중에 겟할게요ㅋㅋㅋ",
          "original_form": "나중에 겟할게요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.14",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.15",
          "form": "감사해요!",
          "original_form": "감사해요!",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.16",
          "form": "전집에서 젤네일도헷다가 ㅋㅋ",
          "original_form": "전집에서 젤네일도헷다가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.17",
          "form": "와 대배",
          "original_form": "와 대배",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.18",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.19",
          "form": "요새는안하는데",
          "original_form": "요새는안하는데",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.20",
          "form": "금손이신가봐요",
          "original_form": "금손이신가봐요",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.21",
          "form": "걍컬러만...ㅋㅋㅋ",
          "original_form": "걍컬러만...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.22",
          "form": "저 매니큐어만 발라도 다 번져서ㅋㅋㅋㅋ",
          "original_form": "저 매니큐어만 발라도 다 번져서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.23",
          "form": "집에서머하는거좋아하시면",
          "original_form": "집에서머하는거좋아하시면",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.24",
          "form": "좋을거같아욬",
          "original_form": "좋을거같아욬",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046026.1.26",
          "form": "한 번 시도 해봐야겠어요ㅋㅋㅋㅋ",
          "original_form": "한 번 시도 해봐야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100046026.1.27",
          "form": "근데이제 처분 ㅠ",
          "original_form": "근데이제 처분 ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:46"
        }
      ]
    }
