        {
          "id": "MDRW2100016406.1.1",
          "form": "넷플릭스 구독하세요?",
          "original_form": "넷플릭스 구독하세요?",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016406.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016406.1.3",
          "form": "주위에서 저만 안했어요ㅋㅋㅋ",
          "original_form": "주위에서 저만 안했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016406.1.4",
          "form": "하려고했는데 계속 미루게되더라구요",
          "original_form": "하려고했는데 계속 미루게되더라구요",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016406.1.5",
          "form": "영화 보는거 좋아하세요?",
          "original_form": "영화 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016406.1.6",
          "form": "영화 보는거 즐겨하는데",
          "original_form": "영화 보는거 즐겨하는데",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016406.1.7",
          "form": "코로나이후론 잘 안가요",
          "original_form": "코로나이후론 잘 안가요",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016406.1.8",
          "form": "인생영화 뭐세요??",
          "original_form": "인생영화 뭐세요??",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016406.1.9",
          "form": "음 아직 인생영화랄거는 없네요",
          "original_form": "음 아직 인생영화랄거는 없네요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016406.1.10",
          "form": "픽사 영화를 좋아하긴해요",
          "original_form": "픽사 영화를 좋아하긴해요",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016406.1.11",
          "form": "루카 보셨어요?",
          "original_form": "루카 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016406.1.12",
          "form": "아니요 이번에 나온건 안봤어요",
          "original_form": "아니요 이번에 나온건 안봤어요",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016406.1.13",
          "form": "영화관 큰 스크린으로 보면",
          "original_form": "영화관 큰 스크린으로 보면",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016406.1.14",
          "form": "좋을 것 같더라구여",
          "original_form": "좋을 것 같더라구여",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016406.1.15",
          "form": "name1님은 보셨나요?",
          "original_form": "&name1&님은 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016406.1.16",
          "form": "네! 감동적이에요",
          "original_form": "네! 감동적이에요",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016406.1.17",
          "form": "코코는 보셨나요?",
          "original_form": "코코는 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016406.1.18",
          "form": "네 봤어요",
          "original_form": "네 봤어요",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016406.1.19",
          "form": "코코 기대 안하고 봤는데",
          "original_form": "코코 기대 안하고 봤는데",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016406.1.20",
          "form": "색감이 엄청 예쁘더라구요",
          "original_form": "색감이 엄청 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016406.1.21",
          "form": "코코 ost좋아해요~",
          "original_form": "코코 ost좋아해요~",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016406.1.22",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100016406.1.23",
          "form": "마지막 장면이 여운 길었어요",
          "original_form": "마지막 장면이 여운 길었어요",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100016406.1.24",
          "form": "누군가에게 잊혀질 때 진정한 죽음을 맞이한다는 것도 흥미로웠구요",
