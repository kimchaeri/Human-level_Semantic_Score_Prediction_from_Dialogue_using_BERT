      "utterance": [
        {
          "id": "MDRW2100015374.1.1",
          "form": "유튜브같은거 많이 보시나요?",
          "original_form": "유튜브같은거 많이 보시나요?",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015374.1.2",
          "form": "네 저는 동물유투브 좋아해요",
          "original_form": "네 저는 동물유투브 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015374.1.3",
          "form": "하하하 애니멀봐 아세요? ㅋㅋ",
          "original_form": "하하하 애니멀봐 아세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015374.1.4",
          "form": "애니멀봐 저도 꽤 봤어요!!",
          "original_form": "애니멀봐 저도 꽤 봤어요!!",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015374.1.5",
          "form": "동물농장 재밌더라고요",
          "original_form": "동물농장 재밌더라고요",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015374.1.6",
          "form": "미뇽님은 뭐보세요?",
          "original_form": "미뇽님은 뭐보세요?",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015374.1.7",
          "form": "어릴때부터 동물농장도 많이 봤어서 그런거 좋아해용",
          "original_form": "어릴때부터 동물농장도 많이 봤어서 그런거 좋아해용",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015374.1.8",
          "form": "저는 운동관련된거 많이봐요!",
          "original_form": "저는 운동관련된거 많이봐요!",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015374.1.9",
          "form": "아 그런거 좋죠 ㅋㅋ도움 많이 되는거같아요",
          "original_form": "아 그런거 좋죠 ㅋㅋ도움 많이 되는거같아요",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015374.1.10",
          "form": "누구꺼보세요?",
          "original_form": "누구꺼보세요?",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015374.1.11",
          "form": "되게 이것저것 많이봐서..!",
          "original_form": "되게 이것저것 많이봐서..!",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015374.1.12",
          "form": "보디빌더들 유튜브도 좀 보구",
          "original_form": "보디빌더들 유튜브도 좀 보구",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015374.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015374.1.14",
          "form": "황철순아시나요? 그사람 운동영상도 요즘 많이 봐요!",
          "original_form": "황철순아시나요? 그사람 운동영상도 요즘 많이 봐요!",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015374.1.15",
          "form": "아 알아요 ㅋㅋ",
          "original_form": "아 알아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015374.1.16",
          "form": "와 김종국도 보세요???",
          "original_form": "와 김종국도 보세요???",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015374.1.17",
          "form": "종국형님을 깜빡하고있었네요! 시작한지 얼마 안되셨는데 다 챙겨봤어요!ㅋㅋㅋ",
          "original_form": "종국형님을 깜빡하고있었네요! 시작한지 얼마 안되셨는데 다 챙겨봤어요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015374.1.18",
          "form": "아 ㅋㅋㅋㅋㅋ 웃긴영상 많더라구요",
          "original_form": "아 ㅋㅋㅋㅋㅋ 웃긴영상 많더라구요",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015374.1.19",
          "form": "재미있기도 하고 운동을 진짜 잘하셔서 좋아요!ㅋㅋㅋ",
          "original_form": "재미있기도 하고 운동을 진짜 잘하셔서 좋아요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015374.1.20",
          "form": "그 빡빡이도 있던데",
          "original_form": "그 빡빡이도 있던데",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015374.1.21",
          "form": "이상한가면쓰고하시는",
          "original_form": "이상한가면쓰고하시는",
          "speaker_id": "1",
