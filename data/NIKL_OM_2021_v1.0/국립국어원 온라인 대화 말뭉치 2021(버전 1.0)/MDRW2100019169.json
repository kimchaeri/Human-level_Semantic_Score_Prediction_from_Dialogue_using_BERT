      "utterance": [
        {
          "id": "MDRW2100019169.1.1",
          "form": "싸이 하시나여",
          "original_form": "싸이 하시나여",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019169.1.2",
          "form": "싸이 했었어요",
          "original_form": "싸이 했었어요",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019169.1.3",
          "form": "다이어리처럼 썼던 기억이",
          "original_form": "다이어리처럼 썼던 기억이",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019169.1.4",
          "form": "저도 중2병 마인드의",
          "original_form": "저도 중2병 마인드의",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019169.1.5",
          "form": "뻘글을썼습니다",
          "original_form": "뻘글을썼습니다",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019169.1.6",
          "form": "넹 비공개로 많이 썼었죠ㅎ",
          "original_form": "넹 비공개로 많이 썼었죠ㅎ",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019169.1.7",
          "form": "비지엠도 많이 사구요",
          "original_form": "비지엠도 많이 사구요",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019169.1.8",
          "form": "오호 저도요 다이어리ㅋ 비밀사진첩ㅋ",
          "original_form": "오호 저도요 다이어리ㅋ 비밀사진첩ㅋ",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019169.1.9",
          "form": "일촌공개 생각나네요",
          "original_form": "일촌공개 생각나네요",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019169.1.10",
          "form": "파도타기도 있었죠ㅋ",
          "original_form": "파도타기도 있었죠ㅋ",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019169.1.11",
          "form": "아바타 옷도 있었던거 같아요",
          "original_form": "아바타 옷도 있었던거 같아요",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019169.1.12",
          "form": "아.. 맞다 배경룸도 샀었네요",
          "original_form": "아.. 맞다 배경룸도 샀었네요",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019169.1.13",
          "form": "넹 전 비지엠에 가장 도토리 많이 쓴거 같아요",
          "original_form": "넹 전 비지엠에 가장 도토리 많이 쓴거 같아요",
          "speaker_id": "1",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019169.1.14",
          "form": "네 저도 연애할때 기분에 따라",
          "original_form": "네 저도 연애할때 기분에 따라",
          "speaker_id": "2",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019169.1.15",
          "form": "저도 연애할때 그랬죠ㅋ",
          "original_form": "저도 연애할때 그랬죠ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019169.1.16",
