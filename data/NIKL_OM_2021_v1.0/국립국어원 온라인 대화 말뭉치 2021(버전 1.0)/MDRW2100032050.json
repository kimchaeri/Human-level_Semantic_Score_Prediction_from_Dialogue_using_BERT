      "utterance": [
        {
          "id": "MDRW2100032050.1.1",
          "form": "최근 PC 돔은 언제 하셨나요",
          "original_form": "최근 PC 돔은 언제 하셨나요",
          "speaker_id": "1",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100032050.1.2",
          "form": "피쉬돔",
          "original_form": "피쉬돔",
          "speaker_id": "1",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100032050.1.3",
          "form": "몇달 안 되었어요 실수로 깔아 놓았는데",
          "original_form": "몇달 안 되었어요 실수로 깔아 놓았는데",
          "speaker_id": "2",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100032050.1.4",
          "form": "비행기에서 실수로 켜 봤더니",
          "original_form": "비행기에서 실수로 켜 봤더니",
          "speaker_id": "2",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100032050.1.5",
          "form": "빨리 도깨비 출시 하면 좋겠어요 너무너무 재미있어 보이더라고요",
          "original_form": "빨리 도깨비 출시 하면 좋겠어요 너무너무 재미있어 보이더라고요",
          "speaker_id": "1",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100032050.1.6",
          "form": "인터넷 없어도 실행 되더라고요",
          "original_form": "인터넷 없어도 실행 되더라고요",
          "speaker_id": "2",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100032050.1.7",
          "form": "한 판 두 판 하던 것이",
          "original_form": "한 판 두 판 하던 것이",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.8",
          "form": "인터넷 없어도 실행되는 거 정말 좋아요 광고 꺼 놓을 수도 있고",
          "original_form": "인터넷 없어도 실행되는 거 정말 좋아요 광고 꺼 놓을 수도 있고",
          "speaker_id": "1",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.9",
          "form": "여기까지 왔네요",
          "original_form": "여기까지 왔네요",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.10",
          "form": "방구는 일도 없어요",
          "original_form": "방구는 일도 없어요",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.11",
          "form": "광고",
          "original_form": "광고",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.12",
          "form": "제 친구도 플래시 동 굉장히고 레벨이라고 합니다",
          "original_form": "제 친구도 플래시 동 굉장히고 레벨이라고 합니다",
          "speaker_id": "1",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.13",
          "form": "저는 광고 많은 게임을 많이 해 봐서요 이제 익숙합니다",
          "original_form": "저는 광고 많은 게임을 많이 해 봐서요 이제 익숙합니다",
          "speaker_id": "1",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.14",
          "form": "도장깨기 하듯이 하는 즐거움이 있어요",
          "original_form": "도장깨기 하듯이 하는 즐거움이 있어요",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.15",
          "form": "타일 마스터라고 아시나요 그건 광고 매일 나오거든요",
          "original_form": "타일 마스터라고 아시나요 그건 광고 매일 나오거든요",
          "speaker_id": "1",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.16",
          "form": "잊을만하면 1시간 무제한으로 할 수 있어요",
          "original_form": "잊을만하면 1시간 무제한으로 할 수 있어요",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.17",
          "form": "그렇게 저는 노예가 되었지요",
          "original_form": "그렇게 저는 노예가 되었지요",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032050.1.18",
          "form": "아 그렇군요 요리중독 생각나네요",
          "original_form": "아 그렇군요 요리중독 생각나네요",
          "speaker_id": "1",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100032050.1.19",
          "form": "요리중독 무시무시하죠",
          "original_form": "요리중독 무시무시하죠",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100032050.1.20",
          "form": "깔고 나서 1시간 무료 갑자기 뿌려서 미친듯이 했던 기억이나요",
          "original_form": "깔고 나서 1시간 무료 갑자기 뿌려서 미친듯이 했던 기억이나요",
          "speaker_id": "1",
          "time": "20210905 11:33"
        }
      ]
