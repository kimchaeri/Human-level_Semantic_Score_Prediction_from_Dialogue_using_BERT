      "utterance": [
        {
          "id": "MDRW2100024212.1.1",
          "form": "아 오늘 야근하고 왔네요ㅎㅎㅎ",
          "original_form": "아 오늘 야근하고 왔네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024212.1.2",
          "form": "오늘도 고생하셨어요ㅎ",
          "original_form": "오늘도 고생하셨어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024212.1.3",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024212.1.4",
          "form": "고생하셨네요ㅠ",
          "original_form": "고생하셨네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024212.1.5",
          "form": "야근수당도 안주는거아니에요?",
          "original_form": "야근수당도 안주는거아니에요?",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024212.1.6",
          "form": "네에 안주죠 하지만 일이 있으니 해야죠ㅋㅋ",
          "original_form": "네에 안주죠 하지만 일이 있으니 해야죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024212.1.7",
          "form": "안그러면 쌓이고ㅠ",
          "original_form": "안그러면 쌓이고ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024212.1.8",
          "form": "오늘도 늦게끝나셨어요?",
          "original_form": "오늘도 늦게끝나셨어요?",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024212.1.9",
          "form": "정말공감하네요ㅎ",
          "original_form": "정말공감하네요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024212.1.10",
          "form": "저는 오늘 칼퇴ㅎㅎ 피곤해서ㅎㅎ",
          "original_form": "저는 오늘 칼퇴ㅎㅎ 피곤해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024212.1.11",
          "form": "5시반 칼퇴했어요ㅎ",
          "original_form": "5시반 칼퇴했어요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024212.1.12",
          "form": "그죠ㅋㅋ오옷 출근이 8시30분인가요?",
          "original_form": "그죠ㅋㅋ오옷 출근이 8시30분인가요?",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024212.1.13",
          "form": "네네ㅎ 맞아요ㅎ",
          "original_form": "네네ㅎ 맞아요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024212.1.14",
          "form": "설마.. 같은회사는아니겟죠?ㄹㅎ",
          "original_form": "설마.. 같은회사는아니겟죠?ㄹㅎ",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024212.1.15",
          "form": "에이ㅎ설마일꺼야ㅎ",
          "original_form": "에이ㅎ설마일꺼야ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024212.1.16",
          "form": "아뇨ㅋㅋ저흰 9시요ㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋ저흰 9시요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024212.1.17",
          "form": "6시가 칼퇴이지요",
          "original_form": "6시가 칼퇴이지요",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024212.1.18",
          "form": "아아ㅎㅎ 다행이네요ㅎ",
          "original_form": "아아ㅎㅎ 다행이네요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024212.1.19",
          "form": "상사욕겁나하려던 참이었ㄴ는데",
          "original_form": "상사욕겁나하려던 참이었ㄴ는데",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024212.1.20",
          "form": "같은회사면 곤란해지니깐ㅎㅎ",
          "original_form": "같은회사면 곤란해지니깐ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024212.1.21",
          "form": "ㅋㅋ괜찮습니다 다 들어드립니다ㅋㅋㅋ",
          "original_form": "ㅋㅋ괜찮습니다 다 들어드립니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024212.1.22",
          "form": "원래 상사가 돈많이받는건 욕먹으라는 거죠ㅋㅋ",
          "original_form": "원래 상사가 돈많이받는건 욕먹으라는 거죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024212.1.23",
          "form": "맞ㅇ아ㅛㅎㅎ",
          "original_form": "맞ㅇ아ㅛㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024212.1.24",
          "form": "저희상사는 완전 딸랑이에요ㅎㅎ",
          "original_form": "저희상사는 완전 딸랑이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024212.1.25",
          "form": "아 입만 살았나보네요",
          "original_form": "아 입만 살았나보네요",
          "speaker_id": "2",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024212.1.26",
          "form": "내가 그래서 본업을 못하고있네요ㅠ",
          "original_form": "내가 그래서 본업을 못하고있네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024212.1.27",
          "form": "지가 하는건 없고ㅎ",
          "original_form": "지가 하는건 없고ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024212.1.28",
          "form": "다시키죠",
          "original_form": "다시키죠",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024212.1.29",
          "form": "헉 제일 최악이다....아 진짜 입만 산사람들 짜증이요\r\n일은 안하고 뭐 있으면 자기가 한것처럼 말하고..",
          "original_form": "헉 제일 최악이다....아 진짜 입만 산사람들 짜증이요\r\n일은 안하고 뭐 있으면 자기가 한것처럼 말하고..",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024212.1.30",
          "form": "ㅎㅎ 그런사람들이 진급은 무지하게 빨라요ㅎ",
          "original_form": "ㅎㅎ 그런사람들이 진급은 무지하게 빨라요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024212.1.31",
          "form": "30대부터 팀장이었대요ㅎㅎ",
          "original_form": "30대부터 팀장이었대요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024212.1.32",
          "form": "그럼뭐 말다한거죠ㅎ",
          "original_form": "그럼뭐 말다한거죠ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100024212.1.33",
          "form": "헉...얼마나 입을 잘털면....그것도 능력이네요",
          "original_form": "헉...얼마나 입을 잘털면....그것도 능력이네요",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024212.1.34",
          "form": "그런사람들이있어요ㅎㅎ",
          "original_form": "그런사람들이있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024212.1.35",
          "form": "근데 싫어하고싶어도 잘해또ㅡㅋ",
          "original_form": "근데 싫어하고싶어도 잘해또ㅡㅋ",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024212.1.36",
          "form": "꼴뵈기시르미...ㅡㅋ",
          "original_form": "꼴뵈기시르미...ㅡㅋ",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024212.1.37",
          "form": "꼴뵈기싫어도 상사라 대놓고 뭐라하지도 못하고ㅜ",
          "original_form": "꼴뵈기싫어도 상사라 대놓고 뭐라하지도 못하고ㅜ",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024212.1.38",
          "form": "ㅎㅎ 그냥 이젠 그런갑다하고 살고이ㅛ어요ㅎ",
          "original_form": "ㅎㅎ 그냥 이젠 그런갑다하고 살고이ㅛ어요ㅎ",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100024212.1.39",
          "form": "누가 그렇게 말하더라구요ㅎ 윗사람들이 먼저나가지ㅎ 우리가 먼저나가진 않을꺼라고ㅎ 기다리면 좋은날온다고 하더라구요ㅎ",
          "original_form": "누가 그렇게 말하더라구요ㅎ 윗사람들이 먼저나가지ㅎ 우리가 먼저나가진 않을꺼라고ㅎ 기다리면 좋은날온다고 하더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100024212.1.40",
          "form": "ㅋㅋㅋ아 그런가요? 반대아니에요ㅜㅜ",
          "original_form": "ㅋㅋㅋ아 그런가요? 반대아니에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 09:11"
        },
        {
          "id": "MDRW2100024212.1.41",
          "form": "우리가 그만두면 지는거죠ㅎㅎ",
          "original_form": "우리가 그만두면 지는거죠ㅎㅎ",
