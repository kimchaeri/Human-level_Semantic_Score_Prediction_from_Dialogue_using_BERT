      "utterance": [
        {
          "id": "MDRW2100004695.1.1",
          "form": "지에스 홈쇼핑 신규가입하면 4만원 할인권준데",
          "original_form": "지에스 홈쇼핑 신규가입하면 4만원 할인권준데",
          "speaker_id": "2",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004695.1.2",
          "form": "난 이미 회원 ㅜ.ㅜ",
          "original_form": "난 이미 회원 ㅜ.ㅜ",
          "speaker_id": "1",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004695.1.3",
          "form": "그렇구나 뭐 살려고 회원가입했었는데",
          "original_form": "그렇구나 뭐 살려고 회원가입했었는데",
          "speaker_id": "2",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004695.1.4",
          "form": "이벤트하길래 가입해서 홈드라이 샀던거 같다",
          "original_form": "이벤트하길래 가입해서 홈드라이 샀던거 같다",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004695.1.5",
          "form": "홈드라이 괜찮나?",
          "original_form": "홈드라이 괜찮나?",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004695.1.6",
          "form": "드라이 세탁소 비용보다 덜 든다",
          "original_form": "드라이 세탁소 비용보다 덜 든다",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004695.1.7",
          "form": "부피가 있는옷을 못하겠네",
          "original_form": "부피가 있는옷을 못하겠네",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004695.1.8",
          "form": "세탁기로 하면 된다는데 안한다 상할까봐",
          "original_form": "세탁기로 하면 된다는데 안한다 상할까봐",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004695.1.9",
          "form": "넌 산 물건 특히 옷을 너무 아낌",
          "original_form": "넌 산 물건 특히 옷을 너무 아낌",
          "speaker_id": "2",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004695.1.10",
          "form": "오래입을려고 하는거지 손상없이",
          "original_form": "오래입을려고 하는거지 손상없이",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004695.1.11",
          "form": "지금도 충분히 오래 입고 있다 의류사업들 굶어죽겠다",
          "original_form": "지금도 충분히 오래 입고 있다 의류사업들 굶어죽겠다",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004695.1.12",
          "form": "그정도는 아니고",
          "original_form": "그정도는 아니고",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004695.1.13",
          "form": "맞지 아니긴 뭐가 아니야",
          "original_form": "맞지 아니긴 뭐가 아니야",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004695.1.14",
          "form": "그럼 맞는걸로 하자",
          "original_form": "그럼 맞는걸로 하자",
          "speaker_id": "1",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004695.1.15",
