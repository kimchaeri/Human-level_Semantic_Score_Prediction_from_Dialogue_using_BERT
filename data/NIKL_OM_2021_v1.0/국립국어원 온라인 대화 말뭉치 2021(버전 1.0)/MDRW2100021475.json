      "utterance": [
        {
          "id": "MDRW2100021475.1.1",
          "form": "안녕하세용 최근에 영화 뭐 보셨어용?",
          "original_form": "안녕하세용 최근에 영화 뭐 보셨어용?",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021475.1.2",
          "form": "요즘 주식, 재테크에 관심 있는터라 그런 쪽으로 봤어요 작전, 돈, 빅쇼트 같은거요",
          "original_form": "요즘 주식, 재테크에 관심 있는터라 그런 쪽으로 봤어요 작전, 돈, 빅쇼트 같은거요",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021475.1.3",
          "form": "돈 저도 봤는데 재밌게 봤어요ㅋㅋㅋ",
          "original_form": "돈 저도 봤는데 재밌게 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021475.1.4",
          "form": "전 유튜브로 짧게 짧게 클립으로 봤어요",
          "original_form": "전 유튜브로 짧게 짧게 클립으로 봤어요",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021475.1.5",
          "form": "아 요즘 유튜브에 영화 결말까지 요약해주는 영상 많더라구요",
          "original_form": "아 요즘 유튜브에 영화 결말까지 요약해주는 영상 많더라구요",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021475.1.6",
          "form": "저도 그런거 잘봐요",
          "original_form": "저도 그런거 잘봐요",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021475.1.7",
          "form": "맞아요 저도 그런걸로 봐서 재밌게 봤어요",
          "original_form": "맞아요 저도 그런걸로 봐서 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021475.1.8",
          "form": "흥미 있었구요",
          "original_form": "흥미 있었구요",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021475.1.9",
          "form": "아무래도 영화한편 다 보려면 집중해서 봐야하는게 있긴하죠ㅋㅋㅋ",
          "original_form": "아무래도 영화한편 다 보려면 집중해서 봐야하는게 있긴하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021475.1.10",
          "form": "짧게짧게 핵심부분만 요약해주는 영상이 더 속편하달까요",
          "original_form": "짧게짧게 핵심부분만 요약해주는 영상이 더 속편하달까요",
          "speaker_id": "1",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100021475.1.11",
          "form": "맞아요 필요한것만 보면 되니깐 좋아요",
          "original_form": "맞아요 필요한것만 보면 되니깐 좋아요",
          "speaker_id": "2",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100021475.1.12",
          "form": "저 이걸로 최근에",
          "original_form": "저 이걸로 최근에",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021475.1.13",
          "form": "재밌어요 추천드려요",
          "original_form": "재밌어요 추천드려요",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021475.1.14",
          "form": "인도영화",
          "original_form": "인도영화",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021475.1.15",
          "form": "뭄바이",
          "original_form": "뭄바이",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021475.1.16",
          "form": "인도 영화 재밌어요",
          "original_form": "인도 영화 재밌어요",
          "speaker_id": "2",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021475.1.17",
          "form": "은근 재밌어요",
          "original_form": "은근 재밌어요",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021475.1.18",
          "form": "좋아하시는 장르가 따로 있으세요?",
          "original_form": "좋아하시는 장르가 따로 있으세요?",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021475.1.19",
          "form": "저번에 요약 영상으로 본건데 인도 하층민이었다가 나중에 사장되는 이야기던데 넷플에 있더라구요",
          "original_form": "저번에 요약 영상으로 본건데 인도 하층민이었다가 나중에 사장되는 이야기던데 넷플에 있더라구요",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100021475.1.20",
          "form": "제목 생각났네요 화이트 타이거였어요",
          "original_form": "제목 생각났네요 화이트 타이거였어요",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021475.1.21",
          "form": "어머 얘기만 들어도 너무 재밌네요",
          "original_form": "어머 얘기만 들어도 너무 재밌네요",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021475.1.22",
          "form": "전 범죄 스릴러 잘 봐요",
          "original_form": "전 범죄 스릴러 잘 봐요",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021475.1.23",
          "form": "저 이런 영화 좋아하는데",
          "original_form": "저 이런 영화 좋아하는데",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021475.1.24",
          "form": "저두요!ㅋㅋㅋ",
          "original_form": "저두요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021475.1.25",
          "form": "님은 어떤 장르 좋아하세요?",
          "original_form": "님은 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021475.1.26",
          "form": "아하 비슷하네요",
          "original_form": "아하 비슷하네요",
          "speaker_id": "2",
          "time": "20210828 20:07"
        }
      ]
    }
  ]
