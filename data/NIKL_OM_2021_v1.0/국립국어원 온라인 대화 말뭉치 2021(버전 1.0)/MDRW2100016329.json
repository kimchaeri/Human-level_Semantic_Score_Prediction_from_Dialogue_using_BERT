      "utterance": [
        {
          "id": "MDRW2100016329.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016329.1.2",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016329.1.3",
          "form": "네 고양이 키우고있어요",
          "original_form": "네 고양이 키우고있어요",
          "speaker_id": "2",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016329.1.4",
          "form": "고양이 ㅠㅠ 어떤 종인가요?",
          "original_form": "고양이 ㅠㅠ 어떤 종인가요?",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016329.1.5",
          "form": "코숏이요. 치즈냥이입니다",
          "original_form": "코숏이요. 치즈냥이입니다",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016329.1.6",
          "form": "아 치즈냥 ㅠㅠ 넘 귀엽겠는데요",
          "original_form": "아 치즈냥 ㅠㅠ 넘 귀엽겠는데요",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016329.1.7",
          "form": "털 많이 날리죠??",
          "original_form": "털 많이 날리죠??",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016329.1.8",
          "form": "생각보단 별로 안날려요ㅋㅋ",
          "original_form": "생각보단 별로 안날려요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016329.1.9",
          "form": "활동량이 너무 많아서 문제지...",
          "original_form": "활동량이 너무 많아서 문제지...",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016329.1.10",
          "form": "ㅋㅋㅋㅋㅋ아 활발한 아이인가보네요",
          "original_form": "ㅋㅋㅋㅋㅋ아 활발한 아이인가보네요",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016329.1.11",
          "form": "맨날 울어요 와서",
          "original_form": "맨날 울어요 와서",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016329.1.12",
          "form": "놀아달라구",
          "original_form": "놀아달라구",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016329.1.13",
          "form": "앜ㅋㅋㅋㅋㅋ귀엽겠는데요? 개냥이같은 스타일인가요?",
          "original_form": "앜ㅋㅋㅋㅋㅋ귀엽겠는데요? 개냥이같은 스타일인가요?",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016329.1.14",
          "form": "전형적인 고양이 성격인데",
          "original_form": "전형적인 고양이 성격인데",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016329.1.15",
          "form": "좀 멍청한것같아요",
          "original_form": "좀 멍청한것같아요",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016329.1.16",
          "form": "ㅋㅋㅋㅋㅋ그것도 넘 귀여운데요..?",
          "original_form": "ㅋㅋㅋㅋㅋ그것도 넘 귀여운데요..?",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016329.1.17",
          "form": "어떻게 멍청한가욬ㅋㅋㅋㅋ",
          "original_form": "어떻게 멍청한가욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016329.1.18",
          "form": "도망도 잘 못가고",
          "original_form": "도망도 잘 못가고",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016329.1.19",
          "form": "도망가다가 간식흔들면 머리 부비적대고",
          "original_form": "도망가다가 간식흔들면 머리 부비적대고",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016329.1.20",
          "form": "물도 제대로 못먹어서 그릇을 세번이나 바꿨어요",
          "original_form": "물도 제대로 못먹어서 그릇을 세번이나 바꿨어요",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016329.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ귀엽내요 ㅠㅠ 물은 왜 못먹는거죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ귀엽내요 ㅠㅠ 물은 왜 못먹는거죠",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016329.1.22",
          "form": "모르겠어요ㅋㅋㅋ 머리에 물 다 묻히고 뒤집어 엎고",
          "original_form": "모르겠어요ㅋㅋㅋ 머리에 물 다 묻히고 뒤집어 엎고",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016329.1.23",
          "form": "얼굴에 막 습식 다묻히고다니고",
          "original_form": "얼굴에 막 습식 다묻히고다니고",
          "speaker_id": "2",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016329.1.24",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210826 12:31"
        }
      ]
    }
