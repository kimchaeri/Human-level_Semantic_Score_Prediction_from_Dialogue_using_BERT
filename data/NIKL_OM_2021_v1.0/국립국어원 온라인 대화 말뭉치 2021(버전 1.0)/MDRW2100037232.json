      "utterance": [
        {
          "id": "MDRW2100037232.1.1",
          "form": "요새 재밌게 보시는 프로 있나요?",
          "original_form": "요새 재밌게 보시는 프로 있나요?",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037232.1.2",
          "form": "Y 좋아해요",
          "original_form": "Y 좋아해요",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037232.1.3",
          "form": "드라마는 짐 홍천기 보고 있구요",
          "original_form": "드라마는 짐 홍천기 보고 있구요",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037232.1.4",
          "form": "아 홍천기 새로 시작한 거 맞죠",
          "original_form": "아 홍천기 새로 시작한 거 맞죠",
          "speaker_id": "1",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100037232.1.5",
          "form": "Y는 궁금한이야기 인가요?",
          "original_form": "Y는 궁금한이야기 인가요?",
          "speaker_id": "1",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100037232.1.6",
          "form": "네 궁금한 이야기요~",
          "original_form": "네 궁금한 이야기요~",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100037232.1.7",
          "form": "홍천기 김유정이 이뻐서 보게 되네요",
          "original_form": "홍천기 김유정이 이뻐서 보게 되네요",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100037232.1.8",
          "form": "김유정 진짜 이쁘죠 ㅎㅎ",
          "original_form": "김유정 진짜 이쁘죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100037232.1.9",
          "form": "그거 약간 판타지인거 맞나요?",
          "original_form": "그거 약간 판타지인거 맞나요?",
          "speaker_id": "1",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100037232.1.10",
          "form": "네 판타지에요~~",
          "original_form": "네 판타지에요~~",
          "speaker_id": "2",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100037232.1.11",
          "form": "캡쳐만 봤는데 분장이 막 화려하더라고요",
          "original_form": "캡쳐만 봤는데 분장이 막 화려하더라고요",
          "speaker_id": "1",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100037232.1.12",
          "form": "CG가 영화 급 이던데요!",
          "original_form": "CG가 영화 급 이던데요!",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100037232.1.13",
          "form": "그쵸 스토리도 재밌나보네요 재방하면 한번 봐야겠어용",
          "original_form": "그쵸 스토리도 재밌나보네요 재방하면 한번 봐야겠어용",
          "speaker_id": "1",
          "time": "20210901 13:36"
        },
        {
          "id": "MDRW2100037232.1.14",
          "form": "요즘 드라마 펜트말고 보는게 없었는데 볼만 하더라구요",
          "original_form": "요즘 드라마 펜트말고 보는게 없었는데 볼만 하더라구요",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100037232.1.15",
          "form": "펜트하우스 저도 봤었는데 시즌 3은 아직 시작을 못했네요 ㅎㅎ",
          "original_form": "펜트하우스 저도 봤었는데 시즌 3은 아직 시작을 못했네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100037232.1.16",
          "form": "다음주면 펜트 끝나요.ㅎㅎ\n저번주에 펜트 무너져 내리는거에서 끝났는데 이번주 어떤 내용일지..",
          "original_form": "다음주면 펜트 끝나요.ㅎㅎ\n저번주에 펜트 무너져 내리는거에서 끝났는데 이번주 어떤 내용일지..",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100037232.1.17",
          "form": "아 그 샹들리에 떨어지는거 맞나요 움짤은 봤는데",
          "original_form": "아 그 샹들리에 떨어지는거 맞나요 움짤은 봤는데",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100037232.1.18",
          "form": "샹들리에는 천서진 한테 떨어진거 같은데요",
          "original_form": "샹들리에는 천서진 한테 떨어진거 같은데요",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100037232.1.19",
          "form": "저번주 내용인거 같아요",
          "original_form": "저번주 내용인거 같아요",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100037232.1.20",
          "form": "아 무너지는 게 집이 무너진건가요?",
          "original_form": "아 무너지는 게 집이 무너진건가요?",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100037232.1.21",
          "form": "펜트하우스 전체요.\n주단태가 폭탄 설치했는데 총 맞고 버튼눌러서 폭파하더라구요",
          "original_form": "펜트하우스 전체요.\n주단태가 폭탄 설치했는데 총 맞고 버튼눌러서 폭파하더라구요",
          "speaker_id": "2",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100037232.1.22",
          "form": "헐 대박 ㅋㅋㅋㅋ 파격전개네요...",
          "original_form": "헐 대박 ㅋㅋㅋㅋ 파격전개네요...",
          "speaker_id": "1",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100037232.1.23",
          "form": "진짜 예측불가능한 드라마군요 ㅋㅋㅋ",
          "original_form": "진짜 예측불가능한 드라마군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100037232.1.24",
          "form": "펜트는 항상 파격적이었죠ㅋ ㅋ",
          "original_form": "펜트는 항상 파격적이었죠ㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100037232.1.25",
          "form": "로건 형이 젤 웃겨요 ㅎ",
          "original_form": "로건 형이 젤 웃겨요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100037232.1.26",
          "form": "맨날 죽었다 살아나고 그래서 ㅋㅋㅋ",
          "original_form": "맨날 죽었다 살아나고 그래서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100037232.1.27",
          "form": "어떻게 끝날지 예측할 수 없는 드라마 인거 같아요",
          "original_form": "어떻게 끝날지 예측할 수 없는 드라마 인거 같아요",
          "speaker_id": "2",
          "time": "20210901 13:49"
        }
