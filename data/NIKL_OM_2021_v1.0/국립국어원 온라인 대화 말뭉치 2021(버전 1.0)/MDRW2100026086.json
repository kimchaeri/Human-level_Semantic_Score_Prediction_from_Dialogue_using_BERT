      "utterance": [
        {
          "id": "MDRW2100026086.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026086.1.2",
          "form": "네 먹었어요!",
          "original_form": "네 먹었어요!",
          "speaker_id": "2",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026086.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026086.1.4",
          "form": "네 저 콩국수 먹었어요 뭐드셨어요?",
          "original_form": "네 저 콩국수 먹었어요 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026086.1.5",
          "form": "시원한거 드셨네요!",
          "original_form": "시원한거 드셨네요!",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026086.1.6",
          "form": "저는 육개장 먹었어요 ㅎㅎ",
          "original_form": "저는 육개장 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026086.1.7",
          "form": "콩국수에 설탕 뿌리시나요 소금 뿌리시나요??",
          "original_form": "콩국수에 설탕 뿌리시나요 소금 뿌리시나요??",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026086.1.8",
          "form": "저 소금이요 설탕은 한 번도 안 뿌려봤어요",
          "original_form": "저 소금이요 설탕은 한 번도 안 뿌려봤어요",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100026086.1.9",
          "form": "님은  뭐 뿌려 드세요",
          "original_form": "님은  뭐 뿌려 드세요",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100026086.1.10",
          "form": "저도 소금 뿌려 먹는데 설탕도 먹을만했어요",
          "original_form": "저도 소금 뿌려 먹는데 설탕도 먹을만했어요",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100026086.1.11",
          "form": "다음엔 설탕 함 도전해봐야겠어요ㅋㅋ",
          "original_form": "다음엔 설탕 함 도전해봐야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100026086.1.12",
          "form": "소금 설탕 하니 생각난건데\r\n팥죽에 소금vs설탕?",
          "original_form": "소금 설탕 하니 생각난건데\r\n팥죽에 소금vs설탕?",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100026086.1.13",
          "form": "소금이요! 팥죽에도 설탕을 넣나봐요 ㅎㅎ",
          "original_form": "소금이요! 팥죽에도 설탕을 넣나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100026086.1.14",
          "form": "네 저 팥죽에 설탕 사랑해요!!",
          "original_form": "네 저 팥죽에 설탕 사랑해요!!",
          "speaker_id": "1",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100026086.1.15",
          "form": "그리고 새알도 좋아요",
          "original_form": "그리고 새알도 좋아요",
          "speaker_id": "1",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100026086.1.16",
          "form": "오! 그럼 다음에 팥죽 먹을 때는 설탕을 넣어봐야겠어요 ㅋㅋㅋㅋ",
          "original_form": "오! 그럼 다음에 팥죽 먹을 때는 설탕을 넣어봐야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100026086.1.17",
          "form": "새알 건져먹는 거 맛있죠! 동지 얼른 왔으면 좋겠네요 ㅎㅎ",
          "original_form": "새알 건져먹는 거 맛있죠! 동지 얼른 왔으면 좋겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100026086.1.18",
          "form": "맞아요ㅎㅎ 팥칼국수는 드셔보셨어요",
          "original_form": "맞아요ㅎㅎ 팥칼국수는 드셔보셨어요",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100026086.1.19",
          "form": "네! 팥칼국수 좋아하는 친구가 있어서 만나면 꼭 그거 먹어요 ㅎㅎ",
          "original_form": "네! 팥칼국수 좋아하는 친구가 있어서 만나면 꼭 그거 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100026086.1.20",
          "form": "아 저도 오랜만에 먹고싶어요 저는 거기에도 설탕뿌려먹어요ㅋㅋ기",
          "original_form": "아 저도 오랜만에 먹고싶어요 저는 거기에도 설탕뿌려먹어요ㅋㅋ기",
          "speaker_id": "1",
          "time": "20210902 17:22"
        }
      ]
