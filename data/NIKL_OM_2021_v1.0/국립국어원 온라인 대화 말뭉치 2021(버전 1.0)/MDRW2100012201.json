      "utterance": [
        {
          "id": "MDRW2100012201.1.1",
          "form": "나는 부모님이랑 동생 한 명 있어",
          "original_form": "나는 부모님이랑 동생 한 명 있어",
          "speaker_id": "2",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100012201.1.2",
          "form": "동생은 몇살이야?",
          "original_form": "동생은 몇살이야?",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100012201.1.3",
          "form": "동생은 이제 고등학교 2학년이야",
          "original_form": "동생은 이제 고등학교 2학년이야",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012201.1.4",
          "form": "중요한 시기네 공부잘해?",
          "original_form": "중요한 시기네 공부잘해?",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012201.1.5",
          "form": "후 맨날 게임만 해서 공부는 잘하지 못하는 것 같아ㅠ",
          "original_form": "후 맨날 게임만 해서 공부는 잘하지 못하는 것 같아ㅠ",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012201.1.6",
          "form": "넌 동생 있어?",
          "original_form": "넌 동생 있어?",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012201.1.7",
          "form": "난 동생없어 그래도 동생있으면 좋은점 있지 않아?",
          "original_form": "난 동생없어 그래도 동생있으면 좋은점 있지 않아?",
          "speaker_id": "1",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012201.1.8",
          "form": "동생 옷 중에 예쁜 옷이 있으면 뺏어입을 수 있어!",
          "original_form": "동생 옷 중에 예쁜 옷이 있으면 뺏어입을 수 있어!",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012201.1.9",
          "form": "남동생이야 여동생이야?",
          "original_form": "남동생이야 여동생이야?",
          "speaker_id": "1",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012201.1.10",
          "form": "남동생이얔ㅋㅋㅋㅋㅋ",
          "original_form": "남동생이얔ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012201.1.11",
          "form": "오 사이 좋은가봐?",
          "original_form": "오 사이 좋은가봐?",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012201.1.12",
          "form": "아니 그렇게 사이좋지가 않아..",
          "original_form": "아니 그렇게 사이좋지가 않아..",
          "speaker_id": "2",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012201.1.13",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100012201.1.14",
          "form": "그냥 먹을 꺼 때문에 좀 많이 다퉈ㅋㅋㅋ",
          "original_form": "그냥 먹을 꺼 때문에 좀 많이 다퉈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100012201.1.15",
          "form": "서로 더 많이 먹으려고?",
          "original_form": "서로 더 많이 먹으려고?",
          "speaker_id": "1",
          "time": "20210824 20:22"
        },
        {
          "id": "MDRW2100012201.1.16",
          "form": "응ㅋㅋㅋㅋ내가 먹는 거 진짜 좋아하거든",
          "original_form": "응ㅋㅋㅋㅋ내가 먹는 거 진짜 좋아하거든",
          "speaker_id": "2",
          "time": "20210824 20:22"
        },
        {
          "id": "MDRW2100012201.1.17",
          "form": "아 그렇구나 그럼 싸우면 누가이겨?",
          "original_form": "아 그렇구나 그럼 싸우면 누가이겨?",
          "speaker_id": "1",
          "time": "20210824 20:23"
        },
        {
          "id": "MDRW2100012201.1.18",
          "form": "동생이 힘이 센데 날 봐줘",
          "original_form": "동생이 힘이 센데 날 봐줘",
          "speaker_id": "2",
          "time": "20210824 20:23"
        },
        {
          "id": "MDRW2100012201.1.19",
          "form": "그래서 내가 이경ㅎㅎ",
          "original_form": "그래서 내가 이경ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:23"
        },
        {
          "id": "MDRW2100012201.1.20",
          "form": "동생이 착하네",
          "original_form": "동생이 착하네",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012201.1.21",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012201.1.22",
          "form": "넌 강아지랑 산다고 했나?",
          "original_form": "넌 강아지랑 산다고 했나?",
          "speaker_id": "2",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012201.1.23",
          "form": "아니 고양이",
          "original_form": "아니 고양이",
          "speaker_id": "1",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012201.1.24",
          "form": "이름이 모야?",
          "original_form": "이름이 모야?",
          "speaker_id": "2",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012201.1.25",
          "form": "또치야",
          "original_form": "또치야",
          "speaker_id": "1",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012201.1.26",
          "form": "헐 강아지 이름 같다!",
          "original_form": "헐 강아지 이름 같다!",
          "speaker_id": "2",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012201.1.27",
          "form": "이름부르면 강아지처럼 뛰어와",
          "original_form": "이름부르면 강아지처럼 뛰어와",
          "speaker_id": "1",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012201.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ또치랑 사이는 어때?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ또치랑 사이는 어때?",
          "speaker_id": "2",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012201.1.29",
          "form": "당연히 사이 좋지",
          "original_form": "당연히 사이 좋지",
          "speaker_id": "1",
          "time": "20210824 20:27"
        }
      ]
    }
  ]
