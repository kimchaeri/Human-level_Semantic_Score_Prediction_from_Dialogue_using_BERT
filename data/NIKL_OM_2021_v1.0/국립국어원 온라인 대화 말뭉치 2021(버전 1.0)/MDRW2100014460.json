      "utterance": [
        {
          "id": "MDRW2100014460.1.1",
          "form": "가고싶은",
          "original_form": "가고싶은",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014460.1.2",
          "form": "여행지가 있나요",
          "original_form": "여행지가 있나요",
          "speaker_id": "2",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014460.1.3",
          "form": "동유럽을 못가봐서 가보고 싶어요",
          "original_form": "동유럽을 못가봐서 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014460.1.4",
          "form": "정말 가고싶네요 ㅜㅜㅜㅜ",
          "original_form": "정말 가고싶네요 ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014460.1.5",
          "form": "혹시 동유럽 가보신 나라 있으세요?",
          "original_form": "혹시 동유럽 가보신 나라 있으세요?",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014460.1.6",
          "form": "아뇨 ㅠ 없어요",
          "original_form": "아뇨 ㅠ 없어요",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014460.1.7",
          "form": "가본 적 있으세요?",
          "original_form": "가본 적 있으세요?",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014460.1.8",
          "form": "체코 갈 기회가 있었는데 예산이 안돼서 못갔어요 ㅠㅠㅠ",
          "original_form": "체코 갈 기회가 있었는데 예산이 안돼서 못갔어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014460.1.9",
          "form": "혹시 해외여행 가보셨나요?!",
          "original_form": "혹시 해외여행 가보셨나요?!",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014460.1.10",
          "form": "네 ㅋㅋ 완전 예전에 일본이요",
          "original_form": "네 ㅋㅋ 완전 예전에 일본이요",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014460.1.11",
          "form": "그게 끝이었습니다..",
          "original_form": "그게 끝이었습니다..",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014460.1.12",
          "form": "저도 일본 가봤어요!! 어디 가보셨어요?",
          "original_form": "저도 일본 가봤어요!! 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014460.1.13",
          "form": "저 오사카요",
          "original_form": "저 오사카요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014460.1.14",
          "form": "어디가셨나여?",
          "original_form": "어디가셨나여?",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014460.1.15",
          "form": "오사카랑 규슈 가봤어요! 그럼 유니버셜 스튜디오 가셨나요??!",
          "original_form": "오사카랑 규슈 가봤어요! 그럼 유니버셜 스튜디오 가셨나요??!",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100014460.1.16",
          "form": "네 당연하죠",
          "original_form": "네 당연하죠",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100014460.1.17",
          "form": "필수코스 아닌가요 ㅎㅎ",
          "original_form": "필수코스 아닌가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100014460.1.18",
          "form": "그때 뭐 사가지고 오셨나요? 전 10년전에 가서 그당시 유행하던 우마이봉이랑 동전파스 사가지고 왔어욬ㅋㅋㅋㅋ",
          "original_form": "그때 뭐 사가지고 오셨나요? 전 10년전에 가서 그당시 유행하던 우마이봉이랑 동전파스 사가지고 왔어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:48"
        },
        {
          "id": "MDRW2100014460.1.19",
          "form": "ㅋㅋㅋㅋ 저도요 곤약젤리도 사오고",
          "original_form": "ㅋㅋㅋㅋ 저도요 곤약젤리도 사오고",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100014460.1.20",
          "form": "막 여러가지 사왔어요",
          "original_form": "막 여러가지 사왔어요",
          "speaker_id": "2",
