        },
        {
          "id": "MDRW2100046881.1.2",
          "form": "뭐 팔았어?",
          "original_form": "뭐 팔았어?",
          "speaker_id": "2",
          "time": "20210930 00:35"
        },
        {
          "id": "MDRW2100046881.1.3",
          "form": "name3이 어릴 때 쓰던 노트북 ㅎ",
          "original_form": "&name3&이 어릴 때 쓰던 노트북 ㅎ",
          "speaker_id": "1",
          "time": "20210930 00:35"
        },
        {
          "id": "MDRW2100046881.1.4",
          "form": "얼마에 팔았어?",
          "original_form": "얼마에 팔았어?",
          "speaker_id": "2",
          "time": "20210930 00:36"
        },
        {
          "id": "MDRW2100046881.1.5",
          "form": "만원 ㅋ 집 벨 눌러서 만원을 흔드시더래 ㅋㅋ",
          "original_form": "만원 ㅋ 집 벨 눌러서 만원을 흔드시더래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210930 00:36"
        },
        {
          "id": "MDRW2100046881.1.6",
          "form": "웃기네 ㅋㅋ 당근은 직거래가 편해서 좋아",
          "original_form": "웃기네 ㅋㅋ 당근은 직거래가 편해서 좋아",
          "speaker_id": "2",
          "time": "20210930 00:36"
        },
        {
          "id": "MDRW2100046881.1.7",
          "form": "주말부터 쌓아둔 폐지도 해결",
          "original_form": "주말부터 쌓아둔 폐지도 해결",
          "speaker_id": "1",
          "time": "20210930 00:37"
        },
        {
          "id": "MDRW2100046881.1.8",
          "form": "오 그건 누가 치움?",
          "original_form": "오 그건 누가 치움?",
          "speaker_id": "2",
          "time": "20210930 00:37"
        },
        {
          "id": "MDRW2100046881.1.9",
          "form": "그것도 당근으로 폐지 나눔 ㅎ 금방 연락 옴",
          "original_form": "그것도 당근으로 폐지 나눔 ㅎ 금방 연락 옴",
          "speaker_id": "1",
          "time": "20210930 00:37"
        },
        {
          "id": "MDRW2100046881.1.10",
          "form": "편하고 좋네 ㅎ 또 뭐 팔려고 ㅋㅋ",
          "original_form": "편하고 좋네 ㅎ 또 뭐 팔려고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210930 00:38"
        },
        {
          "id": "MDRW2100046881.1.11",
          "form": "내일은 name3이 축구게임 ㅋㅋ",
          "original_form": "내일은 &name3&이 축구게임 ㅋㅋ",
