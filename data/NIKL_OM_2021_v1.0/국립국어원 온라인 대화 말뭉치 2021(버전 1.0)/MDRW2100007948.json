      "utterance": [
        {
          "id": "MDRW2100007948.1.1",
          "form": "공연 보러가신적 있으세요?",
          "original_form": "공연 보러가신적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007948.1.2",
          "form": "밥먹느라 답늦었어요",
          "original_form": "밥먹느라 답늦었어요",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007948.1.3",
          "form": "공연은 예전에 한번봤어요.",
          "original_form": "공연은 예전에 한번봤어요.",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007948.1.4",
          "form": "어떤 공연이었나요?",
          "original_form": "어떤 공연이었나요?",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007948.1.5",
          "form": "라이어라이어? 연극이요",
          "original_form": "라이어라이어? 연극이요",
          "speaker_id": "2",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100007948.1.6",
          "form": "연극을 보러 가셨었군요",
          "original_form": "연극을 보러 가셨었군요",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100007948.1.7",
          "form": "저는 연극을 본적이 별로 없어요",
          "original_form": "저는 연극을 본적이 별로 없어요",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100007948.1.8",
          "form": "ㅎ 저도 첨봤어요.  관심은 있는데 넘멀어서 보기가 쉽지않네요",
          "original_form": "ㅎ 저도 첨봤어요.  관심은 있는데 넘멀어서 보기가 쉽지않네요",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100007948.1.9",
          "form": "영화만 자주보러다녀요",
          "original_form": "영화만 자주보러다녀요",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100007948.1.10",
          "form": "영화가 접근성이 제일 크죠",
          "original_form": "영화가 접근성이 제일 크죠",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100007948.1.11",
          "form": "네. 그나마 보기가 젤루 편하죠. 시간대도 다양해서",
          "original_form": "네. 그나마 보기가 젤루 편하죠. 시간대도 다양해서",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100007948.1.12",
          "form": "주로 어느 시간대에 보세요? 저는 조조 좋아해요 싸기도 하고.",
          "original_form": "주로 어느 시간대에 보세요? 저는 조조 좋아해요 싸기도 하고.",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100007948.1.13",
          "form": "옆동네로 원정가야해서 11시아님 점심먹고 2시",
          "original_form": "옆동네로 원정가야해서 11시아님 점심먹고 2시",
          "speaker_id": "2",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100007948.1.14",
          "form": "그때는 사람이 많죠?",
          "original_form": "그때는 사람이 많죠?",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100007948.1.15",
          "form": "평일날 자주가서 별로없어요",
          "original_form": "평일날 자주가서 별로없어요",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100007948.1.16",
          "form": "평일 그 시간이면 없긴 하겠네요",
          "original_form": "평일 그 시간이면 없긴 하겠네요",
          "speaker_id": "1",
          "time": "20210822 13:07"
        }
      ]
