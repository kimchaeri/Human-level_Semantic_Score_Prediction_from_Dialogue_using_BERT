      "utterance": [
        {
          "id": "MDRW2100037103.1.1",
          "form": "님은 제가 못 산 청사과를 사셨네요ㅎㅎ",
          "original_form": "님은 제가 못 산 청사과를 사셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:42"
        },
        {
          "id": "MDRW2100037103.1.2",
          "form": "ㅋㅋㅋㅋ 저희 하나씩 바꿀까요 !",
          "original_form": "ㅋㅋㅋㅋ 저희 하나씩 바꿀까요 !",
          "speaker_id": "1",
          "time": "20210831 19:44"
        },
        {
          "id": "MDRW2100037103.1.3",
          "form": "아 가까운곳 살면 딱 좋은데ㅋㅋ 그쵸ㅋㅋㅋ",
          "original_form": "아 가까운곳 살면 딱 좋은데ㅋㅋ 그쵸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100037103.1.4",
          "form": "그러게욬ㅋㅋㅋ 아 그런거 딱 좋네요",
          "original_form": "그러게욬ㅋㅋㅋ 아 그런거 딱 좋네요",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037103.1.5",
          "form": "많이 사기는 아깝고 ㅋㅋ",
          "original_form": "많이 사기는 아깝고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037103.1.6",
          "form": "맞아요 수박도 하나 사서 반 나누면 좋죠ㅋㅋㅋㅋ",
          "original_form": "맞아요 수박도 하나 사서 반 나누면 좋죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100037103.1.7",
          "form": "헐 맞아요 ㅋㅋㅋ",
          "original_form": "헐 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100037103.1.8",
          "form": "괜히 수박은 작은거 사면 손해인 기분이에요 ㅋㅋ",
          "original_form": "괜히 수박은 작은거 사면 손해인 기분이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100037103.1.9",
          "form": "큰거랑 가격이 비슷해서..",
          "original_form": "큰거랑 가격이 비슷해서..",
          "speaker_id": "1",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100037103.1.10",
          "form": "그니까요 근데 큰거사면 질려서 다 못먹고ㅋㅋ",
          "original_form": "그니까요 근데 큰거사면 질려서 다 못먹고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100037103.1.11",
          "form": "맞아요 ㅋㅋㅋ 그래서 저는 그냥 화채 해먹거나 그래요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 그래서 저는 그냥 화채 해먹거나 그래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037103.1.12",
          "form": "아 화채 좋죠ㅎㅎ 우유랑 사이다 부어서ㅎㅎ",
          "original_form": "아 화채 좋죠ㅎㅎ 우유랑 사이다 부어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100037103.1.13",
          "form": "맞아요 ㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100037103.1.14",
          "form": "배가 금방부른게 단점이죠 ㅋㅋ",
          "original_form": "배가 금방부른게 단점이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100037103.1.15",
          "form": "예전에 한창 친구들하고 술 많이 마실땐 항상 마지막 코스로 화채 주문해서 먹고 술자리 마무리 했었어요ㅋㅋ",
          "original_form": "예전에 한창 친구들하고 술 많이 마실땐 항상 마지막 코스로 화채 주문해서 먹고 술자리 마무리 했었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100037103.1.16",
          "form": "아 헐 저두옄ㅋㅋㅋ",
          "original_form": "아 헐 저두옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:01"
        },
        {
          "id": "MDRW2100037103.1.17",
          "form": "아니면 황도도 시켰었네요 ㅋㅋㅋ",
          "original_form": "아니면 황도도 시켰었네요 ㅋㅋㅋ",
