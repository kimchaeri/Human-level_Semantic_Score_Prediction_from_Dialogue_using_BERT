      "utterance": [
        {
          "id": "MDRW2100015200.1.1",
          "form": "전 오늘 야끼소바 먹었어요",
          "original_form": "전 오늘 야끼소바 먹었어요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015200.1.2",
          "form": "점심에요??",
          "original_form": "점심에요??",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015200.1.3",
          "form": "저는 우동먹었어요",
          "original_form": "저는 우동먹었어요",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015200.1.4",
          "form": "아니다 야끼우동 저녁에요",
          "original_form": "아니다 야끼우동 저녁에요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015200.1.5",
          "form": "야끼우동 만들줄 아세요?",
          "original_form": "야끼우동 만들줄 아세요?",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015200.1.6",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015200.1.7",
          "form": "유투브 선생님보면 다 가능해요.",
          "original_form": "유투브 선생님보면 다 가능해요.",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015200.1.8",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015200.1.9",
          "form": "야끼우동 저 오늘 먹었거든요",
          "original_form": "야끼우동 저 오늘 먹었거든요",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015200.1.10",
          "form": "네. 그런데요??",
          "original_form": "네. 그런데요??",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015200.1.11",
          "form": "야끼우동이 되게 대중적인 맛이 나고 맛있었어요",
          "original_form": "야끼우동이 되게 대중적인 맛이 나고 맛있었어요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015200.1.12",
          "form": "오.. 거기다가 우스터소스랑 후추, 넛맥, 간장, 굴소스, 설탕 만 넣으면 게임이 끝나더라고요",
          "original_form": "오.. 거기다가 우스터소스랑 후추, 넛맥, 간장, 굴소스, 설탕 만 넣으면 게임이 끝나더라고요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015200.1.13",
          "form": "넛맥 어때요?",
          "original_form": "넛맥 어때요?",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015200.1.14",
          "form": "시나몬같이 좀 특이한냄새가나요",
          "original_form": "시나몬같이 좀 특이한냄새가나요",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015200.1.15",
          "form": "와 신기하내요",
          "original_form": "와 신기하내요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015200.1.16",
          "form": "네. 넛맥들어간 모든요리는 다 맛잇어요",
          "original_form": "네. 넛맥들어간 모든요리는 다 맛잇어요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015200.1.17",
          "form": "넛맥은 향신료죠?",
          "original_form": "넛맥은 향신료죠?",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015200.1.18",
          "form": "네. 크림소스랑 정말 잘어울려요",
          "original_form": "네. 크림소스랑 정말 잘어울려요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015200.1.19",
          "form": "넛맥과 크림소스 어떤 요리인가요?",
          "original_form": "넛맥과 크림소스 어떤 요리인가요?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015200.1.20",
          "form": "크림파스타에다가 넛맥을 넣으면 갑자기 드라마틱한 효과가 나게되요",
          "original_form": "크림파스타에다가 넛맥을 넣으면 갑자기 드라마틱한 효과가 나게되요",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015200.1.21",
          "form": "저도 넛맥한번 사봐야겠네요",
          "original_form": "저도 넛맥한번 사봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 22:37"
        }
      ]
