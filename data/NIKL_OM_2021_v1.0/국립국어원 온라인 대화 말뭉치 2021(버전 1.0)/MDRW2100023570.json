      "utterance": [
        {
          "id": "MDRW2100023570.1.1",
          "form": "언니 아침 뭐 드셨나요??",
          "original_form": "언니 아침 뭐 드셨나요??",
          "speaker_id": "1",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023570.1.2",
          "form": "걍 어제먹다 남은거",
          "original_form": "걍 어제먹다 남은거",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023570.1.3",
          "form": "밥 조금이랑 고등어 3분의 1토막",
          "original_form": "밥 조금이랑 고등어 3분의 1토막",
          "speaker_id": "2",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100023570.1.4",
          "form": "그럼 지금 꽤 배고프시겠어요!!",
          "original_form": "그럼 지금 꽤 배고프시겠어요!!",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023570.1.5",
          "form": "저는 아침에",
          "original_form": "저는 아침에",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023570.1.6",
          "form": "켈로그 두 그릇에",
          "original_form": "켈로그 두 그릇에",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023570.1.7",
          "form": "아빠가 만들어준 정체불명의 주스를 마시고 와서",
          "original_form": "아빠가 만들어준 정체불명의 주스를 마시고 와서",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023570.1.8",
          "form": "아직 배가 안고프답니다,,",
          "original_form": "아직 배가 안고프답니다,,",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023570.1.9",
          "form": "전 지금 배고파요",
          "original_form": "전 지금 배고파요",
          "speaker_id": "2",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100023570.1.10",
          "form": "걍 감자랑 양배추랑 소시지넣고 끓인거 반찬으로 먹을거에요",
          "original_form": "걍 감자랑 양배추랑 소시지넣고 끓인거 반찬으로 먹을거에요",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100023570.1.11",
          "form": "이것도 어제 먹다 남은건데",
          "original_form": "이것도 어제 먹다 남은건데",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100023570.1.12",
          "form": "좀 많이 남은...ㅎ",
          "original_form": "좀 많이 남은...ㅎ",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023570.1.13",
          "form": "우와 역시 언니는",
          "original_form": "우와 역시 언니는",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.14",
          "form": "아이디어 뿜뿜하는 요리를",
          "original_form": "아이디어 뿜뿜하는 요리를",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.15",
          "form": "잘 만드시네요!",
          "original_form": "잘 만드시네요!",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.16",
          "form": "전에 먹었던 춘장이 정말 맛났어요~~~",
          "original_form": "전에 먹었던 춘장이 정말 맛났어요~~~",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.17",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.18",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.19",
          "form": "춘장 -> 쌈장",
          "original_form": "춘장 -> 쌈장",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.20",
          "form": "춘장이요?",
          "original_form": "춘장이요?",
          "speaker_id": "2",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.23",
          "form": "사실 다 있는 요리에요",
          "original_form": "사실 다 있는 요리에요",
          "speaker_id": "2",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023570.1.24",
          "form": "제가 개발한건 아니고ㅋㅋ",
          "original_form": "제가 개발한건 아니고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023570.1.25",
          "form": "그렇군요...",
          "original_form": "그렇군요...",
          "speaker_id": "1",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023570.1.26",
          "form": "엄마땜에 만들었는데 조금 드시고 마네요",
          "original_form": "엄마땜에 만들었는데 조금 드시고 마네요",
          "speaker_id": "2",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023570.1.27",
          "form": "우주만큼 광활한 요리의 세계",
          "original_form": "우주만큼 광활한 요리의 세계",
          "speaker_id": "1",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023570.1.28",
          "form": "아이고 ㅠ",
          "original_form": "아이고 ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023570.1.29",
          "form": "나머지는 다 내몫",
          "original_form": "나머지는 다 내몫",
          "speaker_id": "2",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023570.1.30",
          "form": "그래도 언니의 정성을 어머니가",
          "original_form": "그래도 언니의 정성을 어머니가",
          "speaker_id": "1",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023570.1.31",
          "form": "다 아실거예요!!!",
          "original_form": "다 아실거예요!!!",
          "speaker_id": "1",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023570.1.32",
          "form": "name1은 점심 뭐 드실거에요?",
          "original_form": "&name1&은 점심 뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210830 11:51"
        },
        {
          "id": "MDRW2100023570.1.33",
          "form": "저는,,,",
          "original_form": "저는,,,",
          "speaker_id": "1",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100023570.1.34",
          "form": "흰 쌀밥 위에",
          "original_form": "흰 쌀밥 위에",
          "speaker_id": "1",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100023570.1.35",
          "form": "치즈를 얹어서,,",
          "original_form": "치즈를 얹어서,,",
          "speaker_id": "1",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100023570.1.36",
          "form": "겉절이랑 먹을거랍니다,,",
          "original_form": "겉절이랑 먹을거랍니다,,",
          "speaker_id": "1",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100023570.1.37",
          "form": "상상이 안가는 조합이네요",
          "original_form": "상상이 안가는 조합이네요",
          "speaker_id": "2",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100023570.1.38",
          "form": "ㄴ(ㅇ0ㅇ)ㄱ",
          "original_form": "ㄴ(ㅇ0ㅇ)ㄱ",
          "speaker_id": "1",
          "time": "20210830 11:53"
        },
        {
          "id": "MDRW2100023570.1.39",
          "form": "상상도 못한 조합!",
          "original_form": "상상도 못한 조합!",
          "speaker_id": "1",
          "time": "20210830 11:53"
        },
        {
          "id": "MDRW2100023570.1.40",
          "form": "그래도 맛있으면 됐죠",
          "original_form": "그래도 맛있으면 됐죠",
          "speaker_id": "2",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100023570.1.41",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100023570.1.42",
          "form": "점심 언제 드실거예요?",
          "original_form": "점심 언제 드실거예요?",
          "speaker_id": "1",
          "time": "20210830 11:58"
        },
