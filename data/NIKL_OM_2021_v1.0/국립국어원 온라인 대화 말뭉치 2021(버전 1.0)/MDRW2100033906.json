      "utterance": [
        {
          "id": "MDRW2100033906.1.1",
          "form": "퇴근하느라 이제 봤네요 \r\n사고 싶으신 거 있으신가요?",
          "original_form": "퇴근하느라 이제 봤네요 \r\n사고 싶으신 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100033906.1.2",
          "form": "냉장고 사고 싶어요 ㅎㅎ",
          "original_form": "냉장고 사고 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100033906.1.3",
          "form": "요새 냉장고가 오래 되서 바꾸고 싶어요",
          "original_form": "요새 냉장고가 오래 되서 바꾸고 싶어요",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100033906.1.4",
          "form": "집에는 센터갔다 가셨어요?",
          "original_form": "집에는 센터갔다 가셨어요?",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100033906.1.5",
          "form": "네 다녀왔어요!! 요번에 수익으로 냉장고까지 다 바꾸시길 바라요!!",
          "original_form": "네 다녀왔어요!! 요번에 수익으로 냉장고까지 다 바꾸시길 바라요!!",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100033906.1.6",
          "form": "저는 이번에 남편이랑 스마트 워치 한개씩 질렀어요 ㅋㅋ",
          "original_form": "저는 이번에 남편이랑 스마트 워치 한개씩 질렀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100033906.1.7",
          "form": "주식 이렇게 폭락할 줄모르고 돈벌었다고 샀는데 ㅋㅋㅋ \r\n이렇게 과소비를 해버렸네요",
          "original_form": "주식 이렇게 폭락할 줄모르고 돈벌었다고 샀는데 ㅋㅋㅋ \r\n이렇게 과소비를 해버렸네요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100033906.1.8",
          "form": "그러긴 힘들죠 ㅋㅌ",
          "original_form": "그러긴 힘들죠 ㅋㅌ",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100033906.1.9",
          "form": "아 저도 미리 질렀다 폭락한적 있어요",
          "original_form": "아 저도 미리 질렀다 폭락한적 있어요",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100033906.1.10",
          "form": "할부로 했네요 애들 컴요 ㅜㅜ",
          "original_form": "할부로 했네요 애들 컴요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100033906.1.11",
          "form": "ㅋㅋㅋㅋ 돈을 다 빼고 샀어야 했는데.... 흑",
          "original_form": "ㅋㅋㅋㅋ 돈을 다 빼고 샀어야 했는데.... 흑",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100033906.1.12",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100033906.1.13",
          "form": "쇼핑할 땐 늘 좋은데.. 뒤에 남는 돈이 있죠...\r\n할부...",
          "original_form": "쇼핑할 땐 늘 좋은데.. 뒤에 남는 돈이 있죠...\r\n할부...",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100033906.1.14",
          "form": "한치엎을 몰라요",
          "original_form": "한치엎을 몰라요",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100033906.1.15",
          "form": "앞을",
          "original_form": "앞을",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100033906.1.16",
          "form": "맞아요 느는것은 할부죠",
          "original_form": "맞아요 느는것은 할부죠",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100033906.1.17",
          "form": "아 카드롤 뽀샤버려야 해요 ㅋㅋ",
          "original_form": "아 카드롤 뽀샤버려야 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100033906.1.18",
          "form": "그쵸 ㅋㅋ 나눠서 내니 뭔가 싸게 사는 것 같으면서도...",
          "original_form": "그쵸 ㅋㅋ 나눠서 내니 뭔가 싸게 사는 것 같으면서도...",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100033906.1.19",
          "form": "한꺼번에 할부와 할부와 할부 누적되니 ㅋㅋㅋ",
          "original_form": "한꺼번에 할부와 할부와 할부 누적되니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033906.1.20",
          "form": "금액이.. 어마어마..",
          "original_form": "금액이.. 어마어마..",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033906.1.21",
          "form": "오늘은 택배가 산더미더라구요..",
          "original_form": "오늘은 택배가 산더미더라구요..",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033906.1.22",
          "form": "쌀부터 시작해서 뭘 그래 많이 샀는지...",
          "original_form": "쌀부터 시작해서 뭘 그래 많이 샀는지...",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033906.1.23",
          "form": "모이면 목돈이고 매달나가고 ㅜㅜ",
          "original_form": "모이면 목돈이고 매달나가고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033906.1.24",
          "form": "저도 어제 ㅋㅋ",
          "original_form": "저도 어제 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033906.1.25",
          "form": "택배 아저씨 저 욕할것 같아요",
          "original_form": "택배 아저씨 저 욕할것 같아요",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033906.1.26",
          "form": "다들.. 그런거죠???!!",
          "original_form": "다들.. 그런거죠???!!",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033906.1.27",
          "form": "어제 좀 많이 왔어요",
          "original_form": "어제 좀 많이 왔어요",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033906.1.28",
          "form": "그럼요 ㅋㅋ",
          "original_form": "그럼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033906.1.29",
          "form": "저도.. 그래요... ㅋㅋㅋㅋㅋ특히 주말지나고 화요일은...",
          "original_form": "저도.. 그래요... ㅋㅋㅋㅋㅋ특히 주말지나고 화요일은...",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033906.1.30",
          "form": "저만 그러겠어요 ㅋㅋ",
          "original_form": "저만 그러겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033906.1.31",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100033906.1.32",
          "form": "모이고 쌓였던 택배들이....ㅋㅋㅋ",
          "original_form": "모이고 쌓였던 택배들이....ㅋㅋㅋ",
