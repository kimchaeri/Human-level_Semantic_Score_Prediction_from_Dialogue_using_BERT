      "utterance": [
        {
          "id": "MDRW2100029623.1.1",
          "form": "여행가고싶은 날씨네요",
          "original_form": "여행가고싶은 날씨네요",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029623.1.2",
          "form": "그러게요 선선하고 좋네요",
          "original_form": "그러게요 선선하고 좋네요",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029623.1.3",
          "form": "가볍게 드라이브 추천드려요",
          "original_form": "가볍게 드라이브 추천드려요",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029623.1.4",
          "form": "그러고싶은데 회사네요ㅋ",
          "original_form": "그러고싶은데 회사네요ㅋ",
          "speaker_id": "1",
          "time": "20210903 11:19"
        },
        {
          "id": "MDRW2100029623.1.5",
          "form": "휴가 때 어디 다녀오셨어요?",
          "original_form": "휴가 때 어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210903 11:19"
        },
        {
          "id": "MDRW2100029623.1.6",
          "form": "저는 부산 해운대요",
          "original_form": "저는 부산 해운대요",
          "speaker_id": "2",
          "time": "20210903 11:19"
        },
        {
          "id": "MDRW2100029623.1.7",
          "form": "숙소에만 머물렀어요",
          "original_form": "숙소에만 머물렀어요",
          "speaker_id": "2",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029623.1.8",
          "form": "name1님은 어디 다녀오셨어요?",
          "original_form": "&name1&님은 어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029623.1.9",
          "form": "오 좋네요~ 저는 다음주에 서울 가요ㅋㅋㅋ",
          "original_form": "오 좋네요~ 저는 다음주에 서울 가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029623.1.10",
          "form": "저도 그냥 호캉스..",
          "original_form": "저도 그냥 호캉스..",
          "speaker_id": "1",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029623.1.11",
          "form": "사실 여행을 가고싶은데",
          "original_form": "사실 여행을 가고싶은데",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029623.1.12",
          "form": "호캉스 좋더라고요",
          "original_form": "호캉스 좋더라고요",
          "speaker_id": "2",
          "time": "20210903 12:06"
        },
        {
          "id": "MDRW2100029623.1.13",
          "form": "누구랑 가실 예정이에요?",
          "original_form": "누구랑 가실 예정이에요?",
          "speaker_id": "2",
          "time": "20210903 12:06"
        },
        {
          "id": "MDRW2100029623.1.14",
          "form": "저는 남자친구랑요ㅋㅋㅋㅋ",
          "original_form": "저는 남자친구랑요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029623.1.15",
          "form": "여행 갈 거 몰빵해서 좋은 호텔 가기로했어요",
          "original_form": "여행 갈 거 몰빵해서 좋은 호텔 가기로했어요",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029623.1.16",
          "form": "좋겠다 여행은 언제나 좋죠",
          "original_form": "좋겠다 여행은 언제나 좋죠",
          "speaker_id": "2",
          "time": "20210903 12:16"
        },
        {
          "id": "MDRW2100029623.1.17",
          "form": "저도 호캉스 또가고 싶네요",
          "original_form": "저도 호캉스 또가고 싶네요",
          "speaker_id": "2",
          "time": "20210903 12:17"
        },
        {
          "id": "MDRW2100029623.1.18",
          "form": "근데 사실 호캉스보다는 여행을 가고싶어요 ㅋㅋ",
          "original_form": "근데 사실 호캉스보다는 여행을 가고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100029623.1.19",
          "form": "계획하고 계신 여행지 있어요?",
          "original_form": "계획하고 계신 여행지 있어요?",
          "speaker_id": "2",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100029623.1.20",
          "form": "우선은 울릉도여!! 독도도같이",
          "original_form": "우선은 울릉도여!! 독도도같이",
          "speaker_id": "1",
          "time": "20210903 13:20"
        },
        {
          "id": "MDRW2100029623.1.21",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "2",
          "time": "20210903 13:22"
        },
        {
          "id": "MDRW2100029623.1.22",
          "form": "독도는 허가받은 경우만 갈 수 있는 걸로 알고있어요",
          "original_form": "독도는 허가받은 경우만 갈 수 있는 걸로 알고있어요",
          "speaker_id": "2",
          "time": "20210903 13:22"
        },
        {
          "id": "MDRW2100029623.1.23",
          "form": "보니까 날 좋을 때 갈 수 있대요 ㅋㅋ 여행코스로",
          "original_form": "보니까 날 좋을 때 갈 수 있대요 ㅋㅋ 여행코스로",
