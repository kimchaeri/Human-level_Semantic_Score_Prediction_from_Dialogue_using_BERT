      "utterance": [
        {
          "id": "MDRW2100009261.1.1",
          "form": "영화자주보세요?",
          "original_form": "영화자주보세요?",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009261.1.2",
          "form": "음 아뇨 ㅜㅜㅋ",
          "original_form": "음 아뇨 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009261.1.3",
          "form": "좋아는 하는데 간지가 어언 ㅎㅎㅎ",
          "original_form": "좋아는 하는데 간지가 어언 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009261.1.4",
          "form": "집에선 자주 안보게 되네요",
          "original_form": "집에선 자주 안보게 되네요",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009261.1.5",
          "form": "집에서 보시면되죠ㅎ",
          "original_form": "집에서 보시면되죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009261.1.6",
          "form": "내 인생에서 최애 영화는?",
          "original_form": "내 인생에서 최애 영화는?",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009261.1.7",
          "form": "음 저는 인생은아름다워 요",
          "original_form": "음 저는 인생은아름다워 요",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009261.1.8",
          "form": "ㅎㅎ 좋죠~^^",
          "original_form": "ㅎㅎ 좋죠~^^",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009261.1.9",
          "form": "어떤 장면이 가장기억에 남아요?",
          "original_form": "어떤 장면이 가장기억에 남아요?",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009261.1.10",
          "form": "음 아이를 위해서 아빠 희생하던 장면이요",
          "original_form": "음 아이를 위해서 아빠 희생하던 장면이요",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009261.1.11",
          "form": "아름다운 장면이죠ㅎ",
          "original_form": "아름다운 장면이죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009261.1.12",
          "form": "영화는 무조건 감동이 있어야해요ㅎ",
          "original_form": "영화는 무조건 감동이 있어야해요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009261.1.13",
          "form": "이건 선생님이 추천해서 본거였는데 ㅎㅎ",
          "original_form": "이건 선생님이 추천해서 본거였는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009261.1.14",
          "form": "코믹도 감동ㅎ 멜로도 감동ㅎ 그리고 액션도 감동ㅎㅎ",
          "original_form": "코믹도 감동ㅎ 멜로도 감동ㅎ 그리고 액션도 감동ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009261.1.15",
          "form": "명작은 언제봐도 좋죠 ㅡㅎ",
          "original_form": "명작은 언제봐도 좋죠 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009261.1.16",
          "form": "그런거 같아요 ㅎ.ㅎ",
          "original_form": "그런거 같아요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009261.1.17",
          "form": "요즘은 그나마 찾아보는게",
          "original_form": "요즘은 그나마 찾아보는게",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009261.1.18",
          "form": "바라는바다",
          "original_form": "바라는바다",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009261.1.19",
          "form": "힐링하기 딱좋은 ㅎㅎ",
          "original_form": "힐링하기 딱좋은 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009261.1.20",
          "form": "그 영화는 제가 잘몰라서ㅎㅎ 스토리라인이 어떻게되요?ㅎ",
          "original_form": "그 영화는 제가 잘몰라서ㅎㅎ 스토리라인이 어떻게되요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009261.1.21",
          "form": "너무 제가 name1님에게 물어만 보네요ㅎㅎㅎㅎ",
          "original_form": "너무 제가 &name1&님에게 물어만 보네요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009261.1.22",
          "form": "궁금해서 그러니 이해해주시길ㅎㅎ",
          "original_form": "궁금해서 그러니 이해해주시길ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009261.1.23",
          "form": "아뇨 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009261.1.24",
          "form": "ㅎㅎ 괜찮아요 ~~",
          "original_form": "ㅎㅎ 괜찮아요 ~~",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009261.1.25",
          "form": "영화는 아니고 티비 프로그램인데",
          "original_form": "영화는 아니고 티비 프로그램인데",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009261.1.26",
          "form": "아 티비요ㅎ",
          "original_form": "아 티비요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009261.1.27",
          "form": "배우랑 가수들이 카페 운영하면서 노래 들려주는 프로그램이더라구요",
          "original_form": "배우랑 가수들이 카페 운영하면서 노래 들려주는 프로그램이더라구요",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009261.1.28",
          "form": "어쩐지 지금 찾아보는데 없더라구요ㅎ",
          "original_form": "어쩐지 지금 찾아보는데 없더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009261.1.29",
          "form": "노래를 좋아하시나봐요?",
          "original_form": "노래를 좋아하시나봐요?",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009261.1.30",
          "form": "예전에  비긴어게인(tv프로) 비슷해요",
          "original_form": "예전에  비긴어게인(tv프로) 비슷해요",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009261.1.31",
          "form": "마음이  편안해져셔요 ㄹㄹ",
          "original_form": "마음이  편안해져셔요 ㄹㄹ",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009261.1.32",
          "form": "빙고ㅎ",
          "original_form": "빙고ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009261.1.33",
          "form": "저 그말하려고했어요ㅎ 비긴어게인ㅎ",
          "original_form": "저 그말하려고했어요ㅎ 비긴어게인ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009261.1.34",
