      "utterance": [
        {
          "id": "MDRW2100037780.1.1",
          "form": "불고기 밀키트 먹어봤어요",
          "original_form": "불고기 밀키트 먹어봤어요",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037780.1.2",
          "form": "오 구성 잘돼있나요?",
          "original_form": "오 구성 잘돼있나요?",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037780.1.3",
          "form": "딱히 모르겠어요 포장은 예쁘고",
          "original_form": "딱히 모르겠어요 포장은 예쁘고",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037780.1.4",
          "form": "해먹을 수 있으면 그냥 해먹는게 나아요",
          "original_form": "해먹을 수 있으면 그냥 해먹는게 나아요",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037780.1.5",
          "form": "아 ㅎㅎ 그럼 해먹어야겠군요 ㅋㅋㅋ",
          "original_form": "아 ㅎㅎ 그럼 해먹어야겠군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037780.1.6",
          "form": "전 파스타는 맛있었어요",
          "original_form": "전 파스타는 맛있었어요",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037780.1.7",
          "form": "유명맛집 밀키트라는데.그냥저냥 ㅎㅎ",
          "original_form": "유명맛집 밀키트라는데.그냥저냥 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037780.1.8",
          "form": "어디꺼사셨어요?",
          "original_form": "어디꺼사셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037780.1.9",
          "form": "저 프레시지 밀키트요",
          "original_form": "저 프레시지 밀키트요",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037780.1.10",
          "form": "그렇구나 밀키트시장이 엄청 성장한대요",
          "original_form": "그렇구나 밀키트시장이 엄청 성장한대요",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037780.1.11",
          "form": "다들 외식을 못해서 그런가봐요 !",
          "original_form": "다들 외식을 못해서 그런가봐요 !",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037780.1.12",
          "form": "원래 외식 좋아했는데 ㅠㅜ",
          "original_form": "원래 외식 좋아했는데 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037780.1.13",
          "form": "그런가봐요 집에 많이있으니까요 ㅜㅜ",
          "original_form": "그런가봐요 집에 많이있으니까요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100037780.1.14",
          "form": "맛집 가는거 좋아하셨어요?",
          "original_form": "맛집 가는거 좋아하셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037780.1.15",
          "form": "네 맛있는거 먹으면 좋죠 ㅎㅎ",
          "original_form": "네 맛있는거 먹으면 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037780.1.16",
          "form": "추천하는 맛집 있을까요 ㅎㅎ",
          "original_form": "추천하는 맛집 있을까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:37"
        }
      ]
