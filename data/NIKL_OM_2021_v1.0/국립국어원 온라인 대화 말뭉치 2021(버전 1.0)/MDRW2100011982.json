      "utterance": [
        {
          "id": "MDRW2100011982.1.1",
          "form": "초밥좋아하세요",
          "original_form": "초밥좋아하세요",
          "speaker_id": "2",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011982.1.2",
          "form": "ㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011982.1.3",
          "form": "초밥 원래 못먹는데 요즘 갑자기 꽂혀서",
          "original_form": "초밥 원래 못먹는데 요즘 갑자기 꽂혀서",
          "speaker_id": "1",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011982.1.4",
          "form": "조금씩 먹고있어요ㅎ",
          "original_form": "조금씩 먹고있어요ㅎ",
          "speaker_id": "1",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011982.1.5",
          "form": "앗 그렇게 좋아하시는 편은 아니시구나",
          "original_form": "앗 그렇게 좋아하시는 편은 아니시구나",
          "speaker_id": "2",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011982.1.6",
          "form": "연어초밥은 좋아해요!",
          "original_form": "연어초밥은 좋아해요!",
          "speaker_id": "1",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011982.1.7",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.8",
          "form": "연어",
          "original_form": "연어",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.9",
          "form": "그것은 기름진식감",
          "original_form": "그것은 기름진식감",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.10",
          "form": "크으으",
          "original_form": "크으으",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.11",
          "form": "맛있는 맛",
          "original_form": "맛있는 맛",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.12",
          "form": "색이 참 고와요..",
          "original_form": "색이 참 고와요..",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.13",
          "form": "싫어할 수 가 없ㅛ",
          "original_form": "싫어할 수 가 없ㅛ",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.15",
          "form": "붉은 생선계의 화룡점정",
          "original_form": "붉은 생선계의 화룡점정",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.16",
          "form": "어떤 초밥 좋아하세요?",
          "original_form": "어떤 초밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.17",
          "form": "저는 새우요",
          "original_form": "저는 새우요",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.18",
          "form": "새우너무 맛있어",
          "original_form": "새우너무 맛있어",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.19",
          "form": "아하 새우도 맛있는것같아요",
          "original_form": "아하 새우도 맛있는것같아요",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.20",
          "form": "간장새우ㅜ!",
          "original_form": "간장새우ㅜ!",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011982.1.21",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011982.1.22",
          "form": "맛잘알",
          "original_form": "맛잘알",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011982.1.23",
          "form": "간장새우 겁나맛있잖아요",
          "original_form": "간장새우 겁나맛있잖아요",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011982.1.24",
          "form": "ㅋㅋㅋㅋ감사합니다",
          "original_form": "ㅋㅋㅋㅋ감사합니다",
          "speaker_id": "1",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011982.1.25",
          "form": "짭쪼름하고",
          "original_form": "짭쪼름하고",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011982.1.26",
          "form": "역시 배우신분",
          "original_form": "역시 배우신분",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011982.1.27",
          "form": "그쵸 저 원래 날것 잘못먹는데",
          "original_form": "그쵸 저 원래 날것 잘못먹는데",
