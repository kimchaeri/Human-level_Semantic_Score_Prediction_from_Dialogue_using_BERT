      "utterance": [
        {
          "id": "MDRW2100018156.1.1",
          "form": "냉동실 파먹고나서 사야해요",
          "original_form": "냉동실 파먹고나서 사야해요",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018156.1.2",
          "form": "사고싶은건 많고 공간은 항상 한정적이죠 ㅋㅋㅋㄱㅋ",
          "original_form": "사고싶은건 많고 공간은 항상 한정적이죠 ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018156.1.3",
          "form": "아 쟁이는 습관때문에ㅜ 물건도 가득사요",
          "original_form": "아 쟁이는 습관때문에ㅜ 물건도 가득사요",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018156.1.4",
          "form": "유비무환인가요!? ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "유비무환인가요!? ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018156.1.5",
          "form": "적당히 쟁여두는건 좋아요",
          "original_form": "적당히 쟁여두는건 좋아요",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018156.1.6",
          "form": "대식구니까 충분히 소비가능할거라 믿고",
          "original_form": "대식구니까 충분히 소비가능할거라 믿고",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018156.1.7",
          "form": "그러니까요 얼마전엔 대용량스킨... 내년까지 쓸듯싶어요",
          "original_form": "그러니까요 얼마전엔 대용량스킨... 내년까지 쓸듯싶어요",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018156.1.8",
          "form": "정말 많이 쟁이시나? ㅎㅎ 아끼지말고 퍽퍽 사용하셔야 할거예요",
          "original_form": "정말 많이 쟁이시나? ㅎㅎ 아끼지말고 퍽퍽 사용하셔야 할거예요",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018156.1.9",
          "form": "나눔하고싶네요",
          "original_form": "나눔하고싶네요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018156.1.10",
          "form": "당근을 이용해보시는건 어때요?",
          "original_form": "당근을 이용해보시는건 어때요?",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018156.1.11",
          "form": "저 대용량을 나눔하자니 제 몸에다 바르는것이... ㅋㅋ",
          "original_form": "저 대용량을 나눔하자니 제 몸에다 바르는것이... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018156.1.12",
          "form": "아 ㅋㅋㅋㅋㅋ 얼만한걸까요? ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ 얼만한걸까요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018156.1.13",
          "form": "브랜드를 알려주세요",
          "original_form": "브랜드를 알려주세요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018156.1.14",
          "form": "무명의 브랜드라... 패트병만해요",
          "original_form": "무명의 브랜드라... 패트병만해요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018156.1.15",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018156.1.16",
          "form": "나누는것도 일이겠네요",
          "original_form": "나누는것도 일이겠네요",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018156.1.17",
          "form": "ㅋㅋ 집이 가깝다면 드리고싶네요ㅋㅋ",
          "original_form": "ㅋㅋ 집이 가깝다면 드리고싶네요ㅋㅋ",
