      "utterance": [
        {
          "id": "MDRW2100018464.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 02:18"
        },
        {
          "id": "MDRW2100018464.1.2",
          "form": "네 게임 좋아해요",
          "original_form": "네 게임 좋아해요",
          "speaker_id": "2",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018464.1.3",
          "form": "무슨 게임하세요?",
          "original_form": "무슨 게임하세요?",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018464.1.4",
          "form": "롤해요",
          "original_form": "롤해요",
          "speaker_id": "2",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018464.1.5",
          "form": "오래하셧어요?",
          "original_form": "오래하셧어요?",
          "speaker_id": "1",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018464.1.6",
          "form": "음 꽤 오래한거같네요",
          "original_form": "음 꽤 오래한거같네요",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018464.1.7",
          "form": "다양한겜 했었는데",
          "original_form": "다양한겜 했었는데",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018464.1.8",
          "form": "롤을 젤 오래했네요 ㅋㅋㅋ",
          "original_form": "롤을 젤 오래했네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018464.1.9",
          "form": "ㅋㅋㅋㅋ메이플도 하셧어요??",
          "original_form": "ㅋㅋㅋㅋ메이플도 하셧어요??",
          "speaker_id": "1",
          "time": "20210827 02:22"
        },
        {
          "id": "MDRW2100018464.1.10",
          "form": "네 ㅋㅋㅋ 메이플도 했었죠",
          "original_form": "네 ㅋㅋㅋ 메이플도 했었죠",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018464.1.11",
          "form": "올 누구키우셧어요",
          "original_form": "올 누구키우셧어요",
          "speaker_id": "1",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018464.1.12",
          "form": "에반 키웠었어요",
          "original_form": "에반 키웠었어요",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018464.1.13",
          "form": "신캐는 안키우셧어요??",
          "original_form": "신캐는 안키우셧어요??",
          "speaker_id": "1",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018464.1.14",
          "form": "신캐는 거의 안키우고",
          "original_form": "신캐는 거의 안키우고",
          "speaker_id": "2",
          "time": "20210827 02:25"
        },
        {
          "id": "MDRW2100018464.1.15",
          "form": "손이 잘 안가더라구여 ㅋㅋ",
          "original_form": "손이 잘 안가더라구여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018464.1.16",
          "form": "아핰ㅋㅋ 신캐는 이펙트거 화려해서 넘 예쁘던데..",
          "original_form": "아핰ㅋㅋ 신캐는 이펙트거 화려해서 넘 예쁘던데..",
          "speaker_id": "1",
          "time": "20210827 02:27"
        },
        {
          "id": "MDRW2100018464.1.17",
          "form": "ㅋㅋㅋ 요즘은 전부 이펙트가 화려해서",
          "original_form": "ㅋㅋㅋ 요즘은 전부 이펙트가 화려해서",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018464.1.18",
          "form": "에반도 나름 이펙트땜에",
          "original_form": "에반도 나름 이펙트땜에",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018464.1.19",
          "form": "키웠던거같아요",
          "original_form": "키웠던거같아요",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018464.1.20",
          "form": "롤은 안하시나요?",
          "original_form": "롤은 안하시나요?",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018464.1.21",
          "form": "ㅋㅋㅋ 네네 롤은 넘 어렵더라구요",
          "original_form": "ㅋㅋㅋ 네네 롤은 넘 어렵더라구요",
          "speaker_id": "1",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018464.1.22",
          "form": "그럼 요즘 자주하시는 게임이 메이플이신가요",
          "original_form": "그럼 요즘 자주하시는 게임이 메이플이신가요",
          "speaker_id": "2",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018464.1.23",
          "form": "어눀ㅋㅋ 전 pc겜은 잘 안해요",
          "original_form": "어눀ㅋㅋ 전 pc겜은 잘 안해요",
          "speaker_id": "1",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018464.1.24",
          "form": "모바일많이해요",
          "original_form": "모바일많이해요",
          "speaker_id": "1",
          "time": "20210827 02:30"
        }
      ]
