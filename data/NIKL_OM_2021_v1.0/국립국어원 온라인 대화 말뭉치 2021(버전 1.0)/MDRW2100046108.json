      "utterance": [
        {
          "id": "MDRW2100046108.1.1",
          "form": "저 커피 잘 못 마셨을 때 또",
          "original_form": "저 커피 잘 못 마셨을 때 또",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046108.1.2",
          "form": "녹차라떼 마셨어요",
          "original_form": "녹차라떼 마셨어요",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046108.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.4",
          "form": "민트 초코 좋아합니다",
          "original_form": "민트 초코 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.5",
          "form": "민초파에요",
          "original_form": "민초파에요",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.6",
          "form": "우아 ㅋㅋㅋ 아이스크림도",
          "original_form": "우아 ㅋㅋㅋ 아이스크림도",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.7",
          "form": "좋아하시죠",
          "original_form": "좋아하시죠",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.8",
          "form": "일반 아이스크림은 잘 안 먹는데",
          "original_form": "일반 아이스크림은 잘 안 먹는데",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.10",
          "form": "민초만 먹어요",
          "original_form": "민초만 먹어요",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.11",
          "form": "와 저는 반민초 ㅎㅎ",
          "original_form": "와 저는 반민초 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.12",
          "form": "아하 반민초 ㅎㅎ",
          "original_form": "아하 반민초 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.13",
          "form": "다른맛은 아에 안드세요?",
          "original_form": "다른맛은 아에 안드세요?",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046108.1.14",
          "form": "아예 안 먹는 건 아닌데",
          "original_form": "아예 안 먹는 건 아닌데",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046108.1.15",
          "form": "제가 고르진 않구요",
          "original_form": "제가 고르진 않구요",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046108.1.16",
          "form": "남이 골라오면 먹어요ㅎㅎㅎ",
          "original_form": "남이 골라오면 먹어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046108.1.17",
          "form": "오오 넼ㅋㅋㅋ 누가 골라주나용",
          "original_form": "오오 넼ㅋㅋㅋ 누가 골라주나용",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046108.1.18",
