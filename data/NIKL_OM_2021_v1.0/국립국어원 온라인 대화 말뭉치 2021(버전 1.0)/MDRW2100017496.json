      "utterance": [
        {
          "id": "MDRW2100017496.1.1",
          "form": "와오 온라인피트니스하세요?",
          "original_form": "와오 온라인피트니스하세요?",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017496.1.2",
          "form": "네이버 들어갈때마다 광고뜨길래",
          "original_form": "네이버 들어갈때마다 광고뜨길래",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017496.1.3",
          "form": "관심가져보긴 했어요ㅋㅋㅋ하진 않고",
          "original_form": "관심가져보긴 했어요ㅋㅋㅋ하진 않고",
          "speaker_id": "1",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017496.1.4",
          "form": "ㅋㅋ저는 실제 피티하다가 쉬는중인데",
          "original_form": "ㅋㅋ저는 실제 피티하다가 쉬는중인데",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017496.1.5",
          "form": "온라인피티는 안해봤어요",
          "original_form": "온라인피티는 안해봤어요",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017496.1.6",
          "form": "주변에 하는 사람은 없는데",
          "original_form": "주변에 하는 사람은 없는데",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017496.1.7",
          "form": "광고같은거 보면 많더라고요",
          "original_form": "광고같은거 보면 많더라고요",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017496.1.8",
          "form": "기구도 보내주고 관리도 잘 된다고하고..",
          "original_form": "기구도 보내주고 관리도 잘 된다고하고..",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017496.1.9",
          "form": "저 아는분은 그걸로 5키로 뺐댜구 하더라고요",
          "original_form": "저 아는분은 그걸로 5키로 뺐댜구 하더라고요",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017496.1.10",
          "form": "대박사건이었어요ㅎㅎ",
          "original_form": "대박사건이었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017496.1.11",
          "form": "오.... 효과가 있긴한가?",
          "original_form": "오.... 효과가 있긴한가?",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017496.1.12",
          "form": "근데 어떻게 보면 영상보고 따라하는거잖아요",
          "original_form": "근데 어떻게 보면 영상보고 따라하는거잖아요",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017496.1.13",
          "form": "그럼 유튜브운동영상이랑 크게 뭐가 다른건지..",
          "original_form": "그럼 유튜브운동영상이랑 크게 뭐가 다른건지..",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017496.1.14",
          "form": "안하는것보단 나은거같아요",
          "original_form": "안하는것보단 나은거같아요",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017496.1.15",
          "form": "영상찍어보낸대요",
          "original_form": "영상찍어보낸대요",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017496.1.16",
          "form": "아아... 그런거에 안 예민한 사람이 해야겠네요",
          "original_form": "아아... 그런거에 안 예민한 사람이 해야겠네요",
          "speaker_id": "1",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017496.1.17",
          "form": "맞아요 운동복도 괜히 신경쓰인대요",
          "original_form": "맞아요 운동복도 괜히 신경쓰인대요",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017496.1.18",
          "form": "아이디어는 진짜 대박 같아요",
          "original_form": "아이디어는 진짜 대박 같아요",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017496.1.19",
          "form": "단순 영상제공이 아니라 필요한 기구도 챙겨주고 오프라인처럼 피드백도 해주는 서비스라는게",
          "original_form": "단순 영상제공이 아니라 필요한 기구도 챙겨주고 오프라인처럼 피드백도 해주는 서비스라는게",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017496.1.20",
          "form": "ㅋㅋ 맞아요 그분은 클레스 101에서 하셨대요",
          "original_form": "ㅋㅋ 맞아요 그분은 클레스 101에서 하셨대요",
          "speaker_id": "2",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017496.1.21",
          "form": "결국은 본인이 어느정도 하느냐에 따르겠죠?",
          "original_form": "결국은 본인이 어느정도 하느냐에 따르겠죠?",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017496.1.22",
          "form": "혹하긴 하네요ㅋㅋㅋ",
          "original_form": "혹하긴 하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:15"
        }
      ]
