      "utterance": [
        {
          "id": "MDRW2100002110.1.1",
          "form": "정세랑 작가 책 읽어봤나",
          "original_form": "정세랑 작가 책 읽어봤나",
          "speaker_id": "1",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002110.1.2",
          "form": "정세랑작가 니가 좋아하는 분 아냐?",
          "original_form": "정세랑작가 니가 좋아하는 분 아냐?",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002110.1.3",
          "form": "통했네... 나는 니가 추천해 준 거 읽었지",
          "original_form": "통했네... 나는 니가 추천해 준 거 읽었지",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002110.1.4",
          "form": "어어 맞음 작가님 글이 너무 깔끔해",
          "original_form": "어어 맞음 작가님 글이 너무 깔끔해",
          "speaker_id": "1",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002110.1.5",
          "form": "지구에서 한아뿐",
          "original_form": "지구에서 한아뿐",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002110.1.6",
          "form": "맞아 술술 읽히긴 하더라",
          "original_form": "맞아 술술 읽히긴 하더라",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002110.1.7",
          "form": "그리고 한시간이면 독파할 수 있는 분량이고",
          "original_form": "그리고 한시간이면 독파할 수 있는 분량이고",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002110.1.8",
          "form": "그 책 샀나? 살거가?",
          "original_form": "그 책 샀나? 살거가?",
          "speaker_id": "2",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002110.1.9",
          "form": "아니 나는 빌려읽은책은 잘 안사서",
          "original_form": "아니 나는 빌려읽은책은 잘 안사서",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002110.1.10",
          "form": "닌 사서읽었나??",
          "original_form": "닌 사서읽었나??",
          "speaker_id": "1",
          "time": "20210819 23:41"
        },
        {
          "id": "MDRW2100002110.1.11",
          "form": "근데 작가님 인기가 많아져서 도서관에서 빌리기 힘들긴하더라",
          "original_form": "근데 작가님 인기가 많아져서 도서관에서 빌리기 힘들긴하더라",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002110.1.12",
          "form": "나도 이북으로 읽었지",
          "original_form": "나도 이북으로 읽었지",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002110.1.13",
          "form": "나는 책 엄청 맘에 들면 사서 소장하는데",
          "original_form": "나는 책 엄청 맘에 들면 사서 소장하는데",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002110.1.14",
          "form": "소장은 안하는갑지",
          "original_form": "소장은 안하는갑지",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002110.1.15",
          "form": "음 그정도는 아니라서?? ㅎ",
          "original_form": "음 그정도는 아니라서?? ㅎ",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002110.1.16",
          "form": "일단 로맨스라서 와닿는데 한계가 있는듯",
          "original_form": "일단 로맨스라서 와닿는데 한계가 있는듯",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002110.1.17",
          "form": "ㅌㅋㅋㅋㅋㅋ하긴 근데 뭔가 교훈이 남는 책은 아니고",
          "original_form": "ㅌㅋㅋㅋㅋㅋ하긴 근데 뭔가 교훈이 남는 책은 아니고",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002110.1.18",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002110.1.19",
          "form": "사실 사서 소장할 만한 책은..뭐가 있지 잘 모르겠다",
          "original_form": "사실 사서 소장할 만한 책은..뭐가 있지 잘 모르겠다",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002110.1.20",
          "form": "공부하는 책 말고는 모르겠네",
          "original_form": "공부하는 책 말고는 모르겠네",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002110.1.21",
          "form": "맞아 그 기준이 진짜 애매해",
          "original_form": "맞아 그 기준이 진짜 애매해",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002110.1.22",
          "form": "참고서는...돈아까워...",
          "original_form": "참고서는...돈아까워...",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002110.1.23",
          "form": "나는 근데 주로 책을 사서봐서",
          "original_form": "나는 근데 주로 책을 사서봐서",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002110.1.24",
          "form": "꼭필요한데 돈아까워 ㅠ",
          "original_form": "꼭필요한데 돈아까워 ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002110.1.25",
          "form": "종이책이 최고야",
          "original_form": "종이책이 최고야",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002110.1.26",
          "form": "그건그래 이북은 못따라오지",
          "original_form": "그건그래 이북은 못따라오지",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002110.1.27",
          "form": "맞아ㅠ 참고서 매일 개정되는 것도 싫어",
          "original_form": "맞아ㅠ 참고서 매일 개정되는 것도 싫어",
          "speaker_id": "2",
          "time": "20210819 23:43"
        }
      ]
    }
  ]
