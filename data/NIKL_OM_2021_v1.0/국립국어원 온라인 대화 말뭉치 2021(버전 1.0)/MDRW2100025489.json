        {
          "id": "MDRW2100025489.1.1",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100025489.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100025489.1.3",
          "form": "요즘 에어로빅 유튜브로 보면서",
          "original_form": "요즘 에어로빅 유튜브로 보면서",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100025489.1.4",
          "form": "조금씩 연습중이에요!",
          "original_form": "조금씩 연습중이에요!",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100025489.1.5",
          "form": "오 힘들죠?",
          "original_form": "오 힘들죠?",
          "speaker_id": "1",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100025489.1.6",
          "form": "네 힘들드라고요 ㅎㅎ",
          "original_form": "네 힘들드라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 12:10"
        },
        {
          "id": "MDRW2100025489.1.7",
          "form": "전 홈트요",
          "original_form": "전 홈트요",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100025489.1.8",
          "form": "홈트도 좋죠!",
          "original_form": "홈트도 좋죠!",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025489.1.9",
          "form": "플랭크해요",
          "original_form": "플랭크해요",
          "speaker_id": "1",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025489.1.10",
          "form": "플랭크 힘들지 않으세요?",
          "original_form": "플랭크 힘들지 않으세요?",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025489.1.11",
          "form": "1분 이상 넘기기 좀 힘들더라거오",
          "original_form": "1분 이상 넘기기 좀 힘들더라거오",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025489.1.12",
          "form": "3세트해요",
          "original_form": "3세트해요",
          "speaker_id": "1",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100025489.1.13",
          "form": "오 대박이네요",
          "original_form": "오 대박이네요",
          "speaker_id": "2",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025489.1.14",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "1",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100025489.1.15",
          "form": "그래도 3세트나 하시다니 대단해요!",
          "original_form": "그래도 3세트나 하시다니 대단해요!",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
