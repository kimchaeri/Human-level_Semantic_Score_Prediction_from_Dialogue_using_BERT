      "utterance": [
        {
          "id": "MDRW2100003588.1.1",
          "form": "여름휴가는 다녀오셨나요?",
          "original_form": "여름휴가는 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210821 02:55"
        },
        {
          "id": "MDRW2100003588.1.2",
          "form": "저는 내일 강릉으로 가요!",
          "original_form": "저는 내일 강릉으로 가요!",
          "speaker_id": "2",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003588.1.3",
          "form": "좀 늦은 여름휴가로요 ㅋㅋㅎㅎ",
          "original_form": "좀 늦은 여름휴가로요 ㅋㅋㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003588.1.4",
          "form": "우와ㅎ 좋겠어요",
          "original_form": "우와ㅎ 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003588.1.5",
          "form": "리프레시 하고 올려구요",
          "original_form": "리프레시 하고 올려구요",
          "speaker_id": "2",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003588.1.6",
          "form": "날씨가 선선해서 바다는 못들어가겠네요",
          "original_form": "날씨가 선선해서 바다는 못들어가겠네요",
          "speaker_id": "1",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003588.1.7",
          "form": "아 그런가요? 수영복은 어쩐담..",
          "original_form": "아 그런가요? 수영복은 어쩐담..",
          "speaker_id": "2",
          "time": "20210821 02:56"
        },
        {
          "id": "MDRW2100003588.1.8",
          "form": "친구분이랑 가시는거예요?",
          "original_form": "친구분이랑 가시는거예요?",
          "speaker_id": "1",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003588.1.9",
          "form": "네네 친구들끼리 가요!",
          "original_form": "네네 친구들끼리 가요!",
          "speaker_id": "2",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003588.1.10",
          "form": "해수욕 가능할까요?ㅋ",
          "original_form": "해수욕 가능할까요?ㅋ",
          "speaker_id": "1",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003588.1.11",
          "form": "안될거같아요 ㅋㅋㅋ",
          "original_form": "안될거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003588.1.12",
          "form": "생각해보니 거의 9월인데",
          "original_form": "생각해보니 거의 9월인데",
          "speaker_id": "2",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003588.1.13",
          "form": "바닷물이 차갑겠네요 ㅋㅋㅋ!!",
          "original_form": "바닷물이 차갑겠네요 ㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210821 02:57"
        },
        {
          "id": "MDRW2100003588.1.14",
          "form": "입추지나서 갑자기 선선해져가지고 강릉바다도 차가울듯요",
          "original_form": "입추지나서 갑자기 선선해져가지고 강릉바다도 차가울듯요",
          "speaker_id": "1",
          "time": "20210821 02:58"
        },
        {
          "id": "MDRW2100003588.1.15",
          "form": "발이라도 담그고 오셔요",
          "original_form": "발이라도 담그고 오셔요",
          "speaker_id": "1",
          "time": "20210821 02:59"
        },
        {
          "id": "MDRW2100003588.1.16",
          "form": "시진찍을거라서",
          "original_form": "시진찍을거라서",
          "speaker_id": "2",
          "time": "20210821 03:00"
        },
        {
          "id": "MDRW2100003588.1.17",
          "form": "해수욕장 가보려구요 ㅋㅋ!",
          "original_form": "해수욕장 가보려구요 ㅋㅋ!",
          "speaker_id": "2",
          "time": "20210821 03:00"
        },
        {
          "id": "MDRW2100003588.1.18",
          "form": "여행은 사진이 필수죠ㅎㅎ",
          "original_form": "여행은 사진이 필수죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 03:01"
        },
        {
          "id": "MDRW2100003588.1.19",
          "form": "남는건 사진뿐!?",
          "original_form": "남는건 사진뿐!?",
          "speaker_id": "1",
          "time": "20210821 03:01"
        },
        {
          "id": "MDRW2100003588.1.20",
          "form": "맞아요 남는건 사진뿐 !",
          "original_form": "맞아요 남는건 사진뿐 !",
          "speaker_id": "2",
          "time": "20210821 03:03"
        }
      ]
