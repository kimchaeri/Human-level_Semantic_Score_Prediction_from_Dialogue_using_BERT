      "utterance": [
        {
          "id": "MDRW2100014011.1.1",
          "form": "아니 학원 스터디 멤버였어요",
          "original_form": "아니 학원 스터디 멤버였어요",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014011.1.2",
          "form": "좀 높은 통굽힐신고갔는데...",
          "original_form": "좀 높은 통굽힐신고갔는데...",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014011.1.3",
          "form": "멤버들이 저보다 작다고 놀려서...",
          "original_form": "멤버들이 저보다 작다고 놀려서...",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014011.1.4",
          "form": "따흐흐흑",
          "original_form": "따흐흐흑",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014011.1.5",
          "form": "제 여자친구도 힐신으면 저랑 비슷한데",
          "original_form": "제 여자친구도 힐신으면 저랑 비슷한데",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014011.1.6",
          "form": "제 남치니도 힐신으면 작아져서....",
          "original_form": "제 남치니도 힐신으면 작아져서....",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014011.1.7",
          "form": "힐 못신어요...흑...",
          "original_form": "힐 못신어요...흑...",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014011.1.8",
          "form": "그건 누님이 너무 큰탓 아닐까요",
          "original_form": "그건 누님이 너무 큰탓 아닐까요",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014011.1.9",
          "form": "저 그렇게 큰 키아닌데",
          "original_form": "저 그렇게 큰 키아닌데",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014011.1.10",
          "form": "163인데...",
          "original_form": "163인데...",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014011.1.11",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014011.1.12",
          "form": "딱 중간인데...",
          "original_form": "딱 중간인데...",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014011.1.13",
          "form": "힐을 15cm짜리 신으시는가",
          "original_form": "힐을 15cm짜리 신으시는가",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014011.1.14",
          "form": "그럼 저보다 크실듯",
          "original_form": "그럼 저보다 크실듯",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014011.1.15",
          "form": "8센티 9센티..!",
          "original_form": "8센티 9센티..!",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014011.1.16",
          "form": "최고는11센티",
          "original_form": "최고는11센티",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014011.1.17",
          "form": "그냥 남치니가 키가 작은편인거로...ㅋㅋㅋㅋ",
          "original_form": "그냥 남치니가 키가 작은편인거로...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014011.1.18",
          "form": "5센티만 컸어도 좋았을텐데 아쉽...",
          "original_form": "5센티만 컸어도 좋았을텐데 아쉽...",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014011.1.19",
          "form": "에이 뭐",
          "original_form": "에이 뭐",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014011.1.20",
