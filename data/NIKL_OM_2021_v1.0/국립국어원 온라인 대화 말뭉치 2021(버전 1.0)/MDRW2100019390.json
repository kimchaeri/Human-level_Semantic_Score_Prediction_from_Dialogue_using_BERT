        {
          "id": "MDRW2100019390.1.1",
          "form": "ㅎㅎㅎ저도낮에아메리카노마셔서",
          "original_form": "ㅎㅎㅎ저도낮에아메리카노마셔서",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019390.1.2",
          "form": "두유라떼는가족들만줬어용",
          "original_form": "두유라떼는가족들만줬어용",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019390.1.3",
          "form": "아 저도 먹어보고 싶어용 ㅋㅋㅋㅋㅋ",
          "original_form": "아 저도 먹어보고 싶어용 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100019390.1.4",
          "form": "두유로 바꾸면 라떼맛이 좀 많이 달라지나요?",
          "original_form": "두유로 바꾸면 라떼맛이 좀 많이 달라지나요?",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100019390.1.5",
          "form": "거기도 커피가 들어가는거죠?",
          "original_form": "거기도 커피가 들어가는거죠?",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100019390.1.6",
          "form": "네맞아요 좀더달달고소?한거같아용",
          "original_form": "네맞아요 좀더달달고소?한거같아용",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100019390.1.7",
          "form": "어제칵테일마시고뻗었어요",
          "original_form": "어제칵테일마시고뻗었어요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100019390.1.8",
          "form": "어웈ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어웈ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100019390.1.9",
          "form": "커피맛나는칵테일맛잇더라구용",
          "original_form": "커피맛나는칵테일맛잇더라구용",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100019390.1.10",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100019390.1.11",
          "form": "어디서 드셨어요??",
          "original_form": "어디서 드셨어요??",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100019390.1.12",
          "form": "깔루아밀크!!",
          "original_form": "깔루아밀크!!",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100019390.1.13",
          "form": "칵테일바가서마셔써용",
          "original_form": "칵테일바가서마셔써용",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
