        },
        {
          "id": "MDRW2100025522.1.2",
          "form": "네 학교 다니고 있어요",
          "original_form": "네 학교 다니고 있어요",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025522.1.3",
          "form": "대학 요새 재미없죠",
          "original_form": "대학 요새 재미없죠",
          "speaker_id": "2",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025522.1.4",
          "form": "저는 전부 비대면이에요",
          "original_form": "저는 전부 비대면이에요",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025522.1.5",
          "form": "학교 다니시나요??",
          "original_form": "학교 다니시나요??",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025522.1.6",
          "form": "아뇨 전 졸업했어요",
          "original_form": "아뇨 전 졸업했어요",
          "speaker_id": "2",
          "time": "20210901 14:24"
        },
        {
          "id": "MDRW2100025522.1.7",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025522.1.8",
          "form": "부럽네요ㅠㅠ",
          "original_form": "부럽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025522.1.9",
          "form": "회사다니시나요??",
          "original_form": "회사다니시나요??",
          "speaker_id": "1",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025522.1.10",
          "form": "그만큼 늙었죠ㅋㅋ",
          "original_form": "그만큼 늙었죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025522.1.11",
          "form": "네 이제 5년차네요 ㅎㅎ",
          "original_form": "네 이제 5년차네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:25"
        },
        {
          "id": "MDRW2100025522.1.12",
          "form": "우와 멋있네요",
          "original_form": "우와 멋있네요",
          "speaker_id": "1",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100025522.1.13",
          "form": "학교다닐때가 좋으셨어요 회사다닐때가 더 좋으세요??",
          "original_form": "학교다닐때가 좋으셨어요 회사다닐때가 더 좋으세요??",
          "speaker_id": "1",
          "time": "20210901 14:29"
        },
        {
          "id": "MDRW2100025522.1.14",
          "form": "음 재미는 학교인데",
          "original_form": "음 재미는 학교인데",
          "speaker_id": "2",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100025522.1.15",
          "form": "회사가 안정적이라서",
          "original_form": "회사가 안정적이라서",
          "speaker_id": "2",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025522.1.16",
          "form": "회사 선택하겠습니다",
          "original_form": "회사 선택하겠습니다",
          "speaker_id": "2",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025522.1.17",
          "form": "학교생활이 더 불안정?했나요???",
          "original_form": "학교생활이 더 불안정?했나요???",
          "speaker_id": "1",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025522.1.18",
          "form": "학교는 돈이 없자나요 ㅋㅋㅋ",
          "original_form": "학교는 돈이 없자나요 ㅋㅋㅋ",
