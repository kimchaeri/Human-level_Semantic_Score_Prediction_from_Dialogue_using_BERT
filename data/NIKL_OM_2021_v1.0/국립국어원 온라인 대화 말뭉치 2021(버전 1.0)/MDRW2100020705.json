      "utterance": [
        {
          "id": "MDRW2100020705.1.1",
          "form": "배드민턴 동아리 별로였어요!",
          "original_form": "배드민턴 동아리 별로였어요!",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020705.1.2",
          "form": "코트도 별로 없고 자기네끼리만 치느라 바쁘고..",
          "original_form": "코트도 별로 없고 자기네끼리만 치느라 바쁘고..",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020705.1.3",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020705.1.4",
          "form": "운동 좋아하시면",
          "original_form": "운동 좋아하시면",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020705.1.5",
          "form": "학교다니실때 인기많으셨겠어요",
          "original_form": "학교다니실때 인기많으셨겠어요",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020705.1.6",
          "form": "ㅋㅋㅋㅋㅋ운동을 잘해야 인기가 많죠 ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ운동을 잘해야 인기가 많죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020705.1.7",
          "form": "못해서 운동동아리 들어가본건데 흑흑...",
          "original_form": "못해서 운동동아리 들어가본건데 흑흑...",
          "speaker_id": "2",
          "time": "20210828 11:27"
        },
        {
          "id": "MDRW2100020705.1.8",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020705.1.9",
          "form": "저는 학교다닐때",
          "original_form": "저는 학교다닐때",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020705.1.10",
          "form": "대학생때",
          "original_form": "대학생때",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020705.1.11",
          "form": "수영장이 있어서",
          "original_form": "수영장이 있어서",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020705.1.12",
          "form": "거기서 수영 많이 했어요!",
          "original_form": "거기서 수영 많이 했어요!",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020705.1.13",
          "form": "교양 과목중에 수영이 있어서ㅋㅋㅋ",
          "original_form": "교양 과목중에 수영이 있어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020705.1.14",
          "form": "오~ 진짜요? ㅎㅎ",
          "original_form": "오~ 진짜요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020705.1.15",
          "form": "저도 수영수업 들은 적 있는데..!",
          "original_form": "저도 수영수업 들은 적 있는데..!",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020705.1.16",
          "form": "우와 어떠셨어요?",
          "original_form": "우와 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020705.1.17",
          "form": "수영 실기시험 때 갑자기 쥐가 나가지고 죽을뻔했어요",
          "original_form": "수영 실기시험 때 갑자기 쥐가 나가지고 죽을뻔했어요",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020705.1.18",
          "form": "교수님이 가르쳐주셨죠?",
          "original_form": "교수님이 가르쳐주셨죠?",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020705.1.19",
          "form": "헉 ㅠㅠ",
          "original_form": "헉 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020705.1.20",
          "form": "바닥도 3미터인가 5미터인데... ㅠㅠ",
          "original_form": "바닥도 3미터인가 5미터인데... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020705.1.21",
          "form": "교수님인지 아닌지 모르겠어요 그냥 강사같기도 했는데",
          "original_form": "교수님인지 아닌지 모르겠어요 그냥 강사같기도 했는데",
          "speaker_id": "2",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020705.1.22",
          "form": "학교에 그렇게 깊은 수영장이 있었어용?",
          "original_form": "학교에 그렇게 깊은 수영장이 있었어용?",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020705.1.23",
          "form": "우오ㅓ",
          "original_form": "우오ㅓ",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020705.1.24",
          "form": "저희 학교에는 그렇게 깊은 수영장은 없어가지구ㅠㅠ",
          "original_form": "저희 학교에는 그렇게 깊은 수영장은 없어가지구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020705.1.25",
          "form": "저희 학교는 수영장이 없었어요..",
          "original_form": "저희 학교는 수영장이 없었어요..",
          "speaker_id": "2",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020705.1.26",
          "form": "그래서 다른 학교로 원정수업을 ㅋㅋ",
          "original_form": "그래서 다른 학교로 원정수업을 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020705.1.27",
          "form": "아하 그렇게 수업 진행하기도 하는군요",
          "original_form": "아하 그렇게 수업 진행하기도 하는군요",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020705.1.28",
          "form": "첨 알았어용ㅋㅋ",
          "original_form": "첨 알았어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020705.1.29",
          "form": "저도 그런식으로 수업 진행하는 거 첨 봤어요",
          "original_form": "저도 그런식으로 수업 진행하는 거 첨 봤어요",
          "speaker_id": "2",
          "time": "20210828 11:32"
        }
      ]
    }
