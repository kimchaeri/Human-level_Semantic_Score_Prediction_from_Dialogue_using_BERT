      "utterance": [
        {
          "id": "MDRW2100028805.1.1",
          "form": "게임 좋아하는거 있으세요?",
          "original_form": "게임 좋아하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028805.1.2",
          "form": "오 게임 뭐 좋아하세요",
          "original_form": "오 게임 뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028805.1.3",
          "form": "스위치랑 플스 있어요",
          "original_form": "스위치랑 플스 있어요",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028805.1.4",
          "form": "오 저도 플스 있어요",
          "original_form": "오 저도 플스 있어요",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028805.1.5",
          "form": "베그랑 롤해요",
          "original_form": "베그랑 롤해요",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028805.1.6",
          "form": "배그",
          "original_form": "배그",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028805.1.7",
          "form": "배그는 저 간떨려서",
          "original_form": "배그는 저 간떨려서",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028805.1.8",
          "form": "남하는거만 봤어요 ㅋㅋㅋㅋ",
          "original_form": "남하는거만 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028805.1.9",
          "form": "컴도 없구",
          "original_form": "컴도 없구",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028805.1.10",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028805.1.11",
          "form": "혹시 스트리머 보세요?",
          "original_form": "혹시 스트리머 보세요?",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028805.1.12",
          "form": "갑툭튀 때문에 ㅋㅋㅋ",
          "original_form": "갑툭튀 때문에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028805.1.13",
          "form": "당근빠다죠",
          "original_form": "당근빠다죠",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028805.1.14",
          "form": "트위치봐요",
          "original_form": "트위치봐요",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028805.1.15",
          "form": "ㅋㅋㅋ핵도 많긴해요;;",
          "original_form": "ㅋㅋㅋ핵도 많긴해요;;",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028805.1.16",
          "form": "오 저도",
          "original_form": "오 저도",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028805.1.17",
          "form": "누 구 제일 좋아하나요",
          "original_form": "누 구 제일 좋아하나요",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028805.1.18",
          "form": "뜨뜨뜨뜨랑 롤하는거 그냥 돌려봐요",
          "original_form": "뜨뜨뜨뜨랑 롤하는거 그냥 돌려봐요",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028805.1.19",
          "form": "롤 재대로 한지 얼마 안되서",
          "original_form": "롤 재대로 한지 얼마 안되서",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028805.1.20",
          "form": "와 첨들어 보는데 구독자수 엄청나네요",
          "original_form": "와 첨들어 보는데 구독자수 엄청나네요",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028805.1.21",
          "form": "저는 풍월량 방송 봐여",
          "original_form": "저는 풍월량 방송 봐여",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028805.1.22",
          "form": "오 저 풍월량 방송 본적있어요",
          "original_form": "오 저 풍월량 방송 본적있어요",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028805.1.23",
          "form": "후원도 하세요?",
          "original_form": "후원도 하세요?",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028805.1.24",
          "form": "그럴리가요",
          "original_form": "그럴리가요",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028805.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028805.1.26",
          "form": "약간 그런건 돈 아까운",
          "original_form": "약간 그런건 돈 아까운",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028805.1.27",
          "form": "돈 잘벌면 하고 싶긴 해요",
          "original_form": "돈 잘벌면 하고 싶긴 해요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028805.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028805.1.29",
          "form": "미션 하는거 재밌어욬ㅋ",
          "original_form": "미션 하는거 재밌어욬ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028805.1.30",
          "form": "ㅋㅋㅋㅋ 저는 보는걸 즐긴다는",
          "original_form": "ㅋㅋㅋㅋ 저는 보는걸 즐긴다는",
